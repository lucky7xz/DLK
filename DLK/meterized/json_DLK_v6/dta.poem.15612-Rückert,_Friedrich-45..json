{"dta.poem.15612": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem Augenblick, wann still der Geist versunken", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", ",", "wann", "still", "der", "Geist", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In sich und Welt und Gott, nicht wein- noch schlummertrunken,", "tokens": ["In", "sich", "und", "Welt", "und", "Gott", ",", "nicht", "wein", "noch", "schlum\u00b7mer\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KON", "NN", "KON", "NN", "$,", "PTKNEG", "TRUNC", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht trunken, sondern klar, nicht schlummernd, sondern wach,", "tokens": ["Nicht", "trun\u00b7ken", ",", "son\u00b7dern", "klar", ",", "nicht", "schlum\u00b7mernd", ",", "son\u00b7dern", "wach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KON", "ADJD", "$,", "PTKNEG", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie der Sonne Bild im unbewegten Bach;", "tokens": ["Als\u00b7wie", "der", "Son\u00b7ne", "Bild", "im", "un\u00b7be\u00b7weg\u00b7ten", "Bach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wann Fern und Nah, und Ist und War, und Zeit und Raum", "tokens": ["Wann", "Fern", "und", "Nah", ",", "und", "Ist", "und", "War", ",", "und", "Zeit", "und", "Raum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "$,", "KON", "VAFIN", "KON", "VAFIN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zergangen ist, alswie in stiller Flut der Schaum;", "tokens": ["Zer\u00b7gan\u00b7gen", "ist", ",", "als\u00b7wie", "in", "stil\u00b7ler", "Flut", "der", "Schaum", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KON", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wann du des Lebensbaums entfaltet Bl\u00fctenprangen", "tokens": ["Wann", "du", "des", "Le\u00b7bens\u00b7baums", "ent\u00b7fal\u00b7tet", "Bl\u00fc\u00b7ten\u00b7pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An deinem Busen f\u00fchlst von einer Knosp' umfangen;", "tokens": ["An", "dei\u00b7nem", "Bu\u00b7sen", "f\u00fchlst", "von", "ei\u00b7ner", "Knosp'", "um\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wann Erd' und Himmel dir in einen Duft verschwimmt,", "tokens": ["Wann", "Erd'", "und", "Him\u00b7mel", "dir", "in", "ei\u00b7nen", "Duft", "ver\u00b7schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Stern als Blume bl\u00fcht, als Stern die Blume glimmt;", "tokens": ["Der", "Stern", "als", "Blu\u00b7me", "bl\u00fcht", ",", "als", "Stern", "die", "Blu\u00b7me", "glimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "VVFIN", "$,", "KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In solchem Augenblick, wo wie mit heil'gem Rauschen", "tokens": ["In", "sol\u00b7chem", "Au\u00b7gen\u00b7blick", ",", "wo", "wie", "mit", "heil'\u00b7gem", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAV", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Strom der Sch\u00f6pfung geht durch deines Herzens Lauschen;", "tokens": ["Der", "Strom", "der", "Sch\u00f6p\u00b7fung", "geht", "durch", "dei\u00b7nes", "Her\u00b7zens", "Lau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wo du nicht du mehr bist, und nichts mehr ist als du", "tokens": ["Wo", "du", "nicht", "du", "mehr", "bist", ",", "und", "nichts", "mehr", "ist", "als", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "PPER", "ADV", "VAFIN", "$,", "KON", "PIS", "ADV", "VAFIN", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Gott, in dem du bist, dem du dich athmest zu;", "tokens": ["Und", "Gott", ",", "in", "dem", "du", "bist", ",", "dem", "du", "dich", "ath\u00b7mest", "zu", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "$,", "PRELS", "PPER", "PRF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "In solchem Augenblick, der wie ein Blick der Augen,", "tokens": ["In", "sol\u00b7chem", "Au\u00b7gen\u00b7blick", ",", "der", "wie", "ein", "Blick", "der", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Liebesaugen kommt, Besinnung wegzusaugen;", "tokens": ["Der", "Lie\u00b7be\u00b7sau\u00b7gen", "kommt", ",", "Be\u00b7sin\u00b7nung", "weg\u00b7zu\u00b7sau\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "In solchem Augenblick, wer ihn, eh' er geschwunden,", "tokens": ["In", "sol\u00b7chem", "Au\u00b7gen\u00b7blick", ",", "wer", "ihn", ",", "eh'", "er", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWS", "PPER", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Empfinden konnte, der hat Ewigkeit empfunden.", "tokens": ["Emp\u00b7fin\u00b7den", "konn\u00b7te", ",", "der", "hat", "E\u00b7wig\u00b7keit", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "PRELS", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und so wer Ewigkeit empfunden hat einmal,", "tokens": ["Und", "so", "wer", "E\u00b7wig\u00b7keit", "emp\u00b7fun\u00b7den", "hat", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "VVPP", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4lt ewig fest sie, wie der Demant seinen Stral.", "tokens": ["H\u00e4lt", "e\u00b7wig", "fest", "sie", ",", "wie", "der", "De\u00b7mant", "sei\u00b7nen", "Stral", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}