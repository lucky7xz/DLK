{"dta.poem.10190": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "An einen Freund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Noch seh' ich deine schwermutsvollen Augen,             ", "tokens": ["Noch", "seh'", "ich", "dei\u00b7ne", "schwer\u00b7muts\u00b7vol\u00b7len", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein bla\u00df Gesichtchen und den herben Zug,", "tokens": ["Dein", "bla\u00df", "Ge\u00b7sicht\u00b7chen", "und", "den", "her\u00b7ben", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den deine Lippen auch als Mann behielten.", "tokens": ["Den", "dei\u00b7ne", "Lip\u00b7pen", "auch", "als", "Mann", "be\u00b7hiel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir hatten, Knaben, in die Waldesschatten", "tokens": ["Wir", "hat\u00b7ten", ",", "Kna\u00b7ben", ",", "in", "die", "Wal\u00b7des\u00b7schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns scheu zur\u00fcckgezogen von den Spielen,", "tokens": ["Uns", "scheu", "zu\u00b7r\u00fcck\u00b7ge\u00b7zo\u00b7gen", "von", "den", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sprachen wichtig \u00fcber Welt und Menschen.", "tokens": ["Und", "spra\u00b7chen", "wich\u00b7tig", "\u00fc\u00b7ber", "Welt", "und", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich f\u00fchle noch das Grau\u2019n, als erste Zweifel", "tokens": ["Ich", "f\u00fch\u00b7le", "noch", "das", "Grau'n", ",", "als", "ers\u00b7te", "Zwei\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Uns kamen \u00fcber Gott \u2014 Unsterblichkeit,", "tokens": ["Uns", "ka\u00b7men", "\u00fc\u00b7ber", "Gott", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$(", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Uns unerkl\u00e4rlich Schauer \u00fcberliefen,", "tokens": ["Uns", "un\u00b7er\u00b7kl\u00e4r\u00b7lich", "Schau\u00b7er", "\u00fc\u00b7berl\u00b7ie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wenn wir der Liebe Sphinx zu deuten suchten.", "tokens": ["Wenn", "wir", "der", "Lie\u00b7be", "Sphinx", "zu", "deu\u00b7ten", "such\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es flo\u00df der Waldbach pl\u00e4tschernd uns zu F\u00fc\u00dfen,", "tokens": ["Es", "flo\u00df", "der", "Wald\u00b7bach", "pl\u00e4t\u00b7schernd", "uns", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Buchfink trillerte, die Drossel pfiff,", "tokens": ["Der", "Buch\u00b7fink", "tril\u00b7ler\u00b7te", ",", "die", "Dros\u00b7sel", "pfiff", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und stie\u00df der Falke seinen kurzen Schrei", "tokens": ["Und", "stie\u00df", "der", "Fal\u00b7ke", "sei\u00b7nen", "kur\u00b7zen", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In all\u2019 die Stille, flogen wir zusammen.", "tokens": ["In", "all'", "die", "Stil\u00b7le", ",", "flo\u00b7gen", "wir", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie viele Jahre sind seitdem vor\u00fcber.", "tokens": ["Wie", "vie\u00b7le", "Jah\u00b7re", "sind", "seit\u00b7dem", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Du stehst im Leben aufrecht, und des Weges", "tokens": ["Du", "stehst", "im", "Le\u00b7ben", "auf\u00b7recht", ",", "und", "des", "We\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVPP", "$,", "KON", "ART", "NN"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Gehst wohlbewu\u00dft du, klar, und ohne Schwanken.", "tokens": ["Gehst", "wohl\u00b7be\u00b7wu\u00dft", "du", ",", "klar", ",", "und", "oh\u00b7ne", "Schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch denkst du noch zur\u00fcck an jene Stunden,", "tokens": ["Doch", "denkst", "du", "noch", "zu\u00b7r\u00fcck", "an", "je\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn Buchenkronen dir zu H\u00e4upten rauschen,", "tokens": ["Wenn", "Bu\u00b7chen\u00b7kro\u00b7nen", "dir", "zu", "H\u00e4up\u00b7ten", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und hoch am Himmel schrill der Falke schreit?", "tokens": ["Und", "hoch", "am", "Him\u00b7mel", "schrill", "der", "Fal\u00b7ke", "schreit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}