{"dta.poem.11421": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vom undanck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.71", "tr:0.14", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um dir den undanck abzumahlen,", "tokens": ["Um", "dir", "den", "un\u00b7danck", "ab\u00b7zu\u00b7mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bilde dir ihn also ein:", "tokens": ["So", "bil\u00b7de", "dir", "ihn", "al\u00b7so", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der hochmuth will nicht schuldig seyn,", "tokens": ["Der", "hoch\u00b7muth", "will", "nicht", "schul\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eigenliebe will nicht zahlen.", "tokens": ["Und", "ei\u00b7gen\u00b7lie\u00b7be", "will", "nicht", "zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}