{"textgrid.poem.57656": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsabend", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abendstern blinkt durch die Zweige,", "tokens": ["Der", "A\u00b7bends\u00b7tern", "blinkt", "durch", "die", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es schwimmt der Wald in blauem Duft,", "tokens": ["Es", "schwimmt", "der", "Wald", "in", "blau\u00b7em", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die allerletzte Drossel fl\u00f6tet,", "tokens": ["Die", "al\u00b7ler\u00b7letz\u00b7te", "Dros\u00b7sel", "fl\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So weich und milde ist die Luft.", "tokens": ["So", "weich", "und", "mil\u00b7de", "ist", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die gelben Haselk\u00e4tzchen zittern", "tokens": ["Die", "gel\u00b7ben", "Ha\u00b7sel\u00b7k\u00e4tz\u00b7chen", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Abendwinde hin und her,", "tokens": ["Im", "Ab\u00b7end\u00b7win\u00b7de", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich tr\u00e4ume in den Fr\u00fchlingsabend", "tokens": ["Ich", "tr\u00e4u\u00b7me", "in", "den", "Fr\u00fch\u00b7lings\u00b7a\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Brust seufzt tief und schwer.", "tokens": ["Und", "mei\u00b7ne", "Brust", "seufzt", "tief", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist ein Seufzer voller Sehnsucht,", "tokens": ["Es", "ist", "ein", "Seuf\u00b7zer", "vol\u00b7ler", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halb ist es Leid, halb ist es Lust,", "tokens": ["Halb", "ist", "es", "Leid", ",", "halb", "ist", "es", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "NN", "$,", "ADJD", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch du denkst meiner diese Stunde,", "tokens": ["Auch", "du", "denkst", "mei\u00b7ner", "die\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwer hebt sich jetzt auch deine Brust.", "tokens": ["Schwer", "hebt", "sich", "jetzt", "auch", "dei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}