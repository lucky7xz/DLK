{"dta.poem.8295": {"metadata": {"author": {"name": "Milton, John", "birth": "N.A.", "death": "N.A."}, "title": "Vorbericht  \n  zum zweyten Bande  \n des  \n verlohrnen Paradieses.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1763", "urn": "urn:nbn:de:kobv:b4-200905199734", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir haben hier, ihr Thronen, Potentaten,", "tokens": ["Wir", "ha\u00b7ben", "hier", ",", "ihr", "Thro\u00b7nen", ",", "Po\u00b7ten\u00b7ta\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herrschaften, F\u00fcrsten, Tugenden, und Kr\u00e4fte,", "tokens": ["Herr\u00b7schaf\u00b7ten", ",", "F\u00fcrs\u00b7ten", ",", "Tu\u00b7gen\u00b7den", ",", "und", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn anders diese pr\u00e4chtgen Titel nicht", "tokens": ["Wenn", "an\u00b7ders", "die\u00b7se", "pr\u00e4cht\u00b7gen", "Ti\u00b7tel", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PDAT", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Blo\u00df Titel sind, da nun ein anderer", "tokens": ["Blo\u00df", "Ti\u00b7tel", "sind", ",", "da", "nun", "ein", "an\u00b7de\u00b7rer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "$,", "KOUS", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sich aller Macht anma\u00dft, und unter ihm,", "tokens": ["Sich", "al\u00b7ler", "Macht", "an\u00b7ma\u00dft", ",", "und", "un\u00b7ter", "ihm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "VVFIN", "$,", "KON", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und seinem Namen des Gesalbeten", "tokens": ["Und", "sei\u00b7nem", "Na\u00b7men", "des", "Ge\u00b7sal\u00b7be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Wir \u00fcbrigen nun ganz verdunkelt sind:", "tokens": ["Wir", "\u00fcb\u00b7ri\u00b7gen", "nun", "ganz", "ver\u00b7dun\u00b7kelt", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wir haben hier, so bey der Mitternacht,", "tokens": ["Wir", "ha\u00b7ben", "hier", ",", "so", "bey", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So eilig uns versammelt zu erw\u00e4gen,", "tokens": ["So", "ei\u00b7lig", "uns", "ver\u00b7sam\u00b7melt", "zu", "er\u00b7w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit welchen Ehren wir den neuen Herrn", "tokens": ["Mit", "wel\u00b7chen", "Eh\u00b7ren", "wir", "den", "neu\u00b7en", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Allhier begegnen wollen, der von uns", "tokens": ["All\u00b7hier", "be\u00b7geg\u00b7nen", "wol\u00b7len", ",", "der", "von", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "VMFIN", "$,", "PRELS", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Den Knietribut, den wir noch nicht bezahlt,", "tokens": ["Den", "Knie\u00b7tri\u00b7but", ",", "den", "wir", "noch", "nicht", "be\u00b7zahlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Erwartet, und ihn hier empfangen will.", "tokens": ["Er\u00b7war\u00b7tet", ",", "und", "ihn", "hier", "emp\u00b7fan\u00b7gen", "will", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Unbilliger, beschimpfender Tribut!", "tokens": ["Un\u00b7bil\u00b7li\u00b7ger", ",", "be\u00b7schimp\u00b7fen\u00b7der", "Tri\u00b7but", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Dem\u00fcthgende Verehrung! schon zu viel,", "tokens": ["De\u00b7m\u00fcth\u00b7gen\u00b7de", "Ver\u00b7eh\u00b7rung", "!", "schon", "zu", "viel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Sie Einem zu erzeigen! \u2014 aber nun", "tokens": ["Sie", "Ei\u00b7nem", "zu", "er\u00b7zei\u00b7gen", "!", "a\u00b7ber", "nun"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "PIS", "PTKZU", "VVINF", "$.", "$(", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Noch einem Zweyten, seinem Ebenbild", "tokens": ["Noch", "ei\u00b7nem", "Zwey\u00b7ten", ",", "sei\u00b7nem", "E\u00b7ben\u00b7bild"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Sie zu erzeigen, wer ertr\u00e4get das?", "tokens": ["Sie", "zu", "er\u00b7zei\u00b7gen", ",", "wer", "er\u00b7tr\u00e4\u00b7get", "das", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PWS", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Doch wie? wenn uns ein besserer Entschlu\u00df", "tokens": ["Doch", "wie", "?", "wenn", "uns", "ein", "bes\u00b7se\u00b7rer", "Ent\u00b7schlu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$.", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Zu gr\u00f6\u00dferm Edelmuth begeister", "tokens": ["Zu", "gr\u00f6\u00b7\u00dferm", "E\u00b7del\u00b7muth", "be\u00b7geis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und dieses Joch uns abzuwerfen lehrte?", "tokens": ["Und", "die\u00b7ses", "Joch", "uns", "ab\u00b7zu\u00b7wer\u00b7fen", "lehr\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "PPER", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wollt ihr die Nacken beugen? wollt ihr knien,", "tokens": ["Wollt", "ihr", "die", "Na\u00b7cken", "beu\u00b7gen", "?", "wollt", "ihr", "kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Dem\u00fcthig vor ihm knien im Staube? \u2014 Nein!", "tokens": ["De\u00b7m\u00fct\u00b7hig", "vor", "ihm", "kni\u00b7en", "im", "Stau\u00b7be", "?", "Nein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "APPRART", "NN", "$.", "$(", "PTKANT", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Jhr wollt es nicht, kenn ich euch anders recht,", "tokens": ["Ihr", "wollt", "es", "nicht", ",", "kenn", "ich", "euch", "an\u00b7ders", "recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$,", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und kennt ihr selbst euch recht! Jhr alle seyd", "tokens": ["Und", "kennt", "ihr", "selbst", "euch", "recht", "!", "Ihr", "al\u00b7le", "seyd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "ADJD", "$.", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Des Himmels S\u00f6hne, den niemand vor euch", "tokens": ["Des", "Him\u00b7mels", "S\u00f6h\u00b7ne", ",", "den", "nie\u00b7mand", "vor", "euch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PIS", "APPR", "PPER"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Besessen hat; und ob ihr alle zwar", "tokens": ["Be\u00b7ses\u00b7sen", "hat", ";", "und", "ob", "ihr", "al\u00b7le", "zwar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$.", "KON", "KOUS", "PPER", "PIS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Nicht gleich erhaben, nicht gleich herrlich seyd;", "tokens": ["Nicht", "gleich", "er\u00b7ha\u00b7ben", ",", "nicht", "gleich", "herr\u00b7lich", "seyd", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$,", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "So seyd ihr doch deswegen frey \u2014 gleich frey!", "tokens": ["So", "seyd", "ihr", "doch", "des\u00b7we\u00b7gen", "frey", "gleich", "frey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "ADJD", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Denn mit der Freyheit k\u00f6nnen Ordnungen", "tokens": ["Denn", "mit", "der", "Frey\u00b7heit", "k\u00f6n\u00b7nen", "Ord\u00b7nun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Und Gnade wohl bestehn. Wer kann denn nun", "tokens": ["Und", "Gna\u00b7de", "wohl", "be\u00b7stehn", ".", "Wer", "kann", "denn", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "VVINF", "$.", "PWS", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Sich mit Vernunft, mit irgend einem Rechte", "tokens": ["Sich", "mit", "Ver\u00b7nunft", ",", "mit", "ir\u00b7gend", "ei\u00b7nem", "Rech\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "NN", "$,", "APPR", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Der Oberherrschaft \u00fcber die anma\u00dfen,", "tokens": ["Der", "O\u00b7ber\u00b7herr\u00b7schaft", "\u00fc\u00b7ber", "die", "an\u00b7ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Die durch das Recht zusammen gleich ihm sind,", "tokens": ["Die", "durch", "das", "Recht", "zu\u00b7sam\u00b7men", "gleich", "ihm", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Und wenn an Macht und Herrlichkeit geringer", "tokens": ["Und", "wenn", "an", "Macht", "und", "Herr\u00b7lich\u00b7keit", "ge\u00b7rin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN", "KON", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "In Freyheit gleich ihm sind? Und wer kann denn", "tokens": ["In", "Frey\u00b7heit", "gleich", "ihm", "sind", "?", "Und", "wer", "kann", "denn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "PPER", "VAFIN", "$.", "KON", "PWS", "VMFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Gesetze geben, die nicht irren k\u00f6nnen?", "tokens": ["Ge\u00b7set\u00b7ze", "ge\u00b7ben", ",", "die", "nicht", "ir\u00b7ren", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PRELS", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Und wer kann endlich denn zu unserm Herrn", "tokens": ["Und", "wer", "kann", "end\u00b7lich", "denn", "zu", "un\u00b7serm", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Sich aufzuwerfen wagen, und Anbethung", "tokens": ["Sich", "auf\u00b7zu\u00b7wer\u00b7fen", "wa\u00b7gen", ",", "und", "An\u00b7be\u00b7thung"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "VVINF", "VVINF", "$,", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Von uns, von K\u00f6nigen, von G\u00f6ttern fordern?", "tokens": ["Von", "uns", ",", "von", "K\u00f6\u00b7ni\u00b7gen", ",", "von", "G\u00f6t\u00b7tern", "for\u00b7dern", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "NN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Die Titel schon bekr\u00e4ftigen, da\u00df wir", "tokens": ["Die", "Ti\u00b7tel", "schon", "be\u00b7kr\u00e4f\u00b7ti\u00b7gen", ",", "da\u00df", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Geschaffen sind, zu herrschen, nicht zu dienen.", "tokens": ["Ge\u00b7schaf\u00b7fen", "sind", ",", "zu", "herr\u00b7schen", ",", "nicht", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PTKZU", "VVINF", "$,", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bishieher fand er ohne Widerspruch", "tokens": ["Bis\u00b7hie\u00b7her", "fand", "er", "oh\u00b7ne", "Wi\u00b7der\u00b7spruch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit der verwegenen Beredsamkeit", "tokens": ["Mit", "der", "ver\u00b7we\u00b7ge\u00b7nen", "Be\u00b7red\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geh\u00f6r: als von der Schaar der Seraphim", "tokens": ["Ge\u00b7h\u00f6r", ":", "als", "von", "der", "Schaar", "der", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich ", "tokens": ["Sich"], "token_info": ["word"], "pos": ["PRF"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Der mit mehr Eifer den Allm\u00e4chtigen", "tokens": ["Der", "mit", "mehr", "Ei\u00b7fer", "den", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Und sein Geboth verehrte. Voller Gluth", "tokens": ["Und", "sein", "Ge\u00b7both", "ver\u00b7ehr\u00b7te", ".", "Vol\u00b7ler", "Gluth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und heilgen Eifer, setzt\u2019 er sich dem Strom", "tokens": ["Und", "heil\u00b7gen", "Ei\u00b7fer", ",", "setzt'", "er", "sich", "dem", "Strom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"