{"textgrid.poem.35129": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo gingst du hin? Ich wei\u00df es leider nicht.", "tokens": ["Wo", "gingst", "du", "hin", "?", "Ich", "wei\u00df", "es", "lei\u00b7der", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du gingst und bist wahrscheinlich doch geblieben.", "tokens": ["Du", "gingst", "und", "bist", "wahr\u00b7schein\u00b7lich", "doch", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Obzwar die Trauer gern vom Scheiden spricht,", "tokens": ["Ob\u00b7zwar", "die", "Trau\u00b7er", "gern", "vom", "Schei\u00b7den", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Himmel hats wohl anders vorgeschrieben.", "tokens": ["Der", "Him\u00b7mel", "hats", "wohl", "an\u00b7ders", "vor\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du h\u00f6rst vielleicht mein Wort, h\u00f6rst meine Fragen,", "tokens": ["Du", "h\u00f6rst", "viel\u00b7leicht", "mein", "Wort", ",", "h\u00f6rst", "mei\u00b7ne", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch ahne ich, du wei\u00dft es selbst schon kaum,", "tokens": ["Doch", "ah\u00b7ne", "ich", ",", "du", "wei\u00dft", "es", "selbst", "schon", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und f\u00fchlst du es, so kannst du es nicht sagen;", "tokens": ["Und", "f\u00fchlst", "du", "es", ",", "so", "kannst", "du", "es", "nicht", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "ADV", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Im Grabe spricht kein Schl\u00e4fer mehr im Traum.", "tokens": ["Im", "Gra\u00b7be", "spricht", "kein", "Schl\u00e4\u00b7fer", "mehr", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo gingst du hin? O w\u00fc\u00dfte ich es doch!", "tokens": ["Wo", "gingst", "du", "hin", "?", "O", "w\u00fc\u00df\u00b7te", "ich", "es", "doch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "NE", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich mu\u00df ja auch denselben Weg einst gehen", "tokens": ["Ich", "mu\u00df", "ja", "auch", "den\u00b7sel\u00b7ben", "Weg", "einst", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PDAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und werde in der letzten Stunde noch", "tokens": ["Und", "wer\u00b7de", "in", "der", "letz\u00b7ten", "Stun\u00b7de", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit dieser Frage vor der Pforte stehen.", "tokens": ["Mit", "die\u00b7ser", "Fra\u00b7ge", "vor", "der", "Pfor\u00b7te", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denselben Weg? Und auch dieselbe Pforte?", "tokens": ["Den\u00b7sel\u00b7ben", "Weg", "?", "Und", "auch", "die\u00b7sel\u00b7be", "Pfor\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$.", "KON", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wer darf wohl sagen ja, und wer wohl nein!", "tokens": ["Wer", "darf", "wohl", "sa\u00b7gen", "ja", ",", "und", "wer", "wohl", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "VVINF", "PTKANT", "$,", "KON", "PWS", "ADV", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Giebt es denselben Ort am selben Orte?", "tokens": ["Giebt", "es", "den\u00b7sel\u00b7ben", "Ort", "am", "sel\u00b7ben", "Or\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wer da kommt, tritt der auch wirklich ein?", "tokens": ["Und", "wer", "da", "kommt", ",", "tritt", "der", "auch", "wirk\u00b7lich", "ein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "VVFIN", "ART", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo gingst du hin? Ist diese Frage klar?", "tokens": ["Wo", "gingst", "du", "hin", "?", "Ist", "die\u00b7se", "Fra\u00b7ge", "klar", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "VAFIN", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist wohl die Trennung \u00f6rtlich zu verstehen?", "tokens": ["Ist", "wohl", "die", "Tren\u00b7nung", "\u00f6rt\u00b7lich", "zu", "ver\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo hier der Mensch mit seiner Seele war,", "tokens": ["Wo", "hier", "der", "Mensch", "mit", "sei\u00b7ner", "See\u00b7le", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dorthin wird sie, sobald sie frei ist, gehen.", "tokens": ["Dor\u00b7thin", "wird", "sie", ",", "so\u00b7bald", "sie", "frei", "ist", ",", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wir waren Eins im Glauben und im Lieben;", "tokens": ["Wir", "wa\u00b7ren", "Eins", "im", "Glau\u00b7ben", "und", "im", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPRART", "NN", "KON", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du trachtetest wie ich nach Gottes Licht;", "tokens": ["Du", "trach\u00b7te\u00b7test", "wie", "ich", "nach", "Got\u00b7tes", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So sind wir also doch vereint geblieben", "tokens": ["So", "sind", "wir", "al\u00b7so", "doch", "ver\u00b7eint", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und beide gl\u00fccklich; ich verlor dich nicht!", "tokens": ["Und", "bei\u00b7de", "gl\u00fcck\u00b7lich", ";", "ich", "ver\u00b7lor", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}