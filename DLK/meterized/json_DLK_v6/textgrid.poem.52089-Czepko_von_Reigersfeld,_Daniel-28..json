{"textgrid.poem.52089": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist zu wol beredt, drum werden wir nicht zwey,", "tokens": ["Du", "bist", "zu", "wol", "be\u00b7redt", ",", "drum", "wer\u00b7den", "wir", "nicht", "zwey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADV", "ADJD", "$,", "PAV", "VAFIN", "PPER", "PTKNEG", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich stamml und komme dir nicht mit der Zunge bey:", "tokens": ["Ich", "stamml", "und", "kom\u00b7me", "dir", "nicht", "mit", "der", "Zun\u00b7ge", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann offte, wann ich sol hin nach Futt-Futter gehn,", "tokens": ["Dann", "off\u00b7te", ",", "wann", "ich", "sol", "hin", "nach", "Fut\u00b7tFut\u00b7ter", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da wo Ve-Venus steckt, bleib ich beym Mars-Ars stehn.", "tokens": ["Da", "wo", "Ve\u00b7Ve\u00b7nus", "steckt", ",", "bleib", "ich", "beym", "Mar\u00b7sArs", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NE", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}