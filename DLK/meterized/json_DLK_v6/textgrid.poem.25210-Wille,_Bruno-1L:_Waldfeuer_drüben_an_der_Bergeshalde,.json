{"textgrid.poem.25210": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Waldfeuer dr\u00fcben an der Bergeshalde,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Waldfeuer dr\u00fcben an der Bergeshalde,", "tokens": ["Wald\u00b7feu\u00b7er", "dr\u00fc\u00b7ben", "an", "der", "Ber\u00b7ges\u00b7hal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Dein W\u00f6lkchen Rauch", "tokens": ["Dein", "W\u00f6lk\u00b7chen", "Rauch"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Schwebt einsam nicht; aus meinem Tannenwalde", "tokens": ["Schwebt", "ein\u00b7sam", "nicht", ";", "aus", "mei\u00b7nem", "Tan\u00b7nen\u00b7wal\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKNEG", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steigt gleicher Hauch.", "tokens": ["Steigt", "glei\u00b7cher", "Hauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ob dort und hier zwei treue Herzen flammen,", "tokens": ["Ob", "dort", "und", "hier", "zwei", "treu\u00b7e", "Her\u00b7zen", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADV", "CARD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Getrennt durch Kluft und Strom/", "tokens": ["Ge\u00b7trennt", "durch", "Kluft", "und", "Strom", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Rauch, die beiden S\u00e4ulen, schmilzt zusammen", "tokens": ["Den", "Rauch", ",", "die", "bei\u00b7den", "S\u00e4u\u00b7len", ",", "schmilzt", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Himmelsdom.", "tokens": ["Ein", "Him\u00b7mels\u00b7dom", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Ferne hat ein Minnen uns beschieden,", "tokens": ["Die", "Fer\u00b7ne", "hat", "ein", "Min\u00b7nen", "uns", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das nicht genie\u00dft,", "tokens": ["Das", "nicht", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nur segnend gr\u00fc\u00dft/ und sanft zu Gottes Frieden", "tokens": ["Nur", "seg\u00b7nend", "gr\u00fc\u00dft", "/", "und", "sanft", "zu", "Got\u00b7tes", "Frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$(", "KON", "ADJD", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hin\u00fcberflie\u00dft.", "tokens": ["Hin\u00b7\u00fc\u00b7berf\u00b7lie\u00dft", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}