{"textgrid.poem.33396": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Hochw. Gro\u00dfm. von B*n", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In jener alten, gold'nen Ritterzeit,", "tokens": ["In", "je\u00b7ner", "al\u00b7ten", ",", "gold'\u00b7nen", "Rit\u00b7ter\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo jede That der Edeln lauters Gold,", "tokens": ["Wo", "je\u00b7de", "That", "der", "E\u00b7deln", "lau\u00b7ters", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gediegen jedes Wort, und wie im Feu'r", "tokens": ["Ge\u00b7die\u00b7gen", "je\u00b7des", "Wort", ",", "und", "wie", "im", "Feu'r"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "$,", "KON", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gepr\u00fcft der Ritter Brudertreue war,", "tokens": ["Ge\u00b7pr\u00fcft", "der", "Rit\u00b7ter", "Bru\u00b7der\u00b7treu\u00b7e", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da lebt' ein edler Ritter, gro\u00df an Geist,", "tokens": ["Da", "lebt'", "ein", "ed\u00b7ler", "Rit\u00b7ter", ",", "gro\u00df", "an", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch gr\u00f6sser noch an Herz; sein Wandel war", "tokens": ["Doch", "gr\u00f6s\u00b7ser", "noch", "an", "Herz", ";", "sein", "Wan\u00b7del", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "NN", "$.", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gerade wie sein Speer, und seine Redlichkeit", "tokens": ["Ge\u00b7ra\u00b7de", "wie", "sein", "Speer", ",", "und", "sei\u00b7ne", "Red\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So fest und undurchdringlich wie sein Schild.", "tokens": ["So", "fest", "und", "un\u00b7durch\u00b7dring\u00b7lich", "wie", "sein", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Ritter ohne Tadel war und hie\u00df", "tokens": ["Der", "Rit\u00b7ter", "oh\u00b7ne", "Ta\u00b7del", "war", "und", "hie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Er \u00fcberall, und dieser Name galt", "tokens": ["Er", "\u00fc\u00b7be\u00b7rall", ",", "und", "die\u00b7ser", "Na\u00b7me", "galt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "KON", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ihm mehr, als der, den ihm, von Ahnen schwer,", "tokens": ["Ihm", "mehr", ",", "als", "der", ",", "den", "ihm", ",", "von", "Ah\u00b7nen", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "ART", "$,", "PRELS", "PPER", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sein Vater hinterlie\u00df. Wohin er zog,", "tokens": ["Sein", "Va\u00b7ter", "hin\u00b7ter\u00b7lie\u00df", ".", "Wo\u00b7hin", "er", "zog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da schwand des Armen Noth, da wandelte", "tokens": ["Da", "schwand", "des", "Ar\u00b7men", "Noth", ",", "da", "wan\u00b7del\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sich der gedr\u00fcckten Unschuld Thr\u00e4nenblick", "tokens": ["Sich", "der", "ge\u00b7dr\u00fcck\u00b7ten", "Un\u00b7schuld", "Thr\u00e4\u00b7nen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "In L\u00e4cheln um, und wo er wegzog, scholl", "tokens": ["In", "L\u00e4\u00b7cheln", "um", ",", "und", "wo", "er", "weg\u00b7zog", ",", "scholl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "PTKVZ", "$,", "KON", "PWAV", "PPER", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Wittwen und der Waisen Dank ihm nach.", "tokens": ["Der", "Witt\u00b7wen", "und", "der", "Wai\u00b7sen", "Dank", "ihm", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Nie sah man Rittertugenden", "tokens": ["Nie", "sah", "man", "Rit\u00b7ter\u00b7tu\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.18": {"text": "In einem sch\u00f6nern Bund, und wer ihn sah,", "tokens": ["In", "ei\u00b7nem", "sch\u00f6\u00b7nern", "Bund", ",", "und", "wer", "ihn", "sah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Der zweifelt', ob der Ritterorden ihn", "tokens": ["Der", "zwei\u00b7felt'", ",", "ob", "der", "Rit\u00b7ter\u00b7or\u00b7den", "ihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Mehr ehre, oder er die Ritterschaft.", "tokens": ["Mehr", "eh\u00b7re", ",", "o\u00b7der", "er", "die", "Rit\u00b7ter\u00b7schaft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Der Name dieses Ritters, ob er gleich", "tokens": ["Der", "Na\u00b7me", "die\u00b7ses", "Rit\u00b7ters", ",", "ob", "er", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Das, was er Gutes that, so wenig als", "tokens": ["Das", ",", "was", "er", "Gu\u00b7tes", "that", ",", "so", "we\u00b7nig", "als"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PWS", "PPER", "NN", "VVFIN", "$,", "ADV", "PIS", "KOKOM"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Die R\u00fcstung, die er trug, in's Auge schimmern lie\u00df,", "tokens": ["Die", "R\u00fcs\u00b7tung", ",", "die", "er", "trug", ",", "in's", "Au\u00b7ge", "schim\u00b7mern", "lie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPRART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Verbreitete sich bald von Ost bis West,", "tokens": ["Ver\u00b7brei\u00b7te\u00b7te", "sich", "bald", "von", "Ost", "bis", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und zog der Edlen und der Grossen viel", "tokens": ["Und", "zog", "der", "Ed\u00b7len", "und", "der", "Gros\u00b7sen", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Aus allen Landen her zu diesem Mann,", "tokens": ["Aus", "al\u00b7len", "Lan\u00b7den", "her", "zu", "die\u00b7sem", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APZR", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Um unter ihm zu lernen Ritterdienst,", "tokens": ["Um", "un\u00b7ter", "ihm", "zu", "ler\u00b7nen", "Rit\u00b7ter\u00b7dienst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "PPER", "PTKZU", "VVINF", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Zu \u00fcben jede schwere Ritterpflicht,", "tokens": ["Zu", "\u00fc\u00b7ben", "je\u00b7de", "schwe\u00b7re", "Rit\u00b7ter\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und f\u00e4nd' er sie bew\u00e4hrt \u2013 von seiner Hand", "tokens": ["Und", "f\u00e4nd'", "er", "sie", "be\u00b7w\u00e4hrt", "\u2013", "von", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "VVPP", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Den Ritterschlag zum Lohne zu empfah'n.", "tokens": ["Den", "Rit\u00b7ter\u00b7schlag", "zum", "Loh\u00b7ne", "zu", "emp\u00b7fah'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Der Retterzug war einer Wallfahrt gleich;", "tokens": ["Der", "Ret\u00b7ter\u00b7zug", "war", "ei\u00b7ner", "Wall\u00b7fahrt", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Man wollte hin um seinen Ritterschlag,", "tokens": ["Man", "woll\u00b7te", "hin", "um", "sei\u00b7nen", "Rit\u00b7ter\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Wie in's gelobte Land, und mancher F\u00fcrst,", "tokens": ["Wie", "in's", "ge\u00b7lob\u00b7te", "Land", ",", "und", "man\u00b7cher", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Der um zw\u00f6lf Stufen \u2013 freilich nur von Holz \u2013", "tokens": ["Der", "um", "zw\u00f6lf", "Stu\u00b7fen", "\u2013", "frei\u00b7lich", "nur", "von", "Holz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "NN", "$(", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Sonst h\u00f6her war als and're Sterbliche,", "tokens": ["Sonst", "h\u00f6\u00b7her", "war", "als", "an\u00b7d'\u00b7re", "Sterb\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Der kniete willig vor den Ritter hin,", "tokens": ["Der", "knie\u00b7te", "wil\u00b7lig", "vor", "den", "Rit\u00b7ter", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Und f\u00fchlt' es in dem Augenblicke tief,", "tokens": ["Und", "f\u00fchlt'", "es", "in", "dem", "Au\u00b7gen\u00b7bli\u00b7cke", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Da\u00df, trotz dem Fu\u00dfgestell von Holz, der Mann,", "tokens": ["Da\u00df", ",", "trotz", "dem", "Fu\u00df\u00b7ge\u00b7stell", "von", "Holz", ",", "der", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "APPR", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Vor dem er auf den Knien lag, um mehr", "tokens": ["Vor", "dem", "er", "auf", "den", "Kni\u00b7en", "lag", ",", "um", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "KOUI", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Als einen Kopf doch gr\u00f6sser sei als er.", "tokens": ["Als", "ei\u00b7nen", "Kopf", "doch", "gr\u00f6s\u00b7ser", "sei", "als", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VAFIN", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "So sammelten der edlen Ritter viel", "tokens": ["So", "sam\u00b7mel\u00b7ten", "der", "ed\u00b7len", "Rit\u00b7ter", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "Sich um ihn her, ein auserles'ner Kreis", "tokens": ["Sich", "um", "ihn", "her", ",", "ein", "aus\u00b7er\u00b7les'\u00b7ner", "Kreis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPR", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Von edlen K\u00e4mpfern f\u00fcr der Menschheit Wohl,", "tokens": ["Von", "ed\u00b7len", "K\u00e4mp\u00b7fern", "f\u00fcr", "der", "Menschheit", "Wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.44": {"text": "Gro\u00df an sich selbst, doch gr\u00f6sser noch durch ihn:", "tokens": ["Gro\u00df", "an", "sich", "selbst", ",", "doch", "gr\u00f6s\u00b7ser", "noch", "durch", "ihn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "ADV", "$,", "ADV", "ADJD", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Und so entstand der sch\u00f6nste Ritterbund,", "tokens": ["Und", "so", "ent\u00b7stand", "der", "sch\u00f6ns\u00b7te", "Rit\u00b7ter\u00b7bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "So sch\u00f6n als Artus selber keinen sah,", "tokens": ["So", "sch\u00f6n", "als", "Ar\u00b7tus", "sel\u00b7ber", "kei\u00b7nen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NE", "ADV", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Ein Bund, um den ihr unaufl\u00f6sbar Band", "tokens": ["Ein", "Bund", ",", "um", "den", "ihr", "un\u00b7auf\u00b7l\u00f6s\u00b7bar", "Band"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Die Liebe schlang, nicht l\u00f6sbar selbst dem Tod.", "tokens": ["Die", "Lie\u00b7be", "schlang", ",", "nicht", "l\u00f6s\u00b7bar", "selbst", "dem", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", 