{"textgrid.poem.59334": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Erwartung", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sch\u00f6ne, bunte V\u00f6gel,", "tokens": ["O", "sch\u00f6\u00b7ne", ",", "bun\u00b7te", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie singt ihr gar so hell!", "tokens": ["Wie", "singt", "ihr", "gar", "so", "hell", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Wolken, luft'ge Segel,", "tokens": ["O", "Wol\u00b7ken", ",", "luft'\u00b7ge", "Se\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohin so schnell, so schnell?", "tokens": ["Wo\u00b7hin", "so", "schnell", ",", "so", "schnell", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr alle, ach, gemeinsam", "tokens": ["Ihr", "al\u00b7le", ",", "ach", ",", "ge\u00b7mein\u00b7sam"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "PIS", "$,", "ITJ", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fliegt zu der Liebsten hin,", "tokens": ["Fliegt", "zu", "der", "Liebs\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sagt ihr, wie ich hier einsam", "tokens": ["Sagt", "ihr", ",", "wie", "ich", "hier", "ein\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und voller Sorgen bin.", "tokens": ["Und", "vol\u00b7ler", "Sor\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im Walde steh und laur ich,", "tokens": ["Im", "Wal\u00b7de", "steh", "und", "laur", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verhallt ist jeder Laut,", "tokens": ["Ver\u00b7hallt", "ist", "je\u00b7der", "Laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wipfel nur wehn schaurig,", "tokens": ["Die", "Wip\u00b7fel", "nur", "wehn", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O komm, du s\u00fc\u00dfe Braut!", "tokens": ["O", "komm", ",", "du", "s\u00fc\u00b7\u00dfe", "Braut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schon sinkt die dunkelfeuchte", "tokens": ["Schon", "sinkt", "die", "dun\u00b7kel\u00b7feuch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nacht rings auf Wald und Feld,", "tokens": ["Nacht", "rings", "auf", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Des Mondes hohe Leuchte", "tokens": ["Des", "Mon\u00b7des", "ho\u00b7he", "Leuch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tritt in die stille Welt.", "tokens": ["Tritt", "in", "die", "stil\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie schauert nun im Grunde", "tokens": ["Wie", "schau\u00b7ert", "nun", "im", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der tiefsten Seele mich!", "tokens": ["Der", "tiefs\u00b7ten", "See\u00b7le", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie \u00f6de ist die Runde", "tokens": ["Wie", "\u00f6\u00b7de", "ist", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und einsam ohne dich!", "tokens": ["Und", "ein\u00b7sam", "oh\u00b7ne", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was rauscht? \u2013 Sie naht von ferne! \u2013", "tokens": ["Was", "rauscht", "?", "\u2013", "Sie", "naht", "von", "fer\u00b7ne", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "$.", "$(", "PPER", "VVFIN", "APPR", "ADJA", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun, Wald, rausch von den H\u00f6hn,", "tokens": ["Nun", ",", "Wald", ",", "rausch", "von", "den", "H\u00f6hn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun la\u00df Mond, Nacht und Sterne", "tokens": ["Nun", "la\u00df", "Mond", ",", "Nacht", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur auf- und untergehn!", "tokens": ["Nur", "auf", "und", "un\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}