{"textgrid.poem.47485": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ Lieblich in der Rosenzeit]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieblich in der Rosenzeit", "tokens": ["Lieb\u00b7lich", "in", "der", "Ro\u00b7sen\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt die Liebe Schule;", "tokens": ["H\u00e4lt", "die", "Lie\u00b7be", "Schu\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Es docirt die Nachtigall", "tokens": ["Es", "do\u00b7cirt", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom Doktorenstuhle.", "tokens": ["Vom", "Dok\u00b7to\u00b7ren\u00b7stuh\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ihrem Worte horcht Hafis", "tokens": ["Ih\u00b7rem", "Wor\u00b7te", "horcht", "Ha\u00b7fis"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Mit Scholarentreue,", "tokens": ["Mit", "Scho\u00b7la\u00b7ren\u00b7treu\u00b7e", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fr\u00fch und spat diktirt sie ihm", "tokens": ["Fr\u00fch", "und", "spat", "dik\u00b7tirt", "sie", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADV", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Federspule.", "tokens": ["In", "die", "Fe\u00b7der\u00b7spu\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Meine Meinung, k\u00f6nnte sie", "tokens": ["Mei\u00b7ne", "Mei\u00b7nung", ",", "k\u00f6nn\u00b7te", "sie"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl verborgen bleiben?", "tokens": ["Wohl", "ver\u00b7bor\u00b7gen", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wein an einem sch\u00f6nen Ort,", "tokens": ["Wein", "an", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine zarte Buhle!", "tokens": ["Ei\u00b7ne", "zar\u00b7te", "Buh\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ohne diesen \u00c4chtgewinn", "tokens": ["Oh\u00b7ne", "die\u00b7sen", "\u00c4cht\u00b7ge\u00b7winn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind die Weltgebiete", "tokens": ["Sind", "die", "Welt\u00b7ge\u00b7bie\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00fcsteneien, angef\u00fcllt", "tokens": ["W\u00fcs\u00b7te\u00b7nei\u00b7en", ",", "an\u00b7ge\u00b7f\u00fcllt"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom Geschrei der Ghule.", "tokens": ["Vom", "Ge\u00b7schrei", "der", "Ghu\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Darum hei\u00df zum Himmel auf", "tokens": ["Da\u00b7rum", "hei\u00df", "zum", "Him\u00b7mel", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "APPRART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fleh' ich im Gebete:", "tokens": ["Fleh'", "ich", "im", "Ge\u00b7be\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nie, o nie errette mich", "tokens": ["Nie", ",", "o", "nie", "er\u00b7ret\u00b7te", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem S\u00fcndenpfuhle!", "tokens": ["Aus", "dem", "S\u00fcn\u00b7den\u00b7pfuh\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}