{"dta.poem.5459": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "GoTT allein die Ehre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Giebt unser GOTT in allen Dingen,", "tokens": ["Giebt", "un\u00b7ser", "GoTT", "in", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wie er w\u00fcrcklich thut, das Wollen und Voll-", "tokens": ["So", "wie", "er", "w\u00fcrck\u00b7lich", "thut", ",", "das", "Wol\u00b7len", "und", "Voll"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "ADJD", "VVFIN", "$,", "ART", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Was r\u00fchmt sich denn der Mensch, auch in den besten", "tokens": ["Was", "r\u00fchmt", "sich", "denn", "der", "Mensch", ",", "auch", "in", "den", "bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "ART", "NN", "$,", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da sie ja nicht durch ihn, durch GOtt allein gerahten.", "tokens": ["Da", "sie", "ja", "nicht", "durch", "ihn", ",", "durch", "Gott", "al\u00b7lein", "ge\u00b7rah\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "APPR", "PPER", "$,", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nichts ist mein;", "tokens": ["Nichts", "ist", "mein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Alles dein;", "tokens": ["Al\u00b7les", "dein", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "PPOSAT", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dir allein", "tokens": ["Dir", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "HeRR, soll Lob' und Ehre seyn!", "tokens": ["HeRR", ",", "soll", "Lob'", "und", "Eh\u00b7re", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "NN", "KON", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}