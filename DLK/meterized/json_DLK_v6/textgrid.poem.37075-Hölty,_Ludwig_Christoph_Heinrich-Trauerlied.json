{"textgrid.poem.37075": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauerlied", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.57", "fr:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du blaues Aug, du Quelle meiner Freuden,", "tokens": ["Du", "blau\u00b7es", "Aug", ",", "du", "Quel\u00b7le", "mei\u00b7ner", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann lachst du mir,", "tokens": ["Wann", "lachst", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wann find ich, wann, nach tausend Seelenleiden,", "tokens": ["Wann", "find", "ich", ",", "wann", ",", "nach", "tau\u00b7send", "See\u00b7len\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PWAV", "$,", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Ruh in dir?", "tokens": ["Die", "Ruh", "in", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wirst du mir nicht die Wonne wiedergeben,", "tokens": ["Wirst", "du", "mir", "nicht", "die", "Won\u00b7ne", "wie\u00b7der\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mir entfloh,", "tokens": ["Die", "mir", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So werd ich nie, in diesem Pilgerleben,", "tokens": ["So", "werd", "ich", "nie", ",", "in", "die\u00b7sem", "Pil\u00b7ger\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Daseyns froh.", "tokens": ["Des", "Da\u00b7seyns", "froh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "So werd ich bald, ach! bald verwelken m\u00fc\u00dfen,", "tokens": ["So", "werd", "ich", "bald", ",", "ach", "!", "bald", "ver\u00b7wel\u00b7ken", "m\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ITJ", "$.", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Bl\u00fcthenblatt,", "tokens": ["Ein", "Bl\u00fc\u00b7then\u00b7blatt", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das Boreas, dem Rosenstock' entri\u00dfen,", "tokens": ["Das", "Bo\u00b7re\u00b7as", ",", "dem", "Ro\u00b7sen\u00b7stock'", "ent\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verwehet hat.", "tokens": ["Ver\u00b7we\u00b7het", "hat", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "O l\u00e4chle mir, worin das Bild des Himmels", "tokens": ["O", "l\u00e4ch\u00b7le", "mir", ",", "wo\u00b7rin", "das", "Bild", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So hell sich zeigt,", "tokens": ["So", "hell", "sich", "zeigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bevor mein Geist, satt dieses Erdgewimmels,", "tokens": ["Be\u00b7vor", "mein", "Geist", ",", "satt", "die\u00b7ses", "Erd\u00b7ge\u00b7wim\u00b7mels", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUI", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu Gott entfleugt.", "tokens": ["Zu", "Gott", "ent\u00b7fleugt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}