{"textgrid.poem.25660": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Als die Studenten einem mittelm\u00e4\u00dfigen Tonk\u00fcnstler die Fenster einwarfen", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum wenigsten hat unser Mann", "tokens": ["Zum", "we\u00b7nigs\u00b7ten", "hat", "un\u00b7ser", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "PIS", "VAFIN", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mit Orpheus doch der Aehnlichkeiten Eine:", "tokens": ["Mit", "Or\u00b7pheus", "doch", "der", "A\u00b7ehn\u00b7lich\u00b7kei\u00b7ten", "Ei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "ART", "NN", "ART", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df er, wie dieser, auch die Steine", "tokens": ["Da\u00df", "er", ",", "wie", "die\u00b7ser", ",", "auch", "die", "Stei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PDS", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Selbst in Bewegung setzen kann.", "tokens": ["Selbst", "in", "Be\u00b7we\u00b7gung", "set\u00b7zen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}