{"textgrid.poem.41558": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am neunten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O h\u00fctet, h\u00fctet euch!", "tokens": ["O", "h\u00fc\u00b7tet", ",", "h\u00fc\u00b7tet", "euch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Luft hat sich umzogen", "tokens": ["Die", "Luft", "hat", "sich", "um\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVIZU"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Und in den Wolken grell und reich", "tokens": ["Und", "in", "den", "Wol\u00b7ken", "grell", "und", "reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hebt sich ein falscher Friedensbogen,", "tokens": ["Hebt", "sich", "ein", "fal\u00b7scher", "Frie\u00b7dens\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von dem ein D\u00e4mon niederstieg,", "tokens": ["Von", "dem", "ein", "D\u00e4\u00b7mon", "nie\u00b7ders\u00b7tieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der mit dem \u00d6lzweig bringt den Krieg.", "tokens": ["Der", "mit", "dem", "\u00d6l\u00b7zweig", "bringt", "den", "Krieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und allerorten stehn", "tokens": ["Und", "al\u00b7le\u00b7ror\u00b7ten", "stehn"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Posaunende Propheten,", "tokens": ["Po\u00b7sau\u00b7nen\u00b7de", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "So aus dem Staube Stricke drehn,", "tokens": ["So", "aus", "dem", "Stau\u00b7be", "Stri\u00b7cke", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So flach die Berge wollen treten.", "tokens": ["So", "flach", "die", "Ber\u00b7ge", "wol\u00b7len", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O h\u00fcte dich, ehrw\u00fcrd'ger Art", "tokens": ["O", "h\u00fc\u00b7te", "dich", ",", "ehr\u00b7w\u00fcrd'\u00b7ger", "Art"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Ist ihr Gesicht, und grau ihr Bart!", "tokens": ["Ist", "ihr", "Ge\u00b7sicht", ",", "und", "grau", "ihr", "Bart", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Der eine zeigt den Ri\u00df,", "tokens": ["Der", "ei\u00b7ne", "zeigt", "den", "Ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo soll auf nackten H\u00f6hen", "tokens": ["Wo", "soll", "auf", "nack\u00b7ten", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die g\u00f6ttliche Akropolis", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Ak\u00b7ro\u00b7po\u00b7lis"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der christlichen Minerva stehen:", "tokens": ["Der", "christ\u00b7li\u00b7chen", "Mi\u00b7ner\u00b7va", "ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Folgst du ihm nach, du bleibst gebannt", "tokens": ["Folgst", "du", "ihm", "nach", ",", "du", "bleibst", "ge\u00b7bannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo noch kein H\u00e4lmchen Nahrung fand.", "tokens": ["Wo", "noch", "kein", "H\u00e4lm\u00b7chen", "Nah\u00b7rung", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da magst vor \u00f6dem Stein", "tokens": ["Da", "magst", "vor", "\u00f6\u00b7dem", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du betend niedersinken,", "tokens": ["Du", "be\u00b7tend", "nie\u00b7der\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lange noch wird dein Gebein", "tokens": ["Und", "lan\u00b7ge", "noch", "wird", "dein", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein warnend Beispiel niederblinken,", "tokens": ["Ein", "war\u00b7nend", "Bei\u00b7spiel", "nie\u00b7der\u00b7blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als eines, der zu eigner Not", "tokens": ["Als", "ei\u00b7nes", ",", "der", "zu", "eig\u00b7ner", "Not"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verwandelte in Stein das Brod.", "tokens": ["Ver\u00b7wan\u00b7del\u00b7te", "in", "Stein", "das", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der andre deutet tief", "tokens": ["Der", "and\u00b7re", "deu\u00b7tet", "tief"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nach einer H\u00f6hle Gr\u00fcnden", "tokens": ["Nach", "ei\u00b7ner", "H\u00f6h\u00b7le", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und horcht in seinem Wahn, als rief", "tokens": ["Und", "horcht", "in", "sei\u00b7nem", "Wahn", ",", "als", "rief"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm eine Stimme aus den Schl\u00fcnden:", "tokens": ["Ihm", "ei\u00b7ne", "Stim\u00b7me", "aus", "den", "Schl\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hieher! was klar das ist ein Schein,", "tokens": ["Hie\u00b7her", "!", "was", "klar", "das", "ist", "ein", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "ADJD", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Schachte wohnt der Edelstein!", "tokens": ["Im", "Schach\u00b7te", "wohnt", "der", "E\u00b7del\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O diesem folge nicht,", "tokens": ["O", "die\u00b7sem", "fol\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Gottes Haus zum Schreine,", "tokens": ["Der", "Got\u00b7tes", "Haus", "zum", "Schrei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wehe, jenem folge nicht,", "tokens": ["Und", "we\u00b7he", ",", "je\u00b7nem", "fol\u00b7ge", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PDAT", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gottes Nahrung macht zum Steine!", "tokens": ["Der", "Got\u00b7tes", "Nah\u00b7rung", "macht", "zum", "Stei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch besser dumpf im Schachte stehn", "tokens": ["Doch", "bes\u00b7ser", "dumpf", "im", "Schach\u00b7te", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als droben frech gen Himmel sehn!", "tokens": ["Als", "dro\u00b7ben", "frech", "gen", "Him\u00b7mel", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und auf dem gr\u00fcnen Plan,", "tokens": ["Und", "auf", "dem", "gr\u00fc\u00b7nen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo frisch die Kr\u00e4uter schwellen,", "tokens": ["Wo", "frisch", "die", "Kr\u00e4u\u00b7ter", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da liegt so hellbetaut die Bahn,", "tokens": ["Da", "liegt", "so", "hell\u00b7be\u00b7taut", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sprudeln die lebend'gen Quellen,", "tokens": ["Da", "spru\u00b7deln", "die", "le\u00b7ben\u00b7d'\u00b7gen", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und aus der Demut grauem Stein", "tokens": ["Und", "aus", "der", "De\u00b7mut", "grau\u00b7em", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hebt sich ein Tempel schlicht und klein.", "tokens": ["Hebt", "sich", "ein", "Tem\u00b7pel", "schlicht", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dort findest du ein Mahl", "tokens": ["Dort", "fin\u00b7dest", "du", "ein", "Mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So ganz f\u00fcr dein Bed\u00fcrfen,", "tokens": ["So", "ganz", "f\u00fcr", "dein", "Be\u00b7d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort darfst du aus dem heil'gen Gral", "tokens": ["Dort", "darfst", "du", "aus", "dem", "heil'\u00b7gen", "Gral"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Glaubens milde Labung schl\u00fcrfen,", "tokens": ["Des", "Glau\u00b7bens", "mil\u00b7de", "La\u00b7bung", "schl\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So wie sie einem Wesen recht,", "tokens": ["So", "wie", "sie", "ei\u00b7nem", "We\u00b7sen", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das noch des ird'schen Leibes Knecht.", "tokens": ["Das", "noch", "des", "ird'\u00b7schen", "Lei\u00b7bes", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O hemme nur dein Ohr,", "tokens": ["O", "hem\u00b7me", "nur", "dein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vom fremden Klang umzogen!", "tokens": ["Vom", "frem\u00b7den", "Klang", "um\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O blicke l\u00fcstern nicht empor", "tokens": ["O", "bli\u00b7cke", "l\u00fcs\u00b7tern", "nicht", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "PTKNEG", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum bunten falschen Friedensbogen!", "tokens": ["Zum", "bun\u00b7ten", "fal\u00b7schen", "Frie\u00b7dens\u00b7bo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An deinem Tempel sollst du knien,", "tokens": ["An", "dei\u00b7nem", "Tem\u00b7pel", "sollst", "du", "kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Wetter wird vor\u00fcberziehn.", "tokens": ["Das", "Wet\u00b7ter", "wird", "vor\u00b7\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}