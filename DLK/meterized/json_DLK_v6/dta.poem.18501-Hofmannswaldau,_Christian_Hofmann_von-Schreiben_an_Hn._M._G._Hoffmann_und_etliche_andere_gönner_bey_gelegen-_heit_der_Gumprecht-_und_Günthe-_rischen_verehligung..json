{"dta.poem.18501": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Schreiben an Hn.  M.  G. Hoffmann  \n und etliche andere g\u00f6nner bey gelegen-  \n heit der Gumprecht- und G\u00fcnthe-  \n rischen verehligung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "P. S. Es ist sehr wenig platz und Peter Sqventz der knecht", "tokens": ["P.", "S.", "Es", "ist", "sehr", "we\u00b7nig", "platz", "und", "Pe\u00b7ter", "Sqventz", "der", "knecht"], "token_info": ["abbreviation", "abbreviation", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "PPER", "VAFIN", "ADV", "PIAT", "NN", "KON", "NE", "NE", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf unser post eilt fort: sonst h\u00e4tt ich vom vergn\u00fcgen", "tokens": ["Auf", "un\u00b7ser", "post", "eilt", "fort", ":", "sonst", "h\u00e4tt", "ich", "vom", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der braut herrn vater noch viel w\u00fcntsche beyzuf\u00fcgen/", "tokens": ["Der", "braut", "herrn", "va\u00b7ter", "noch", "viel", "w\u00fcnt\u00b7sche", "bey\u00b7zu\u00b7f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch kurtz: Den jahren nach sey er und sein geschlecht", "tokens": ["Doch", "kurtz", ":", "Den", "jah\u00b7ren", "nach", "sey", "er", "und", "sein", "ge\u00b7schlecht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "ART", "NN", "APPR", "VAFIN", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dem Consul gleich/ den nechst Venedig eingeb\u00fcst/", "tokens": ["Dem", "Con\u00b7sul", "gleich", "/", "den", "nechst", "Ve\u00b7ne\u00b7dig", "ein\u00b7ge\u00b7b\u00fcst", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "ART", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der selber achtzehn jahr im andern hundert zehlte/", "tokens": ["Der", "sel\u00b7ber", "acht\u00b7zehn", "jahr", "im", "an\u00b7dern", "hun\u00b7dert", "zehl\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "CARD", "NN", "APPRART", "ADJA", "CARD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und dem es \u00fcberdi\u00df nicht an zwey s\u00f6hnen fehlte/", "tokens": ["Und", "dem", "es", "\u00fc\u00b7ber\u00b7di\u00df", "nicht", "an", "zwey", "s\u00f6h\u00b7nen", "fehl\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "PTKNEG", "APPR", "CARD", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Davon der j\u00fcngere schon vier und achtzig ist.", "tokens": ["Da\u00b7von", "der", "j\u00fcn\u00b7ge\u00b7re", "schon", "vier", "und", "acht\u00b7zig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "ADV", "CARD", "KON", "CARD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}