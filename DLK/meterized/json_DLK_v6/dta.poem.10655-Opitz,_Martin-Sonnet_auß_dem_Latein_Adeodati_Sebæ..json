{"dta.poem.10655": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet   au\u00df dem Latein   Adeodati Seb\u00e6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.85", "nl:0.14"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Heint als der Monde war in seinen Crai\u00df gezogen/", "tokens": ["Heint", "als", "der", "Mon\u00b7de", "war", "in", "sei\u00b7nen", "Crai\u00df", "ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NE", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd mich der s\u00fcsse Schlaf vmbfangen durch die Nacht/", "tokens": ["Vnd", "mich", "der", "s\u00fcs\u00b7se", "Schlaf", "vmb\u00b7fan\u00b7gen", "durch", "die", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ward mir mein Augentrost im Traume f\u00fcrgebracht/", "tokens": ["Ward", "mir", "mein", "Au\u00b7gen\u00b7trost", "im", "Trau\u00b7me", "f\u00fcr\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als lege sie bey mir an meine Brust gebogen/", "tokens": ["Als", "le\u00b7ge", "sie", "bey", "mir", "an", "mei\u00b7ne", "Brust", "ge\u00b7bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ihr Hertze war in mich/ mein Hertz\u2019 in sie geflogen/", "tokens": ["Ihr", "Hert\u00b7ze", "war", "in", "mich", "/", "mein", "Hertz'", "in", "sie", "ge\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAFIN", "APPR", "PPER", "$(", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Fand aber g\u00e4ntzlich nichts/ wie ich de\u00df Morgens wacht/", "tokens": ["Fand", "a\u00b7ber", "g\u00e4ntz\u00b7lich", "nichts", "/", "wie", "ich", "de\u00df", "Mor\u00b7gens", "wacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PIS", "$(", "PWAV", "PPER", "ART", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vnd hielt den Locken in den Armen/ drumb ich lacht/", "tokens": ["Vnd", "hielt", "den", "Lo\u00b7cken", "in", "den", "Ar\u00b7men", "/", "drumb", "ich", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$(", "PAV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als ich recht jnnen ward da\u00df ich so sehr betrogen.", "tokens": ["Als", "ich", "recht", "jn\u00b7nen", "ward", "da\u00df", "ich", "so", "sehr", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verr\u00e4ther/ loser Trawm/ warumb den fleuchstu bald/", "tokens": ["Ver\u00b7r\u00e4\u00b7ther", "/", "lo\u00b7ser", "Trawm", "/", "wa\u00b7rumb", "den", "fleuchs\u00b7tu", "bald", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$(", "PWAV", "ART", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "La\u00df mich doch l\u00e4nger sehen die liebliche Gestalt/", "tokens": ["La\u00df", "mich", "doch", "l\u00e4n\u00b7ger", "se\u00b7hen", "die", "lieb\u00b7li\u00b7che", "Ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "La\u00df sich doch mehr bey mir di\u00df sch\u00f6ne Vorbild s\u00e4umen.", "tokens": ["La\u00df", "sich", "doch", "mehr", "bey", "mir", "di\u00df", "sch\u00f6\u00b7ne", "Vor\u00b7bild", "s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PRF", "ADV", "ADV", "APPR", "PPER", "PDS", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Betrieger/ krieg ich nichts als Hohn vnd Spott von jhr/", "tokens": ["Be\u00b7trie\u00b7ger", "/", "krieg", "ich", "nichts", "als", "Hohn", "vnd", "Spott", "von", "jhr", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "KON", "NN", "APPR", "PPOSAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vnd jhrer Sch\u00f6nheit ro\u00df\u2019/ ach bitt ich/ la\u00df doch mir", "tokens": ["Vnd", "jhrer", "Sch\u00f6n\u00b7heit", "ro\u00df'", "/", "ach", "bitt", "ich", "/", "la\u00df", "doch", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NE", "$(", "XY", "VVFIN", "PPER", "$(", "VVIMP", "ADV", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "Drey tansent Jahr so s\u00fc\u00df/ ohn alles Wachen/ tr\u00e4umen.", "tokens": ["Drey", "tan\u00b7sent", "Jahr", "so", "s\u00fc\u00df", "/", "ohn", "al\u00b7les", "Wa\u00b7chen", "/", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "CARD", "NN", "ADV", "ADJD", "$(", "APPR", "PIAT", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}