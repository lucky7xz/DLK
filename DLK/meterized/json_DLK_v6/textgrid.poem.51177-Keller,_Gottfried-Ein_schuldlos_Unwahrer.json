{"textgrid.poem.51177": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ein schuldlos Unwahrer", "genre": "verse", "period": "N.A.", "pub_year": 1883, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Launig erlog die Natur und bemalte den stattlichen Golem,", "tokens": ["Lau\u00b7nig", "er\u00b7log", "die", "Na\u00b7tur", "und", "be\u00b7mal\u00b7te", "den", "statt\u00b7li\u00b7chen", "Go\u00b7lem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Dann, auf sich selber gestellt, log das Gebilde sich durch;", "tokens": ["Dann", ",", "auf", "sich", "sel\u00b7ber", "ge\u00b7stellt", ",", "log", "das", "Ge\u00b7bil\u00b7de", "sich", "durch", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PRF", "ADV", "VVPP", "$,", "VVFIN", "ART", "NN", "PRF", "APPR", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was es ber\u00fchrt, wird unwahr, Gold zu glei\u00dfendem Tombak,", "tokens": ["Was", "es", "be\u00b7r\u00fchrt", ",", "wird", "un\u00b7wahr", ",", "Gold", "zu", "glei\u00b7\u00dfen\u00b7dem", "Tom\u00b7bak", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "ADJD", "$,", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Kl\u00e4glich im festlichen Krug macht es zu Wasser den Wein!", "tokens": ["Kl\u00e4g\u00b7lich", "im", "fest\u00b7li\u00b7chen", "Krug", "macht", "es", "zu", "Was\u00b7ser", "den", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "M\u00f6chte Natura naturans mit solchem Betrieb uns verschonen,", "tokens": ["M\u00f6ch\u00b7te", "Na\u00b7tu\u00b7ra", "na\u00b7tu\u00b7rans", "mit", "sol\u00b7chem", "Be\u00b7trieb", "uns", "ver\u00b7scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "NE", "APPR", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.6": {"text": "Laufen ja mehr als genug wirkliche Schelme herum!", "tokens": ["Lau\u00b7fen", "ja", "mehr", "als", "ge\u00b7nug", "wirk\u00b7li\u00b7che", "Schel\u00b7me", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "KOKOM", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "+---+-++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}