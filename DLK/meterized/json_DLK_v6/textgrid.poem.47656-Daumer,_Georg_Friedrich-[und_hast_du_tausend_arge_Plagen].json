{"textgrid.poem.47656": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[und hast du tausend arge Plagen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und hast du tausend arge Plagen,", "tokens": ["Und", "hast", "du", "tau\u00b7send", "ar\u00b7ge", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und tausend Fehle zu verklagen,", "tokens": ["Und", "tau\u00b7send", "Feh\u00b7le", "zu", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einer Blume nur zu schlagen", "tokens": ["Mit", "ei\u00b7ner", "Blu\u00b7me", "nur", "zu", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Frauenbild \u2013 nicht sollst du wagen.", "tokens": ["Ein", "Frau\u00b7en\u00b7bild", "\u2013", "nicht", "sollst", "du", "wa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PTKNEG", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}