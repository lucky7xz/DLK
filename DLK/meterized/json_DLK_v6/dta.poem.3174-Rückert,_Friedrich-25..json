{"dta.poem.3174": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vergeblich alles, was du f\u00fcr die Welt gebildet,", "tokens": ["Ver\u00b7geb\u00b7lich", "al\u00b7les", ",", "was", "du", "f\u00fcr", "die", "Welt", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "$,", "PWS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat es dich selber nicht geschmeidigt und entwildet.", "tokens": ["Hat", "es", "dich", "sel\u00b7ber", "nicht", "ge\u00b7schmei\u00b7digt", "und", "ent\u00b7wil\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "PTKNEG", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erst mu\u00df dich das Gef\u00fchl der eignen Bildung laben,", "tokens": ["Erst", "mu\u00df", "dich", "das", "Ge\u00b7f\u00fchl", "der", "eig\u00b7nen", "Bil\u00b7dung", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann mag es dich erfreu'n, die Welt geschm\u00fcckt zu haben.", "tokens": ["Dann", "mag", "es", "dich", "er\u00b7freu'n", ",", "die", "Welt", "ge\u00b7schm\u00fcckt", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER",