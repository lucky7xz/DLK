{"dta.poem.10160": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Viehische Unempfindlichkeit  \n der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Ochs', ob er gleich sinnlich ist,", "tokens": ["Ein", "Ochs'", ",", "ob", "er", "gleich", "sinn\u00b7lich", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kann doch, wann er sein Futter frisst,", "tokens": ["Kann", "doch", ",", "wann", "er", "sein", "Fut\u00b7ter", "frisst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht, im ", "tokens": ["Nicht", ",", "im"], "token_info": ["word", "punct", "word"], "pos": ["PTKNEG", "$,", "APPRART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Ein Mensch den GOttes Hauch", "tokens": ["Ein", "Mensch", "den", "Got\u00b7tes", "Hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gew\u00fcrdigt, ihm dazu die F\u00e4higkeit zu schencken,", "tokens": ["Ge\u00b7w\u00fcr\u00b7digt", ",", "ihm", "da\u00b7zu", "die", "F\u00e4\u00b7hig\u00b7keit", "zu", "schen\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "PAV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sollt es im ", "tokens": ["Sollt", "es", "im"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ein Pferd, vermag es gleich zu ", "tokens": ["Ein", "Pferd", ",", "ver\u00b7mag", "es", "gleich", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kan doch, bey s\u00fcsser V\u00f6gel Ch\u00f6ren", "tokens": ["Kan", "doch", ",", "bey", "s\u00fcs\u00b7ser", "V\u00f6\u00b7gel", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "$,", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lieblichster Musie, am Sch\u00f6pfer nicht gedencken.", "tokens": ["Und", "lieb\u00b7lichs\u00b7ter", "Mu\u00b7sie", ",", "am", "Sch\u00f6p\u00b7fer", "nicht", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Mensch, den selbst des Sch\u00f6pfers Hauch", "tokens": ["Ein", "Mensch", ",", "den", "selbst", "des", "Sch\u00f6p\u00b7fers", "Hauch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gew\u00fcrdigt, ihm dazu die F\u00e4higkeit zu schencken,", "tokens": ["Ge\u00b7w\u00fcr\u00b7digt", ",", "ihm", "da\u00b7zu", "die", "F\u00e4\u00b7hig\u00b7keit", "zu", "schen\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "PAV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sollt es im ", "tokens": ["Sollt", "es", "im"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Ein Hund, ob er gleich ", "tokens": ["Ein", "Hund", ",", "ob", "er", "gleich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Riecht ihm gleich etwas lieblichs an;", "tokens": ["Riecht", "ihm", "gleich", "et\u00b7was", "lieb\u00b7lichs", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann er doch, bey der Lust, nicht an den Ursprung dencken.", "tokens": ["Kann", "er", "doch", ",", "bey", "der", "Lust", ",", "nicht", "an", "den", "Ur\u00b7sprung", "den\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "APPR", "ART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Mensch, den GOttes Hauch", "tokens": ["Ein", "Mensch", ",", "den", "Got\u00b7tes", "Hauch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gew\u00fcrdigt, ihm dazu die F\u00e4higkeit zu schencken,", "tokens": ["Ge\u00b7w\u00fcr\u00b7digt", ",", "ihm", "da\u00b7zu", "die", "F\u00e4\u00b7hig\u00b7keit", "zu", "schen\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "PAV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sollt es im ", "tokens": ["Sollt", "es", "im"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Ein Schwein zeigt sein ", "tokens": ["Ein", "Schwein", "zeigt", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mit halb geschlossnem Aug, sanft gruntzend reibt; allein", "tokens": ["Mit", "halb", "ge\u00b7schloss\u00b7nem", "Aug", ",", "sanft", "grunt\u00b7zend", "reibt", ";", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "ADJD", "ADJD", "VVFIN", "$.", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es kann, bey seiner Lust, nicht weiter dencken.", "tokens": ["Es", "kann", ",", "bey", "sei\u00b7ner", "Lust", ",", "nicht", "wei\u00b7ter", "den\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Mensch dazu formirt, durch GOttes Hauch,", "tokens": ["Ein", "Mensch", "da\u00b7zu", "for\u00b7mirt", ",", "durch", "Got\u00b7tes", "Hauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sollt im ", "tokens": ["Sollt", "im"], "token_info": ["word", "word"], "pos": ["VMFIN", "APPRART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Es kann ein iedes Thier, wie Wunder-sch\u00f6n", "tokens": ["Es", "kann", "ein", "ie\u00b7des", "Thier", ",", "wie", "Wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "PIAT", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Sonnen heller Strahl die C\u00f6rper f\u00e4rbet, ", "tokens": ["Der", "Son\u00b7nen", "hel\u00b7ler", "Strahl", "die", "C\u00f6r\u00b7per", "f\u00e4r\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jedoch kann es dabey am Sch\u00f6pfer nicht gedencken.", "tokens": ["Je\u00b7doch", "kann", "es", "da\u00b7bey", "am", "Sch\u00f6p\u00b7fer", "nicht", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PAV", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ein Mensch, den GOttes Hauch", "tokens": ["Ein", "Mensch", ",", "den", "Got\u00b7tes", "Hauch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gew\u00fcrdigt, ihm dazu die F\u00e4higkeit zu schencken,", "tokens": ["Ge\u00b7w\u00fcr\u00b7digt", ",", "ihm", "da\u00b7zu", "die", "F\u00e4\u00b7hig\u00b7keit", "zu", "schen\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "PAV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sollt es im ", "tokens": ["Sollt", "es", "im"], "token_info": ["word", "word", "word"], "pos": ["VMFIN