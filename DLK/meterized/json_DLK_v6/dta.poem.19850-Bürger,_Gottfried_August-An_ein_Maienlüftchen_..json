{"dta.poem.19850": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An  \n ein Maienl\u00fcftchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Auf, Maienl\u00fcftchen, aus den Blumenbe-               ", "tokens": ["Auf", ",", "Mai\u00b7en\u00b7l\u00fcft\u00b7chen", ",", "aus", "den", "Blu\u00b7men\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ten!", "tokens": ["ten", "!"], "token_info": ["word", "punct"], "pos": ["FM", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wo deine K\u00fcsse Florens T\u00f6chter r\u00f6ten;", "tokens": ["Wo", "dei\u00b7ne", "K\u00fcs\u00b7se", "Flo\u00b7rens", "T\u00f6ch\u00b7ter", "r\u00f6\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo du so liebetraulich allen heuchelst,", "tokens": ["Wo", "du", "so", "lie\u00b7be\u00b7trau\u00b7lich", "al\u00b7len", "heu\u00b7chelst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "PIAT", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Duft entschmeichelst.", "tokens": ["Und", "Duft", "ent\u00b7schmei\u00b7chelst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Erhebe dich, mit allem s\u00fcssen Raube,", "tokens": ["Er\u00b7he\u00b7be", "dich", ",", "mit", "al\u00b7lem", "s\u00fcs\u00b7sen", "Rau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach jener d\u00e4mmernden Holunderlaube!", "tokens": ["Nach", "je\u00b7ner", "d\u00e4m\u00b7mern\u00b7den", "Ho\u00b7lun\u00b7der\u00b7lau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort lauschet Lina. Las sie deines s\u00fcssen", "tokens": ["Dort", "lau\u00b7schet", "Li\u00b7na", ".", "Las", "sie", "dei\u00b7nes", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$.", "NE", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geruchs geniessen!", "tokens": ["Ge\u00b7ruchs", "ge\u00b7nies\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mir hat das Gl\u00fck noch keinen Kus bescheret.", "tokens": ["Mir", "hat", "das", "Gl\u00fck", "noch", "kei\u00b7nen", "Kus", "be\u00b7sche\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir aber, Liebchen, wird ja nichts verwehret.", "tokens": ["Dir", "a\u00b7ber", ",", "Lieb\u00b7chen", ",", "wird", "ja", "nichts", "ver\u00b7weh\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VAFIN", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nim drei f\u00fcr einen! Kom zur\u00fck! Nur Einer", "tokens": ["Nim", "drei", "f\u00fcr", "ei\u00b7nen", "!", "Kom", "zu\u00b7r\u00fck", "!", "Nur", "Ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "CARD", "APPR", "ART", "$.", "VVFIN", "PTKVZ", "$.", "ADV", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Davon sey meiner!", "tokens": ["Da\u00b7von", "sey", "mei\u00b7ner", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}