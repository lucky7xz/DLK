{"textgrid.poem.40141": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit wann du mein? ich wei\u00df es nicht:", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit wann du mein? ich wei\u00df es nicht:", "tokens": ["Seit", "wann", "du", "mein", "?", "ich", "wei\u00df", "es", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "PPER", "PPOSAT", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "was wei\u00df das Herz von Zeit und Raum!", "tokens": ["was", "wei\u00df", "das", "Herz", "von", "Zeit", "und", "Raum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ist, als w\u00e4r's seit Gestern erst,", "tokens": ["Mir", "ist", ",", "als", "w\u00e4r's", "seit", "Ge\u00b7stern", "erst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "APPR", "NN", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "da\u00df du erf\u00fclltest meinen Traum, \u2013", "tokens": ["da\u00df", "du", "er\u00b7f\u00fcll\u00b7test", "mei\u00b7nen", "Traum", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "mir ist, als w\u00e4r's seit Immer schon,", "tokens": ["mir", "ist", ",", "als", "w\u00e4r's", "seit", "Im\u00b7mer", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "APPR", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so ganz und gar sind wir vertraut:", "tokens": ["so", "ganz", "und", "gar", "sind", "wir", "ver\u00b7traut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "so ewig lange du mein Weib,", "tokens": ["so", "e\u00b7wig", "lan\u00b7ge", "du", "mein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "so immer wieder meine Braut!", "tokens": ["so", "im\u00b7mer", "wie\u00b7der", "mei\u00b7ne", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}