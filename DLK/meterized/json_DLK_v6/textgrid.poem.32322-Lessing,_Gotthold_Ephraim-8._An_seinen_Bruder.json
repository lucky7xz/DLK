{"textgrid.poem.32322": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "8. An seinen Bruder", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch dich hat, da du wardst geboren,", "tokens": ["Auch", "dich", "hat", ",", "da", "du", "wardst", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Muse l\u00e4chelnd angeblickt;", "tokens": ["Die", "Mu\u00b7se", "l\u00e4\u00b7chelnd", "an\u00b7ge\u00b7blickt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch du hast dich dem Schwarm der Toren", "tokens": ["Auch", "du", "hast", "dich", "dem", "Schwarm", "der", "To\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf jungen Fl\u00fcgeln k\u00fchn entr\u00fcckt!", "tokens": ["Auf", "jun\u00b7gen", "Fl\u00fc\u00b7geln", "k\u00fchn", "ent\u00b7r\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihm nach, dem Liebling des M\u00e4cenen!", "tokens": ["Ihm", "nach", ",", "dem", "Lieb\u00b7ling", "des", "M\u00e4\u00b7ce\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ihm nach, sein Name sporne dich!", "tokens": ["Ihm", "nach", ",", "sein", "Na\u00b7me", "spor\u00b7ne", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er lehrte dich, das Laster h\u00f6hnen;", "tokens": ["Er", "lehr\u00b7te", "dich", ",", "das", "Las\u00b7ter", "h\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er mache dich ihm f\u00fcrchterlich!", "tokens": ["Er", "ma\u00b7che", "dich", "ihm", "f\u00fcrch\u00b7ter\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O! schnitten wir mit gleichem Fluge", "tokens": ["O", "!", "schnit\u00b7ten", "wir", "mit", "glei\u00b7chem", "Flu\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die L\u00fcfte durch zur Ewigkeit!", "tokens": ["Die", "L\u00fcf\u00b7te", "durch", "zur", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O! schilderte mit Einem Zuge", "tokens": ["O", "!", "schil\u00b7der\u00b7te", "mit", "Ei\u00b7nem", "Zu\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zwei Br\u00fcder einst die Richterzeit!", "tokens": ["Zwei", "Br\u00fc\u00b7der", "einst", "die", "Rich\u00b7ter\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdie zwei, so soll die Nachwelt sprechen,", "tokens": ["\u00bb", "die", "zwei", ",", "so", "soll", "die", "Nach\u00b7welt", "spre\u00b7chen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "CARD", "$,", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Betaumelte kein Modewahn,", "tokens": ["Be\u00b7tau\u00b7mel\u00b7te", "kein", "Mo\u00b7de\u00b7wahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sprache sch\u00f6n zu radebrechen,", "tokens": ["Die", "Spra\u00b7che", "sch\u00f6n", "zu", "ra\u00b7de\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu stolz f\u00fcr eine Nebenbahn.\u00ab", "tokens": ["Zu", "stolz", "f\u00fcr", "ei\u00b7ne", "Ne\u00b7ben\u00b7bahn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Betritt der Alten sichre Wege!", "tokens": ["Be\u00b7tritt", "der", "Al\u00b7ten", "sich\u00b7re", "We\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Feiger nur geht davon ab.", "tokens": ["Ein", "Fei\u00b7ger", "nur", "geht", "da\u00b7von", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er suchet blumenreichre Stege,", "tokens": ["Er", "su\u00b7chet", "blu\u00b7men\u00b7reich\u00b7re", "Ste\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und findet seines Ruhmes Grab.", "tokens": ["Und", "fin\u00b7det", "sei\u00b7nes", "Ruh\u00b7mes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch lerne fr\u00fch das Lob entbehren,", "tokens": ["Doch", "ler\u00b7ne", "fr\u00fch", "das", "Lob", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das hier die Scheelsucht vorenth\u00e4lt.", "tokens": ["Das", "hier", "die", "Scheel\u00b7sucht", "vor\u00b7ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gnug, wann versetzt in h\u00f6hre Sph\u00e4ren,", "tokens": ["Gnug", ",", "wann", "ver\u00b7setzt", "in", "h\u00f6h\u00b7re", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Nachkomm uns ins Helle stellt!", "tokens": ["Ein", "Nach\u00b7komm", "uns", "ins", "Hel\u00b7le", "stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}