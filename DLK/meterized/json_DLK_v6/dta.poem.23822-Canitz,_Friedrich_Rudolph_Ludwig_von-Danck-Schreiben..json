{"dta.poem.23822": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Danck-Schreiben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Verg\u00f6nnt mir Sch\u00f6nsten/ da\u00df ich mag", "tokens": ["Ver\u00b7g\u00f6nnt", "mir", "Sch\u00f6ns\u00b7ten", "/", "da\u00df", "ich", "mag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$(", "KOUS", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch diesen Brieff die H\u00e4nde k\u00fcssen/", "tokens": ["Durch", "die\u00b7sen", "Brieff", "die", "H\u00e4n\u00b7de", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die gestern einen gantzen Tag", "tokens": ["Die", "ge\u00b7stern", "ei\u00b7nen", "gant\u00b7zen", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meinem Dienst sich regen m\u00fcssen;", "tokens": ["Zu", "mei\u00b7nem", "Dienst", "sich", "re\u00b7gen", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df ich meine Danckbarkeit/", "tokens": ["Und", "da\u00df", "ich", "mei\u00b7ne", "Dan\u00b7ck\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Zu der ich euch verbunden lebe/", "tokens": ["Zu", "der", "ich", "euch", "ver\u00b7bun\u00b7den", "le\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bey dieser fr\u00fchen Morgen-Zeit/", "tokens": ["Bey", "die\u00b7ser", "fr\u00fc\u00b7hen", "Mor\u00b7gen\u00b7Zeit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gehorsamst zu ", "tokens": ["Ge\u00b7hor\u00b7samst", "zu"], "token_info": ["word", "word"], "pos": ["NN", "PTKZU"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Denn da\u00df die liebe Dorilis", "tokens": ["Denn", "da\u00df", "die", "lie\u00b7be", "Do\u00b7ri\u00b7lis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vielleicht nicht meiner gantz vergessen/", "tokens": ["Viel\u00b7leicht", "nicht", "mei\u00b7ner", "gantz", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PPOSAT", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das hab\u2019 ich keinem sonst gewi\u00df", "tokens": ["Das", "hab'", "ich", "kei\u00b7nem", "sonst", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als eurer Arbeit beyzumessen.", "tokens": ["Als", "eu\u00b7rer", "Ar\u00b7beit", "bey\u00b7zu\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sehe noch in meinem Sinn", "tokens": ["Ich", "se\u00b7he", "noch", "in", "mei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die zarten Fingerchen spatziren/", "tokens": ["Die", "zar\u00b7ten", "Fin\u00b7ge\u00b7rchen", "spat\u00b7zi\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Am diese/ der ich eigen bin/", "tokens": ["Am", "die\u00b7se", "/", "der", "ich", "ei\u00b7gen", "bin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PDAT", "$(", "PRELS", "PPER", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit hundert Schleiffen auszuzieren.", "tokens": ["Mit", "hun\u00b7dert", "Schleif\u00b7fen", "aus\u00b7zu\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So lange wie ich reden kan/", "tokens": ["So", "lan\u00b7ge", "wie", "ich", "re\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll immer euer Lob erschallen/", "tokens": ["Soll", "im\u00b7mer", "eu\u00b7er", "Lob", "er\u00b7schal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil ihr so manchen Stich gethan/", "tokens": ["Weil", "ihr", "so", "man\u00b7chen", "Stich", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir armen Diener zu gefallen.", "tokens": ["Mir", "ar\u00b7men", "Die\u00b7ner", "zu", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Hertz das stellt sich selber ein/", "tokens": ["Mein", "Hertz", "das", "stellt", "sich", "sel\u00b7ber", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "PRF", "ADV", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit diesem wil ich euch begaben/", "tokens": ["Mit", "die\u00b7sem", "wil", "ich", "euch", "be\u00b7ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn ihr nur wollt zu frieden seyn/", "tokens": ["Wenn", "ihr", "nur", "wollt", "zu", "frie\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "APPR", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein schlechtes Macher-Lohn zu haben.", "tokens": ["Ein", "schlech\u00b7tes", "Ma\u00b7cher\u00b7Lohn", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es schien als woltet/ sch\u00f6nstes Paar/", "tokens": ["Es", "schien", "als", "wol\u00b7tet", "/", "sch\u00f6ns\u00b7tes", "Paar", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VMFIN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr beyde mit einander streiten/", "tokens": ["Ihr", "bey\u00b7de", "mit", "ein\u00b7an\u00b7der", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer/ was noch sonder Ordnung war/", "tokens": ["Wer", "/", "was", "noch", "son\u00b7der", "Ord\u00b7nung", "war", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$(", "PWS", "ADV", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am besten k\u00f6nte zubereiten.", "tokens": ["Am", "bes\u00b7ten", "k\u00f6n\u00b7te", "zu\u00b7be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr habt zu eurem Ruhm und Prei\u00df/", "tokens": ["Ihr", "habt", "zu", "eu\u00b7rem", "Ruhm", "und", "Prei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir etwas gutes ausgelesen/", "tokens": ["Mir", "et\u00b7was", "gu\u00b7tes", "aus\u00b7ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jedoch ist eure M\u00fch und Flei\u00df", "tokens": ["Je\u00b7doch", "ist", "eu\u00b7re", "M\u00fch", "und", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mehr wehrt als mein Geschenck gewesen.", "tokens": ["Mehr", "wehrt", "als", "mein", "Ge\u00b7schenck", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur da\u00df ihr ohne Fingerhut", "tokens": ["Nur", "da\u00df", "ihr", "oh\u00b7ne", "Fin\u00b7ger\u00b7hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gefochten/ und den Daum verletzet/", "tokens": ["Ge\u00b7foch\u00b7ten", "/", "und", "den", "Daum", "ver\u00b7let\u00b7zet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df euer sch\u00f6nes Purpur-Blut", "tokens": ["Da\u00df", "eu\u00b7er", "sch\u00f6\u00b7nes", "Pur\u00b7pur\u00b7Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die eine Liljen-Hand verletzet.", "tokens": ["Die", "ei\u00b7ne", "Lil\u00b7jen\u00b7Hand", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hat mir so weh\u2019 als euch gethan/", "tokens": ["Hat", "mir", "so", "weh'", "als", "euch", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil ich mir die Gedancken mache/", "tokens": ["Weil", "ich", "mir", "die", "Ge\u00b7dan\u00b7cken", "ma\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df dieses Blut schreyt Himmel an/", "tokens": ["Da\u00df", "die\u00b7ses", "Blut", "schreyt", "Him\u00b7mel", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und fodert die verdiente Rache.", "tokens": ["Und", "fo\u00b7dert", "die", "ver\u00b7dien\u00b7te", "Ra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Verfluchte Nadel die du dich", "tokens": ["Ver\u00b7fluch\u00b7te", "Na\u00b7del", "die", "du", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So eines Frevels unternommen/", "tokens": ["So", "ei\u00b7nes", "Fre\u00b7vels", "un\u00b7ter\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich w\u00fcnsche da\u00df kein guter Stich", "tokens": ["Ich", "w\u00fcn\u00b7sche", "da\u00df", "kein", "gu\u00b7ter", "Stich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mag mehr von deiner Spitze kommen/", "tokens": ["Mag", "mehr", "von", "dei\u00b7ner", "Spit\u00b7ze", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst aber w\u00fcnsch ich zum Beschlu\u00df/", "tokens": ["Sonst", "a\u00b7ber", "w\u00fcnsch", "ich", "zum", "Be\u00b7schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um mich nicht l\u00e4nger zu verweilen/", "tokens": ["Um", "mich", "nicht", "l\u00e4n\u00b7ger", "zu", "ver\u00b7wei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df bald mein Demuths-voller Ku\u00df/", "tokens": ["Da\u00df", "bald", "mein", "De\u00b7muths\u00b7vol\u00b7ler", "Ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Den b\u00f6sen Daum mag wieder heilen.", "tokens": ["Den", "b\u00f6\u00b7sen", "Daum", "mag", "wie\u00b7der", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}