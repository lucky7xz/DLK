{"dta.poem.11095": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie z\u00f6rnete, da\u00df er ihre brust  \n ber\u00fchret.  \n  ARIA.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erbarmest du dich nicht?", "tokens": ["Er\u00b7bar\u00b7mest", "du", "dich", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O strenge r\u00e4cherin der s\u00fcnden!", "tokens": ["O", "stren\u00b7ge", "r\u00e4\u00b7che\u00b7rin", "der", "s\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kanst du in dem nur k\u00fchlung finden,", "tokens": ["Kanst", "du", "in", "dem", "nur", "k\u00fch\u00b7lung", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PRELS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn mir dein grimm das urtheil spricht,", "tokens": ["Wenn", "mir", "dein", "grimm", "das", "ur\u00b7theil", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dein so zornig angesichte", "tokens": ["Und", "dein", "so", "zor\u00b7nig", "an\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den \u00e4rmsten Seladon stellt vor sein blut-gerichte?", "tokens": ["Den", "\u00e4rms\u00b7ten", "Se\u00b7la\u00b7don", "stellt", "vor", "sein", "blut\u00b7ge\u00b7rich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was dich so sehr ger\u00fchrt,", "tokens": ["Was", "dich", "so", "sehr", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "War nur ein griff nach deinem hertzen;", "tokens": ["War", "nur", "ein", "griff", "nach", "dei\u00b7nem", "hert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Indessen mu\u00df ich es verschmertzen,", "tokens": ["In\u00b7des\u00b7sen", "mu\u00df", "ich", "es", "ver\u00b7schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du mein hertze gar entf\u00fchrt:", "tokens": ["Da\u00df", "du", "mein", "hert\u00b7ze", "gar", "ent\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie hast denn du nun z\u00f6rnen sollen?", "tokens": ["Wie", "hast", "denn", "du", "nun", "z\u00f6r\u00b7nen", "sol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "KON", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist nicht entf\u00fchren mehr, als nur entf\u00fchren wollen?", "tokens": ["Ist", "nicht", "ent\u00b7f\u00fch\u00b7ren", "mehr", ",", "als", "nur", "ent\u00b7f\u00fch\u00b7ren", "wol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVFIN", "ADV", "$,", "KOUS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach! ist dein hertze stein,", "tokens": ["Ach", "!", "ist", "dein", "hert\u00b7ze", "stein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das sich nur diamanten gleichet,", "tokens": ["Das", "sich", "nur", "di\u00b7a\u00b7man\u00b7ten", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Das kein erbarmend trieb erweichet?", "tokens": ["Das", "kein", "er\u00b7bar\u00b7mend", "trieb", "er\u00b7wei\u00b7chet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So reime mir es dannoch ein!", "tokens": ["So", "rei\u00b7me", "mir", "es", "dan\u00b7noch", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will es als ein altar brauchen;", "tokens": ["Ich", "will", "es", "als", "ein", "al\u00b7tar", "brau\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df, strenge g\u00f6ttin! nur darauf mein hertze rauchen!", "tokens": ["La\u00df", ",", "stren\u00b7ge", "g\u00f6t\u00b7tin", "!", "nur", "da\u00b7rauf", "mein", "hert\u00b7ze", "rau\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "NE", "$.", "ADV", "PAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der stein wird endlich weich,", "tokens": ["Der", "stein", "wird", "end\u00b7lich", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn ihn nur tropffen offt begiessen.", "tokens": ["Wenn", "ihn", "nur", "tropf\u00b7fen", "offt", "be\u00b7gies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf dein hertz la\u00df ich str\u00f6hnte fliessen;", "tokens": ["Auf", "dein", "hertz", "la\u00df", "ich", "str\u00f6hn\u00b7te", "flies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIMP", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du aber bist nicht steinen gleich:", "tokens": ["Du", "a\u00b7ber", "bist", "nicht", "stei\u00b7nen", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst w\u00fcrde, wenn die thr\u00e4nen rinnen,", "tokens": ["Sonst", "w\u00fcr\u00b7de", ",", "wenn", "die", "thr\u00e4\u00b7nen", "rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein felsen-festes hertz erweichet werden k\u00f6nnen.", "tokens": ["Dein", "fel\u00b7sen\u00b7fes\u00b7tes", "hertz", "er\u00b7wei\u00b7chet", "wer\u00b7den", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die sonne wirfft den strahl", "tokens": ["Die", "son\u00b7ne", "wirfft", "den", "strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf klipp\u2019 und sumpff und schlechte dinge:", "tokens": ["Auf", "klipp'", "und", "sumpff", "und", "schlech\u00b7te", "din\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur ich, o sonne! bin zu g\u2019ringe.", "tokens": ["Nur", "ich", ",", "o", "son\u00b7ne", "!", "bin", "zu", "g'\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "FM", "FM", "$.", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du w\u00fcrdigst hecken, berg und thal,", "tokens": ["Du", "w\u00fcr\u00b7digst", "he\u00b7cken", ",", "berg", "und", "thal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "NE", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und l\u00e4\u00dft sie deinen glantz gen\u00fcssen:", "tokens": ["Und", "l\u00e4\u00dft", "sie", "dei\u00b7nen", "glantz", "ge\u00b7n\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur ich soll weiter nichts von deinen strahlen wissen.", "tokens": ["Nur", "ich", "soll", "wei\u00b7ter", "nichts", "von", "dei\u00b7nen", "strah\u00b7len", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich seufftze voll verdru\u00df,", "tokens": ["Ich", "seufft\u00b7ze", "voll", "ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und qu\u00e4le mich mit gram und reue,", "tokens": ["Und", "qu\u00e4\u00b7le", "mich", "mit", "gram", "und", "reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bi\u00df mich der tod davon befreye,", "tokens": ["Bi\u00df", "mich", "der", "tod", "da\u00b7von", "be\u00b7fre\u00b7ye", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Weil ich durch dich doch sterben mu\u00df.", "tokens": ["Weil", "ich", "durch", "dich", "doch", "ster\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schreib du auf meines grabes stelle:", "tokens": ["Schreib", "du", "auf", "mei\u00b7nes", "gra\u00b7bes", "stel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Lernt, menschen! wie der zorn getreue liebe f\u00e4lle.", "tokens": ["Lernt", ",", "men\u00b7schen", "!", "wie", "der", "zorn", "ge\u00b7treu\u00b7e", "lie\u00b7be", "f\u00e4l\u00b7le", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVINF", "$.", "PWAV", "ART", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}