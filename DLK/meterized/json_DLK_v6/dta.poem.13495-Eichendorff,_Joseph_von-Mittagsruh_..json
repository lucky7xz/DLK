{"dta.poem.13495": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mittagsruh .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber Bergen, Flu\u00df und Thalen,", "tokens": ["Ue\u00b7ber", "Ber\u00b7gen", ",", "Flu\u00df", "und", "Tha\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stiller Lust und tiefen Qualen", "tokens": ["Stil\u00b7ler", "Lust", "und", "tie\u00b7fen", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Webet heimlich, schillert, Strahlen!", "tokens": ["We\u00b7bet", "heim\u00b7lich", ",", "schil\u00b7lert", ",", "Strah\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinnend ruht des Tags Gew\u00fchle", "tokens": ["Sin\u00b7nend", "ruht", "des", "Tags", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der dunkelblauen Schw\u00fcle,", "tokens": ["In", "der", "dun\u00b7kel\u00b7blau\u00b7en", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die ewigen Gef\u00fchle,", "tokens": ["Und", "die", "e\u00b7wi\u00b7gen", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was dir selber unbewu\u00dft,", "tokens": ["Was", "dir", "sel\u00b7ber", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Treten heimlich, gro\u00df und leise", "tokens": ["Tre\u00b7ten", "heim\u00b7lich", ",", "gro\u00df", "und", "lei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aus der Wirrung fester Gleise,", "tokens": ["Aus", "der", "Wir\u00b7rung", "fes\u00b7ter", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aus der unbewachten Brust,", "tokens": ["Aus", "der", "un\u00b7be\u00b7wach\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "In die stillen, weiten Kreise.", "tokens": ["In", "die", "stil\u00b7len", ",", "wei\u00b7ten", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}