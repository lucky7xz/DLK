{"textgrid.poem.66214": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Vom T\u00fcrkenfrieden", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geschlossen, geschlossen der w\u00fcthige Kampf!", "tokens": ["Ge\u00b7schlos\u00b7sen", ",", "ge\u00b7schlos\u00b7sen", "der", "w\u00fct\u00b7hi\u00b7ge", "Kampf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Blitze des S\u00e4bels morden nicht mehr!", "tokens": ["Blit\u00b7ze", "des", "S\u00e4\u00b7bels", "mor\u00b7den", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "PTKNEG", "ADV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Metallene Donner t\u00f6dten nicht mehr!", "tokens": ["Me\u00b7tal\u00b7le\u00b7ne", "Don\u00b7ner", "t\u00f6d\u00b7ten", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gottes Sonne trinkt nicht mehr", "tokens": ["Got\u00b7tes", "Son\u00b7ne", "trinkt", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "PTKNEG", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gew\u00fcrgter Leichen wallenden Dampf!", "tokens": ["Ge\u00b7w\u00fcrg\u00b7ter", "Lei\u00b7chen", "wal\u00b7len\u00b7den", "Dampf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Die schrecklichen Reussen,", "tokens": ["Die", "schreck\u00b7li\u00b7chen", "Reus\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die M\u00e4nner von Eisen", "tokens": ["Die", "M\u00e4n\u00b7ner", "von", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Bieten dem T\u00fcrken die Hand.", "tokens": ["Bie\u00b7ten", "dem", "T\u00fcr\u00b7ken", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Die K\u00e4mpfer sind des langen Haders m\u00fcde,", "tokens": ["Die", "K\u00e4mp\u00b7fer", "sind", "des", "lan\u00b7gen", "Ha\u00b7ders", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Herunter schaut der l\u00e4chelnde Friede", "tokens": ["Her\u00b7un\u00b7ter", "schaut", "der", "l\u00e4\u00b7cheln\u00b7de", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Auf das ver\u00f6dete Land.", "tokens": ["Auf", "das", "ver\u00b7\u00f6\u00b7de\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}