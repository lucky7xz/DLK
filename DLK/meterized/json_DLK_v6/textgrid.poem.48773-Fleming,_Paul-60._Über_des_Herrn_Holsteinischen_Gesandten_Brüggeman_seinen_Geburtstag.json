{"textgrid.poem.48773": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "60. \u00dcber des Herrn Holsteinischen Gesandten Br\u00fcggeman seinen Geburtstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vors erste Gott gedankt f\u00fcr alle seine G\u00fcten,", "tokens": ["Vors", "ers\u00b7te", "Gott", "ge\u00b7dankt", "f\u00fcr", "al\u00b7le", "sei\u00b7ne", "G\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die er auf unser H\u00e4upt durch so viel lange Zeit", "tokens": ["die", "er", "auf", "un\u00b7ser", "H\u00e4upt", "durch", "so", "viel", "lan\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in ungez\u00e4lter Zal hat mildreich ausgestreut,", "tokens": ["in", "un\u00b7ge\u00b7z\u00e4l\u00b7ter", "Zal", "hat", "mil\u00b7dreich", "aus\u00b7ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da Neid und List und Ort so vielmal uns verrieten,", "tokens": ["da", "Neid", "und", "List", "und", "Ort", "so", "viel\u00b7mal", "uns", "ver\u00b7rie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "bald auf der Skyten ", "tokens": ["bald", "auf", "der", "Sky\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hernach ihn angeruft mit wahrer Innigkeit,", "tokens": ["Her\u00b7nach", "ihn", "an\u00b7ge\u00b7ruft", "mit", "wah\u00b7rer", "In\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "um da\u00df er noch fortan uns gebe gut Geleit'", "tokens": ["um", "da\u00df", "er", "noch", "for\u00b7tan", "uns", "ge\u00b7be", "gut", "Ge\u00b7leit'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADV", "ADV", "PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und bis an unser Haus uns wolle wol beh\u00fcten!", "tokens": ["und", "bis", "an", "un\u00b7ser", "Haus", "uns", "wol\u00b7le", "wol", "be\u00b7h\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Darauf gleich alsobald zur Tafel sich gesetzt,", "tokens": ["Da\u00b7rauf", "gleich", "al\u00b7so\u00b7bald", "zur", "Ta\u00b7fel", "sich", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sich mit gesunder Kost und sichrer Lust ergetzt.", "tokens": ["sich", "mit", "ge\u00b7sun\u00b7der", "Kost", "und", "sich\u00b7rer", "Lust", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das macht die Seele froh, das gibt dem Leibe Freude.", "tokens": ["Das", "macht", "die", "See\u00b7le", "froh", ",", "das", "gibt", "dem", "Lei\u00b7be", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "$,", "PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Herr, erste beide la\u00dft uns heute zwiefach tun,", "tokens": ["Herr", ",", "ers\u00b7te", "bei\u00b7de", "la\u00dft", "uns", "heu\u00b7te", "zwie\u00b7fach", "tun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PIS", "VVFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das dritte lassen wir in eurer Gunst beruhn,", "tokens": ["das", "drit\u00b7te", "las\u00b7sen", "wir", "in", "eu\u00b7rer", "Gunst", "be\u00b7ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und solts auch dreifach sein, so gl\u00fcckts zu keinem Leide.", "tokens": ["und", "solts", "auch", "drei\u00b7fach", "sein", ",", "so", "gl\u00fcckts", "zu", "kei\u00b7nem", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VAINF", "$,", "ADV", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In Moskaw 1639.", "tokens": ["In", "Mos\u00b7kaw", "16\u00b739", "."], "token_info": ["word", "word", "number", "punct"], "pos": ["APPR", "NE", "CARD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}