{"dta.poem.795": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXI.  \n H.  Nathanael  Ro\u00dfteuschers vnd  Alith\u00e6\u00e6  \n Roussi\u00e6  Hochzeit.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jn dem das feste Genpf der Helden k\u00fchnheit \u00fcbt/ ", "tokens": ["In", "dem", "das", "fes\u00b7te", "Genpf", "der", "Hel\u00b7den", "k\u00fchn\u00b7heit", "\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd endlich jhren ruhm durch seinen fall au\u00dfbreittet:", "tokens": ["Vnd", "end\u00b7lich", "jhren", "ruhm", "durch", "sei\u00b7nen", "fall", "au\u00df\u00b7breit\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bricht auch der harte Sinn/ den Jhr bi\u00df noch bestreittet", "tokens": ["Bricht", "auch", "der", "har\u00b7te", "Sinn", "/", "den", "Ihr", "bi\u00df", "noch", "be\u00b7streit\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$(", "ART", "PPER", "ADV", "ADV", "VVPP"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Vnd Ewer ", "tokens": ["Vnd", "E\u00b7wer"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Nun sch\u00f6pfft Jhr lust aus dem/ was Euch hi\u00dfher betr\u00fcbt/", "tokens": ["Nun", "sch\u00f6pfft", "Ihr", "lust", "aus", "dem", "/", "was", "Euch", "hi\u00df\u00b7her", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "$(", "PWS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Hochzeit G\u00f6ttin hat den einzug schon bereittet:", "tokens": ["Die", "Hoch\u00b7zeit", "G\u00f6t\u00b7tin", "hat", "den", "ein\u00b7zug", "schon", "be\u00b7reit\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Vnd den so werthen Feind euch g\u00e4ntzlich \u00fcbergibt.", "tokens": ["Vnd", "den", "so", "wert\u00b7hen", "Feind", "euch", "g\u00e4ntz\u00b7lich", "\u00fc\u00b7berg\u00b7ibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVFIN", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er wolle noch mit jhm Gl\u00fcck/ Ehre/ sanfftes leben/", "tokens": ["Er", "wol\u00b7le", "noch", "mit", "jhm", "Gl\u00fcck", "/", "Eh\u00b7re", "/", "sanff\u00b7tes", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "NN", "$(", "NN", "$(", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gew\u00fcndschte Lust vnd Frewd/ vnd Heil vnd Segen geben", "tokens": ["Ge\u00b7w\u00fcnd\u00b7schte", "Lust", "vnd", "Frewd", "/", "vnd", "Heil", "vnd", "Se\u00b7gen", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "$(", "KON", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd fortgang vnd gewinn/ vnd was mein ", "tokens": ["Vnd", "fort\u00b7gang", "vnd", "ge\u00b7winn", "/", "vnd", "was", "mein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$(", "KON", "PWS", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Der Wundsch ist zwar nicht new\u2019 vnd voll gemeiner Sachen", "tokens": ["Der", "Wund\u00b7sch", "ist", "zwar", "nicht", "ne\u00b7w'", "vnd", "voll", "ge\u00b7mei\u00b7ner", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Herr Br\u00e4utgam: jhr m\u00f6gt selbst der Brautt was newes ma-", "tokens": ["Herr", "Br\u00e4ut\u00b7gam", ":", "jhr", "m\u00f6gt", "selbst", "der", "Brautt", "was", "ne\u00b7wes", "ma"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "PPER", "VMFIN", "ADV", "ART", "NN", "PWS", "ADJA", "TRUNC"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.3": {"text": "chen.", "tokens": ["chen", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Das Leben/ Seel vnd Geist/ vnd H\u00e4nd vnd F\u00fc\u00dfe hatt.", "tokens": ["Das", "Le\u00b7ben", "/", "Seel", "vnd", "Geist", "/", "vnd", "H\u00e4nd", "vnd", "F\u00fc\u00b7\u00dfe", "hatt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "KON", "NN", "$(", "KON", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}