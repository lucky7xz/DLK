{"textgrid.poem.50207": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "39. Dieselbe in Knittel-Versen.", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Dass Du aus deinem ", "tokens": ["Dass", "Du", "aus", "dei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Im Fall man ihm bei Tag' und Nacht", "tokens": ["Im", "Fall", "man", "ihm", "bei", "Tag'", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PIS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Tage w\u00e4r es nicht beschwerlich,", "tokens": ["Im", "Ta\u00b7ge", "w\u00e4r", "es", "nicht", "be\u00b7schwer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch in der Nacht ist es gef\u00e4hrlich;", "tokens": ["Doch", "in", "der", "Nacht", "ist", "es", "ge\u00b7f\u00e4hr\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du weisst, ", "tokens": ["Du", "weisst", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Doch w\u00e4r' ich Du, so wolt ich lieber", "tokens": ["Doch", "w\u00e4r'", "ich", "Du", ",", "so", "wolt", "ich", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "$,", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Und wenn ich steckt' in Deinem Loch,", "tokens": ["Und", "wenn", "ich", "steckt'", "in", "Dei\u00b7nem", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Eh' ", "tokens": ["Eh'"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}}}}