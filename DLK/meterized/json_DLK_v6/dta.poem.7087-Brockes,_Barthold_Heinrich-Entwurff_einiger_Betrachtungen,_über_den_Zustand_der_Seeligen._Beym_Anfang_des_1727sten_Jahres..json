{"dta.poem.7087": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer kan der frohen Seel Empfindlichkeit ergr\u00fcnden?", "tokens": ["Wer", "kan", "der", "fro\u00b7hen", "Seel", "Emp\u00b7find\u00b7lich\u00b7keit", "er\u00b7gr\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn GOTT, der alles kan, ihr eine F\u00e4higkeit,", "tokens": ["Wenn", "GoTT", ",", "der", "al\u00b7les", "kan", ",", "ihr", "ei\u00b7ne", "F\u00e4\u00b7hig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PRELS", "PIS", "VMFIN", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit immer gr\u00f6sserer Vollkommenheit,", "tokens": ["Mit", "im\u00b7mer", "gr\u00f6s\u00b7se\u00b7rer", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Wunder Seiner Lieb und Allmacht zu bedencken,", "tokens": ["Die", "Wun\u00b7der", "Sei\u00b7ner", "Lieb", "und", "All\u00b7macht", "zu", "be\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bey der Unz\u00e4hlbarkeit derselbigen, wird schencken!", "tokens": ["Bey", "der", "Un\u00b7z\u00e4hl\u00b7bar\u00b7keit", "der\u00b7sel\u00b7bi\u00b7gen", ",", "wird", "schen\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDS", "$,", "VAFIN", "VVINF", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}}}}}