{"textgrid.poem.47338": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Ostwind kam ans Schenkethor,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ostwind kam ans Schenkethor,", "tokens": ["Der", "Ost\u00b7wind", "kam", "ans", "Schen\u00b7ke\u00b7thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit lautem Gru\u00df zu pochen;", "tokens": ["Mit", "lau\u00b7tem", "Gru\u00df", "zu", "po\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da trat der alte Wirt hervor,", "tokens": ["Da", "trat", "der", "al\u00b7te", "Wirt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den hat er angesprochen:", "tokens": ["Den", "hat", "er", "an\u00b7ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbich w\u00fcnsche dir Gl\u00fcck zu dieser Zeit,", "tokens": ["\u00bb", "ich", "w\u00fcn\u00b7sche", "dir", "Gl\u00fcck", "zu", "die\u00b7ser", "Zeit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Herr Fr\u00fchling ist angekommen,", "tokens": ["Herr", "Fr\u00fch\u00b7ling", "ist", "an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf Flur und Anger weit und breit", "tokens": ["Auf", "Flur", "und", "An\u00b7ger", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist neues Leben entglommen.", "tokens": ["Ist", "neu\u00b7es", "Le\u00b7ben", "ent\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun ist die Luft ein Balsamhauch,", "tokens": ["Nun", "ist", "die", "Luft", "ein", "Bal\u00b7sam\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Moschusreh die Erde,", "tokens": ["Ein", "Mo\u00b7schus\u00b7reh", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Unter Blumen am Bl\u00fctenstrauch", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "am", "Bl\u00fc\u00b7ten\u00b7strauch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Froh spielender Geb\u00e4rde.", "tokens": ["Froh", "spie\u00b7len\u00b7der", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von meinem Weh'n ist der Kamin", "tokens": ["Von", "mei\u00b7nem", "Weh'n", "ist", "der", "Ka\u00b7min"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tulpen angefachet,", "tokens": ["Der", "Tul\u00b7pen", "an\u00b7ge\u00b7fa\u00b7chet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Blicke w\u00e4rmen sich am Karmin,", "tokens": ["Und", "Bli\u00b7cke", "w\u00e4r\u00b7men", "sich", "am", "Kar\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Rosenwangen entlachet.", "tokens": ["Der", "Ro\u00b7sen\u00b7wan\u00b7gen", "ent\u00b7la\u00b7chet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Nimm meinen Rat in kluges Ohr,", "tokens": ["Nimm", "mei\u00b7nen", "Rat", "in", "klu\u00b7ges", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun fege die alte Schenke,", "tokens": ["Nun", "fe\u00b7ge", "die", "al\u00b7te", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Steck' einen gr\u00fcnen Busch ans Thor", "tokens": ["Steck'", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Busch", "ans", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und r\u00fcste frisches Getr\u00e4nke.", "tokens": ["Und", "r\u00fcs\u00b7te", "fri\u00b7sches", "Ge\u00b7tr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Schon sah ich drau\u00dfen im Sonnenschein", "tokens": ["Schon", "sah", "ich", "drau\u00b7\u00dfen", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schmachten die lechzende Liebe,", "tokens": ["Schmach\u00b7ten", "die", "lech\u00b7zen\u00b7de", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Sie kommt zu st\u00fcrzen in deinen Wein,", "tokens": ["Sie", "kommt", "zu", "st\u00fcr\u00b7zen", "in", "dei\u00b7nen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihre entflammten Triebe.", "tokens": ["Ih\u00b7re", "ent\u00b7flamm\u00b7ten", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Wehr' einen Trunk der Labung nicht", "tokens": ["Wehr'", "ei\u00b7nen", "Trunk", "der", "La\u00b7bung", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jedem aufrichtigen Zecher;", "tokens": ["Je\u00b7dem", "auf\u00b7rich\u00b7ti\u00b7gen", "Ze\u00b7cher", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Doch kommt ein Heuchler, ein kluger Wicht,", "tokens": ["Doch", "kommt", "ein", "Heuch\u00b7ler", ",", "ein", "klu\u00b7ger", "Wicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So decke zu die Becher.", "tokens": ["So", "de\u00b7cke", "zu", "die", "Be\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Versauern w\u00fcrde sogleich der Wein,", "tokens": ["Ver\u00b7sau\u00b7ern", "w\u00fcr\u00b7de", "sog\u00b7leich", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wenn sauere Blick' ihn tr\u00e4fen;", "tokens": ["Wenn", "sau\u00b7e\u00b7re", "Blick'", "ihn", "tr\u00e4\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und fl\u00f6ss' unlautere Weisheit darein,", "tokens": ["Und", "fl\u00f6ss'", "un\u00b7lau\u00b7te\u00b7re", "Weis\u00b7heit", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So w\u00fcrde der Trank zu Hefen.", "tokens": ["So", "w\u00fcr\u00b7de", "der", "Trank", "zu", "He\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Leb' wohl! ich will nun meinen Herrn", "tokens": ["Leb'", "wohl", "!", "ich", "will", "nun", "mei\u00b7nen", "Herrn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "PPER", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hafisen sogleich dir schicken,", "tokens": ["Ha\u00b7fi\u00b7sen", "sog\u00b7leich", "dir", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Du wirst den Freund von selber gern", "tokens": ["Du", "wirst", "den", "Freund", "von", "sel\u00b7ber", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinem Besten erquicken.", "tokens": ["Mit", "dei\u00b7nem", "Bes\u00b7ten", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Sich wagt, wo er in der Schenke zecht,", "tokens": ["Sich", "wagt", ",", "wo", "er", "in", "der", "Schen\u00b7ke", "zecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Heuchling, kein M\u00f6nch, kein Fr\u00f6mmling;", "tokens": ["Kein", "Heuch\u00b7ling", ",", "kein", "M\u00f6nch", ",", "kein", "Fr\u00f6mm\u00b7ling", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn der Hafis ist schlecht und recht", "tokens": ["Denn", "der", "Ha\u00b7fis", "ist", "schlecht", "und", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der alten Treu' Abk\u00f6mmling.\u00ab", "tokens": ["Der", "al\u00b7ten", "Treu'", "Ab\u00b7k\u00f6mm\u00b7ling", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}