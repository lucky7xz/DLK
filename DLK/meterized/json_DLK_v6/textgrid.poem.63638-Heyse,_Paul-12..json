{"textgrid.poem.63638": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein reingestimmtes Saitenspiel;", "tokens": ["Ich", "war", "ein", "rein\u00b7ge\u00b7stimm\u00b7tes", "Sai\u00b7ten\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich erklang, so war's zur Freude vielen.", "tokens": ["Wenn", "ich", "er\u00b7klang", ",", "so", "wa\u00b7r's", "zur", "Freu\u00b7de", "vie\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Warum's dem Meister Schicksal nur gefiel,", "tokens": ["War\u00b7um's", "dem", "Meis\u00b7ter", "Schick\u00b7sal", "nur", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So ungest\u00fcm und rauh mir mitzuspielen?", "tokens": ["So", "un\u00b7ge\u00b7st\u00fcm", "und", "rauh", "mir", "mit\u00b7zu\u00b7spie\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun ist die edle Harmonie zerst\u00f6rt;", "tokens": ["Nun", "ist", "die", "ed\u00b7le", "Har\u00b7mo\u00b7nie", "zer\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verstimmen mu\u00df ich jeden, der mich h\u00f6rt.", "tokens": ["Ver\u00b7stim\u00b7men", "mu\u00df", "ich", "je\u00b7den", ",", "der", "mich", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun sind die andern Saiten all' zersprungen;", "tokens": ["Nun", "sind", "die", "an\u00b7dern", "Sai\u00b7ten", "all'", "zer\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur eine t\u00f6nt noch, von Erinnerungen.", "tokens": ["Nur", "ei\u00b7ne", "t\u00f6nt", "noch", ",", "von", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "ADV", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}