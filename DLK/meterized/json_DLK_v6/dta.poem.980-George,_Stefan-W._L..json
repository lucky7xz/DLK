{"dta.poem.980": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "W. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Der seltnen Einer die das loos ersch&#252;ttert                 ", "tokens": ["Der", "selt\u00b7nen", "Ei\u00b7ner", "die", "das", "loos", "ersch", "&#252;", "ttert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ART", "NN", "ADJD", "$(", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verbannter herscher ihr erhabnes trauern                 ", "tokens": ["Ver\u00b7bann\u00b7ter", "her\u00b7scher", "ihr", "er\u00b7hab\u00b7nes", "trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APZR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und unbemerkter tod &#183; schon weil du bist                 ", "tokens": ["Und", "un\u00b7be\u00b7merk\u00b7ter", "tod", "&#183;", "schon", "weil", "du", "bist"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$(", "ADV", "KOUS", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sei dir in dank genaht &#183; durch deine hoheit                 ", "tokens": ["Sei", "dir", "in", "dank", "ge\u00b7naht", "&#183;", "durch", "dei\u00b7ne", "ho\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJD", "VVPP", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Best&#228;tigst du uns unser recht auf hoheit                 ", "tokens": ["Best", "&#228;", "tigst", "du", "uns", "un\u00b7ser", "recht", "auf", "ho\u00b7heit"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "VVFIN", "PPER", "PRF", "PPOSAT", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Verwirfst und nimmst mit k\u00f6niglichem wink                 ", "tokens": ["Ver\u00b7wirfst", "und", "nimmst", "mit", "k\u00f6\u00b7nig\u00b7li\u00b7chem", "wink"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du richte unsrer manchmal schwanken tritte                 ", "tokens": ["Du", "rich\u00b7te", "uns\u00b7rer", "manch\u00b7mal", "schwan\u00b7ken", "trit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und leitstern &#252;ber jeder edlen fahrt.               ", "tokens": ["Und", "leits\u00b7tern", "&#252;", "ber", "je\u00b7der", "ed\u00b7len", "fahrt", "."], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "ADV", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}