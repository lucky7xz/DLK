{"dta.poem.9222": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n Als die b\u00f6se Catharis ihren Namens-Tag  \n begieng.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.85", "sv:0.14"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "B\u00f6se jnngfer sol ich nun", "tokens": ["B\u00f6\u00b7se", "jnng\u00b7fer", "sol", "ich", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich der sachen unterwinden/", "tokens": ["Mich", "der", "sa\u00b7chen", "un\u00b7ter\u00b7win\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der zeit ihr recht zu thun/", "tokens": ["Und", "der", "zeit", "ihr", "recht", "zu", "thun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie mit einem bande binden?", "tokens": ["Sie", "mit", "ei\u00b7nem", "ban\u00b7de", "bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein/ ach nein/ ach warlich nein/", "tokens": ["Nein", "/", "ach", "nein", "/", "ach", "war\u00b7lich", "nein", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "XY", "PTKANT", "$(", "ITJ", "ADV", "PTKANT", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein/ sie m\u00f6chte b\u00f6se seyn.", "tokens": ["Nein", "/", "sie", "m\u00f6ch\u00b7te", "b\u00f6\u00b7se", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Jhren scharffen text gelesen:", "tokens": ["Ih\u00b7ren", "scharf\u00b7fen", "text", "ge\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ichs weiter treiben wil/", "tokens": ["Wo", "ichs", "wei\u00b7ter", "trei\u00b7ben", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So bekomm ich gar den besen;", "tokens": ["So", "be\u00b7komm", "ich", "gar", "den", "be\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein ich wag es nicht mit ihr/", "tokens": ["Nein", "ich", "wag", "es", "nicht", "mit", "ihr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann wer ist mir gut daf\u00fcr?", "tokens": ["Dann", "wer", "ist", "mir", "gut", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "PPER", "ADJD", "PAV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "3. L\u00e4st sie sich nicht sauer an", "tokens": ["L\u00e4st", "sie", "sich", "nicht", "sau\u00b7er", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den reden und geberden/", "tokens": ["In", "den", "re\u00b7den", "und", "ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVINF", "KON", "VVFIN", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Da\u00df ich armer hampel-mann", "tokens": ["Da\u00df", "ich", "ar\u00b7mer", "ham\u00b7pel\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6chte zum salate werden/", "tokens": ["M\u00f6ch\u00b7te", "zum", "sa\u00b7la\u00b7te", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "VVFIN", "VAINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann ich nur den \u00fcberflu\u00df", "tokens": ["Wann", "ich", "nur", "den", "\u00fc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhrer bo\u00dfheit kosten mu\u00df.", "tokens": ["Ih\u00b7rer", "bo\u00df\u00b7heit", "kos\u00b7ten", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ich bin from als wie ein lamm/", "tokens": ["Ich", "bin", "from", "als", "wie", "ein", "lamm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "KOKOM", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleichwohl sind die jungfern alle", "tokens": ["Gleich\u00b7wohl", "sind", "die", "jung\u00b7fern", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "PIAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mir zum blossen possen gram/", "tokens": ["Mir", "zum", "blos\u00b7sen", "pos\u00b7sen", "gram", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich offt in schwermuth falle/", "tokens": ["Da\u00df", "ich", "offt", "in", "schwer\u00b7muth", "fal\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann ich dencke/ wie ich wohl", "tokens": ["Wann", "ich", "den\u00b7cke", "/", "wie", "ich", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$(", "PWAV", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Diesen sachen helffen soll.", "tokens": ["Die\u00b7sen", "sa\u00b7chen", "helf\u00b7fen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Wann doch itzt mein schlechtes band", "tokens": ["Wann", "doch", "itzt", "mein", "schlech\u00b7tes", "band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6flich auffgezogen k\u00e4me/", "tokens": ["H\u00f6f\u00b7lich", "auff\u00b7ge\u00b7zo\u00b7gen", "k\u00e4\u00b7me", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVIZU", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und entweder ihre hand", "tokens": ["Und", "ent\u00b7we\u00b7der", "ih\u00b7re", "hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Solches nicht vor willen n\u00e4hme/", "tokens": ["Sol\u00b7ches", "nicht", "vor", "wil\u00b7len", "n\u00e4h\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder mir zum \u00fcberdru\u00df", "tokens": ["O\u00b7der", "mir", "zum", "\u00fc\u00b7berd\u00b7ru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4te sie es untern fu\u00df.", "tokens": ["Tr\u00e4\u00b7te", "sie", "es", "un\u00b7tern", "fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Ey/ wie w\u00fcrd ich da bestehn/", "tokens": ["Ey", "/", "wie", "w\u00fcrd", "ich", "da", "be\u00b7stehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOKOM", "VAFIN", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich armer diebel solte", "tokens": ["Da\u00df", "ich", "ar\u00b7mer", "die\u00b7bel", "sol\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So bezahlt nach hause gehn/", "tokens": ["So", "be\u00b7zahlt", "nach", "hau\u00b7se", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann ich courtisiren wolte?", "tokens": ["Wann", "ich", "cour\u00b7ti\u00b7si\u00b7ren", "wol\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein/ ich gebe mich nicht blo\u00df/", "tokens": ["Nein", "/", "ich", "ge\u00b7be", "mich", "nicht", "blo\u00df", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine scham ist viel zu gro\u00df.", "tokens": ["Mei\u00b7ne", "scham", "ist", "viel", "zu", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Alle leute wiesen ja", "tokens": ["Al\u00b7le", "leu\u00b7te", "wie\u00b7sen", "ja"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den fingern/ wo ich st\u00fcnde;", "tokens": ["Mit", "den", "fin\u00b7gern", "/", "wo", "ich", "st\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVINF", "$(", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht doch/ seht den kerlen da", "tokens": ["Seht", "doch", "/", "seht", "den", "ker\u00b7len", "da"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$(", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem sch\u00f6nen angebinde/", "tokens": ["Mit", "dem", "sch\u00f6\u00b7nen", "an\u00b7ge\u00b7bin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht doch/ seht wie er sich ziert", "tokens": ["Seht", "doch", "/", "seht", "wie", "er", "sich", "ziert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$(", "VVFIN", "KOKOM", "PPER", "PRF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den rotz ans schienbein schmirt.", "tokens": ["Und", "den", "rotz", "ans", "schien\u00b7bein", "schmirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Dieses geh ich endlich ein/", "tokens": ["Die\u00b7ses", "geh", "ich", "end\u00b7lich", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das b\u00f6se donner-hertze", "tokens": ["Wo", "das", "b\u00f6\u00b7se", "don\u00b7ner\u00b7hert\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will schier k\u00fcnfftig fr\u00f6mmer seyn/", "tokens": ["Will", "schier", "k\u00fcnff\u00b7tig", "fr\u00f6m\u00b7mer", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun so schwer ich nicht im schertze", "tokens": ["Nun", "so", "schwer", "ich", "nicht", "im", "schert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "PPER", "PTKNEG", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey dem schacht und damen-spiel/", "tokens": ["Bey", "dem", "schacht", "und", "da\u00b7men\u00b7spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "KON", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich sie noch binden will.", "tokens": ["Da\u00df", "ich", "sie", "noch", "bin\u00b7den", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}