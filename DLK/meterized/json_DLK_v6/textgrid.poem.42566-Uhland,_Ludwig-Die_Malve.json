{"textgrid.poem.42566": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Malve", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder hab ich dich gesehen,", "tokens": ["Wie\u00b7der", "hab", "ich", "dich", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blasse Malve! bl\u00fchst du schon?", "tokens": ["Blas\u00b7se", "Mal\u00b7ve", "!", "bl\u00fchst", "du", "schon", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja! mich traf ein schaurig Wehen,", "tokens": ["Ja", "!", "mich", "traf", "ein", "schau\u00b7rig", "We\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All mein Fr\u00fchling welkt davon.", "tokens": ["All", "mein", "Fr\u00fch\u00b7ling", "welkt", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist du doch des Herbstes Rose,", "tokens": ["Bist", "du", "doch", "des", "Herbs\u00b7tes", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der gesunknen Sonne Kind,", "tokens": ["Der", "ge\u00b7sun\u00b7knen", "Son\u00b7ne", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bist die starre, d\u00fcftelose,", "tokens": ["Bist", "die", "star\u00b7re", ",", "d\u00fcf\u00b7te\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deren Bl\u00fcten keine sind.", "tokens": ["De\u00b7ren", "Bl\u00fc\u00b7ten", "kei\u00b7ne", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gerne wollt ich dich begr\u00fc\u00dfen,", "tokens": ["Ger\u00b7ne", "wollt", "ich", "dich", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchtest du nicht rosenfarb,", "tokens": ["Bl\u00fch\u00b7test", "du", "nicht", "ro\u00b7sen\u00b7farb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00f6gst du nicht das Rot der S\u00fc\u00dfen,", "tokens": ["L\u00f6gst", "du", "nicht", "das", "Rot", "der", "S\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die noch eben gl\u00fcht' und starb.", "tokens": ["Die", "noch", "e\u00b7ben", "gl\u00fcht'", "und", "starb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heuchle nicht des Lenzes Dauer!", "tokens": ["Heuch\u00b7le", "nicht", "des", "Len\u00b7zes", "Dau\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du bedarfst des Scheines nicht;", "tokens": ["Du", "be\u00b7darfst", "des", "Schei\u00b7nes", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hast ja sch\u00f6ne, dunkle Trauer,", "tokens": ["Hast", "ja", "sch\u00f6\u00b7ne", ",", "dunk\u00b7le", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hast ja wei\u00dfes, sanftes Licht.", "tokens": ["Hast", "ja", "wei\u00b7\u00dfes", ",", "sanf\u00b7tes", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}