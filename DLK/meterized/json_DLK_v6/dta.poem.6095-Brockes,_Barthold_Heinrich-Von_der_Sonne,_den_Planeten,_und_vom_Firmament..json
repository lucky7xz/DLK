{"dta.poem.6095": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst uns die Thorheit doch verlachen,", "tokens": ["Lasst", "uns", "die", "Thor\u00b7heit", "doch", "ver\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6ren nur der Weisen Lehre!", "tokens": ["Und", "h\u00f6\u00b7ren", "nur", "der", "Wei\u00b7sen", "Leh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lasst uns auf andre Weis\u2019 uns doch zu Nutzen machen", "tokens": ["Lasst", "uns", "auf", "and\u00b7re", "Weis'", "uns", "doch", "zu", "Nut\u00b7zen", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die leuchtende Gesch\u00f6pff\u2019! O Himmel! welche Heere!", "tokens": ["Die", "leuch\u00b7ten\u00b7de", "Ge\u00b7sch\u00f6pf\u00b7f'", "!", "O", "Him\u00b7mel", "!", "wel\u00b7che", "Hee\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "NN", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wie weit geht ihre Gr\u00f6\u00df und ungeheure Zahl!", "tokens": ["Wie", "weit", "geht", "ih\u00b7re", "Gr\u00f6\u00df", "und", "un\u00b7ge\u00b7heu\u00b7re", "Zahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie starck ihr nahes Feur, wie hell ihr Glantz und Strahl!", "tokens": ["Wie", "starck", "ihr", "na\u00b7hes", "Feur", ",", "wie", "hell", "ihr", "Glantz", "und", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn man begreifft, da\u00df in saphirnen Blauen,", "tokens": ["Wenn", "man", "be\u00b7greifft", ",", "da\u00df", "in", "sa\u00b7phir\u00b7nen", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durch so viel Million- und Millionen Meilen,", "tokens": ["Durch", "so", "viel", "Mil\u00b7li\u00b7o\u00b7n", "und", "Mil\u00b7lion\u00b7en", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "TRUNC", "KON", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Man doch von einem Stern das Licht vermag zu schauen.", "tokens": ["Man", "doch", "von", "ei\u00b7nem", "Stern", "das", "Licht", "ver\u00b7mag", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was f\u00fcr ein helles Licht, was f\u00fcr ein strenger Schein", "tokens": ["Was", "f\u00fcr", "ein", "hel\u00b7les", "Licht", ",", "was", "f\u00fcr", "ein", "stren\u00b7ger", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mu\u00df in denselbigem geh\u00e4uffet seyn?", "tokens": ["Mu\u00df", "in", "den\u00b7sel\u00b7bi\u00b7gem", "ge\u00b7h\u00e4uf\u00b7fet", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PDS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "O ungemessne Tieff\u2019 und H\u00f6h\u2019 der h\u00f6chsten Macht!", "tokens": ["O", "un\u00b7ge\u00b7mess\u00b7ne", "Tief\u00b7f'", "und", "H\u00f6h'", "der", "h\u00f6chs\u00b7ten", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Wenn man von jedem Stern gedencket,", "tokens": ["Wenn", "man", "von", "je\u00b7dem", "Stern", "ge\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df er in Wirbel sich, als eine Sonne, lencket,", "tokens": ["Da\u00df", "er", "in", "Wir\u00b7bel", "sich", ",", "als", "ei\u00b7ne", "Son\u00b7ne", ",", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PRF", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und da\u00df die Wirbel dann, den unsern gleich geacht:", "tokens": ["Und", "da\u00df", "die", "Wir\u00b7bel", "dann", ",", "den", "un\u00b7sern", "gleich", "ge\u00b7acht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "O! wie viel Sonnen hat denn GOTT hervorgebracht!", "tokens": ["O", "!", "wie", "viel", "Son\u00b7nen", "hat", "denn", "GoTT", "her\u00b7vor\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PIAT", "NN", "VAFIN", "ADV", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}