{"textgrid.poem.24068": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da noch Blut in meinen Adern ist", "tokens": ["Da", "noch", "Blut", "in", "mei\u00b7nen", "A\u00b7dern", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "NN", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und Kraftspannen in meinen Muskeln,", "tokens": ["Und", "Kraft\u00b7span\u00b7nen", "in", "mei\u00b7nen", "Mus\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Will ich lieben, \u2013 lieben wie ein seliger Gott und ein gesundes Tier.", "tokens": ["Will", "ich", "lie\u00b7ben", ",", "\u2013", "lie\u00b7ben", "wie", "ein", "se\u00b7li\u00b7ger", "Gott", "und", "ein", "ge\u00b7sun\u00b7des", "Tier", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "$(", "VVINF", "KOKOM", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Die faule Furcht der Menschheit blas ich hinweg", "tokens": ["Die", "fau\u00b7le", "Furcht", "der", "Menschheit", "blas", "ich", "hin\u00b7weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit meinem Odem voll rasender Sehnsucht.", "tokens": ["Mit", "mei\u00b7nem", "O\u00b7dem", "voll", "ra\u00b7sen\u00b7der", "Sehn\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Meine dr\u00e4ngende Brust hebt sich nach den bebenden", "tokens": ["Mei\u00b7ne", "dr\u00e4n\u00b7gen\u00b7de", "Brust", "hebt", "sich", "nach", "den", "be\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA"], "meter": "+-+--++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Vollen Br\u00fcsten unendlicher Hingabe.", "tokens": ["Vol\u00b7len", "Br\u00fcs\u00b7ten", "un\u00b7end\u00b7li\u00b7cher", "Hin\u00b7ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Zwingen will ich den ausweichenden Blick", "tokens": ["Zwin\u00b7gen", "will", "ich", "den", "aus\u00b7wei\u00b7chen\u00b7den", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Sehnender Weichheit.", "tokens": ["Seh\u00b7nen\u00b7der", "Weich\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Her zu mir alle, ihr Liebeskr\u00e4ftigen,", "tokens": ["Her", "zu", "mir", "al\u00b7le", ",", "ihr", "Lie\u00b7bes\u00b7kr\u00e4f\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PIS", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ich will euch umarmen.", "tokens": ["Ich", "will", "euch", "um\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Wer aber liebesfeige ist, der gehe hin und ers\u00e4ufe sich", "tokens": ["Wer", "a\u00b7ber", "lie\u00b7be\u00b7sfei\u00b7ge", "ist", ",", "der", "ge\u00b7he", "hin", "und", "er\u00b7s\u00e4u\u00b7fe", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADJA", "VAFIN", "$,", "PRELS", "VVFIN", "PTKVZ", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "In veilchenfarbener Tinte.", "tokens": ["In", "veil\u00b7chen\u00b7far\u00b7be\u00b7ner", "Tin\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Seinem Tode will ich ein Tanzlied singen.", "tokens": ["Sei\u00b7nem", "To\u00b7de", "will", "ich", "ein", "Tanz\u00b7lied", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.8": {"line.1": {"text": "Sela.", "tokens": ["Se\u00b7la", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}