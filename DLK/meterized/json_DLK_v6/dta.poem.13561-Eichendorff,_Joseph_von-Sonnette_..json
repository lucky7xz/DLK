{"dta.poem.13561": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Tritt erst die Lieb' auf seine bl\u00fch'nden H\u00fcgel,", "tokens": ["Tritt", "erst", "die", "Lieb'", "auf", "sei\u00b7ne", "bl\u00fch'n\u00b7den", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchlt er die reichen Kr\u00e4nze in den Haaren,", "tokens": ["F\u00fchlt", "er", "die", "rei\u00b7chen", "Kr\u00e4n\u00b7ze", "in", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Morgenroth mu\u00df sich die Erde schm\u00fccken;", "tokens": ["Mit", "Mor\u00b7gen\u00b7roth", "mu\u00df", "sich", "die", "Er\u00b7de", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}