{"textgrid.poem.62562": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Tage St. Stephani", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, der Stephanum entz\u00fccket,", "tokens": ["Du", ",", "der", "Ste\u00b7pha\u00b7num", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er sterbend dich erblicket,", "tokens": ["Da\u00df", "er", "ster\u00b7bend", "dich", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Marter nicht gef\u00fchlt,", "tokens": ["Und", "die", "Mar\u00b7ter", "nicht", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Steh' mir bei in meinen N\u00f6then,", "tokens": ["Steh'", "mir", "bei", "in", "mei\u00b7nen", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei es Qu\u00e4len, sei es T\u00f6dten,", "tokens": ["Sei", "es", "Qu\u00e4\u00b7len", ",", "sei", "es", "T\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was den Ha\u00df des Feindes k\u00fchlt \u2013", "tokens": ["Was", "den", "Ha\u00df", "des", "Fein\u00b7des", "k\u00fchlt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Tritt ein Saulus mir entgegen,", "tokens": ["Tritt", "ein", "Sau\u00b7lus", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oeffne mir des Himmels H\u00f6h'n,", "tokens": ["Oeff\u00b7ne", "mir", "des", "Him\u00b7mels", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "La\u00df mich unter seinen Schl\u00e4gen", "tokens": ["La\u00df", "mich", "un\u00b7ter", "sei\u00b7nen", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dich zur Rechten Gottes sehn!", "tokens": ["Dich", "zur", "Rech\u00b7ten", "Got\u00b7tes", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}