{"textgrid.poem.47294": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ich des Lebens Lust und Leid erfuhr,", "tokens": ["Da", "ich", "des", "Le\u00b7bens", "Lust", "und", "Leid", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz vermag zu z\u00fcrnen und zu lieben,", "tokens": ["Mein", "Herz", "ver\u00b7mag", "zu", "z\u00fcr\u00b7nen", "und", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu mir vernehmlich redet die Natur,", "tokens": ["Zu", "mir", "ver\u00b7nehm\u00b7lich", "re\u00b7det", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir jede Sprache lebt, die Menschen schrieben;", "tokens": ["Mir", "je\u00b7de", "Spra\u00b7che", "lebt", ",", "die", "Men\u00b7schen", "schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und alles, das ich nicht zu denken nur,", "tokens": ["Und", "al\u00b7les", ",", "das", "ich", "nicht", "zu", "den\u00b7ken", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "PTKNEG", "PTKZU", "VVINF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auch auszusprechen f\u00fchle mich getrieben:", "tokens": ["Auch", "aus\u00b7zu\u00b7spre\u00b7chen", "f\u00fch\u00b7le", "mich", "ge\u00b7trie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie sollt' ich nicht, zum Trotz den Splitterrichtern,", "tokens": ["Wie", "sollt'", "ich", "nicht", ",", "zum", "Trotz", "den", "Split\u00b7ter\u00b7rich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mich selber z\u00e4hlen zu den wahren Dichtern?", "tokens": ["Mich", "sel\u00b7ber", "z\u00e4h\u00b7len", "zu", "den", "wah\u00b7ren", "Dich\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}