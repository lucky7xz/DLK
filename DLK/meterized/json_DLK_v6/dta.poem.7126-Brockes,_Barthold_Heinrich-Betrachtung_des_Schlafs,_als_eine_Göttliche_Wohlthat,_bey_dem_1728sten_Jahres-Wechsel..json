{"dta.poem.7126": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie angenehm nun aber auch die Ruh;", "tokens": ["Wie", "an\u00b7ge\u00b7nehm", "nun", "a\u00b7ber", "auch", "die", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So ist jedoch ein neues Wunder-Werck,", "tokens": ["So", "ist", "je\u00b7doch", "ein", "neu\u00b7es", "Wun\u00b7der\u00b7\u00b7Werck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wohl verdient, da\u00df ich es hier bemerck,", "tokens": ["So", "wohl", "ver\u00b7dient", ",", "da\u00df", "ich", "es", "hier", "be\u00b7merck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "KOUS", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df, ob sie noch so s\u00fc\u00df; sie doch nicht immerzu,", "tokens": ["Da\u00df", ",", "ob", "sie", "noch", "so", "s\u00fc\u00df", ";", "sie", "doch", "nicht", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "$.", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und nur gewisse Zeit bey Thier und Menschen w\u00e4hret,", "tokens": ["Und", "nur", "ge\u00b7wis\u00b7se", "Zeit", "bey", "Thier", "und", "Men\u00b7schen", "w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Indem uns allen ja ein \u00fcberm\u00e4\u00dfigs Liegen", "tokens": ["In\u00b7dem", "uns", "al\u00b7len", "ja", "ein", "\u00fc\u00b7berm\u00b7\u00e4\u00b7\u00dfigs", "Lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht Ruh, nicht Lust, Erquickung noch Vergn\u00fcgen", "tokens": ["Nicht", "Ruh", ",", "nicht", "Lust", ",", "Er\u00b7quic\u00b7kung", "noch", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Erweckt und schafft, nein, uns vielmehr beschwert:", "tokens": ["Er\u00b7weckt", "und", "schafft", ",", "nein", ",", "uns", "viel\u00b7mehr", "be\u00b7schwert", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,", "PTKANT", "$,", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So mein bewundernd Hertz denn zum Bewei\u00dfthum nimmt,", "tokens": ["So", "mein", "be\u00b7wun\u00b7dernd", "Hertz", "denn", "zum", "Be\u00b7wei\u00df\u00b7thum", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJD", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df wir zum M\u00fc\u00dfiggang und Faulheit nicht bestimmt;", "tokens": ["Da\u00df", "wir", "zum", "M\u00fc\u00b7\u00dfig\u00b7gang", "und", "Faul\u00b7heit", "nicht", "be\u00b7stimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hingegen, da\u00df von einem weisen Wesen,", "tokens": ["Hin\u00b7ge\u00b7gen", ",", "da\u00df", "von", "ei\u00b7nem", "wei\u00b7sen", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wir, ordentlich zu w\u00fcrcken auserlesen.", "tokens": ["Wir", ",", "or\u00b7dent\u00b7lich", "zu", "w\u00fcr\u00b7cken", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Denn, w\u00e4re nicht ein Ziel der Ruh uns angeschaffen;", "tokens": ["Denn", ",", "w\u00e4\u00b7re", "nicht", "ein", "Ziel", "der", "Ruh", "uns", "an\u00b7ge\u00b7schaf\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So w\u00fcrd\u2019 ein grosses Theil der Menschen immer schlaffen.", "tokens": ["So", "w\u00fcrd'", "ein", "gros\u00b7ses", "Theil", "der", "Men\u00b7schen", "im\u00b7mer", "schlaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}