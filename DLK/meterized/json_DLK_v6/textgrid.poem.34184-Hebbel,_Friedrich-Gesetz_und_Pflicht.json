{"textgrid.poem.34184": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gesetz und Pflicht", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Gesetz erf\u00fcllst du und glaubst schon der Pflicht zu gen\u00fcgen?", "tokens": ["Das", "Ge\u00b7setz", "er\u00b7f\u00fcllst", "du", "und", "glaubst", "schon", "der", "Pflicht", "zu", "ge\u00b7n\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was der Galgen beherrscht, w\u00e4r' das Gebiet der Moral?", "tokens": ["Was", "der", "Gal\u00b7gen", "be\u00b7herrscht", ",", "w\u00e4r'", "das", "Ge\u00b7biet", "der", "Mo\u00b7ral", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Freund, der himmlische Richter wird nimmer schon darum dich kr\u00f6nen,", "tokens": ["Freund", ",", "der", "himm\u00b7li\u00b7sche", "Rich\u00b7ter", "wird", "nim\u00b7mer", "schon", "da\u00b7rum", "dich", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "PAV", "PPER", "VVINF", "$,"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Weil dich der ird'sche nicht hing! Also erweit're den Kreis!", "tokens": ["Weil", "dich", "der", "ird'\u00b7sche", "nicht", "hing", "!", "Al\u00b7so", "er\u00b7weit'\u00b7re", "den", "Kreis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "PTKNEG", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}}}}