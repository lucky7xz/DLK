{"dta.poem.21116": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, &#8212; ich nenne Dich so auch vor den Augen\nder Welt,", "tokens": ["Freund", ",", "&#8212;", "ich", "nen\u00b7ne", "Dich", "so", "auch", "vor", "den", "Au\u00b7gen", "der", "Welt", ","], "token_info": ["word", "punct", "XML_entity", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "$(", "PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als Dich mein hingerissenes Herz", "tokens": ["Als", "Dich", "mein", "hin\u00b7ge\u00b7ris\u00b7se\u00b7nes", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Jm sympathetischen Zug der ersten Wallungen nannte,", "tokens": ["Jm", "sym\u00b7pa\u00b7the\u00b7ti\u00b7schen", "Zug", "der", "ers\u00b7ten", "Wal\u00b7lun\u00b7gen", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die meine durchdrungene Seele gef\u00fchlt.", "tokens": ["Die", "mei\u00b7ne", "durch\u00b7drun\u00b7ge\u00b7ne", "See\u00b7le", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Denn sie kante Dich schon, da ich zuerst Dich erblickte,", "tokens": ["Denn", "sie", "kan\u00b7te", "Dich", "schon", ",", "da", "ich", "zu\u00b7erst", "Dich", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Als h\u00e4tten wir uns seit Aeonen gesehn.", "tokens": ["Als", "h\u00e4t\u00b7ten", "wir", "uns", "seit", "A\u00b7e\u00b7o\u00b7nen", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Welch ein seliger Tag war nicht am Leinenstrand der,", "tokens": ["Welch", "ein", "se\u00b7li\u00b7ger", "Tag", "war", "nicht", "am", "Lei\u00b7nen\u00b7strand", "der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "VAFIN", "PTKNEG", "APPRART", "NN", "ART", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Da unsre Herzen zuerst sich vereint!", "tokens": ["Da", "uns\u00b7re", "Her\u00b7zen", "zu\u00b7erst", "sich", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als wir in himmlischer Luft, in einem l\u00e4ndlichen Gar-", "tokens": ["Als", "wir", "in", "himm\u00b7li\u00b7scher", "Luft", ",", "in", "ei\u00b7nem", "l\u00e4nd\u00b7li\u00b7chen", "Ga\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Die g\u00f6ttliche Freundschaft auf hellem Gew\u00f6lk", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Freund\u00b7schaft", "auf", "hel\u00b7lem", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "L\u00e4chelnd \u00fcber uns sahn, wie sie mit blumichten Van-", "tokens": ["L\u00e4\u00b7chelnd", "\u00fc\u00b7ber", "uns", "sahn", ",", "wie", "sie", "mit", "blu\u00b7mich\u00b7ten", "Van"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$,", "PWAV", "PPER", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "Die sich gefundnen Seelen umzog.", "tokens": ["Die", "sich", "ge\u00b7fund\u00b7nen", "See\u00b7len", "um\u00b7zog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Liebt euch z\u00e4rtlich und treu! (so sprach harmonisch ihr", "tokens": ["Liebt", "euch", "z\u00e4rt\u00b7lich", "und", "treu", "!", "(", "so", "sprach", "har\u00b7mo\u00b7nisch", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "$(", "ADV", "VVFIN", "ADJD", "PPOSAT"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jhr wart l\u00e4ngst f\u00fcr einander bestimmt.", "tokens": ["Ihr", "wart", "l\u00e4ngst", "f\u00fcr", "ein\u00b7an\u00b7der", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich floh vom stralenden Tand, und von dem P\u00f6bel in", "tokens": ["Ich", "floh", "vom", "stra\u00b7len\u00b7den", "Tand", ",", "und", "von", "dem", "P\u00f6\u00b7bel", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,", "KON", "APPR", "ART", "NN", "APPR"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der meine holdseligen Freuden nicht schmeckt.", "tokens": ["Der", "mei\u00b7ne", "hold\u00b7se\u00b7li\u00b7gen", "Freu\u00b7den", "nicht", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Bey dem m\u00e4chtigen Thron gieng ich unsichtbar vor-", "tokens": ["Bey", "dem", "m\u00e4ch\u00b7ti\u00b7gen", "Thron", "gieng", "ich", "un\u00b7sicht\u00b7bar", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "TRUNC"], "meter": "+-+--++-+--+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und schenk euch im Tempel der Musen mein", "tokens": ["Und", "schenk", "euch", "im", "Tem\u00b7pel", "der", "Mu\u00b7sen", "mein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PPOSAT"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Nicht vergebens winkt euch durch jenen heiligen Hain", "tokens": ["Nicht", "ver\u00b7ge\u00b7bens", "winkt", "euch", "durch", "je\u00b7nen", "hei\u00b7li\u00b7gen", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die hohe Dichtkunst in sp\u00e4tere Welt.", "tokens": ["Die", "ho\u00b7he", "Dicht\u00b7kunst", "in", "sp\u00e4\u00b7te\u00b7re", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie giebt euch auch nicht umsonst die hohe melodische", "tokens": ["Sie", "giebt", "euch", "auch", "nicht", "um\u00b7sonst", "die", "ho\u00b7he", "me\u00b7lo\u00b7di\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "F\u00fcr jeden in gl\u00fccklichem Gleichlaut gestimmt.", "tokens": ["F\u00fcr", "je\u00b7den", "in", "gl\u00fcck\u00b7li\u00b7chem", "Gleich\u00b7laut", "ge\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Singt die Freundschaft darauf, das gr\u00f6\u00dfte Geschenke", "tokens": ["Singt", "die", "Freund\u00b7schaft", "da\u00b7rauf", ",", "das", "gr\u00f6\u00df\u00b7te", "Ge\u00b7schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PAV", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Das von dem Menschen zum Engel erhebt.", "tokens": ["Das", "von", "dem", "Men\u00b7schen", "zum", "En\u00b7gel", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wir umarmten uns, Freund, und sahn mit f\u00fchlendem", "tokens": ["Wir", "um\u00b7arm\u00b7ten", "uns", ",", "Freund", ",", "und", "sahn", "mit", "f\u00fch\u00b7len\u00b7dem"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der holden G\u00f6ttin im Stralenweg nach.", "tokens": ["Der", "hol\u00b7den", "G\u00f6t\u00b7tin", "im", "Stra\u00b7len\u00b7weg", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Musen und Grazien Chor schlo\u00df uns in l\u00e4chelnde", "tokens": ["Der", "Mu\u00b7sen", "und", "Gra\u00b7zi\u00b7en", "Chor", "schlo\u00df", "uns", "in", "l\u00e4\u00b7cheln\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "NN", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "-+--+--++-++--", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Dichtkunst gab uns gef\u00e4llig die Hand.", "tokens": ["Die", "Dicht\u00b7kunst", "gab", "uns", "ge\u00b7f\u00e4l\u00b7lig", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und sie reichte Dir, Freund, die m\u00e4chtigt\u00f6nende Leyer,", "tokens": ["Und", "sie", "reich\u00b7te", "Dir", ",", "Freund", ",", "die", "m\u00e4ch\u00b7tig\u00b7t\u00f6\u00b7nen\u00b7de", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Die noch dem Kenner in Nachwelten schallt.", "tokens": ["Die", "noch", "dem", "Ken\u00b7ner", "in", "Nach\u00b7wel\u00b7ten", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich war lauter Gef\u00fchl, als deine zaubernde Hand", "tokens": ["Ich", "war", "lau\u00b7ter", "Ge\u00b7f\u00fchl", ",", "als", "dei\u00b7ne", "zau\u00b7bern\u00b7de", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-++--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die reinen silbernen Saiten durchflog.", "tokens": ["Die", "rei\u00b7nen", "sil\u00b7ber\u00b7nen", "Sai\u00b7ten", "durch\u00b7flog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erstaunend sah ich, wie schnell Du Harmonien geler-", "tokens": ["Er\u00b7stau\u00b7nend", "sah", "ich", ",", "wie", "schnell", "Du", "Har\u00b7mo\u00b7ni\u00b7en", "ge\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "PPER", "NE", "TRUNC"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nur einem Haller und Klopstock bekant.", "tokens": ["Nur", "ei\u00b7nem", "Hal\u00b7ler", "und", "Klops\u00b7tock", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kaum gedachte mein Stolz des Lehrlings T\u00f6ne zu h\u00f6-", "tokens": ["Kaum", "ge\u00b7dach\u00b7te", "mein", "Stolz", "des", "Lehr\u00b7lings", "T\u00f6\u00b7ne", "zu", "h\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "APPR", "TRUNC"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und ihn best\u00fcrzte des Meisters Gesang.", "tokens": ["Und", "ihn", "be\u00b7st\u00fcrz\u00b7te", "des", "Meis\u00b7ters", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Furchtsam sing ich Dir ietzt \u2014 Denn eines Pultes", "tokens": ["Furcht\u00b7sam", "sing", "ich", "Dir", "ietzt", "Denn", "ei\u00b7nes", "Pul\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ADV", "$(", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und einer eroberten Locke Homer,", "tokens": ["Und", "ei\u00b7ner", "er\u00b7o\u00b7ber\u00b7ten", "Lo\u00b7cke", "Ho\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hat mich vielleicht nur umsonst mit hohen T\u00f6nen ent-", "tokens": ["Hat", "mich", "viel\u00b7leicht", "nur", "um\u00b7sonst", "mit", "ho\u00b7hen", "T\u00f6\u00b7nen", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Die unnachahmlich dem Deutschen noch sind.", "tokens": ["Die", "un\u00b7nach\u00b7ahm\u00b7lich", "dem", "Deut\u00b7schen", "noch", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Doch der Beyfall von Dir soll meine K\u00fchnheit bede-", "tokens": ["Doch", "der", "Bey\u00b7fall", "von", "Dir", "soll", "mei\u00b7ne", "K\u00fchn\u00b7heit", "be\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VMFIN", "PPOSAT", "NN", "TRUNC"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Mit der ich zu schwindelnden Pfaden geklimmt.", "tokens": ["Mit", "der", "ich", "zu", "schwin\u00b7deln\u00b7den", "Pfa\u00b7den", "ge\u00b7klimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Blicke g\u00fctig auf mich von jenen umleuchteten H\u00f6hn,", "tokens": ["Bli\u00b7cke", "g\u00fc\u00b7tig", "auf", "mich", "von", "je\u00b7nen", "um\u00b7leuch\u00b7te\u00b7ten", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PRF", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Auf die Dich die g\u00fcnstige Muse gef\u00fchrt.", "tokens": ["Auf", "die", "Dich", "die", "g\u00fcns\u00b7ti\u00b7ge", "Mu\u00b7se", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relax