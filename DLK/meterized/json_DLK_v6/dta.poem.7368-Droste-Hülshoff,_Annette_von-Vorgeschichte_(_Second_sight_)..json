{"dta.poem.7368": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Vorgeschichte (   Second sight   ).", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Kennst du die Blassen im Haideland,", "tokens": ["Kennst", "du", "die", "Blas\u00b7sen", "im", "Hai\u00b7de\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mit blonden fl\u00e4chsenen Haaren?", "tokens": ["Mit", "blon\u00b7den", "fl\u00e4ch\u00b7se\u00b7nen", "Haa\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit Augen so klar wie an Weihers Rand", "tokens": ["Mit", "Au\u00b7gen", "so", "klar", "wie", "an", "Wei\u00b7hers", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ADJD", "KOKOM", "APPR", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Blitze der Welle fahren?", "tokens": ["Die", "Blit\u00b7ze", "der", "Wel\u00b7le", "fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O sprich ein Gebet, inbr\u00fcnstig, \u00e4cht,", "tokens": ["O", "sprich", "ein", "Ge\u00b7bet", ",", "in\u00b7br\u00fcns\u00b7tig", ",", "\u00e4cht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "F\u00fcr die Seher der Nacht, das gequ\u00e4lte Geschlecht.", "tokens": ["F\u00fcr", "die", "Se\u00b7her", "der", "Nacht", ",", "das", "ge\u00b7qu\u00e4l\u00b7te", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "So klar die L\u00fcfte, am Aether rein", "tokens": ["So", "klar", "die", "L\u00fcf\u00b7te", ",", "am", "A\u00b7e\u00b7ther", "rein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tr\u00e4umt nicht die zarteste Flocke,", "tokens": ["Tr\u00e4umt", "nicht", "die", "zar\u00b7tes\u00b7te", "Flo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Der Vollmond lagert den blauen Schein", "tokens": ["Der", "Voll\u00b7mond", "la\u00b7gert", "den", "blau\u00b7en", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf des schlafenden Freiherrn Locke,", "tokens": ["Auf", "des", "schla\u00b7fen\u00b7den", "Frei\u00b7herrn", "Lo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Hernieder bohrend in kalter Kraft", "tokens": ["Her\u00b7nie\u00b7der", "boh\u00b7rend", "in", "kal\u00b7ter", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Vampyrzunge, des Strahles Schaft.", "tokens": ["Die", "Vam\u00b7pyr\u00b7zun\u00b7ge", ",", "des", "Strah\u00b7les", "Schaft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Schl\u00e4fer st\u00f6hnt, ein Traum voll Noth", "tokens": ["Der", "Schl\u00e4\u00b7fer", "st\u00f6hnt", ",", "ein", "Traum", "voll", "Noth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Scheint seine Sinne zu qu\u00e4len,", "tokens": ["Scheint", "sei\u00b7ne", "Sin\u00b7ne", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es zuckt die Wimper, ein leises Roth", "tokens": ["Es", "zuckt", "die", "Wim\u00b7per", ",", "ein", "lei\u00b7ses", "Roth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Will \u00fcber die Wange sich stehlen;", "tokens": ["Will", "\u00fc\u00b7ber", "die", "Wan\u00b7ge", "sich", "steh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Schau, wie er woget und rudert und f\u00e4hrt,", "tokens": ["Schau", ",", "wie", "er", "wo\u00b7get", "und", "ru\u00b7dert", "und", "f\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Wie Einer so gegen den Strom sich wehrt.", "tokens": ["Wie", "Ei\u00b7ner", "so", "ge\u00b7gen", "den", "Strom", "sich", "wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Nun zuckt er auf \u2014 ob ihn getr\u00e4umt,", "tokens": ["Nun", "zuckt", "er", "auf", "ob", "ihn", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "$(", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht kann er sich dessen entsinnen \u2014", "tokens": ["Nicht", "kann", "er", "sich", "des\u00b7sen", "ent\u00b7sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "PRF", "PDS", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ihn fr\u00f6stelt, fr\u00f6stelt, ob's drinnen sch\u00e4umt", "tokens": ["Ihn", "fr\u00f6s\u00b7telt", ",", "fr\u00f6s\u00b7telt", ",", "ob's", "drin\u00b7nen", "sch\u00e4umt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "ADV", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie Fluthen zum Strudel rinnen;", "tokens": ["Wie", "Flut\u00b7hen", "zum", "Stru\u00b7del", "rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Was ihn ge\u00e4ngstet, er wei\u00df es auch:", "tokens": ["Was", "ihn", "ge\u00b7\u00e4ngs\u00b7tet", ",", "er", "wei\u00df", "es", "auch", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es war des Mondes giftiger Hauch.", "tokens": ["Es", "war", "des", "Mon\u00b7des", "gif\u00b7ti\u00b7ger", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "O Fluch der Haide, gleich Ahasver", "tokens": ["O", "Fluch", "der", "Hai\u00b7de", ",", "gleich", "A\u00b7has\u00b7ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "ART", "NE", "$,", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unter'm Nachtgestirne zu kreisen!", "tokens": ["Un\u00b7ter'm", "Nacht\u00b7ges\u00b7tir\u00b7ne", "zu", "krei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wenn seiner Strahlen z\u00fcngelndes Meer", "tokens": ["Wenn", "sei\u00b7ner", "Strah\u00b7len", "z\u00fcn\u00b7geln\u00b7des", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Aufbohret der Seele Schleusen,", "tokens": ["Auf\u00b7boh\u00b7ret", "der", "See\u00b7le", "Schleu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und der Prophet, ein verzweifelnd Wild,", "tokens": ["Und", "der", "Pro\u00b7phet", ",", "ein", "ver\u00b7zwei\u00b7felnd", "Wild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "VVPP", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "K\u00e4mpft gegen das m\u00e4hlig steigende Bild.", "tokens": ["K\u00e4mpft", "ge\u00b7gen", "das", "m\u00e4h\u00b7lig", "stei\u00b7gen\u00b7de", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Im Mantel schaudernd mi\u00dft das Parquet", "tokens": ["Im", "Man\u00b7tel", "schau\u00b7dernd", "mi\u00dft", "das", "Par\u00b7quet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Freiherr die L\u00e4ng' und Breite,", "tokens": ["Der", "Frei\u00b7herr", "die", "L\u00e4ng'", "und", "Brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und wo am Boden ein Schimmer steht,", "tokens": ["Und", "wo", "am", "Bo\u00b7den", "ein", "Schim\u00b7mer", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Weitaus er beuget zur Seite,", "tokens": ["Weit\u00b7aus", "er", "beu\u00b7get", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er hat einen Willen und hat eine Kraft,", "tokens": ["Er", "hat", "ei\u00b7nen", "Wil\u00b7len", "und", "hat", "ei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die sollen nicht liegen in Blutes Haft.", "tokens": ["Die", "sol\u00b7len", "nicht", "lie\u00b7gen", "in", "Blu\u00b7tes", "Haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Es will ihn krallen, es saugt ihn an,", "tokens": ["Es", "will", "ihn", "kral\u00b7len", ",", "es", "saugt", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo Glanz die Scheiben umgleitet,", "tokens": ["Wo", "Glanz", "die", "Schei\u00b7ben", "um\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch langsam weichend, Spann' um Spann',", "tokens": ["Doch", "lang\u00b7sam", "wei\u00b7chend", ",", "Spann'", "um", "Spann'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ein wunder Edelhirsch schreitet,", "tokens": ["Wie", "ein", "wun\u00b7der", "E\u00b7del\u00b7hirsch", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In immer engerem Kreis gehetzt,", "tokens": ["In", "im\u00b7mer", "en\u00b7ge\u00b7rem", "Kreis", "ge\u00b7hetzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Des Lagers Pfosten ergreift er zuletzt.", "tokens": ["Des", "La\u00b7gers", "Pfos\u00b7ten", "er\u00b7greift", "er", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Da steht er keuchend, sinnt und sinnt,", "tokens": ["Da", "steht", "er", "keu\u00b7chend", ",", "sinnt", "und", "sinnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die m\u00fcde Seele zu laben,", "tokens": ["Die", "m\u00fc\u00b7de", "See\u00b7le", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denkt an sein liebes einziges Kind,", "tokens": ["Denkt", "an", "sein", "lie\u00b7bes", "ein\u00b7zi\u00b7ges", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seinen zarten, schw\u00e4chlichen Knaben,", "tokens": ["Sei\u00b7nen", "zar\u00b7ten", ",", "schw\u00e4ch\u00b7li\u00b7chen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ob dessen Leben des Vaters Gebet", "tokens": ["Ob", "des\u00b7sen", "Le\u00b7ben", "des", "Va\u00b7ters", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRELAT", "NN", "ART", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie eine zitternde Flamme steht.", "tokens": ["Wie", "ei\u00b7ne", "zit\u00b7tern\u00b7de", "Flam\u00b7me", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Hat er des Kleinen Stammbaum doch", "tokens": ["Hat", "er", "des", "Klei\u00b7nen", "Stamm\u00b7baum", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gestellt an des Lagers Ende,", "tokens": ["Ge\u00b7stellt", "an", "des", "La\u00b7gers", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nach dem Abendkusse und Segen noch", "tokens": ["Nach", "dem", "A\u00b7bend\u00b7kus\u00b7se", "und", "Se\u00b7gen", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN", "ADV"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dr\u00fcber br\u00fcnstig zu falten die H\u00e4nde;", "tokens": ["Dr\u00fc\u00b7ber", "br\u00fcns\u00