{"dta.poem.10780": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "An ein gewisse Jungfraw/ da\u00df sie nit mehr  \n nach jhrem Allerliebsten sehen soll.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht also festiglich mit brennendem verlangen", "tokens": ["Nicht", "al\u00b7so", "fes\u00b7tig\u00b7lich", "mit", "bren\u00b7nen\u00b7dem", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vber den Berg ", "tokens": ["Vber", "den", "Berg"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Als der au\u00df sonderm trieb sein flammenreich Gesicht", "tokens": ["Als", "der", "au\u00df", "son\u00b7derm", "trieb", "sein", "flam\u00b7men\u00b7reich", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "PIS", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auff den zierlichen Schlaff ", "tokens": ["Auff", "den", "zier\u00b7li\u00b7chen", "Schlaff"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wie ewer Augen glantz mit scharffen stralen spielet/", "tokens": ["Wie", "e\u00b7wer", "Au\u00b7gen", "glantz", "mit", "scharf\u00b7fen", "stra\u00b7len", "spie\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd vnablessiglich durchs offne Fenster zielet", "tokens": ["Vnd", "vnab\u00b7les\u00b7sig\u00b7lich", "durchs", "off\u00b7ne", "Fens\u00b7ter", "zie\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Vff ewers Buhlen Bart/ vom Eisen krau\u00df gemacht/", "tokens": ["Vff", "e\u00b7wers", "Buh\u00b7len", "Bart", "/", "vom", "Ei\u00b7sen", "krau\u00df", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "APPRART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der jhm Jungfrawen Lieb zuwegen hat gebracht/", "tokens": ["Der", "jhm", "Jung\u00b7fra\u00b7wen", "Lieb", "zu\u00b7we\u00b7gen", "hat", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "NN", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schont ewer selbst/ thut nicht so vberfl\u00fcssig spitzen", "tokens": ["Schont", "e\u00b7wer", "selbst", "/", "thut", "nicht", "so", "vberf\u00b7l\u00fcs\u00b7sig", "spit\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADV", "$(", "VVFIN", "PTKNEG", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Das klar Gesicht/ last ab von ewrem vielen sitzen/", "tokens": ["Das", "klar", "Ge\u00b7sicht", "/", "last", "ab", "von", "ew\u00b7rem", "vie\u00b7len", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "$(", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Rame wird vernutzt/ vnd wunderlich versehrt/", "tokens": ["Die", "Ra\u00b7me", "wird", "ver\u00b7nutzt", "/", "vnd", "wun\u00b7der\u00b7lich", "ver\u00b7sehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Simster ferner jhr vergeblich nur beschwert/", "tokens": ["Die", "Sims\u00b7ter", "fer\u00b7ner", "jhr", "ver\u00b7geb\u00b7lich", "nur", "be\u00b7schwert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gar weit wird vber Meer der Nort ewrn Buhlen f\u00fchren/", "tokens": ["Gar", "weit", "wird", "vber", "Meer", "der", "Nort", "ewrn", "Buh\u00b7len", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "NN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Juner de\u00df Fensters jhr nun euch m\u00fcst erlustieren/", "tokens": ["Ju\u00b7ner", "de\u00df", "Fens\u00b7ters", "jhr", "nun", "euch", "m\u00fcst", "er\u00b7lus\u00b7tie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADV", "PPER", "VMFIN", "VVINF", "$("], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "Mit seinem Bildnu\u00df/ das voran ins buch gesetzt", "tokens": ["Mit", "sei\u00b7nem", "Bild\u00b7nu\u00df", "/", "das", "vo\u00b7ran", "ins", "buch", "ge\u00b7setzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PDS", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In sehr k\u00f6stlichem bund verlassen euch zur letzt.", "tokens": ["In", "sehr", "k\u00f6st\u00b7li\u00b7chem", "bund", "ver\u00b7las\u00b7sen", "euch", "zur", "letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "ADV", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}