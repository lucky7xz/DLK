{"dta.poem.15646": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie schwer ist der Begriff von etwas zu erlangen;", "tokens": ["Wie", "schwer", "ist", "der", "Be\u00b7griff", "von", "et\u00b7was", "zu", "er\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am schwersten aber wird der von uns selbst empfangen.", "tokens": ["Am", "schwers\u00b7ten", "a\u00b7ber", "wird", "der", "von", "uns", "selbst", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "VAFIN", "ART", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum wenn du von dir selbst hast den Begriff, so halt", "tokens": ["Drum", "wenn", "du", "von", "dir", "selbst", "hast", "den", "Be\u00b7griff", ",", "so", "halt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "KOUS", "PPER", "APPR", "PPER", "ADV", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihn fest, es raube dir ihn keinerlei Gewalt.", "tokens": ["Ihn", "fest", ",", "es", "rau\u00b7be", "dir", "ihn", "kei\u00b7ner\u00b7lei", "Ge\u00b7walt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht bl\u00f6der Misverstand, noch theilnamloser Frost", "tokens": ["Nicht", "bl\u00f6\u00b7der", "Mis\u00b7ver\u00b7stand", ",", "noch", "theil\u00b7nam\u00b7lo\u00b7ser", "Frost"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besch\u00e4dige des Selbstbewu\u00dftseyns edlen Trost.", "tokens": ["Be\u00b7sch\u00e4\u00b7di\u00b7ge", "des", "Selbst\u00b7be\u00b7wu\u00df\u00b7tseyns", "ed\u00b7len", "Trost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Frisch wisse gleich dem Baum, dem wintersturmentlaubten,", "tokens": ["Frisch", "wis\u00b7se", "gleich", "dem", "Baum", ",", "dem", "win\u00b7ter\u00b7stur\u00b7ment\u00b7laub\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf bessre Zeit den Trieb im Innern zu behaupten.", "tokens": ["Auf", "bess\u00b7re", "Zeit", "den", "Trieb", "im", "In\u00b7nern", "zu", "be\u00b7haup\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}