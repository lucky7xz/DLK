, "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wenn mich in Stunden heiliger Trunkenheit", "tokens": ["Wenn", "mich", "in", "Stun\u00b7den", "hei\u00b7li\u00b7ger", "Trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Barden alter Tage besucheten,", "tokens": ["Die", "Bar\u00b7den", "al\u00b7ter", "Ta\u00b7ge", "be\u00b7su\u00b7che\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn Oscar's Vater", "tokens": ["Wenn", "Os\u00b7ca\u00b7r's", "Va\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NE", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auf deutschen Saiten l\u00e4chelnd horchte,", "tokens": ["Auf", "deut\u00b7schen", "Sai\u00b7ten", "l\u00e4\u00b7chelnd", "horch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mich verweg'ne Griffe lehrte;", "tokens": ["Und", "mich", "ver\u00b7weg'\u00b7ne", "Grif\u00b7fe", "lehr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn auf ihren Wirbeln hergetragen", "tokens": ["Wenn", "auf", "ih\u00b7ren", "Wir\u00b7beln", "her\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Werdomar und Rhingulph", "tokens": ["Wer\u00b7do\u00b7mar", "und", "Rhin\u00b7gu