{"textgrid.poem.41185": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Klagen will ich. Du g\u00f6nnst es mir endlich,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klagen will ich. Du g\u00f6nnst es mir endlich,", "tokens": ["Kla\u00b7gen", "will", "ich", ".", "Du", "g\u00f6nnst", "es", "mir", "end\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Milder gewordenes Herzeleid!", "tokens": ["Mil\u00b7der", "ge\u00b7wor\u00b7de\u00b7nes", "Her\u00b7ze\u00b7leid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Klagen will ich. Du h\u00f6rest mich, Winterhain!", "tokens": ["Kla\u00b7gen", "will", "ich", ".", "Du", "h\u00f6\u00b7rest", "mich", ",", "Win\u00b7ter\u00b7hain", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Denn bist du nicht selber ein Kl\u00e4ger?", "tokens": ["Denn", "bist", "du", "nicht", "sel\u00b7ber", "ein", "Kl\u00e4\u00b7ger", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ein Kl\u00e4ger deines abgefall'nen Laubes", "tokens": ["Ein", "Kl\u00e4\u00b7ger", "dei\u00b7nes", "ab\u00b7ge\u00b7fall'\u00b7nen", "Lau\u00b7bes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Kl\u00e4ger deiner ausgestorb'nen Schatten!", "tokens": ["Ein", "Kl\u00e4\u00b7ger", "dei\u00b7ner", "aus\u00b7ge\u00b7stor\u00b7b'\u00b7nen", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Zwar dein Klagen stillt der Lenz,", "tokens": ["Zwar", "dein", "Kla\u00b7gen", "stillt", "der", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bringt dir Laub und Schatten wieder;", "tokens": ["Bringt", "dir", "Laub", "und", "Schat\u00b7ten", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber soll dem Barden", "tokens": ["A\u00b7ber", "soll", "dem", "Bar\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Seine Freude wieder werden,", "tokens": ["Sei\u00b7ne", "Freu\u00b7de", "wie\u00b7der", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Die der Tod ihm vom Herzen ri\u00df?", "tokens": ["Die", "der", "Tod", "ihm", "vom", "Her\u00b7zen", "ri\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Klein, voll Unschuld war sie, meine Freude,", "tokens": ["Klein", ",", "voll", "Un\u00b7schuld", "war", "sie", ",", "mei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "NN", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sittsam grau war ihr Gefieder,", "tokens": ["Sitt\u00b7sam", "grau", "war", "ihr", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4nzend schwarz war ihre Scheitel.", "tokens": ["Gl\u00e4n\u00b7zend", "schwarz", "war", "ih\u00b7re", "Schei\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach ich denke noch den Tag des Herbstes,", "tokens": ["Ach", "ich", "den\u00b7ke", "noch", "den", "Tag", "des", "Herbs\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Da sie durch die falben Hecken", "tokens": ["Da", "sie", "durch", "die", "fal\u00b7ben", "He\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00fcrstend zu der Quelle strich,", "tokens": ["D\u00fcrs\u00b7tend", "zu", "der", "Quel\u00b7le", "strich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die mit meinen Mistelruthen", "tokens": ["Die", "mit", "mei\u00b7nen", "Mis\u00b7tel\u00b7ru\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rund umpflanzet war.", "tokens": ["Rund", "um\u00b7pflan\u00b7zet", "war", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Da fing ich dich, S\u00e4nger der Wipfel!", "tokens": ["Da", "fing", "ich", "dich", ",", "S\u00e4n\u00b7ger", "der", "Wip\u00b7fel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie schlug dir der Busen! wie str\u00e4ubtest du dich!", "tokens": ["Wie", "schlug", "dir", "der", "Bu\u00b7sen", "!", "wie", "str\u00e4ub\u00b7test", "du", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$.", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Denn kanntest du damal mein Herz?", "tokens": ["Denn", "kann\u00b7test", "du", "da\u00b7mal", "mein", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aber bald lehrte die freundliche Miene,", "tokens": ["A\u00b7ber", "bald", "lehr\u00b7te", "die", "freund\u00b7li\u00b7che", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Die niedliche Speise, die reinliche Pflege,", "tokens": ["Die", "nied\u00b7li\u00b7che", "Spei\u00b7se", ",", "die", "rein\u00b7li\u00b7che", "Pfle\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Mein lispelnder Mund", "tokens": ["Mein", "lis\u00b7peln\u00b7der", "Mund"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Deinen Wirth dich kennen.", "tokens": ["Dei\u00b7nen", "Wirth", "dich", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Und jetzo vertrugen dein tonvoll Geschlecht", "tokens": ["Und", "jet\u00b7zo", "ver\u00b7tru\u00b7gen", "dein", "ton\u00b7voll", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Schon zehnmal die letzteren Hauche des Herbstes", "tokens": ["Schon", "zehn\u00b7mal", "die", "letz\u00b7te\u00b7ren", "Hau\u00b7che", "des", "Herbs\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Zu w\u00e4rmeren Himmeln,", "tokens": ["Zu", "w\u00e4r\u00b7me\u00b7ren", "Him\u00b7meln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und zehnmal kehrte dein tonvoll Geschlecht", "tokens": ["Und", "zehn\u00b7mal", "kehr\u00b7te", "dein", "ton\u00b7voll", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Im ersten Hauche des Lenzes,", "tokens": ["Im", "ers\u00b7ten", "Hau\u00b7che", "des", "Len\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Und sang vom hohen Schottendorne", "tokens": ["Und", "sang", "vom", "ho\u00b7hen", "Schot\u00b7ten\u00b7dor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Den Gru\u00df in mein einsam' Gemach dir zu,", "tokens": ["Den", "Gru\u00df", "in", "mein", "ein\u00b7sam'", "Ge\u00b7mach", "dir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "PIAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und h\u00f6rte den freundlichen Dank von dir. \u2013", "tokens": ["Und", "h\u00f6r\u00b7te", "den", "freund\u00b7li\u00b7chen", "Dank", "von", "dir", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Tonvoll Geschlecht meines Entrissenen!", "tokens": ["Ton\u00b7voll", "Ge\u00b7schlecht", "mei\u00b7nes", "Ent\u00b7ris\u00b7se\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPOSAT", "NN", "$."], "meter": "+--+---+--", "measure": "dactylic.di.plus"}, "line.17": {"text": "Kehrst du wieder diesen Lenz,", "tokens": ["Kehrst", "du", "wie\u00b7der", "die\u00b7sen", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Singe nimmer deinen Gru\u00df", "tokens": ["Sin\u00b7ge", "nim\u00b7mer", "dei\u00b7nen", "Gru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Von dem hohen Schottendorne!", "tokens": ["Von", "dem", "ho\u00b7hen", "Schot\u00b7ten\u00b7dor\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Still ist mein einsam' Gemach.", "tokens": ["Still", "ist", "mein", "ein\u00b7sam'", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Ich h\u00f6re den Gru\u00df, und mir blutet das Herz!", "tokens": ["Ich", "h\u00f6\u00b7re", "den", "Gru\u00df", ",", "und", "mir", "blu\u00b7tet", "das", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Uebel vergaltst du dem Barden die Wohlthat,", "tokens": ["Ue\u00b7bel", "ver\u00b7galtst", "du", "dem", "Bar\u00b7den", "die", "Wohlt\u00b7hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Du frosterstarreter Hund!", "tokens": ["Du", "fros\u00b7ter\u00b7star\u00b7re\u00b7ter", "Hund", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dich hatt' ich in grimmiger Winternacht", "tokens": ["Dich", "hatt'", "ich", "in", "grim\u00b7mi\u00b7ger", "Win\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Unter mein w\u00e4rmendes Dach genommen.", "tokens": ["Un\u00b7ter", "mein", "w\u00e4r\u00b7men\u00b7des", "Dach", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ich kehrte zur\u00fccke.", "tokens": ["Ich", "kehr\u00b7te", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wedelnd kamst du mir entgegen,", "tokens": ["We\u00b7delnd", "kamst", "du", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit dir ahnungsvoller Schauer \u2013", "tokens": ["Und", "mit", "dir", "ah\u00b7nungs\u00b7vol\u00b7ler", "Schau\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich ri\u00df mich hinein.", "tokens": ["Ich", "ri\u00df", "mich", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Da gab die kleinste Saite", "tokens": ["Da", "gab", "die", "kleins\u00b7te", "Sai\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Meines ruhenden Harfenspiels", "tokens": ["Mei\u00b7nes", "ru\u00b7hen\u00b7den", "Har\u00b7fen\u00b7spiels"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Einen Wehlaut, dem letzten Seufzer", "tokens": ["Ei\u00b7nen", "Wehl\u00b7aut", ",", "dem", "letz\u00b7ten", "Seuf\u00b7zer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Scheidender Liebenden \u00e4hnlich.", "tokens": ["Schei\u00b7den\u00b7der", "Lie\u00b7ben\u00b7den", "\u00e4hn\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Da lag mein alter, treuer Lebenszeuge", "tokens": ["Da", "lag", "mein", "al\u00b7ter", ",", "treu\u00b7er", "Le\u00b7bens\u00b7zeu\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Erw\u00fcrgt, zerfiedert auf der Erde!", "tokens": ["Er\u00b7w\u00fcrgt", ",", "zer\u00b7fie\u00b7dert", "auf", "der", "Er\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Undankbarer Gast! \u2013 Aber konnt' ich damals klagen?", "tokens": ["Un\u00b7dank\u00b7ba\u00b7rer", "Gast", "!", "\u2013", "A\u00b7ber", "konnt'", "ich", "da\u00b7mals", "kla\u00b7gen", "?"], "t