{"textgrid.poem.36757": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Rosa Taddei", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4umt' ich die Muse zu sehn, so la\u00df mir den Wahn! auf Papier nur,", "tokens": ["Tr\u00e4umt'", "ich", "die", "Mu\u00b7se", "zu", "sehn", ",", "so", "la\u00df", "mir", "den", "Wahn", "!", "auf", "Pa\u00b7pier", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,", "ADV", "VVIMP", "PPER", "ART", "NN", "$.", "APPR", "NN", "ADV", "$,"], "meter": "+--+--+-+--++-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Doch auf lebendigem Mund sah ich noch nie ein Gedicht.", "tokens": ["Doch", "auf", "le\u00b7ben\u00b7di\u00b7gem", "Mund", "sah", "ich", "noch", "nie", "ein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}}}}