{"textgrid.poem.36903": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Osterlied", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesus lebt, mit ihm auch ich.", "tokens": ["Je\u00b7sus", "lebt", ",", "mit", "ihm", "auch", "ich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "PPER", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tod, wo sind nun deine Schrecken?", "tokens": ["Tod", ",", "wo", "sind", "nun", "dei\u00b7ne", "Schre\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er, er lebt, und wird auch mich", "tokens": ["Er", ",", "er", "lebt", ",", "und", "wird", "auch", "mich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "VVFIN", "$,", "KON", "VAFIN", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den Toten auferwecken.", "tokens": ["Von", "den", "To\u00b7ten", "au\u00b7fer\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Er verkl\u00e4rt mich in sein Licht;", "tokens": ["Er", "ver\u00b7kl\u00e4rt", "mich", "in", "sein", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dies ist meine Zuversicht.", "tokens": ["Dies", "ist", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jesus lebt, ihm ist das Reich", "tokens": ["Je\u00b7sus", "lebt", ",", "ihm", "ist", "das", "Reich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber alle Welt gegeben;", "tokens": ["\u00dc\u00b7ber", "al\u00b7le", "Welt", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit ihm werd auch ich zugleich", "tokens": ["Mit", "ihm", "werd", "auch", "ich", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig herrschen, ewig leben.", "tokens": ["E\u00b7wig", "herr\u00b7schen", ",", "e\u00b7wig", "le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott erf\u00fcllt, was er verspricht;", "tokens": ["Gott", "er\u00b7f\u00fcllt", ",", "was", "er", "ver\u00b7spricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dies ist meine Zuversicht.", "tokens": ["Dies", "ist", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jesus lebt, wer nun verzagt,", "tokens": ["Je\u00b7sus", "lebt", ",", "wer", "nun", "ver\u00b7zagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4stert ihn und Gottes Ehre.", "tokens": ["L\u00e4s\u00b7tert", "ihn", "und", "Got\u00b7tes", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gnade hat er zugesagt,", "tokens": ["Gna\u00b7de", "hat", "er", "zu\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der S\u00fcnder sich bekehre.", "tokens": ["Da\u00df", "der", "S\u00fcn\u00b7der", "sich", "be\u00b7keh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott verst\u00f6\u00dft in Christo nicht;", "tokens": ["Gott", "ver\u00b7st\u00f6\u00dft", "in", "Chris\u00b7to", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dies ist meine Zuversicht.", "tokens": ["Dies", "ist", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jesus lebt, sein Heil ist mein;", "tokens": ["Je\u00b7sus", "lebt", ",", "sein", "Heil", "ist", "mein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein sei auch mein ganzes Leben.", "tokens": ["Sein", "sei", "auch", "mein", "gan\u00b7zes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Reines Herzens will ich sein,", "tokens": ["Rei\u00b7nes", "Her\u00b7zens", "will", "ich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den L\u00fcsten widerstreben.", "tokens": ["Und", "den", "L\u00fcs\u00b7ten", "wi\u00b7der\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Er verl\u00e4\u00dft den Schwachen nicht;", "tokens": ["Er", "ver\u00b7l\u00e4\u00dft", "den", "Schwa\u00b7chen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dies ist meine Zuversicht.", "tokens": ["Dies", "ist", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jesus lebt, ich bin gewi\u00df,", "tokens": ["Je\u00b7sus", "lebt", ",", "ich", "bin", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nichts soll mich von Jesu scheiden,", "tokens": ["Nichts", "soll", "mich", "von", "Je\u00b7su", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Macht der Finsternis,", "tokens": ["Kei\u00b7ne", "Macht", "der", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Herrlichkeit, kein Leiden.", "tokens": ["Kei\u00b7ne", "Herr\u00b7lich\u00b7keit", ",", "kein", "Lei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Er gibt Kraft zu dieser Pflicht;", "tokens": ["Er", "gibt", "Kraft", "zu", "die\u00b7ser", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dies ist meine Zuversicht.", "tokens": ["Dies", "ist", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jesus lebt, nun ist der Tod", "tokens": ["Je\u00b7sus", "lebt", ",", "nun", "ist", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir der Eingang in das Leben.", "tokens": ["Mir", "der", "Ein\u00b7gang", "in", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welchen Trost in Todesnot", "tokens": ["Wel\u00b7chen", "Trost", "in", "To\u00b7des\u00b7not"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird er meiner Seele geben,", "tokens": ["Wird", "er", "mei\u00b7ner", "See\u00b7le", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sie gl\u00e4ubig zu ihm spricht:", "tokens": ["Wenn", "sie", "gl\u00e4u\u00b7big", "zu", "ihm", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herr, Herr, meine Zuversicht!", "tokens": ["Herr", ",", "Herr", ",", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}