{"dta.poem.20335": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Flavien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Flavia/ du kennest ja mein hertze/", "tokens": ["Ge\u00b7lieb\u00b7te", "Fla\u00b7via", "/", "du", "ken\u00b7nest", "ja", "mein", "hert\u00b7ze", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du kennst es allzuwohl/ es steht in deiner hand.", "tokens": ["Du", "kennst", "es", "all\u00b7zu\u00b7wohl", "/", "es", "steht", "in", "dei\u00b7ner", "hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es w\u00e4chst das andre mahl Dianens weisse kertze/", "tokens": ["Es", "w\u00e4chst", "das", "and\u00b7re", "mahl", "Di\u00b7a\u00b7nens", "weis\u00b7se", "kert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als du das hertz und mich dir selber hast entwand.", "tokens": ["Als", "du", "das", "hertz", "und", "mich", "dir", "sel\u00b7ber", "hast", "ent\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "PPER", "PPER", "ADV", "VAFIN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wird ein gef\u00e4llig wort auff diese bl\u00e4tter fliessen/", "tokens": ["Wird", "ein", "ge\u00b7f\u00e4l\u00b7lig", "wort", "auff", "die\u00b7se", "bl\u00e4t\u00b7ter", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "NN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So r\u00fchme deinen trieb/ nicht meinen geist und mich.", "tokens": ["So", "r\u00fch\u00b7me", "dei\u00b7nen", "trieb", "/", "nicht", "mei\u00b7nen", "geist", "und", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "VVFIN", "$(", "PTKNEG", "PPOSAT", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich werde sonder zwang dir doch bekennen m\u00fcssen/", "tokens": ["Ich", "wer\u00b7de", "son\u00b7der", "zwang", "dir", "doch", "be\u00b7ken\u00b7nen", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "PPER", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die liebste Flavia die schreibt hier mehr als ich.", "tokens": ["Die", "liebs\u00b7te", "Fla\u00b7via", "die", "schreibt", "hier", "mehr", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "VVFIN", "ADV", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Doch bist du meisterin von meinen treuen sinnen/", "tokens": ["Doch", "bist", "du", "meis\u00b7te\u00b7rin", "von", "mei\u00b7nen", "treu\u00b7en", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So schaue diesen brieff mit holden augen an/", "tokens": ["So", "schau\u00b7e", "die\u00b7sen", "brieff", "mit", "hol\u00b7den", "au\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du wirst die feder ja mit recht nicht tadeln k\u00f6nnen/", "tokens": ["Du", "wirst", "die", "fe\u00b7der", "ja", "mit", "recht", "nicht", "ta\u00b7deln", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "ADV", "PTKNEG", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die ohne deinen zug kein wort mehr schreiben kan.", "tokens": ["Die", "oh\u00b7ne", "dei\u00b7nen", "zug", "kein", "wort", "mehr", "schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich hoff es werde mich die richtsucht nicht verdammen/", "tokens": ["Ich", "hoff", "es", "wer\u00b7de", "mich", "die", "richt\u00b7sucht", "nicht", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PPER", "ART", "VVFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vor der die tugend selbst nicht unber\u00fchrt kan stehn/", "tokens": ["Vor", "der", "die", "tu\u00b7gend", "selbst", "nicht", "un\u00b7be\u00b7r\u00fchrt", "kan", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es kennt der himmel ja die reinen freundschaffts flammen/", "tokens": ["Es", "kennt", "der", "him\u00b7mel", "ja", "die", "rei\u00b7nen", "freund\u00b7schaffts", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die auch an sauberkeit den sternen gleiche gehn.", "tokens": ["Die", "auch", "an", "sau\u00b7ber\u00b7keit", "den", "ster\u00b7nen", "glei\u00b7che", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "ART", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach! liebste Flavia/ die schrifft und die gedancken", "tokens": ["Ach", "!", "liebs\u00b7te", "Fla\u00b7via", "/", "die", "schrifft", "und", "die", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "NE", "$(", "ART", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Sind ja ein wunderwerck und kleinod dieser welt;", "tokens": ["Sind", "ja", "ein", "wun\u00b7der\u00b7werck", "und", "klei\u00b7nod", "die\u00b7ser", "welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "KON", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was spielen wir doch nicht in des gem\u00fcthes schrancken?", "tokens": ["Was", "spie\u00b7len", "wir", "doch", "nicht", "in", "des", "ge\u00b7m\u00fc\u00b7thes", "schran\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was haben wir da nicht verwegen f\u00fcrgestellt?", "tokens": ["Was", "ha\u00b7ben", "wir", "da", "nicht", "ver\u00b7we\u00b7gen", "f\u00fcr\u00b7ge\u00b7stellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was uns verboten wird/ das kan man hier erf\u00fcllen/", "tokens": ["Was", "uns", "ver\u00b7bo\u00b7ten", "wird", "/", "das", "kan", "man", "hier", "er\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$(", "PDS", "VMFIN", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Man lachet/ schertzt und k\u00fc\u00dft/ thut was uns wolgef\u00e4llt.", "tokens": ["Man", "la\u00b7chet", "/", "schertzt", "und", "k\u00fc\u00dft", "/", "thut", "was", "uns", "wol\u00b7ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "$(", "VVFIN", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Kein scharff gesetze st\u00f6hrt allhier den freyen willen/", "tokens": ["Kein", "scharff", "ge\u00b7set\u00b7ze", "st\u00f6hrt", "all\u00b7hier", "den", "frey\u00b7en", "wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJA", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und nichts ist starck genug/ das uns zur\u00fccke h\u00e4lt.", "tokens": ["Und", "nichts", "ist", "starck", "ge\u00b7nug", "/", "das", "uns", "zu\u00b7r\u00fc\u00b7cke", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "ADV", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Man mag die sch\u00f6nste brust hier ohne scheu ber\u00fchren/", "tokens": ["Man", "mag", "die", "sch\u00f6ns\u00b7te", "brust", "hier", "oh\u00b7ne", "scheu", "be\u00b7r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und schauen/ was man sonst nicht wohl bef\u00fchlen darff.", "tokens": ["Und", "schau\u00b7en", "/", "was", "man", "sonst", "nicht", "wohl", "be\u00b7f\u00fch\u00b7len", "darff", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "PWS", "PIS", "ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Man kan die heisse lust bi\u00df auff den gipffel f\u00fchren:", "tokens": ["Man", "kan", "die", "heis\u00b7se", "lust", "bi\u00df", "auff", "den", "gipf\u00b7fel", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dann den gedancken ist der richter allzuscharff.", "tokens": ["Dann", "den", "ge\u00b7dan\u00b7cken", "ist", "der", "rich\u00b7ter", "all\u00b7zu\u00b7scharff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Kein riegel h\u00e4lt sie auff/ es kan sie nichts verdecken/", "tokens": ["Kein", "rie\u00b7gel", "h\u00e4lt", "sie", "auff", "/", "es", "kan", "sie", "nichts", "ver\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "$(", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wann ihre r\u00e4der nur in scharffen triebe gehn;", "tokens": ["Wann", "ih\u00b7re", "r\u00e4\u00b7der", "nur", "in", "scharf\u00b7fen", "trie\u00b7be", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Es kan kein zarter ort vor ihnen sich verstecken/", "tokens": ["Es", "kan", "kein", "zar\u00b7ter", "ort", "vor", "ih\u00b7nen", "sich", "ver\u00b7ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN", "APPR", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Kein zahn und nagel wei\u00df hier recht zu widerstehn.", "tokens": ["Kein", "zahn", "und", "na\u00b7gel", "wei\u00df", "hier", "recht", "zu", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Zeit und gelegenheit wei\u00df keinem nicht zu fehlen/", "tokens": ["Zeit", "und", "ge\u00b7le\u00b7gen\u00b7heit", "wei\u00df", "kei\u00b7nem", "nicht", "zu", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PIS", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.34": {"text": "Hier bricht man rosen ab/ und f\u00fchlt die dornen nicht.", "tokens": ["Hier", "bricht", "man", "ro\u00b7sen", "ab", "/", "und", "f\u00fchlt", "die", "dor\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "PTKVZ", "$(", "KON", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Man kan was/ wo und wie nach seiner lust erwehlen/", "tokens": ["Man", "kan", "was", "/", "wo", "und", "wie", "nach", "sei\u00b7ner", "lust", "er\u00b7weh\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIS", "$(", "PWAV", "KON", "PWAV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Man findt kein thor allhier so unsern f\u00fcrsatz bricht.", "tokens": ["Man", "findt", "kein", "thor", "all\u00b7hier", "so", "un\u00b7sern", "f\u00fcr\u00b7satz", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die schrifften/ die man sonst verdolmetscht durchs gem\u00fcthe/", "tokens": ["Die", "schriff\u00b7ten", "/", "die", "man", "sonst", "ver\u00b7dol\u00b7metscht", "durchs", "ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "PRELS", "PIS", "ADV", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die stumme redens-art/ so aus der feder qvillt/", "tokens": ["Die", "stum\u00b7me", "re\u00b7den\u00b7sart", "/", "so", "aus", "der", "fe\u00b7der", "qvillt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Hat eine solche krafft/ und ist von solcher g\u00fcte/", "tokens": ["Hat", "ei\u00b7ne", "sol\u00b7che", "krafft", "/", "und", "ist", "von", "sol\u00b7cher", "g\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIAT", "NN", "$(", "KON", "VAFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df offt ein schreiben mehr als ein gespr\u00e4che gillt.", "tokens": ["Da\u00df", "offt", "ein", "schrei\u00b7ben", "mehr", "als", "ein", "ge\u00b7spr\u00e4\u00b7che", "gillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "PIAT", "KOKOM", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "F\u00e4llt gleich ein s\u00fcsser schall uns in die d\u00fcnnen ohren/", "tokens": ["F\u00e4llt", "gleich", "ein", "s\u00fcs\u00b7ser", "schall", "uns", "in", "die", "d\u00fcn\u00b7nen", "oh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So pr\u00e4gt die feder uns doch dessen meynung ein.", "tokens": ["So", "pr\u00e4gt", "die", "fe\u00b7der", "uns", "doch", "des\u00b7sen", "mey\u00b7nung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Es hat des menschen witz die littern ihm erkohren/", "tokens": ["Es", "hat", "des", "men\u00b7schen", "witz", "die", "lit\u00b7tern", "ihm", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.44": {"text": "Da\u00df sie der sterbligkeit geschw\u00e4rtzte bothen seyn.", "tokens": ["Da\u00df", "sie", "der", "ster\u00b7blig\u00b7keit", "ge\u00b7schw\u00e4rtz\u00b7te", "bo\u00b7then", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Sie lauffen \u00fcber berg und schwimmen \u00fcber fl\u00fcsse/", "tokens": ["Sie", "lauf\u00b7fen", "\u00fc\u00b7ber", "berg", "und", "schwim\u00b7men", "\u00fc\u00b7ber", "fl\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Sie stifften buhlerey/ und richten freundschafft an:", "tokens": ["Sie", "stiff\u00b7ten", "buh\u00b7le\u00b7rey", "/", "und", "rich\u00b7ten", "freund\u00b7schafft", "an", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$(", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Sie f\u00fchren gut und geld/ sie bringen gru\u00df und k\u00fcsse/", "tokens": ["Sie", "f\u00fch\u00b7ren", "gut", "und", "geld", "/", "sie", "brin\u00b7gen", "gru\u00df", "und", "k\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "NN", "$(", "PPER", "VVINF", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und schwingen offtermahls der liebe sieges-fahn.", "tokens": ["Und", "schwin\u00b7gen", "off\u00b7ter\u00b7mahls", "der", "lie\u00b7be", "sie\u00b7ges\u00b7fahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Sie pressen thr\u00e4nen aus/ sie regen unsre hertzen/", "tokens": ["Sie", "pres\u00b7sen", "thr\u00e4\u00b7nen", "aus", "/", "sie", "re\u00b7gen", "uns\u00b7re", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$(", "PPER", "ADJA", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Sie blasen feuer auff/ sie st\u00e4rcken die gedult.", "tokens": ["Sie", "bla\u00b7sen", "feu\u00b7er", "auff", "/", "sie", "st\u00e4r\u00b7cken", "die", "ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Sie sagen reichlich zu/ sie wissen wohl zu schertzen/", "tokens": ["Sie", "sa\u00b7gen", "reich\u00b7lich", "zu", "/", "sie", "wis\u00b7sen", "wohl", "zu", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKZU", "$(", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und ein geschmeider brieff zahlt offt die gr\u00f6ste schuld.", "tokens": ["Und", "ein", "ge\u00b7schmei\u00b7der", "brieff", "zahlt", "offt", "die", "gr\u00f6s\u00b7te", "schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Schrifft und gedancken sind der trost entfernter seelen/", "tokens": ["Schrifft", "und", "ge\u00b7dan\u00b7cken", "sind", "der", "trost", "ent\u00b7fern\u00b7ter", "see\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Damit bestillen sie die regung heisser pein/", "tokens": ["Da\u00b7mit", "be\u00b7stil\u00b7len", "sie", "die", "re\u00b7gung", "heis\u00b7ser", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und was man vor der welt aus wohlstand mu\u00df verh\u00f6len/", "tokens": ["Und", "was", "man", "vor", "der", "welt", "aus", "wohl\u00b7stand", "mu\u00df", "ver\u00b7h\u00f6\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "APPR", "ART", "NN", "APPR", "NE", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Das kan im sinn gespielt/ im brieff geschrieben seyn.", "tokens": ["Das", "kan", "im", "sinn", "ge\u00b7spielt", "/", "im", "brieff", "ge\u00b7schrie\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "VVPP", "$(", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Geliebte Flavia/ in meinen angedencken", "tokens": ["Ge\u00b7lieb\u00b7te", "Fla\u00b7via", "/", "in", "mei\u00b7nen", "an\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.58": {"text": "Schwebt itzt dein freundlich seyn/ dein anmuths \u00fcberstu\u00df;", "tokens": ["Schwebt", "itzt", "dein", "freund\u00b7lich", "seyn", "/", "dein", "an\u00b7muths", "\u00fc\u00b7bers\u00b7tu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJD", "VAINF", "$(", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Mich deucht du wilst mich itzt mit rosen-thau beschencken/", "tokens": ["Mich", "deucht", "du", "wilst", "mich", "itzt", "mit", "ro\u00b7sen\u00b7\u00b7thau", "be\u00b7schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VMFIN", "PPER", "ADV", "APPR", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Vor dem die rose bleicht/ und thau vertrocknen mu\u00df.", "tokens": ["Vor", "dem", "die", "ro\u00b7se", "bleicht", "/", "und", "thau", "ver\u00b7trock\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "VVFIN", "$(", "KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Mich d\u00e4ucht es r\u00fchren mich der hellen augen flammen/", "tokens": ["Mich", "d\u00e4ucht", "es", "r\u00fch\u00b7ren", "mich", "der", "hel\u00b7len", "au\u00b7gen", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Und das geschwinde gifft/ so aus rubinen f\u00e4hrt.", "tokens": ["Und", "das", "ge\u00b7schwin\u00b7de", "gifft", "/", "so", "aus", "ru\u00b7bi\u00b7nen", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "ADV", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Es schl\u00e4gt itzt \u00fcber mir die wollust-flut zusammen/", "tokens": ["Es", "schl\u00e4gt", "itzt", "\u00fc\u00b7ber", "mir", "die", "wol\u00b7lust\u00b7flut", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "So mir die h\u00f6llen-angst ins paradi\u00df verkehrt.", "tokens": ["So", "mir", "die", "h\u00f6l\u00b7len\u00b7angst", "ins", "pa\u00b7ra\u00b7di\u00df", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Ich schau auff warmen schnee die rothen beeren stehen/", "tokens": ["Ich", "schau", "auff", "war\u00b7men", "schnee", "die", "ro\u00b7then", "bee\u00b7ren", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Die ohne zucker auch dem zucker \u00e4hnlich seyn;", "tokens": ["Die", "oh\u00b7ne", "zu\u00b7cker", "auch", "dem", "zu\u00b7cker", "\u00e4hn\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Es scheint du heissest mich auff tuber-rosen gehen/", "tokens": ["Es", "scheint", "du", "heis\u00b7sest", "mich", "auff", "tu\u00b7ber\u00b7ro\u00b7sen", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Und machst die schwartze nacht zum hellen sonnenschein.", "tokens": ["Und", "machst", "die", "schwart\u00b7ze", "nacht", "zum", "hel\u00b7len", "son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Ich schliesse diesen brieff/ der hin zu dir begehret/", "tokens": ["Ich", "schlies\u00b7se", "die\u00b7sen", "brieff", "/", "der", "hin", "zu", "dir", "be\u00b7ge\u00b7hret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$(", "ART", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.70": {"text": "Und der die h\u00e4nde k\u00fcst/ die ich nicht k\u00fcssen kan.", "tokens": ["Und", "der", "die", "h\u00e4n\u00b7de", "k\u00fcst", "/", "die", "ich", "nicht", "k\u00fcs\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "$(", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Was mir versaget ist/ das wird ihm itzt gew\u00e4hret/", "tokens": ["Was", "mir", "ver\u00b7sa\u00b7get", "ist", "/", "das", "wird", "ihm", "itzt", "ge\u00b7w\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$(", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Es scheint/ als st\u00f6sse mich ein kleiner eif