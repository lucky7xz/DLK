{"textgrid.poem.35035": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Ewig", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr sucht und sucht: \u00bbWo ist die Ewigkeit?\u00ab", "tokens": ["Ihr", "sucht", "und", "sucht", ":", "\u00bb", "Wo", "ist", "die", "E\u00b7wig\u00b7keit", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "$(", "PWAV", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbjenseits des Todes! Ueber unsern Sternen!", "tokens": ["\u00bb", "jen\u00b7seits", "des", "To\u00b7des", "!", "Ue\u00b7ber", "un\u00b7sern", "Ster\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier ist die Zeit, und grad nur in der Zeit", "tokens": ["Hier", "ist", "die", "Zeit", ",", "und", "grad", "nur", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KON", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat f\u00fcr das ewge Leben man zu lernen.", "tokens": ["Hat", "f\u00fcr", "das", "ew\u00b7ge", "Le\u00b7ben", "man", "zu", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hier sind die Jahre, Monde, Tage, Stunden;", "tokens": ["Hier", "sind", "die", "Jah\u00b7re", ",", "Mon\u00b7de", ",", "Ta\u00b7ge", ",", "Stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "NE", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wir leben nach des Uhrenzeigers Lauf.", "tokens": ["Wir", "le\u00b7ben", "nach", "des", "Uh\u00b7ren\u00b7zei\u00b7gers", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hat er die Zw\u00f6lf, die Mitternacht, gefunden,", "tokens": ["Hat", "er", "die", "Zw\u00f6lf", ",", "die", "Mit\u00b7ter\u00b7nacht", ",", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So kommt die Ewigkeit, die Zeit h\u00f6rt auf.\u00ab", "tokens": ["So", "kommt", "die", "E\u00b7wig\u00b7keit", ",", "die", "Zeit", "h\u00f6rt", "auf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So wird von euch gesprochen und gedacht;", "tokens": ["So", "wird", "von", "euch", "ge\u00b7spro\u00b7chen", "und", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So h\u00f6ren es die Sch\u00fcler von den Meistern,", "tokens": ["So", "h\u00f6\u00b7ren", "es", "die", "Sch\u00fc\u00b7ler", "von", "den", "Meis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00e4hrend Einer frech dar\u00fcber lacht,", "tokens": ["Und", "w\u00e4h\u00b7rend", "Ei\u00b7ner", "frech", "da\u00b7r\u00fc\u00b7ber", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADJD", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4\u00dft sich der Andere davon begeistern.", "tokens": ["L\u00e4\u00dft", "sich", "der", "An\u00b7de\u00b7re", "da\u00b7von", "be\u00b7geis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "PIS", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr meint, die Ewigkeit sei nur zu glauben,", "tokens": ["Ihr", "meint", ",", "die", "E\u00b7wig\u00b7keit", "sei", "nur", "zu", "glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sei eine Zweifelssache, ein Vielleicht,", "tokens": ["Sei", "ei\u00b7ne", "Zwei\u00b7fels\u00b7sa\u00b7che", ",", "ein", "Viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und sendet aus der Arche eure Tauben,", "tokens": ["Und", "sen\u00b7det", "aus", "der", "Ar\u00b7che", "eu\u00b7re", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von denen keine auf zur Wahrheit steigt.", "tokens": ["Von", "de\u00b7nen", "kei\u00b7ne", "auf", "zur", "Wahr\u00b7heit", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So h\u00f6rt es denn: Die Ewigkeit ist dort,", "tokens": ["So", "h\u00f6rt", "es", "denn", ":", "Die", "E\u00b7wig\u00b7keit", "ist", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist hier, ist vor und nach euch, allerorten,", "tokens": ["Ist", "hier", ",", "ist", "vor", "und", "nach", "euch", ",", "al\u00b7le\u00b7ror\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "VAFIN", "APPR", "KON", "APPR", "PPER", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Zeitenraum, der grenzenlose Ort,", "tokens": ["Der", "Zei\u00b7ten\u00b7raum", ",", "der", "gren\u00b7zen\u00b7lo\u00b7se", "Ort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der nur im Wechsel endlich ist geworden.", "tokens": ["Der", "nur", "im", "Wech\u00b7sel", "end\u00b7lich", "ist", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ADV", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sobald die ewge Liebe sch\u00f6pfrisch handelt,", "tokens": ["So\u00b7bald", "die", "ew\u00b7ge", "Lie\u00b7be", "sch\u00f6pf\u00b7risch", "han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hat ihren Rathschlu\u00df sie in Form gebracht", "tokens": ["Hat", "ih\u00b7ren", "Rath\u00b7schlu\u00df", "sie", "in", "Form", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und die Unendlichkeit in Zeit verwandelt,", "tokens": ["Und", "die", "Un\u00b7end\u00b7lich\u00b7keit", "in", "Zeit", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch diese Zeit als ewig sich gedacht.", "tokens": ["Doch", "die\u00b7se", "Zeit", "als", "e\u00b7wig", "sich", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "KOKOM", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So lebt ihr also in der Ewigkeit;", "tokens": ["So", "lebt", "ihr", "al\u00b7so", "in", "der", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Euch ward die Gnade, sie als Zeit zu fassen.", "tokens": ["Euch", "ward", "die", "Gna\u00b7de", ",", "sie", "als", "Zeit", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ben\u00fctzt ihr sie, so wird als Seligkeit", "tokens": ["Be\u00b7n\u00fctzt", "ihr", "sie", ",", "so", "wird", "als", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "KOKOM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Herr sie euch f\u00fcr ewig, ewig lassen.", "tokens": ["Der", "Herr", "sie", "euch", "f\u00fcr", "e\u00b7wig", ",", "e\u00b7wig", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPER", "APPR", "ADJD", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer dies nicht thut, dem steht der Abgrund offen,", "tokens": ["Wer", "dies", "nicht", "thut", ",", "dem", "steht", "der", "Ab\u00b7grund", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "PTKNEG", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus dem die Erdenstunde ihn gebar,", "tokens": ["Aus", "dem", "die", "Er\u00b7dens\u00b7tun\u00b7de", "ihn", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und nur vom Himmel ist f\u00fcr ihn zu hoffen,", "tokens": ["Und", "nur", "vom", "Him\u00b7mel", "ist", "f\u00fcr", "ihn", "zu", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VAFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df er das wieder wird, was hier er war.", "tokens": ["Da\u00df", "er", "das", "wie\u00b7der", "wird", ",", "was", "hier", "er", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VAFIN", "$,", "PRELS", "ADV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}