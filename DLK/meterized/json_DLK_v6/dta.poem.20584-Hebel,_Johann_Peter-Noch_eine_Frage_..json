{"dta.poem.20584": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Noch eine Frage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Und weisch denn selber an du liebi Seel,             ", "tokens": ["Und", "weisch", "denn", "sel\u00b7ber", "an", "du", "lie\u00b7bi", "Seel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADV", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "worum de dine zarte Chinde d\u2019 Freud", "tokens": ["wo\u00b7rum", "de", "di\u00b7ne", "zar\u00b7te", "Chin\u00b7de", "d'", "Freud"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "PPOSAT", "ADJA", "NN", "NE", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "in so ne stachlig B\u00e4umli ", "tokens": ["in", "so", "ne", "stach\u00b7lig", "B\u00e4um\u00b7li"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wil\u2019s gr\u00fceni Bl\u00e4ttli het im Winter, meinsch,", "tokens": ["Wil's", "gr\u00fce\u00b7ni", "Bl\u00e4tt\u00b7li", "het", "im", "Win\u00b7ter", ",", "meinsch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "NE", "VAFIN", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "und D\u00f6rnli dra, a\u00df \u2019s B\u00fcebli nit, wie \u2019s will", "tokens": ["und", "D\u00f6rn\u00b7li", "dra", ",", "a\u00df", "'s", "B\u00fceb\u00b7li", "nit", ",", "wie", "'s", "will"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "NE", "$,", "VVFIN", "PPER", "NE", "PTKNEG", "$,", "PWAV", "PPER", "VMFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "die sch\u00f6ne Sachen use h\u00f6ckle cha.", "tokens": ["die", "sch\u00f6\u00b7ne", "Sa\u00b7chen", "u\u00b7se", "h\u00f6ck\u00b7le", "cha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u2019s w\u00e4r nit gar \u00fcbel gfehlt, doch weischs nit", "tokens": ["'s", "w\u00e4r", "nit", "gar", "\u00fc\u00b7bel", "gfehlt", ",", "doch", "weischs", "nit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,", "ADV", "PIS", "PTKNEG"], "meter": "-+-----+-+", "measure": "dactylic.init"}, "line.8": {"text": "recht!", "tokens": ["recht", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Denkwol, i sag ders, und i freu mi druf;", "tokens": ["Denk\u00b7wol", ",", "i", "sag", "ders", ",", "und", "i", "freu", "mi", "druf", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "FM", "FM", "$,", "KON", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Lueg, liebi Seel, vom Menschelebe soll", "tokens": ["Lu\u00b7eg", ",", "lie\u00b7bi", "Seel", ",", "vom", "Men\u00b7sche\u00b7le\u00b7be", "soll"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "APPRART", "NN", "VMFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "der dornig Freudebaum en Abbild sy.", "tokens": ["der", "dor\u00b7nig", "Freu\u00b7de\u00b7baum", "en", "Ab\u00b7bild", "sy", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nooch by nenander wohne Leid und Freud,", "tokens": ["Nooch", "by", "nen\u00b7an\u00b7der", "woh\u00b7ne", "Leid", "und", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "und was der \u2019s Lebe s\u00fce\u00df und liebli macht,", "tokens": ["und", "was", "der", "'s", "Le\u00b7be", "s\u00fce\u00df", "und", "lieb\u00b7li", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "PPER", "NN", "VVFIN", "KON", "NE", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "und was no sch\u00f6ner in der Zukunft schwebt,", "tokens": ["und", "was", "no", "sch\u00f6\u00b7ner", "in", "der", "Zu\u00b7kunft", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NE", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "de freusch di druf, doch in de D\u00f6rne hangts!", "tokens": ["de", "freusch", "di", "druf", ",", "doch", "in", "de", "D\u00f6r\u00b7ne", "hangts", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NE", "PAV", "$,", "ADV", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Was denksch derzu? ", "tokens": ["Was", "denksch", "der\u00b7zu", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Wenn Wermeth in di Freudebecher flie\u00dft", "tokens": ["Wenn", "Wer\u00b7meth", "in", "di", "Freu\u00b7de\u00b7be\u00b7cher", "flie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "NE", "NN", "VVFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "und wenn e scharfe Schmerz dur\u2019s Lebe zuckt,", "tokens": ["und", "wenn", "e", "schar\u00b7fe", "Schmerz", "dur's", "Le\u00b7be", "zuckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "verschrick nit drab, und stell di nit so fremd!", "tokens": ["ver\u00b7schrick", "nit", "drab", ",", "und", "stell", "di", "nit", "so", "fremd", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ADV", "$,", "KON", "ADJD", "NE", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Di eigeni Mutter selig, tr\u00f6st sie Gott,", "tokens": ["Di", "ei\u00b7ge\u00b7ni", "Mut\u00b7ter", "se\u00b7lig", ",", "tr\u00f6st", "sie", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "sie het der \u2019s Zeichen in der Chindheit ge;", "tokens": ["sie", "het", "der", "'s", "Zei\u00b7chen", "in", "der", "Chind\u00b7heit", "ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PPER", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "drum denk: \u201eEs isch e Wienechtchindli-", "tokens": ["drum", "denk", ":", "\u201e", "Es", "isch", "e", "Wienecht\u00b7chind\u00b7li"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$.", "$(", "PPER", "ADJD", "NE", "TRUNC"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Baum,", "tokens": ["Baum", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "nooch by nenander wohne Freud und Leid.\u201c", "tokens": ["nooch", "by", "nen\u00b7an\u00b7der", "woh\u00b7ne", "Freud", "und", "Leid", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "FM", "FM", "FM", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Zum Zweyte sagi das: Es w\u00e4r nit gut,             ", "tokens": ["Zum", "Zwey\u00b7te", "sa\u00b7gi", "das", ":", "Es", "w\u00e4r", "nit", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PDS", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "wenns auderst w\u00e4r. Was us de Dorne", "tokens": ["wenns", "au\u00b7derst", "w\u00e4r", ".", "Was", "us", "de", "Dor\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VAFIN", "$.", "PWS", "NE", "NE", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "luegt,", "tokens": ["luegt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "sieht gar viel gattiger und sch\u00f6ner us,", "tokens": ["sieht", "gar", "viel", "gat\u00b7ti\u00b7ger", "und", "sch\u00f6\u00b7ner", "us", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "KON", "ADJA", "NE", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.29": {"text": "und \u2019s f\u00fcrnehmst isch, me het au l\u00e4nger dra.", "tokens": ["und", "'s", "f\u00fcr\u00b7nehmst", "isch", ",", "me", "het", "au", "l\u00e4n\u00b7ger", "dra", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "\u2019s w\u00e4r iust, as wemme Zuckerbrod und Nu\u00df,", "tokens": ["'s", "w\u00e4r", "i\u00b7ust", ",", "as", "wem\u00b7me", "Zu\u00b7cker\u00b7brod", "und", "Nu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "NE", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "und was am B\u00e4umli sch\u00f6n und glitz\u2019rig", "tokens": ["und", "was", "am", "B\u00e4um\u00b7li", "sch\u00f6n", "und", "glitz'\u00b7rig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPRART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "hangt,", "tokens": ["hangt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.33": {"text": "uf eimol in e Suppesch\u00fc\u00dfle th\u00e4t,", "tokens": ["uf", "ei\u00b7mol", "in", "e", "Sup\u00b7pe\u00b7sch\u00fc\u00df\u00b7le", "th\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "und stellti \u2019s umme: \u201eI\u00df so lang de magsch,", "tokens": ["und", "stell\u00b7ti", "'s", "um\u00b7me", ":", "\u201e", "I\u00df", "so", "lang", "de", "magsch", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "$.", "$(", "NN", "ADV", "ADJD", "NE", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "\u201eund n\u00e4umis do isch!\u201c W\u00e4rs nit Uh-", "tokens": ["\u201e", "und", "n\u00e4u\u00b7mis", "do", "isch", "!", "\u201c", "W\u00e4rs", "nit", "Uh"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "KON", "ADV", "ADV", "ADJD", "$.", "$(", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "verstand?", "tokens": ["ver\u00b7stand", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.37": {"text": "Zum Dritte sagi: Wemmen in der Welt             ", "tokens": ["Zum", "Drit\u00b7te", "sa\u00b7gi", ":", "Wem\u00b7men", "in", "der", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "will Freude hasche, Vorsicht gh\u00f6rt derzu;", "tokens": ["will", "Freu\u00b7de", "ha\u00b7sche", ",", "Vor\u00b7sicht", "gh\u00f6rt", "der\u00b7zu", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVFIN", "$,", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "sust lengt me bald in d\u2019 Aglen und in D\u00f6rn", "tokens": ["sust", "lengt", "me", "bald", "in", "d'", "Ag\u00b7len", "und", "in", "D\u00f6rn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVFIN", "ADV", "APPR", "NE", "NE", "KON", "APPR", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "und zieht e leeri Hand voll Schrunde z\u2019ruck.", "tokens": ["und", "zieht", "e", "lee\u00b7ri", "Hand", "voll", "Schrun\u00b7de", "z'\u00b7ruck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ADJA", "NN", "ADJD", "NN", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.41": {"text": "Denn d\u2019Freud hangt in de Dorne. Denk", "tokens": ["Denn", "d'\u00b7Freud", "hangt", "in", "de", "Dor\u00b7ne", ".", "Denk"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NE", "VVFIN", "APPR", "NE", "NE", "$.", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.42": {"text": "mer dra,", "tokens": ["mer", "dra", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.43": {"text": "und thue ne wenig gmach! Doch wenn de\u2019s", "tokens": ["und", "thue", "ne", "we\u00b7nig", "gmach", "!", "Doch", "wenn", "de's"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "ADV", "ADJD", "$.", "KON", "KOUS", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "hesch,", "tokens": ["hesch", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.45": {"text": "se lo\u00df ders schmecke! Gunn ders Gott der Her!", "tokens": ["se", "lo\u00df", "ders", "schme\u00b7cke", "!", "Gunn", "ders", "Gott", "der", "Her", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$.", "NE", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}