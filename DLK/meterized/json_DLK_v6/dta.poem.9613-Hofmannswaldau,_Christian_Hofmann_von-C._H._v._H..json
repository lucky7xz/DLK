{"dta.poem.9613": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df Sylvia die reine glut/", "tokens": ["La\u00df", "Syl\u00b7via", "die", "rei\u00b7ne", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "ART", "ADJA", "NN", "$("], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "So mir entz\u00fcndet geist und blut/", "tokens": ["So", "mir", "ent\u00b7z\u00fcn\u00b7det", "geist", "und", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich liebste nicht zum zorn bewegen.", "tokens": ["Dich", "liebs\u00b7te", "nicht", "zum", "zorn", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer kan f\u00fcr deinen augen stehn/", "tokens": ["Wer", "kan", "f\u00fcr", "dei\u00b7nen", "au\u00b7gen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und unentbrant von dannen gehn/", "tokens": ["Und", "un\u00b7ent\u00b7brant", "von", "dan\u00b7nen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn sich des geistes trieb will regen", "tokens": ["Wenn", "sich", "des", "geis\u00b7tes", "trieb", "will", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "VVFIN", "VMFIN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht falle doch der meinung bey/", "tokens": ["Nicht", "fal\u00b7le", "doch", "der", "mei\u00b7nung", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df reine liebe s\u00fcnde sey/", "tokens": ["Da\u00df", "rei\u00b7ne", "lie\u00b7be", "s\u00fcn\u00b7de", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die GOtt in unser hertz geschrieben/", "tokens": ["Die", "Gott", "in", "un\u00b7ser", "hertz", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die selbst sein mund im paradies", "tokens": ["Die", "selbst", "sein", "mund", "im", "pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In uns mit unserm athem blie\u00df/", "tokens": ["In", "uns", "mit", "un\u00b7serm", "at\u00b7hem", "blie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der uns geboten hat zu lieben.", "tokens": ["Der", "uns", "ge\u00b7bo\u00b7ten", "hat", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Soll meine liebe s\u00fcnde seyn/", "tokens": ["Soll", "mei\u00b7ne", "lie\u00b7be", "s\u00fcn\u00b7de", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wisse/ da\u00df dein sch\u00f6ner schein", "tokens": ["So", "wis\u00b7se", "/", "da\u00df", "dein", "sch\u00f6\u00b7ner", "schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dieser s\u00fcnde mich getrieben/", "tokens": ["Zu", "die\u00b7ser", "s\u00fcn\u00b7de", "mich", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und glaube/ da\u00df die kluge welt", "tokens": ["Und", "glau\u00b7be", "/", "da\u00df", "die", "klu\u00b7ge", "welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor leibliche geschwister h\u00e4lt/", "tokens": ["Vor", "leib\u00b7li\u00b7che", "ge\u00b7schwis\u00b7ter", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die sch\u00f6nheit und den trieb zu lieben.", "tokens": ["Die", "sch\u00f6n\u00b7heit", "und", "den", "trieb", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum folg ich der natur gebot/", "tokens": ["Drum", "folg", "ich", "der", "na\u00b7tur", "ge\u00b7bot", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin kein stein und auch kein gott/", "tokens": ["Ich", "bin", "kein", "stein", "und", "auch", "kein", "gott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df in deinen flammen brennen.", "tokens": ["Ich", "mu\u00df", "in", "dei\u00b7nen", "flam\u00b7men", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir ist gefesselt geist und muth/", "tokens": ["Mir", "ist", "ge\u00b7fes\u00b7selt", "geist", "und", "muth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum will ich auch des hertzens glut", "tokens": ["Drum", "will", "ich", "auch", "des", "hert\u00b7zens", "glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor GOtt und dir nur frey bekennen.", "tokens": ["Vor", "Gott", "und", "dir", "nur", "frey", "be\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier ist mein demuth-volles hertz/", "tokens": ["Hier", "ist", "mein", "de\u00b7muth\u00b7vol\u00b7les", "hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "So sich verbindt in lieb und schmertz", "tokens": ["So", "sich", "ver\u00b7bindt", "in", "lieb", "und", "schmertz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "VVPP", "APPR", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit gleicher andacht dir zu dienen.", "tokens": ["Mit", "glei\u00b7cher", "an\u00b7dacht", "dir", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun Sylvia das opfer hin/", "tokens": ["Nun", "Syl\u00b7via", "das", "op\u00b7fer", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ART", "NN", "PTKVZ", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "La\u00df augen-trost in deinem sinn/", "tokens": ["La\u00df", "au\u00b7gen\u00b7trost", "in", "dei\u00b7nem", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergi\u00df mein nicht im hertzen gr\u00fcnen.", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht", "im", "hert\u00b7zen", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich bleibe dein/ bi\u00df da\u00df mein geist", "tokens": ["Ich", "blei\u00b7be", "dein", "/", "bi\u00df", "da\u00df", "mein", "geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "$(", "APPR", "KOUS", "PPOSAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Aus meinem reinem hertzen reist/", "tokens": ["Aus", "mei\u00b7nem", "rei\u00b7nem", "hert\u00b7zen", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bi\u00df man mich wird zur leiche machen.", "tokens": ["Bi\u00df", "man", "mich", "wird", "zur", "lei\u00b7che", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VAFIN", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df Sylvia mein tausend-sch\u00f6n", "tokens": ["La\u00df", "Syl\u00b7via", "mein", "tau\u00b7sen\u00b7dsch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "NE", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich nur bey deinen rosen stehn/", "tokens": ["Mich", "nur", "bey", "dei\u00b7nen", "ro\u00b7sen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So will ich aller dornen lachen.", "tokens": ["So", "will", "ich", "al\u00b7ler", "dor\u00b7nen", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}