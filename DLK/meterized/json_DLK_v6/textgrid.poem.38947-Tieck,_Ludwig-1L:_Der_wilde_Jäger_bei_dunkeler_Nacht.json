{"textgrid.poem.38947": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der wilde J\u00e4ger bei dunkeler Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der wilde J\u00e4ger bei dunkeler Nacht", "tokens": ["Der", "wil\u00b7de", "J\u00e4\u00b7ger", "bei", "dun\u00b7ke\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im wildesten Dickicht des Forstes erwacht,", "tokens": ["Im", "wil\u00b7des\u00b7ten", "Di\u00b7ckicht", "des", "Fors\u00b7tes", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Er h\u00f6ret den Sturm, und erhebt sich im Zorn,", "tokens": ["Er", "h\u00f6\u00b7ret", "den", "Sturm", ",", "und", "er\u00b7hebt", "sich", "im", "Zorn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er nimmt seine Hunde, das t\u00f6nende Horn.", "tokens": ["Er", "nimmt", "sei\u00b7ne", "Hun\u00b7de", ",", "das", "t\u00f6\u00b7nen\u00b7de", "Horn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Besteigt seinen Rappen, mit Blitzesgewalt", "tokens": ["Be\u00b7steigt", "sei\u00b7nen", "Rap\u00b7pen", ",", "mit", "Blit\u00b7zes\u00b7ge\u00b7walt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Durchf\u00e4hrt er lautschnaubend den zitternden Wald,", "tokens": ["Durch\u00b7f\u00e4hrt", "er", "laut\u00b7schnau\u00b7bend", "den", "zit\u00b7tern\u00b7den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es wiehert sein Ro\u00df, t\u00f6nt das Horn in die Runde,", "tokens": ["Es", "wie\u00b7hert", "sein", "Ro\u00df", ",", "t\u00f6nt", "das", "Horn", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er hetzt die Gef\u00e4hrten, es bellen die Hunde.", "tokens": ["Er", "hetzt", "die", "Ge\u00b7f\u00e4hr\u00b7ten", ",", "es", "bel\u00b7len", "die", "Hun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Wohlarf meine Jagd! wohlauf meine Jagd!", "tokens": ["Wohl\u00b7arf", "mei\u00b7ne", "Jagd", "!", "wohl\u00b7auf", "mei\u00b7ne", "Jagd", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$.", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Revier ist unser, denn jetzt ist es Nacht,", "tokens": ["Das", "Re\u00b7vier", "ist", "un\u00b7ser", ",", "denn", "jetzt", "ist", "es", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$,", "KON", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von fl\u00fcchtigen Geistern wird gerne gehetzt,", "tokens": ["Von", "fl\u00fcch\u00b7ti\u00b7gen", "Geis\u00b7tern", "wird", "ger\u00b7ne", "ge\u00b7hetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wer sich vor Geheul und Gebelle entsetzt.", "tokens": ["Wer", "sich", "vor", "Ge\u00b7heul", "und", "Ge\u00b7bel\u00b7le", "ent\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "So fahren sie polternd durch L\u00fcfte dahin", "tokens": ["So", "fah\u00b7ren", "sie", "pol\u00b7ternd", "durch", "L\u00fcf\u00b7te", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "NN", "PAV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein Grauen dem frommen und furchtsamen Sinn,", "tokens": ["Ein", "Grau\u00b7en", "dem", "from\u00b7men", "und", "furcht\u00b7sa\u00b7men", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Doch wer sich vor Wald und vor Nacht nicht entsetzt,", "tokens": ["Doch", "wer", "sich", "vor", "Wald", "und", "vor", "Nacht", "nicht", "ent\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "APPR", "NN", "KON", "APPR", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der wird vom Get\u00fcmmel der Geister erg\u00f6tzt.", "tokens": ["Der", "wird", "vom", "Ge\u00b7t\u00fcm\u00b7mel", "der", "Geis\u00b7ter", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}