{"textgrid.poem.44056": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwey Herzen, deren Bund Verstand und Liebe schliest", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwey Herzen, deren Bund Verstand und Liebe schliest", "tokens": ["Zwey", "Her\u00b7zen", ",", "de\u00b7ren", "Bund", "Ver\u00b7stand", "und", "Lie\u00b7be", "schliest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "PRELAT", "NN", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die einander blos zu Freunden auserkiest,", "tokens": ["Und", "die", "ein\u00b7an\u00b7der", "blos", "zu", "Freun\u00b7den", "aus\u00b7er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit, o gro\u00dfer Gott, dein Nahmen auf der Erde", "tokens": ["Da\u00b7mit", ",", "o", "gro\u00b7\u00dfer", "Gott", ",", "dein", "Nah\u00b7men", "auf", "der", "Er\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "FM", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch ihre Redligkeit der Welt zum Wunder werde,", "tokens": ["Durch", "ih\u00b7re", "Red\u00b7lig\u00b7keit", "der", "Welt", "zum", "Wun\u00b7der", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zween Br\u00fcder knien hier vor deiner Majest\u00e4t", "tokens": ["Zween", "Br\u00fc\u00b7der", "kni\u00b7en", "hier", "vor", "dei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Bu\u00dfe, Zuversicht, Ernst, Hofnung und Gebeth,", "tokens": ["Mit", "Bu\u00b7\u00dfe", ",", "Zu\u00b7ver\u00b7sicht", ",", "Ernst", ",", "Hof\u00b7nung", "und", "Ge\u00b7beth", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von deiner Gnadenhand das Siegel zu empfangen,", "tokens": ["Von", "dei\u00b7ner", "Gna\u00b7den\u00b7hand", "das", "Sie\u00b7gel", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wodurch sie ihre Treu genau verkn\u00fcpft verlangen.", "tokens": ["Wo\u00b7durch", "sie", "ih\u00b7re", "Treu", "ge\u00b7nau", "ver\u00b7kn\u00fcpft", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADJD", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Tugend ist ihr Zweck, die Redligkeit ihr Grund,", "tokens": ["Die", "Tu\u00b7gend", "ist", "ihr", "Zweck", ",", "die", "Red\u00b7lig\u00b7keit", "ihr", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und wie dort Salomo vor deinem Antliz stund,", "tokens": ["Und", "wie", "dort", "Sa\u00b7lo\u00b7mo", "vor", "dei\u00b7nem", "Ant\u00b7liz", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So flehn sie ebenfalls als ehrliche Gem\u00fcther", "tokens": ["So", "flehn", "sie", "e\u00b7ben\u00b7falls", "als", "ehr\u00b7li\u00b7che", "Ge\u00b7m\u00fc\u00b7ther"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit Vorsaz und Begier um nichts als Wei\u00dfheitsg\u00fcter.", "tokens": ["Mit", "Vor\u00b7saz", "und", "Be\u00b7gier", "um", "nichts", "als", "Wei\u00df\u00b7heits\u00b7g\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir wi\u00dfen, gro\u00dfer Gott, da\u00df unser S\u00fcnden Macht", "tokens": ["Wir", "wi\u00b7\u00dfen", ",", "gro\u00b7\u00dfer", "Gott", ",", "da\u00df", "un\u00b7ser", "S\u00fcn\u00b7den", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Uns leider nach Verdienst um Gl\u00fcck und Heil gebracht", "tokens": ["Uns", "lei\u00b7der", "nach", "Ver\u00b7dienst", "um", "Gl\u00fcck", "und", "Heil", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und da\u00df auch, wenn dein Arm gerecht verfahren wollte,", "tokens": ["Und", "da\u00df", "auch", ",", "wenn", "dein", "Arm", "ge\u00b7recht", "ver\u00b7fah\u00b7ren", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Rache, so jezt brennt, von uns nicht la\u00dfen sollte;", "tokens": ["Die", "Ra\u00b7che", ",", "so", "jezt", "brennt", ",", "von", "uns", "nicht", "la\u00b7\u00dfen", "soll\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "VVFIN", "$,", "APPR", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wir wi\u00dfen aber auch, da\u00df Be\u00dferung und Reu", "tokens": ["Wir", "wi\u00b7\u00dfen", "a\u00b7ber", "auch", ",", "da\u00df", "Be\u00b7\u00dfe\u00b7rung", "und", "Reu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "NN", "KON", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Durch deines Sohnes Blut uns statt der Unschuld sey;", "tokens": ["Durch", "dei\u00b7nes", "Soh\u00b7nes", "Blut", "uns", "statt", "der", "Un\u00b7schuld", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPER", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Drum f\u00e4llt dir unsre Noth mit Sehnsucht in die Armen,", "tokens": ["Drum", "f\u00e4llt", "dir", "uns\u00b7re", "Noth", "mit", "Sehn\u00b7sucht", "in", "die", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du must dich, gro\u00dfer Gott, und wirst dich auch erbarmen.", "tokens": ["Du", "must", "dich", ",", "gro\u00b7\u00dfer", "Gott", ",", "und", "wirst", "dich", "auch", "er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,", "KON", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Bedencke doch dein Kind, in de\u00dfen Nahm und Wort", "tokens": ["Be\u00b7den\u00b7cke", "doch", "dein", "Kind", ",", "in", "de\u00b7\u00dfen", "Nahm", "und", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wir zween versammlet sind; wir glauben diesem Hort,", "tokens": ["Wir", "zween", "ver\u00b7samm\u00b7let", "sind", ";", "wir", "glau\u00b7ben", "die\u00b7sem", "Hort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "VAFIN", "$.", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er heist uns fest vertraun. Ach, schone doch der Jugend", "tokens": ["Er", "heist", "uns", "fest", "ver\u00b7traun", ".", "Ach", ",", "scho\u00b7ne", "doch", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$.", "ITJ", "$,", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und f\u00fchre sie doch nur gelinder zu der Tugend.", "tokens": ["Und", "f\u00fch\u00b7re", "sie", "doch", "nur", "ge\u00b7lin\u00b7der", "zu", "der", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du weist, der Weg dazu ist an sich selber schwer,", "tokens": ["Du", "weist", ",", "der", "Weg", "da\u00b7zu", "ist", "an", "sich", "sel\u00b7ber", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PAV", "VAFIN", "APPR", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und wer ihn wandeln soll, geht stets mit Furcht einher,", "tokens": ["Und", "wer", "ihn", "wan\u00b7deln", "soll", ",", "geht", "stets", "mit", "Furcht", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "VMFIN", "$,", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wir, da\u00df uns noch dabey besondre Wetter krachen", "tokens": ["Wir", ",", "da\u00df", "uns", "noch", "da\u00b7bey", "be\u00b7sond\u00b7re", "Wet\u00b7ter", "kra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "PPER", "ADV", "PAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und Armuth, Last und Schmach die Schenckel taumelnd machen.", "tokens": ["Und", "Ar\u00b7muth", ",", "Last", "und", "Schmach", "die", "Schen\u00b7ckel", "tau\u00b7melnd", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach, rette mit Gedult und warthe, bis die Zeit", "tokens": ["Ach", ",", "ret\u00b7te", "mit", "Ge\u00b7dult", "und", "wart\u00b7he", ",", "bis", "die", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "APPR", "NN", "KON", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und \u00dcbung und Vernunft uns gr\u00f6\u00dfre Kraft verleiht,", "tokens": ["Und", "\u00dc\u00b7bung", "und", "Ver\u00b7nunft", "uns", "gr\u00f6\u00df\u00b7re", "Kraft", "ver\u00b7leiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Proben auszustehn, die bey des P\u00f6bels H\u00f6hnen", "tokens": ["Die", "Pro\u00b7ben", "aus\u00b7zu\u00b7stehn", ",", "die", "bey", "des", "P\u00f6\u00b7bels", "H\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Gro\u00dfmuth und den Geist der weisen M\u00e4rtrer cr\u00f6nen.", "tokens": ["Die", "Gro\u00df\u00b7muth", "und", "den", "Geist", "der", "wei\u00b7sen", "M\u00e4r\u00b7trer", "cr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Gieb Nothdurft, freyen Muth, gesunden Wiz und Leib,", "tokens": ["Gieb", "Noth\u00b7durft", ",", "frey\u00b7en", "Muth", ",", "ge\u00b7sun\u00b7den", "Wiz", "und", "Leib", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Denn alles andre Creuz ist guter Zeitvertreib.", "tokens": ["Denn", "al\u00b7les", "and\u00b7re", "Creuz", "ist", "gu\u00b7ter", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wir werden uns bem\u00fchn, durch Wohlthun und Studiren", "tokens": ["Wir", "wer\u00b7den", "uns", "be\u00b7m\u00fchn", ",", "durch", "Wohl\u00b7thun", "und", "Stu\u00b7di\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Als Wercke deiner Hand des Sch\u00f6pfers Ruhm zu zieren.", "tokens": ["Als", "Wer\u00b7cke", "dei\u00b7ner", "Hand", "des", "Sch\u00f6p\u00b7fers", "Ruhm", "zu", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wir gehn auf gutes Gl\u00fcck und wi\u00dfen nicht wohin;", "tokens": ["Wir", "gehn", "auf", "gu\u00b7tes", "Gl\u00fcck", "und", "wi\u00b7\u00dfen", "nicht", "wo\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KON", "VVFIN", "PTKNEG", "PWAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Doch du bist \u00fcberall und wirst den frommen Sinn,", "tokens": ["Doch", "du", "bist", "\u00fc\u00b7be\u00b7rall", "und", "wirst", "den", "from\u00b7men", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "KON", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der unsre Neigung paart, mit reichen Seegensblicken", "tokens": ["Der", "uns\u00b7re", "Nei\u00b7gung", "paart", ",", "mit", "rei\u00b7chen", "See\u00b7gens\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Auch durch Gefahr und Sturm in rechten Hafen r\u00fccken.", "tokens": ["Auch", "durch", "Ge\u00b7fahr", "und", "Sturm", "in", "rech\u00b7ten", "Ha\u00b7fen", "r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}