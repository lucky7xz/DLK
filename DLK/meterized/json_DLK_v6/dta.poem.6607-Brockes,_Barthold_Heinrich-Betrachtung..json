{"dta.poem.6607": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein in welchem Vorwurff stecket", "tokens": ["Al\u00b7lein", "in", "wel\u00b7chem", "Vor\u00b7wurff", "ste\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PWAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Handlung, welche uns empfinden, dencken macht?", "tokens": ["Die", "Hand\u00b7lung", ",", "wel\u00b7che", "uns", "emp\u00b7fin\u00b7den", ",", "den\u00b7cken", "macht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie wird dieselbe doch entdecket?", "tokens": ["Wie", "wird", "die\u00b7sel\u00b7be", "doch", "ent\u00b7de\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PDAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verwirren unsichtbar\u2019 und zarte R\u00f6hren", "tokens": ["Ver\u00b7wir\u00b7ren", "un\u00b7sicht\u00b7ba\u00b7r'", "und", "zar\u00b7te", "R\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Den C\u00f6rper und den Geist? Nein, Nein!", "tokens": ["Den", "C\u00f6r\u00b7per", "und", "den", "Geist", "?", "Nein", ",", "Nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$.", "PTKANT", "$,", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Seel\u2019, als \u00fcberf\u00fchret,", "tokens": ["Die", "Seel'", ",", "als", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erkennt sich selbst und sp\u00fcret,", "tokens": ["Er\u00b7kennt", "sich", "selbst", "und", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie sey ein eigenes, ein unterschiednes Seyn.", "tokens": ["Sie", "sey", "ein", "ei\u00b7ge\u00b7nes", ",", "ein", "un\u00b7ter\u00b7schied\u00b7nes", "Seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Vorsatz, den wir uns so edel vorgenommen,", "tokens": ["Der", "Vor\u00b7satz", ",", "den", "wir", "uns", "so", "e\u00b7del", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sey weiter ausgef\u00fchrt. Auf! bis zum Schlu\u00df zu kommen.", "tokens": ["Sey", "wei\u00b7ter", "aus\u00b7ge\u00b7f\u00fchrt", ".", "Auf", "!", "bis", "zum", "Schlu\u00df", "zu", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "APPR", "$.", "APPR", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Damit in meiner Schrifft die Wahrheit offenbar", "tokens": ["Da\u00b7mit", "in", "mei\u00b7ner", "Schrifft", "die", "Wahr\u00b7heit", "of\u00b7fen\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Erwiesen, und darbey", "tokens": ["Er\u00b7wie\u00b7sen", ",", "und", "dar\u00b7bey"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "KON", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Ein Beyfall meines Muths, von jener weisen Schaar", "tokens": ["Ein", "Bey\u00b7fall", "mei\u00b7nes", "Muths", ",", "von", "je\u00b7ner", "wei\u00b7sen", "Schaar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So Cron, als Prei\u00df, von meiner Arbeit sey!", "tokens": ["So", "Cron", ",", "als", "Prei\u00df", ",", "von", "mei\u00b7ner", "Ar\u00b7beit", "sey", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUS", "NN", "$,", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}