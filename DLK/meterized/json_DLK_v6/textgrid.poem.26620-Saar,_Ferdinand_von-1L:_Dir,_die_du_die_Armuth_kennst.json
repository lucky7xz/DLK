{"textgrid.poem.26620": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir, die du die Armuth kennst", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, die du die Armuth kennst", "tokens": ["Dir", ",", "die", "du", "die", "Ar\u00b7muth", "kennst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und des Darbens Qual \u2013", "tokens": ["Und", "des", "Dar\u00b7bens", "Qual", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht weil du sie selbst erlitten,", "tokens": ["Nicht", "weil", "du", "sie", "selbst", "er\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern weil du", "tokens": ["Son\u00b7dern", "weil", "du"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit sehendem Aug'", "tokens": ["Mit", "se\u00b7hen\u00b7dem", "Aug'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und f\u00fchlendem Herzen", "tokens": ["Und", "f\u00fch\u00b7len\u00b7dem", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sie bei And'ren wahrnimmst", "tokens": ["Sie", "bei", "An\u00b7d'\u00b7ren", "wahr\u00b7nimmst"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und linderst tausendfach;", "tokens": ["Und", "lin\u00b7derst", "tau\u00b7send\u00b7fach", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und weil du,", "tokens": ["Und", "weil", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Wenn es fruchtete,", "tokens": ["Wenn", "es", "fruch\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Dem Pelikan gleich,", "tokens": ["Dem", "Pe\u00b7li\u00b7kan", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Die Brust dir \u00f6ffnen w\u00fcrdest,", "tokens": ["Die", "Brust", "dir", "\u00f6ff\u00b7nen", "w\u00fcr\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Um mit dem eigenen", "tokens": ["Um", "mit", "dem", "ei\u00b7ge\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.14": {"text": "Warmen Blut", "tokens": ["War\u00b7men", "Blut"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Die darbende Menschheit zu letzen:", "tokens": ["Die", "dar\u00b7ben\u00b7de", "Menschheit", "zu", "let\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Dies B\u00fcchlein", "tokens": ["Dies", "B\u00fcch\u00b7lein"], "token_info": ["word", "word"], "pos": ["PDS", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Menschlicher Noth,", "tokens": ["Menschli\u00b7cher", "Noth", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Menschlichen Kummers,", "tokens": ["Menschli\u00b7chen", "Kum\u00b7mers", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Menschlichen Duldens:", "tokens": ["Menschli\u00b7chen", "Dul\u00b7dens", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Ein B\u00fcchlein,", "tokens": ["Ein", "B\u00fcch\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "++-", "measure": "unknown.measure.di"}, "line.21": {"text": "Das ein Dichter schrieb,", "tokens": ["Das", "ein", "Dich\u00b7ter", "schrieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.22": {"text": "Der in allem Diesen", "tokens": ["Der", "in", "al\u00b7lem", "Die\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PIS", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Wohlge\u00fcbt ist.", "tokens": ["Wohl\u00b7ge\u00b7\u00fcbt", "ist", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}