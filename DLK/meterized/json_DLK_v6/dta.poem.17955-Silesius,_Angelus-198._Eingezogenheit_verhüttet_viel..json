{"dta.poem.17955": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "198.  Eingezogenheit verh\u00fcttet viel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Braut ists da\u00df du nicht gern l\u00e4st frembde Buhler f\u00fct;", "tokens": ["Braut", "ists", "da\u00df", "du", "nicht", "gern", "l\u00e4st", "fremb\u00b7de", "Buh\u00b7ler", "f\u00fct", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So halt die Fenster zue und steh nicht in der Th\u00fcr.", "tokens": ["So", "halt", "die", "Fens\u00b7ter", "zue", "und", "steh", "nicht", "in", "der", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}