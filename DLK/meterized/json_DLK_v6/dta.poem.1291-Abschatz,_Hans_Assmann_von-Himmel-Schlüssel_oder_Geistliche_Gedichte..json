{"dta.poem.1291": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Bey hellem Monden-Licht ward mir das Licht gegeben/", "tokens": ["Bey", "hel\u00b7lem", "Mon\u00b7den\u00b7Licht", "ward", "mir", "das", "Licht", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df mich/ o h\u00f6chstes Licht/ in deinem Lichte leben/", "tokens": ["La\u00df", "mich", "/", "o", "h\u00f6chs\u00b7tes", "Licht", "/", "in", "dei\u00b7nem", "Lich\u00b7te", "le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "FM", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df mich di\u00df wahre Licht zu keiner Zeit verlieren/", "tokens": ["La\u00df", "mich", "di\u00df", "wah\u00b7re", "Licht", "zu", "kei\u00b7ner", "Zeit", "ver\u00b7lie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn mich manch irrend Licht will auff die Seite f\u00fchren;", "tokens": ["Wenn", "mich", "manch", "ir\u00b7rend", "Licht", "will", "auff", "die", "Sei\u00b7te", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "VVPP", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn mich der schn\u00f6de Glantz der eitlen Wollust blendet/", "tokens": ["Wenn", "mich", "der", "schn\u00f6\u00b7de", "Glantz", "der", "eit\u00b7len", "Wol\u00b7lust", "blen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn mir dein helles Licht des Creutzes Wolck entwendet/", "tokens": ["Wenn", "mir", "dein", "hel\u00b7les", "Licht", "des", "Creut\u00b7zes", "Wolck", "ent\u00b7wen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn Fleisch und Blutt/ wenn Welt und H\u00f6lle sich bem\u00fc-", "tokens": ["Wenn", "Fleisch", "und", "Blutt", "/", "wenn", "Welt", "und", "H\u00f6l\u00b7le", "sich", "be\u00b7m\u00fc"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "ADJD", "$(", "KOUS", "NN", "KON", "NN", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "hen/", "tokens": ["hen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Den Leit-Stern deiner Gunst der Seele zu entziehen/", "tokens": ["Den", "Leit\u00b7S\u00b7tern", "dei\u00b7ner", "Gunst", "der", "See\u00b7le", "zu", "ent\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "La\u00df deine Sonne mir im finstern Tode scheinen/", "tokens": ["La\u00df", "dei\u00b7ne", "Son\u00b7ne", "mir", "im", "fins\u00b7tern", "To\u00b7de", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und mich dein ewges Licht umgeben bey den Deinen.", "tokens": ["Und", "mich", "dein", "ew\u00b7ges", "Licht", "um\u00b7ge\u00b7ben", "bey", "den", "Dei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "APPR", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}