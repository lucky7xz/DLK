{"textgrid.poem.32283": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Der Geschmack der Alten", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob wir, wir Neuern, vor den Alten", "tokens": ["Ob", "wir", ",", "wir", "Neu\u00b7ern", ",", "vor", "den", "Al\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PPER", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Vorzug des Geschmacks erhalten,", "tokens": ["Den", "Vor\u00b7zug", "des", "Ge\u00b7schmacks", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was les't ihr darum vieles nach,", "tokens": ["Was", "les't", "ihr", "da\u00b7rum", "vie\u00b7les", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PAV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was der und jener Franze sprach?", "tokens": ["Was", "der", "und", "je\u00b7ner", "Fran\u00b7ze", "sprach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "KON", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Franzen sind die Leute nicht,", "tokens": ["Die", "Fran\u00b7zen", "sind", "die", "Leu\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus welchen ein Orakel spricht.", "tokens": ["Aus", "wel\u00b7chen", "ein", "O\u00b7ra\u00b7kel", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich will ein neues Urteil wagen.", "tokens": ["Ich", "will", "ein", "neu\u00b7es", "Ur\u00b7teil", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geschmack und Witz, es frei zu sagen,", "tokens": ["Ge\u00b7schmack", "und", "Witz", ",", "es", "frei", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "War bei den Alten allgemein.", "tokens": ["War", "bei", "den", "Al\u00b7ten", "all\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Warum? sie tranken alle Wein.", "tokens": ["Wa\u00b7rum", "?", "sie", "tran\u00b7ken", "al\u00b7le", "Wein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ihr Geschmack war noch nicht fein;", "tokens": ["Doch", "ihr", "Ge\u00b7schmack", "war", "noch", "nicht", "fein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Warum? sie mischten Wasser drein.", "tokens": ["Wa\u00b7rum", "?", "sie", "mischten", "Was\u00b7ser", "drein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}