{"dta.poem.20090": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der Wettersee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne sinkt in den Wettersee;", "tokens": ["Die", "Son\u00b7ne", "sinkt", "in", "den", "Wet\u00b7ter\u00b7see", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da steigt \u2014 mit dem Neck und der Wasserfee \u2014", "tokens": ["Da", "steigt", "mit", "dem", "Neck", "und", "der", "Was\u00b7ser\u00b7fee"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "APPR", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von Gold und Rubin, aus des Seees Gruft,", "tokens": ["Von", "Gold", "und", "Ru\u00b7bin", ",", "aus", "des", "See\u00b7es", "Gruft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Schlo\u00df an die abendger\u00f6thete Luft.", "tokens": ["Ein", "Schlo\u00df", "an", "die", "a\u00b7bend\u00b7ge\u00b7r\u00f6\u00b7the\u00b7te", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Der Mond geht auf; da blassen Rubin", "tokens": ["Der", "Mond", "geht", "auf", ";", "da", "blas\u00b7sen", "Ru\u00b7bin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und Gold zu Silber und Aquamarin;", "tokens": ["und", "Gold", "zu", "Sil\u00b7ber", "und", "A\u00b7quam\u00b7a\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und hervor aus dem Schlo\u00df, hinaus zum Tanz,", "tokens": ["Und", "her\u00b7vor", "aus", "dem", "Schlo\u00df", ",", "hin\u00b7aus", "zum", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "APPR", "ART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Lockt die Nixen der Mondesglanz.", "tokens": ["Lockt", "die", "Ni\u00b7xen", "der", "Mon\u00b7des\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Teichrosen flechten sie, drau\u00dfen im Saal,", "tokens": ["Tei\u00b7chro\u00b7sen", "flech\u00b7ten", "sie", ",", "drau\u00b7\u00dfen", "im", "Saal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Um Stirn und Nacken sich allzumal,", "tokens": ["Um", "Stirn", "und", "Na\u00b7cken", "sich", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "PRF", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als bangte jede, des Mondes Licht", "tokens": ["Als", "bang\u00b7te", "je\u00b7de", ",", "des", "Mon\u00b7des", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PIAT", "$,", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Selbst k\u00f6nne br\u00e4unen ihr Angesicht.", "tokens": ["Selbst", "k\u00f6n\u00b7ne", "br\u00e4u\u00b7nen", "ihr", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Dann schlingen sie T\u00e4nze, dann t\u00f6nt ihr Gesang", "tokens": ["Dann", "schlin\u00b7gen", "sie", "T\u00e4n\u00b7ze", ",", "dann", "t\u00f6nt", "ihr", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zu Necken\u2019s melodischem Saitenklang,", "tokens": ["Zu", "Ne\u00b7cken's", "me\u00b7lo\u00b7di\u00b7schem", "Sai\u00b7ten\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Bis blasser das scheidende Mondlicht blinkt,", "tokens": ["Bis", "blas\u00b7ser", "das", "schei\u00b7den\u00b7de", "Mond\u00b7licht", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und Schlo\u00df und Neck und Nixe versinkt.", "tokens": ["Und", "Schlo\u00df", "und", "Neck", "und", "Ni\u00b7xe", "ver\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Schon baut ihren finstern Palast die Nacht,", "tokens": ["Schon", "baut", "ih\u00b7ren", "fins\u00b7tern", "Pa\u00b7last", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da heult es im Walde, da knickt es und kracht, \u2014", "tokens": ["Da", "heult", "es", "im", "Wal\u00b7de", ",", "da", "knickt", "es", "und", "kracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ihren Renner, zottig und grau,", "tokens": ["Ih\u00b7ren", "Ren\u00b7ner", ",", "zot\u00b7tig", "und", "grau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Reitet zur Tr\u00e4nke die Haidefrau.", "tokens": ["Rei\u00b7tet", "zur", "Tr\u00e4n\u00b7ke", "die", "Hai\u00b7de\u00b7frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Ihr Ro\u00df ist ein Wolf, schnell wie der Wind,", "tokens": ["Ihr", "Ro\u00df", "ist", "ein", "Wolf", ",", "schnell", "wie", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NE", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Blindschleichen die Z\u00fcgel des Renners sind,", "tokens": ["Blind\u00b7schlei\u00b7chen", "die", "Z\u00fc\u00b7gel", "des", "Ren\u00b7ners", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Eine Natter ist Peitsche, ein Igel ist Sporn,", "tokens": ["Ei\u00b7ne", "Nat\u00b7ter", "ist", "Peit\u00b7sche", ",", "ein", "I\u00b7gel", "ist", "Sporn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "So jagt sie herbei durch Dickicht und Dorn.", "tokens": ["So", "jagt", "sie", "her\u00b7bei", "durch", "Di\u00b7ckicht", "und", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Wetteifernd funkelt das Katzengrau", "tokens": ["Wett\u00b7ei\u00b7fernd", "fun\u00b7kelt", "das", "Kat\u00b7zen\u00b7grau"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Augen von Wolf und Haidefrau,", "tokens": ["Der", "Au\u00b7gen", "von", "Wolf", "und", "Hai\u00b7de\u00b7frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Man sieht, bei solchem Blitzen und Spr\u00fchn,", "tokens": ["Man", "sieht", ",", "bei", "sol\u00b7chem", "Blit\u00b7zen", "und", "Spr\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die lechzende Zunge des Wolfes gl\u00fchn.", "tokens": ["Die", "lech\u00b7zen\u00b7de", "Zun\u00b7ge", "des", "Wol\u00b7fes", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Er trinkt aus dem See, dann lenkt er den Schritt,", "tokens": ["Er", "trinkt", "aus", "dem", "See", ",", "dann", "lenkt", "er", "den", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und am Ufer entlang geht der n\u00e4chtliche Ritt,", "tokens": ["Und", "am", "U\u00b7fer", "ent\u00b7lang", "geht", "der", "n\u00e4cht\u00b7li\u00b7che", "Ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPO", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Bis fr\u00fch am Morgen statt Neck und Fee,", "tokens": ["Bis", "fr\u00fch", "am", "Mor\u00b7gen", "statt", "Neck", "und", "Fee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fischer durchfurchen den Wettersee.", "tokens": ["Fi\u00b7scher", "durch\u00b7fur\u00b7chen", "den", "Wet\u00b7ter\u00b7see", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}