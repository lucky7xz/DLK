{"textgrid.poem.29626": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Schmied verlie\u00df sein Weib, war aussen manches Jahr;", "tokens": ["Ein", "Schmied", "ver\u00b7lie\u00df", "sein", "Weib", ",", "war", "aus\u00b7sen", "man\u00b7ches", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "VAFIN", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indessen ward der Knecht und dieses Weib ein Paar.", "tokens": ["In\u00b7des\u00b7sen", "ward", "der", "Knecht", "und", "die\u00b7ses", "Weib", "ein", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als wieder kam der Schmied, da theilten sie die Stelle:", "tokens": ["Als", "wie\u00b7der", "kam", "der", "Schmied", ",", "da", "theil\u00b7ten", "sie", "die", "Stel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr Meister arbte der, und jener f\u00fcr Geselle.", "tokens": ["F\u00fcr", "Meis\u00b7ter", "arb\u00b7te", "der", ",", "und", "je\u00b7ner", "f\u00fcr", "Ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "$,", "KON", "PDS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}