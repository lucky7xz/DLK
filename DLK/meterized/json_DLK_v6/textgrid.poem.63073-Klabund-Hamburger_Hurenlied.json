{"textgrid.poem.63073": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Hamburger Hurenlied", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir Hamburger M\u00e4dchens habens fein,", "tokens": ["Wir", "Ham\u00b7bur\u00b7ger", "M\u00e4d\u00b7chens", "ha\u00b7bens", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir brauchen nicht auf dem Striche sein.", "tokens": ["Wir", "brau\u00b7chen", "nicht", "auf", "dem", "Stri\u00b7che", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir wohnen in sch\u00f6nen H\u00e4usern", "tokens": ["Wir", "woh\u00b7nen", "in", "sch\u00f6\u00b7nen", "H\u00e4u\u00b7sern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wohl bei der Nacht,", "tokens": ["Wohl", "bei", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ahoi!", "tokens": ["A\u00b7hoi", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Weil es uns Freude macht.", "tokens": ["Weil", "es", "uns", "Freu\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es kommen Kavaliere, Neger und Matros,", "tokens": ["Es", "kom\u00b7men", "Ka\u00b7va\u00b7lie\u00b7re", ",", "Ne\u00b7ger", "und", "Mat\u00b7ros", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die werden bei uns ihre Pfundst\u00fccke los,", "tokens": ["Die", "wer\u00b7den", "bei", "uns", "ih\u00b7re", "Pfund\u00b7st\u00fc\u00b7cke", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie liegen uns am Busen", "tokens": ["Sie", "lie\u00b7gen", "uns", "am", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl bei der Nacht,", "tokens": ["Wohl", "bei", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ahoi!", "tokens": ["A\u00b7hoi", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Weil es uns Freude macht.", "tokens": ["Weil", "es", "uns", "Freu\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Madam kocht schlechtes Essen, Sami spielt Klavier,", "tokens": ["Ma\u00b7dam", "kocht", "schlech\u00b7tes", "Es\u00b7sen", ",", "Sa\u00b7mi", "spielt", "Kla\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit den Kavalieren tanzen wir,", "tokens": ["Mit", "den", "Ka\u00b7va\u00b7lie\u00b7ren", "tan\u00b7zen", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Flie\u00dft ein Taler dr\u00fcber,", "tokens": ["Flie\u00dft", "ein", "Ta\u00b7ler", "dr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wird er Madam gebracht,", "tokens": ["Wird", "er", "Ma\u00b7dam", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ahoi!", "tokens": ["A\u00b7hoi", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Weil es uns Freude macht.", "tokens": ["Weil", "es", "uns", "Freu\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Eines Tages holt die Sitte uns hinaus,", "tokens": ["Ei\u00b7nes", "Ta\u00b7ges", "holt", "die", "Sit\u00b7te", "uns", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PRF", "APZR", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und sie sperrt uns in das graue Krankenhaus.", "tokens": ["Und", "sie", "sperrt", "uns", "in", "das", "grau\u00b7e", "Kran\u00b7ken\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Dann sind wir tot und sterben", "tokens": ["Dann", "sind", "wir", "tot", "und", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl bei der Nacht,", "tokens": ["Wohl", "bei", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ahoi!", "tokens": ["A\u00b7hoi", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Weil es uns Freude macht.", "tokens": ["Weil", "es", "uns", "Freu\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}