{"textgrid.poem.44601": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Zeloten", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Mariazell vor dem Gnadenbild", "tokens": ["In", "Ma\u00b7ria\u00b7zell", "vor", "dem", "Gna\u00b7den\u00b7bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Knieen die Pilger aus fernem Gefild.", "tokens": ["Kni\u00b7e\u00b7en", "die", "Pil\u00b7ger", "aus", "fer\u00b7nem", "Ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Im Opfer der W\u00fcnsche, die fr\u00fcher emp\u00f6rt,", "tokens": ["Im", "Op\u00b7fer", "der", "W\u00fcn\u00b7sche", ",", "die", "fr\u00fc\u00b7her", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "F\u00fchlt jeder zur Stelle schon halb sich erh\u00f6rt.", "tokens": ["F\u00fchlt", "je\u00b7der", "zur", "Stel\u00b7le", "schon", "halb", "sich", "er\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "ADV", "ADJD", "PRF", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Da schreit ein Eifrer mit gl\u00fchendem Blick,", "tokens": ["Da", "schreit", "ein", "Eif\u00b7rer", "mit", "gl\u00fc\u00b7hen\u00b7dem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das Bild sei als Bild auch ein Meisterst\u00fcck.", "tokens": ["Das", "Bild", "sei", "als", "Bild", "auch", "ein", "Meis\u00b7ter\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Die Gl\u00e4ubigen sehen genauer hin", "tokens": ["Die", "Gl\u00e4u\u00b7bi\u00b7gen", "se\u00b7hen", "ge\u00b7nau\u00b7er", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "ADJD", "PTKVZ"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und jetzt erst belehrt sie der eigene Sinn,", "tokens": ["Und", "jetzt", "erst", "be\u00b7lehrt", "sie", "der", "ei\u00b7ge\u00b7ne", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Als kritische Note zum gl\u00e4ubigen Text,", "tokens": ["Als", "kri\u00b7ti\u00b7sche", "No\u00b7te", "zum", "gl\u00e4u\u00b7bi\u00b7gen", "Text", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Das Wunderbild sei nur eben geklext.", "tokens": ["Das", "Wun\u00b7der\u00b7bild", "sei", "nur", "e\u00b7ben", "ge\u00b7klext", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}