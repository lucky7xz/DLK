{"textgrid.poem.42908": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Fliegerleute", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vielleicht wird sich das sp\u00e4ter \u00e4ndern.", "tokens": ["Viel\u00b7leicht", "wird", "sich", "das", "sp\u00e4\u00b7ter", "\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch ist es vielleicht in verschiedenen L\u00e4ndern", "tokens": ["Auch", "ist", "es", "viel\u00b7leicht", "in", "ver\u00b7schie\u00b7de\u00b7nen", "L\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Anders. Doch wie das in Deutschland heute", "tokens": ["An\u00b7ders", ".", "Doch", "wie", "das", "in", "Deutschland", "heu\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KON", "PWAV", "ART", "APPR", "NE", "ADV"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Liegt, mu\u00df ich sagen: Die Fliegerleute,", "tokens": ["Liegt", ",", "mu\u00df", "ich", "sa\u00b7gen", ":", "Die", "Flie\u00b7ger\u00b7leu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$.", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Piloten, Bordmonteure, Flugleiter,", "tokens": ["Pi\u00b7lo\u00b7ten", ",", "Bord\u00b7mon\u00b7teu\u00b7re", ",", "Flug\u00b7lei\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Bezirksleiter, Funker und so weiter,", "tokens": ["Be\u00b7zirks\u00b7lei\u00b7ter", ",", "Fun\u00b7ker", "und", "so", "wei\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Auch die im B\u00fcro und der luftige Boy", "tokens": ["Auch", "die", "im", "B\u00fc\u00b7ro", "und", "der", "luf\u00b7ti\u00b7ge", "Boy"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "APPRART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Sind goldige Kerls. \u2013 Ihnen Gutes! Ahoi!", "tokens": ["Sind", "gol\u00b7di\u00b7ge", "Kerls", ".", "\u2013", "Ih\u00b7nen", "Gu\u00b7tes", "!", "A\u00b7hoi", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$.", "$(", "PPER", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur ehrliche Leistung bringt nach der Ferne", "tokens": ["Nur", "ehr\u00b7li\u00b7che", "Leis\u00b7tung", "bringt", "nach", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch Wetter und Wogen ein Schiff.", "tokens": ["Durch", "Wet\u00b7ter", "und", "Wo\u00b7gen", "ein", "Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch bei der Luftfahrt kommt die moderne", "tokens": ["Doch", "bei", "der", "Luft\u00b7fahrt", "kommt", "die", "mo\u00b7der\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+----", "measure": "unknown.measure.tri"}, "line.4": {"text": "Weltm\u00e4nnische Bildung hinzu und der Schliff.", "tokens": ["Welt\u00b7m\u00e4n\u00b7ni\u00b7sche", "Bil\u00b7dung", "hin\u00b7zu", "und", "der", "Schliff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PTKVZ", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Humorvoll und k\u00fchn, sich beherrschend, bescheiden \u2013.", "tokens": ["Hu\u00b7mor\u00b7voll", "und", "k\u00fchn", ",", "sich", "be\u00b7herr\u00b7schend", ",", "be\u00b7schei\u00b7den", "\u2013", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "PRF", "VVPP", "$,", "ADJD", "$(", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Heraus ohne Schmeichelei:", "tokens": ["He\u00b7raus", "oh\u00b7ne", "Schmei\u00b7che\u00b7lei", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich mag diese Kerls leiden.", "tokens": ["Ich", "mag", "die\u00b7se", "Kerls", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihre Welt ist noch frei.", "tokens": ["Ih\u00b7re", "Welt", "ist", "noch", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So, wie sie sind, und dort, wo sie sind,", "tokens": ["So", ",", "wie", "sie", "sind", ",", "und", "dort", ",", "wo", "sie", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VAFIN", "$,", "KON", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wehn alle Flaggen und ein guter Wind.", "tokens": ["Wehn", "al\u00b7le", "Flag\u00b7gen", "und", "ein", "gu\u00b7ter", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Wie wohl das heute tut.", "tokens": ["Wie", "wohl", "das", "heu\u00b7te", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Prost Fliegerleute, ich denke an euch!", "tokens": ["Prost", "Flie\u00b7ger\u00b7leu\u00b7te", ",", "ich", "den\u00b7ke", "an", "euch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00c4ndert euch nicht. Ich schwenke vor euch", "tokens": ["\u00c4n\u00b7dert", "euch", "nicht", ".", "Ich", "schwen\u00b7ke", "vor", "euch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Meinen Hut.", "tokens": ["Mei\u00b7nen", "Hut", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}