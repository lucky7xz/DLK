{"textgrid.poem.47497": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[stehl' ich mich aus der Moscheee - hadre nicht!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stehl' ich mich aus der Moscheee \u2013 hadre nicht!", "tokens": ["Stehl'", "ich", "mich", "aus", "der", "Mo\u00b7schee\u00b7e", "\u2013", "had\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$(", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wenn ich in die Schenke gehe \u2013 hadre nicht!", "tokens": ["Wenn", "ich", "in", "die", "Schen\u00b7ke", "ge\u00b7he", "\u2013", "had\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$(", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn die Predigt ist so trocken, ist so lang,", "tokens": ["Denn", "die", "Pre\u00b7digt", "ist", "so", "tro\u00b7cken", ",", "ist", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Da\u00df ich gar kein Ende sehe \u2013 hadre nicht!", "tokens": ["Da\u00df", "ich", "gar", "kein", "En\u00b7de", "se\u00b7he", "\u2013", "had\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$(", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn ich mich, o meine Sonn', in deinem Strahl", "tokens": ["Wenn", "ich", "mich", ",", "o", "mei\u00b7ne", "Sonn'", ",", "in", "dei\u00b7nem", "Strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "$,", "FM", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Als ein armes St\u00e4ubchen drehe \u2013 hadre nicht!", "tokens": ["Als", "ein", "ar\u00b7mes", "St\u00e4ub\u00b7chen", "dre\u00b7he", "\u2013", "had\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$(", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn ich, armer Papagei, zu deinem Mund", "tokens": ["Wenn", "ich", ",", "ar\u00b7mer", "Pa\u00b7pa\u00b7gei", ",", "zu", "dei\u00b7nem", "Mund"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Um ein wenig Zucker flehe \u2013 hadre nicht!", "tokens": ["Um", "ein", "we\u00b7nig", "Zu\u00b7cker", "fle\u00b7he", "\u2013", "had\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "PIAT", "NN", "VVFIN", "$(", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach, du bist so sch\u00f6n, so sch\u00f6n, so wundersch\u00f6n!", "tokens": ["Ach", ",", "du", "bist", "so", "sch\u00f6n", ",", "so", "sch\u00f6n", ",", "so", "wun\u00b7der\u00b7sch\u00f6n", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Taumel' ich in deiner N\u00e4he \u2013 hadre nicht!", "tokens": ["Tau\u00b7mel'", "ich", "in", "dei\u00b7ner", "N\u00e4\u00b7he", "\u2013", "had\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$(", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Sonder Ende klagt die Nachtigall ihr Weh,", "tokens": ["Son\u00b7der", "En\u00b7de", "klagt", "die", "Nach\u00b7ti\u00b7gall", "ihr", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sing' ich ewig Ach und Wehe \u2013 hadre nicht! \u2013", "tokens": ["Sing'", "ich", "e\u00b7wig", "Ach", "und", "We\u00b7he", "\u2013", "had\u00b7re", "nicht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "NN", "KON", "NN", "$(", "VVFIN", "PTKNEG", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Kr\u00e4chzen, o Hafis, dich fromme Schn\u00e4bel an,", "tokens": ["Kr\u00e4ch\u00b7zen", ",", "o", "Ha\u00b7fis", ",", "dich", "from\u00b7me", "Schn\u00e4\u00b7bel", "an", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "$,", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Stille, still, mit Rab' und Kr\u00e4he \u2013 hadre nicht!", "tokens": ["Stil\u00b7le", ",", "still", ",", "mit", "Rab'", "und", "Kr\u00e4\u00b7he", "\u2013", "had\u00b7re", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "APPR", "NN", "KON", "NN", "$(", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}