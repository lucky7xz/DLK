{"dta.poem.23585": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Freiheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wozu sollen die Beschwerden?", "tokens": ["Wo\u00b7zu", "sol\u00b7len", "die", "Be\u00b7schwer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freiheit ist genug auf Erden,", "tokens": ["Frei\u00b7heit", "ist", "ge\u00b7nug", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenig, viel und nichts zu werden.", "tokens": ["We\u00b7nig", ",", "viel", "und", "nichts", "zu", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "KON", "PIS", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freiheit ward uns in Gewerben,", "tokens": ["Frei\u00b7heit", "ward", "uns", "in", "Ge\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Verthun und im Verderben,", "tokens": ["Im", "Ver\u00b7thun", "und", "im", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Verhungern und im Sterben.", "tokens": ["Im", "Ver\u00b7hun\u00b7gern", "und", "im", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weiter kannst du's hier nicht bringen;", "tokens": ["Wei\u00b7ter", "kannst", "du's", "hier", "nicht", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Andre Freiheit zu erringen,", "tokens": ["And\u00b7re", "Frei\u00b7heit", "zu", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird dir dort nur erst gelingen.", "tokens": ["Wird", "dir", "dort", "nur", "erst", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}