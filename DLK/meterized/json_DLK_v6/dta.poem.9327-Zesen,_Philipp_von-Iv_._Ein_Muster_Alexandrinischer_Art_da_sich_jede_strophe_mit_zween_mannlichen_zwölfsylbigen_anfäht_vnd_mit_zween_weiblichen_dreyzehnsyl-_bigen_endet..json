{"dta.poem.9327": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Iv .  \n Ein Muster Alexandrinischer Art/ da  \n sich jede strophe mit zween mannlichen zw\u00f6lfsylbigen  \n anf\u00e4ht/ vnd mit zween weiblichen dreyzehnsyl-  \n bigen endet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aeh schmertz! ach weh! ach leid! wie bricht herein", "tokens": ["A\u00b7eh", "schmertz", "!", "ach", "weh", "!", "ach", "leid", "!", "wie", "bricht", "her\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "$.", "XY", "PTKVZ", "$.", "ADV", "ADJD", "$.", "PWAV", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "die nacht/", "tokens": ["die", "nacht", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "der schwartze s\u00fcnde\u0303schwarm/ kein einig stern der wacht/", "tokens": ["der", "schwart\u00b7ze", "s\u00fcn\u00b7d\u1ebdschwarm", "/", "kein", "ei\u00b7nig", "stern", "der", "wacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADJD", "$(", "PIAT", "ADJD", "VVFIN", "ART", "VVFIN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die Sonne scheint nicht mehr/ der Mond ist auch", "tokens": ["die", "Son\u00b7ne", "scheint", "nicht", "mehr", "/", "der", "Mond", "ist", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$(", "ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "verdunckelt/ (ckelt/", "tokens": ["ver\u00b7dun\u00b7ckelt", "/", "(", "ckelt", "/"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["VVPP", "$(", "$(", "NE", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "die Nacht ist schreckens voll kein einig stern der fun-", "tokens": ["die", "Nacht", "ist", "schre\u00b7ckens", "voll", "kein", "ei\u00b7nig", "stern", "der", "fun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "PIAT", "ADJD", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "wie hab ichs doch verdient/ ", "tokens": ["wie", "hab", "ichs", "doch", "ver\u00b7dient", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "da\u00df du dich kehrst von mier und so besch\u00e4mest mich.", "tokens": ["da\u00df", "du", "dich", "kehrst", "von", "mier", "und", "so", "be\u00b7sch\u00e4\u00b7mest", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPER", "KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich bin ja voller angst/ wei\u00df nicht was ich beginne/", "tokens": ["Ich", "bin", "ja", "vol\u00b7ler", "angst", "/", "wei\u00df", "nicht", "was", "ich", "be\u00b7gin\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$(", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ja hellen-angst f\u00fchl\u2019 ich es will mier nicht zu sinne/", "tokens": ["ja", "hel\u00b7len\u00b7angst", "f\u00fchl'", "ich", "es", "will", "mier", "nicht", "zu", "sin\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "VMFIN", "PPER", "PTKNEG", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "da\u00df mehr gew\u00f6lcke sey/ da/ wo zu r\u00fccke geht/", "tokens": ["da\u00df", "mehr", "ge\u00b7w\u00f6l\u00b7cke", "sey", "/", "da", "/", "wo", "zu", "r\u00fc\u00b7cke", "geht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "$(", "KOUS", "$(", "PWAV", "PTKZU", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "der g\u00fcldnen Sonnenlicht/ al\u00df hier bey mier entsteht/", "tokens": ["der", "g\u00fcld\u00b7nen", "Son\u00b7nen\u00b7licht", "/", "al\u00df", "hier", "bey", "mier", "ent\u00b7steht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOKOM", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich halte g\u00e4ntzlich nicht da\u00df grausamer die Helle/", "tokens": ["Ich", "hal\u00b7te", "g\u00e4ntz\u00b7lich", "nicht", "da\u00df", "grau\u00b7sa\u00b7mer", "die", "Hel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKNEG", "KOUS", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "der schwartzen N\u00e4chteburg/ da Lerm und Schwarm", "tokens": ["der", "schwart\u00b7zen", "N\u00e4ch\u00b7te\u00b7burg", "/", "da", "Lerm", "und", "Schwarm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "zur stelle/", "tokens": ["zur", "stel\u00b7le", "/"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "ADJA", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Ich halte g\u00e4ntzlich nicht/ da\u00df trauriger sich zeigt", "tokens": ["Ich", "hal\u00b7te", "g\u00e4ntz\u00b7lich", "nicht", "/", "da\u00df", "trau\u00b7ri\u00b7ger", "sich", "zeigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKNEG", "$(", "KOUS", "ADJA", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "der vorverblasste Mond/ dort wo der Veer auffsteigt.", "tokens": ["der", "vor\u00b7ver\u00b7blass\u00b7te", "Mond", "/", "dort", "wo", "der", "Veer", "auffs\u00b7teigt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bey nacht sehr ihre nacht die Scyther wohl und", "tokens": ["Bey", "nacht", "sehr", "ih\u00b7re", "nacht", "die", "Scyt\u00b7her", "wohl", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "PPOSAT", "NN", "ART", "NN", "ADV", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "wissen/", "tokens": ["wis\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "da\u00df ohne Sonnenschein Sie offte leben m\u00fcssen.", "tokens": ["da\u00df", "oh\u00b7ne", "Son\u00b7nen\u00b7schein", "Sie", "off\u00b7te", "le\u00b7ben", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "VVFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn gegen mitternacht der mond zwey vierthel jahr", "tokens": ["Wenn", "ge\u00b7gen", "mit\u00b7ter\u00b7nacht", "der", "mond", "zwey", "vier\u00b7thel", "jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "VVFIN", "ART", "NN", "CARD", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "regieret/ giebt er doch der Sonnen jmmerdar", "tokens": ["re\u00b7gie\u00b7ret", "/", "giebt", "er", "doch", "der", "Son\u00b7nen", "jm\u00b7mer\u00b7dar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "das dritte vierthel ein. Ich aber bin verdammet", "tokens": ["das", "drit\u00b7te", "vier\u00b7thel", "ein", ".", "Ich", "a\u00b7ber", "bin", "ver\u00b7dam\u00b7met"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "PPER", "ADV", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "gantz gantz zur finstern\u00fcs/ kein einig sternlein", "tokens": ["gantz", "gantz", "zur", "fins\u00b7ter\u00b7n\u00fcs", "/", "kein", "ei\u00b7nig", "stern\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NE", "$(", "PIAT", "ADJD", "VAINF"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.25": {"text": "flammet/", "tokens": ["flam\u00b7met", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "und doch (was blinden trost un\u0303 rechtes laabsaal macht/)", "tokens": ["und", "doch", "(", "was", "blin\u00b7den", "trost", "u\u00f1", "rech\u00b7tes", "la\u00b7ab\u00b7saal", "macht", "/", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "PWS", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$(", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "siht mein gem\u00fcthe nicht/ ", "tokens": ["siht", "mein", "ge\u00b7m\u00fc\u00b7the", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "es liebet sie viel mehr, das licht ist jhm verhasset/", "tokens": ["es", "lie\u00b7bet", "sie", "viel", "mehr", ",", "das", "licht", "ist", "jhm", "ver\u00b7has\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "weil vor den hellen tag die f\u00fcnstre Nacht es fasset/", "tokens": ["weil", "vor", "den", "hel\u00b7len", "tag", "die", "f\u00fcnst\u00b7re", "Nacht", "es", "fas\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "denn Jhm hatt schon geraubt die hoffart flam\u0303 un\u0303 licht/", "tokens": ["denn", "Jhm", "hatt", "schon", "ge\u00b7raubt", "die", "hof\u00b7fart", "flam\u0303", "u\u00f1", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "ART", "NN", "VVINF", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "und ist verdunckelt gantz; Der Ehrgeitz l\u00e4sset nicht", "tokens": ["und", "ist", "ver\u00b7dun\u00b7ckelt", "gantz", ";", "Der", "Ehr\u00b7geitz", "l\u00e4s\u00b7set", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "ADV", "$.", "ART", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "der Sonnenglantz hinnein: die b\u00f6se lust verdun ckelt", "tokens": ["der", "Son\u00b7nen\u00b7glantz", "hin\u00b7nein", ":", "die", "b\u00f6\u00b7se", "lust", "ver\u00b7dun", "ckelt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "dieselbe gantz und gar/ da\u00df auch kein straal nicht", "tokens": ["die\u00b7sel\u00b7be", "gantz", "und", "gar", "/", "da\u00df", "auch", "kein", "straal", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADV", "KON", "ADV", "$(", "KOUS", "ADV", "PIAT", "NN", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "funckelt;", "tokens": ["fun\u00b7ckelt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "So offt derselben Bild will schleichen ins gebl\u00fcth/", "tokens": ["So", "offt", "der\u00b7sel\u00b7ben", "Bild", "will", "schlei\u00b7chen", "ins", "ge\u00b7bl\u00fcth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDAT", "NN", "VMFIN", "VVINF", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "so offt die schwartze nacht zu folgen sich bem\u00fcht.", "tokens": ["so", "offt", "die", "schwart\u00b7ze", "nacht", "zu", "fol\u00b7gen", "sich", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die augen habens guht/ so zu gewissen zeiten", "tokens": ["Die", "au\u00b7gen", "ha\u00b7bens", "guht", "/", "so", "zu", "ge\u00b7wis\u00b7sen", "zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "$(", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "dem blassen Monde nach die Sonne sehen schreiten/", "tokens": ["dem", "blas\u00b7sen", "Mon\u00b7de", "nach", "die", "Son\u00b7ne", "se\u00b7hen", "schrei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich aber bin nun gantz mit f\u00fcnstern\u00fc\u00df \u00fcmbringt/", "tokens": ["Ich", "a\u00b7ber", "bin", "nun", "gantz", "mit", "f\u00fcns\u00b7ter\u00b7n\u00fc\u00df", "\u00fcm\u00b7bringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "jemehr ich ruff und schrey/ jemehr es auf mich dringt;", "tokens": ["je\u00b7mehr", "ich", "ruff", "und", "schrey", "/", "je\u00b7mehr", "es", "auf", "mich", "dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "ADJD", "$(", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es schmertzet traun viel mehr wenn augen seyn zu", "tokens": ["Es", "schmert\u00b7zet", "traun", "viel", "mehr", "wenn", "au\u00b7gen", "seyn", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ADV", "KOUS", "NN", "VAINF", "PTKZU"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.42": {"text": "finden/ (blinden", "tokens": ["fin\u00b7den", "/", "(", "blin\u00b7den"], "token_info": ["word", "punct", "punct", "word"], "pos": ["VVINF", "$(", "$(", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.43": {"text": "und darff sie brauchen nicht/ als wenn sie einem", "tokens": ["und", "darff", "sie", "brau\u00b7chen", "nicht", "/", "als", "wenn", "sie", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "PTKNEG", "$(", "KOKOM", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "zu brauchen Gott nicht gibt: Ein Wandersman\u0303 der ist", "tokens": ["zu", "brau\u00b7chen", "Gott", "nicht", "gibt", ":", "Ein", "Wan\u00b7ders\u00b7ma\u00f1", "der", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVFIN", "NN", "PTKNEG", "VVFIN", "$.", "ART", "NN", "ART", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "voll trauren/ wenn die nacht zu kommen ist ger\u00fcst/", "tokens": ["voll", "trau\u00b7ren", "/", "wenn", "die", "nacht", "zu", "kom\u00b7men", "ist", "ge\u00b7r\u00fcst", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$(", "KOUS", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Des morgens aber kann er wieder sich erg\u00f6tzen", "tokens": ["Des", "mor\u00b7gens", "a\u00b7ber", "kann", "er", "wie\u00b7der", "sich", "er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "VMFIN", "PPER", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "mit seiner Wanderschafft/ mich aber helt in netzen", "tokens": ["mit", "sei\u00b7ner", "Wan\u00b7der\u00b7schafft", "/", "mich", "a\u00b7ber", "helt", "in", "net\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PPER", "ADV", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "gefangen allzulang anjetzt die schwartze nacht/", "tokens": ["ge\u00b7fan\u00b7gen", "all\u00b7zu\u00b7lang", "an\u00b7jetzt", "die", "schwart\u00b7ze", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "so dier/ ", "tokens": ["so", "dier", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$("], "meter": "-+", "measure": "iambic.single"}, "line.50": {"text": "Ja wann aus mitternacht die Sonne k\u00f6mt zu r\u00fccke/", "tokens": ["Ja", "wann", "aus", "mit\u00b7ter\u00b7nacht", "die", "Son\u00b7ne", "k\u00f6mt", "zu", "r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "APPR", "VVFIN", "ART", "NN", "VVFIN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "so w\u00fcndschet jedermann/ da\u00df er sie auch erblicke;", "tokens": ["so", "w\u00fcnd\u00b7schet", "je\u00b7der\u00b7mann", "/", "da\u00df", "er", "sie", "auch", "er\u00b7bli\u00b7cke", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$(", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Der R\u00f6thin-volck wendt sich dort gegen morgen hin/", "tokens": ["Der", "R\u00f6\u00b7thin\u00b7volck", "wendt", "sich", "dort", "ge\u00b7gen", "mor\u00b7gen", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "ADV", "PTKVZ", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.53": {"text": "und spricht/ willkommen sey/ du tagesbringerin.", "tokens": ["und", "spricht", "/", "will\u00b7kom\u00b7men", "sey", "/", "du", "ta\u00b7ges\u00b7brin\u00b7ge\u00b7rin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADJD", "VAFIN", "$(", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Also ich offtermahls mein traurigs Angesichte/", "tokens": ["Al\u00b7so", "ich", "off\u00b7ter\u00b7mahls", "mein", "trau\u00b7rigs", "An\u00b7ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.55": {"text": "mein hochbetr\u00fcbtes heupt hinn auf gen him\u0303el richte/", "tokens": ["mein", "hoch\u00b7be\u00b7tr\u00fcb\u00b7tes", "heupt", "hinn", "auf", "gen", "him\u0303el", "rich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.56": {"text": "und spreche/ brich herein/ du Sonne meine zier/", "tokens": ["und", "spre\u00b7che", "/", "brich", "her\u00b7ein", "/", "du", "Son\u00b7ne", "mei\u00b7ne", "zier", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "VVIMP", "PTKVZ", "$(", "PPER", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "die du so lange zeit dein Licht verborgen mier/ ", "tokens": ["die", "du", "so", "lan\u00b7ge", "zeit", "dein", "Licht", "ver\u00b7bor\u00b7gen", "mier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Ach kom\u0303! ach kom\u0303! ach kom\u0303! un\u0303 z\u00fcnd\u2019 in meinem her-", "tokens": ["Ach", "kom\u0303", "!", "ach", "kom\u0303", "!", "ach", "kom\u0303", "!", "u\u00f1", "z\u00fcnd'", "in", "mei\u00b7nem", "her"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$.", "XY", "VVIMP", "$.", "XY", "VVIMP", "$.", "KON", "VVFIN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "dein licht und fackel an/ und lindre meine schmertzen/", "tokens": ["dein", "licht", "und", "fa\u00b7ckel", "an", "/", "und", "lind\u00b7re", "mei\u00b7ne", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "PTKVZ", "$(", "KON", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "ob auch gleich solchen glantz noch nicht erlanget ich/", "tokens": ["ob", "auch", "gleich", "sol\u00b7chen", "glantz", "noch", "nicht", "er\u00b7lan\u00b7get", "ich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIAT", "NN", "ADV", "PTKNEG", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "so ist es doch genug/ da\u00df ich erbethen dich; ", "tokens": ["so", "ist", "es", "doch", "ge\u00b7nug", "/", "da\u00df", "ich", "er\u00b7be\u00b7then", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Die hoffnung l\u00e4sst mich nicht an dier zu schanden", "tokens": ["Die", "hoff\u00b7nung", "l\u00e4sst", "mich", "nicht", "an", "dier", "zu", "schan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.63": {"text": "(da\u00df ich so reden mag) weil ich noch bin auff Erden/", "tokens": ["(", "da\u00df", "ich", "so", "re\u00b7den", "mag", ")", "weil", "ich", "noch", "bin", "auff", "Er\u00b7den", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$(", "KOUS", "PPER", "ADV", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}