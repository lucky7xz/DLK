{"textgrid.poem.63067": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Friedhof", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In graden Reihen epheudichtbedeckt,", "tokens": ["In", "gra\u00b7den", "Rei\u00b7hen", "ep\u00b7heu\u00b7dicht\u00b7be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleich Betten im Spital, stehen die Gr\u00e4ber.", "tokens": ["Gleich", "Bet\u00b7ten", "im", "Spi\u00b7tal", ",", "ste\u00b7hen", "die", "Gr\u00e4\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Kreuz aus Stein vernarrte Neugier weckt,", "tokens": ["Ein", "Kreuz", "aus", "Stein", "ver\u00b7narr\u00b7te", "Neu\u00b7gier", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer hier verscharrt. Der Tag, der helle Weber,", "tokens": ["Wer", "hier", "ver\u00b7scharrt", ".", "Der", "Tag", ",", "der", "hel\u00b7le", "We\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$.", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Webt Lichterf\u00e4den um der Treu Geranien,", "tokens": ["Webt", "Lich\u00b7ter\u00b7f\u00e4\u00b7den", "um", "der", "Treu", "Ge\u00b7ra\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ein leiser Widerschein spielt in den Sarg.", "tokens": ["Ein", "lei\u00b7ser", "Wi\u00b7der\u00b7schein", "spielt", "in", "den", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie ruhen unter bl\u00fchenden Kastanien,", "tokens": ["Sie", "ru\u00b7hen", "un\u00b7ter", "bl\u00fc\u00b7hen\u00b7den", "Kas\u00b7ta\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ihr Lebenssaft steigt denen tief ins Mark.", "tokens": ["Ihr", "Le\u00b7bens\u00b7saft", "steigt", "de\u00b7nen", "tief", "ins", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PDS", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Zwischen zwei Gr\u00e4bern welken rote Blumen,", "tokens": ["Zwi\u00b7schen", "zwei", "Gr\u00e4\u00b7bern", "wel\u00b7ken", "ro\u00b7te", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Das Erdreich ist zerdr\u00fcckt, das Laub zerfetzt.", "tokens": ["Das", "Er\u00b7dreich", "ist", "zer\u00b7dr\u00fcckt", ",", "das", "Laub", "zer\u00b7fetzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Hier w\u00e4lzten sich die Nacht auf weichen Krumen", "tokens": ["Hier", "w\u00e4lz\u00b7ten", "sich", "die", "Nacht", "auf", "wei\u00b7chen", "Kru\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zwei Wildverliebte, von der Brunst gehetzt.", "tokens": ["Zwei", "Wild\u00b7ver\u00b7lieb\u00b7te", ",", "von", "der", "Brunst", "ge\u00b7hetzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "In ihre Schreie sprangen klirrend Knochen", "tokens": ["In", "ih\u00b7re", "Schrei\u00b7e", "spran\u00b7gen", "klir\u00b7rend", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und Sch\u00e4del, die nach reifem Heumond rochen.", "tokens": ["Und", "Sch\u00e4\u00b7del", ",", "die", "nach", "rei\u00b7fem", "Heu\u00b7mond", "ro\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}