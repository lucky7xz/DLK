{"textgrid.poem.47155": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann die Rosen aufgebl\u00fcht,", "tokens": ["Wann", "die", "Ro\u00b7sen", "auf\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht der Lenz zu Ende;", "tokens": ["Geht", "der", "Lenz", "zu", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wann die Sonn' am h\u00f6chsten gl\u00fcht,", "tokens": ["Wann", "die", "Sonn'", "am", "h\u00f6chs\u00b7ten", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Naht die Sonnenwende.", "tokens": ["Naht", "die", "Son\u00b7nen\u00b7wen\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Alles Leben mu\u00df hinab,", "tokens": ["Al\u00b7les", "Le\u00b7ben", "mu\u00df", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das nicht mehr kann steigen;", "tokens": ["Das", "nicht", "mehr", "kann", "stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und so will ich in mein Grab", "tokens": ["Und", "so", "will", "ich", "in", "mein", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich, o Liebchen, neigen.", "tokens": ["Mich", ",", "o", "Lieb\u00b7chen", ",", "nei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Da die Lieb' ich fand, um was", "tokens": ["Da", "die", "Lieb'", "ich", "fand", ",", "um", "was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "APPR", "PRELS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnt' ich hier noch werben?", "tokens": ["K\u00f6nnt'", "ich", "hier", "noch", "wer\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Thu' den Arm mir auf und la\u00df", "tokens": ["Thu'", "den", "Arm", "mir", "auf", "und", "la\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "PPER", "PTKVZ", "KON", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich im Kusse sterben!", "tokens": ["Mich", "im", "Kus\u00b7se", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}