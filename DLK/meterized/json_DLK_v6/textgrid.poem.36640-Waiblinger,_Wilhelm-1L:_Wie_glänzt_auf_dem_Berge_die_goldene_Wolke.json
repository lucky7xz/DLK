{"textgrid.poem.36640": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie gl\u00e4nzt auf dem Berge die goldene Wolke", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gl\u00e4nzt auf dem Berge die goldene Wolke", "tokens": ["Wie", "gl\u00e4nzt", "auf", "dem", "Ber\u00b7ge", "die", "gol\u00b7de\u00b7ne", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "So heiter und lauter dem heiteren Volke!", "tokens": ["So", "hei\u00b7ter", "und", "lau\u00b7ter", "dem", "hei\u00b7te\u00b7ren", "Vol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da wallen und schweben", "tokens": ["Da", "wal\u00b7len", "und", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und k\u00fcssen und weben", "tokens": ["Und", "k\u00fcs\u00b7sen", "und", "we\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die wankenden Bilder im wechselnden Tanz", "tokens": ["Die", "wan\u00b7ken\u00b7den", "Bil\u00b7der", "im", "wech\u00b7seln\u00b7den", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Um silberne Stirnen mit rosigem Kranz!", "tokens": ["Um", "sil\u00b7ber\u00b7ne", "Stir\u00b7nen", "mit", "ro\u00b7si\u00b7gem", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Auf j\u00e4hem Gebirge, durch gr\u00fcnende Matten,", "tokens": ["Auf", "j\u00e4\u00b7hem", "Ge\u00b7bir\u00b7ge", ",", "durch", "gr\u00fc\u00b7nen\u00b7de", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "An Str\u00f6men und Quellen, im k\u00fchlenden Schatten,", "tokens": ["An", "Str\u00f6\u00b7men", "und", "Quel\u00b7len", ",", "im", "k\u00fch\u00b7len\u00b7den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zu Meer und zu Lande,", "tokens": ["Zu", "Meer", "und", "zu", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Am blumigen Strande,", "tokens": ["Am", "blu\u00b7mi\u00b7gen", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da schreitet der Grieche, so kr\u00e4ftig und k\u00fchn!", "tokens": ["Da", "schrei\u00b7tet", "der", "Grie\u00b7che", ",", "so", "kr\u00e4f\u00b7tig", "und", "k\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es schwillt ihm der Busen im wachsenden Gl\u00fchn!", "tokens": ["Es", "schwillt", "ihm", "der", "Bu\u00b7sen", "im", "wach\u00b7sen\u00b7den", "Gl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Noch f\u00fchlt in den Tiefen, auf wolkigen Wegen,", "tokens": ["Noch", "f\u00fchlt", "in", "den", "Tie\u00b7fen", ",", "auf", "wol\u00b7ki\u00b7gen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den schwellenden Geist er der Mutter sich regen,", "tokens": ["Den", "schwel\u00b7len\u00b7den", "Geist", "er", "der", "Mut\u00b7ter", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ART", "NN", "PRF", "ADJA", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Noch f\u00fchlt er in Liebe,", "tokens": ["Noch", "f\u00fchlt", "er", "in", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit sehnendem Triebe,", "tokens": ["Mit", "seh\u00b7nen\u00b7dem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mit heil'gem Verlangen, auf jeglicher Spur,", "tokens": ["Mit", "heil'\u00b7gem", "Ver\u00b7lan\u00b7gen", ",", "auf", "jeg\u00b7li\u00b7cher", "Spur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Geliebt sich an w\u00e4rmender Brust der Natur.", "tokens": ["Ge\u00b7liebt", "sich", "an", "w\u00e4r\u00b7men\u00b7der", "Brust", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Wo hoch um die d\u00e4mmernden, ragenden H\u00f6hen", "tokens": ["Wo", "hoch", "um", "die", "d\u00e4m\u00b7mern\u00b7den", ",", "ra\u00b7gen\u00b7den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Des Adlers geschwungene Fittige wehen,", "tokens": ["Des", "Ad\u00b7lers", "ge\u00b7schwun\u00b7ge\u00b7ne", "Fit\u00b7ti\u00b7ge", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Um Kronen und Wipfel,", "tokens": ["Um", "Kro\u00b7nen", "und", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auf felsigem Gipfel;", "tokens": ["Auf", "fel\u00b7si\u00b7gem", "Gip\u00b7fel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der Wind durch die Eichen, die riesigen, saust,", "tokens": ["Der", "Wind", "durch", "die", "Ei\u00b7chen", ",", "die", "rie\u00b7si\u00b7gen", ",", "saust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da wild der Malnotte, der kr\u00e4ftige, braust.", "tokens": ["Da", "wild", "der", "Mal\u00b7not\u00b7te", ",", "der", "kr\u00e4f\u00b7ti\u00b7ge", ",", "braust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Wo mild auf das heitere Menschengewimmel", "tokens": ["Wo", "mild", "auf", "das", "hei\u00b7te\u00b7re", "Men\u00b7schen\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und jugendlich quillet der lautere Himmel:", "tokens": ["Und", "ju\u00b7gend\u00b7lich", "quil\u00b7let", "der", "lau\u00b7te\u00b7re", "Him\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Ferne, gel\u00e4utert", "tokens": ["Die", "Fer\u00b7ne", ",", "ge\u00b7l\u00e4u\u00b7tert"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und duftig erweitert,", "tokens": ["Und", "duf\u00b7tig", "er\u00b7wei\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Verschwimmt in des Meeres zerflie\u00dfendem Blau,", "tokens": ["Ver\u00b7schwimmt", "in", "des", "Mee\u00b7res", "zer\u00b7flie\u00b7\u00dfen\u00b7dem", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da geht der Korinther auf l\u00e4chelnder Au!", "tokens": ["Da", "geht", "der", "Ko\u00b7rin\u00b7ther", "auf", "l\u00e4\u00b7cheln\u00b7der", "Au", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Und unserem Auge, dem reinen, entfalten", "tokens": ["Und", "un\u00b7se\u00b7rem", "Au\u00b7ge", ",", "dem", "rei\u00b7nen", ",", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sich reicher, als allen, die ew'gen Gestalten,", "tokens": ["Sich", "rei\u00b7cher", ",", "als", "al\u00b7len", ",", "die", "ew'\u00b7gen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "KOUS", "PIS", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "In heiliger Stille,", "tokens": ["In", "hei\u00b7li\u00b7ger", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In rauschender F\u00fclle!", "tokens": ["In", "rau\u00b7schen\u00b7der", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wir sind's, die Geliebten! vom Ew'gen erf\u00fcllt!", "tokens": ["Wir", "sin\u00b7d's", ",", "die", "Ge\u00b7lieb\u00b7ten", "!", "vom", "Ew'\u00b7gen", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "$.", "APPRART", "NN", "VVPP", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Des H\u00f6chsten und Gr\u00f6\u00dften lebendigstes Bild!", "tokens": ["Des", "H\u00f6chs\u00b7ten", "und", "Gr\u00f6\u00df\u00b7ten", "le\u00b7ben\u00b7digs\u00b7tes", "Bild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "D'rum sind wir auch frey wie die Schwalb in der Wolke", "tokens": ["D'\u00b7rum", "sind", "wir", "auch", "frey", "wie", "die", "Schwalb", "in", "der", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wir sammeln uns wieder zum herrschenden Volke!", "tokens": ["Wir", "sam\u00b7meln", "uns", "wie\u00b7der", "zum", "herr\u00b7schen\u00b7den", "Vol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wir schlagen den T\u00fcrken", "tokens": ["Wir", "schla\u00b7gen", "den", "T\u00fcr\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und schaffen und wirken,", "tokens": ["Und", "schaf\u00b7fen", "und", "wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Uns f\u00fchlend, und dr\u00e4ngen uns wieder hinan!", "tokens": ["Uns", "f\u00fch\u00b7lend", ",", "und", "dr\u00e4n\u00b7gen", "uns", "wie\u00b7der", "hi\u00b7nan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und reih'n an die heiligen V\u00e4ter uns an!", "tokens": ["Und", "reih'n", "an", "die", "hei\u00b7li\u00b7gen", "V\u00e4\u00b7ter", "uns", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}