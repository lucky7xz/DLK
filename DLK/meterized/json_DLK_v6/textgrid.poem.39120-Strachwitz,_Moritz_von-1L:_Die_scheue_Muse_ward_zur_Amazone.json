{"textgrid.poem.39120": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die scheue Muse ward zur Amazone", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die scheue Muse ward zur Amazone", "tokens": ["Die", "scheu\u00b7e", "Mu\u00b7se", "ward", "zur", "A\u00b7ma\u00b7zo\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tummelt sich auf erzbeschupptem Renner;", "tokens": ["Und", "tum\u00b7melt", "sich", "auf", "erz\u00b7be\u00b7schupp\u00b7tem", "Ren\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ums Haupt den Stahlhelm statt der Bl\u00fctenkrone,", "tokens": ["Ums", "Haupt", "den", "Stahl\u00b7helm", "statt", "der", "Bl\u00fc\u00b7ten\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So st\u00fcrzt sie freudig in die Schlacht der M\u00e4nner.", "tokens": ["So", "st\u00fcrzt", "sie", "freu\u00b7dig", "in", "die", "Schlacht", "der", "M\u00e4n\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der sch\u00f6ne Busen starrt von Panzerplatten,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Bu\u00b7sen", "starrt", "von", "Pan\u00b7zer\u00b7plat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die H\u00fcfte tr\u00e4gt das Schlachtschwert an der Zona,", "tokens": ["Die", "H\u00fcf\u00b7te", "tr\u00e4gt", "das", "Schlacht\u00b7schwert", "an", "der", "Zo\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht Liebeslust und trunkenes Ermatten,", "tokens": ["Nicht", "Lie\u00b7bes\u00b7lust", "und", "trun\u00b7ke\u00b7nes", "Er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zorn glimmt im Aug' der herrlichen Bellona.", "tokens": ["Zorn", "glimmt", "im", "Aug'", "der", "herr\u00b7li\u00b7chen", "Bel\u00b7lo\u00b7na", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NE", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Die wei\u00dfe Hand, die Rosen sonst gebrochen,", "tokens": ["Die", "wei\u00b7\u00dfe", "Hand", ",", "die", "Ro\u00b7sen", "sonst", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Greift k\u00fchn nach Lorbeer jetzt und Eichenreisern,", "tokens": ["Greift", "k\u00fchn", "nach", "Lor\u00b7beer", "jetzt", "und", "Ei\u00b7chen\u00b7rei\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NE", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Straff ward die Muskel, Mark erf\u00fcllt die Knochen,", "tokens": ["Straff", "ward", "die", "Mus\u00b7kel", ",", "Mark", "er\u00b7f\u00fcllt", "die", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die weichen Glieder wurden fest und eisern.", "tokens": ["Die", "wei\u00b7chen", "Glie\u00b7der", "wur\u00b7den", "fest", "und", "ei\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer freit das Weib? Ein K\u00e4mpfer mu\u00df es werben,", "tokens": ["Wer", "freit", "das", "Weib", "?", "Ein", "K\u00e4mp\u00b7fer", "mu\u00df", "es", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergessen sind der Siegwart und der Werther;", "tokens": ["Ver\u00b7ges\u00b7sen", "sind", "der", "Sieg\u00b7wart", "und", "der", "Wert\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Das Brautlied singt vom Siegen oder Sterben,", "tokens": ["Das", "Braut\u00b7lied", "singt", "vom", "Sie\u00b7gen", "o\u00b7der", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Brautfackeln sind entbl\u00f6\u00dfte Flammenschwerter.", "tokens": ["Braut\u00b7fac\u00b7keln", "sind", "ent\u00b7bl\u00f6\u00df\u00b7te", "Flam\u00b7men\u00b7schwer\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Reicht mir den Speer, doch sei er von den schwersten,", "tokens": ["Reicht", "mir", "den", "Speer", ",", "doch", "sei", "er", "von", "den", "schwers\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schnallt mir den Panzer um, ich will es wagen;", "tokens": ["Schnallt", "mir", "den", "Pan\u00b7zer", "um", ",", "ich", "will", "es", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die Besten seh' ich meines Volks, die Ersten,", "tokens": ["Die", "Bes\u00b7ten", "seh'", "ich", "mei\u00b7nes", "Volks", ",", "die", "Ers\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An mir vor\u00fcber auf das Schlachtfeld jagen.", "tokens": ["An", "mir", "vor\u00b7\u00fc\u00b7ber", "auf", "das", "Schlacht\u00b7feld", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Fort mit dem Helm, es soll mich Jeder kennen,", "tokens": ["Fort", "mit", "dem", "Helm", ",", "es", "soll", "mich", "Je\u00b7der", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ich will dich freien, sch\u00f6ne Bradamante.", "tokens": ["Ich", "will", "dich", "frei\u00b7en", ",", "sch\u00f6\u00b7ne", "Bra\u00b7da\u00b7man\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}