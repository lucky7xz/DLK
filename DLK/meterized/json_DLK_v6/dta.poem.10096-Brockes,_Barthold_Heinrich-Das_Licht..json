{"dta.poem.10096": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann ich, in des Himmels H\u00f6he,", "tokens": ["Wann", "ich", ",", "in", "des", "Him\u00b7mels", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen Glantz, o Sonne, sehe,", "tokens": ["Dei\u00b7nen", "Glantz", ",", "o", "Son\u00b7ne", ",", "se\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Sch\u00f6nheit, deine Pracht;", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", ",", "dei\u00b7ne", "Pracht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fchmt mein fr\u00f6hliches Gem\u00fcthe", "tokens": ["R\u00fchmt", "mein", "fr\u00f6h\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dessen Allmacht, Weisheit, G\u00fcte,", "tokens": ["Des\u00b7sen", "All\u00b7macht", ",", "Weis\u00b7heit", ",", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welcher dich so sch\u00f6n gemacht.", "tokens": ["Wel\u00b7cher", "dich", "so", "sch\u00f6n", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In der Lust, die ich empfinde,", "tokens": ["In", "der", "Lust", ",", "die", "ich", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchl ich, da\u00df ich mich entz\u00fcnde.", "tokens": ["F\u00fchl", "ich", ",", "da\u00df", "ich", "mich", "ent\u00b7z\u00fcn\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PRF", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die entflammte Seele spricht:", "tokens": ["Die", "ent\u00b7flamm\u00b7te", "See\u00b7le", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da sie an den Sch\u00f6pfer dencket:", "tokens": ["Da", "sie", "an", "den", "Sch\u00f6p\u00b7fer", "den\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schencke mir das ew'ge Licht.", "tokens": ["Schen\u00b7cke", "mir", "das", "ew'\u00b7ge", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}