{"textgrid.poem.43286": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ei lache nicht, es werden wohl", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei lache nicht, es werden wohl", "tokens": ["Ei", "la\u00b7che", "nicht", ",", "es", "wer\u00b7den", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch einmal meine Tr\u00e4ume wahr,", "tokens": ["Noch", "ein\u00b7mal", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn es nicht morgen kommen soll,", "tokens": ["Wenn", "es", "nicht", "mor\u00b7gen", "kom\u00b7men", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt alles Gl\u00fcck doch \u00fcber's Jahr.", "tokens": ["Kommt", "al\u00b7les", "Gl\u00fcck", "doch", "\u00fc\u00b7ber's", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist die Jugend, ich bin jung,", "tokens": ["Du", "bist", "die", "Ju\u00b7gend", ",", "ich", "bin", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sehen weit, wir gehen weit,", "tokens": ["Wir", "se\u00b7hen", "weit", ",", "wir", "ge\u00b7hen", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir haben Muth und Kraft genung,", "tokens": ["Wir", "ha\u00b7ben", "Muth", "und", "Kraft", "ge\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor uns liegt eine lange Zeit.", "tokens": ["Vor", "uns", "liegt", "ei\u00b7ne", "lan\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ei lache nicht! und sage nicht,", "tokens": ["Ei", "la\u00b7che", "nicht", "!", "und", "sa\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "$.", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sei ein Tr\u00e4umer, ein Poet,", "tokens": ["Ich", "sei", "ein", "Tr\u00e4u\u00b7mer", ",", "ein", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du selber bist mir ein Gedicht,", "tokens": ["Du", "sel\u00b7ber", "bist", "mir", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie keines in den B\u00fcchern steht.", "tokens": ["Wie", "kei\u00b7nes", "in", "den", "B\u00fc\u00b7chern", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}