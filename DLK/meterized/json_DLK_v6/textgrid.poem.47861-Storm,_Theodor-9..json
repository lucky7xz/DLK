{"textgrid.poem.47861": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgen wird's! Am Waldesrande", "tokens": ["Mor\u00b7gen", "wird's", "!", "Am", "Wal\u00b7des\u00b7ran\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "$.", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitz ich hier und spintisier;", "tokens": ["Sitz", "ich", "hier", "und", "spin\u00b7ti\u00b7sier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, jedweder meiner Schritte", "tokens": ["Ach", ",", "jed\u00b7we\u00b7der", "mei\u00b7ner", "Schrit\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trug mich weiter fort von dir!", "tokens": ["Trug", "mich", "wei\u00b7ter", "fort", "von", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKVZ", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vielen ging ich schon vor\u00fcber;", "tokens": ["Vie\u00b7len", "ging", "ich", "schon", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer w\u00fcnscht ich mich zur\u00fcck;", "tokens": ["Nim\u00b7mer", "w\u00fcnscht", "ich", "mich", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum fl\u00fcstern heut die L\u00fcfte:", "tokens": ["Wa\u00b7rum", "fl\u00fcs\u00b7tern", "heut", "die", "L\u00fcf\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesmal aber war's das Gl\u00fcck!", "tokens": ["Dies\u00b7mal", "a\u00b7ber", "wa\u00b7r's", "das", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Von den B\u00e4umen Tauestropfen", "tokens": ["Von", "den", "B\u00e4u\u00b7men", "Tau\u00b7es\u00b7trop\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fallen auf mein hei\u00df Gesicht \u2013", "tokens": ["Fal\u00b7len", "auf", "mein", "hei\u00df", "Ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sankt C\u00e4cilia! Solch Paar Augen", "tokens": ["Sankt", "C\u00e4\u00b7ci\u00b7lia", "!", "Solch", "Paar", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "$.", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah ich all mein Lebtag nicht!", "tokens": ["Sah", "ich", "all", "mein", "Leb\u00b7tag", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Stadttrompeter, Organiste!", "tokens": ["Stadt\u00b7trom\u00b7pe\u00b7ter", ",", "Or\u00b7ga\u00b7nis\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4r's denn wirklich gar so dumm? \u2013", "tokens": ["W\u00e4r's", "denn", "wirk\u00b7lich", "gar", "so", "dumm", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Holla hoch, ihr jungen Beine,", "tokens": ["Hol\u00b7la", "hoch", ",", "ihr", "jun\u00b7gen", "Bei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht euch auf! Wir kehren um.", "tokens": ["Macht", "euch", "auf", "!", "Wir", "keh\u00b7ren", "um", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ruf nur, Kuckuck, dort im Walde!", "tokens": ["Ruf", "nur", ",", "Ku\u00b7ckuck", ",", "dort", "im", "Wal\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehst so bald mich nun nicht mehr,", "tokens": ["Siehst", "so", "bald", "mich", "nun", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn in Puder und Manschetten", "tokens": ["Denn", "in", "Pu\u00b7der", "und", "Man\u00b7schet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schreit ich ehrenfest einher.", "tokens": ["Schreit", "ich", "eh\u00b7ren\u00b7fest", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Golden spielt der Staub der Stra\u00dfen \u2013", "tokens": ["Gol\u00b7den", "spielt", "der", "Staub", "der", "Stra\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz, Geduld! bald bist du da.", "tokens": ["Herz", ",", "Ge\u00b7duld", "!", "bald", "bist", "du", "da", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei! wie lieblich soll es klingen:", "tokens": ["Hei", "!", "wie", "lieb\u00b7lich", "soll", "es", "klin\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Musikus und Musika!", "tokens": ["Mu\u00b7si\u00b7kus", "und", "Mu\u00b7si\u00b7ka", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}