{"dta.poem.9186": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n  Da\u00df n\u00e4rrische Ding die Liebe.  \n Worinn meistentheils auf gewisse spr\u00fcchw\u00f6rter gezielet  \n wird/ welche bey einer bekannten Compagnie  \n im brauche waren.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr leute lasset euch in liebessachen ein!", "tokens": ["Ihr", "leu\u00b7te", "las\u00b7set", "euch", "in", "lie\u00b7bes\u00b7sa\u00b7chen", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann wo die liebe nicht auf erden solte seyn/", "tokens": ["Dann", "wo", "die", "lie\u00b7be", "nicht", "auf", "er\u00b7den", "sol\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "VVFIN", "PTKNEG", "APPR", "NN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00e4r das liebe ding die Eva nicht geschaffen/", "tokens": ["So", "w\u00e4r", "das", "lie\u00b7be", "ding", "die", "E\u00b7va", "nicht", "ge\u00b7schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wann es s\u00fcnde w\u00e4r/ solth\u00e4tens nicht die pfaffen.", "tokens": ["Und", "wann", "es", "s\u00fcn\u00b7de", "w\u00e4r", "/", "solt\u00b7h\u00e4\u00b7tens", "nicht", "die", "pfaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJA", "VAFIN", "$(", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es kan nicht unrecht seyn/ wells die Juristen thun:", "tokens": ["Es", "kan", "nicht", "un\u00b7recht", "seyn", "/", "wells", "die", "Ju\u00b7ris\u00b7ten", "thun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "VAINF", "$(", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Es ist nicht ungesund/ weil nicht die Aertzte ruhn:", "tokens": ["Es", "ist", "nicht", "un\u00b7ge\u00b7sund", "/", "weil", "nicht", "die", "A\u00b7ertz\u00b7te", "ruhn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$(", "KOUS", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Wanns unnat\u00fcrlich w\u00e4r/ so w\u00fcrd es die nicht jucken/", "tokens": ["Wanns", "un\u00b7na\u00b7t\u00fcr\u00b7lich", "w\u00e4r", "/", "so", "w\u00fcrd", "es", "die", "nicht", "ju\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$(", "ADV", "VAFIN", "PPER", "ART", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die der Philosophie bi\u00df an den nabel gucken.", "tokens": ["Die", "der", "Phi\u00b7lo\u00b7so\u00b7phie", "bi\u00df", "an", "den", "na\u00b7bel", "gu\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "(manum inserere in sinum Philosophiae. Terentius", "tokens": ["(", "ma\u00b7num", "in\u00b7se\u00b7re\u00b7re", "in", "si\u00b7num", "Phi\u00b7lo\u00b7so\u00b7phi\u00b7ae", ".", "Te\u00b7ren\u00b7ti\u00b7us"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "NE"], "meter": "+-+-+-+-+-+-+-+--+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Heaut. act. 3. sc. 3. v. 2. &amp; 3.)", "tokens": ["He\u00b7aut", ".", "act", ".", "3.", "sc.", "3.", "v.", "2.", "&amp;", "3.", ")"], "token_info": ["word", "punct", "word", "punct", "ordinal", "abbreviation", "ordinal", "abbreviation", "ordinal", "XML_entity", "ordinal", "punct"], "pos": ["NE", "$.", "FM.la", "$.", "ADJA", "NN", "ADV", "APPRART", "ADJA", "NN", "ADJA", "$("], "meter": "+--", "measure": "dactylic.init"}, "line.11": {"text": "Wann es altv\u00e4trisch w\u00e4r/ so w\u00e4r es nicht beliebt", "tokens": ["Wann", "es", "alt\u00b7v\u00e4t\u00b7risch", "w\u00e4r", "/", "so", "w\u00e4r", "es", "nicht", "be\u00b7liebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "$(", "ADV", "VAFIN", "PPER", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von einem der sich auf die neue mode giebt:", "tokens": ["Von", "ei\u00b7nem", "der", "sich", "auf", "die", "neu\u00b7e", "mo\u00b7de", "giebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PRELS", "PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und w\u00e4r es kinder-spiel/ so w\u00fcrden nicht die alten", "tokens": ["Und", "w\u00e4r", "es", "kin\u00b7der\u00b7spiel", "/", "so", "w\u00fcr\u00b7den", "nicht", "die", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "$(", "ADV", "VAFIN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Auf diese leckerey so grosse st\u00fccke halten.", "tokens": ["Auf", "die\u00b7se", "le\u00b7cke\u00b7rey", "so", "gros\u00b7se", "st\u00fc\u00b7cke", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es ist auch nicht Franz\u00f6sch/ die Teutschen kom\u0303en auch", "tokens": ["Es", "ist", "auch", "nicht", "Fran\u00b7z\u00f6sch", "/", "die", "Teut\u00b7schen", "kom\u0303en", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "NN", "$(", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Von augen in das hertz/ vom hertzen in den bauch.", "tokens": ["Von", "au\u00b7gen", "in", "das", "hertz", "/", "vom", "hert\u00b7zen", "in", "den", "bauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$(", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es ist auch nicht Catolsch/ wan\u0303 wir die h\u00e4ndel treiben/", "tokens": ["Es", "ist", "auch", "nicht", "Ca\u00b7tol\u00b7sch", "/", "wa\u00f1", "wir", "die", "h\u00e4n\u00b7del", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "NE", "$(", "PWAV", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Denn sonsten lissen es die Lutheraner bleiben. (mann/", "tokens": ["Denn", "sons\u00b7ten", "lis\u00b7sen", "es", "die", "Lu\u00b7the\u00b7ra\u00b7ner", "blei\u00b7ben", ".", "(", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$.", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "Es ist kein schelmenst\u00fcck/ sonst gieng ein handwercks-", "tokens": ["Es", "ist", "kein", "schel\u00b7men\u00b7st\u00fcck", "/", "sonst", "gieng", "ein", "handwercks"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$(", "ADV", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Der ehrlich bleiben wil/ nicht so mit freuden dran.", "tokens": ["Der", "ehr\u00b7lich", "blei\u00b7ben", "wil", "/", "nicht", "so", "mit", "freu\u00b7den", "dran", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "VMFIN", "$(", "PTKNEG", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es ist auch keine kunst es kans ein ieder bauer/", "tokens": ["Es", "ist", "auch", "kei\u00b7ne", "kunst", "es", "kans", "ein", "ie\u00b7der", "bau\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "PPER", "VMFIN", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der liebet seine frau und hertzt sie auf die thauer.", "tokens": ["Der", "lie\u00b7bet", "sei\u00b7ne", "frau", "und", "hertzt", "sie", "auf", "die", "thau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die liebe klinget nicht/ sonst w\u00e4rs f\u00fcrwahr nicht gut/", "tokens": ["Die", "lie\u00b7be", "klin\u00b7get", "nicht", "/", "sonst", "w\u00e4rs", "f\u00fcr\u00b7wahr", "nicht", "gut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "$(", "ADV", "VAFIN", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Man h\u00f6rte draussen all\u2019s was man im hause thut.", "tokens": ["Man", "h\u00f6r\u00b7te", "draus\u00b7sen", "all's", "was", "man", "im", "hau\u00b7se", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIS", "PWS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die liebe stincket nicht sonst k\u00f6nte man es riechen/", "tokens": ["Die", "lie\u00b7be", "stin\u00b7cket", "nicht", "sonst", "k\u00f6n\u00b7te", "man", "es", "rie\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "ADV", "VMFIN", "PIS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Alsbald wann ha und sie in einen winckel kriechen.", "tokens": ["Als\u00b7bald", "wann", "ha", "und", "sie", "in", "ei\u00b7nen", "win\u00b7ckel", "krie\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NE", "KON", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die liebe sieht man nicht/ was w\u00fcrde sonst daraus/", "tokens": ["Die", "lie\u00b7be", "sieht", "man", "nicht", "/", "was", "w\u00fcr\u00b7de", "sonst", "da\u00b7raus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "PTKNEG", "$(", "PWS", "VAFIN", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die leute leschen ja darzu die liechter au\u00df.", "tokens": ["Die", "leu\u00b7te", "le\u00b7schen", "ja", "dar\u00b7zu", "die", "liech\u00b7ter", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sie kan nicht bitter seyn/ dann solt es herbe schmecken/", "tokens": ["Sie", "kan", "nicht", "bit\u00b7ter", "seyn", "/", "dann", "solt", "es", "her\u00b7be", "schme\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$(", "ADV", "VMFIN", "PPER", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So w\u00fcrde mancher nicht darnach die finger lecken.", "tokens": ["So", "w\u00fcr\u00b7de", "man\u00b7cher", "nicht", "dar\u00b7nach", "die", "fin\u00b7ger", "le\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKNEG", "PAV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die lieb ist nicht berust/ sonst w\u00e4re niemand wei\u00df:", "tokens": ["Die", "lieb", "ist", "nicht", "be\u00b7rust", "/", "sonst", "w\u00e4\u00b7re", "nie\u00b7mand", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PTKNEG", "VVFIN", "$(", "ADV", "VAFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wei\u00df kan sie auch nicht seyn/ dann schwartz beh\u00e4lt den", "tokens": ["Wei\u00df", "kan", "sie", "auch", "nicht", "seyn", "/", "dann", "schwartz", "be\u00b7h\u00e4lt", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PTKNEG", "VAINF", "$(", "ADV", "ADJD", "VVFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "(prei\u00df:", "tokens": ["(", "prei\u00df", ":"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Viel wenger ist sie schwer/ man kan sie leichtlich fassen/", "tokens": ["Viel", "wen\u00b7ger", "ist", "sie", "schwer", "/", "man", "kan", "sie", "leicht\u00b7lich", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "ADJD", "$(", "PIS", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und der ist wol ein narr/ der sich wil helffen lassen.", "tokens": ["Und", "der", "ist", "wol", "ein", "narr", "/", "der", "sich", "wil", "helf\u00b7fen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADV", "ART", "NN", "$(", "PRELS", "PRF", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die liebe kan ja nicht von pfefferkuchen seyn/", "tokens": ["Die", "lie\u00b7be", "kan", "ja", "nicht", "von", "pfef\u00b7fer\u00b7ku\u00b7chen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "PTKNEG", "APPR", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die bauren w\u00fcrden sie sonst auf den hirse streun:", "tokens": ["Die", "bau\u00b7ren", "w\u00fcr\u00b7den", "sie", "sonst", "auf", "den", "hir\u00b7se", "streun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Kein welckes r\u00fcbgen nicht/ sie w\u00fcrde nicht gelitten/", "tokens": ["Kein", "wel\u00b7ckes", "r\u00fcb\u00b7gen", "nicht", "/", "sie", "w\u00fcr\u00b7de", "nicht", "ge\u00b7lit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKNEG", "$(", "PPER", "VAFIN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die jungfern h\u00e4tten sie schon l\u00e4ngsten klein geschnitte\u0303.", "tokens": ["Die", "jung\u00b7fern", "h\u00e4t\u00b7ten", "sie", "schon", "l\u00e4ngs\u00b7ten", "klein", "ge\u00b7schnit\u00b7t\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sie ist kein k\u00e4se nicht/ denn solte dieses seyn/", "tokens": ["Sie", "ist", "kein", "k\u00e4\u00b7se", "nicht", "/", "denn", "sol\u00b7te", "die\u00b7ses", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$(", "KON", "VMFIN", "PDAT", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So steckte mancher eh sein scharffes messer ein:", "tokens": ["So", "steck\u00b7te", "man\u00b7cher", "eh", "sein", "scharf\u00b7fes", "mes\u00b7ser", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "KOUS", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Sie ist kein h\u00e4scher nicht/ wer h\u00e4tte sonst verlangen/", "tokens": ["Sie", "ist", "kein", "h\u00e4\u00b7scher", "nicht", "/", "wer", "h\u00e4t\u00b7te", "sonst", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "PTKNEG", "$(", "PWS", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da\u00df ihn die liebe solt in ihrem loche fangen?", "tokens": ["Da\u00df", "ihn", "die", "lie\u00b7be", "solt", "in", "ih\u00b7rem", "lo\u00b7che", "fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VMFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die liebe brennet nicht/ sonst w\u00fcrd ein gantzes land", "tokens": ["Die", "lie\u00b7be", "bren\u00b7net", "nicht", "/", "sonst", "w\u00fcrd", "ein", "gant\u00b7zes", "land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "$(", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Den ersten hochzeit-tag bi\u00dfweilen abgebrannt:", "tokens": ["Den", "ers\u00b7ten", "hoch\u00b7zeit\u00b7tag", "bi\u00df\u00b7wei\u00b7len", "ab\u00b7ge\u00b7brannt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die liebe k\u00fchlet nicht/ sonst w\u00fcrde man nicht schwitze\u0303;", "tokens": ["Die", "lie\u00b7be", "k\u00fch\u00b7let", "nicht", "/", "sonst", "w\u00fcr\u00b7de", "man", "nicht", "schwitz\u1ebd", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "$(", "ADV", "VAFIN", "PIS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die liebe sticht auch nicht/ sonst w\u00fcrde man sich ritzen;", "tokens": ["Die", "lie\u00b7be", "sticht", "auch", "nicht", "/", "sonst", "w\u00fcr\u00b7de", "man", "sich", "rit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "PTKNEG", "$(", "ADV", "VAFIN", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Die lieb ist auch kein floh/ sonst w\u00e4r gefahr dabey/", "tokens": ["Die", "lieb", "ist", "auch", "kein", "floh", "/", "sonst", "w\u00e4r", "ge\u00b7fahr", "da\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "PIAT", "VVFIN", "$(", "ADV", "VAFIN", "ADJD", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Die jungfern knickten ihr den kopf geschwind entzwey;", "tokens": ["Die", "jung\u00b7fern", "knick\u00b7ten", "ihr", "den", "kopf", "ge\u00b7schwind", "ent\u00b7zwey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Sie ist kein klapperstorch/ sonst w\u00e4r es grosse schande/", "tokens": ["Sie", "ist", "kein", "klap\u00b7per\u00b7storch", "/", "sonst", "w\u00e4r", "es", "gros\u00b7se", "schan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$(", "ADV", "VAFIN", "PPER", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "So bald es winter w\u00e4r/ so z\u00f6g sie aus dem lande.", "tokens": ["So", "bald", "es", "win\u00b7ter", "w\u00e4r", "/", "so", "z\u00f6g", "sie", "aus", "dem", "lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VAFIN", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Die liebe thut nicht weh und bringet schlechte pein/", "tokens": ["Die", "lie\u00b7be", "thut", "nicht", "weh", "und", "brin\u00b7get", "schlech\u00b7te", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "PTKVZ", "KON", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Sonst w\u00fcrd ein jungfer-maul nicht also wolfeil seyn;", "tokens": ["Sonst", "w\u00fcrd", "ein", "jung\u00b7fer\u00b7maul", "nicht", "al\u00b7so", "wol\u00b7feil", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Die w\u00e4re wol ein kind/ wann jemand greiffen wolte/", "tokens": ["Die", "w\u00e4\u00b7re", "wol", "ein", "kind", "/", "wann", "je\u00b7mand", "greif\u00b7fen", "wol\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$(", "PWAV", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Da\u00df sie vor freuden nicht ein bi\u00dfgen lachen solte.", "tokens": ["Da\u00df", "sie", "vor", "freu\u00b7den", "nicht", "ein", "bi\u00df\u00b7gen", "la\u00b7chen", "sol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Es bleibet wol dabey/ nur hurtig lieb gehabt/", "tokens": ["Es", "blei\u00b7bet", "wol", "da\u00b7bey", "/", "nur", "hur\u00b7tig", "lieb", "ge\u00b7habt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$(", "ADV", "ADJD", "ADJD", "VAPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Ach wohl dem/ welcher sich in seiner jugend labt/", "tokens": ["Ach", "wohl", "dem", "/", "wel\u00b7cher", "sich", "in", "sei\u00b7ner", "ju\u00b7gend", "labt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "$(", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Ein alter taugt nicht mehr/ er spielt nur mit geberden/", "tokens": ["Ein", "al\u00b7ter", "taugt", "nicht", "mehr", "/", "er", "spielt", "nur", "mit", "ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "ADV", "$(", "PPER", "VVFIN", "ADV", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Und weil er sonst nichts kan/ mu\u00df er zum han - - -", "tokens": ["Und", "weil", "er", "sonst", "nichts", "kan", "/", "mu\u00df", "er", "zum", "han"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "VMFIN", "$(", "VMFIN", "PPER", "APPRART", "VAFIN", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}