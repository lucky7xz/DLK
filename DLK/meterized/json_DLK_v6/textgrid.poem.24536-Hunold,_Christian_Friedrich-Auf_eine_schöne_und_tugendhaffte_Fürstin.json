{"textgrid.poem.24536": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine sch\u00f6ne und tugendhaffte F\u00fcrstin", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erz\u00fcrne F\u00fcrstin nicht/ da\u00df dich mein Hertz verehrt/", "tokens": ["Er\u00b7z\u00fcr\u00b7ne", "F\u00fcrs\u00b7tin", "nicht", "/", "da\u00df", "dich", "mein", "Hertz", "ver\u00b7ehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachdem es mich dein Stand und deine Sch\u00f6nheit lehrt.", "tokens": ["Nach\u00b7dem", "es", "mich", "dein", "Stand", "und", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Himmel machet dich zur F\u00fcrstin auf der Erden:", "tokens": ["Der", "Him\u00b7mel", "ma\u00b7chet", "dich", "zur", "F\u00fcrs\u00b7tin", "auf", "der", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Liebe soll ja nur die Welt beherrschet werden.", "tokens": ["Mit", "Lie\u00b7be", "soll", "ja", "nur", "die", "Welt", "be\u00b7herr\u00b7schet", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APP