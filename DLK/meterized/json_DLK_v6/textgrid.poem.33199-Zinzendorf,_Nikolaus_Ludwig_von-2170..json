{"textgrid.poem.33199": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2170.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil das T\u00e4ublein darinne sitzt \u2013 weinet nach Christi seel.", "tokens": ["Weil", "das", "T\u00e4ub\u00b7lein", "da\u00b7rin\u00b7ne", "sitzt", "\u2013", "wei\u00b7net", "nach", "Chris\u00b7ti", "seel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PAV", "VVFIN", "$(", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "--+--+-++--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Gib ihnen zur \u2013 wirthschafts-gabe.", "tokens": ["Gib", "ih\u00b7nen", "zur", "\u2013", "wirth\u00b7schafts\u00b7ga\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "$(", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Gott verleih dir \u2013 M\u00fctter-seele.", "tokens": ["Gott", "ver\u00b7leih", "dir", "\u2013", "M\u00fct\u00b7te\u00b7rsee\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$(", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Breit aus die fl\u00fcgel \u2013 unverletzet seyn.", "tokens": ["Breit", "aus", "die", "fl\u00fc\u00b7gel", "\u2013", "un\u00b7ver\u00b7let\u00b7zet", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Den leib, die seel \u2013 kein gutes sparen.", "tokens": ["Den", "leib", ",", "die", "seel", "\u2013", "kein", "gu\u00b7tes", "spa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$(", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Im friede JESU Christ, \u2013 siegle deine wahl.", "tokens": ["Im", "frie\u00b7de", "Je\u00b7SU", "Christ", ",", "\u2013", "sieg\u00b7le", "dei\u00b7ne", "wahl", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "NE", "$,", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}