{"dta.poem.17801": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "44.  Was verdirbt/ wird zu nichts.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Was fort un\u0304 fort verdirbt/ das kan nicht stehn noch seyn/", "tokens": ["Was", "fort", "un\u0304", "fort", "ver\u00b7dirbt", "/", "das", "kan", "nicht", "stehn", "noch", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$(", "PDS", "VMFIN", "PTKNEG", "VVFIN", "ADV", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es eilt zum untergang und wird dem nichts gemein.", "tokens": ["Es", "eilt", "zum", "un\u00b7ter\u00b7gang", "und", "wird", "dem", "nichts", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VAFIN", "ART", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}