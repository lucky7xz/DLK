{"textgrid.poem.40751": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der tapfre kluge Held Roland", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der tapfre kluge Held Roland", "tokens": ["Der", "tapf\u00b7re", "klu\u00b7ge", "Held", "Ro\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "NE"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Kam wandernd an der Loire Strand,", "tokens": ["Kam", "wan\u00b7dernd", "an", "der", "Loi\u00b7re", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Wasser ist nicht sein Element,", "tokens": ["Das", "Was\u00b7ser", "ist", "nicht", "sein", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hei, wer da schwimmen und fliegen k\u00f6nnt',", "tokens": ["Hei", ",", "wer", "da", "schwim\u00b7men", "und", "flie\u00b7gen", "k\u00f6nnt'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "ADV", "VVINF", "KON", "VVINF", "VMFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Als Fischlein darinnen z\u00f6ge,", "tokens": ["Als", "Fisc\u00b7hlein", "da\u00b7rin\u00b7nen", "z\u00f6\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als V\u00f6glein dar\u00fcber fl\u00f6ge!", "tokens": ["Als", "V\u00f6\u00b7glein", "da\u00b7r\u00fc\u00b7ber", "fl\u00f6\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbho, F\u00e4hrmann, rudre flink heran", "tokens": ["\u00bb", "ho", ",", "F\u00e4hr\u00b7mann", ",", "rud\u00b7re", "flink", "he\u00b7ran"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hol' mich hin\u00fcber auf gutem Kahn;", "tokens": ["Und", "hol'", "mich", "hin\u00b7\u00fc\u00b7ber", "auf", "gu\u00b7tem", "Kahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Doch wisse zuvor, ich bin ein Held", "tokens": ["Doch", "wis\u00b7se", "zu\u00b7vor", ",", "ich", "bin", "ein", "Held"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht ohne Ruhm, doch ohne Geld,", "tokens": ["Nicht", "oh\u00b7ne", "Ruhm", ",", "doch", "oh\u00b7ne", "Geld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Degen klingt in der Scheide,", "tokens": ["Der", "De\u00b7gen", "klingt", "in", "der", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der S\u00e4ckel verstummt im Leide.\u00ab", "tokens": ["Der", "S\u00e4\u00b7ckel", "ver\u00b7stummt", "im", "Lei\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbei, Ritter, ihr zahlt wohl bessern Sold,", "tokens": ["\u00bb", "ei", ",", "Rit\u00b7ter", ",", "ihr", "zahlt", "wohl", "bes\u00b7sern", "Sold", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weisheit geht \u00fcber Silber und Gold;", "tokens": ["Weis\u00b7heit", "geht", "\u00fc\u00b7ber", "Sil\u00b7ber", "und", "Gold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Gebt mir drei Lehren klug und fein,", "tokens": ["Gebt", "mir", "drei", "Leh\u00b7ren", "klug", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "CARD", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das soll mein liebster F\u00e4hrlohn sein!", "tokens": ["Das", "soll", "mein", "liebs\u00b7ter", "F\u00e4hr\u00b7lohn", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Geld versinkt im Weine,", "tokens": ["Das", "Geld", "ver\u00b7sinkt", "im", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Klugheit hilft auf die Beine.\u00ab", "tokens": ["Klug\u00b7heit", "hilft", "auf", "die", "Bei\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Und als er stie\u00df vom Ufer ab,", "tokens": ["Und", "als", "er", "stie\u00df", "vom", "U\u00b7fer", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Roland die erste Lehr' ihm gab:", "tokens": ["Ro\u00b7land", "die", "ers\u00b7te", "Lehr'", "ihm", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bbein Anfang ist kein Meisterst\u00fcck,", "tokens": ["\u00bb", "ein", "An\u00b7fang", "ist", "kein", "Meis\u00b7ter\u00b7st\u00fcck", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch guter Anfang halbes Gl\u00fcck.\u00ab", "tokens": ["Doch", "gu\u00b7ter", "An\u00b7fang", "hal\u00b7bes", "Gl\u00fcck", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Ferge meint: \u00bbErtr\u00e4glich!", "tokens": ["Der", "Fer\u00b7ge", "meint", ":", "\u00bb", "Er\u00b7tr\u00e4g\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir sang's die Amme schon t\u00e4glich.\u00ab", "tokens": ["Mir", "sang's", "die", "Am\u00b7me", "schon", "t\u00e4g\u00b7lich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Und als inmitten des Stroms der Kahn,", "tokens": ["Und", "als", "in\u00b7mit\u00b7ten", "des", "Stroms", "der", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da hub Roland zum zweiten an:", "tokens": ["Da", "hub", "Ro\u00b7land", "zum", "zwei\u00b7ten", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPRART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbnichts Halbes thu' ein ganzer Mann,", "tokens": ["\u00bb", "nichts", "Hal\u00b7bes", "thu'", "ein", "gan\u00b7zer", "Mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ganz vollende, was er begann!\u00ab", "tokens": ["Der", "ganz", "voll\u00b7en\u00b7de", ",", "was", "er", "be\u00b7gann", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ADJA", "$,", "PWS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Der Ferge seufzt: \u00bbVerst\u00e4ndlich!", "tokens": ["Der", "Fer\u00b7ge", "seufzt", ":", "\u00bb", "Ver\u00b7st\u00e4nd\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch wann kommt Neues endlich?\u00ab", "tokens": ["Doch", "wann", "kommt", "Neu\u00b7es", "end\u00b7lich", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "VVFIN", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als der Kahn nun dr\u00fcben am Strand,", "tokens": ["Und", "als", "der", "Kahn", "nun", "dr\u00fc\u00b7ben", "am", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da sprach zum dritten Held Roland:", "tokens": ["Da", "sprach", "zum", "drit\u00b7ten", "Held", "Ro\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbsei leicht der Anfang oder schwer,", "tokens": ["\u00bb", "sei", "leicht", "der", "An\u00b7fang", "o\u00b7der", "schwer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ende nur bringt Lob und Ehr.\u00ab", "tokens": ["Das", "En\u00b7de", "nur", "bringt", "Lob", "und", "Ehr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Ferge murmelt leise:", "tokens": ["Der", "Fer\u00b7ge", "mur\u00b7melt", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbfast bin ich selbst so weise!\u00ab", "tokens": ["\u00bb", "fast", "bin", "ich", "selbst", "so", "wei\u00b7se", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbnun, Freund, des Paktes sind wir quitt,", "tokens": ["\u00bb", "nun", ",", "Freund", ",", "des", "Pak\u00b7tes", "sind", "wir", "quitt", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nimm noch Eins als Aufgeld mit:", "tokens": ["Doch", "nimm", "noch", "Eins", "als", "Auf\u00b7geld", "mit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "PIS", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn stets dein Handel diesem gleicht,", "tokens": ["Wenn", "stets", "dein", "Han\u00b7del", "die\u00b7sem", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann tr\u00e4gst du deinen Reichthum leicht,", "tokens": ["Dann", "tr\u00e4gst", "du", "dei\u00b7nen", "Reicht\u00b7hum", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ersparst du Streit den Erben,", "tokens": ["Er\u00b7sparst", "du", "Streit", "den", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wirst nicht als Kr\u00f6sus sterben.\u00ab", "tokens": ["Wirst", "nicht", "als", "Kr\u00f6\u00b7sus", "ster\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PTKNEG", "KOUS", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ans Ufer Held Roland sich schwang", "tokens": ["Ans", "U\u00b7fer", "Held", "Ro\u00b7land", "sich", "schwang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "NE", "PRF", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und schritt des Wegs f\u00fcrba\u00df mit Sang;", "tokens": ["Und", "schritt", "des", "Wegs", "f\u00fcr\u00b7ba\u00df", "mit", "Sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schiffer ruderte heim gemach", "tokens": ["Der", "Schif\u00b7fer", "ru\u00b7der\u00b7te", "heim", "ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dachte den guten Lehren nach;", "tokens": ["Und", "dach\u00b7te", "den", "gu\u00b7ten", "Leh\u00b7ren", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Wellen fl\u00fcstern und kreisen", "tokens": ["Die", "Wel\u00b7len", "fl\u00fcs\u00b7tern", "und", "krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und singen die alten Weisen.", "tokens": ["Und", "sin\u00b7gen", "die", "al\u00b7ten", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}