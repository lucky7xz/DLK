{"textgrid.poem.25062": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "In Granada", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie oft mit ihr vom Winterherde,", "tokens": ["Wie", "oft", "mit", "ihr", "vom", "Win\u00b7ter\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn au\u00dfen kalt die Flocke fiel,", "tokens": ["Wenn", "au\u00b7\u00dfen", "kalt", "die", "Flo\u00b7cke", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tr\u00e4umt' ich mich nach dem Lenz der Erde,", "tokens": ["Tr\u00e4umt'", "ich", "mich", "nach", "dem", "Lenz", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem gr\u00fcnen Hochthal am Genil!", "tokens": ["Dem", "gr\u00fc\u00b7nen", "Hoch\u00b7thal", "am", "Ge\u00b7nil", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da durch der Mondnacht D\u00e4mmerhelle", "tokens": ["Da", "durch", "der", "Mond\u00b7nacht", "D\u00e4m\u00b7mer\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu der Alhambra Zackenthor", "tokens": ["Zu", "der", "Al\u00b7hamb\u00b7ra", "Za\u00b7cken\u00b7thor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trug sie beim hellen Klang der Schelle", "tokens": ["Trug", "sie", "beim", "hel\u00b7len", "Klang", "der", "Schel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Saumtier neben mir empor.", "tokens": ["Das", "Saum\u00b7tier", "ne\u00b7ben", "mir", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir ruhten in den Zauberhallen,", "tokens": ["Wir", "ruh\u00b7ten", "in", "den", "Zau\u00b7ber\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo einsam nun der Brunnen rauscht", "tokens": ["Wo", "ein\u00b7sam", "nun", "der", "Brun\u00b7nen", "rauscht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit des Westens Nachtigallen", "tokens": ["Und", "mit", "des", "Wes\u00b7tens", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Peri Bagdads Worte tauscht.", "tokens": ["Die", "Pe\u00b7ri", "Bag\u00b7dads", "Wor\u00b7te", "tauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und unten aus der Schlucht der Myrten", "tokens": ["Und", "un\u00b7ten", "aus", "der", "Schlucht", "der", "Myr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stob mit der wilden Str\u00e4uche Duft", "tokens": ["Stob", "mit", "der", "wil\u00b7den", "Str\u00e4u\u00b7che", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zu uns das n\u00e4cht'ge Lied der Hirten", "tokens": ["Zu", "uns", "das", "n\u00e4cht'\u00b7ge", "Lied", "der", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Empor durch die berauschte Luft.", "tokens": ["Em\u00b7por", "durch", "die", "be\u00b7rauschte", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Es war ein Traum; nicht nach dem S\u00fcden,", "tokens": ["Es", "war", "ein", "Traum", ";", "nicht", "nach", "dem", "S\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu fernern K\u00fcsten brach sie auf,", "tokens": ["Zu", "fer\u00b7nern", "K\u00fcs\u00b7ten", "brach", "sie", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weiter trug allein mich M\u00fcden", "tokens": ["Und", "wei\u00b7ter", "trug", "al\u00b7lein", "mich", "M\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Lebensstromes irrer Lauf.", "tokens": ["Des", "Le\u00b7bens\u00b7stro\u00b7mes", "ir\u00b7rer", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun spielt um mich auf wei\u00dfen Platten", "tokens": ["Nun", "spielt", "um", "mich", "auf", "wei\u00b7\u00dfen", "Plat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im L\u00f6wenhof der Mondenschein;", "tokens": ["Im", "L\u00f6\u00b7wen\u00b7hof", "der", "Mon\u00b7den\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein er wirft nur ", "tokens": ["Al\u00b7lein", "er", "wirft", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Nur meinen auf den Marmorstein.", "tokens": ["Nur", "mei\u00b7nen", "auf", "den", "Mar\u00b7mor\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}