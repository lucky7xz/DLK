{"textgrid.poem.42468": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Schlacht ist verloren, die Schlacht ist aus,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schlacht ist verloren, die Schlacht ist aus,", "tokens": ["Die", "Schlacht", "ist", "ver\u00b7lo\u00b7ren", ",", "die", "Schlacht", "ist", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der K\u00f6nig taumelt ins n\u00e4chstbeste Haus,", "tokens": ["Der", "K\u00f6\u00b7nig", "tau\u00b7melt", "ins", "n\u00e4chst\u00b7bes\u00b7te", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die letzten Sch\u00fcsse verschallen.", "tokens": ["Die", "letz\u00b7ten", "Sch\u00fcs\u00b7se", "ver\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wie er todm\u00fcde sinkt aufs Stroh,", "tokens": ["Und", "wie", "er", "tod\u00b7m\u00fc\u00b7de", "sinkt", "aufs", "Stroh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wer \u00e4fft ihn? Ein Spottbrief schadenfroh:", "tokens": ["Wer", "\u00e4fft", "ihn", "?", "Ein", "Spott\u00b7brief", "scha\u00b7den\u00b7froh", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bbphaeton ist gefallen.\u00ab", "tokens": ["\u00bb", "phae\u00b7ton", "ist", "ge\u00b7fal\u00b7len", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Bosheit selbst hier, und Scheelsucht und Neid?", "tokens": ["Bos\u00b7heit", "selbst", "hier", ",", "und", "Scheel\u00b7sucht", "und", "Neid", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Bleiben die drei in Ewigkeit", "tokens": ["Blei\u00b7ben", "die", "drei", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "CARD", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der Menschheit h\u00fcndischer Bettel?", "tokens": ["Der", "Menschheit", "h\u00fcn\u00b7di\u00b7scher", "Bet\u00b7tel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der K\u00f6nig las es und l\u00e4chelte, schlief,", "tokens": ["Der", "K\u00f6\u00b7nig", "las", "es", "und", "l\u00e4\u00b7chel\u00b7te", ",", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Schlief ein paar Stunden gut und tief,", "tokens": ["Schlief", "ein", "paar", "Stun\u00b7den", "gut", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und erwacht und sieht wieder den Zettel.", "tokens": ["Und", "er\u00b7wacht", "und", "sieht", "wie\u00b7der", "den", "Zet\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Er stutzt, er besinnt sich. Wer hat sich erkeckt?", "tokens": ["Er", "stutzt", ",", "er", "be\u00b7sinnt", "sich", ".", "Wer", "hat", "sich", "er\u00b7keckt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "$.", "PWS", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wer h\u00f6hnt ihn? Wer glaubt ihn zu Boden gestreckt?", "tokens": ["Wer", "h\u00f6hnt", "ihn", "?", "Wer", "glaubt", "ihn", "zu", "Bo\u00b7den", "ge\u00b7streckt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sein Auge wird hell und heiter.", "tokens": ["Sein", "Au\u00b7ge", "wird", "hell", "und", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbdie Herren Generals!\u00ab Er nimmt den Wisch", "tokens": ["\u00bb", "die", "Her\u00b7ren", "Ge\u00b7ne\u00b7rals", "!", "\u00ab", "Er", "nimmt", "den", "Wisch"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und legt ihn ruhig auf den Tisch:", "tokens": ["Und", "legt", "ihn", "ru\u00b7hig", "auf", "den", "Tisch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbwir bataillieren weiter!\u00ab", "tokens": ["\u00bb", "wir", "ba\u00b7tail\u00b7lie\u00b7ren", "wei\u00b7ter", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja, wer verstand je das Genie;", "tokens": ["Ja", ",", "wer", "ver\u00b7stand", "je", "das", "Ge\u00b7nie", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wandert allein, es begreift sich nie,", "tokens": ["Es", "wan\u00b7dert", "al\u00b7lein", ",", "es", "be\u00b7greift", "sich", "nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und niemand wirds fassen lernen.", "tokens": ["Und", "nie\u00b7mand", "wirds", "fas\u00b7sen", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVINF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Fridericus Rex, deine Sonne loht,", "tokens": ["Fri\u00b7de\u00b7ri\u00b7cus", "Rex", ",", "dei\u00b7ne", "Son\u00b7ne", "loht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Du einsamer Mensch in Leben und Tod,", "tokens": ["Du", "ein\u00b7sa\u00b7mer", "Mensch", "in", "Le\u00b7ben", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Unter den ewigen Sternen.", "tokens": ["Un\u00b7ter", "den", "e\u00b7wi\u00b7gen", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}