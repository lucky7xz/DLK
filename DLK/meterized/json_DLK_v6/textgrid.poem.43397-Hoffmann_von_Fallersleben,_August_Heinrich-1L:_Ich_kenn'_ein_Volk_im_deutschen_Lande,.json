{"textgrid.poem.43397": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kenn' ein Volk im deutschen Lande,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenn' ein Volk im deutschen Lande,", "tokens": ["Ich", "kenn'", "ein", "Volk", "im", "deut\u00b7schen", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das macht von sich ein gro\u00df Geschrei,", "tokens": ["Das", "macht", "von", "sich", "ein", "gro\u00df", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PRF", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Als ob auf seinem d\u00fcrren Sande", "tokens": ["Als", "ob", "auf", "sei\u00b7nem", "d\u00fcr\u00b7ren", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Tugend, Kunst und Weisheit sei,", "tokens": ["Nur", "Tu\u00b7gend", ",", "Kunst", "und", "Weis\u00b7heit", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nirgend wachs' und bl\u00fch' als dort", "tokens": ["Und", "nir\u00b7gend", "wachs'", "und", "bl\u00fch'", "als", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "KON", "VVFIN", "KOKOM", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch freie Schrift und freies Wort.", "tokens": ["Noch", "frei\u00b7e", "Schrift", "und", "frei\u00b7es", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich kenn' ein Volk, das sich hienieden", "tokens": ["Ich", "kenn'", "ein", "Volk", ",", "das", "sich", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sehr heilig zu geberden wei\u00df,", "tokens": ["Sehr", "hei\u00b7lig", "zu", "ge\u00b7ber\u00b7den", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und Demuth, Seelenruh' und Frieden", "tokens": ["Und", "De\u00b7muth", ",", "See\u00b7len\u00b7ruh'", "und", "Frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4lt f\u00fcr den h\u00f6chsten Erdenpreis,", "tokens": ["H\u00e4lt", "f\u00fcr", "den", "h\u00f6chs\u00b7ten", "Er\u00b7den\u00b7preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und alle B\u00f6cke gro\u00df und klein", "tokens": ["Und", "al\u00b7le", "B\u00f6\u00b7cke", "gro\u00df", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verwandeln m\u00f6cht in L\u00e4mmelein.", "tokens": ["Ver\u00b7wan\u00b7deln", "m\u00f6cht", "in", "L\u00e4m\u00b7mel\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich kenn' ein Volk, das Alles meistert,", "tokens": ["Ich", "kenn'", "ein", "Volk", ",", "das", "Al\u00b7les", "meis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Alles besser wei\u00df und kann,", "tokens": ["Und", "Al\u00b7les", "bes\u00b7ser", "wei\u00df", "und", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sich f\u00fcr Alles schnell begeistert,", "tokens": ["Das", "sich", "f\u00fcr", "Al\u00b7les", "schnell", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr allerlei und jedermann,", "tokens": ["F\u00fcr", "al\u00b7ler\u00b7lei", "und", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das jeden thut in Bann und Acht,", "tokens": ["Das", "je\u00b7den", "thut", "in", "Bann", "und", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "NN", "KON", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der's nicht so meint und anders macht.", "tokens": ["Der's", "nicht", "so", "meint", "und", "an\u00b7ders", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich kenn' ein Volk, das sich alleine", "tokens": ["Ich", "kenn'", "ein", "Volk", ",", "das", "sich", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom lieben Gott begnadet h\u00e4lt,", "tokens": ["Vom", "lie\u00b7ben", "Gott", "be\u00b7gna\u00b7det", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und glaubt, da\u00df Seine Sonne scheine", "tokens": ["Und", "glaubt", ",", "da\u00df", "Sei\u00b7ne", "Son\u00b7ne", "schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am sch\u00f6nsten ", "tokens": ["Am", "sch\u00f6ns\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPRART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und da\u00df es ohne Schmeichelei", "tokens": ["Und", "da\u00df", "es", "oh\u00b7ne", "Schmei\u00b7che\u00b7lei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Erde Licht- und Glanzpunkt sei.", "tokens": ["Der", "Er\u00b7de", "Licht", "und", "Glanz\u00b7punkt", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "TRUNC", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich kenn' ein Volk, das sich f\u00fcr G\u00e4ste", "tokens": ["Ich", "kenn'", "ein", "Volk", ",", "das", "sich", "f\u00fcr", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Paradieses hier schon h\u00e4lt,", "tokens": ["Des", "Pa\u00b7ra\u00b7die\u00b7ses", "hier", "schon", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem t\u00e4glich Gott das Allerbeste", "tokens": ["Dem", "t\u00e4g\u00b7lich", "Gott", "das", "Al\u00b7ler\u00b7bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seinen Tisch zur Labung stellt,", "tokens": ["Auf", "sei\u00b7nen", "Tisch", "zur", "La\u00b7bung", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dem sein eignes D\u00fcnnebier", "tokens": ["Und", "dem", "sein", "eig\u00b7nes", "D\u00fcn\u00b7ne\u00b7bier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mehr ist als Sect und Malvasier.", "tokens": ["Mehr", "ist", "als", "Sect", "und", "Mal\u00b7va\u00b7sier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich kenn' ein Volk, das vor dem Lichte", "tokens": ["Ich", "kenn'", "ein", "Volk", ",", "das", "vor", "dem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wahrheit nicht zu beben meint,", "tokens": ["Der", "Wahr\u00b7heit", "nicht", "zu", "be\u00b7ben", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sich als Quell der Weltgeschichte", "tokens": ["Das", "sich", "als", "Quell", "der", "Welt\u00b7ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz wohlgef\u00e4llig selbst erscheint,", "tokens": ["Ganz", "wohl\u00b7ge\u00b7f\u00e4l\u00b7lig", "selbst", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und denkt: die Welt versiegt gar schnell,", "tokens": ["Und", "denkt", ":", "die", "Welt", "ver\u00b7siegt", "gar", "schnell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn sie nicht sch\u00f6pft aus diesem Quell.", "tokens": ["Wenn", "sie", "nicht", "sch\u00f6pft", "aus", "die\u00b7sem", "Quell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Zu diesem Volke m\u00fc\u00dft ihr wandern", "tokens": ["Zu", "die\u00b7sem", "Vol\u00b7ke", "m\u00fc\u00dft", "ihr", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und unter ihnen H\u00fctten bau'n,", "tokens": ["Und", "un\u00b7ter", "ih\u00b7nen", "H\u00fct\u00b7ten", "bau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr m\u00fc\u00dft vergessen alles Andern", "tokens": ["Ihr", "m\u00fc\u00dft", "ver\u00b7ges\u00b7sen", "al\u00b7les", "An\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVPP", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nur ", "tokens": ["Und", "nur"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Dann wird euch allen hell und klar,", "tokens": ["Dann", "wird", "euch", "al\u00b7len", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie viel an diesem Liede wahr.", "tokens": ["Wie", "viel", "an", "die\u00b7sem", "Lie\u00b7de", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}