{"textgrid.poem.36689": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Monte Pincio", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O h\u00e4tt' ich Farben, Aether und Seel' und Geist,", "tokens": ["O", "h\u00e4tt'", "ich", "Far\u00b7ben", ",", "A\u00b7e\u00b7ther", "und", "Seel'", "und", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du unaussprechlich himmlische Luft, getaucht", "tokens": ["Du", "un\u00b7aus\u00b7sprech\u00b7lich", "himm\u00b7li\u00b7sche", "Luft", ",", "ge\u00b7taucht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In deiner Sch\u00f6nheit s\u00fc\u00dfen Abgrund,", "tokens": ["In", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "s\u00fc\u00b7\u00dfen", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4r' ich dein Priester, dein reinster S\u00e4ugling,", "tokens": ["W\u00e4r'", "ich", "dein", "Pries\u00b7ter", ",", "dein", "reins\u00b7ter", "S\u00e4ug\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "W\u00e4r' ich geboren, w\u00e4r' ich erzogen auch", "tokens": ["W\u00e4r'", "ich", "ge\u00b7bo\u00b7ren", ",", "w\u00e4r'", "ich", "er\u00b7zo\u00b7gen", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "VAFIN", "PPER", "VVPP", "ADV"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "An deinem Lichtquell, k\u00f6nnt' ich die Reinheit doch,", "tokens": ["An", "dei\u00b7nem", "Licht\u00b7quell", ",", "k\u00f6nnt'", "ich", "die", "Rein\u00b7heit", "doch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "K\u00f6nnt' ich die Milde nicht, die W\u00e4rme", "tokens": ["K\u00f6nnt'", "ich", "die", "Mil\u00b7de", "nicht", ",", "die", "W\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dieser Natur in mein Lied einhauchen.", "tokens": ["Die\u00b7ser", "Na\u00b7tur", "in", "mein", "Lied", "ein\u00b7hau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wenn du zuerst in schauender Seligkeit", "tokens": ["Wenn", "du", "zu\u00b7erst", "in", "schau\u00b7en\u00b7der", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Voll Unschuld in ein gl\u00fchendes Auge schaust,", "tokens": ["Voll", "Un\u00b7schuld", "in", "ein", "gl\u00fc\u00b7hen\u00b7des", "Au\u00b7ge", "schaust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn du vergehst in seiner Tiefe,", "tokens": ["Wenn", "du", "ver\u00b7gehst", "in", "sei\u00b7ner", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du verschmachtest in seiner Seele,", "tokens": ["Wenn", "du", "ver\u00b7schmach\u00b7test", "in", "sei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "O dann vielleicht verstehest die Wonne du,", "tokens": ["O", "dann", "viel\u00b7leicht", "ver\u00b7ste\u00b7hest", "die", "Won\u00b7ne", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Verstehst mein \u00fcberschw\u00e4nglich Entz\u00fccken du,", "tokens": ["Ver\u00b7stehst", "mein", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich", "Ent\u00b7z\u00fc\u00b7cken", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und meine Liebe zu des S\u00fcdens", "tokens": ["Und", "mei\u00b7ne", "Lie\u00b7be", "zu", "des", "S\u00fc\u00b7dens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bl\u00fchendem Gr\u00fcn und krystallnem Himmel.", "tokens": ["Bl\u00fc\u00b7hen\u00b7dem", "Gr\u00fcn", "und", "krys\u00b7tall\u00b7nem", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Drum w\u00e4hlt' ich dich zum Liebling, mein Pincio,", "tokens": ["Drum", "w\u00e4hlt'", "ich", "dich", "zum", "Lieb\u00b7ling", ",", "mein", "Pin\u00b7cio", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Roms un\u00fcbersehbare Herrlichkeit,", "tokens": ["Wo", "Roms", "un\u00b7\u00fc\u00b7ber\u00b7seh\u00b7ba\u00b7re", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie ein emp\u00f6rtes Meer, vom sonn'gen", "tokens": ["Wie", "ein", "em\u00b7p\u00f6r\u00b7tes", "Meer", ",", "vom", "sonn'\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fcgel das Marsfeld hinunter leuchtet.", "tokens": ["H\u00fc\u00b7gel", "das", "Mars\u00b7feld", "hin\u00b7un\u00b7ter", "leuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPO", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Denn solch ein Anblick gr\u00f6\u00dfer erscheint er stets,", "tokens": ["Denn", "solch", "ein", "An\u00b7blick", "gr\u00f6\u00b7\u00dfer", "er\u00b7scheint", "er", "stets", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So oft er dir in all' der Gewalt ersteht,", "tokens": ["So", "oft", "er", "dir", "in", "all'", "der", "Ge\u00b7walt", "er\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "APPR", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Womit das Schicksal ihn geheiligt,", "tokens": ["Wo\u00b7mit", "das", "Schick\u00b7sal", "ihn", "ge\u00b7hei\u00b7ligt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6nige, Consuln, C\u00e4sare, P\u00e4bste.", "tokens": ["K\u00f6\u00b7ni\u00b7ge", ",", "Con\u00b7suln", ",", "C\u00e4\u00b7sa\u00b7re", ",", "P\u00e4bs\u00b7te", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NE", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Doch oft im d\u00fcnnen Laubwald versteck' ich mich.", "tokens": ["Doch", "oft", "im", "d\u00fcn\u00b7nen", "Laub\u00b7wald", "ver\u00b7steck'", "ich", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn so uns\u00e4glich blendend aus gl\u00e4nzenden", "tokens": ["Wenn", "so", "un\u00b7s\u00e4g\u00b7lich", "blen\u00b7dend", "aus", "gl\u00e4n\u00b7zen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "ADJD", "APPR", "ADJA"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Lichthellen Bl\u00fcthen und Geb\u00fcschen", "tokens": ["Licht\u00b7hel\u00b7len", "Bl\u00fc\u00b7then", "und", "Ge\u00b7b\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach wie elysisch die Ferne strahlet.", "tokens": ["Ach", "wie", "e\u00b7ly\u00b7sisch", "die", "Fer\u00b7ne", "strah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Dann auf Borghese's \u00fcppigem Schattenhain,", "tokens": ["Dann", "auf", "Bor\u00b7ghe\u00b7se's", "\u00fcp\u00b7pi\u00b7gem", "Schat\u00b7ten\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Des Pinienwaldes m\u00e4chtiger Masse ruht", "tokens": ["Des", "Pi\u00b7ni\u00b7en\u00b7wal\u00b7des", "m\u00e4ch\u00b7ti\u00b7ger", "Mas\u00b7se", "ruht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mein Blick, sich an der Villa wilder", "tokens": ["Mein", "Blick", ",", "sich", "an", "der", "Vil\u00b7la", "wil\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRF", "APPR", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lockend arkadischer Nacht erquickend.", "tokens": ["Lo\u00b7ckend", "ar\u00b7ka\u00b7di\u00b7scher", "Nacht", "er\u00b7qui\u00b7ckend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Blaud\u00e4mmernd \u00fcber wallendem Eichengr\u00fcn,", "tokens": ["Blau\u00b7d\u00e4m\u00b7mernd", "\u00fc\u00b7ber", "wal\u00b7len\u00b7dem", "Ei\u00b7chen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "++-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "In seiner L\u00fcfte liebliches Bad getaucht,", "tokens": ["In", "sei\u00b7ner", "L\u00fcf\u00b7te", "lieb\u00b7li\u00b7ches", "Bad", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NE", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entsteigt mir selbst von fern mein sch\u00f6ner", "tokens": ["Ent\u00b7steigt", "mir", "selbst", "von", "fern", "mein", "sch\u00f6\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJD", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einsamer Freund, der Sorakte, m\u00e4hlig.", "tokens": ["Ein\u00b7sa\u00b7mer", "Freund", ",", "der", "So\u00b7rak\u00b7te", ",", "m\u00e4h\u00b7lig", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Kein Tag vergeht, wo, tr\u00e4umender Muse voll,", "tokens": ["Kein", "Tag", "ver\u00b7geht", ",", "wo", ",", "tr\u00e4u\u00b7men\u00b7der", "Mu\u00b7se", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PWAV", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich dieses Paradies nicht durchwandere,", "tokens": ["Ich", "die\u00b7ses", "Pa\u00b7ra\u00b7dies", "nicht", "durch\u00b7wan\u00b7de\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch schwach ist nur mein Lied, das bess're", "tokens": ["Doch", "schwach", "ist", "nur", "mein", "Lied", ",", "das", "bess'\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fliehet als Seufzer von meiner Lippe.", "tokens": ["Flie\u00b7het", "als", "Seuf\u00b7zer", "von", "mei\u00b7ner", "Lip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}