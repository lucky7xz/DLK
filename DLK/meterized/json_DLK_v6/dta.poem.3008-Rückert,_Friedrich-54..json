{"dta.poem.3008": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es gibt der Dinge viel, von denen, statt zu wissen,", "tokens": ["Es", "gibt", "der", "Din\u00b7ge", "viel", ",", "von", "de\u00b7nen", ",", "statt", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "APPR", "PRELS", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Weisen irgendwas zu meinen sind beflissen:", "tokens": ["Die", "Wei\u00b7sen", "ir\u00b7gend\u00b7was", "zu", "mei\u00b7nen", "sind", "be\u00b7flis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Meinen h\u00e4nget zwar das Irren an gemeinlich,", "tokens": ["Dem", "Mei\u00b7nen", "h\u00e4n\u00b7get", "zwar", "das", "Ir\u00b7ren", "an", "ge\u00b7mein\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was dir halbwahr scheint, das ist halbfalsch wahrscheinlich.", "tokens": ["Und", "was", "dir", "halb\u00b7wahr", "scheint", ",", "das", "ist", "halb\u00b7falsch", "wahr\u00b7schein\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVFIN", "$,", "PDS", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Doch ohn' ein hier und dort vorl\u00e4ufig Ausgedachtes,", "tokens": ["Doch", "ohn'", "ein", "hier", "und", "dort", "vor\u00b7l\u00e4u\u00b7fig", "Aus\u00b7ge\u00b7dach\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADV", "KON", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4r' endlich nirgendwo ein wirklich Ausgemachtes.", "tokens": ["W\u00e4r'", "end\u00b7lich", "nir\u00b7gend\u00b7wo", "ein", "wirk\u00b7lich", "Aus\u00b7ge\u00b7mach\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Darum entschlie\u00dfe dich zu Schl\u00fcssen kurz und gut,", "tokens": ["Da\u00b7rum", "ent\u00b7schlie\u00b7\u00dfe", "dich", "zu", "Schl\u00fcs\u00b7sen", "kurz", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zu Vermuthungen verliere nicht den Muth.", "tokens": ["Und", "zu", "Ver\u00b7mu\u00b7thun\u00b7gen", "ver\u00b7lie\u00b7re", "nicht", "den", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Seis nur ein mit Vernunft nicht Unvereinliches,", "tokens": ["Seis", "nur", "ein", "mit", "Ver\u00b7nunft", "nicht", "Un\u00b7ver\u00b7ein\u00b7li\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "APPR", "NN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo noch ein Wahres fehlt, steh' ein Wahrscheinliches!", "tokens": ["Wo", "noch", "ein", "Wah\u00b7res", "fehlt", ",", "steh'", "ein", "Wahr\u00b7schein\u00b7li\u00b7ches", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+--+---", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Du mu\u00dft nur immer fein bereit seyn und nicht s\u00e4umen,", "tokens": ["Du", "mu\u00dft", "nur", "im\u00b7mer", "fein", "be\u00b7reit", "seyn", "und", "nicht", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJD", "ADJD", "VAINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald das Wahre kommt, den Platz ihm einzur\u00e4umen.", "tokens": ["So\u00b7bald", "das", "Wah\u00b7re", "kommt", ",", "den", "Platz", "ihm", "ein\u00b7zu\u00b7r\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "$,", "ART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}