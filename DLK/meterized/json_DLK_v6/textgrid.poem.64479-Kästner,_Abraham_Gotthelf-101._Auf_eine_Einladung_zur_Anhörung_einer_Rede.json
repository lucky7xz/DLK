{"textgrid.poem.64479": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "101. Auf eine Einladung zur Anh\u00f6rung einer Rede", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, deine Wissenschaft, dein redlich Herz zu ehren,", "tokens": ["Freund", ",", "dei\u00b7ne", "Wis\u00b7sen\u00b7schaft", ",", "dein", "red\u00b7lich", "Herz", "zu", "eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Erkenn' ich stets f\u00fcr meine Pflicht:", "tokens": ["Er\u00b7kenn'", "ich", "stets", "f\u00fcr", "mei\u00b7ne", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lachen \u00fcber dich will ich gewi\u00dflich nicht!", "tokens": ["Und", "la\u00b7chen", "\u00fc\u00b7ber", "dich", "will", "ich", "ge\u00b7wi\u00df\u00b7lich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Deswegen mag ich dich nicht h\u00f6ren.", "tokens": ["Des\u00b7we\u00b7gen", "mag", "ich", "dich", "nicht", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}