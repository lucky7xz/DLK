{"textgrid.poem.37379": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Fuchs und Gans", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war die erste Maiennacht.", "tokens": ["Es", "war", "die", "ers\u00b7te", "Mai\u00b7en\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Mensch im Dorf hat mehr gewacht.", "tokens": ["Kein", "Mensch", "im", "Dorf", "hat", "mehr", "ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hielten, wie es stets der Fall,", "tokens": ["Da", "hiel\u00b7ten", ",", "wie", "es", "stets", "der", "Fall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Tiere ihren Fr\u00fchlingsball.", "tokens": ["Die", "Tie\u00b7re", "ih\u00b7ren", "Fr\u00fch\u00b7lings\u00b7ball", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Gans, die gute Adelheid,", "tokens": ["Die", "Gans", ",", "die", "gu\u00b7te", "A\u00b7del\u00b7heid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fehlt nie bei solcher Festlichkeit,", "tokens": ["Fehlt", "nie", "bei", "sol\u00b7cher", "Fest\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Obgleich man sie nach altem Brauch", "tokens": ["Ob\u00b7gleich", "man", "sie", "nach", "al\u00b7tem", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu necken pflegt. So heute auch.", "tokens": ["Zu", "ne\u00b7cken", "pflegt", ".", "So", "heu\u00b7te", "auch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$.", "ADV", "ADV", "ADV", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Frau Schnabel, nannte sie der Kater,", "tokens": ["Frau", "Schna\u00b7bel", ",", "nann\u00b7te", "sie", "der", "Ka\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Frau Plattfu\u00df, rief der Ziegenvater;", "tokens": ["Frau", "Platt\u00b7fu\u00df", ",", "rief", "der", "Zie\u00b7gen\u00b7va\u00b7ter", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sie, zwar l\u00e4chelnd aber k\u00fchl,", "tokens": ["Doch", "sie", ",", "zwar", "l\u00e4\u00b7chelnd", "a\u00b7ber", "k\u00fchl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fcllt sich in sanftes Selbstgef\u00fchl.", "tokens": ["H\u00fcllt", "sich", "in", "sanf\u00b7tes", "Selbst\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "So sa\u00df sie denn in \u00f6dem Schweigen", "tokens": ["So", "sa\u00df", "sie", "denn", "in", "\u00f6\u00b7dem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Allein f\u00fcr sich bei Spiel und Reigen,", "tokens": ["Al\u00b7lein", "f\u00fcr", "sich", "bei", "Spiel", "und", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Freudenl\u00e4rm und Jubeljux.", "tokens": ["Bei", "Freu\u00b7den\u00b7l\u00e4rm", "und", "Ju\u00b7bel\u00b7jux", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh da, zum Schlu\u00df hat auch der Fuchs", "tokens": ["Sieh", "da", ",", "zum", "Schlu\u00df", "hat", "auch", "der", "Fuchs"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "APPRART", "NN", "VAFIN", "ADV", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich ungeladen eingedr\u00e4ngelt.", "tokens": ["Sich", "un\u00b7ge\u00b7la\u00b7den", "ein\u00b7ge\u00b7dr\u00e4n\u00b7gelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schlau hat er sich herangeschl\u00e4ngelt.", "tokens": ["Schlau", "hat", "er", "sich", "her\u00b7an\u00b7ge\u00b7schl\u00e4n\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr Diener, s\u00e4uselt er galant,", "tokens": ["Ihr", "Die\u00b7ner", ",", "s\u00e4u\u00b7selt", "er", "ga\u00b7lant", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie geht's der Sch\u00f6nsten in Brabant?", "tokens": ["Wie", "geht's", "der", "Sch\u00f6ns\u00b7ten", "in", "Bra\u00b7bant", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich k\u00fcss' der gn\u00e4d'gen Frau den Fittich.", "tokens": ["Ich", "k\u00fcss'", "der", "gn\u00e4d'\u00b7gen", "Frau", "den", "Fit\u00b7tich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist noch ein T\u00e4nzchen frei, so bitt ich.", "tokens": ["Ist", "noch", "ein", "T\u00e4nz\u00b7chen", "frei", ",", "so", "bitt", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie nickt versch\u00e4mt: O Herr Baron!", "tokens": ["Sie", "nickt", "ver\u00b7sch\u00e4mt", ":", "O", "Herr", "Ba\u00b7ron", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "NE", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Indem, so walzen sie auch schon.", "tokens": ["In\u00b7dem", ",", "so", "wal\u00b7zen", "sie", "auch", "schon", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie trippeln die F\u00fc\u00dfe, wie wippeln die Schw\u00e4nze", "tokens": ["Wie", "trip\u00b7peln", "die", "F\u00fc\u00b7\u00dfe", ",", "wie", "wip\u00b7peln", "die", "Schw\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Im lustigen Kehraus, dem letzten der T\u00e4nze.", "tokens": ["Im", "lus\u00b7ti\u00b7gen", "Ke\u00b7hraus", ",", "dem", "letz\u00b7ten", "der", "T\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da t\u00f6nt es vier mit lautem Schlag.", "tokens": ["Da", "t\u00f6nt", "es", "vier", "mit", "lau\u00b7tem", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Fest ist aus. Es naht der Tag. \u2013", "tokens": ["Das", "Fest", "ist", "aus", ".", "Es", "naht", "der", "Tag", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Bald drauf im fr\u00fchsten Morgenschimmer", "tokens": ["Bald", "drauf", "im", "fr\u00fchs\u00b7ten", "Mor\u00b7gen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ging Mutter Urschel aus, wie immer", "tokens": ["Ging", "Mut\u00b7ter", "Ur\u00b7schel", "aus", ",", "wie", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "NE", "PTKVZ", "$,", "PWAV", "ADV"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.3": {"text": "Mit Korb und Sichel, um verstohlen", "tokens": ["Mit", "Korb", "und", "Si\u00b7chel", ",", "um", "ver\u00b7stoh\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NE", "$,", "KOUI", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich etwas fremden Klee zu holen.", "tokens": ["Sich", "et\u00b7was", "frem\u00b7den", "Klee", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An einer Hecke bleibt sie stehn:", "tokens": ["An", "ei\u00b7ner", "He\u00b7cke", "bleibt", "sie", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herrje, was ist denn hier geschehn?", "tokens": ["Herr\u00b7je", ",", "was", "ist", "denn", "hier", "ge\u00b7schehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die F\u00fcchse, sag ich, soll man r\u00e4dern.", "tokens": ["Die", "F\u00fcch\u00b7se", ",", "sag", "ich", ",", "soll", "man", "r\u00e4\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das sind wahrhaftig G\u00e4nsefedern.", "tokens": ["Das", "sind", "wahr\u00b7haf\u00b7tig", "G\u00e4n\u00b7se\u00b7fe\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein frisches Ei liegt dicht daneben.", "tokens": ["Ein", "fri\u00b7sches", "Ei", "liegt", "dicht", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich bin so frei es aufzuheben.", "tokens": ["Ich", "bin", "so", "frei", "es", "auf\u00b7zu\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ach, armes Tier, sprach sie bewegt,", "tokens": ["Ach", ",", "ar\u00b7mes", "Tier", ",", "sprach", "sie", "be\u00b7wegt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dies Ei hast du vor Angst gelegt.", "tokens": ["Dies", "Ei", "hast", "du", "vor", "Angst", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}