{"dta.poem.10064": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schmuck der Seele.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie fromme GOTT-ergebne Seelen,", "tokens": ["Wie", "from\u00b7me", "GoT\u00b7T\u00b7er\u00b7geb\u00b7ne", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit den Wundern sich verm\u00e4hlen,", "tokens": ["Die", "mit", "den", "Wun\u00b7dern", "sich", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Seine Lieb\u2019 hervorgebracht,", "tokens": ["Die", "Sei\u00b7ne", "Lieb'", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch ihre Pracht", "tokens": ["Durch", "ih\u00b7re", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vergn\u00fcgt erquicket,", "tokens": ["Ver\u00b7gn\u00fcgt", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Durch ihre Sch\u00f6nheit selbst geschm\u00fccket,", "tokens": ["Durch", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "selbst", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Sch\u00f6pfer Himmels und der Erden", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "Him\u00b7mels", "und", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vergn\u00fcgen und gefallen werden;", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "und", "ge\u00b7fal\u00b7len", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So kommen solche Seelen mir,", "tokens": ["So", "kom\u00b7men", "sol\u00b7che", "See\u00b7len", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die nichts geh\u00f6ret, nichts betrachtet,", "tokens": ["Die", "nichts", "ge\u00b7h\u00f6\u00b7ret", ",", "nichts", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Auf alle Wunder nicht geachtet,", "tokens": ["Auf", "al\u00b7le", "Wun\u00b7der", "nicht", "ge\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Als eckelhaffte Monstra f\u00fcr,", "tokens": ["Als", "ec\u00b7kel\u00b7haff\u00b7te", "Mons\u00b7tra", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die gleichsam sonder Nas\u2019 und Ohren,", "tokens": ["Die", "gleich\u00b7sam", "son\u00b7der", "Nas'", "und", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ohn Zung\u2019 und Aug\u2019 und Hand gebohren,", "tokens": ["Ohn", "Zung'", "und", "Aug'", "und", "Hand", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ja, welche durch selbst eigne Schuld", "tokens": ["Ja", ",", "wel\u00b7che", "durch", "selbst", "eig\u00b7ne", "Schuld"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sich selbst gest\u00fcmmelt und der Gaben", "tokens": ["Sich", "selbst", "ge\u00b7st\u00fcm\u00b7melt", "und", "der", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "VVPP", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Vater-Liebe, Gnad\u2019 und Huld", "tokens": ["Der", "Va\u00b7ter\u00b7Lie\u00b7be", ",", "Gnad'", "und", "Huld"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Muthwillig sich beraubet haben.", "tokens": ["Mut\u00b7hwil\u00b7lig", "sich", "be\u00b7rau\u00b7bet", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob sie in diesem Stand\u2019 entbl\u00f6ss\u2019t von allen,", "tokens": ["Ob", "sie", "in", "die\u00b7sem", "Stand'", "ent\u00b7bl\u00f6ss't", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wird wol kaum glaublich seyn.", "tokens": ["Wird", "wol", "kaum", "glaub\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach lasst uns denn, vom Undancks-Laster rein,", "tokens": ["Ach", "lasst", "uns", "denn", ",", "vom", "Un\u00b7dancks\u00b7Las\u00b7ter", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wann wir des Sch\u00f6pfers Werck mit Lust und Danck", "tokens": ["Wann", "wir", "des", "Sch\u00f6p\u00b7fers", "Werck", "mit", "Lust", "und", "Danck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und man Sein\u2019 Allmacht, Huld und weise Lieb\u2019 em-", "tokens": ["Und", "man", "Sein'", "All\u00b7macht", ",", "Huld", "und", "wei\u00b7se", "Lieb'", "em"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PPOSAT", "NN", "$,", "NN", "KON", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch die empfundne Lust, im Danck, die Seelen schm\u00fc-", "tokens": ["Durch", "die", "emp\u00b7fund\u00b7ne", "Lust", ",", "im", "Danck", ",", "die", "See\u00b7len", "schm\u00fc"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Damit wir Dem, der an den Menschen-Kindern", "tokens": ["Da\u00b7mit", "wir", "Dem", ",", "der", "an", "den", "Men\u00b7schen\u00b7Kin\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein G\u00f6ttliches Vergn\u00fcgen findet,", "tokens": ["Sein", "G\u00f6tt\u00b7li\u00b7ches", "Ver\u00b7gn\u00fc\u00b7gen", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein G\u00f6ttliches Vergn\u00fcgn nicht vermindern.", "tokens": ["Sein", "G\u00f6tt\u00b7li\u00b7ches", "Ver\u00b7gn\u00fcgn", "nicht", "ver\u00b7min\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}