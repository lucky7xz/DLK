{"textgrid.poem.59328": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Klang gekommen", "tokens": ["Es", "ist", "ein", "Klang", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Her\u00fcber durch die Luft,", "tokens": ["Her\u00b7\u00fc\u00b7ber", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wind hat's gebracht und genommen,", "tokens": ["Der", "Wind", "hat's", "ge\u00b7bracht", "und", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich wei\u00df nicht, wer mich ruft.", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wer", "mich", "ruft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es schallt der Grund von Hufen,", "tokens": ["Es", "schallt", "der", "Grund", "von", "Hu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In der Ferne fiel ein Schu\u00df \u2013", "tokens": ["In", "der", "Fer\u00b7ne", "fiel", "ein", "Schu\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das sind die J\u00e4ger, die rufen,", "tokens": ["Das", "sind", "die", "J\u00e4\u00b7ger", ",", "die", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Da\u00df ich hinunter mu\u00df!", "tokens": ["Da\u00df", "ich", "hin\u00b7un\u00b7ter", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}