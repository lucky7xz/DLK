{"textgrid.poem.65087": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir sind getrennt, und tr\u00fcb und tr\u00fcber", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sind getrennt, und tr\u00fcb und tr\u00fcber", "tokens": ["Wir", "sind", "ge\u00b7trennt", ",", "und", "tr\u00fcb", "und", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verglimmt das rasch entflammte Licht,", "tokens": ["Ver\u00b7glimmt", "das", "rasch", "ent\u00b7flamm\u00b7te", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schwebt kein Blatt zu mir her\u00fcber,", "tokens": ["Es", "schwebt", "kein", "Blatt", "zu", "mir", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mir von liebvollem Erinnern,", "tokens": ["Das", "mir", "von", "lieb\u00b7vol\u00b7lem", "E\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Von Sehnsucht und Verlangen spricht,", "tokens": ["Von", "Sehn\u00b7sucht", "und", "Ver\u00b7lan\u00b7gen", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch f\u00fchl' ich es im tiefsten Innern:", "tokens": ["Doch", "f\u00fchl'", "ich", "es", "im", "tiefs\u00b7ten", "In\u00b7nern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von dir vergessen bin ich nicht!", "tokens": ["Von", "dir", "ver\u00b7ges\u00b7sen", "bin", "ich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn eine Stunde schlug uns Beiden,", "tokens": ["Denn", "ei\u00b7ne", "Stun\u00b7de", "schlug", "uns", "Bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit des Schmerzens Priesterhand,", "tokens": ["Die", "mit", "des", "Schmer\u00b7zens", "Pries\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu gleichen Wonnen, gleichen Leiden,", "tokens": ["Zu", "glei\u00b7chen", "Won\u00b7nen", ",", "glei\u00b7chen", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu gleichem Fluch und gleichem Segen", "tokens": ["Zu", "glei\u00b7chem", "Fluch", "und", "glei\u00b7chem", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Uns unaufl\u00f6slich fest verband!", "tokens": ["Uns", "un\u00b7auf\u00b7l\u00f6s\u00b7lich", "fest", "ver\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir wallen auf getrennten Wegen", "tokens": ["Wir", "wal\u00b7len", "auf", "ge\u00b7trenn\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch in dasselbe Heimathsland.", "tokens": ["Doch", "in", "das\u00b7sel\u00b7be", "Hei\u00b7maths\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Lass' mahnen dich an jene Stunde,", "tokens": ["Lass'", "mah\u00b7nen", "dich", "an", "je\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Herz an Herz mit bangem Schlag", "tokens": ["Wo", "Herz", "an", "Herz", "mit", "ban\u00b7gem", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich zugeraunt die Trauerkunde,", "tokens": ["Sich", "zu\u00b7ge\u00b7raunt", "die", "Trau\u00b7er\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die\u00df sei der letzte gold'ne Schimmer", "tokens": ["Die\u00df", "sei", "der", "letz\u00b7te", "gold'\u00b7ne", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von einem sonnenhellen Tag,", "tokens": ["Von", "ei\u00b7nem", "son\u00b7nen\u00b7hel\u00b7len", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dann vergi\u00df mich auf allimmer", "tokens": ["Und", "dann", "ver\u00b7gi\u00df", "mich", "auf", "al\u00b7lim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVIMP", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn deine Seele es vermag.", "tokens": ["Wenn", "dei\u00b7ne", "See\u00b7le", "es", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}