{"textgrid.poem.41773": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ahnet niemand meines Herzens Regen,", "tokens": ["Es", "ah\u00b7net", "nie\u00b7mand", "mei\u00b7nes", "Her\u00b7zens", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das dunkle Meer von meiner Seele Tiefen,", "tokens": ["Das", "dunk\u00b7le", "Meer", "von", "mei\u00b7ner", "See\u00b7le", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie seine Wogen endlos sich bewegen", "tokens": ["Wie", "sei\u00b7ne", "Wo\u00b7gen", "end\u00b7los", "sich", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wilde St\u00fcrme aus dem Grunde riefen!", "tokens": ["Und", "wil\u00b7de", "St\u00fcr\u00b7me", "aus", "dem", "Grun\u00b7de", "rie\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ahnt es niemand \u2013 auch das Meer verhehlet,", "tokens": ["Es", "ahnt", "es", "nie\u00b7mand", "\u2013", "auch", "das", "Meer", "ver\u00b7heh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$(", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was es verbirgt in seinem tiefsten Grunde,", "tokens": ["Was", "es", "ver\u00b7birgt", "in", "sei\u00b7nem", "tiefs\u00b7ten", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zuweilen nur ist's, da\u00df ein Sturm erz\u00e4hlet", "tokens": ["Zu\u00b7wei\u00b7len", "nur", "ist's", ",", "da\u00df", "ein", "Sturm", "er\u00b7z\u00e4h\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von seinem Innern mit beredtem Munde.", "tokens": ["Von", "sei\u00b7nem", "In\u00b7nern", "mit", "be\u00b7red\u00b7tem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So ist wohl mir auch manch ein Lied entquollen,", "tokens": ["So", "ist", "wohl", "mir", "auch", "manch", "ein", "Lied", "ent\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADV", "PIAT", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Kampf des eignen Herzens zu begleiten,", "tokens": ["Den", "Kampf", "des", "eig\u00b7nen", "Her\u00b7zens", "zu", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch sah ich's gern vergessen und verschollen", "tokens": ["Doch", "sah", "ich's", "gern", "ver\u00b7ges\u00b7sen", "und", "ver\u00b7schol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "ADV", "VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor den Trommetenschmettern dieser Zeiten.", "tokens": ["Vor", "den", "Trom\u00b7me\u00b7ten\u00b7schmet\u00b7tern", "die\u00b7ser", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich stie\u00df ja selber in die L\u00e4rmtrommete,", "tokens": ["Ich", "stie\u00df", "ja", "sel\u00b7ber", "in", "die", "L\u00e4rm\u00b7trom\u00b7me\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die tr\u00e4gen V\u00f6lker aus dem Schlaf zu r\u00fctteln'", "tokens": ["Die", "tr\u00e4\u00b7gen", "V\u00f6l\u00b7ker", "aus", "dem", "Schlaf", "zu", "r\u00fct\u00b7teln'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich mahnte immerdar mit lauter Rede:", "tokens": ["Ich", "mahn\u00b7te", "im\u00b7mer\u00b7dar", "mit", "lau\u00b7ter", "Re\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbder Knechtschaft Ketten m\u00fc\u00dft ihr von Euch sch\u00fctteln!\u00ab", "tokens": ["\u00bb", "der", "Knecht\u00b7schaft", "Ket\u00b7ten", "m\u00fc\u00dft", "ihr", "von", "Euch", "sch\u00fct\u00b7teln", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "NN", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich warf mich in das regste Weltgetriebe", "tokens": ["Ich", "warf", "mich", "in", "das", "regs\u00b7te", "Welt\u00b7ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sprach von Freiheit, Recht, vom Vaterlande!", "tokens": ["Und", "sprach", "von", "Frei\u00b7heit", ",", "Recht", ",", "vom", "Va\u00b7ter\u00b7lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch schwieg ich immer von der Glut der Liebe,", "tokens": ["Doch", "schwieg", "ich", "im\u00b7mer", "von", "der", "Glut", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mir im Innern unverl\u00f6schbar brannte. \u2013", "tokens": ["Die", "mir", "im", "In\u00b7nern", "un\u00b7ver\u00b7l\u00f6schbar", "brann\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Wohl wei\u00df ich wie die flache Welt entscheidet \u2013", "tokens": ["Wohl", "wei\u00df", "ich", "wie", "die", "fla\u00b7che", "Welt", "ent\u00b7schei\u00b7det", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was man nicht laut verk\u00fcndet auf den Gassen,", "tokens": ["Was", "man", "nicht", "laut", "ver\u00b7k\u00fcn\u00b7det", "auf", "den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKNEG", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was nicht in Worten ihr vor\u00fcber gleitet,", "tokens": ["Was", "nicht", "in", "Wor\u00b7ten", "ihr", "vor\u00b7\u00fc\u00b7ber", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das kann sie nicht verstehen und nicht fassen.", "tokens": ["Das", "kann", "sie", "nicht", "ver\u00b7ste\u00b7hen", "und", "nicht", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Drum ahnet niemand meines Innern Regen,", "tokens": ["Drum", "ah\u00b7net", "nie\u00b7mand", "mei\u00b7nes", "In\u00b7nern", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drum hat kein Herz das meine ganz verstanden!", "tokens": ["Drum", "hat", "kein", "Herz", "das", "mei\u00b7ne", "ganz", "ver\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIAT", "NN", "ART", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo laut es pocht im st\u00fcrmischen Bewegen,", "tokens": ["Wo", "laut", "es", "pocht", "im", "st\u00fcr\u00b7mi\u00b7schen", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verstummt der Mund, und liegt das Wort in Banden.", "tokens": ["Ver\u00b7stummt", "der", "Mund", ",", "und", "liegt", "das", "Wort", "in", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "So mu\u00df ich unerkannt durchs Leben gehen,", "tokens": ["So", "mu\u00df", "ich", "un\u00b7er\u00b7kannt", "durchs", "Le\u00b7ben", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Strome gleich, der sich durch Felsen windet;", "tokens": ["Dem", "Stro\u00b7me", "gleich", ",", "der", "sich", "durch", "Fel\u00b7sen", "win\u00b7det", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die N\u00e4chsten mir seh' ich am Ufer stehen,", "tokens": ["Die", "N\u00e4chs\u00b7ten", "mir", "seh'", "ich", "am", "U\u00b7fer", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wo jede Tiefe ja zur Schwachheit schwindet.", "tokens": ["Wo", "je\u00b7de", "Tie\u00b7fe", "ja", "zur", "Schwach\u00b7heit", "schwin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Es ist mein Los! \u2013 ich kann um Lieb' nicht bitten,", "tokens": ["Es", "ist", "mein", "Los", "!", "\u2013", "ich", "kann", "um", "Lieb'", "nicht", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "$(", "PPER", "VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch lieben kann ich noch aus tiefstem Herzen.", "tokens": ["Doch", "lie\u00b7ben", "kann", "ich", "noch", "aus", "tiefs\u00b7tem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um laut zu k\u00fcnden, was ich still gelitten,", "tokens": ["Um", "laut", "zu", "k\u00fcn\u00b7den", ",", "was", "ich", "still", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF", "$,", "PWS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu heilig sind mir meiner Liebe Schmerzen!", "tokens": ["Zu", "hei\u00b7lig", "sind", "mir", "mei\u00b7ner", "Lie\u00b7be", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}