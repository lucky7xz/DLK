{"textgrid.poem.24808": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Gen\u00fcgen in der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst war in allen ihren R\u00e4umen", "tokens": ["Einst", "war", "in", "al\u00b7len", "ih\u00b7ren", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erde mir kaum weit genug;", "tokens": ["Die", "Er\u00b7de", "mir", "kaum", "weit", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Land, kein Meer, wohin in Tr\u00e4umen", "tokens": ["Kein", "Land", ",", "kein", "Meer", ",", "wo\u00b7hin", "in", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich nicht der Seele Fl\u00fcgel trug.", "tokens": ["Mich", "nicht", "der", "See\u00b7le", "Fl\u00fc\u00b7gel", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf H\u00f6hn, zuerst bestrahlt vom Morgen,", "tokens": ["Auf", "H\u00f6hn", ",", "zu\u00b7erst", "be\u00b7strahlt", "vom", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Tiefen, die kein Senkblei mi\u00dft,", "tokens": ["In", "Tie\u00b7fen", ",", "die", "kein", "Senk\u00b7blei", "mi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00e4hnt' ich den gro\u00dfen Schatz verborgen,", "tokens": ["W\u00e4hnt'", "ich", "den", "gro\u00b7\u00dfen", "Schatz", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der einzig wert des Suchens ist.", "tokens": ["Der", "ein\u00b7zig", "wert", "des", "Su\u00b7chens", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch jetzt: o mehr, als was ich ehe", "tokens": ["Doch", "jetzt", ":", "o", "mehr", ",", "als", "was", "ich", "e\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "FM", "ADV", "$,", "KOUS", "PRELS", "PPER", "KOUS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesucht am fernsten Meeressaum,", "tokens": ["Ge\u00b7sucht", "am", "ferns\u00b7ten", "Mee\u00b7res\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fand ich bei dir in trauter N\u00e4he;", "tokens": ["Fand", "ich", "bei", "dir", "in", "trau\u00b7ter", "N\u00e4\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch fass' ich alles, alles kaum.", "tokens": ["Noch", "fass'", "ich", "al\u00b7les", ",", "al\u00b7les", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und, ganz das Gl\u00fcck nun zu genie\u00dfen,", "tokens": ["Und", ",", "ganz", "das", "Gl\u00fcck", "nun", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das mir der sch\u00f6nste Tag geschenkt,", "tokens": ["Das", "mir", "der", "sch\u00f6ns\u00b7te", "Tag", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "M\u00f6cht' ich der Welt mich rings verschlie\u00dfen,", "tokens": ["M\u00f6cht'", "ich", "der", "Welt", "mich", "rings", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deinen Anblick nur versenkt.", "tokens": ["In", "dei\u00b7nen", "An\u00b7blick", "nur", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}