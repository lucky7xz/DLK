{"textgrid.poem.26141": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Endlos nur der Mensch verliebt sein kann", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachts schlug der Regen die Baumbl\u00fct' ein,", "tokens": ["Nachts", "schlug", "der", "Re\u00b7gen", "die", "Baum\u00b7bl\u00fct'", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Gr\u00fcn sehn jetzt die B\u00e4ume zum Tag hinein", "tokens": ["Gr\u00fcn", "sehn", "jetzt", "die", "B\u00e4u\u00b7me", "zum", "Tag", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "APZR"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und sind mit den groben Bl\u00e4ttern allein.", "tokens": ["Und", "sind", "mit", "den", "gro\u00b7ben", "Bl\u00e4t\u00b7tern", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die verliebte Bl\u00fcte floh geschwind;", "tokens": ["Die", "ver\u00b7lieb\u00b7te", "Bl\u00fc\u00b7te", "floh", "ge\u00b7schwind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Noch alle Wege wei\u00df heut sind,", "tokens": ["Noch", "al\u00b7le", "We\u00b7ge", "wei\u00df", "heut", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und morgen fegt sie der Wind.", "tokens": ["Und", "mor\u00b7gen", "fegt", "sie", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Dann geht bei den B\u00e4umen die Arbeit an,", "tokens": ["Dann", "geht", "bei", "den", "B\u00e4u\u00b7men", "die", "Ar\u00b7beit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie ziehn \u00c4pfel und Kirschen wie Kinder heran.", "tokens": ["Sie", "ziehn", "\u00c4p\u00b7fel", "und", "Kir\u00b7schen", "wie", "Kin\u00b7der", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Endlos nur der Mensch verliebt sein kann.", "tokens": ["End\u00b7los", "nur", "der", "Mensch", "ver\u00b7liebt", "sein", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}