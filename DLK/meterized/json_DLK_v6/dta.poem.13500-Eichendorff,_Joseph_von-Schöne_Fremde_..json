{"dta.poem.13500": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6ne Fremde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es rauschen die Wipfel und schauern,", "tokens": ["Es", "rau\u00b7schen", "die", "Wip\u00b7fel", "und", "schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Als machten zu dieser Stund'", "tokens": ["Als", "mach\u00b7ten", "zu", "die\u00b7ser", "Stund'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Um die halbversunkenen Mauern", "tokens": ["Um", "die", "halb\u00b7ver\u00b7sun\u00b7ke\u00b7nen", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die alten G\u00f6tter die Rund'.", "tokens": ["Die", "al\u00b7ten", "G\u00f6t\u00b7ter", "die", "Rund'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Hier hinter den Myrthenb\u00e4umen", "tokens": ["Hier", "hin\u00b7ter", "den", "Myr\u00b7then\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In heimlich d\u00e4mmernder Pracht,", "tokens": ["In", "heim\u00b7lich", "d\u00e4m\u00b7mern\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Was sprichst du wirr wie in Tr\u00e4umen", "tokens": ["Was", "sprichst", "du", "wirr", "wie", "in", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "KOKOM", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu mir, phantastische Nacht?", "tokens": ["Zu", "mir", ",", "phan\u00b7tas\u00b7ti\u00b7sche", "Nacht", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Es funkeln auf mich alle Sterne", "tokens": ["Es", "fun\u00b7keln", "auf", "mich", "al\u00b7le", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit gl\u00fchendem Liebesblick,", "tokens": ["Mit", "gl\u00fc\u00b7hen\u00b7dem", "Lie\u00b7bes\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es redet trunken die Ferne", "tokens": ["Es", "re\u00b7det", "trun\u00b7ken", "die", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie von k\u00fcnftigem gro\u00dfen Gl\u00fcck! \u2014", "tokens": ["Wie", "von", "k\u00fcnf\u00b7ti\u00b7gem", "gro\u00b7\u00dfen", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "APPR", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}