{"textgrid.poem.42112": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bin ich nun verf\u00fchrt von jedem Schatten,", "tokens": ["So", "bin", "ich", "nun", "ver\u00b7f\u00fchrt", "von", "je\u00b7dem", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der niederglitt aus eines Vogels Flug,", "tokens": ["Der", "nie\u00b7der\u00b7glitt", "aus", "ei\u00b7nes", "Vo\u00b7gels", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als tr\u00e4fe mich mit t\u00f6dlichem Ermatten", "tokens": ["Als", "tr\u00e4\u00b7fe", "mich", "mit", "t\u00f6d\u00b7li\u00b7chem", "Er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Wimpernaufschlag. In dem wilden Zug", "tokens": ["Dein", "Wim\u00b7per\u00b7nauf\u00b7schlag", ".", "In", "dem", "wil\u00b7den", "Zug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Windes r\u00fchrt mich deine harte Hand", "tokens": ["Des", "Win\u00b7des", "r\u00fchrt", "mich", "dei\u00b7ne", "har\u00b7te", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sehns\u00fcchtig an, im Schilfe rauscht dein Haar,", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "an", ",", "im", "Schil\u00b7fe", "rauscht", "dein", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Stundenschlag, wie er verzitternd schwand,", "tokens": ["Dem", "Stun\u00b7den\u00b7schlag", ",", "wie", "er", "ver\u00b7zit\u00b7ternd", "schwand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Glich oft dein L\u00e4cheln, fern und unhaltbar.", "tokens": ["Glich", "oft", "dein", "L\u00e4\u00b7cheln", ",", "fern", "und", "un\u00b7halt\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch, wenn du weintest, w\u00e4r im Niederregnen", "tokens": ["Auch", ",", "wenn", "du", "wein\u00b7test", ",", "w\u00e4r", "im", "Nie\u00b7der\u00b7reg\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch deine Tr\u00e4ne \u2013 ach ich m\u00fc\u00dfte dir", "tokens": ["Noch", "dei\u00b7ne", "Tr\u00e4\u00b7ne", "\u2013", "ach", "ich", "m\u00fc\u00df\u00b7te", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$(", "XY", "PPER", "VMFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Selbst wo du niemals gingest, noch begegnen", "tokens": ["Selbst", "wo", "du", "nie\u00b7mals", "gin\u00b7gest", ",", "noch", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}