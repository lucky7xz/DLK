{"textgrid.poem.46509": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[hab' ich jetzt erst eingesehn]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab' ich jetzt erst eingesehn,", "tokens": ["Hab'", "ich", "jetzt", "erst", "ein\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mir Sch\u00f6nstes lebte,", "tokens": ["Was", "mir", "Sch\u00f6ns\u00b7tes", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Seit es mir gestorben ist?", "tokens": ["Seit", "es", "mir", "ge\u00b7stor\u00b7ben", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein, ich wu\u00dft' es lange.", "tokens": ["Nein", ",", "ich", "wu\u00dft'", "es", "lan\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wollt' es nur nicht eingestehn,", "tokens": ["Wollt'", "es", "nur", "nicht", "ein\u00b7ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil vor dir ich bebte,", "tokens": ["Weil", "vor", "dir", "ich", "beb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schicksal, das du neidisch bist", "tokens": ["Schick\u00b7sal", ",", "das", "du", "nei\u00b7disch", "bist"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Allem Ueberschwange.", "tokens": ["Al\u00b7lem", "Ue\u00b7ber\u00b7schwan\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nun das Ungl\u00fcck ist geschehn,", "tokens": ["Nun", "das", "Un\u00b7gl\u00fcck", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Zierd' entschwebte,", "tokens": ["Und", "die", "Zierd'", "ent\u00b7schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht mehr deine Hinterlist", "tokens": ["Nicht", "mehr", "dei\u00b7ne", "Hin\u00b7ter\u00b7list"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrcht' ich nun, o Schlange.", "tokens": ["F\u00fcrcht'", "ich", "nun", ",", "o", "Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und im Liede soll es stehn,", "tokens": ["Und", "im", "Lie\u00b7de", "soll", "es", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ein Sch\u00f6nstes lebte,", "tokens": ["Da\u00df", "ein", "Sch\u00f6ns\u00b7tes", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und mir leben jeder Frist", "tokens": ["Und", "mir", "le\u00b7ben", "je\u00b7der", "Frist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll es im Gesange.", "tokens": ["Soll", "es", "im", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}