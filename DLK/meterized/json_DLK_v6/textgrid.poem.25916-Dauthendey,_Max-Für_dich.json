{"textgrid.poem.25916": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr dich", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6cht' mich als Staub vor die F\u00fc\u00dfe dir legen,", "tokens": ["M\u00f6cht'", "mich", "als", "Staub", "vor", "die", "F\u00fc\u00b7\u00dfe", "dir", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Will dich bewegen wie die Winde das Laub,", "tokens": ["Will", "dich", "be\u00b7we\u00b7gen", "wie", "die", "Win\u00b7de", "das", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Wollt' K\u00fcsse dir geben, soviel Tropfen im Regen,", "tokens": ["Wollt'", "K\u00fcs\u00b7se", "dir", "ge\u00b7ben", ",", "so\u00b7viel", "Trop\u00b7fen", "im", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVINF", "$,", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--++--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Liebe ist blind, doch du Geliebte bist taub.", "tokens": ["Lie\u00b7be", "ist", "blind", ",", "doch", "du", "Ge\u00b7lieb\u00b7te", "bist", "taub", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,", "KON", "PPER", "NN", "VAFIN", "ADJD", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "H\u00e4tte ich H\u00e4nde, soviel Bl\u00e4tter die B\u00e4ume,", "tokens": ["H\u00e4t\u00b7te", "ich", "H\u00e4n\u00b7de", ",", "so\u00b7viel", "Bl\u00e4t\u00b7ter", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sie alle sollten f\u00fcr dich nur sich regen.", "tokens": ["Sie", "al\u00b7le", "soll\u00b7ten", "f\u00fcr", "dich", "nur", "sich", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "APPR", "PPER", "ADV", "PRF", "ADJA", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "F\u00fcr dich sterb ich st\u00fcndlich im Lied meiner Tr\u00e4ume", "tokens": ["F\u00fcr", "dich", "sterb", "ich", "st\u00fcnd\u00b7lich", "im", "Lied", "mei\u00b7ner", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und kann mich selbst nur im Traum noch bewegen.", "tokens": ["Und", "kann", "mich", "selbst", "nur", "im", "Traum", "noch", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}