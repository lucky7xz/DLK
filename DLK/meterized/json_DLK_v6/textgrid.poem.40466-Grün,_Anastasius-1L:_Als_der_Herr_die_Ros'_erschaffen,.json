{"textgrid.poem.40466": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als der Herr die Ros' erschaffen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als der Herr die Ros' erschaffen,", "tokens": ["Als", "der", "Herr", "die", "Ros'", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach er: du sollst bl\u00fchn und duften!", "tokens": ["Sprach", "er", ":", "du", "sollst", "bl\u00fchn", "und", "duf\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VMFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als er hie\u00df die Sonne werden,", "tokens": ["Als", "er", "hie\u00df", "die", "Son\u00b7ne", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprach er: du sollst gl\u00fchn und w\u00e4rmen!", "tokens": ["Sprach", "er", ":", "du", "sollst", "gl\u00fchn", "und", "w\u00e4r\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VMFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als der Herr die Lerch' erschaffen,", "tokens": ["Als", "der", "Herr", "die", "Ler\u00b7ch'", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sprach er: flieg' empor und singe!", "tokens": ["Sprach", "er", ":", "flieg'", "em\u00b7por", "und", "sin\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als geformt des Mondes Scheibe,", "tokens": ["Als", "ge\u00b7formt", "des", "Mon\u00b7des", "Schei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprach er: rolle hin und leuchte!", "tokens": ["Sprach", "er", ":", "rol\u00b7le", "hin", "und", "leuch\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als der Herr das Weib erschaffen,", "tokens": ["Als", "der", "Herr", "das", "Weib", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach er: sei geliebt und liebe!", "tokens": ["Sprach", "er", ":", "sei", "ge\u00b7liebt", "und", "lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VAFIN", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber als er dich erschaffen,", "tokens": ["A\u00b7ber", "als", "er", "dich", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat er wohl die\u00df Wort vergessen.", "tokens": ["Hat", "er", "wohl", "die\u00df", "Wort", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PDS", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn wie k\u00f6nntest du sonst sehen", "tokens": ["Denn", "wie", "k\u00f6nn\u00b7test", "du", "sonst", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Mond und Sonne gl\u00fchn und leuchten,", "tokens": ["Mond", "und", "Son\u00b7ne", "gl\u00fchn", "und", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosen bl\u00fchen, Lerchen steigen,", "tokens": ["Ro\u00b7sen", "bl\u00fc\u00b7hen", ",", "Ler\u00b7chen", "stei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und geliebt sein und \u2013 nicht lieben?", "tokens": ["Und", "ge\u00b7liebt", "sein", "und", "\u2013", "nicht", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAINF", "KON", "$(", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}