{"textgrid.poem.37520": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Folgen der Kraft", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit k\u00fchnem Mut aus seinem Bett", "tokens": ["Mit", "k\u00fch\u00b7nem", "Mut", "aus", "sei\u00b7nem", "Bett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwingt sich der Turner Hoppenstedt.", "tokens": ["Schwingt", "sich", "der", "Tur\u00b7ner", "Hop\u00b7pen\u00b7stedt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schon ist das Hantelpaar bereit", "tokens": ["Schon", "ist", "das", "Han\u00b7tel\u00b7paar", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu frisch-fromm-freier T\u00e4tigkeit.", "tokens": ["Zu", "frischfrom\u00b7mfrei\u00b7er", "T\u00e4\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Bizeps wird zuerst ge\u00fcbt,", "tokens": ["Der", "Bi\u00b7zeps", "wird", "zu\u00b7erst", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er, der dem Arm die Spannkraft gibt.", "tokens": ["Er", ",", "der", "dem", "Arm", "die", "Spann\u00b7kraft", "gibt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Einseitig aber ist der Mann,", "tokens": ["Ein\u00b7sei\u00b7tig", "a\u00b7ber", "ist", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der's nicht mit beiden H\u00e4nden kann.", "tokens": ["Der's", "nicht", "mit", "bei\u00b7den", "H\u00e4n\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "APPR", "PIAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Stramm sei der Nacken, da\u00df man trage", "tokens": ["Stramm", "sei", "der", "Na\u00b7cken", ",", "da\u00df", "man", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Vollgewicht in k\u00fchner Wage.", "tokens": ["Das", "Voll\u00b7ge\u00b7wicht", "in", "k\u00fch\u00b7ner", "Wa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Besonders auch vers\u00e4umt er nie", "tokens": ["Be\u00b7son\u00b7ders", "auch", "ver\u00b7s\u00e4umt", "er", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Beines Muskelenergie.", "tokens": ["Des", "Bei\u00b7nes", "Mus\u00b7ke\u00b7le\u00b7ner\u00b7gie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Derweil sitzt unten beim Kaffee", "tokens": ["Der\u00b7weil", "sitzt", "un\u00b7ten", "beim", "Kaf\u00b7fee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Herr Meck und deutet in die H\u00f6h'.", "tokens": ["Herr", "Meck", "und", "deu\u00b7tet", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es w\u00e4chst die Kraft. \u2013 Doch unten hier", "tokens": ["Es", "w\u00e4chst", "die", "Kraft", ".", "\u2013", "Doch", "un\u00b7ten", "hier"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$(", "KON", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liest Vater Meck in dem Kurier.", "tokens": ["Liest", "Va\u00b7ter", "Meck", "in", "dem", "Ku\u00b7rier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kracks! \u2013 zu gro\u00df wird das Gewicht;", "tokens": ["Und", "kracks", "!", "\u2013", "zu", "gro\u00df", "wird", "das", "Ge\u00b7wicht", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "$(", "PTKA", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Decke tr\u00e4gt es nicht \u2013 und \u2013 bricht.", "tokens": ["Die", "De\u00b7cke", "tr\u00e4gt", "es", "nicht", "\u2013", "und", "\u2013", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "KON", "$(", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und Hoppenstedt, wie er sich stemme,", "tokens": ["Und", "Hop\u00b7pen\u00b7stedt", ",", "wie", "er", "sich", "stem\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Saust schon in Topf und Butterbemme.", "tokens": ["Saust", "schon", "in", "Topf", "und", "But\u00b7ter\u00b7bem\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Man l\u00e4uft, man f\u00e4llt nach allen Seiten,", "tokens": ["Man", "l\u00e4uft", ",", "man", "f\u00e4llt", "nach", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Hoppenstedt f\u00e4ngt an zu reiten.", "tokens": ["Und", "Hop\u00b7pen\u00b7stedt", "f\u00e4ngt", "an", "zu", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PTKVZ", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er eilt hinaus mit schnellem Schritt,", "tokens": ["Er", "eilt", "hin\u00b7aus", "mit", "schnel\u00b7lem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APZR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Topf und Butter eilen mit.", "tokens": ["Und", "Topf", "und", "But\u00b7ter", "ei\u00b7len", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Am schlimmsten aber \u2013 oh! oh! oh! \u2013", "tokens": ["Am", "schlimms\u00b7ten", "a\u00b7ber", "\u2013", "oh", "!", "oh", "!", "oh", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "ADV", "$(", "FM", "$.", "FM", "$.", "FM", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erging es dem guten Fidelio.", "tokens": ["Er\u00b7ging", "es", "dem", "gu\u00b7ten", "Fi\u00b7de\u00b7lio", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}