{"textgrid.poem.24063": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Der schwarze Ritter", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Thale unten die blaue Tiefe,", "tokens": ["Im", "Tha\u00b7le", "un\u00b7ten", "die", "blau\u00b7e", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Grau am Himmel jagende Wolken;", "tokens": ["Grau", "am", "Him\u00b7mel", "ja\u00b7gen\u00b7de", "Wol\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Langsam reitet,", "tokens": ["Lang\u00b7sam", "rei\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Die Lanze im Arm,", "tokens": ["Die", "Lan\u00b7ze", "im", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Auf braunem Rosse ein schwarzer Ritter;", "tokens": ["Auf", "brau\u00b7nem", "Ros\u00b7se", "ein", "schwar\u00b7zer", "Rit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Rote Ebereschentrauben", "tokens": ["Ro\u00b7te", "E\u00b7be\u00b7re\u00b7schen\u00b7trau\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Leuchten aus dunklem Gr\u00fcn heraus", "tokens": ["Leuch\u00b7ten", "aus", "dunk\u00b7lem", "Gr\u00fcn", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Wie offene Wunden ...", "tokens": ["Wie", "of\u00b7fe\u00b7ne", "Wun\u00b7den", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}