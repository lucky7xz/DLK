{"dta.poem.8335": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der kaltsinnige Beter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen beten offt mit einem\nkalten Herzen,", "tokens": ["Die", "Men\u00b7schen", "be\u00b7ten", "offt", "mit", "ei\u00b7nem", "kal\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und diese gleichen mir den s\u00fcssen", "tokens": ["Und", "die\u00b7se", "glei\u00b7chen", "mir", "den", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie nicht in dem Brand, wie", "tokens": ["Wenn", "sie", "nicht", "in", "dem", "Brand", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,", "PWAV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "So kan man den Geruch davon", "tokens": ["So", "kan", "man", "den", "Ge\u00b7ruch", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer im Gebete ", "tokens": ["Wer", "im", "Ge\u00b7be\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PWS", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und keine Inbrunst hat, die in die H\u00f6he dringet:", "tokens": ["Und", "kei\u00b7ne", "In\u00b7brunst", "hat", ",", "die", "in", "die", "H\u00f6\u00b7he", "drin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So ists nicht angenehm, dem es gefallen soll,", "tokens": ["So", "ists", "nicht", "an\u00b7ge\u00b7nehm", ",", "dem", "es", "ge\u00b7fal\u00b7len", "soll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADJD", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Andacht ihr Gebet, gef\u00e4llt dem H\u00f6chsten", "tokens": ["Der", "An\u00b7dacht", "ihr", "Ge\u00b7bet", ",", "ge\u00b7f\u00e4llt", "dem", "H\u00f6chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn es dem Kerzen gleich, die von dem Feur ent-", "tokens": ["Wenn", "es", "dem", "Ker\u00b7zen", "gleich", ",", "die", "von", "dem", "Feur", "ent"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Doch wie man ohne Brand nicht dem Geruch em-", "tokens": ["Doch", "wie", "man", "oh\u00b7ne", "Brand", "nicht", "dem", "Ge\u00b7ruch", "em"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "APPR", "NN", "PTKNEG", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So ist ohn Andachtsfeur, auch keine Liebligkeit,", "tokens": ["So", "ist", "ohn", "An\u00b7dachts\u00b7feur", ",", "auch", "kei\u00b7ne", "Lieb\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Ach! m\u00f6chten dieses doch die Menschen stets be-", "tokens": ["Ach", "!", "m\u00f6ch\u00b7ten", "die\u00b7ses", "doch", "die", "Men\u00b7schen", "stets", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VMFIN", "PDS", "ADV", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "So w\u00fcrden sie ihr Herz nicht zu dem Sch\u00f6pfer len-", "tokens": ["So", "w\u00fcr\u00b7den", "sie", "ihr", "Herz", "nicht", "zu", "dem", "Sch\u00f6p\u00b7fer", "len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.15": {"text": "Wenn es so kalt wie Ei\u00df, wenn erst das Herz ent-", "tokens": ["Wenn", "es", "so", "kalt", "wie", "Ei\u00df", ",", "wenn", "erst", "das", "Herz", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KOKOM", "NN", "$,", "KOUS", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Durchs heilgen Geistes Trieb, und die gefaltn", "tokens": ["Durchs", "heil\u00b7gen", "Geis\u00b7tes", "Trieb", ",", "und", "die", "ge\u00b7faltn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Des Glaubens Christum fa\u00dft, so kan man ernstlich", "tokens": ["Des", "Glau\u00b7bens", "Chris\u00b7tum", "fa\u00dft", ",", "so", "kan", "man", "ernst\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "$,", "ADV", "VMFIN", "PIS", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und vor das Angesicht des heilgen Sch\u00f6pfers tre-", "tokens": ["Und", "vor", "das", "An\u00b7ge\u00b7sicht", "des", "heil\u00b7gen", "Sch\u00f6p\u00b7fers", "tre"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "O! ", "tokens": ["O", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Denn der Erl\u00f6ser uns durch sein Verdienst ver-", "tokens": ["Denn", "der", "Er\u00b7l\u00f6\u00b7ser", "uns", "durch", "sein", "Ver\u00b7dienst", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}