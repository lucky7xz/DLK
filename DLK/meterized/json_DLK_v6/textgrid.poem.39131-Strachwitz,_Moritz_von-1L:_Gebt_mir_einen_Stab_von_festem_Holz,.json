{"textgrid.poem.39131": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gebt mir einen Stab von festem Holz,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebt mir einen Stab von festem Holz,", "tokens": ["Gebt", "mir", "ei\u00b7nen", "Stab", "von", "fes\u00b7tem", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ich dran durch die L\u00e4nder schreite;", "tokens": ["Da\u00df", "ich", "dran", "durch", "die", "L\u00e4n\u00b7der", "schrei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gebt mir einen Segler mastenstolz,", "tokens": ["Gebt", "mir", "ei\u00b7nen", "Seg\u00b7ler", "mas\u00b7ten\u00b7stolz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df ich drauf durch die Wellen reite.", "tokens": ["Da\u00df", "ich", "drauf", "durch", "die", "Wel\u00b7len", "rei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gebt mir das Ro\u00df aus dem M\u00e4rchenland,", "tokens": ["Gebt", "mir", "das", "Ro\u00df", "aus", "dem", "M\u00e4r\u00b7chen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df ich drauf um die Erde jage;", "tokens": ["Da\u00df", "ich", "drauf", "um", "die", "Er\u00b7de", "ja\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gebt mir des Adlers Federgewand,", "tokens": ["Gebt", "mir", "des", "Ad\u00b7lers", "Fe\u00b7der\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da\u00df es mich in den Himmel trage.", "tokens": ["Da\u00df", "es", "mich", "in", "den", "Him\u00b7mel", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ob ich nun segle durchs Wellengebraus,", "tokens": ["Und", "ob", "ich", "nun", "seg\u00b7le", "durchs", "Wel\u00b7len\u00b7ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ob ich fliege, wandele, reite:", "tokens": ["Ob", "ich", "flie\u00b7ge", ",", "wan\u00b7de\u00b7le", ",", "rei\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Nur la\u00dft mich hinaus, nur la\u00dft mich hinaus", "tokens": ["Nur", "la\u00dft", "mich", "hin\u00b7aus", ",", "nur", "la\u00dft", "mich", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APZR", "$,", "ADV", "VVFIN", "PRF", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus dem Engen, hinaus in das Weite!", "tokens": ["Aus", "dem", "En\u00b7gen", ",", "hin\u00b7aus", "in", "das", "Wei\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Die Erde, sie ist so lang und breit,", "tokens": ["Die", "Er\u00b7de", ",", "sie", "ist", "so", "lang", "und", "breit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Das Meer ist noch viel breiter,", "tokens": ["Das", "Meer", "ist", "noch", "viel", "brei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel, er ist so hoch und weit", "tokens": ["Der", "Him\u00b7mel", ",", "er", "ist", "so", "hoch", "und", "weit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und r\u00fcckt mir t\u00e4glich weiter.", "tokens": ["Und", "r\u00fcckt", "mir", "t\u00e4g\u00b7lich", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was frommt mir die Erde, was frommt mir das Meer,", "tokens": ["Was", "frommt", "mir", "die", "Er\u00b7de", ",", "was", "frommt", "mir", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,", "PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn ich drinnen nicht darf mich ergehen;", "tokens": ["Wenn", "ich", "drin\u00b7nen", "nicht", "darf", "mich", "er\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Was frommt mir des Himmels Sternenheer,", "tokens": ["Was", "frommt", "mir", "des", "Him\u00b7mels", "Ster\u00b7nen\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn ich's darf nicht n\u00e4her besehen?", "tokens": ["Wenn", "ich's", "darf", "nicht", "n\u00e4\u00b7her", "be\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Drum ob ich nun segle durchs Wellengebraus,", "tokens": ["Drum", "ob", "ich", "nun", "seg\u00b7le", "durchs", "Wel\u00b7len\u00b7ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ob ich fliege, wandele, reite!", "tokens": ["Ob", "ich", "flie\u00b7ge", ",", "wan\u00b7de\u00b7le", ",", "rei\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Nur la\u00dft mich hinaus, nur la\u00dft mich hinaus", "tokens": ["Nur", "la\u00dft", "mich", "hin\u00b7aus", ",", "nur", "la\u00dft", "mich", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APZR", "$,", "ADV", "VVFIN", "PRF", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus dem Engen, hinaus in das Weite!", "tokens": ["Aus", "dem", "En\u00b7gen", ",", "hin\u00b7aus", "in", "das", "Wei\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}