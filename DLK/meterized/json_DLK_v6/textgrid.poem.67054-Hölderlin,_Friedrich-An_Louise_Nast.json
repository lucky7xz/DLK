{"textgrid.poem.67054": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Louise Nast", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df sie drohen, die St\u00fcrme, die Leiden", "tokens": ["La\u00df", "sie", "dro\u00b7hen", ",", "die", "St\u00fcr\u00b7me", ",", "die", "Lei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "La\u00df trennen \u2013 der Trennung Jahre,", "tokens": ["La\u00df", "tren\u00b7nen", "\u2013", "der", "Tren\u00b7nung", "Jah\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$(", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie trennen uns nicht!", "tokens": ["Sie", "tren\u00b7nen", "uns", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Sie trennen uns nicht!", "tokens": ["Sie", "tren\u00b7nen", "uns", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Denn mein bist du! Und \u00fcber das Grab hinaus", "tokens": ["Denn", "mein", "bist", "du", "!", "Und", "\u00fc\u00b7ber", "das", "Grab", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "VAFIN", "PPER", "$.", "KON", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Soll sie dauren, die unzertrennbare Liebe.", "tokens": ["Soll", "sie", "dau\u00b7ren", ",", "die", "un\u00b7zer\u00b7trenn\u00b7ba\u00b7re", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "O! wenns einst da ist,", "tokens": ["O", "!", "wenns", "einst", "da", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "ADV", "ADV", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Das gro\u00dfe selige Jenseits,", "tokens": ["Das", "gro\u00b7\u00dfe", "se\u00b7li\u00b7ge", "Jen\u00b7seits", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo die Krone dem leidenden Pilger,", "tokens": ["Wo", "die", "Kro\u00b7ne", "dem", "lei\u00b7den\u00b7den", "Pil\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Die Palme dem Sieger blinkt,", "tokens": ["Die", "Pal\u00b7me", "dem", "Sie\u00b7ger", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dann, Freundin \u2013 lohnet auch Freundschaft \u2013", "tokens": ["Dann", ",", "Freun\u00b7din", "\u2013", "loh\u00b7net", "auch", "Freund\u00b7schaft", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$(", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Auch Freundschaft \u2013 der Ewige.", "tokens": ["Auch", "Freund\u00b7schaft", "\u2013", "der", "E\u00b7wi\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}