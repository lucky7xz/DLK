{"dta.poem.11101": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihn nicht wieder liebte.  \n  Tal.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bin ich zwar entz\u00fcndet;", "tokens": ["So", "bin", "ich", "zwar", "ent\u00b7z\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jedoch weil meine gluth", "tokens": ["Je\u00b7doch", "weil", "mei\u00b7ne", "gluth"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die k\u00fchlung nicht nach ihrem wunsche findet,", "tokens": ["Die", "k\u00fch\u00b7lung", "nicht", "nach", "ih\u00b7rem", "wun\u00b7sche", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So kr\u00e4ncket sich ja billig geist und muth,", "tokens": ["So", "kr\u00e4n\u00b7cket", "sich", "ja", "bil\u00b7lig", "geist", "und", "muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADJD", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bi\u00df da\u00df es wird verzehrt durch solche wuth.", "tokens": ["Bi\u00df", "da\u00df", "es", "wird", "ver\u00b7zehrt", "durch", "sol\u00b7che", "wuth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VAFIN", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was hab ich doch verbrochen,", "tokens": ["Was", "hab", "ich", "doch", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich verbrannt soll seyn?", "tokens": ["Da\u00df", "ich", "ver\u00b7brannt", "soll", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den zaubrern wird di\u00df urthel sonst gesprochen;", "tokens": ["Den", "zau\u00b7brern", "wird", "di\u00df", "ur\u00b7thel", "sonst", "ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unschuld f\u00e4llt hier in die gluth hinein,", "tokens": ["Die", "un\u00b7schuld", "f\u00e4llt", "hier", "in", "die", "gluth", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wird zur asch durch zweyer augen schein.", "tokens": ["Und", "wird", "zur", "asch", "durch", "zwey\u00b7er", "au\u00b7gen", "schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun wohl, ich will verbrennen,", "tokens": ["Nun", "wohl", ",", "ich", "will", "ver\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil dir es so gef\u00e4llt.", "tokens": ["Weil", "dir", "es", "so", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man wird die treu noch aus der asche kennen,", "tokens": ["Man", "wird", "die", "treu", "noch", "aus", "der", "asc\u00b7he", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJD", "ADV", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mein geschick zu deinen f\u00fcssen stellt,", "tokens": ["Die", "mein", "ge\u00b7schick", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Weil\u2019s meinen staub auch vor dein eigen h\u00e4lt.", "tokens": ["Weil's", "mei\u00b7nen", "staub", "auch", "vor", "dein", "ei\u00b7gen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}