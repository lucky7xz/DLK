{"textgrid.poem.57020": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die zwei Wurzeln", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Tannenwurzeln gro\u00df und alt", "tokens": ["Zwei", "Tan\u00b7nen\u00b7wur\u00b7zeln", "gro\u00df", "und", "alt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "unterhalten sich im Wald.", "tokens": ["un\u00b7ter\u00b7hal\u00b7ten", "sich", "im", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was droben in den Wipfeln rauscht,", "tokens": ["Was", "dro\u00b7ben", "in", "den", "Wip\u00b7feln", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das wird hier unten ausgetauscht.", "tokens": ["das", "wird", "hier", "un\u00b7ten", "aus\u00b7ge\u00b7tauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein altes Eichhorn sitzt dabei", "tokens": ["Ein", "al\u00b7tes", "Eich\u00b7horn", "sitzt", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und strickt wohl Str\u00fcmpfe f\u00fcr die zwei.", "tokens": ["und", "strickt", "wohl", "Str\u00fcmp\u00b7fe", "f\u00fcr", "die", "zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "APPR", "ART", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die eine sagt: knig. Die andre sagt: knag.", "tokens": ["Die", "ei\u00b7ne", "sagt", ":", "knig", ".", "Die", "and\u00b7re", "sagt", ":", "knag", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "ADJD", "$.", "ART", "PIS", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das ist genug f\u00fcr einen Tag.", "tokens": ["Das", "ist", "ge\u00b7nug", "f\u00fcr", "ei\u00b7nen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}