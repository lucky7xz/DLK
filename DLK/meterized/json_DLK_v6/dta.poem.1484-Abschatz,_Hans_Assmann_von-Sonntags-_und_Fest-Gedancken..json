{"dta.poem.1484": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonntags- und Fest-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wir Menschen sind wohl schwach an GOttes Wort zu", "tokens": ["Wir", "Men\u00b7schen", "sind", "wohl", "schwach", "an", "Got\u00b7tes", "Wort", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VAFIN", "ADV", "ADJD", "APPR", "NN", "NN", "PTKZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dencken/", "tokens": ["den\u00b7cken", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Doch will er seinen Geist uns zu erinnern schencken:", "tokens": ["Doch", "will", "er", "sei\u00b7nen", "Geist", "uns", "zu", "e\u00b7rin\u00b7nern", "schen\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Komm angenehmer Gast und bla\u00df mir Tr\u00f6stung ein/", "tokens": ["Komm", "an\u00b7ge\u00b7neh\u00b7mer", "Gast", "und", "bla\u00df", "mir", "Tr\u00f6s\u00b7tung", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "KON", "ADJD", "PPER", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn Auge/ Zung\u2019 und Ohr in lezter Ohnmacht seyn.", "tokens": ["Wenn", "Au\u00b7ge", "/", "Zung'", "und", "Ohr", "in", "lez\u00b7ter", "Ohn\u00