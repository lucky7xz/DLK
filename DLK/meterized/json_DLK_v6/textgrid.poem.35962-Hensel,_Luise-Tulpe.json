{"textgrid.poem.35962": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Tulpe", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir wohl ist der Sinn bewu\u00dft,", "tokens": ["Dir", "wohl", "ist", "der", "Sinn", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in meinen Farben bl\u00fcht,", "tokens": ["Der", "in", "mei\u00b7nen", "Far\u00b7ben", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4gst du mit dir in der Brust", "tokens": ["Tr\u00e4gst", "du", "mit", "dir", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Flamme, die dein Herz durchgl\u00fcht.", "tokens": ["Flam\u00b7me", ",", "die", "dein", "Herz", "durch\u00b7gl\u00fcht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Trag in Flammen eingeh\u00fcllt,", "tokens": ["Trag", "in", "Flam\u00b7men", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was nicht mit dem Lenz vergeht;", "tokens": ["Was", "nicht", "mit", "dem", "Lenz", "ver\u00b7geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein geheimni\u00dfvolles Bild,", "tokens": ["Ein", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7les", "Bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das wohl Mancher nicht versteht.", "tokens": ["Das", "wohl", "Man\u00b7cher", "nicht", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}