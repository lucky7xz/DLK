{"textgrid.poem.38259": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Armer Kinder Bettlerlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sungen drei Engel einen s\u00fc\u00dfen Gesang,", "tokens": ["Es", "sun\u00b7gen", "drei", "En\u00b7gel", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Freuden es im Himmel klang;", "tokens": ["Mit", "Freu\u00b7den", "es", "im", "Him\u00b7mel", "klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie jauchzten fr\u00f6hlich auch dabei,", "tokens": ["Sie", "jauchz\u00b7ten", "fr\u00f6h\u00b7lich", "auch", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Petrus sey von S\u00fcnden frey,", "tokens": ["Da\u00df", "Pet\u00b7rus", "sey", "von", "S\u00fcn\u00b7den", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM", "VAFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von S\u00fcnden frey.", "tokens": ["Von", "S\u00fcn\u00b7den", "frey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Denn als der Herr Jesus zu Tische sa\u00df,", "tokens": ["Denn", "als", "der", "Herr", "Je\u00b7sus", "zu", "Ti\u00b7sche", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit seinen zw\u00f6lf J\u00fcngern das Abendmahl a\u00df,", "tokens": ["Mit", "sei\u00b7nen", "zw\u00f6lf", "J\u00fcn\u00b7gern", "das", "A\u00b7bend\u00b7mahl", "a\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So sprach der Herr Jesus: Was stehest du hier,", "tokens": ["So", "sprach", "der", "Herr", "Je\u00b7sus", ":", "Was", "ste\u00b7hest", "du", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "$.", "PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wenn ich dich ansehe, so weinest du mir,", "tokens": ["Wenn", "ich", "dich", "an\u00b7se\u00b7he", ",", "so", "wei\u00b7nest", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-----+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "So weinest du mir.", "tokens": ["So", "wei\u00b7nest", "du", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ach! sollt ich nicht weinen du g\u00fctiger Gott!", "tokens": ["Ach", "!", "sollt", "ich", "nicht", "wei\u00b7nen", "du", "g\u00fc\u00b7ti\u00b7ger", "Gott", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PTKNEG", "VVINF", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich hab \u00fcbertreten die zehen Gebot;", "tokens": ["Ich", "hab", "\u00fc\u00b7bert\u00b7re\u00b7ten", "die", "ze\u00b7hen", "Ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich gehe und weine ja bitterlich,", "tokens": ["Ich", "ge\u00b7he", "und", "wei\u00b7ne", "ja", "bit\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ach komm, erbarme dich \u00fcber mich,", "tokens": ["Ach", "komm", ",", "er\u00b7bar\u00b7me", "dich", "\u00fc\u00b7ber", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ach \u00fcber mich!", "tokens": ["Ach", "\u00fc\u00b7ber", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Hast du dann \u00fcbertreten die zehen Gebot,", "tokens": ["Hast", "du", "dann", "\u00fc\u00b7bert\u00b7re\u00b7ten", "die", "ze\u00b7hen", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So fall auf die Knie und bete zu Gott,", "tokens": ["So", "fall", "auf", "die", "Knie", "und", "be\u00b7te", "zu", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und bete zu Gott nur allezeit,", "tokens": ["Und", "be\u00b7te", "zu", "Gott", "nur", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So wirst du erlangen die himmlische Freud,", "tokens": ["So", "wirst", "du", "er\u00b7lan\u00b7gen", "die", "himm\u00b7li\u00b7sche", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die himmlische Freud.", "tokens": ["Die", "himm\u00b7li\u00b7sche", "Freud", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Die himmlische Freud ist eine selige Stadt,", "tokens": ["Die", "himm\u00b7li\u00b7sche", "Freud", "ist", "ei\u00b7ne", "se\u00b7li\u00b7ge", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die himmlische Freud die kein End mehr hat;", "tokens": ["Die", "himm\u00b7li\u00b7sche", "Freud", "die", "kein", "End", "mehr", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "PIAT", "NN", "ADV", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die himmlische Freude war Petro bereit,", "tokens": ["Die", "himm\u00b7li\u00b7sche", "Freu\u00b7de", "war", "Pe\u00b7tro", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NE", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Durch Jesum und allen zur Seeligkeit,", "tokens": ["Durch", "Je\u00b7sum", "und", "al\u00b7len", "zur", "See\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "PIAT", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Zur Seeligkeit.", "tokens": ["Zur", "See\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}