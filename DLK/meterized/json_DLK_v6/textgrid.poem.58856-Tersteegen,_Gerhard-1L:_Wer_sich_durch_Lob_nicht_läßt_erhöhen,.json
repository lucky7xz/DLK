{"textgrid.poem.58856": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer sich durch Lob nicht l\u00e4\u00dft erh\u00f6hen,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich durch Lob nicht l\u00e4\u00dft erh\u00f6hen,", "tokens": ["Wer", "sich", "durch", "Lob", "nicht", "l\u00e4\u00dft", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "PTKNEG", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der bleibt auch ruhig beim Verschm\u00e4hen;", "tokens": ["Der", "bleibt", "auch", "ru\u00b7hig", "beim", "Ver\u00b7schm\u00e4\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sei du der Menschen Urteil tot", "tokens": ["Sei", "du", "der", "Men\u00b7schen", "Ur\u00b7teil", "tot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und liebe Gott, so hat's nicht Not!", "tokens": ["Und", "lie\u00b7be", "Gott", ",", "so", "hat's", "nicht", "Not", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "ADV", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}