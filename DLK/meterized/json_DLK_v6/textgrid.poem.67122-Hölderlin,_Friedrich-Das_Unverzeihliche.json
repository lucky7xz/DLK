{"textgrid.poem.67122": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Unverzeihliche", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr Freunde verge\u00dft, wenn ihr den K\u00fcnstler h\u00f6hnt,", "tokens": ["Wenn", "ihr", "Freun\u00b7de", "ver\u00b7ge\u00dft", ",", "wenn", "ihr", "den", "K\u00fcnst\u00b7ler", "h\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und den tieferen Geist klein und gemein versteht,", "tokens": ["Und", "den", "tie\u00b7fe\u00b7ren", "Geist", "klein", "und", "ge\u00b7mein", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gott vergibt es, doch st\u00f6rt nur", "tokens": ["Gott", "ver\u00b7gibt", "es", ",", "doch", "st\u00f6rt", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie den Frieden der Liebenden.", "tokens": ["Nie", "den", "Frie\u00b7den", "der", "Lie\u00b7ben\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}}}}