{"textgrid.poem.24896": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was weckt ihr mich? \u2013 Ich h\u00f6r' ein leises Ach,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was weckt ihr mich? \u2013 Ich h\u00f6r' ein leises Ach,", "tokens": ["Was", "weckt", "ihr", "mich", "?", "\u2013", "Ich", "h\u00f6r'", "ein", "lei\u00b7ses", "Ach", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$.", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Todesseufzer durch die S\u00e4le schallen;", "tokens": ["Wie", "To\u00b7des\u00b7seuf\u00b7zer", "durch", "die", "S\u00e4\u00b7le", "schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Traum rauscht bebend hin durch das Gemach,", "tokens": ["Der", "Traum", "rauscht", "be\u00b7bend", "hin", "durch", "das", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-++-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und \u00f6de stehn die k\u00f6niglichen Hallen.", "tokens": ["Und", "\u00f6\u00b7de", "stehn", "die", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den Boden, welcher Persiens Teppich trug,", "tokens": ["Den", "Bo\u00b7den", ",", "wel\u00b7cher", "Per\u00b7siens", "Tep\u00b7pich", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durchh\u00fcpfen nun die schillernden Cikaden;", "tokens": ["Durch\u00b7h\u00fcp\u00b7fen", "nun", "die", "schil\u00b7lern\u00b7den", "Ci\u00b7ka\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schwalbe schwingt mit ungewissem Flug", "tokens": ["Die", "Schwal\u00b7be", "schwingt", "mit", "un\u00b7ge\u00b7wis\u00b7sem", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich zwitschernd durch die st\u00fcrzenden Arkaden.", "tokens": ["Sich", "zwit\u00b7schernd", "durch", "die", "st\u00fcr\u00b7zen\u00b7den", "Ar\u00b7ka\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Zeit, da Lindaraja hier getr\u00e4umt", "tokens": ["O", "Zeit", ",", "da", "Lind\u00b7a\u00b7ra\u00b7ja", "hier", "ge\u00b7tr\u00e4umt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "NE", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bei B\u00fclb\u00fcls Fl\u00f6ten an dem Rosengitter,", "tokens": ["Bei", "B\u00fcl\u00b7b\u00fcls", "Fl\u00f6\u00b7ten", "an", "dem", "Ro\u00b7sen\u00b7git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da Musa hier sein Berberro\u00df gez\u00e4umt", "tokens": ["Da", "Mu\u00b7sa", "hier", "sein", "Ber\u00b7berr\u00b7o\u00df", "ge\u00b7z\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Kampf mit Manuel Leon, dem Ritter!", "tokens": ["Zum", "Kampf", "mit", "Ma\u00b7nu\u00b7el", "Le\u00b7on", ",", "dem", "Rit\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Waffenspiel, der Laute sanfter Schall,", "tokens": ["Das", "Waf\u00b7fen\u00b7spiel", ",", "der", "Lau\u00b7te", "sanf\u00b7ter", "Schall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Pracht der Feste und der Liebe Kosen,", "tokens": ["Die", "Pracht", "der", "Fes\u00b7te", "und", "der", "Lie\u00b7be", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das alles schwand \u2013 nur noch die Nachtigall", "tokens": ["Das", "al\u00b7les", "schwand", "\u2013", "nur", "noch", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "$(", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erz\u00e4hlt davon in Sommernacht den Rosen.", "tokens": ["Er\u00b7z\u00e4hlt", "da\u00b7von", "in", "Som\u00b7mer\u00b7nacht", "den", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gebrochen hat die Zeit den Talisman,", "tokens": ["Ge\u00b7bro\u00b7chen", "hat", "die", "Zeit", "den", "Ta\u00b7lis\u00b7man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An den gebunden war das sch\u00f6ne Leben;", "tokens": ["An", "den", "ge\u00b7bun\u00b7den", "war", "das", "sch\u00f6\u00b7ne", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Dichter aber murmelt einen Bann,", "tokens": ["Der", "Dich\u00b7ter", "a\u00b7ber", "mur\u00b7melt", "ei\u00b7nen", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bei dem sich aus der Gruft die Toten heben.", "tokens": ["Bei", "dem", "sich", "aus", "der", "Gruft", "die", "To\u00b7ten", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}