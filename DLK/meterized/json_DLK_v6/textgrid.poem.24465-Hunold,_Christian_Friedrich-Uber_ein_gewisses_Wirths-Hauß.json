{"textgrid.poem.24465": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber ein gewisses Wirths-Hau\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komt hier in dieses Hau\u00df/ di\u00df sind des Wirthes Gaben/", "tokens": ["Komt", "hier", "in", "die\u00b7ses", "Hau\u00df", "/", "di\u00df", "sind", "des", "Wirt\u00b7hes", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PDAT", "NN", "$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er schiert die Schaafe nur/ nach dem sie Wolle haben.", "tokens": ["Er", "schiert", "die", "Schaa\u00b7fe", "nur", "/", "nach", "dem", "sie", "Wol\u00b7le", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$(", "APPR", "PRELS", "PPER", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}