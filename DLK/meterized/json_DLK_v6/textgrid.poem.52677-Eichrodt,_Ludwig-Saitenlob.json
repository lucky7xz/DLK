{"textgrid.poem.52677": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Saitenlob", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selig, wem in Lautenkl\u00e4ngen", "tokens": ["Se\u00b7lig", ",", "wem", "in", "Lau\u00b7ten\u00b7kl\u00e4n\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Seele Schmerz entflieht!", "tokens": ["Sei\u00b7ner", "See\u00b7le", "Schmerz", "ent\u00b7flieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wem gepflanzet tief im Herzen", "tokens": ["Wem", "ge\u00b7pflan\u00b7zet", "tief", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Natur ein wuchernd Lied.", "tokens": ["Die", "Na\u00b7tur", "ein", "wu\u00b7chernd", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Selig wem des Sanges Geister", "tokens": ["Se\u00b7lig", "wem", "des", "San\u00b7ges", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PWS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu verstehen, ist gew\u00e4hrt \u2013", "tokens": ["Zu", "ver\u00b7ste\u00b7hen", ",", "ist", "ge\u00b7w\u00e4hrt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Trauer schmilzt in weichen T\u00f6nen", "tokens": ["Trau\u00b7er", "schmilzt", "in", "wei\u00b7chen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wonne wird im Lied verkl\u00e4rt.", "tokens": ["Won\u00b7ne", "wird", "im", "Lied", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Selig wer mit Lautenkl\u00e4ngen", "tokens": ["Se\u00b7lig", "wer", "mit", "Lau\u00b7ten\u00b7kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PWS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Seinen innern Sturm beschw\u00f6rt,", "tokens": ["Sei\u00b7nen", "in\u00b7nern", "Sturm", "be\u00b7schw\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df im Handeln er geruhig", "tokens": ["Da\u00df", "im", "Han\u00b7deln", "er", "ge\u00b7ru\u00b7hig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und im Wandel unbeth\u00f6rt;", "tokens": ["Und", "im", "Wan\u00b7del", "un\u00b7be\u00b7th\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df er frisch ein Lied des Muthes", "tokens": ["Da\u00df", "er", "frisch", "ein", "Lied", "des", "Mu\u00b7thes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Singt dem Tod ins Angesicht,", "tokens": ["Singt", "dem", "Tod", "ins", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Selig wer auch Andern singend,", "tokens": ["Se\u00b7lig", "wer", "auch", "An\u00b7dern", "sin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "ADV", "ADJA", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Ewgen Trostes Kr\u00e4nze flicht!", "tokens": ["Ew\u00b7gen", "Tros\u00b7tes", "Kr\u00e4n\u00b7ze", "flicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}