{"dta.poem.19218": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh diese Muschel, Kind, gewunden, gl\u00e4nzendroth,", "tokens": ["Sieh", "die\u00b7se", "Mu\u00b7schel", ",", "Kind", ",", "ge\u00b7wun\u00b7den", ",", "gl\u00e4n\u00b7zend\u00b7roth", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "NN", "$,", "VAPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sag' ob Menschenkunst je baute solch ein Boot!", "tokens": ["Und", "sag'", "ob", "Men\u00b7schen\u00b7kunst", "je", "bau\u00b7te", "solch", "ein", "Boot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "ADV", "VVFIN", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als noch der Steuermann darin, der Nautil, lebte;", "tokens": ["Als", "noch", "der", "Steu\u00b7er\u00b7mann", "da\u00b7rin", ",", "der", "Nau\u00b7til", ",", "leb\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PAV", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sicher und gewandt durchs Meer der Nachen schwebte!", "tokens": ["Wie", "si\u00b7cher", "und", "ge\u00b7wandt", "durchs", "Meer", "der", "Na\u00b7chen", "schweb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVPP", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schiffkuttel hie\u00df er auch, und nie hat Schiff und Kutter", "tokens": ["Schiff\u00b7kut\u00b7tel", "hie\u00df", "er", "auch", ",", "und", "nie", "hat", "Schiff", "und", "Kut\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ihm wol gleich gethan, wenn er schwamm aus auf Futter.", "tokens": ["Es", "ihm", "wol", "gleich", "ge\u00b7than", ",", "wenn", "er", "schwamm", "aus", "auf", "Fut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "ADV", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "APPR", "APPR", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Man sagt, es hat von ihm der Mensch gelernt das Schiffen,", "tokens": ["Man", "sagt", ",", "es", "hat", "von", "ihm", "der", "Mensch", "ge\u00b7lernt", "das", "Schif\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch hat er von der Kunst nur einen Theil begriffen,", "tokens": ["Doch", "hat", "er", "von", "der", "Kunst", "nur", "ei\u00b7nen", "Theil", "be\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und braucht dazu viel mehr Gezeug, Ger\u00e4th, Ger\u00fcst,", "tokens": ["Und", "braucht", "da\u00b7zu", "viel", "mehr", "Ge\u00b7zeug", ",", "Ge\u00b7r\u00e4\u00b7th", ",", "Ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADV", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Als unser Nautil, der sich selber Alles ist.", "tokens": ["Als", "un\u00b7ser", "Nau\u00b7til", ",", "der", "sich", "sel\u00b7ber", "Al\u00b7les", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "PRF", "ADV", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wenn eben war die Flut, und es ihm d\u00fcnkte gut", "tokens": ["Wenn", "e\u00b7ben", "war", "die", "Flut", ",", "und", "es", "ihm", "d\u00fcnk\u00b7te", "gut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu schiffen, r\u00fcstet' er sein Schifflein wohlgemut.", "tokens": ["Zu", "schif\u00b7fen", ",", "r\u00fcs\u00b7tet'", "er", "sein", "Schif\u00b7flein", "wohl\u00b7ge\u00b7mut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.7": {"line.1": {"text": "Von seinen Aermen, den in gro\u00dfer Zahl verliehnen,", "tokens": ["Von", "sei\u00b7nen", "A\u00b7er\u00b7men", ",", "den", "in", "gro\u00b7\u00dfer", "Zahl", "ver\u00b7lieh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Streckt' er ein Paar empor, zu Masten ihm zu dienen.", "tokens": ["Streckt'", "er", "ein", "Paar", "em\u00b7por", ",", "zu", "Mas\u00b7ten", "ihm", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und zwischen ihnen dehnt' er aus nach rechter Regel", "tokens": ["Und", "zwi\u00b7schen", "ih\u00b7nen", "dehnt'", "er", "aus", "nach", "rech\u00b7ter", "Re\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein H\u00e4utchen zart und fein, das schwoll im Wind als Segel.", "tokens": ["Ein", "H\u00e4ut\u00b7chen", "zart", "und", "fein", ",", "das", "schwoll", "im", "Wind", "als", "Se\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,", "PRELS", "ADJD", "APPRART", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Als Steuer ist ins Meer ein andres Glied getaucht,", "tokens": ["Als", "Steu\u00b7er", "ist", "ins", "Meer", "ein", "and\u00b7res", "Glied", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Ruder rechts und links, soviel er immer braucht.", "tokens": ["Und", "Ru\u00b7der", "rechts", "und", "links", ",", "so\u00b7viel", "er", "im\u00b7mer", "braucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "KON", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Gliedma\u00dfen blieben ihm frei immer noch genug,", "tokens": ["Glied\u00b7ma\u00b7\u00dfen", "blie\u00b7ben", "ihm", "frei", "im\u00b7mer", "noch", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zu haschen einen Raub auch im Vor\u00fcberflug.", "tokens": ["Zu", "ha\u00b7schen", "ei\u00b7nen", "Raub", "auch", "im", "Vor\u00b7\u00fc\u00b7berf\u00b7lug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Wann aber naht ein Feind, wann droht ein Ungewitter,", "tokens": ["Wann", "a\u00b7ber", "naht", "ein", "Feind", ",", "wann", "droht", "ein", "Un\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wovor ein Menschenschiff verzagt und geht in Splitter;", "tokens": ["Wo\u00b7vor", "ein", "Men\u00b7schen\u00b7schiff", "ver\u00b7zagt", "und", "geht", "in", "Split\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Dann zeigt weit ihre Macht ob aller Kunst Natur:", "tokens": ["Dann", "zeigt", "weit", "ih\u00b7re", "Macht", "ob", "al\u00b7ler", "Kunst", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "KOUS", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich selbst und sein Ger\u00e4th zieht ein der Kuttel nur.", "tokens": ["Sich", "selbst", "und", "sein", "Ge\u00b7r\u00e4\u00b7th", "zieht", "ein", "der", "Kut\u00b7tel", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "PPOSAT", "NN", "VVFIN", "ART", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Meerwasser nimmt er ein, nicht f\u00fcrchtend zu ertrinken;", "tokens": ["Meer\u00b7was\u00b7ser", "nimmt", "er", "ein", ",", "nicht", "f\u00fcrch\u00b7tend", "zu", "er\u00b7trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Statt Untergang dient ihm zur Rettung das Versinken.", "tokens": ["Statt", "Un\u00b7ter\u00b7gang", "dient", "ihm", "zur", "Ret\u00b7tung", "das", "Ver\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Zum ewig stillen Grund versinkt er ohne Grausen,", "tokens": ["Zum", "e\u00b7wig", "stil\u00b7len", "Grund", "ver\u00b7sinkt", "er", "oh\u00b7ne", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wartet bis sich legt der Oberfl\u00e4che Brausen.", "tokens": ["Und", "war\u00b7tet", "bis", "sich", "legt", "der", "O\u00b7berf\u00b7l\u00e4\u00b7che", "Brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Dann taucht er wieder auf im umgest\u00fclpten Nachen,", "tokens": ["Dann", "taucht", "er", "wie\u00b7der", "auf", "im", "um\u00b7ge\u00b7st\u00fclp\u00b7ten", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Taucherglocke gleich, um nun die Fahrt zu machen.", "tokens": ["Der", "Tau\u00b7cher\u00b7glo\u00b7cke", "gleich", ",", "um", "nun", "die", "Fahrt", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUI", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "So, lang gesegelt und gesteuert ist sein Schiff,", "tokens": ["So", ",", "lang", "ge\u00b7se\u00b7gelt", "und", "ge\u00b7steu\u00b7ert", "ist", "sein", "Schiff", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VVPP", "KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Ribben wund gerieben hat kein Riff.", "tokens": ["Und", "sei\u00b7ne", "Rib\u00b7ben", "wund", "ge\u00b7rie\u00b7ben", "hat", "kein", "Riff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVPP", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Und als der Tod gebot dem Bootsmann doch sein Boot", "tokens": ["Und", "als", "der", "Tod", "ge\u00b7bot", "dem", "Boots\u00b7mann", "doch", "sein", "Boot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu r\u00e4umen, blieb der Raum der leere gl\u00e4nzend roth.", "tokens": ["Zu", "r\u00e4u\u00b7men", ",", "blieb", "der", "Raum", "der", "lee\u00b7re", "gl\u00e4n\u00b7zend", "roth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "ART", "NN", "ART", "ADJA", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "So ist es dir, mein Kind, zum Spiel ans Land geschwommen,", "tokens": ["So", "ist", "es", "dir", ",", "mein", "Kind", ",", "zum", "Spiel", "ans", "Land", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Farben sind nach Jahren unverglommen.", "tokens": ["Und", "sei\u00b7ne", "Far\u00b7ben", "sind", "nach", "Jah\u00b7ren", "un\u00b7ver\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}