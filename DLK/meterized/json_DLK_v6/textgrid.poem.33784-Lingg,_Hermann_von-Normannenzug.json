{"textgrid.poem.33784": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Normannenzug", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Nordland auf dem Felsenh\u00fcgel", "tokens": ["Im", "Nord\u00b7land", "auf", "dem", "Fel\u00b7sen\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gl\u00e4nzt hell im Mond der tiefe Schnee,", "tokens": ["Gl\u00e4nzt", "hell", "im", "Mond", "der", "tie\u00b7fe", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ruft ein Aar und schwingt die Fl\u00fcgel:", "tokens": ["Da", "ruft", "ein", "Aar", "und", "schwingt", "die", "Fl\u00fc\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbwo seid ihr, K\u00f6nige der See?", "tokens": ["\u00bb", "wo", "seid", "ihr", ",", "K\u00f6\u00b7ni\u00b7ge", "der", "See", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seid ihr zum Speerkampf nach den Sunden,", "tokens": ["Seid", "ihr", "zum", "Speer\u00b7kampf", "nach", "den", "Sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verhalten euch am Strande Wunden,", "tokens": ["Ver\u00b7hal\u00b7ten", "euch", "am", "Stran\u00b7de", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Seid ihr den Wolf zu jagen aus?", "tokens": ["Seid", "ihr", "den", "Wolf", "zu", "ja\u00b7gen", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ART", "NE", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo m\u00f6gen eure Schwerter gl\u00e4nzen?", "tokens": ["Wo", "m\u00f6\u00b7gen", "eu\u00b7re", "Schwer\u00b7ter", "gl\u00e4n\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo lacht zu euren Siegest\u00e4nzen", "tokens": ["Wo", "lacht", "zu", "eu\u00b7ren", "Sie\u00b7ge\u00b7st\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Beim Ruderschlag das Meergebraus?\u00ab", "tokens": ["Beim", "Ru\u00b7der\u00b7schlag", "das", "Meer\u00b7ge\u00b7braus", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Feind hat Wunden uns geschlagen,", "tokens": ["Kein", "Feind", "hat", "Wun\u00b7den", "uns", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sind nicht aus, den grauen Wolf", "tokens": ["Wir", "sind", "nicht", "aus", ",", "den", "grau\u00b7en", "Wolf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKVZ", "$,", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus seinen Schluchten aufzujagen,", "tokens": ["Aus", "sei\u00b7nen", "Schluch\u00b7ten", "auf\u00b7zu\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir segeln nicht im D\u00e4nengolf.", "tokens": ["Wir", "se\u00b7geln", "nicht", "im", "D\u00e4\u00b7nen\u00b7golf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach S\u00fcden riefen uns die Wellen,", "tokens": ["Nach", "S\u00fc\u00b7den", "rie\u00b7fen", "uns", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Morgen gl\u00fcht, die Segel schwellen", "tokens": ["Der", "Mor\u00b7gen", "gl\u00fcht", ",", "die", "Se\u00b7gel", "schwel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Im frischen Hauch der blauen Flut;", "tokens": ["Im", "fri\u00b7schen", "Hauch", "der", "blau\u00b7en", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie purpurn gl\u00e4nzt der Schiffe Br\u00fcstung,", "tokens": ["Wie", "pur\u00b7purn", "gl\u00e4nzt", "der", "Schif\u00b7fe", "Br\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Sonne blitzt in unsrer R\u00fcstung,", "tokens": ["Die", "Son\u00b7ne", "blitzt", "in", "uns\u00b7rer", "R\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In unsern Herzen blitzt der Mut.", "tokens": ["In", "un\u00b7sern", "Her\u00b7zen", "blitzt", "der", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Seewind spielt in unsern Locken;", "tokens": ["Der", "See\u00b7wind", "spielt", "in", "un\u00b7sern", "Lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bald ruft, da\u00df unsre Schiffe nahn,", "tokens": ["Bald", "ruft", ",", "da\u00df", "uns\u00b7re", "Schif\u00b7fe", "nahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Ufer rings mit Sturmesglocken", "tokens": ["Am", "U\u00b7fer", "rings", "mit", "Stur\u00b7mes\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Schlo\u00df zu Schlo\u00df der Kastellan;", "tokens": ["Von", "Schlo\u00df", "zu", "Schlo\u00df", "der", "Kas\u00b7tel\u00b7lan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald bebt mit ihrem hohen Dome", "tokens": ["Bald", "bebt", "mit", "ih\u00b7rem", "ho\u00b7hen", "Do\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Stadt, die sich am Tajostrome,", "tokens": ["Die", "Stadt", ",", "die", "sich", "am", "Ta\u00b7jost\u00b7ro\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die an Biscaya's Bucht sich sonnt;", "tokens": ["Die", "an", "Bis\u00b7ca\u00b7ya's", "Bucht", "sich", "sonnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bald fliehn die Flaggen der Korsaren", "tokens": ["Bald", "fliehn", "die", "Flag\u00b7gen", "der", "Kor\u00b7sa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und jene mit Venedigs Waren", "tokens": ["Und", "je\u00b7ne", "mit", "Ve\u00b7ne\u00b7digs", "Wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "APPR", "NE", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Von Malta bis zum Hellespont.", "tokens": ["Von", "Mal\u00b7ta", "bis", "zum", "Hel\u00b7les\u00b7pont", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu Boden schmettern wir die Krieger,", "tokens": ["Zu", "Bo\u00b7den", "schmet\u00b7tern", "wir", "die", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die unsrer Landung widerstehn;", "tokens": ["Die", "uns\u00b7rer", "Lan\u00b7dung", "wi\u00b7der\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sch\u00f6nste Dame gibt dem Sieger", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Da\u00b7me", "gibt", "dem", "Sie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gebiet und Herz und Burg zu Leh'n.", "tokens": ["Ge\u00b7biet", "und", "Herz", "und", "Burg", "zu", "Leh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "NN", "KON", "NN", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kredenzend Wein im Goldpokale", "tokens": ["Kre\u00b7den\u00b7zend", "Wein", "im", "Gold\u00b7po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erscheint sie bei dem Hochzeitmahle", "tokens": ["Er\u00b7scheint", "sie", "bei", "dem", "Hoch\u00b7zeit\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf dem Balkon vor ihrem Gast;", "tokens": ["Auf", "dem", "Bal\u00b7kon", "vor", "ih\u00b7rem", "Gast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Dann schallen Jubel und Trompeten,", "tokens": ["Dann", "schal\u00b7len", "Ju\u00b7bel", "und", "Trom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Dann gibt es Jagden, Spiele, F\u00eaten", "tokens": ["Dann", "gibt", "es", "Jag\u00b7den", ",", "Spie\u00b7le", ",", "F\u00eaten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und T\u00e4nze bis die Nacht erbla\u00dft.", "tokens": ["Und", "T\u00e4n\u00b7ze", "bis", "die", "Nacht", "er\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O sch\u00f6ne Damen, schlanke Lilien,", "tokens": ["O", "sch\u00f6\u00b7ne", "Da\u00b7men", ",", "schlan\u00b7ke", "Li\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor e-uch wir beugen unser Knie;", "tokens": ["Vor", "e\u00b7uch", "wir", "beu\u00b7gen", "un\u00b7ser", "Knie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wir sind die F\u00fcrsten von Sizilien,", "tokens": ["Wir", "sind", "die", "F\u00fcrs\u00b7ten", "von", "Si\u00b7zi\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir sind die Herrn der Normandie.", "tokens": ["Wir", "sind", "die", "Herrn", "der", "Nor\u00b7man\u00b7die", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor unsern guten Speeren sanken", "tokens": ["Vor", "un\u00b7sern", "gu\u00b7ten", "Spee\u00b7ren", "san\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sarazenen und die Franken,", "tokens": ["Die", "Sa\u00b7ra\u00b7ze\u00b7nen", "und", "die", "Fran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir sind die K\u00f6nige der See.", "tokens": ["Wir", "sind", "die", "K\u00f6\u00b7ni\u00b7ge", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Norden auf dem Grab der H\u00fcnen,", "tokens": ["Im", "Nor\u00b7den", "auf", "dem", "Grab", "der", "H\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auf unsrer V\u00e4ter Grab, der K\u00fchnen,", "tokens": ["Auf", "uns\u00b7rer", "V\u00e4\u00b7ter", "Grab", ",", "der", "K\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Gl\u00e4nzt hell im Mond der tiefe Schnee.", "tokens": ["Gl\u00e4nzt", "hell", "im", "Mond", "der", "tie\u00b7fe", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}