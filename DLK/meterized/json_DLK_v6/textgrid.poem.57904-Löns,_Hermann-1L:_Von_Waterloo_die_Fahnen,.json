{"textgrid.poem.57904": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Waterloo die Fahnen,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Waterloo die Fahnen,", "tokens": ["Von", "Wa\u00b7ter\u00b7loo", "die", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sind jetzt in Berlin,", "tokens": ["Die", "sind", "jetzt", "in", "Ber\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "NE", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und die von Langensalza,", "tokens": ["Und", "die", "von", "Lan\u00b7gen\u00b7sal\u00b7za", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die sollen jetzt auch dahin.", "tokens": ["Die", "sol\u00b7len", "jetzt", "auch", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "PAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Silberfund von Hilmsen,", "tokens": ["Der", "Sil\u00b7ber\u00b7fund", "von", "Hilm\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ist schon lange da;", "tokens": ["Der", "ist", "schon", "lan\u00b7ge", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In L\u00fcneburg vom Ratsschatz", "tokens": ["In", "L\u00fc\u00b7ne\u00b7burg", "vom", "Rats\u00b7schatz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich auch nur Kopien sah.", "tokens": ["Ich", "auch", "nur", "Ko\u00b7pi\u00b7en", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Bald wird's in unsern Kirchen", "tokens": ["Bald", "wird's", "in", "un\u00b7sern", "Kir\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und unsern Museen leer;", "tokens": ["Und", "un\u00b7sern", "Mu\u00b7se\u00b7en", "leer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hann\u00f6versche Reliquien,", "tokens": ["Han\u00b7n\u00f6\u00b7ver\u00b7sche", "Re\u00b7li\u00b7qui\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die findest du da nicht mehr.", "tokens": ["Die", "fin\u00b7dest", "du", "da", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch la\u00df den Mut nicht sinken,", "tokens": ["Doch", "la\u00df", "den", "Mut", "nicht", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sei traurig nicht, mein Sohn;", "tokens": ["Sei", "trau\u00b7rig", "nicht", ",", "mein", "Sohn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PTKNEG", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was brauchst du der V\u00e4ter Erbe?", "tokens": ["Was", "brauchst", "du", "der", "V\u00e4\u00b7ter", "Er\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du hast ja die Tradition!", "tokens": ["Du", "hast", "ja", "die", "Tra\u00b7di\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}