{"textgrid.poem.33160": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1972.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heilige f\u00fcnf wunden-maal in seit, hand und fusse, ursach meiner gnadenwahl, zeugen g\u00fcltger busse!", "tokens": ["Hei\u00b7li\u00b7ge", "f\u00fcnf", "wun\u00b7den\u00b7maal", "in", "seit", ",", "hand", "und", "fus\u00b7se", ",", "ur\u00b7sach", "mei\u00b7ner", "gna\u00b7den\u00b7wahl", ",", "zeu\u00b7gen", "g\u00fclt\u00b7ger", "bus\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "CARD", "NN", "APPR", "NN", "$,", "NN", "KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "+---+-+--+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn ich an die seligkeit denk im \u00fcberflusse, die mir euer kampf bereit! Dank habt f\u00fcr die busse.", "tokens": ["Wenn", "ich", "an", "die", "se\u00b7lig\u00b7keit", "denk", "im", "\u00fc\u00b7berf\u00b7lus\u00b7se", ",", "die", "mir", "eu\u00b7er", "kampf", "be\u00b7reit", "!", "Dank", "habt", "f\u00fcr", "die", "bus\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "APPRART", "ADJA", "$,", "PRELS", "PPER", "PPOSAT", "NN", "ADJD", "$.", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus"}}}}}