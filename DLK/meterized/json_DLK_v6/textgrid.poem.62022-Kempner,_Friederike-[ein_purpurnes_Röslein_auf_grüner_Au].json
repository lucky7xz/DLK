{"textgrid.poem.62022": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[ein purpurnes R\u00f6slein auf gr\u00fcner Au]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein purpurnes R\u00f6slein auf gr\u00fcner Au,", "tokens": ["Ein", "pur\u00b7pur\u00b7nes", "R\u00f6s\u00b7lein", "auf", "gr\u00fc\u00b7ner", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein g\u00fcldenes Sternlein am himmlischen Blau,", "tokens": ["Ein", "g\u00fcl\u00b7de\u00b7nes", "Stern\u00b7lein", "am", "himm\u00b7li\u00b7schen", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein singendes V\u00f6glein auf schwankendem Ast,", "tokens": ["Ein", "sin\u00b7gen\u00b7des", "V\u00f6\u00b7glein", "auf", "schwan\u00b7ken\u00b7dem", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sag' an, was Du Sch\u00f6nres zu zeigen hast?", "tokens": ["Sag'", "an", ",", "was", "Du", "Sch\u00f6n\u00b7res", "zu", "zei\u00b7gen", "hast", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWS", "PPER", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Gar sch\u00f6n ist solch' R\u00f6slein auf gr\u00fcner Au,", "tokens": ["Gar", "sch\u00f6n", "ist", "solch'", "R\u00f6s\u00b7lein", "auf", "gr\u00fc\u00b7ner", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gar pr\u00e4chtig solch' Sternlein am himmlischen Blau,", "tokens": ["Gar", "pr\u00e4ch\u00b7tig", "solch'", "Stern\u00b7lein", "am", "himm\u00b7li\u00b7schen", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Gar frei ist das V\u00f6glein auf schwankendem Ast,", "tokens": ["Gar", "frei", "ist", "das", "V\u00f6\u00b7glein", "auf", "schwan\u00b7ken\u00b7dem", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und Freiheit und Sch\u00f6nheit zusammen wohl pa\u00dft!", "tokens": ["Und", "Frei\u00b7heit", "und", "Sch\u00f6n\u00b7heit", "zu\u00b7sam\u00b7men", "wohl", "pa\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}