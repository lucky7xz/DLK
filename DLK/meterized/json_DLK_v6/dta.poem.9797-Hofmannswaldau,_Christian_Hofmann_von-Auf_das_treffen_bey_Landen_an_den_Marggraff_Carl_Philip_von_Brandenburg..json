{"dta.poem.9797": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das treffen bey Landen an den Marggraff  \n Carl Philip von Brandenburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den sieg/ der durch den spruch der tugend unser war/", "tokens": ["Den", "sieg", "/", "der", "durch", "den", "spruch", "der", "tu\u00b7gend", "un\u00b7ser", "war", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "ART", "NN", "ART", "NN", "PPOSAT", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat mit gewalt das gl\u00fcck uns aus der faust gerissen/", "tokens": ["Hat", "mit", "ge\u00b7walt", "das", "gl\u00fcck", "uns", "aus", "der", "faust", "ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das gl\u00fcck/ so uns verfolgt/ und denen gantz und gar", "tokens": ["Das", "gl\u00fcck", "/", "so", "uns", "ver\u00b7folgt", "/", "und", "de\u00b7nen", "gantz", "und", "gar"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$(", "KON", "PRELS", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich nicht verbinden will/ die unsers bunds geniessen.", "tokens": ["Sich", "nicht", "ver\u00b7bin\u00b7den", "will", "/", "die", "un\u00b7sers", "bunds", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VVINF", "VMFIN", "$(", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drumb m\u00fcssen wir das feld/ so mit des feindes blut", "tokens": ["Drumb", "m\u00fcs\u00b7sen", "wir", "das", "feld", "/", "so", "mit", "des", "fein\u00b7des", "blut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ART", "NN", "$(", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir reichlich durchgenetzt/ ihm/ als dem fleger/ lassen/", "tokens": ["Wir", "reich\u00b7lich", "durch\u00b7ge\u00b7netzt", "/", "ihm", "/", "als", "dem", "fle\u00b7ger", "/", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$(", "PPER", "$(", "KOUS", "ART", "ADJA", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und um ein spann-breit erd/ so wir mit grossem muth", "tokens": ["Und", "um", "ein", "spann\u00b7breit", "erd", "/", "so", "wir", "mit", "gros\u00b7sem", "muth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$(", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auffs \u00e4usserste behaupt/ den r\u00fcckweg endlich fassen.", "tokens": ["Auffs", "\u00e4us\u00b7sers\u00b7te", "be\u00b7haupt", "/", "den", "r\u00fcck\u00b7weg", "end\u00b7lich", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "ART", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wann uns der nachruff nicht un\u00fcberwindlich schreibt/", "tokens": ["Wann", "uns", "der", "nach\u00b7ruff", "nicht", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "schreibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wird uns die ehre doch das lob der braven geben:", "tokens": ["Wird", "uns", "die", "eh\u00b7re", "doch", "das", "lob", "der", "bra\u00b7ven", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auch \u00fcberwunden seyn/ wann die gewalt es treibt/", "tokens": ["Auch", "\u00fc\u00b7berw\u00b7un\u00b7den", "seyn", "/", "wann", "die", "ge\u00b7walt", "es", "treibt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$(", "PWAV", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat einen sieg bey sich/ und l\u00e4st mit ruhme leben.", "tokens": ["Hat", "ei\u00b7nen", "sieg", "bey", "sich", "/", "und", "l\u00e4st", "mit", "ruh\u00b7me", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PRF", "$(", "KON", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So steht man unbewegt den gro\u00dfen Wilhelm stehn/", "tokens": ["So", "steht", "man", "un\u00b7be\u00b7wegt", "den", "gro\u00b7\u00dfen", "Wil\u00b7helm", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "ART", "ADJA", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und hertzen mit der macht und menge zu vergleichen/", "tokens": ["Und", "hert\u00b7zen", "mit", "der", "macht", "und", "men\u00b7ge", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "VVFIN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der tapfre Bojoar l\u00e4st unerschrocken sehn/", "tokens": ["Der", "tapf\u00b7re", "Bo\u00b7joar", "l\u00e4st", "un\u00b7er\u00b7schro\u00b7cken", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Wie man zum grossen werck die tapffre hand mu\u00df reichen.", "tokens": ["Wie", "man", "zum", "gros\u00b7sen", "werck", "die", "tapf\u00b7fre", "hand", "mu\u00df", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du auch/ des vaterlands geweihter trost und lust/", "tokens": ["Du", "auch", "/", "des", "va\u00b7ter\u00b7lands", "ge\u00b7weih\u00b7ter", "trost", "und", "lust", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ART", "NN", "ADJA", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du unerschreckner Carl/ des grossen Vaters spiegel/", "tokens": ["Du", "un\u00b7er\u00b7schreck\u00b7ner", "Carl", "/", "des", "gros\u00b7sen", "Va\u00b7ters", "spie\u00b7gel", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "$(", "ART", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Stehst diesen beyden bey mit felsen-gleicher brust/", "tokens": ["Stehst", "die\u00b7sen", "bey\u00b7den", "bey", "mit", "fel\u00b7sen\u00b7glei\u00b7cher", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "PIAT", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "F\u00fchrst an dein heer/ und l\u00e4st der tapfferkeit den z\u00fcgel.", "tokens": ["F\u00fchrst", "an", "dein", "heer", "/", "und", "l\u00e4st", "der", "tapf\u00b7fer\u00b7keit", "den", "z\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$(", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dir/ Hertzog/ jauchtzt dein volck/ und deine gegenwart", "tokens": ["Dir", "/", "Hert\u00b7zog", "/", "jauchtzt", "dein", "volck", "/", "und", "dei\u00b7ne", "ge\u00b7gen\u00b7wart"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$(", "NE", "$(", "VVFIN", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Z\u00fcndt glut und feuer an den muthigen soldaten:", "tokens": ["Z\u00fcndt", "glut", "und", "feu\u00b7er", "an", "den", "mut\u00b7hi\u00b7gen", "sol\u00b7da\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "APPR", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Des feindes raserey hingegen stutzt und starrt/", "tokens": ["Des", "fein\u00b7des", "ra\u00b7se\u00b7rey", "hin\u00b7ge\u00b7gen", "stutzt", "und", "starrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wird als bl\u00f6d und feig durch deine helden-thaten.", "tokens": ["Und", "wird", "als", "bl\u00f6d", "und", "feig", "durch", "dei\u00b7ne", "hel\u00b7den\u00b7tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da\u00df dein so junges blut so tapffres wesen hegt/", "tokens": ["Da\u00df", "dein", "so", "jun\u00b7ges", "blut", "so", "tapf\u00b7fres", "we\u00b7sen", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ist/ was dem feinde selbst unsch\u00e4tzbar scheint zu sch\u00e4tzen/", "tokens": ["Ist", "/", "was", "dem", "fein\u00b7de", "selbst", "un\u00b7sch\u00e4tz\u00b7bar", "scheint", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "PWS", "ART", "NN", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df was dein grosser muth ihm als f\u00fcr augen legt/", "tokens": ["Da\u00df", "was", "dein", "gros\u00b7ser", "muth", "ihm", "als", "f\u00fcr", "au\u00b7gen", "legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "ADJA", "NN", "PPER", "KOKOM", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Zwingt ihn/ da\u00df er dir selbst mu\u00df ehren-bogen setzen.", "tokens": ["Zwingt", "ihn", "/", "da\u00df", "er", "dir", "selbst", "mu\u00df", "eh\u00b7ren\u00b7bo\u00b7gen", "set\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Di\u00df lachet der Jber/ und l\u00e4st sein sch\u00f6nes gold/", "tokens": ["Di\u00df", "la\u00b7chet", "der", "Jber", "/", "und", "l\u00e4st", "sein", "sch\u00f6\u00b7nes", "gold", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Das er und Tagus zinst/ mit reichen adern fliessen/", "tokens": ["Das", "er", "und", "Ta\u00b7gus", "zinst", "/", "mit", "rei\u00b7chen", "a\u00b7dern", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "KON", "NN", "VVFIN", "$(", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der Belg und Bataver liebt dich/ und ist dir hold/", "tokens": ["Der", "Belg", "und", "Ba\u00b7ta\u00b7ver", "liebt", "dich", "/", "und", "ist", "dir", "hold", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "VVFIN", "PPER", "$(", "KON", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Der Britten strand erschallt von deinen ehren-gr\u00fcssen.", "tokens": ["Der", "Brit\u00b7ten", "strand", "er\u00b7schallt", "von", "dei\u00b7nen", "eh\u00b7ren\u00b7gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Di\u00df ist ein trost f\u00fcr uns! hierdurch wird uns ersetzt/", "tokens": ["Di\u00df", "ist", "ein", "trost", "f\u00fcr", "uns", "!", "hier\u00b7durch", "wird", "uns", "er\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PPER", "$.", "PAV", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was uns vorhin betr\u00fcbt und unsern geist gequ\u00e4let:", "tokens": ["Was", "uns", "vor\u00b7hin", "be\u00b7tr\u00fcbt", "und", "un\u00b7sern", "geist", "ge\u00b7qu\u00e4\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wir lachen unsers bluts/ und wie wir seyn verletzt/", "tokens": ["Wir", "la\u00b7chen", "un\u00b7sers", "bluts", "/", "und", "wie", "wir", "seyn", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "PWAV", "PPER", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dieweil man selbiges zu deinem ruhme zehlet.", "tokens": ["Die\u00b7weil", "man", "sel\u00b7bi\u00b7ges", "zu", "dei\u00b7nem", "ruh\u00b7me", "zeh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wenn das verh\u00e4ngni\u00df g\u00f6nnt/ da\u00df Brennus adler sich/", "tokens": ["Wenn", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "g\u00f6nnt", "/", "da\u00df", "Bren\u00b7nus", "ad\u00b7ler", "sich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$(", "KOUS", "NE", "NE", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die ietzt zerstreuet seyn/ zusammen m\u00f6gen bringen/", "tokens": ["Die", "ietzt", "zer\u00b7streu\u00b7et", "seyn", "/", "zu\u00b7sam\u00b7men", "m\u00f6\u00b7gen", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAINF", "$(", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und durch den k\u00fchnen flug/ vereiniget durch dich", "tokens": ["Und", "durch", "den", "k\u00fch\u00b7nen", "flug", "/", "ver\u00b7ei\u00b7ni\u00b7get", "durch", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$(", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und deine tapfre faust/ dem feind entgegen schwingen:", "toke