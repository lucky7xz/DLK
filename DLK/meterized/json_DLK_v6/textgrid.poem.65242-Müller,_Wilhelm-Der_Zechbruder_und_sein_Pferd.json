{"textgrid.poem.65242": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Zechbruder und sein Pferd", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatt' einmal ein Gaul,", "tokens": ["Ich", "hatt'", "ein\u00b7mal", "ein", "Gaul", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das th\u00e4t sch\u00f6n galoppiren,", "tokens": ["Das", "th\u00e4t", "sch\u00f6n", "ga\u00b7lop\u00b7pi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "War von gar frommer Art,", "tokens": ["War", "von", "gar", "from\u00b7mer", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Kindlein konnt' es f\u00fchren;", "tokens": ["Ein", "Kin\u00b7dlein", "konnt'", "es", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch wenn es an ein Wirthshaus kam,", "tokens": ["Doch", "wenn", "es", "an", "ein", "Wirths\u00b7haus", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Kopf es in die Beine nahm,", "tokens": ["Den", "Kopf", "es", "in", "die", "Bei\u00b7ne", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Warf in den Sand mich lieber,", "tokens": ["Warf", "in", "den", "Sand", "mich", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Als da\u00df es ging vor\u00fcber.", "tokens": ["Als", "da\u00df", "es", "ging", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Wirth sa\u00df vor der Th\u00fcr,", "tokens": ["Der", "Wirth", "sa\u00df", "vor", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und sprang herzu behende;", "tokens": ["Und", "sprang", "her\u00b7zu", "be\u00b7hen\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleich stand das R\u00f6\u00dflein still,", "tokens": ["Gleich", "stand", "das", "R\u00f6\u00df\u00b7lein", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als ob's ein Zauber b\u00e4nde.", "tokens": ["Als", "ob's", "ein", "Zau\u00b7ber", "b\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So ging's in Stall und Stub' hinein,", "tokens": ["So", "ging's", "in", "Stall", "und", "Stub'", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Ro\u00df fra\u00df Hafer, ich trank Wein:", "tokens": ["Das", "Ro\u00df", "fra\u00df", "Ha\u00b7fer", ",", "ich", "trank", "Wein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das R\u00f6\u00dflein wurde w\u00e4hlig,", "tokens": ["Das", "R\u00f6\u00df\u00b7lein", "wur\u00b7de", "w\u00e4h\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Reiter wurde selig.", "tokens": ["Der", "Rei\u00b7ter", "wur\u00b7de", "se\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da fiel es denn mir ein,", "tokens": ["Da", "fiel", "es", "denn", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das R\u00f6\u00dflein zu verkaufen,", "tokens": ["Das", "R\u00f6\u00df\u00b7lein", "zu", "ver\u00b7kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das mich so t\u00fcckisch zwang,", "tokens": ["Das", "mich", "so", "t\u00fc\u00b7ckisch", "zwang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mich t\u00e4glich zu besaufen.", "tokens": ["Mich", "t\u00e4g\u00b7lich", "zu", "be\u00b7sau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn ach! viel Schenken giebt es hier,", "tokens": ["Denn", "ach", "!", "viel", "Schen\u00b7ken", "giebt", "es", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00fcberall gut Wein und Bier:", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "gut", "Wein", "und", "Bier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In jeder nur ", "tokens": ["In", "je\u00b7der", "nur"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "So wirbelt's schon im N\u00e4schen.", "tokens": ["So", "wir\u00b7belt's", "schon", "im", "N\u00e4\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Verruchtes Teufelsthier!", "tokens": ["Ver\u00b7ruch\u00b7tes", "Teu\u00b7fel\u00b7sthier", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nun hatt' ich's in den Taschen,", "tokens": ["Nun", "hatt'", "ich's", "in", "den", "Ta\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als baares blankes Geld,", "tokens": ["Als", "baa\u00b7res", "blan\u00b7kes", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vollauf zu tausend Flaschen.", "tokens": ["Vol\u00b7lauf", "zu", "tau\u00b7send", "Fla\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch um zu zeigen, wer ich sei,", "tokens": ["Doch", "um", "zu", "zei\u00b7gen", ",", "wer", "ich", "sei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PTKZU", "VVINF", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wollt' ich am Wirthshaus frank und frei", "tokens": ["Wollt'", "ich", "am", "Wirths\u00b7haus", "frank", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gleich 'mal vor\u00fcbergehen,", "tokens": ["Gleich", "'mal", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ohn' auch hinein zu sehen.", "tokens": ["Ohn'", "auch", "hin\u00b7ein", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als ich ging vorbei,", "tokens": ["Und", "als", "ich", "ging", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da ward das Geld lebendig,", "tokens": ["Da", "ward", "das", "Geld", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00fchlt' und stie\u00df und sprang", "tokens": ["Und", "w\u00fchlt'", "und", "stie\u00df", "und", "sprang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Umher so ganz unb\u00e4ndig,", "tokens": ["Um\u00b7her", "so", "ganz", "un\u00b7b\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als wollt' es auf der Stelle schier", "tokens": ["Als", "wollt'", "es", "auf", "der", "Stel\u00b7le", "schier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zermalmen alle Rippen mir,", "tokens": ["Zer\u00b7mal\u00b7men", "al\u00b7le", "Rip\u00b7pen", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis ich mich lie\u00df beth\u00f6ren,", "tokens": ["Bis", "ich", "mich", "lie\u00df", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In's Wirthshaus einzukehren.", "tokens": ["In's", "Wirths\u00b7haus", "ein\u00b7zu\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da fand das arge Geld", "tokens": ["Da", "fand", "das", "ar\u00b7ge", "Geld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bald seine gute Ruhe.", "tokens": ["Bald", "sei\u00b7ne", "gu\u00b7te", "Ru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun liegt der ganze Schatz", "tokens": ["Nun", "liegt", "der", "gan\u00b7ze", "Schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schon in des Schenken Truhe.", "tokens": ["Schon", "in", "des", "Schen\u00b7ken", "Tru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach, aber tief in meinem Bauch", "tokens": ["Ach", ",", "a\u00b7ber", "tief", "in", "mei\u00b7nem", "Bauch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da liegt das Gaul, die Thaler auch,", "tokens": ["Da", "liegt", "das", "Gaul", ",", "die", "Tha\u00b7ler", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und treiben's zum Erbarmen", "tokens": ["Und", "trei\u00b7ben's", "zum", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "APPRART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Noch immer mit mir Armen.", "tokens": ["Noch", "im\u00b7mer", "mit", "mir", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn ich ein Wirthshaus seh',", "tokens": ["Wenn", "ich", "ein", "Wirths\u00b7haus", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "F\u00e4ngt's in mir an zu toben,", "tokens": ["F\u00e4ngt's", "in", "mir", "an", "zu", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als wollt' es kehren gleich", "tokens": ["Als", "wollt'", "es", "keh\u00b7ren", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Unterste zu oben.", "tokens": ["Das", "Un\u00b7ters\u00b7te", "zu", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sprech' ich in dem Wirthshaus ein,", "tokens": ["Und", "sprech'", "ich", "in", "dem", "Wirths\u00b7haus", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wirth, der Schuft, giebt keinen Wein", "tokens": ["Der", "Wirth", ",", "der", "Schuft", ",", "giebt", "kei\u00b7nen", "Wein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr's Gaul und's Geld im Magen,", "tokens": ["F\u00fcr's", "Gaul", "un\u00b7d's", "Geld", "im", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So arg sie mich auch plagen.", "tokens": ["So", "arg", "sie", "mich", "auch", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}