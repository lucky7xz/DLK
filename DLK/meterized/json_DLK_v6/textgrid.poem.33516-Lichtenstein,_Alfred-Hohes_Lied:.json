{"textgrid.poem.33516": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Hohes Lied:", "genre": "verse", "period": "N.A.", "pub_year": 1902, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maria du \u2013 daran zu denken, wie", "tokens": ["Ma\u00b7ria", "du", "\u2013", "da\u00b7ran", "zu", "den\u00b7ken", ",", "wie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "NE", "$(", "PAV", "PTKZU", "VVINF", "$,", "PWAV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich dich empfand ... Der schwere Kopf versinkt \u2013", "tokens": ["Ich", "dich", "emp\u00b7fand", "...", "Der", "schwe\u00b7re", "Kopf", "ver\u00b7sinkt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Meer nur und Mond \u2013 Meermond und Wind und Welt \u2013", "tokens": ["Meer", "nur", "und", "Mond", "\u2013", "Meer\u00b7mond", "und", "Wind", "und", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "$(", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Um deine wei\u00dfe Haut der wei\u00dfe Sand, Maria \u2013", "tokens": ["Um", "dei\u00b7ne", "wei\u00b7\u00dfe", "Haut", "der", "wei\u00b7\u00dfe", "Sand", ",", "Ma\u00b7ria", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Haar ... Dein L\u00e4cheln ... Rings ist Meer und Not", "tokens": ["Dein", "Haar", "...", "Dein", "L\u00e4\u00b7cheln", "...", "Rings", "ist", "Meer", "und", "Not"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "NN", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Ruf und Sehnsucht und ein sanftes Gl\u00fcck \u2013", "tokens": ["Und", "Ruf", "und", "Sehn\u00b7sucht", "und", "ein", "sanf\u00b7tes", "Gl\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "All dieses Singen, das so m\u00fcde macht ...", "tokens": ["All", "die\u00b7ses", "Sin\u00b7gen", ",", "das", "so", "m\u00fc\u00b7de", "macht", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kommt nicht der Himmel wie ein Mutterlied", "tokens": ["Kommt", "nicht", "der", "Him\u00b7mel", "wie", "ein", "Mut\u00b7ter\u00b7lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zur Stirn des Kindes hin und hin zu uns \u2013", "tokens": ["Zur", "Stirn", "des", "Kin\u00b7des", "hin", "und", "hin", "zu", "uns", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "KON", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}