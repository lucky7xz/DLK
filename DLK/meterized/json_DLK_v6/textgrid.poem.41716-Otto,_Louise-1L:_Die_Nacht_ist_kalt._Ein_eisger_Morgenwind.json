{"textgrid.poem.41716": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nacht ist kalt. Ein eisger Morgenwind", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist kalt. Ein eisger Morgenwind", "tokens": ["Die", "Nacht", "ist", "kalt", ".", "Ein", "eis\u00b7ger", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Klirrt um die dichtgefrornen Fensterscheiben,", "tokens": ["Klirrt", "um", "die", "dicht\u00b7ge\u00b7fror\u00b7nen", "Fens\u00b7ter\u00b7schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als wollt mit starrem Hauchen er geschwind", "tokens": ["Als", "wollt", "mit", "star\u00b7rem", "Hau\u00b7chen", "er", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "APPR", "ADJA", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Blumen dran noch immer h\u00f6her treiben,", "tokens": ["Die", "Blu\u00b7men", "dran", "noch", "im\u00b7mer", "h\u00f6\u00b7her", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nur da\u00df es Blumen sind aus Eis und Frost,", "tokens": ["Nur", "da\u00df", "es", "Blu\u00b7men", "sind", "aus", "Eis", "und", "Frost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Um die verbuhlte Lieder heult der Ost.", "tokens": ["Um", "die", "ver\u00b7buhl\u00b7te", "Lie\u00b7der", "heult", "der", "Ost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gespenstig lacht das Feuer im Kamin.", "tokens": ["Ge\u00b7spens\u00b7tig", "lacht", "das", "Feu\u00b7er", "im", "Ka\u00b7min", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als hab im Zorn es eine Sprache funden,", "tokens": ["Als", "hab", "im", "Zorn", "es", "ei\u00b7ne", "Spra\u00b7che", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "APPRART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sterne, die am hohen Himmel ziehn,", "tokens": ["Die", "Ster\u00b7ne", ",", "die", "am", "ho\u00b7hen", "Him\u00b7mel", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie schimmern hell zu Tausenden verbunden,", "tokens": ["Sie", "schim\u00b7mern", "hell", "zu", "Tau\u00b7sen\u00b7den", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie glitzern golden leuchtend wie Krystall \u2013", "tokens": ["Sie", "glit\u00b7zern", "gol\u00b7den", "leuch\u00b7tend", "wie", "Krys\u00b7tall", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVPP", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In Eis und Schnee bespiegeln sie sich all. \u2013", "tokens": ["In", "Eis", "und", "Schnee", "be\u00b7spie\u00b7geln", "sie", "sich", "all", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PRF", "PIAT", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sitze einsam bei der Kerze Licht;", "tokens": ["Ich", "sit\u00b7ze", "ein\u00b7sam", "bei", "der", "Ker\u00b7ze", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Menschen rings sind schlafen schon gegangen,", "tokens": ["Die", "Men\u00b7schen", "rings", "sind", "schla\u00b7fen", "schon", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wach allein, ich mag die Ruhe nicht,", "tokens": ["Ich", "wach", "al\u00b7lein", ",", "ich", "mag", "die", "Ru\u00b7he", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es flieht der Schlaf, wenn Sorgen uns umfangen,", "tokens": ["Es", "flieht", "der", "Schlaf", ",", "wenn", "Sor\u00b7gen", "uns", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn sich ein Herz zum hei\u00dfen Kampfe st\u00e4hlt,", "tokens": ["Wenn", "sich", "ein", "Herz", "zum", "hei\u00b7\u00dfen", "Kamp\u00b7fe", "st\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr Menschenrecht und Freiheit still sich qu\u00e4lt.", "tokens": ["F\u00fcr", "Men\u00b7schen\u00b7recht", "und", "Frei\u00b7heit", "still", "sich", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch sieh, doch sieh, \u2013 ein L\u00e4mpchen traurig scheint", "tokens": ["Doch", "sieh", ",", "doch", "sieh", ",", "\u2013", "ein", "L\u00e4mp\u00b7chen", "trau\u00b7rig", "scheint"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "PTKVZ", "$,", "$(", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gegen\u00fcber in dem Fensterlein der H\u00fctte,", "tokens": ["Ge\u00b7gen\u00b7\u00fc\u00b7ber", "in", "dem", "Fens\u00b7ter\u00b7lein", "der", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Dort sitzt die Kl\u00f6pplerin noch wach und weint", "tokens": ["Dort", "sitzt", "die", "Kl\u00f6pp\u00b7le\u00b7rin", "noch", "wach", "und", "weint"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kl\u00f6ppelt m\u00fchsam nach der M\u00fctter Sitte.", "tokens": ["Und", "kl\u00f6p\u00b7pelt", "m\u00fch\u00b7sam", "nach", "der", "M\u00fct\u00b7ter", "Sit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und kl\u00f6ppelt emsig ohne Ruh' und Rast,", "tokens": ["Und", "kl\u00f6p\u00b7pelt", "em\u00b7sig", "oh\u00b7ne", "Ruh'", "und", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df ihre Wange immer mehr verbla\u00dft.", "tokens": ["Da\u00df", "ih\u00b7re", "Wan\u00b7ge", "im\u00b7mer", "mehr", "ver\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie kl\u00f6ppelt nicht f\u00fcr Mutter oder Kind,", "tokens": ["Sie", "kl\u00f6p\u00b7pelt", "nicht", "f\u00fcr", "Mut\u00b7ter", "o\u00b7der", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie kl\u00f6ppelt nur, da\u00df sie nicht selbst erfriere,", "tokens": ["Sie", "kl\u00f6p\u00b7pelt", "nur", ",", "da\u00df", "sie", "nicht", "selbst", "er\u00b7frie\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie sich ehrlich trocknes Brot gewinnt,", "tokens": ["Da\u00df", "sie", "sich", "ehr\u00b7lich", "trock\u00b7nes", "Brot", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr einzges Gut, die Unschuld, nicht verliere,", "tokens": ["Ihr", "einz\u00b7ges", "Gut", ",", "die", "Un\u00b7schuld", ",", "nicht", "ver\u00b7lie\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der l\u00e4ngst der reiche L\u00fcstling nachgestellt \u2013", "tokens": ["Der", "l\u00e4ngst", "der", "rei\u00b7che", "L\u00fcst\u00b7ling", "nach\u00b7ge\u00b7stellt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie kl\u00f6ppelt, da\u00df sie nicht vor Hunger \u2013 f\u00e4llt.", "tokens": ["Sie", "kl\u00f6p\u00b7pelt", ",", "da\u00df", "sie", "nicht", "vor", "Hun\u00b7ger", "\u2013", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und horch und horch! an dieser Nebenwand,", "tokens": ["Und", "horch", "und", "horch", "!", "an", "die\u00b7ser", "Ne\u00b7ben\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "KON", "VVIMP", "$.", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da klappert noch des Webers schnelle Spule,", "tokens": ["Da", "klap\u00b7pert", "noch", "des", "We\u00b7bers", "schnel\u00b7le", "Spu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie rastet nicht und mit geschickter Hand", "tokens": ["Sie", "ras\u00b7tet", "nicht", "und", "mit", "ge\u00b7schick\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Arbeitet er noch nachts am Webestuhle.", "tokens": ["Ar\u00b7bei\u00b7tet", "er", "noch", "nachts", "am", "We\u00b7be\u00b7stuh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das bleiche Weib, der Kinder blasse Schar,", "tokens": ["Das", "blei\u00b7che", "Weib", ",", "der", "Kin\u00b7der", "blas\u00b7se", "Schar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er sieht auf sie \u2013 und ist des Trostes bar;", "tokens": ["Er", "sieht", "auf", "sie", "\u2013", "und", "ist", "des", "Tros\u00b7tes", "bar", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$(", "KON", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Drum ist er wach, noch um die Mitternacht!", "tokens": ["Drum", "ist", "er", "wach", ",", "noch", "um", "die", "Mit\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PTKVZ", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie diese Mitternacht ist all sein Leben!", "tokens": ["Wie", "die\u00b7se", "Mit\u00b7ter\u00b7nacht", "ist", "all", "sein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er hat es ruh- und freudenlos verbracht,", "tokens": ["Er", "hat", "es", "ruh", "und", "freu\u00b7den\u00b7los", "ver\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "TRUNC", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er h\u00f6rte Tag und Nacht nicht auf zu weben,", "tokens": ["Er", "h\u00f6r\u00b7te", "Tag", "und", "Nacht", "nicht", "auf", "zu", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKNEG", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und kaum, da\u00df er erhielt den siechen Leib,", "tokens": ["Und", "kaum", ",", "da\u00df", "er", "er\u00b7hielt", "den", "sie\u00b7chen", "Leib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Elends Bildnis ist so Kind als Weib.", "tokens": ["Des", "E\u00b7lends", "Bild\u00b7nis", "ist", "so", "Kind", "als", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Es hat nicht not, da\u00df Ihr mich also mahnt,", "tokens": ["Es", "hat", "nicht", "not", ",", "da\u00df", "Ihr", "mich", "al\u00b7so", "mahnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du arme Schwester an den Kl\u00f6ppelkissen,", "tokens": ["Du", "ar\u00b7me", "Schwes\u00b7ter", "an", "den", "Kl\u00f6p\u00b7pel\u00b7kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du armer Bruder, der es schrecklich ahnt,", "tokens": ["Du", "ar\u00b7mer", "Bru\u00b7der", ",", "der", "es", "schreck\u00b7lich", "ahnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Euch das Recht zu leben fast entrissen!", "tokens": ["Da\u00df", "Euch", "das", "Recht", "zu", "le\u00b7ben", "fast", "ent\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKZU", "VVINF", "ADV", "VVINF", "$."], "