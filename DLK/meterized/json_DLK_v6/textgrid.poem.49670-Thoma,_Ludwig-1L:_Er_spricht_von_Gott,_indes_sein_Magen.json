{"textgrid.poem.49670": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er spricht von Gott, indes sein Magen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er spricht von Gott, indes sein Magen", "tokens": ["Er", "spricht", "von", "Gott", ",", "in\u00b7des", "sein", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch etwas s\u00e4uert von Bordeaux,", "tokens": ["Noch", "et\u00b7was", "s\u00e4u\u00b7ert", "von", "Bor\u00b7dea\u00b7ux", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er wei\u00df von Jesus was zu sagen, \u2013", "tokens": ["Er", "wei\u00df", "von", "Je\u00b7sus", "was", "zu", "sa\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "PIS", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allein der Heiland roch nicht so.", "tokens": ["Al\u00b7lein", "der", "Hei\u00b7land", "roch", "nicht", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er stochert dann aus seinen Z\u00e4hnen", "tokens": ["Er", "sto\u00b7chert", "dann", "aus", "sei\u00b7nen", "Z\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die letzten Reste Kaviar", "tokens": ["Die", "letz\u00b7ten", "Res\u00b7te", "Ka\u00b7vi\u00b7ar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spricht mit unterdr\u00fcckten Tr\u00e4nen", "tokens": ["Und", "spricht", "mit", "un\u00b7ter\u00b7dr\u00fcck\u00b7ten", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von seines Vaterlands Gefahr.", "tokens": ["Von", "sei\u00b7nes", "Va\u00b7ter\u00b7lands", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von Leuten, die das Volk betrogen", "tokens": ["Von", "Leu\u00b7ten", ",", "die", "das", "Volk", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um seinen kindlich treuen Sinn, \u2013", "tokens": ["Um", "sei\u00b7nen", "kind\u00b7lich", "treu\u00b7en", "Sinn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PPOSAT", "ADJD", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da blitzt es in den Karpfenoogen,", "tokens": ["Da", "blitzt", "es", "in", "den", "Karp\u00b7fen\u00b7oo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da zittert manches Doppelkinn.", "tokens": ["Da", "zit\u00b7tert", "man\u00b7ches", "Dop\u00b7pel\u00b7kinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Um seinen Kaiser tiefe Schmerzen,", "tokens": ["Um", "sei\u00b7nen", "Kai\u00b7ser", "tie\u00b7fe", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ums Vaterland ein arges Weh,", "tokens": ["Ums", "Va\u00b7ter\u00b7land", "ein", "ar\u00b7ges", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie brennen in des Adels Herzen.", "tokens": ["Sie", "bren\u00b7nen", "in", "des", "A\u00b7dels", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Maule brennt die Henry Clay.", "tokens": ["Im", "Mau\u00b7le", "brennt", "die", "Hen\u00b7ry", "Clay", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und seht nur die Gesichter bl\u00fchen", "tokens": ["Und", "seht", "nur", "die", "Ge\u00b7sich\u00b7ter", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Rot und Blau und Violett,", "tokens": ["In", "Rot", "und", "Blau", "und", "Vi\u00b7o\u00b7lett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sehet jedes Antlitz gl\u00fchen", "tokens": ["Und", "se\u00b7het", "je\u00b7des", "Ant\u00b7litz", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Pathos und von Schweinefett.", "tokens": ["Von", "Pa\u00b7thos", "und", "von", "Schwei\u00b7ne\u00b7fett", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}