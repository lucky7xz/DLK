{"textgrid.poem.65120": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es giebt zwei selige Gef\u00fchle,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es giebt zwei selige Gef\u00fchle,", "tokens": ["Es", "giebt", "zwei", "se\u00b7li\u00b7ge", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die unser Herz erst dann erfa\u00dft,", "tokens": ["Die", "un\u00b7ser", "Herz", "erst", "dann", "er\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn in des Lebensmittags Schw\u00fcle", "tokens": ["Wenn", "in", "des", "Le\u00b7bens\u00b7mit\u00b7tags", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Morgens duft'ger Reiz verbla\u00dft.", "tokens": ["Des", "Mor\u00b7gens", "duft'\u00b7ger", "Reiz", "ver\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann kommen jene Engel beide,", "tokens": ["Dann", "kom\u00b7men", "je\u00b7ne", "En\u00b7gel", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Gott zu unserm Trost bestellt:", "tokens": ["Die", "Gott", "zu", "un\u00b7serm", "Trost", "be\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An der Natur die heil'ge Freude,", "tokens": ["An", "der", "Na\u00b7tur", "die", "heil'\u00b7ge", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Liebe zu der Kinderwelt.", "tokens": ["Die", "Lie\u00b7be", "zu", "der", "Kin\u00b7der\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So lang auf sturmdurchfurchter Welle", "tokens": ["So", "lang", "auf", "sturm\u00b7durch\u00b7furch\u00b7ter", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Leidenschaft die Seele schwankt,", "tokens": ["Der", "Lei\u00b7den\u00b7schaft", "die", "See\u00b7le", "schwankt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Erde Lust, die Qual der H\u00f6lle", "tokens": ["Der", "Er\u00b7de", "Lust", ",", "die", "Qual", "der", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um unser Inn'res wild sich zankt,", "tokens": ["Um", "un\u00b7ser", "Inn'\u00b7res", "wild", "sich", "zankt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In brennenden Verlangens Grimme", "tokens": ["In", "bren\u00b7nen\u00b7den", "Ver\u00b7lan\u00b7gens", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Jugend ihre Schlachten ficht,", "tokens": ["Die", "Ju\u00b7gend", "ih\u00b7re", "Schlach\u00b7ten", "ficht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So lang dringt ihre sanfte Stimme", "tokens": ["So", "lang", "dringt", "ih\u00b7re", "sanf\u00b7te", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durch des Orkanes Toben nicht.", "tokens": ["Durch", "des", "Or\u00b7ka\u00b7nes", "To\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Erst, wenn der W\u00fcnsche wilde Horden", "tokens": ["Erst", ",", "wenn", "der", "W\u00fcn\u00b7sche", "wil\u00b7de", "Hor\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entmuthigt flieh'n, besiegt und bleich,", "tokens": ["Ent\u00b7mu\u00b7thigt", "flieh'n", ",", "be\u00b7siegt", "und", "bleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erst, wenn es still in uns geworden,", "tokens": ["Erst", ",", "wenn", "es", "still", "in", "uns", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beginnet jener Engel Reich:", "tokens": ["Be\u00b7gin\u00b7net", "je\u00b7ner", "En\u00b7gel", "Reich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Christus einst den Armen, dienen", "tokens": ["Wie", "Chris\u00b7tus", "einst", "den", "Ar\u00b7men", ",", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "NE", "ADV", "ART", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie mild des Gl\u00fccks verstoss'nem Sohn,", "tokens": ["Sie", "mild", "des", "Gl\u00fccks", "ver\u00b7sto\u00b7ss'\u00b7nem", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und bau'n auf rauchenden Ruinen", "tokens": ["Und", "bau'n", "auf", "rau\u00b7chen\u00b7den", "Ru\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm einen neuen Freudenthron.", "tokens": ["Ihm", "ei\u00b7nen", "neu\u00b7en", "Freu\u00b7den\u00b7thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Tief selige Mysterien k\u00fcnden", "tokens": ["Tief", "se\u00b7li\u00b7ge", "Mys\u00b7te\u00b7ri\u00b7en", "k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Im Rauschen sie des Abendwinds,", "tokens": ["Im", "Rau\u00b7schen", "sie", "des", "Ab\u00b7end\u00b7winds", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Trostes lichten Strahl entz\u00fcnden", "tokens": ["Des", "Tros\u00b7tes", "lich\u00b7ten", "Strahl", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie in dem gro\u00dfen Aug' des Kinds,", "tokens": ["Sie", "in", "dem", "gro\u00b7\u00dfen", "Aug'", "des", "Kinds", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Herz, das sich in banger Scheue", "tokens": ["Das", "Herz", ",", "das", "sich", "in", "ban\u00b7ger", "Scheu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verschlo\u00df, von Bitterkeit geschwellt,", "tokens": ["Ver\u00b7schlo\u00df", ",", "von", "Bit\u00b7ter\u00b7keit", "ge\u00b7schwellt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie kn\u00fcpfen liebend es aufs neue", "tokens": ["Sie", "kn\u00fcp\u00b7fen", "lie\u00b7bend", "es", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An Gott und seine sch\u00f6ne Welt.", "tokens": ["An", "Gott", "und", "sei\u00b7ne", "sch\u00f6\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wer sich ihnen hingegeben,", "tokens": ["Und", "wer", "sich", "ih\u00b7nen", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer sich zu ihren Treuen schwur,", "tokens": ["Wer", "sich", "zu", "ih\u00b7ren", "Treu\u00b7en", "schwur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der lebt ein tausendfaches Leben", "tokens": ["Der", "lebt", "ein", "tau\u00b7send\u00b7fa\u00b7ches", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Keimen aller Creatur!", "tokens": ["Im", "Kei\u00b7men", "al\u00b7ler", "Crea\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der Winter, der mit starren Banden", "tokens": ["Der", "Win\u00b7ter", ",", "der", "mit", "star\u00b7ren", "Ban\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den schwerbedr\u00e4ngten Sinn umeis't,", "tokens": ["Den", "schwer\u00b7be\u00b7dr\u00e4ng\u00b7ten", "Sinn", "um\u00b7eis't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er hat ihn siegreich \u00fcberstanden", "tokens": ["Er", "hat", "ihn", "sieg\u00b7reich", "\u00fc\u00b7bers\u00b7tan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Fr\u00fchlingsd\u00fcfte trinkt sein Geist!", "tokens": ["Und", "Fr\u00fch\u00b7lings\u00b7d\u00fcf\u00b7te", "trinkt", "sein", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}