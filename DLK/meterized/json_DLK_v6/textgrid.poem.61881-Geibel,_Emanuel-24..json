{"textgrid.poem.61881": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun um deine Pfade leis", "tokens": ["Nun", "um", "dei\u00b7ne", "Pfa\u00b7de", "leis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welke Bl\u00e4tter stieben,", "tokens": ["Wel\u00b7ke", "Bl\u00e4t\u00b7ter", "stie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eng und enger wird der Kreis", "tokens": ["Eng", "und", "en\u00b7ger", "wird", "der", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4glich deiner Lieben.", "tokens": ["T\u00e4g\u00b7lich", "dei\u00b7ner", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die im Jugendmorgenrot", "tokens": ["Die", "im", "Ju\u00b7gend\u00b7mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir Geleit gegeben,", "tokens": ["Dir", "Ge\u00b7leit", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, wie viele nahm der Tod,", "tokens": ["Ach", ",", "wie", "vie\u00b7le", "nahm", "der", "Tod", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie viel mehr das Leben!", "tokens": ["Wie", "viel", "mehr", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Neue Freundschaft schlie\u00dft sich schwer", "tokens": ["Neu\u00b7e", "Freund\u00b7schaft", "schlie\u00dft", "sich", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An des Winters Grenze,", "tokens": ["An", "des", "Win\u00b7ters", "Gren\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wurzeln treibt das Herz nicht mehr,", "tokens": ["Wur\u00b7zeln", "treibt", "das", "Herz", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dereinst im Lenze.", "tokens": ["Wie", "de\u00b7reinst", "im", "Len\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Zwar im Kampf nicht wird es dir", "tokens": ["Zwar", "im", "Kampf", "nicht", "wird", "es", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PTKNEG", "VAFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An Genossen fehlen,", "tokens": ["An", "Ge\u00b7nos\u00b7sen", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch euch kn\u00fcpft ein gleich Panier,", "tokens": ["Doch", "euch", "kn\u00fcpft", "ein", "gleich", "Pa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht der Zug der Seelen.", "tokens": ["Nicht", "der", "Zug", "der", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Auch mit J\u00fcngren wohl ein St\u00fcck", "tokens": ["Auch", "mit", "J\u00fcng\u00b7ren", "wohl", "ein", "St\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft sich's fr\u00f6hlich schweifen,", "tokens": ["L\u00e4\u00dft", "sich's", "fr\u00f6h\u00b7lich", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch nur halb dein Leid und Gl\u00fcck", "tokens": ["Doch", "nur", "halb", "dein", "Leid", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6gen sie begreifen.", "tokens": ["M\u00f6\u00b7gen", "sie", "be\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Darum, soll nicht freudenarm", "tokens": ["Da\u00b7rum", ",", "soll", "nicht", "freu\u00b7den\u00b7arm"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "VMFIN", "PTKNEG", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir die Welt verblassen,", "tokens": ["Dir", "die", "Welt", "ver\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lern' in Liebe doppelt warm,", "tokens": ["Lern'", "in", "Lie\u00b7be", "dop\u00b7pelt", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dir blieb, umfassen.", "tokens": ["Was", "dir", "blieb", ",", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Den du jung umhergestreut", "tokens": ["Den", "du", "jung", "um\u00b7her\u00b7ge\u00b7streut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leicht in leichten Gaben,", "tokens": ["Leicht", "in", "leich\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "La\u00df an deinem Schatz sich heut", "tokens": ["La\u00df", "an", "dei\u00b7nem", "Schatz", "sich", "heut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PRF", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wen'ge ganz erlaben.", "tokens": ["Wen'\u00b7ge", "ganz", "er\u00b7la\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Eisumfrornem Rebensaft", "tokens": ["Ei\u00b7sum\u00b7fror\u00b7nem", "Re\u00b7ben\u00b7saft"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleiche, der zusammen-", "tokens": ["Glei\u00b7che", ",", "der", "zu\u00b7sam\u00b7men"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "TRUNC"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dr\u00e4ngt im engsten Raum die Kraft", "tokens": ["Dr\u00e4ngt", "im", "engs\u00b7ten", "Raum", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller seiner Flammen.", "tokens": ["Al\u00b7ler", "sei\u00b7ner", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}