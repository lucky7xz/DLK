{"dta.poem.12846": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Sylvien, warum er nicht mehr  \n so fertig dichten k\u00f6nne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es kommt dir fremde vor, da\u00df ich mit meinen versen,", "tokens": ["Es", "kommt", "dir", "frem\u00b7de", "vor", ",", "da\u00df", "ich", "mit", "mei\u00b7nen", "ver\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sonst geschwinde gehn, itzund gar langsam bin.", "tokens": ["Die", "sonst", "ge\u00b7schwin\u00b7de", "gehn", ",", "it\u00b7zund", "gar", "lang\u00b7sam", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "VVINF", "$,", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Musen wollen nicht best\u00e4ndig in mir herrschen,", "tokens": ["Die", "Mu\u00b7sen", "wol\u00b7len", "nicht", "be\u00b7st\u00e4n\u00b7dig", "in", "mir", "herr\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es weicht ihr hei\u00dfer zug mit deinen augen hin.", "tokens": ["Es", "weicht", "ihr", "hei\u00b7\u00dfer", "zug", "mit", "dei\u00b7nen", "au\u00b7gen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich habe, wie es scheint, mich g\u00e4ntzlich ausgeleeret,", "tokens": ["Ich", "ha\u00b7be", ",", "wie", "es", "scheint", ",", "mich", "g\u00e4ntz\u00b7lich", "aus\u00b7ge\u00b7lee\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wenn ich dichten will, so fehlt es \u00fcberall.", "tokens": ["Und", "wenn", "ich", "dich\u00b7ten", "will", ",", "so", "fehlt", "es", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verdru\u00df und einsamkeit hat alle krafft verzehret,", "tokens": ["Ver\u00b7dru\u00df", "und", "ein\u00b7sam\u00b7keit", "hat", "al\u00b7le", "krafft", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und meine poesie giebt schon das todten-mahl.", "tokens": ["Und", "mei\u00b7ne", "po\u00b7e\u00b7sie", "giebt", "schon", "das", "tod\u00b7ten\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Denn ob ich dann und wann gleich ein paar reime dichte,", "tokens": ["Denn", "ob", "ich", "dann", "und", "wann", "gleich", "ein", "paar", "rei\u00b7me", "dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "KON", "PWAV", "ADV", "ART", "PIAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So fehlet ihnen doch die alte liebligkeit:", "tokens": ["So", "feh\u00b7let", "ih\u00b7nen", "doch", "die", "al\u00b7te", "lieb\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der frische lorbeerbaum, wird eine d\u00fcrre sichte,", "tokens": ["Der", "fri\u00b7sche", "lor\u00b7beer\u00b7baum", ",", "wird", "ei\u00b7ne", "d\u00fcr\u00b7re", "sich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und der erhitzte geist bald in die lufft zerstreut.", "tokens": ["Und", "der", "er\u00b7hitz\u00b7te", "geist", "bald", "in", "die", "lufft", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das macht, da\u00df ich nicht mehr aus diesen brunnen trincke,", "tokens": ["Das", "macht", ",", "da\u00df", "ich", "nicht", "mehr", "aus", "die\u00b7sen", "brun\u00b7nen", "trin\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die itzund in der welt der dichter st\u00e4rcke sind,", "tokens": ["Die", "it\u00b7zund", "in", "der", "welt", "der", "dich\u00b7ter", "st\u00e4r\u00b7cke", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und da\u00df ich gantz umsonst den s\u00fc\u00dfen sternen wincke,", "tokens": ["Und", "da\u00df", "ich", "gantz", "um\u00b7sonst", "den", "s\u00fc\u00b7\u00dfen", "ster\u00b7nen", "win\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ART", "ADJA", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Von denen thau und krafft in haupt und adern rinnt.", "tokens": ["Von", "de\u00b7nen", "thau", "und", "krafft", "in", "haupt", "und", "a\u00b7dern", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "KON", "NN", "APPR", "NN", "KON", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Musen wollen sich nicht meiner mehr bedienen,", "tokens": ["Die", "Mu\u00b7sen", "wol\u00b7len", "sich", "nicht", "mei\u00b7ner", "mehr", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "PTKNEG", "PPOSAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sie wenden ihre gunst weit edlern schwanen zu.", "tokens": ["Sie", "wen\u00b7den", "ih\u00b7re", "gunst", "weit", "ed\u00b7lern", "schwa\u00b7nen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich darff mich nicht, wie vor, auf den Parna\u00df erk\u00fchnen,", "tokens": ["Ich", "darff", "mich", "nicht", ",", "wie", "vor", ",", "auf", "den", "Par\u00b7na\u00df", "er\u00b7k\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$,", "PWAV", "PTKVZ", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Und finde kaum zur noth bey seiner wurtzel ruh.", "tokens": ["Und", "fin\u00b7de", "kaum", "zur", "noth", "bey", "sei\u00b7ner", "wurt\u00b7zel", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein einsames revier, wo turteltauben \u00e4chzen,", "tokens": ["Mein", "ein\u00b7sa\u00b7mes", "re\u00b7vier", ",", "wo", "tur\u00b7tel\u00b7tau\u00b7ben", "\u00e4ch\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und keine nachtigaln den tr\u00fcben sinn erfreun,", "tokens": ["Und", "kei\u00b7ne", "nach\u00b7ti\u00b7galn", "den", "tr\u00fc\u00b7ben", "sinn", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Macht zwar, da\u00df hertz und mund offt nach erqvickung lechzen,", "tokens": ["Macht", "zwar", ",", "da\u00df", "hertz", "und", "mund", "offt", "nach", "er\u00b7qvic\u00b7kung", "lech\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "NN", "KON", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Doch giebt es mir nicht viel von sch\u00f6nen liedern ein.", "tokens": ["Doch", "giebt", "es", "mir", "nicht", "viel", "von", "sch\u00f6\u00b7nen", "lie\u00b7dern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wer lauter tholen h\u00f6rt, schreyt endlich auch wie tholen,", "tokens": ["Wer", "lau\u00b7ter", "tho\u00b7len", "h\u00f6rt", ",", "schreyt", "end\u00b7lich", "auch", "wie", "tho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und wen kein muntrer trieb aus seinem schlaf erweckt,", "tokens": ["Und", "wen", "kein", "mun\u00b7trer", "trieb", "aus", "sei\u00b7nem", "schlaf", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIAT", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der sucht gewi\u00df sein licht bey ausgel\u00f6schten kohlen,", "tokens": ["Der", "sucht", "ge\u00b7wi\u00df", "sein", "licht", "bey", "aus\u00b7ge\u00b7l\u00f6schten", "koh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Und macht kein s\u00fc\u00dfes lied, das zarten geistern schmeckt.", "tokens": ["Und", "macht", "kein", "s\u00fc\u00b7\u00dfes", "lied", ",", "das", "zar\u00b7ten", "geis\u00b7tern", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Will aber Sylvia mich gleichwol singen h\u00f6ren,", "tokens": ["Will", "a\u00b7ber", "Syl\u00b7via", "mich", "gleich\u00b7wol", "sin\u00b7gen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Und meine muse sehn, wie sie vor diesem war;", "tokens": ["Und", "mei\u00b7ne", "mu\u00b7se", "sehn", ",", "wie", "sie", "vor", "die\u00b7sem", "war", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,", "PWAV", "PPER", "APPR", "PDAT", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So mu\u00df sie mich fein offt mit ihren blicken ehren,", "tokens": ["So", "mu\u00df", "sie", "mich", "fein", "offt", "mit", "ih\u00b7ren", "bli\u00b7cken", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn solcher strahlen glantz macht tr\u00fcbe sinnen klar.", "tokens": ["Denn", "sol\u00b7cher", "strah\u00b7len", "glantz", "macht", "tr\u00fc\u00b7be", "sin\u00b7nen", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sobald als ich dich seh\u2019, und deiner augen sonnen", "tokens": ["So\u00b7bald", "als", "ich", "dich", "seh'", ",", "und", "dei\u00b7ner", "au\u00b7gen", "son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PRF", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mir im gesichte stehn, so stehn die geister auf,", "tokens": ["Mir", "im", "ge\u00b7sich\u00b7te", "stehn", ",", "so", "stehn", "die", "geis\u00b7ter", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "VVINF", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der schon erstorbne trieb hat wiederum gewonnen,", "tokens": ["Der", "schon", "ers\u00b7torb\u00b7ne", "trieb", "hat", "wie\u00b7de\u00b7rum", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "VVFIN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und meine poesie beginnt den ersten lauf.", "tokens": ["Und", "mei\u00b7ne", "po\u00b7e\u00b7sie", "be\u00b7ginnt", "den", "ers\u00b7ten", "lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Gleichwie ein nelcken-knopff, wenn er im schatten stecket,", "tokens": ["Gleich\u00b7wie", "ein", "nel\u00b7cken\u00b7knopff", ",", "wenn", "er", "im", "schat\u00b7ten", "ste\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Geringe kr\u00e4ffte zeigt und zugeschlossen steht,", "tokens": ["Ge\u00b7rin\u00b7ge", "kr\u00e4ff\u00b7te", "zeigt", "und", "zu\u00b7ge\u00b7schlos\u00b7sen", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVFIN", "KON", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dann aber offen wird und seine bl\u00e4tter strecket,", "tokens": ["Dann", "a\u00b7ber", "of\u00b7fen", "wird", "und", "sei\u00b7ne", "bl\u00e4t\u00b7ter", "stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn das gew\u00fcnschte licht sich \u00fcber ihn erh\u00f6ht;", "tokens": ["Wenn", "das", "ge\u00b7w\u00fcnschte", "licht", "sich", "\u00fc\u00b7ber", "ihn", "er\u00b7h\u00f6ht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "So weckt auch deine gunst mein schl\u00e4friges gem\u00fcthe", "tokens": ["So", "weckt", "auch", "dei\u00b7ne", "gunst", "mein", "schl\u00e4f\u00b7ri\u00b7ges", "ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Durch ihre strahlen auf, und giebt den sinnen krafft:", "tokens": ["Durch", "ih\u00b7re", "strah\u00b7len", "auf", ",", "und", "giebt", "den", "sin\u00b7nen", "krafft", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Sie r\u00fchret und erhitzt das laugsame gebl\u00fcte,", "tokens": ["Sie", "r\u00fch\u00b7ret", "und", "er\u00b7hitzt", "das", "laug\u00b7sa\u00b7me", "ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Du glaubest nimmermehr, wieviel dein anblick schafft.", "tokens": ["Du", "glau\u00b7best", "nim\u00b7mer\u00b7mehr", ",", "wie\u00b7viel", "dein", "an\u00b7blick", "schafft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Drum, Edle Sylvia! soll ich so fertig dichten,", "tokens": ["Drum", ",", "Ed\u00b7le", "Syl\u00b7via", "!", "soll", "ich", "so", "fer\u00b7tig", "dich\u00b7ten", ","], "token_info": [