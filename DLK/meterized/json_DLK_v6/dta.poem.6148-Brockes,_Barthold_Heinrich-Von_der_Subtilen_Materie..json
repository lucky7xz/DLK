{"dta.poem.6148": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Subtilen Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint derselbe Stoff, als ob er selbst das Leere", "tokens": ["Es", "scheint", "der\u00b7sel\u00b7be", "Stoff", ",", "als", "ob", "er", "selbst", "das", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "KOKOM", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Recht eigentlich und w\u00fcrcklich w\u00e4re.", "tokens": ["Recht", "ei\u00b7gent\u00b7lich", "und", "w\u00fcrck\u00b7lich", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "VAFIN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Er flieht, er eilt davon, so bald ein C\u00f6rper ihn,", "tokens": ["Er", "flieht", ",", "er", "eilt", "da\u00b7von", ",", "so", "bald", "ein", "C\u00f6r\u00b7per", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PAV", "$,", "ADV", "ADV", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er einen C\u00f6rper trifft. Mit grosser Fertigkeit", "tokens": ["Er", "ei\u00b7nen", "C\u00f6r\u00b7per", "trifft", ".", "Mit", "gros\u00b7ser", "Fer\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Thut er sich ihnen auf, und l\u00e4sst sie allezeit,", "tokens": ["Thut", "er", "sich", "ih\u00b7nen", "auf", ",", "und", "l\u00e4sst", "sie", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ohn\u2019 alle Hinderung, sich regen, eilen, fliehn.", "tokens": ["Ohn'", "al\u00b7le", "Hin\u00b7de\u00b7rung", ",", "sich", "re\u00b7gen", ",", "ei\u00b7len", ",", "fliehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRF", "ADJA", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}