{"textgrid.poem.44424": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was schiltst du mich? Und wenn auch noch so leise,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schiltst du mich? Und wenn auch noch so leise,", "tokens": ["Was", "schiltst", "du", "mich", "?", "Und", "wenn", "auch", "noch", "so", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$.", "KON", "KOUS", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn auch noch so sch\u00f6n in Ton und Wort,", "tokens": ["Und", "wenn", "auch", "noch", "so", "sch\u00f6n", "in", "Ton", "und", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch schiltst du mich und tadelst meine Gleise,", "tokens": ["Doch", "schiltst", "du", "mich", "und", "ta\u00b7delst", "mei\u00b7ne", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und w\u00fcnschtest mich an einen andern Ort.", "tokens": ["Und", "w\u00fcnschtest", "mich", "an", "ei\u00b7nen", "an\u00b7dern", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Allein zugleich so freundlich ist die Weise,", "tokens": ["Al\u00b7lein", "zu\u00b7gleich", "so", "freund\u00b7lich", "ist", "die", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df sie den Geist mir zieht, den Willen fort,", "tokens": ["Da\u00df", "sie", "den", "Geist", "mir", "zieht", ",", "den", "Wil\u00b7len", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und, was sonst l\u00e4stig mir in Red und Liedern,", "tokens": ["Und", ",", "was", "sonst", "l\u00e4s\u00b7tig", "mir", "in", "Red", "und", "Lie\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADV", "ADJD", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich f\u00fchle mich gedr\u00e4ngt, dir zu erwidern.", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "ge\u00b7dr\u00e4ngt", ",", "dir", "zu", "er\u00b7wi\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es rinnt der Bach, wie schlammig die Gestade,", "tokens": ["Es", "rinnt", "der", "Bach", ",", "wie", "schlam\u00b7mig", "die", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein der sch\u00f6pft, pr\u00fcft wohl, was er erh\u00e4lt;", "tokens": ["Al\u00b7lein", "der", "sch\u00f6pft", ",", "pr\u00fcft", "wohl", ",", "was", "er", "er\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "$,", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Waldbaum streut den Samen auf die Pfade,", "tokens": ["Der", "Wald\u00b7baum", "streut", "den", "Sa\u00b7men", "auf", "die", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Ackersmann sucht ein gepfl\u00fcgtes Feld;", "tokens": ["Der", "A\u00b7ckers\u00b7mann", "sucht", "ein", "ge\u00b7pfl\u00fcg\u00b7tes", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der dunkle Trieb strebt, da\u00df er sich entlade,", "tokens": ["Der", "dunk\u00b7le", "Trieb", "strebt", ",", "da\u00df", "er", "sich", "ent\u00b7la\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein zwingend Mu\u00df ist ihm als Ziel gestellt;", "tokens": ["Ein", "zwin\u00b7gend", "Mu\u00df", "ist", "ihm", "als", "Ziel", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "VAFIN", "PPER", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.7": {"text": "Der Menschengeist, in sonnigern Bezirken,", "tokens": ["Der", "Men\u00b7schen\u00b7geist", ",", "in", "son\u00b7ni\u00b7gern", "Be\u00b7zir\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Will nicht nur t\u00e4tig sein, er will bewirken.", "tokens": ["Will", "nicht", "nur", "t\u00e4\u00b7tig", "sein", ",", "er", "will", "be\u00b7wir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Glaubst du, des Liedes Ahn, der M\u00e4onide,", "tokens": ["Glaubst", "du", ",", "des", "Lie\u00b7des", "Ahn", ",", "der", "M\u00e4o\u00b7ni\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er sang den Winden seine Rhythmen vor?", "tokens": ["Er", "sang", "den", "Win\u00b7den", "sei\u00b7ne", "Rhyth\u00b7men", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ihm zun\u00e4chst k\u00f6mmt im erhabnen Liede,", "tokens": ["Der", "ihm", "zu\u00b7n\u00e4chst", "k\u00f6mmt", "im", "er\u00b7hab\u00b7nen", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sah still geneigt der Briten stolzes Ohr;", "tokens": ["Sah", "still", "ge\u00b7neigt", "der", "Bri\u00b7ten", "stol\u00b7zes", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Tasson, Goethen, wenn vom Schaffen m\u00fcde,", "tokens": ["Und", "Tas\u00b7son", ",", "Goe\u00b7then", ",", "wenn", "vom", "Schaf\u00b7fen", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NE", "$,", "KOUS", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "H\u00f6rt zu Amalia, lauscht Leonor.", "tokens": ["H\u00f6rt", "zu", "A\u00b7ma\u00b7lia", ",", "lauscht", "Le\u00b7o\u00b7nor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,", "VVFIN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Die Welt ist da, weil Menschen sind, die sehen;", "tokens": ["Die", "Welt", "ist", "da", ",", "weil", "Men\u00b7schen", "sind", ",", "die", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "NN", "VAFIN", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was niemand wei\u00df, ist niemand auch geschehen.", "tokens": ["Was", "nie\u00b7mand", "wei\u00df", ",", "ist", "nie\u00b7mand", "auch", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es war die Zeit, da noch im Heiligtume", "tokens": ["Es", "war", "die", "Zeit", ",", "da", "noch", "im", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Germania gern den eignen Sohn empfing,", "tokens": ["Ger\u00b7ma\u00b7nia", "gern", "den", "eig\u00b7nen", "Sohn", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da Jung und Alt umherstand um die Blume,", "tokens": ["Da", "Jung", "und", "Alt", "um\u00b7her\u00b7stand", "um", "die", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die frisch hervor aus H\u00f6ltys Garten ging,", "tokens": ["Die", "frisch", "her\u00b7vor", "aus", "H\u00f6l\u00b7tys", "Gar\u00b7ten", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKVZ", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Strengen Hand, so schwer erborgtem Ruhme,", "tokens": ["Des", "Stren\u00b7gen", "Hand", ",", "so", "schwer", "er\u00b7borg\u00b7tem", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Leichtmahnend nur ob Wei\u00dfens Haupte hing,", "tokens": ["Leicht\u00b7mah\u00b7nend", "nur", "ob", "Wei\u00b7\u00dfens", "Haup\u00b7te", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KOUS", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da der Genu\u00df noch froh war zu genie\u00dfen,", "tokens": ["Da", "der", "Ge\u00b7nu\u00df", "noch", "froh", "war", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Aug bereit, ins Anschaun zu zerflie\u00dfen.", "tokens": ["Das", "Aug", "be\u00b7reit", ",", "ins", "An\u00b7schaun", "zu", "zer\u00b7flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Allein da kam das Paar der Herben, D\u00fcstern,", "tokens": ["Al\u00b7lein", "da", "kam", "das", "Paar", "der", "Her\u00b7ben", ",", "D\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zwar Br\u00fcder, doch in ", "tokens": ["Zwar", "Br\u00fc\u00b7der", ",", "doch", "in"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$,", "ADV", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Ersten sie der Zweiten, aber l\u00fcstern", "tokens": ["Die", "Ers\u00b7ten", "sie", "der", "Zwei\u00b7ten", ",", "a\u00b7ber", "l\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "ART", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach h\u00f6herm Ruhm, der Vordersten Bereich;", "tokens": ["Nach", "h\u00f6\u00b7herm", "Ruhm", ",", "der", "Vor\u00b7ders\u00b7ten", "Be\u00b7reich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.5": {"text": "Und da die eigne Tat nur leises Fl\u00fcstern,", "tokens": ["Und", "da", "die", "eig\u00b7ne", "Tat", "nur", "lei\u00b7ses", "Fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht Jubelruf erweckt und Glockenstreich,", "tokens": ["Nicht", "Ju\u00b7bel\u00b7ruf", "er\u00b7weckt", "und", "Glo\u00b7cken\u00b7streich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da alle Tempel andern schon geh\u00f6ren,", "tokens": ["Da", "al\u00b7le", "Tem\u00b7pel", "an\u00b7dern", "schon", "ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJA", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "D\u00fcnkts ihnen gut, statt bauen zu zerst\u00f6ren.", "tokens": ["D\u00fcnkts", "ih\u00b7nen", "gut", ",", "statt", "bau\u00b7en", "zu", "zer\u00b7st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "$,", "KOUI", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und Schanzen bilden sie von luftgen Worten,", "tokens": ["Und", "Schan\u00b7zen", "bil\u00b7den", "sie", "von", "luft\u00b7gen", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mi\u00dfbrauchter Scharfsinn beut die Waffen dar;", "tokens": ["Mi\u00df\u00b7brauch\u00b7ter", "Scharf\u00b7sinn", "beut", "die", "Waf\u00b7fen", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was wahr, beschr\u00e4nkt auf Zeiten und an Orten,", "tokens": ["Was", "wahr", ",", "be\u00b7schr\u00e4nkt", "auf", "Zei\u00b7ten", "und", "an", "Or\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird ausgedehnt und aller Zukunft wahr.", "tokens": ["Wird", "aus\u00b7ge\u00b7dehnt", "und", "al\u00b7ler", "Zu\u00b7kunft", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Ahnung Lauschen an der Geister Pforten", "tokens": ["Der", "Ah\u00b7nung", "Lau\u00b7schen", "an", "der", "Geis\u00b7ter", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist ihnen wie des Dreiecks Winkel klar,", "tokens": ["Ist", "ih\u00b7nen", "wie", "des", "Drei\u00b7ecks", "Win\u00b7kel", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKO