{"dta.poem.10463": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Xx.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Still ist die Nacht, es ruhen die Gassen,", "tokens": ["Still", "ist", "die", "Nacht", ",", "es", "ru\u00b7hen", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In diesem Hause wohnte mein Schatz;", "tokens": ["In", "die\u00b7sem", "Hau\u00b7se", "wohn\u00b7te", "mein", "Schatz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sie hat schon l\u00e4ngst die Stadt verlassen,", "tokens": ["Sie", "hat", "schon", "l\u00e4ngst", "die", "Stadt", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch steht noch das Haus auf demselben Platz.", "tokens": ["Doch", "steht", "noch", "das", "Haus", "auf", "dem\u00b7sel\u00b7ben", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Da steht auch ein Mensch und starrt in die H\u00f6he,", "tokens": ["Da", "steht", "auch", "ein", "Mensch", "und", "starrt", "in", "die", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ringt die H\u00e4nde, vor Schmerzensgewalt;", "tokens": ["Und", "ringt", "die", "H\u00e4n\u00b7de", ",", "vor", "Schmer\u00b7zens\u00b7ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mir graust es, wenn ich sein Antlitz sehe, \u2014", "tokens": ["Mir", "graust", "es", ",", "wenn", "ich", "sein", "Ant\u00b7litz", "se\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Mond zeigt mir meine eigne Gestalt.", "tokens": ["Der", "Mond", "zeigt", "mir", "mei\u00b7ne", "eig\u00b7ne", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Du Doppeltg\u00e4nger! du bleicher Geselle!", "tokens": ["Du", "Dop\u00b7pelt\u00b7g\u00e4n\u00b7ger", "!", "du", "blei\u00b7cher", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was \u00e4ffst du nach mein Liebesleid,", "tokens": ["Was", "\u00e4ffst", "du", "nach", "mein", "Lie\u00b7be\u00b7sleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das mich gequ\u00e4lt auf dieser Stelle,", "tokens": ["Das", "mich", "ge\u00b7qu\u00e4lt", "auf", "die\u00b7ser", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So manche Nacht, in alter Zeit?", "tokens": ["So", "man\u00b7che", "Nacht", ",", "in", "al\u00b7ter", "Zeit", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}