{"dta.poem.18152": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken \u00fcber das Blut  \n bei dem Aderlassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Flies hin geliebtes Blut, du Schaum", "tokens": ["Flies", "hin", "ge\u00b7lieb\u00b7tes", "Blut", ",", "du", "Schaum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von meinen Leben,", "tokens": ["von", "mei\u00b7nen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Doch denk ich dies dabei, wie", "tokens": ["Doch", "denk", "ich", "dies", "da\u00b7bei", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "PDS", "PAV", "$,", "PWAV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "du von GOtt gegeben,", "tokens": ["du", "von", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df du sehr edel bist, und als", "tokens": ["Da\u00df", "du", "sehr", "e\u00b7del", "bist", ",", "und", "als"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "KON", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ein rother Safft,", "tokens": ["ein", "ro\u00b7ther", "Safft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Des Lebens Lauf erh\u00e4ltst, durch", "tokens": ["Des", "Le\u00b7bens", "Lauf", "er\u00b7h\u00e4ltst", ",", "durch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "APPR"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.8": {"text": "deine Wallungs Krafft.", "tokens": ["dei\u00b7ne", "Wal\u00b7lungs", "Krafft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Dich hab ich bis daher in Speisen eingegessen,", "tokens": ["Dich", "hab", "ich", "bis", "da\u00b7her", "in", "Spei\u00b7sen", "ein\u00b7ge\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PAV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du must, als weisse Milch, dich durch die Leber", "tokens": ["Du", "must", ",", "als", "weis\u00b7se", "Milch", ",", "dich", "durch", "die", "Le\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ADJA", "NN", "$,", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "pressen.", "tokens": ["pres\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Die hat dich roth gef\u00e4rbt auf deiner Wallung Spur:", "tokens": ["Die", "hat", "dich", "roth", "ge\u00b7f\u00e4rbt", "auf", "dei\u00b7ner", "Wal\u00b7lung", "Spur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O! grosses Wunderwerk der w\u00fcrkenden Natur,", "tokens": ["O", "!", "gros\u00b7ses", "Wun\u00b7der\u00b7werk", "der", "w\u00fcr\u00b7ken\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es ist kein Tropfen Blut, in meiner Adern G\u00e4ngen", "tokens": ["Es", "ist", "kein", "Trop\u00b7fen", "Blut", ",", "in", "mei\u00b7ner", "A\u00b7dern", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "NN", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es mu\u00df sich durch das Hertz, als Mittelpuncte", "tokens": ["Es", "mu\u00df", "sich", "durch", "das", "Hertz", ",", "als", "Mit\u00b7tel\u00b7punc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "drengen", "tokens": ["dren\u00b7gen"], "token_info": ["word"], "pos": ["PAV"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Ach ginge dies ans Hertz! ach! d\u00e4chte ich daran,", "tokens": ["Ach", "gin\u00b7ge", "dies", "ans", "Hertz", "!", "ach", "!", "d\u00e4ch\u00b7te", "ich", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PDS", "APPRART", "NN", "$.", "XY", "$.", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df keiner ohne Blut von Menschen leben kan,", "tokens": ["Da\u00df", "kei\u00b7ner", "oh\u00b7ne", "Blut", "von", "Men\u00b7schen", "le\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df keiner ohne Blut aus unsers Heilands Wunden,", "tokens": ["Da\u00df", "kei\u00b7ner", "oh\u00b7ne", "Blut", "aus", "un\u00b7sers", "Hei\u00b7lands", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das wahre Leben hat in jener Welt gefunden.", "tokens": ["Das", "wah\u00b7re", "Le\u00b7ben", "hat", "in", "je\u00b7ner", "Welt", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du solt das Denkbild sein, woran mir f\u00e4llet bei,", "tokens": ["Du", "solt", "das", "Denk\u00b7bild", "sein", ",", "wo\u00b7ran", "mir", "f\u00e4l\u00b7let", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,", "PWAV", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df auch der Mensch hierin, die Welt im Kleinen sei,", "tokens": ["Da\u00df", "auch", "der", "Mensch", "hie\u00b7rin", ",", "die", "Welt", "im", "Klei\u00b7nen", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "$,", "ART", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und da\u00df dies Perlen Na\u00df das in dem Adern schleichet,", "tokens": ["Und", "da\u00df", "dies", "Per\u00b7len", "Na\u00df", "das", "in", "dem", "A\u00b7dern", "schlei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "NN", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Auch Seen, Fl\u00fc\u00df und Meer auf unsrer Erde gleichet:", "tokens": ["Auch", "Seen", ",", "Fl\u00fc\u00df", "und", "Meer", "auf", "uns\u00b7rer", "Er\u00b7de", "glei\u00b7chet", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.25": {"text": "Hie schl\u00e4gt bald Wind und Sturm die Wellen in", "tokens": ["Hie", "schl\u00e4gt", "bald", "Wind", "und", "Sturm", "die", "Wel\u00b7len", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "die H\u00f6h,", "tokens": ["die", "H\u00f6h", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Das Blut bewegt der Zorn, die Lust, die Furcht,", "tokens": ["Das", "Blut", "be\u00b7wegt", "der", "Zorn", ",", "die", "Lust", ",", "die", "Furcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "das Weh,", "tokens": ["das", "Weh", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Mit vollen Ungest\u00fcm der aufgesch\u00e4umten Wellen,", "tokens": ["Mit", "vol\u00b7len", "Un\u00b7ge\u00b7st\u00fcm", "der", "auf\u00b7ge\u00b7sch\u00e4um\u00b7ten", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die an ein steinern Hertz, als an ein Ufer schwellen", "tokens": ["Die", "an", "ein", "stei\u00b7nern", "Hertz", ",", "als", "an", "ein", "U\u00b7fer", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wer ist der diesen Trieb der Neigung stillen kan?", "tokens": ["Wer", "ist", "der", "die\u00b7sen", "Trieb", "der", "Nei\u00b7gung", "stil\u00b7len", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "PDAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mein JEsus du allein bist hier ein Wundermann ", "tokens": ["Mein", "Je\u00b7sus", "du", "al\u00b7lein", "bist", "hier", "ein", "Wun\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "PPER", "ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dem Wind und Meer gehorcht. Drum wenn mein", "tokens": ["Dem", "Wind", "und", "Meer", "ge\u00b7horcht", ".", "Drum", "wenn", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$.", "PAV", "KOUS", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.34": {"text": "Blut sich reget,", "tokens": ["Blut", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "So sei du der die Wuth in meinen Adern leget.", "tokens": ["So", "sei", "du", "der", "die", "Wuth", "in", "mei\u00b7nen", "A\u00b7dern", "le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}