{"textgrid.poem.29099": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df gern ein F\u00fcrsten-Hof an Narren fruchtbar sey,", "tokens": ["Da\u00df", "gern", "ein", "F\u00fcrs\u00b7ten\u00b7Hof", "an", "Nar\u00b7ren", "frucht\u00b7bar", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleibt wahr; doch sind daselbst von solchen meistens zwey:", "tokens": ["Bleibt", "wahr", ";", "doch", "sind", "da\u00b7selbst", "von", "sol\u00b7chen", "meis\u00b7tens", "zwey", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ADV", "VAFIN", "PAV", "APPR", "PIAT", "ADV", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der eine, den der F\u00fcrst nach Willen stets vexirt,", "tokens": ["Der", "ei\u00b7ne", ",", "den", "der", "F\u00fcrst", "nach", "Wil\u00b7len", "stets", "ve\u00b7xirt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "PRELS", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der andre, der nach Lust den F\u00fcrsten rumher f\u00fchrt.", "tokens": ["Der", "and\u00b7re", ",", "der", "nach", "Lust", "den", "F\u00fcrs\u00b7ten", "rum\u00b7her", "f\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "APPR", "NN