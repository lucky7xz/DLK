{"textgrid.poem.63842": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlucht", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tret' ich, die Brust zu l\u00fcften, aus dem Haus", "tokens": ["Tret'", "ich", ",", "die", "Brust", "zu", "l\u00fcf\u00b7ten", ",", "aus", "dem", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aufatmend in den Wintertag hinaus,", "tokens": ["Auf\u00b7at\u00b7mend", "in", "den", "Win\u00b7ter\u00b7tag", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So lockt mich, eh' ich f\u00fcnfzig Schritte tat,", "tokens": ["So", "lockt", "mich", ",", "eh'", "ich", "f\u00fcnf\u00b7zig", "Schrit\u00b7te", "tat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Fahrweg links hinweg ein Schattenpfad", "tokens": ["Vom", "Fahr\u00b7weg", "links", "hin\u00b7weg", "ein", "Schat\u00b7ten\u00b7pfad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu einem Gittertor. Da tret' ich ein,", "tokens": ["Zu", "ei\u00b7nem", "Git\u00b7ter\u00b7tor", ".", "Da", "tret'", "ich", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und mich empf\u00e4ngt ein lichter Erlenhain,", "tokens": ["Und", "mich", "emp\u00b7f\u00e4ngt", "ein", "lich\u00b7ter", "Er\u00b7len\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich w\u00f6lbend \u00fcber eines B\u00e4chleins Lauf.", "tokens": ["Sich", "w\u00f6l\u00b7bend", "\u00fc\u00b7ber", "ei\u00b7nes", "B\u00e4ch\u00b7leins", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Links steigt der Abhang dichtbelaubt hinauf,", "tokens": ["Links", "steigt", "der", "Ab\u00b7hang", "dicht\u00b7be\u00b7laubt", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Rechts breitet sich ein sanfter Wiesengrund", "tokens": ["Rechts", "brei\u00b7tet", "sich", "ein", "sanf\u00b7ter", "Wie\u00b7sen\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "(der Lieblingstummelplatz f\u00fcr meinen Hund)", "tokens": ["(", "der", "Lieb\u00b7lings\u00b7tum\u00b7mel\u00b7platz", "f\u00fcr", "mei\u00b7nen", "Hund", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und dr\u00fcber, auf des Tales Rand erh\u00f6ht,", "tokens": ["Und", "dr\u00fc\u00b7ber", ",", "auf", "des", "Ta\u00b7les", "Rand", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ein wei\u00df Kapellchen. Ihm zur Seite steht", "tokens": ["Ein", "wei\u00df", "Ka\u00b7pell\u00b7chen", ".", "Ihm", "zur", "Sei\u00b7te", "steht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "NN", "$.", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein dunkles Paar Zypressen, hingestellt", "tokens": ["Ein", "dunk\u00b7les", "Paar", "Zyp\u00b7res\u00b7sen", ",", "hin\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "VVFIN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.14": {"text": "Als W\u00e4chter dieser traumhaft stillen Welt.", "tokens": ["Als", "W\u00e4ch\u00b7ter", "die\u00b7ser", "traum\u00b7haft", "stil\u00b7len", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Rings unten auf dem dichtbegras'ten Plan", "tokens": ["Rings", "un\u00b7ten", "auf", "dem", "dicht\u00b7be\u00b7gras'\u00b7ten", "Plan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und zu den schattigen Halden hoch hinan", "tokens": ["Und", "zu", "den", "schat\u00b7ti\u00b7gen", "Hal\u00b7den", "hoch", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJD", "PTKVZ"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Wird, wenn die ersten lauen L\u00fcfte wehn,", "tokens": ["Wird", ",", "wenn", "die", "ers\u00b7ten", "lau\u00b7en", "L\u00fcf\u00b7te", "wehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "ART", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ein m\u00e4rchenbunter Lenzesflor erstehn,", "tokens": ["Ein", "m\u00e4r\u00b7chen\u00b7bun\u00b7ter", "Len\u00b7zes\u00b7flor", "er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Von Primeln schimmert's golden, Veilchen bl\u00fchn,", "tokens": ["Von", "Pri\u00b7meln", "schim\u00b7mert's", "gol\u00b7den", ",", "Veil\u00b7chen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Aus wilden Myrten \u00e4ugelt Immergr\u00fcn,", "tokens": ["Aus", "wil\u00b7den", "Myr\u00b7ten", "\u00e4u\u00b7gelt", "Im\u00b7mer\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Doch jetzt ist Winter.", "tokens": ["Doch", "jetzt", "ist", "Win\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Sacht schreit' ich empor,", "tokens": ["Sacht", "schreit'", "ich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Bis wo sich auftut hoch und schmal ein Tor:", "tokens": ["Bis", "wo", "sich", "auf\u00b7tut", "hoch", "und", "schmal", "ein", "Tor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "PRF", "ADJD", "ADJD", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Zwei schlanke St\u00e4mme, wuchernd dicht umrankt", "tokens": ["Zwei", "schlan\u00b7ke", "St\u00e4m\u00b7me", ",", "wu\u00b7chernd", "dicht", "um\u00b7rankt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "$,", "ADJD", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Von Epheu, der bis in die Wipfel langt.", "tokens": ["Von", "E\u00b7pheu", ",", "der", "bis", "in", "die", "Wip\u00b7fel", "langt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Hier ist der Eingang, wo die Schlucht sich engt", "tokens": ["Hier", "ist", "der", "Ein\u00b7gang", ",", "wo", "die", "Schlucht", "sich", "engt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und ew'ge Wildnis d\u00e4mmernd dich umf\u00e4ngt.", "tokens": ["Und", "ew'\u00b7ge", "Wild\u00b7nis", "d\u00e4m\u00b7mernd", "dich", "um\u00b7f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Vom Bach, der rauschend in die Tiefe f\u00e4hrt,", "tokens": ["Vom", "Bach", ",", "der", "rau\u00b7schend", "in", "die", "Tie\u00b7fe", "f\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Wird \u00fcppig gr\u00fcne Pflanzenbrut gen\u00e4hrt,", "tokens": ["Wird", "\u00fcp\u00b7pig", "gr\u00fc\u00b7ne", "Pflan\u00b7zen\u00b7brut", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "H\u00e4ngt sich in wirren Ranken links und rechts", "tokens": ["H\u00e4ngt", "sich", "in", "wir\u00b7ren", "Ran\u00b7ken", "links", "und", "rechts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Um nackte Zweige jedes Baumgeschlechts,", "tokens": ["Um", "nack\u00b7te", "Zwei\u00b7ge", "je\u00b7des", "Baum\u00b7ge\u00b7schlechts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Hirschzungen, Farn und Brombeer, urwalddicht,", "tokens": ["Hirschzun\u00b7gen", ",", "Farn", "und", "Brom\u00b7beer", ",", "ur\u00b7wald\u00b7dicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Schwach trieft herein von oben her das Licht.", "tokens": ["Schwach", "trieft", "her\u00b7ein", "von", "o\u00b7ben", "her", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "APPR", "ADV", "APZR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Hier kannst nach Herzenslust du einsam sein,", "tokens": ["Hier", "kannst", "nach", "Her\u00b7zens\u00b7lust", "du", "ein\u00b7sam", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "NN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Denn selten nur verirrt sich hier hinein", "tokens": ["Denn", "sel\u00b7ten", "nur", "ver\u00b7irrt", "sich", "hier", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PRF", "ADV", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Ein Wintergast. Und wo die Kluft sich schlie\u00dft,", "tokens": ["Ein", "Win\u00b7ter\u00b7gast", ".", "Und", "wo", "die", "Kluft", "sich", "schlie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Siehst du den Bach, der rauschend sich ergie\u00dft", "tokens": ["Siehst", "du", "den", "Bach", ",", "der", "rau\u00b7schend", "sich", "er\u00b7gie\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Aus braunem Felsspalt und zerstiebt im Fall", "tokens": ["Aus", "brau\u00b7nem", "Fels\u00b7spalt", "und", "zer\u00b7stiebt", "im", "Fall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und f\u00fcllt die Schlucht mit seines Sturzes Schall.", "tokens": ["Und", "f\u00fcllt", "die", "Schlucht", "mit", "sei\u00b7nes", "Stur\u00b7zes", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Das B\u00e4nklein hier, vom hellen Gischt umspr\u00fcht,", "tokens": ["Das", "B\u00e4n\u00b7klein", "hier", ",", "vom", "hel\u00b7len", "Gischt", "um\u00b7spr\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Lockt nur zur Rast, wenn schwer der Sommer gl\u00fcht.", "tokens": ["Lockt", "nur", "zur", "Rast", ",", "wenn", "schwer", "der", "Som\u00b7mer", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Doch jetzt ist Winter; aber weich die Luft", "tokens": ["Doch", "jetzt", "ist", "Win\u00b7ter", ";", "a\u00b7ber", "weich", "die", "Luft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "NN", "$.", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "In dieser moderk\u00fchlen Felsengruft,", "tokens": ["In", "die\u00b7ser", "mo\u00b7der\u00b7k\u00fch\u00b7len", "Fel\u00b7sen\u00b7gruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Und w\u00fcrzig weht dich an um Weihnacht auch", "tokens": ["Und", "w\u00fcr\u00b7zig", "weht", "dich", "an", "um", "Weih\u00b7nacht", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPR", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Des immergr\u00fcnen Unkrauts feuchter Hauch.", "tokens": ["Des", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "Un\u00b7krauts", "feuch\u00b7ter", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier ist's, wo manche Stund' an manchem Tag", "tokens": ["Hier", "ist's", ",", "wo", "man\u00b7che", "Stund'", "an", "man\u00b7chem", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "PIAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich still verweilend der Betrachtung pflag,", "tokens": ["Ich", "still", "ver\u00b7wei\u00b7lend", "der", "Be\u00b7trach\u00b7tung", "pflag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Welt und ihrem L\u00e4rmen weit entr\u00fcckt,", "tokens": ["Der", "Welt", "und", "ih\u00b7rem", "L\u00e4r\u00b7men", "weit", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Geistergru\u00df im Innersten begl\u00fcckt,", "tokens": ["Von", "Geis\u00b7ter\u00b7gru\u00df", "im", "In\u00b7ners\u00b7ten", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "w