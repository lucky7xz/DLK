{"textgrid.poem.49113": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Vater aller Lust und Freud,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vater aller Lust und Freud,", "tokens": ["O", "Va\u00b7ter", "al\u00b7ler", "Lust", "und", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Bacchus, Bacchus gieb mir heut", "tokens": ["O", "Bac\u00b7chus", ",", "Bac\u00b7chus", "gieb", "mir", "heut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "VVIMP", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und allezeit zu trinken;", "tokens": ["Und", "al\u00b7le\u00b7zeit", "zu", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch, Gram Verscheucher gieb auch du", "tokens": ["Doch", ",", "Gram", "Ver\u00b7scheu\u00b7cher", "gieb", "auch", "du"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "NE", "NN", "VVIMP", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir immer, immer Durst dazu,", "tokens": ["Mir", "im\u00b7mer", ",", "im\u00b7mer", "Durst", "da\u00b7zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich kann immer trinken.", "tokens": ["Da\u00df", "ich", "kann", "im\u00b7mer", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Denn ach! wie hart ist es zu sehn,", "tokens": ["Denn", "ach", "!", "wie", "hart", "ist", "es", "zu", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Einen goldnen Becher blinkend stehn,", "tokens": ["Ei\u00b7nen", "gold\u00b7nen", "Be\u00b7cher", "blin\u00b7kend", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Ohn' da\u00df ich ihn k\u00f6nnt' trinken. \u2013", "tokens": ["Ohn'", "da\u00df", "ich", "ihn", "k\u00f6nnt'", "trin\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}