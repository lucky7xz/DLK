{"textgrid.poem.33718": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "9. Ersatz der Natur", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat jahrelanger Krieg ein Land durchw\u00fctet", "tokens": ["Hat", "jah\u00b7re\u00b7lan\u00b7ger", "Krieg", "ein", "Land", "durch\u00b7w\u00fc\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Not verzehrt und Hagelschlag geschlagen,", "tokens": ["Und", "Not", "ver\u00b7zehrt", "und", "Ha\u00b7gel\u00b7schlag", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann kommt doch einmal noch von Segenstagen", "tokens": ["Dann", "kommt", "doch", "ein\u00b7mal", "noch", "von", "Se\u00b7gens\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Sonnenjahr, das jeden Schmerz verg\u00fctet.", "tokens": ["Ein", "Son\u00b7nen\u00b7jahr", ",", "das", "je\u00b7den", "Schmerz", "ver\u00b7g\u00fc\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im M\u00e4rz schon bl\u00fcht's, die fr\u00fche Schwalbe br\u00fctet,", "tokens": ["Im", "M\u00e4rz", "schon", "bl\u00fcht's", ",", "die", "fr\u00fc\u00b7he", "Schwal\u00b7be", "br\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hoch steht das Gras, zehnfache Fr\u00fcchte tragen", "tokens": ["Hoch", "steht", "das", "Gras", ",", "zehn\u00b7fa\u00b7che", "Fr\u00fcch\u00b7te", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Felder noch dem zweiten Erntewagen,", "tokens": ["Die", "Fel\u00b7der", "noch", "dem", "zwei\u00b7ten", "Ern\u00b7te\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Alpen wird im Sp\u00e4tjahr noch geh\u00fctet.", "tokens": ["Auf", "Al\u00b7pen", "wird", "im", "Sp\u00e4t\u00b7jahr", "noch", "ge\u00b7h\u00fc\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kaum will das Laub zu fallen sich entschlie\u00dfen,", "tokens": ["Kaum", "will", "das", "Laub", "zu", "fal\u00b7len", "sich", "ent\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKZU", "VVINF", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob auch die Tenne dr\u00f6hnt, ob auch die B\u00fctten", "tokens": ["Ob", "auch", "die", "Ten\u00b7ne", "dr\u00f6hnt", ",", "ob", "auch", "die", "B\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Keller schon der G\u00e4rung Duft ergie\u00dfen.", "tokens": ["Und", "Kel\u00b7ler", "schon", "der", "G\u00e4\u00b7rung", "Duft", "er\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun segnet sich der Greis noch, Fr\u00fcchte sch\u00fctten", "tokens": ["Nun", "seg\u00b7net", "sich", "der", "Greis", "noch", ",", "Fr\u00fcch\u00b7te", "sch\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADV", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die \u00e4ltsten St\u00e4mme noch, und Wein genie\u00dfen", "tokens": ["Die", "\u00e4lts\u00b7ten", "St\u00e4m\u00b7me", "noch", ",", "und", "Wein", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wei\u00dfes Brot die \u00c4rmsten in den H\u00fctten.", "tokens": ["Und", "wei\u00b7\u00dfes", "Brot", "die", "\u00c4rms\u00b7ten", "in", "den", "H\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}