{"textgrid.poem.56113": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die achte Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1922, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit allen Augen sieht die Kreatur", "tokens": ["Mit", "al\u00b7len", "Au\u00b7gen", "sieht", "die", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das Offene. Nur unsre Augen sind", "tokens": ["das", "Of\u00b7fe\u00b7ne", ".", "Nur", "uns\u00b7re", "Au\u00b7gen", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "PPOSAT", "NN", "VAFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "wie umgekehrt und ganz um sie gestellt", "tokens": ["wie", "um\u00b7ge\u00b7kehrt", "und", "ganz", "um", "sie", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "als Fallen, rings um ihren freien Ausgang.", "tokens": ["als", "Fal\u00b7len", ",", "rings", "um", "ih\u00b7ren", "frei\u00b7en", "Aus\u00b7gang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was drau\u00dfen ", "tokens": ["Was", "drau\u00b7\u00dfen"], "token_info": ["word", "word"], "pos": ["PWS", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Antlitz allein; denn schon das fr\u00fche Kind", "tokens": ["Ant\u00b7litz", "al\u00b7lein", ";", "denn", "schon", "das", "fr\u00fc\u00b7he", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$.", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "wenden wir um und zwingens, da\u00df es r\u00fcckw\u00e4rts", "tokens": ["wen\u00b7den", "wir", "um", "und", "zwin\u00b7gens", ",", "da\u00df", "es", "r\u00fcck\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "KON", "ADV", "$,", "KOUS", "PPER", "ADV"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Gestaltung sehe, nicht das Offne, das", "tokens": ["Ge\u00b7stal\u00b7tung", "se\u00b7he", ",", "nicht", "das", "Off\u00b7ne", ",", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "$,", "PTKNEG", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "im Tiergesicht so tief ist. Frei von Tod.", "tokens": ["im", "Tier\u00b7ge\u00b7sicht", "so", "tief", "ist", ".", "Frei", "von", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VAFIN", "$.", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "hat seinen Untergang stets hinter sich", "tokens": ["hat", "sei\u00b7nen", "Un\u00b7ter\u00b7gang", "stets", "hin\u00b7ter", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPR", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "und vor sich Gott, und wenn es geht, so gehts", "tokens": ["und", "vor", "sich", "Gott", ",", "und", "wenn", "es", "geht", ",", "so", "gehts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PRF", "NN", "$,", "KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "in Ewigkeit, so wie die Brunnen gehen.", "tokens": ["in", "E\u00b7wig\u00b7keit", ",", "so", "wie", "die", "Brun\u00b7nen", "ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "den reinen Raum vor uns, in den die Blumen", "tokens": ["den", "rei\u00b7nen", "Raum", "vor", "uns", ",", "in", "den", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,", "APPR", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "unendlich aufgehn. Immer ist es Welt", "tokens": ["un\u00b7end\u00b7lich", "auf\u00b7gehn", ".", "Im\u00b7mer", "ist", "es", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "$.", "ADV", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und niemals Nirgends ohne Nicht: das Reine,", "tokens": ["und", "nie\u00b7mals", "Nir\u00b7gends", "oh\u00b7ne", "Nicht", ":", "das", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PTKNEG", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Un\u00fcberwachte, das man atmet und", "tokens": ["Un\u00b7\u00fc\u00b7ber\u00b7wach\u00b7te", ",", "das", "man", "at\u00b7met", "und"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PIS", "VVFIN", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "unendlich ", "tokens": ["un\u00b7end\u00b7lich"], "token_info": ["word"], "pos": ["ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "verliert sich eins im Stilln an dies und wird", "tokens": ["ver\u00b7liert", "sich", "eins", "im", "Stilln", "an", "dies", "und", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PIS", "APPRART", "NN", "APPR", "PDS", "KON", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "ger\u00fcttelt. Oder jener stirbt und ", "tokens": ["ge\u00b7r\u00fct\u00b7telt", ".", "O\u00b7der", "je\u00b7ner", "stirbt", "und"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "KON", "PDS", "VVFIN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn nah am Tod sieht man den Tod nicht mehr", "tokens": ["Denn", "nah", "am", "Tod", "sieht", "man", "den", "Tod", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PIS", "ART", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "und starrt ", "tokens": ["und", "starrt"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Liebende, w\u00e4re nicht der andre, der", "tokens": ["Lie\u00b7ben\u00b7de", ",", "w\u00e4\u00b7re", "nicht", "der", "and\u00b7re", ",", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "VAFIN", "PTKNEG", "ART", "ADJA", "$,", "PRELS"], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}, "line.11": {"text": "die Sicht verstellt, sind nah daran und staunen...", "tokens": ["die", "Sicht", "ver\u00b7stellt", ",", "sind", "nah", "da\u00b7ran", "und", "stau\u00b7nen", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VAFIN", "ADJD", "PAV", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wie aus Versehn ist ihnen aufgetan", "tokens": ["Wie", "aus", "Ver\u00b7sehn", "ist", "ih\u00b7nen", "auf\u00b7ge\u00b7tan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "hinter dem andern... Aber \u00fcber ihn", "tokens": ["hin\u00b7ter", "dem", "an\u00b7dern", "...", "A\u00b7ber", "\u00fc\u00b7ber", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$(", "KON", "APPR", "PPER"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "kommt keiner fort, und wieder wird ihm Welt.", "tokens": ["kommt", "kei\u00b7ner", "fort", ",", "und", "wie\u00b7der", "wird", "ihm", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "KON", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Sch\u00f6pfung immer zugewendet, sehn", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "im\u00b7mer", "zu\u00b7ge\u00b7wen\u00b7det", ",", "sehn"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "wir nur auf ihr die Spiegelung des Frein,", "tokens": ["wir", "nur", "auf", "ihr", "die", "Spie\u00b7ge\u00b7lung", "des", "Frein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "von uns verdunkelt. Oder da\u00df ein Tier,", "tokens": ["von", "uns", "ver\u00b7dun\u00b7kelt", ".", "O\u00b7der", "da\u00df", "ein", "Tier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$.", "KON", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "ein stummes, aufschaut, ruhig durch uns durch.", "tokens": ["ein", "stum\u00b7mes", ",", "auf\u00b7schaut", ",", "ru\u00b7hig", "durch", "uns", "durch", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVPP", "$,", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Dieses hei\u00dft Schicksal: gegen\u00fcber sein", "tokens": ["Die\u00b7ses", "hei\u00dft", "Schick\u00b7sal", ":", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "sein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "$.", "APPR", "PPOSAT"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.20": {"text": "und nichts als das und immer gegen\u00fcber.", "tokens": ["und", "nichts", "als", "das", "und", "im\u00b7mer", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ART", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "W\u00e4re Bewu\u00dftheit unsrer Art in dem", "tokens": ["W\u00e4\u00b7re", "Be\u00b7wu\u00df\u00b7theit", "uns\u00b7rer", "Art", "in", "dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "APPR", "ART"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "sicheren Tier, das uns entgegenzieht", "tokens": ["si\u00b7che\u00b7ren", "Tier", ",", "das", "uns", "ent\u00b7ge\u00b7gen\u00b7zieht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "in anderer Richtung \u2013, ri\u00df es uns herum", "tokens": ["in", "an\u00b7de\u00b7rer", "Rich\u00b7tung", "\u2013", ",", "ri\u00df", "es", "uns", "he\u00b7rum"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "$,", "VVFIN", "PPER", "PRF", "APZR"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "mit seinem Wandel. Doch sein Sein ist ihm", "tokens": ["mit", "sei\u00b7nem", "Wan\u00b7del", ".", "Doch", "sein", "Sein", "ist", "ihm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "KON", "PPOSAT", "NN", "VAFIN", "PPER"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "unendlich, ungefa\u00dft und ohne Blick", "tokens": ["un\u00b7end\u00b7lich", ",", "un\u00b7ge\u00b7fa\u00dft", "und", "oh\u00b7ne", "Blick"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "auf seinen Zustand, rein, so wie sein Ausblick.", "tokens": ["auf", "sei\u00b7nen", "Zu\u00b7stand", ",", "rein", ",", "so", "wie", "sein", "Aus\u00b7blick", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "$,", "ADV", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wo wir Zukunft sehn, dort sieht es Alles", "tokens": ["Und", "wo", "wir", "Zu\u00b7kunft", "sehn", ",", "dort", "sieht", "es", "Al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "NN", "VVINF", "$,", "ADV", "VVFIN", "PPER", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und sich in Allem und geheilt f\u00fcr immer.", "tokens": ["und", "sich", "in", "Al\u00b7lem", "und", "ge\u00b7heilt", "f\u00fcr", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PIS", "KON", "VVPP", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und doch ist in dem wachsam warmen Tier", "tokens": ["Und", "doch", "ist", "in", "dem", "wach\u00b7sam", "war\u00b7men", "Tier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gewicht und Sorge einer gro\u00dfen Schwermut.", "tokens": ["Ge\u00b7wicht", "und", "Sor\u00b7ge", "ei\u00b7ner", "gro\u00b7\u00dfen", "Schwer\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn ihm auch haftet immer an, was uns", "tokens": ["Denn", "ihm", "auch", "haf\u00b7tet", "im\u00b7mer", "an", ",", "was", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "oft \u00fcberw\u00e4ltigt, \u2013 die Erinnerung,", "tokens": ["oft", "\u00fc\u00b7berw\u00b7\u00e4l\u00b7tigt", ",", "\u2013", "die", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "$(", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "als sei schon einmal das, wonach man dr\u00e4ngt,", "tokens": ["als", "sei", "schon", "ein\u00b7mal", "das", ",", "wo\u00b7nach", "man", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOKOM", "VAFIN", "ADV", "ADV", "PDS", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "n\u00e4her gewesen, treuer und sein Anschlu\u00df", "tokens": ["n\u00e4\u00b7her", "ge\u00b7we\u00b7sen", ",", "treu\u00b7er", "und", "sein", "An\u00b7schlu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAPP", "$,", "ADJA", "KON", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "unendlich z\u00e4rtlich. Hier ist alles Abstand,", "tokens": ["un\u00b7end\u00b7lich", "z\u00e4rt\u00b7lich", ".", "Hier", "ist", "al\u00b7les", "Ab\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$.", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und dort wars Atem. Nach der ersten Heimat", "tokens": ["und", "dort", "wars", "A\u00b7tem", ".", "Nach", "der", "ers\u00b7ten", "Hei\u00b7mat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "ist ihm die zweite zwitterig und windig.", "tokens": ["ist", "ihm", "die", "zwei\u00b7te", "zwit\u00b7te\u00b7rig", "und", "win\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": 