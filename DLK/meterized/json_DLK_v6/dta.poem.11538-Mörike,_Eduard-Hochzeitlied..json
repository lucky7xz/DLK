{"dta.poem.11538": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Hochzeitlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht weit vom Dorf zwei Linden stehen,", "tokens": ["Nicht", "weit", "vom", "Dorf", "zwei", "Lin\u00b7den", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPRART", "NN", "CARD", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Einsam, der Felder stille Hut,", "tokens": ["Ein\u00b7sam", ",", "der", "Fel\u00b7der", "stil\u00b7le", "Hut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wo in der Sommern\u00e4chte Wehen", "tokens": ["Wo", "in", "der", "Som\u00b7mer\u00b7n\u00e4ch\u00b7te", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Hirte gern, ein Dichter, ruht.", "tokens": ["Ein", "Hir\u00b7te", "gern", ",", "ein", "Dich\u00b7ter", ",", "ruht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hell schwamm auf Duft und Nebelh\u00fclle", "tokens": ["Hell", "schwamm", "auf", "Duft", "und", "Ne\u00b7bel\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Mondes leiser Zaubertag,", "tokens": ["Des", "Mon\u00b7des", "lei\u00b7ser", "Zau\u00b7ber\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum unterbrach die holde Stille", "tokens": ["Kaum", "un\u00b7ter\u00b7brach", "die", "hol\u00b7de", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von fern bescheidner Wachtelschlag.", "tokens": ["Von", "fern", "be\u00b7scheid\u00b7ner", "Wach\u00b7tel\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie ich ruhig so in Mitten", "tokens": ["Und", "wie", "ich", "ru\u00b7hig", "so", "in", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "All dieser Sch\u00f6nheit lag und sann,", "tokens": ["All", "die\u00b7ser", "Sch\u00f6n\u00b7heit", "lag", "und", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kam mit leichtgehobnen Schritten", "tokens": ["Da", "kam", "mit", "leicht\u00b7ge\u00b7hob\u00b7nen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein g\u00f6ttlich Frauenbild heran.", "tokens": ["Ein", "g\u00f6tt\u00b7lich", "Frau\u00b7en\u00b7bild", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gewi\u00df, es war der Musen eine,", "tokens": ["Ge\u00b7wi\u00df", ",", "es", "war", "der", "Mu\u00b7sen", "ei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erschrocken merkt' ich's, lustbewegt;", "tokens": ["Er\u00b7schro\u00b7cken", "merkt'", "ich's", ",", "lust\u00b7be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sezt sich zu mir an dem Raine,", "tokens": ["Sie", "sezt", "sich", "zu", "mir", "an", "dem", "Rai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hand auf meinen Arm gelegt.", "tokens": ["Die", "Hand", "auf", "mei\u00b7nen", "Arm", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sch\u00fcttelt l\u00e4chelnd aus dem Kleide", "tokens": ["Und", "sch\u00fct\u00b7telt", "l\u00e4\u00b7chelnd", "aus", "dem", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Blaue Cyanen, Stern an Stern:", "tokens": ["Blau\u00b7e", "Cy\u00b7a\u00b7nen", ",", "Stern", "an", "Stern", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u201edich st\u00f6rt's nicht, wenn an deiner Seite", "tokens": ["\u201e", "dich", "st\u00f6rt's", "nicht", ",", "wenn", "an", "dei\u00b7ner", "Sei\u00b7te"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich heut' ein Kr\u00e4nzlein b\u00e4nde gern.", "tokens": ["Ich", "heut'", "ein", "Kr\u00e4nz\u00b7lein", "b\u00e4n\u00b7de", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nicht wahr, mit Schw\u00e4rmen und mit Plaudern", "tokens": ["Nicht", "wahr", ",", "mit", "Schw\u00e4r\u00b7men", "und", "mit", "Plau\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verbr\u00e4chte gern mein Freund die Nacht?", "tokens": ["Ver\u00b7br\u00e4ch\u00b7te", "gern", "mein", "Freund", "die", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch flecht' ich still, und ohne Zaudern", "tokens": ["Doch", "flecht'", "ich", "still", ",", "und", "oh\u00b7ne", "Zau\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sey du mir auf ein Lied bedacht!", "tokens": ["Sey", "du", "mir", "auf", "ein", "Lied", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh, wo das D\u00f6rflein mit der Spitze", "tokens": ["Sieh", ",", "wo", "das", "D\u00f6r\u00b7flein", "mit", "der", "Spit\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des gelben Thurms her\u00fcberschaut,", "tokens": ["Des", "gel\u00b7ben", "Thurms", "her\u00b7\u00fc\u00b7bersc\u00b7haut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort schlummert auf dem Elternsitze", "tokens": ["Dort", "schlum\u00b7mert", "auf", "dem", "El\u00b7tern\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch wenig N\u00e4chte eine Braut.", "tokens": ["Noch", "we\u00b7nig", "N\u00e4ch\u00b7te", "ei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie schl\u00e4ft. Der Wange Rosen beben,", "tokens": ["Sie", "schl\u00e4ft", ".", "Der", "Wan\u00b7ge", "Ro\u00b7sen", "be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir beide ahnen wohl, wovon;", "tokens": ["Wir", "bei\u00b7de", "ah\u00b7nen", "wohl", ",", "wo\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PIS", "VVINF", "ADV", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um die halb offne Lippe schweben", "tokens": ["Um", "die", "halb", "off\u00b7ne", "Lip\u00b7pe", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Tr\u00e4ume gl\u00fchnder K\u00fcsse schon.", "tokens": ["Die", "Tr\u00e4u\u00b7me", "gl\u00fchn\u00b7der", "K\u00fcs\u00b7se", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nicht doch! mit lauten Herzensschl\u00e4gen", "tokens": ["Nicht", "doch", "!", "mit", "lau\u00b7ten", "Her\u00b7zens\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt sie vielleicht der Glocken Klang,", "tokens": ["H\u00f6rt", "sie", "viel\u00b7leicht", "der", "Glo\u00b7cken", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt am Altar den Vatersegen", "tokens": ["H\u00f6rt", "am", "Al\u00b7tar", "den", "Va\u00b7ter\u00b7se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und eines Engels Brautgesang.", "tokens": ["Und", "ei\u00b7nes", "En\u00b7gels", "Braut\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Sieht unter Weinen sich umschlungen", "tokens": ["Sieht", "un\u00b7ter", "Wei\u00b7nen", "sich", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Mutter-Lieb', von Schwester-Treu',", "tokens": ["Von", "Mut\u00b7ter\u00b7Lieb'", ",", "von", "Schwes\u00b7ter\u00b7Treu'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Herz von Lust und Schmerz gedrungen,", "tokens": ["Das", "Herz", "von", "Lust", "und", "Schmerz", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Macht sich mit tausend Thr\u00e4nen frei.", "tokens": ["Macht", "sich", "mit", "tau\u00b7send", "Thr\u00e4\u00b7nen", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Und alle diese sel'gen Tr\u00e4ume,", "tokens": ["Und", "al\u00b7le", "die\u00b7se", "sel'\u00b7gen", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der n\u00e4chste Morgen macht sie wahr;", "tokens": ["Der", "n\u00e4chs\u00b7te", "Mor\u00b7gen", "macht", "sie", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es stehen schon des Hauses R\u00e4ume", "tokens": ["Es", "ste\u00b7hen", "schon", "des", "Hau\u00b7ses", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschm\u00fcckt f\u00fcr froher G\u00e4ste Schaar.", "tokens": ["Ge\u00b7schm\u00fcckt", "f\u00fcr", "fro\u00b7her", "G\u00e4s\u00b7te", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Hier aber, wo mit den Gespielen", "tokens": ["Hier", "a\u00b7ber", ",", "wo", "mit", "den", "Ge\u00b7spie\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das M\u00e4dchen oft sich Veilchen las,", "tokens": ["Das", "M\u00e4d\u00b7chen", "oft", "sich", "Veil\u00b7chen", "las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht alleine mit Gef\u00fchlen", "tokens": ["Viel\u00b7leicht", "al\u00b7lei\u00b7ne", "mit", "Ge\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sehnsuchtsvollen Ahnung sa\u00df,", "tokens": ["Der", "sehn\u00b7suchts\u00b7vol\u00b7len", "Ah\u00b7nung", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Hier, unterm Blick prophet'scher Sterne,", "tokens": ["Hier", ",", "un\u00b7term", "Blick", "pro\u00b7phet'\u00b7scher", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weih' ich mit dir dies Fest voraus:", "tokens": ["Weih'", "ich", "mit", "dir", "dies", "Fest", "vo\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PDS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tief schaut die Muse in die Ferne", "tokens": ["Tief", "schaut", "die", "Mu\u00b7se", "in", "die", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des br\u00e4utlichen Geschicks hinaus.", "tokens": ["Des", "br\u00e4ut\u00b7li\u00b7chen", "Ge\u00b7schicks", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Wie golden winkt die neue Schwelle", "tokens": ["Wie", "gol\u00b7den", "winkt", "die", "neu\u00b7e", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens jedem jungen Paar!", "tokens": ["Des", "Le\u00b7bens", "je\u00b7dem", "jun\u00b7gen", "Paar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wei\u00df man, da\u00df nicht s