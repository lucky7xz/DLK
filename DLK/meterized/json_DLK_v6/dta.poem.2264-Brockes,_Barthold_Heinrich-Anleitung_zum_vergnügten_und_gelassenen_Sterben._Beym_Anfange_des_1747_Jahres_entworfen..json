{"dta.poem.2264": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anleitung  \n zum vergn\u00fcgten und gelassenen  \n Sterben.  \n Beym Anfange des 1747 Jahres entworfen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist nichts in unsrer Macht, als die blo\u00dfe", "tokens": ["Es", "ist", "nichts", "in", "uns\u00b7rer", "Macht", ",", "als", "die", "blo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "PPOSAT", "NN", "$,", "KOUS", "ART", "ADJA"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kraft allein,", "tokens": ["Kraft", "al\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Was der Sch\u00f6pfer will, zu wollen.", "tokens": ["Was", "der", "Sch\u00f6p\u00b7fer", "will", ",", "zu", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VMFIN", "$,", "PTKZU", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jhm gef\u00e4llig nun zu seyn,", "tokens": ["Jhm", "ge\u00b7f\u00e4l\u00b7lig", "nun", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft uns ihm doch wenigstens diese Kraft", "tokens": ["La\u00dft", "uns", "ihm", "doch", "we\u00b7nigs\u00b7tens", "die\u00b7se", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "im Sterben zollen!", "tokens": ["im", "Ster\u00b7ben", "zol\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}