{"textgrid.poem.55475": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "56", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singen sie Blumen der kindlichen Ruh,", "tokens": ["Sin\u00b7gen", "sie", "Blu\u00b7men", "der", "kind\u00b7li\u00b7chen", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "K\u00e4fer und V\u00f6gel und Tierchen dazu;", "tokens": ["K\u00e4\u00b7fer", "und", "V\u00f6\u00b7gel", "und", "Tier\u00b7chen", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "PAV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Aber du wachest, wir treten herein,", "tokens": ["A\u00b7ber", "du", "wa\u00b7chest", ",", "wir", "tre\u00b7ten", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Bringen was Ruhiges, bringen den Stein.", "tokens": ["Brin\u00b7gen", "was", "Ru\u00b7hi\u00b7ges", ",", "brin\u00b7gen", "den", "Stein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Steinchen, die bunten, ein lustiges Spiel!", "tokens": ["Stein\u00b7chen", ",", "die", "bun\u00b7ten", ",", "ein", "lus\u00b7ti\u00b7ges", "Spiel", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Was man auch w\u00fcrfe und wie es auch fiel'.", "tokens": ["Was", "man", "auch", "w\u00fcr\u00b7fe", "und", "wie", "es", "auch", "fiel'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVFIN", "KON", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kindischen H\u00e4ndchen entschnickt sich so fein", "tokens": ["Kin\u00b7di\u00b7schen", "H\u00e4nd\u00b7chen", "ent\u00b7schnickt", "sich", "so", "fein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "ADV", "ADJD"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Kn\u00f6chlein und Bohnen und Edelgestein.", "tokens": ["Kn\u00f6c\u00b7hlein", "und", "Boh\u00b7nen", "und", "E\u00b7del\u00b7ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Knabe, du siehest nun Steine behaun,", "tokens": ["Kna\u00b7be", ",", "du", "sie\u00b7hest", "nun", "Stei\u00b7ne", "be\u00b7haun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "NN", "VVINF", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ordnend sich f\u00fcgen, zu H\u00e4usern sich baun.", "tokens": ["Ord\u00b7nend", "sich", "f\u00fc\u00b7gen", ",", "zu", "H\u00e4u\u00b7sern", "sich", "baun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$,", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wohl! du verwunderst dich, stimmest mit ein:", "tokens": ["Wohl", "!", "du", "ver\u00b7wun\u00b7derst", "dich", ",", "stim\u00b7mest", "mit", "ein", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ART", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das ist wahrhaftig ein n\u00fctzlicher Stein!", "tokens": ["Das", "ist", "wahr\u00b7haf\u00b7tig", "ein", "n\u00fctz\u00b7li\u00b7cher", "Stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Spielst du mit Schussern, das K\u00fcgelchen rollt,", "tokens": ["Spielst", "du", "mit", "Schus\u00b7sern", ",", "das", "K\u00fc\u00b7gel\u00b7chen", "rollt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Dreht sich zur Grube, so wie du gewollt,", "tokens": ["Dreht", "sich", "zur", "Gru\u00b7be", ",", "so", "wie", "du", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,", "ADV", "KOKOM", "PPER", "VMPP", "$,"], "meter": "+--+-+++-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "L\u00e4ufest begierig auch hinter ihm drein,", "tokens": ["L\u00e4u\u00b7fest", "be\u00b7gie\u00b7rig", "auch", "hin\u00b7ter", "ihm", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Das ist f\u00fcrwahr wohl ein lustiger Stein.", "tokens": ["Das", "ist", "f\u00fcr\u00b7wahr", "wohl", "ein", "lus\u00b7ti\u00b7ger", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Steinchen um Steinchen verzettelt die Welt,", "tokens": ["Stein\u00b7chen", "um", "Stein\u00b7chen", "ver\u00b7zet\u00b7telt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Wissende haben's zusammengestellt;", "tokens": ["Wis\u00b7sen\u00b7de", "ha\u00b7ben's", "zu\u00b7sam\u00b7men\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Trittst du begierig zu S\u00e4len herein,", "tokens": ["Trittst", "du", "be\u00b7gie\u00b7rig", "zu", "S\u00e4\u00b7len", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Siehst du zuerst nicht den Stein vor dem Stein.", "tokens": ["Siehst", "du", "zu\u00b7erst", "nicht", "den", "Stein", "vor", "dem", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Doch unterscheidest und merkest genau:", "tokens": ["Doch", "un\u00b7ter\u00b7schei\u00b7dest", "und", "mer\u00b7kest", "ge\u00b7nau", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dieser ist rot, und ein andrer ist blau,", "tokens": ["Die\u00b7ser", "ist", "rot", ",", "und", "ein", "an\u00b7drer", "ist", "blau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$,", "KON", "ART", "ADJA", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Einer, der kl\u00e4rste, von Farben so rein,", "tokens": ["Ei\u00b7ner", ",", "der", "kl\u00e4rs\u00b7te", ",", "von", "Far\u00b7ben", "so", "rein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ART", "ADJA", "$,", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Farbig erblitzet der edelste Stein.", "tokens": ["Far\u00b7big", "er\u00b7blit\u00b7zet", "der", "e\u00b7dels\u00b7te", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber die S\u00e4ulchen, wer schliff sie so glatt,", "tokens": ["A\u00b7ber", "die", "S\u00e4ul\u00b7chen", ",", "wer", "schliff", "sie", "so", "glatt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Spitzte sie, sch\u00e4rfte sie gl\u00e4nzend und matt?", "tokens": ["Spitz\u00b7te", "sie", ",", "sch\u00e4rf\u00b7te", "sie", "gl\u00e4n\u00b7zend", "und", "matt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Schau in die Kl\u00fcfte des Berges hinein,", "tokens": ["Schau", "in", "die", "Kl\u00fcf\u00b7te", "des", "Ber\u00b7ges", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Ruhig entwickelt sich Stein aus Gestein.", "tokens": ["Ru\u00b7hig", "ent\u00b7wi\u00b7ckelt", "sich", "Stein", "aus", "Ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Ewig nat\u00fcrlich bewegende Kraft", "tokens": ["E\u00b7wig", "na\u00b7t\u00fcr\u00b7lich", "be\u00b7we\u00b7gen\u00b7de", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "G\u00f6ttlich gesetzlich entbindet und schafft;", "tokens": ["G\u00f6tt\u00b7lich", "ge\u00b7setz\u00b7lich", "ent\u00b7bin\u00b7det", "und", "schafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVPP", "KON", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Trennendes Leben, im Leben Verein,", "tokens": ["Tren\u00b7nen\u00b7des", "Le\u00b7ben", ",", "im", "Le\u00b7ben", "Ver\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Oben die Geister und unten der Stein.", "tokens": ["O\u00b7ben", "die", "Geis\u00b7ter", "und", "un\u00b7ten", "der", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun, wie es Vater und Ahn dir erprobt,", "tokens": ["Nun", ",", "wie", "es", "Va\u00b7ter", "und", "Ahn", "dir", "er\u00b7probt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gott und Natur und das All ist gelobt!", "tokens": ["Gott", "und", "Na\u00b7tur", "und", "das", "All", "ist", "ge\u00b7lobt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Komme! der Stiftende f\u00fchret dich ein,", "tokens": ["Kom\u00b7me", "!", "der", "Stif\u00b7ten\u00b7de", "f\u00fch\u00b7ret", "dich", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Unserem Ringe willkommener Stein!", "tokens": ["Un\u00b7se\u00b7rem", "Rin\u00b7ge", "will\u00b7kom\u00b7me\u00b7ner", "Stein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}