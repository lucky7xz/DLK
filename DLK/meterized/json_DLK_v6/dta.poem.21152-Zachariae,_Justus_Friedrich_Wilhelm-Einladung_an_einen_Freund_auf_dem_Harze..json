{"dta.poem.21152": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Einladung  \n an einen Freund auf dem Harze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fliehe doch einmal, o Freund, aus zugest\u00f6berten Th\u00e4-\nlern,", "tokens": ["Flie\u00b7he", "doch", "ein\u00b7mal", ",", "o", "Freund", ",", "aus", "zu\u00b7ge\u00b7st\u00f6\u00b7ber\u00b7ten", "Th\u00e4", "lern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "FM", "NN", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-++--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Welche so bald noch die Sonne nicht sehn.", "tokens": ["Wel\u00b7che", "so", "bald", "noch", "die", "Son\u00b7ne", "nicht", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Bist du von St\u00fcrmen nicht taub, die hohe Tannen", "tokens": ["Bist", "du", "von", "St\u00fcr\u00b7men", "nicht", "taub", ",", "die", "ho\u00b7he", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "PTKNEG", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "W\u00fcnschest du ewig in Bergen zu seyn?", "tokens": ["W\u00fcn\u00b7schest", "du", "e\u00b7wig", "in", "Ber\u00b7gen", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Komm in die muntere Stadt! In einem fl\u00fcchtigen", "tokens": ["Komm", "in", "die", "mun\u00b7te\u00b7re", "Stadt", "!", "In", "ei\u00b7nem", "fl\u00fcch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "APPR", "ART", "ADJA"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Fliegest du \u00fcber den gl\u00e4nzenden Schnee.", "tokens": ["Flie\u00b7gest", "du", "\u00fc\u00b7ber", "den", "gl\u00e4n\u00b7zen\u00b7den", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Fr\u00f6hlicher sch\u00fcttelt dein Ro\u00df schon alle die jauchzenden", "tokens": ["Fr\u00f6h\u00b7li\u00b7cher", "sch\u00fct\u00b7telt", "dein", "Ro\u00df", "schon", "al\u00b7le", "die", "jauch\u00b7zen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "ADV", "PIS", "ART", "NN"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "Fr\u00f6hlicher setzt es den Reigerbusch auf.", "tokens": ["Fr\u00f6h\u00b7li\u00b7cher", "setzt", "es", "den", "Rei\u00b7ger\u00b7busch", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}