{"textgrid.poem.32661": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Minnehold an Teuthard", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war kein Schwur; es war ein Blick,", "tokens": ["Es", "war", "kein", "Schwur", ";", "es", "war", "ein", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und drauf ein Druck der Hand,", "tokens": ["Und", "drauf", "ein", "Druck", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der, Freund, im ersten Augenblick,", "tokens": ["Der", ",", "Freund", ",", "im", "ers\u00b7ten", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz an deines band.", "tokens": ["Mein", "Herz", "an", "dei\u00b7nes", "band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Deutsche kennt den Deutschen bald", "tokens": ["Der", "Deut\u00b7sche", "kennt", "den", "Deut\u00b7schen", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am offenen Gesicht,", "tokens": ["Am", "of\u00b7fe\u00b7nen", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Feuer, das vom Auge wallt,", "tokens": ["Am", "Feu\u00b7er", ",", "das", "vom", "Au\u00b7ge", "wallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Ton, worin er spricht.", "tokens": ["Am", "Ton", ",", "wo\u00b7rin", "er", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So kannt' ich dich! Es sprach dein Ton", "tokens": ["So", "kannt'", "ich", "dich", "!", "Es", "sprach", "dein", "Ton"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In wenig Worten viel;", "tokens": ["In", "we\u00b7nig", "Wor\u00b7ten", "viel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem leeren Franzen sprach er Hohn,", "tokens": ["Dem", "lee\u00b7ren", "Fran\u00b7zen", "sprach", "er", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in mein Herz Gef\u00fchl.", "tokens": ["Und", "in", "mein", "Herz", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da war der Bund gemacht! Da schlug", "tokens": ["Da", "war", "der", "Bund", "ge\u00b7macht", "!", "Da", "schlug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz dem deinen zu!", "tokens": ["Mein", "Herz", "dem", "dei\u00b7nen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "K\u00fchn sagt' ich es; denn ohne Trug,", "tokens": ["K\u00fchn", "sagt'", "ich", "es", ";", "denn", "oh\u00b7ne", "Trug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "$.", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und frei bin ich, wie du.", "tokens": ["Und", "frei", "bin", "ich", ",", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun wandl' ich ruhig meinen Gang", "tokens": ["Nun", "wandl'", "ich", "ru\u00b7hig", "mei\u00b7nen", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dir durchs Leben hin,", "tokens": ["Mit", "dir", "durchs", "Le\u00b7ben", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und horch' auf deines Liedes Klang,", "tokens": ["Und", "horch'", "auf", "dei\u00b7nes", "Lie\u00b7des", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Wolken mich umziehn.", "tokens": ["Wenn", "Wol\u00b7ken", "mich", "um\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}