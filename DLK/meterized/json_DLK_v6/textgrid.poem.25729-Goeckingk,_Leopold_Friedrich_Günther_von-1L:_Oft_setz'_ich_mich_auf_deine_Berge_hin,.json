{"textgrid.poem.25729": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft setz' ich mich auf deine Berge hin,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft setz' ich mich auf deine Berge hin,", "tokens": ["Oft", "setz'", "ich", "mich", "auf", "dei\u00b7ne", "Ber\u00b7ge", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und w\u00fchle mit dem Aug' in deinen Sch\u00e4tzen,", "tokens": ["Und", "w\u00fch\u00b7le", "mit", "dem", "Aug'", "in", "dei\u00b7nen", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lasse sich mein Herz an dem Bewu\u00dftseyn letzen,", "tokens": ["Und", "las\u00b7se", "sich", "mein", "Herz", "an", "dem", "Be\u00b7wu\u00df\u00b7tseyn", "let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich gen\u00fcgsam bin.", "tokens": ["Da\u00df", "ich", "ge\u00b7n\u00fcg\u00b7sam", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich den braunen Kohl und Kopfsalat,", "tokens": ["Da\u00df", "ich", "den", "brau\u00b7nen", "Kohl", "und", "Kopf\u00b7sa\u00b7lat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die meine Sch\u00fcssel f\u00fcllen, selbst mir baute;", "tokens": ["Die", "mei\u00b7ne", "Sch\u00fcs\u00b7sel", "f\u00fcl\u00b7len", ",", "selbst", "mir", "bau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df ich den Himmel nie bei Butterbrod und Raute", "tokens": ["Da\u00df", "ich", "den", "Him\u00b7mel", "nie", "bei", "But\u00b7ter\u00b7brod", "und", "Rau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Um etwas mehr noch bat.", "tokens": ["Um", "et\u00b7was", "mehr", "noch", "bat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Da\u00df von dem Saft', der Rhingaus Trauben schwillt,", "tokens": ["Da\u00df", "von", "dem", "Saft'", ",", "der", "Rhin\u00b7gaus", "Trau\u00b7ben", "schwillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auch mir die Winzer ein paar Flaschen pre\u00dften,", "tokens": ["Auch", "mir", "die", "Win\u00b7zer", "ein", "paar", "Fla\u00b7schen", "pre\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Doch ich den Becher nur damit bei Freundschaftsfesten,", "tokens": ["Doch", "ich", "den", "Be\u00b7cher", "nur", "da\u00b7mit", "bei", "Freund\u00b7schafts\u00b7fes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und nie f\u00fcr mich gef\u00fcllt.", "tokens": ["Und", "nie", "f\u00fcr", "mich", "ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Da\u00df man f\u00fcr mich noch keinen Strau\u00df erschlug,", "tokens": ["Da\u00df", "man", "f\u00fcr", "mich", "noch", "kei\u00b7nen", "Strau\u00df", "er\u00b7schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df, wenn sich von den Herden, die dort weiden,", "tokens": ["Da\u00df", ",", "wenn", "sich", "von", "den", "Her\u00b7den", ",", "die", "dort", "wei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PRF", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ein Berg von Wolle th\u00fcrmt, um mich nach Wunsch zu kleiden,", "tokens": ["Ein", "Berg", "von", "Wol\u00b7le", "th\u00fcrmt", ",", "um", "mich", "nach", "Wunsch", "zu", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "$,", "KOUI", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Drei Schafe sind genug.", "tokens": ["Drei", "Scha\u00b7fe", "sind", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "F\u00fcr dieses Beet voll Kohl, die\u00df F\u00e4\u00dfchen Wein,", "tokens": ["F\u00fcr", "die\u00b7ses", "Beet", "voll", "Kohl", ",", "die\u00df", "F\u00e4\u00df\u00b7chen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "NN", "$,", "PDS", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und diese Handvoll Wolle, schlicht' ich bieder", "tokens": ["Und", "die\u00b7se", "Hand\u00b7voll", "Wol\u00b7le", ",", "schlicht'", "ich", "bie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "NE", "$,", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der Menschen H\u00e4ndel, bin ein S\u00e4nger froher Lieder,", "tokens": ["Der", "Men\u00b7schen", "H\u00e4n\u00b7del", ",", "bin", "ein", "S\u00e4n\u00b7ger", "fro\u00b7her", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und karg nur mir allein.", "tokens": ["Und", "karg", "nur", "mir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "O die du meinen Lebensfaden sponnst!", "tokens": ["O", "die", "du", "mei\u00b7nen", "Le\u00b7bens\u00b7fa\u00b7den", "sponnst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Gib, bis er in die Gruft sich einst verlieret,", "tokens": ["Gib", ",", "bis", "er", "in", "die", "Gruft", "sich", "einst", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Mir nur das wenige, was mir als Lohn geb\u00fchret:", "tokens": ["Mir", "nur", "das", "we\u00b7ni\u00b7ge", ",", "was", "mir", "als", "Lohn", "ge\u00b7b\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "PIS", "$,", "PWS", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Denn ich mag nichts umsonst!", "tokens": ["Denn", "ich", "mag", "nichts", "um\u00b7sonst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Und bin ich einst zu kraftlos f\u00fcr mein Joch,", "tokens": ["Und", "bin", "ich", "einst", "zu", "kraft\u00b7los", "f\u00fcr", "mein", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Nur da noch, um zu essen: o so spanne", "tokens": ["Nur", "da", "noch", ",", "um", "zu", "es\u00b7sen", ":", "o", "so", "span\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$.", "FM", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Mich aus! und schenke mir im Harz' die kleinste Tanne", "tokens": ["Mich", "aus", "!", "und", "schen\u00b7ke", "mir", "im", "Ha\u00b7rz'", "die", "kleins\u00b7te", "Tan\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$.", "KON", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Zu einem Sarge noch!", "tokens": ["Zu", "ei\u00b7nem", "Sar\u00b7ge", "noch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Ich will den Bruch um keinen Marmorstein", "tokens": ["Ich", "will", "den", "Bruch", "um", "kei\u00b7nen", "Mar\u00b7mor\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Vermindern. Weg, du Thor, von meinem Grabe,", "tokens": ["Ver\u00b7min\u00b7dern", ".", "Weg", ",", "du", "Thor", ",", "von", "mei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "PPER", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Der keinen Armen fragt, ob er auch Hunger habe?", "tokens": ["Der", "kei\u00b7nen", "Ar\u00b7men", "fragt", ",", "ob", "er", "auch", "Hun\u00b7ger", "ha\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und mir will Urnen weihn!", "tokens": ["Und", "mir", "will", "Ur\u00b7nen", "weihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}