{"textgrid.poem.59463": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das kalte Liebchen", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich ein, mein s\u00fc\u00dfes Sch\u00e4tzchen!", "tokens": ["La\u00df", "mich", "ein", ",", "mein", "s\u00fc\u00b7\u00dfes", "Sch\u00e4tz\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Finster ist mein K\u00e4mmerlein.", "tokens": ["Fins\u00b7ter", "ist", "mein", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, ich finde doch ein Pl\u00e4tzchen.", "tokens": ["Ach", ",", "ich", "fin\u00b7de", "doch", "ein", "Pl\u00e4tz\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Bett ist eng und klein.", "tokens": ["Und", "mein", "Bett", "ist", "eng", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fern komm ich vom weichen Pf\u00fchle.", "tokens": ["Fern", "komm", "ich", "vom", "wei\u00b7chen", "Pf\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, mein Lager ist von Stein.", "tokens": ["Ach", ",", "mein", "La\u00b7ger", "ist", "von", "Stein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Drau\u00dfen ist die Nacht so k\u00fchle.", "tokens": ["Drau\u00b7\u00dfen", "ist", "die", "Nacht", "so", "k\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hier wird's noch viel k\u00fchler sein.", "tokens": ["Hier", "wird's", "noch", "viel", "k\u00fch\u00b7ler", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Sieh! die Sterne schon erblassen.", "tokens": ["Sieh", "!", "die", "Ster\u00b7ne", "schon", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schwerer Schlummer f\u00e4llt mich an. \u2013", "tokens": ["Schwe\u00b7rer", "Schlum\u00b7mer", "f\u00e4llt", "mich", "an", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nun, so will ich schnell dich fassen!", "tokens": ["Nun", ",", "so", "will", "ich", "schnell", "dich", "fas\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "R\u00fchr mich nicht so gl\u00fchend an.", "tokens": ["R\u00fchr", "mich", "nicht", "so", "gl\u00fc\u00b7hend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Fieberschauer mich durchbeben.", "tokens": ["Fie\u00b7ber\u00b7schau\u00b7er", "mich", "durch\u00b7be\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wahnsinn bringt der Toten Ku\u00df. \u2013", "tokens": ["Wahn\u00b7sinn", "bringt", "der", "To\u00b7ten", "Ku\u00df", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Weh! es bricht mein junges Leben!", "tokens": ["Weh", "!", "es", "bricht", "mein", "jun\u00b7ges", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Mit ins Grab hinunter mu\u00df.", "tokens": ["Mit", "ins", "Grab", "hin\u00b7un\u00b7ter", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "APPO", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}