e": "trochaic.tri"}, "line.3": {"text": "Doch du l\u00e4chelst unter Schmerzen:", "tokens": ["Doch", "du", "l\u00e4\u00b7chelst", "un\u00b7ter", "Schmer\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein \u2013 ich liebe dich.", "tokens": ["Nein", "\u2013", "ich", "lie\u00b7be", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}