{"textgrid.poem.62239": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[bittrer als der Tod ist Leben]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bittrer als der Tod ist Leben,", "tokens": ["Bit\u00b7trer", "als", "der", "Tod", "ist", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ein stolzes Herz verletzt, \u2013", "tokens": ["Wenn", "ein", "stol\u00b7zes", "Herz", "ver\u00b7letzt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh', die Furien sich erheben,", "tokens": ["Sieh'", ",", "die", "Fu\u00b7ri\u00b7en", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Deren Stahl die H\u00f6lle wetzt.", "tokens": ["De\u00b7ren", "Stahl", "die", "H\u00f6l\u00b7le", "wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O, der kennt die Seelen wenig,", "tokens": ["O", ",", "der", "kennt", "die", "See\u00b7len", "we\u00b7nig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die besten stille schm\u00e4ht,", "tokens": ["Der", "die", "bes\u00b7ten", "stil\u00b7le", "schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah man jemals einen K\u00f6nig,", "tokens": ["Sah", "man", "je\u00b7mals", "ei\u00b7nen", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der um Schwarzbrot betteln geht?", "tokens": ["Der", "um", "Schwarz\u00b7brot", "bet\u00b7teln", "geht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In dem lichten Sonnenglanze,", "tokens": ["In", "dem", "lich\u00b7ten", "Son\u00b7nen\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelnd durch das Leben hin,", "tokens": ["Wan\u00b7delnd", "durch", "das", "Le\u00b7ben", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaffend, wirkend f\u00fcr das Ganze,", "tokens": ["Schaf\u00b7fend", ",", "wir\u00b7kend", "f\u00fcr", "das", "Gan\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treu mit hei\u00dfem, festem Sinn.", "tokens": ["Treu", "mit", "hei\u00b7\u00dfem", ",", "fes\u00b7tem", "Sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer f\u00fcr jedes Glied des Ganzen,", "tokens": ["Wer", "f\u00fcr", "je\u00b7des", "Glied", "des", "Gan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gerne g\u00e4b' sein Herzensblut,", "tokens": ["Ger\u00b7ne", "g\u00e4b'", "sein", "Her\u00b7zens\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebend, selbst das Tier, die Pflanzen,", "tokens": ["Lie\u00b7bend", ",", "selbst", "das", "Tier", ",", "die", "Pflan\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dessen Herz ist rein und gut.", "tokens": ["Des\u00b7sen", "Herz", "ist", "rein", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bittrer als der Tod ist Leben,", "tokens": ["Bit\u00b7trer", "als", "der", "Tod", "ist", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ein solches Herz verletzt,", "tokens": ["Wenn", "ein", "sol\u00b7ches", "Herz", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh', die Furien sich erheben,", "tokens": ["Sieh'", ",", "die", "Fu\u00b7ri\u00b7en", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Deren Stahl die H\u00f6lle wetzt.", "tokens": ["De\u00b7ren", "Stahl", "die", "H\u00f6l\u00b7le", "wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O, der kennt die Seelen wenig,", "tokens": ["O", ",", "der", "kennt", "die", "See\u00b7len", "we\u00b7nig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die besten wagt zu schm\u00e4h'n,", "tokens": ["Der", "die", "bes\u00b7ten", "wagt", "zu", "schm\u00e4h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah man jemals einen K\u00f6nig,", "tokens": ["Sah", "man", "je\u00b7mals", "ei\u00b7nen", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen K\u00f6nig betteln geh'n!", "tokens": ["Ei\u00b7nen", "K\u00f6\u00b7nig", "bet\u00b7teln", "geh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}