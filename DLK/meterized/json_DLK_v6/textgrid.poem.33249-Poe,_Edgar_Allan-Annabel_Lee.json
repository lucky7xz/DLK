{"textgrid.poem.33249": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "Annabel Lee", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist ein K\u00f6nigreich an des Meeres Strand,", "tokens": ["Ist", "ein", "K\u00f6\u00b7nig\u00b7reich", "an", "des", "Mee\u00b7res", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da war es, da lebte sie \u2013", "tokens": ["Da", "war", "es", ",", "da", "leb\u00b7te", "sie", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lang, lang ist es her \u2013 und sie sei euch genannt", "tokens": ["Lang", ",", "lang", "ist", "es", "her", "\u2013", "und", "sie", "sei", "euch", "ge\u00b7nannt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "VAFIN", "PPER", "PTKVZ", "$(", "KON", "PPER", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Mit dem Namen ", "tokens": ["Mit", "dem", "Na\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und ihr Leben und Denken war ganz gebannt", "tokens": ["Und", "ihr", "Le\u00b7ben", "und", "Den\u00b7ken", "war", "ganz", "ge\u00b7bannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "VAFIN", "ADV", "VVPP"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "In Liebe \u2013 und ", "tokens": ["In", "Lie\u00b7be", "\u2013", "und"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$(", "KON"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "In dem K\u00f6nigreich an des Meeres Strand", "tokens": ["In", "dem", "K\u00f6\u00b7nig\u00b7reich", "an", "des", "Mee\u00b7res", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ein Kind noch war ich und war sie,", "tokens": ["Ein", "Kind", "noch", "war", "ich", "und", "war", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "KON", "VAFIN", "PPER", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch wir liebten mit Liebe, die mehr war denn dies \u2013", "tokens": ["Doch", "wir", "lieb\u00b7ten", "mit", "Lie\u00b7be", ",", "die", "mehr", "war", "denn", "dies", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,", "PRELS", "ADV", "VAFIN", "ADV", "PDS", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ich und meine ", "tokens": ["Ich", "und", "mei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Mit Liebe, da\u00df strahlende Seraphim", "tokens": ["Mit", "Lie\u00b7be", ",", "da\u00df", "strah\u00b7len\u00b7de", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Begehrten mich und sie.", "tokens": ["Be\u00b7gehr\u00b7ten", "mich", "und", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und das war der Grund, da\u00df vor Jahren und Jahr", "tokens": ["Und", "das", "war", "der", "Grund", ",", "da\u00df", "vor", "Jah\u00b7ren", "und", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eine Wolke Winde spie,", "tokens": ["Ei\u00b7ne", "Wol\u00b7ke", "Win\u00b7de", "spie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die frostig durchfuhren am Meeresstrand", "tokens": ["Die", "fros\u00b7tig", "durch\u00b7fuh\u00b7ren", "am", "Mee\u00b7res\u00b7strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Meine sch\u00f6ne ", "tokens": ["Mei\u00b7ne", "sch\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und ihre hochedele Sippe kam,", "tokens": ["Und", "ih\u00b7re", "ho\u00b7che\u00b7de\u00b7le", "Sip\u00b7pe", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ach! man entf\u00fchrte mir sie,", "tokens": ["Und", "ach", "!", "man", "ent\u00b7f\u00fchr\u00b7te", "mir", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PIS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Um sie einzuschlie\u00dfen in Gruft und Grab,", "tokens": ["Um", "sie", "ein\u00b7zu\u00b7schlie\u00b7\u00dfen", "in", "Gruft", "und", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Meine sch\u00f6ne ", "tokens": ["Mei\u00b7ne", "sch\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Die Engel, nicht halb so gl\u00fccklich als wir,", "tokens": ["Die", "En\u00b7gel", ",", "nicht", "halb", "so", "gl\u00fcck\u00b7lich", "als", "wir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ADJD", "ADV", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Waren neidisch auf mich und auf sie \u2013", "tokens": ["Wa\u00b7ren", "nei\u00b7disch", "auf", "mich", "und", "auf", "sie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "KON", "APPR", "PPER", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ja! das war der Grund (und alle im Land", "tokens": ["Ja", "!", "das", "war", "der", "Grund", "(", "und", "al\u00b7le", "im", "Land"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PDS", "VAFIN", "ART", "NN", "$(", "KON", "PIS", "APPRART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Sie wissen, vergessen es nie),", "tokens": ["Sie", "wis\u00b7sen", ",", "ver\u00b7ges\u00b7sen", "es", "nie", ")", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$(", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Da\u00df der Nachtwind so rauh aus der Wolke fuhr", "tokens": ["Da\u00df", "der", "Nacht\u00b7wind", "so", "rauh", "aus", "der", "Wol\u00b7ke", "fuhr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Und mordete ", "tokens": ["Und", "mor\u00b7de\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Weit st\u00e4rker doch war unsre Liebe als die", "tokens": ["Weit", "st\u00e4r\u00b7ker", "doch", "war", "uns\u00b7re", "Lie\u00b7be", "als", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "ADV", "VAFIN", "PPOSAT", "NN", "KOKOM", "ART"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "All derer, die \u00e4lter als wir \u2013", "tokens": ["All", "de\u00b7rer", ",", "die", "\u00e4l\u00b7ter", "als", "wir", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "$,", "PRELS", "ADJD", "KOKOM", "PPER", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und mancher, die weiser als wir \u2013", "tokens": ["Und", "man\u00b7cher", ",", "die", "wei\u00b7ser", "als", "wir", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "ART", "ADJA", "KOUS", "PPER", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und die Engel in H\u00f6hen verm\u00f6gen es nie", "tokens": ["Und", "die", "En\u00b7gel", "in", "H\u00f6\u00b7hen", "ver\u00b7m\u00f6\u00b7gen", "es", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "PPER", "ADV"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Und die Teufel in Tiefen nie,", "tokens": ["Und", "die", "Teu\u00b7fel", "in", "Tie\u00b7fen", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Nie k\u00f6nnen sie trennen die Seelen von mir", "tokens": ["Nie", "k\u00f6n\u00b7nen", "sie", "tren\u00b7nen", "die", "See\u00b7len", "von", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und der sch\u00f6nen ", "tokens": ["Und", "der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Kein Mondenlicht blinkt, das nicht Tr\u00e4ume mir bringt", "tokens": ["Kein", "Mon\u00b7den\u00b7licht", "blinkt", ",", "das", "nicht", "Tr\u00e4u\u00b7me", "mir", "bringt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PRELS", "PTKNEG", "NN", "PPER", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Von der sch\u00f6nen ", "tokens": ["Von", "der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Jedes Sternlein das steigt, hell die Augen mir zeigt", "tokens": ["Je\u00b7des", "Stern\u00b7lein", "das", "steigt", ",", "hell", "die", "Au\u00b7gen", "mir", "zeigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "PDS", "VVFIN", "$,", "ADJD", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Meiner sch\u00f6nen ", "tokens": ["Mei\u00b7ner", "sch\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und so jede Nacht lieg zur Seite ich sacht", "tokens": ["Und", "so", "je\u00b7de", "Nacht", "lieg", "zur", "Sei\u00b7te", "ich", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN", "VVFIN", "APPRART", "NN", "PPER", "VVFIN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Meinem Lieb, meinem Leben in br\u00e4utlicher Pracht:", "tokens": ["Mei\u00b7nem", "Lieb", ",", "mei\u00b7nem", "Le\u00b7ben", "in", "br\u00e4ut\u00b7li\u00b7cher", "Pracht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Im Grabe da k\u00fcsse ich sie \u2013", "tokens": ["Im", "Gra\u00b7be", "da", "k\u00fcs\u00b7se", "ich", "sie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PPER", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Im Grabe da k\u00fcsse ich sie.", "tokens": ["Im", "Gra\u00b7be", "da", "k\u00fcs\u00b7se", "ich", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}