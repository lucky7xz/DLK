{"textgrid.poem.41002": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Graf Asper", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Waldesdunkel steht ein Bronn", "tokens": ["In", "Wal\u00b7des\u00b7dun\u00b7kel", "steht", "ein", "Bronn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Kloster der wei\u00dfen Frauen,", "tokens": ["Beim", "Klos\u00b7ter", "der", "wei\u00b7\u00dfen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Bronn viel hundert Klafter tief", "tokens": ["Der", "Bronn", "viel", "hun\u00b7dert", "Klaf\u00b7ter", "tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "CARD", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Felsen gut gehauen.", "tokens": ["In", "Fel\u00b7sen", "gut", "ge\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sa\u00df auf dem Baum Waldv\u00f6gelein,", "tokens": ["Sa\u00df", "auf", "dem", "Baum", "Wald\u00b7v\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sank auf den Berg die Sonne,", "tokens": ["Sank", "auf", "den", "Berg", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rt an, o h\u00f6rt an! was sich begab", "tokens": ["H\u00f6rt", "an", ",", "o", "h\u00f6rt", "an", "!", "was", "sich", "be\u00b7gab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "VVFIN", "PTKVZ", "$.", "PRELS", "PRF", "VVFIN"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Da bei demselben Bronne.", "tokens": ["Da", "bei", "dem\u00b7sel\u00b7ben", "Bron\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Graf Asper von der Heerfahrt kam,", "tokens": ["Graf", "As\u00b7per", "von", "der", "Heer\u00b7fahrt", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt' k\u00fchlen Trunk sich langen,", "tokens": ["Wollt'", "k\u00fch\u00b7len", "Trunk", "sich", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er trieb wohl um das eiserne Rad,", "tokens": ["Er", "trieb", "wohl", "um", "das", "ei\u00b7ser\u00b7ne", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Ketten hell erklangen.", "tokens": ["Die", "Ket\u00b7ten", "hell", "er\u00b7klan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bum! bum! herauf der Eimer flog,", "tokens": ["Bum", "!", "bum", "!", "her\u00b7auf", "der", "Ei\u00b7mer", "flog", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "XY", "$.", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dumpf t\u00f6nt es in dem Grunde,", "tokens": ["Dumpf", "t\u00f6nt", "es", "in", "dem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein k\u00fchles Wasser in ihm war,", "tokens": ["Kein", "k\u00fch\u00b7les", "Was\u00b7ser", "in", "ihm", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Zwerglein darin stunde.", "tokens": ["Ein", "Zwerg\u00b7lein", "da\u00b7rin", "stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbsteig ein, steig ein, du Recke k\u00fchn!", "tokens": ["\u00bb", "steig", "ein", ",", "steig", "ein", ",", "du", "Re\u00b7cke", "k\u00fchn", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein begehrt mein Herr zur Stunde!\u00ab", "tokens": ["Dein", "be\u00b7gehrt", "mein", "Herr", "zur", "Stun\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "VVPP", "PPOSAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Graf Asper kehrt nicht mehr zur Burg. \u2013", "tokens": ["Graf", "As\u00b7per", "kehrt", "nicht", "mehr", "zur", "Burg", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dumpf t\u00f6nt es in dem Grunde.", "tokens": ["Dumpf", "t\u00f6nt", "es", "in", "dem", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Flog von dem Baum Waldv\u00f6gelein,", "tokens": ["Flog", "von", "dem", "Baum", "Wald\u00b7v\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Stieg \u00fcber den Berg die Sonne,", "tokens": ["Stieg", "\u00fc\u00b7ber", "den", "Berg", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "H\u00f6rt an, o h\u00f6rt an! was sich begab", "tokens": ["H\u00f6rt", "an", ",", "o", "h\u00f6rt", "an", "!", "was", "sich", "be\u00b7gab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "VVFIN", "PTKVZ", "$.", "PRELS", "PRF", "VVFIN"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Da bei demselben Bronne.", "tokens": ["Da", "bei", "dem\u00b7sel\u00b7ben", "Bron\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Eine Klosterjungfrau trat heraus,", "tokens": ["Ei\u00b7ne", "Klos\u00b7ter\u00b7jung\u00b7frau", "trat", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wollt' k\u00fchlen Trunk sich langen,", "tokens": ["Wollt'", "k\u00fch\u00b7len", "Trunk", "sich", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie trieb wohl um das eiserne Rad,", "tokens": ["Sie", "trieb", "wohl", "um", "das", "ei\u00b7ser\u00b7ne", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Ketten hell erklangen.", "tokens": ["Die", "Ket\u00b7ten", "hell", "er\u00b7klan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Bum! bum! herauf der Eimer flog,", "tokens": ["Bum", "!", "bum", "!", "her\u00b7auf", "der", "Ei\u00b7mer", "flog", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "XY", "$.", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dumpf t\u00f6nt' es in dem Grunde,", "tokens": ["Dumpf", "t\u00f6nt'", "es", "in", "dem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein k\u00