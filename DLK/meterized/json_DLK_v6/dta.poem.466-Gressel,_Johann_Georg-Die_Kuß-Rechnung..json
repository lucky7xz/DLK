{"dta.poem.466": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Ku\u00df-Rechnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Gib mir drey K\u00fcsse her/ soll ich nicht dreye haben?", "tokens": ["Gib", "mir", "drey", "K\u00fcs\u00b7se", "her", "/", "soll", "ich", "nicht", "drey\u00b7e", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "CARD", "NN", "PTKVZ", "$(", "VMFIN", "PPER", "PTKNEG", "CARD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So la\u00df es zweene seyn beliebte ", "tokens": ["So", "la\u00df", "es", "zwee\u00b7ne", "seyn", "be\u00b7lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du nimst/ und giebst den Ku\u00df/ wie viel sind nun der Gaben?", "tokens": ["Du", "nimst", "/", "und", "giebst", "den", "Ku\u00df", "/", "wie", "viel", "sind", "nun", "der", "Ga\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "ART", "NN", "$(", "KOKOM", "ADV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gieb mir mein K\u00fc\u00dfgen her/ sieh deiner ist schon da.", "tokens": ["Gieb", "mir", "mein", "K\u00fc\u00df\u00b7gen", "her", "/", "sieh", "dei\u00b7ner", "ist", "schon", "da", "."], "token_info": ["word", "wor