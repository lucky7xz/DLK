{"textgrid.poem.59199": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Schlimme Wahl", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sahst die Fei ihr goldnes Haar sich str\u00e4hlen,", "tokens": ["Du", "sahst", "die", "Fei", "ihr", "gold\u00b7nes", "Haar", "sich", "str\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn morgens fr\u00fch noch alle W\u00e4lder schweigen,", "tokens": ["Wenn", "mor\u00b7gens", "fr\u00fch", "noch", "al\u00b7le", "W\u00e4l\u00b7der", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gar viele da im Felsgrund sich versteigen,", "tokens": ["Gar", "vie\u00b7le", "da", "im", "Fels\u00b7grund", "sich", "ver\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wei\u00df doch keiner, wen sie wird erw\u00e4hlen.", "tokens": ["Und", "wei\u00df", "doch", "kei\u00b7ner", ",", "wen", "sie", "wird", "er\u00b7w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "$,", "PWS", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von einer andern Dam h\u00f6rt ich erz\u00e4hlen", "tokens": ["Von", "ei\u00b7ner", "an\u00b7dern", "Dam", "h\u00f6rt", "ich", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im platten Land, die Bauern rings dir zeigen", "tokens": ["Im", "plat\u00b7ten", "Land", ",", "die", "Bau\u00b7ern", "rings", "dir", "zei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Schlo\u00df, Park, Weiler \u2013 alles ist dein eigen,", "tokens": ["Ihr", "Schlo\u00df", ",", "Park", ",", "Wei\u00b7ler", "\u2013", "al\u00b7les", "ist", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NE", "$(", "PIS", "VAFIN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Freist du das Weib \u2013 wer m\u00f6cht im Wald sich qu\u00e4len!", "tokens": ["Freist", "du", "das", "Weib", "\u2013", "wer", "m\u00f6cht", "im", "Wald", "sich", "qu\u00e4\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "PWS", "VMFIN", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie werden dich auf einen Phaeton heben,", "tokens": ["Sie", "wer\u00b7den", "dich", "auf", "ei\u00b7nen", "Phae\u00b7ton", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Hochzeitskarmen t\u00f6nt, es blinkt die Flasche,", "tokens": ["Das", "Hoch\u00b7zeits\u00b7kar\u00b7men", "t\u00f6nt", ",", "es", "blinkt", "die", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weitrauschend hinterdrein viel vornehm Wesen.", "tokens": ["Weit\u00b7rau\u00b7schend", "hin\u00b7ter\u00b7drein", "viel", "vor\u00b7nehm", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch streift beim Zug dich aus dem Walde eben", "tokens": ["Doch", "streift", "beim", "Zug", "dich", "aus", "dem", "Wal\u00b7de", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PRF", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Feie Blick, und brennt dich nicht zu Asche:", "tokens": ["Der", "Fei\u00b7e", "Blick", ",", "und", "brennt", "dich", "nicht", "zu", "A\u00b7sche", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fahr wohl, bist nimmer ein Poet gewesen!", "tokens": ["Fahr", "wohl", ",", "bist", "nim\u00b7mer", "ein", "Po\u00b7et", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VAFIN", "ADV", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}