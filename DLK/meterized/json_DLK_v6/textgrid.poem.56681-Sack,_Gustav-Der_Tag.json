{"textgrid.poem.56681": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Tag", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wiederum entquoll ein Tag", "tokens": ["Und", "wie\u00b7de\u00b7rum", "ent\u00b7quoll", "ein", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dem alten qualzerri\u00dfnen Scho\u00df der Nacht \u2013", "tokens": ["dem", "al\u00b7ten", "qual\u00b7zer\u00b7ri\u00df\u00b7nen", "Scho\u00df", "der", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und hat sich gleich daran gemacht", "tokens": ["und", "hat", "sich", "gleich", "da\u00b7ran", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PRF", "ADV", "PAV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit Hotteh\u00fc! und Peitschenschlag", "tokens": ["mit", "Hot\u00b7te\u00b7h\u00fc", "!", "und", "Peit\u00b7schen\u00b7schlag"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$.", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "durch alle Stra\u00dfen zu rumoren,", "tokens": ["durch", "al\u00b7le", "Stra\u00b7\u00dfen", "zu", "ru\u00b7mo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "bis sich das Uhrwerk wieder dreht", "tokens": ["bis", "sich", "das", "Uhr\u00b7werk", "wie\u00b7der", "dreht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und alles seine Wege geht;", "tokens": ["und", "al\u00b7les", "sei\u00b7ne", "We\u00b7ge", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dann r\u00e4kelt er sich traumverloren", "tokens": ["dann", "r\u00e4\u00b7kelt", "er", "sich", "traum\u00b7ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "und w\u00e4lzt sich ohne Ziel und Sinn", "tokens": ["und", "w\u00e4lzt", "sich", "oh\u00b7ne", "Ziel", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und faul und grau, ganz \u00fcberfl\u00fcssig", "tokens": ["und", "faul", "und", "grau", ",", "ganz", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und seiner selber \u00fcberdr\u00fcssig", "tokens": ["und", "sei\u00b7ner", "sel\u00b7ber", "\u00fc\u00b7berd\u00b7r\u00fcs\u00b7sig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00fcber die feuchten D\u00e4cher hin;", "tokens": ["\u00fc\u00b7ber", "die", "feuch\u00b7ten", "D\u00e4\u00b7cher", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "und sehnt sich nach der Nacht zur\u00fcck,", "tokens": ["und", "sehnt", "sich", "nach", "der", "Nacht", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in der er weich und brunnentief", "tokens": ["in", "der", "er", "weich", "und", "brun\u00b7nen\u00b7tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sein Nichtsein selig weiterschlief,", "tokens": ["sein", "Nich\u00b7tsein", "se\u00b7lig", "wei\u00b7ter\u00b7schlief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und sehnt sich nach der Nacht zur\u00fcck", "tokens": ["und", "sehnt", "sich", "nach", "der", "Nacht", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "den ganzen Tag, den langen Tag", "tokens": ["den", "gan\u00b7zen", "Tag", ",", "den", "lan\u00b7gen", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sehnt er sich nach der Nacht zur\u00fcck,", "tokens": ["sehnt", "er", "sich", "nach", "der", "Nacht", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "nach ihrem daunenweichen Gl\u00fcck", "tokens": ["nach", "ih\u00b7rem", "dau\u00b7nen\u00b7wei\u00b7chen", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und unh\u00f6rbaren Stundenschlag.", "tokens": ["und", "un\u00b7h\u00f6r\u00b7ba\u00b7ren", "Stun\u00b7den\u00b7schlag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}