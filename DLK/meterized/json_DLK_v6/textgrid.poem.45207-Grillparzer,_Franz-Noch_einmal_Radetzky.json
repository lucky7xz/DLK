{"textgrid.poem.45207": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Noch einmal Radetzky", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wagte Sieg vorauszusagen,", "tokens": ["Ich", "wag\u00b7te", "Sieg", "vor\u00b7aus\u00b7zu\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wunsch trennt sich von Hoffnung nie,", "tokens": ["Der", "Wunsch", "trennt", "sich", "von", "Hoff\u00b7nung", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lie\u00dfest du vom Feind dich schlagen", "tokens": ["Da", "lie\u00b7\u00dfest", "du", "vom", "Feind", "dich", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlugst damit die Poesie.", "tokens": ["Und", "schlugst", "da\u00b7mit", "die", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}