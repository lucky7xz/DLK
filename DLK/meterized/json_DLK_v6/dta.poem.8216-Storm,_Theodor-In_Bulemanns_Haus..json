{"dta.poem.8216": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "In Bulemanns Haus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist die zierliche Kleine,", "tokens": ["Das", "ist", "die", "zier\u00b7li\u00b7che", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die geht auf ihren Pant\u00f6ffelein", "tokens": ["Die", "geht", "auf", "ih\u00b7ren", "Pan\u00b7t\u00f6f\u00b7fe\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Behend und mutterseelen allein", "tokens": ["Be\u00b7hend", "und", "mut\u00b7ter\u00b7see\u00b7len", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "KON", "VMFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Durch die Gassen im Mondenscheine.", "tokens": ["Durch", "die", "Gas\u00b7sen", "im", "Mon\u00b7den\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie geht in ein alt verfallenes Haus;", "tokens": ["Sie", "geht", "in", "ein", "alt", "ver\u00b7fal\u00b7le\u00b7nes", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Flur ist die Tafel gedecket,", "tokens": ["Im", "Flur", "ist", "die", "Ta\u00b7fel", "ge\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da tanzt vor dem Monde die Maus mit der Maus,", "tokens": ["Da", "tanzt", "vor", "dem", "Mon\u00b7de", "die", "Maus", "mit", "der", "Maus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NE", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da setzt sich das Kind mit den M\u00e4usen zu Schmaus,", "tokens": ["Da", "setzt", "sich", "das", "Kind", "mit", "den", "M\u00e4u\u00b7sen", "zu", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Tellerlein werden gelecket.", "tokens": ["Die", "Tel\u00b7ler\u00b7lein", "wer\u00b7den", "ge\u00b7le\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und leer sind die Sch\u00fcsseln; die M\u00e4uslein im Nu", "tokens": ["Und", "leer", "sind", "die", "Sch\u00fcs\u00b7seln", ";", "die", "M\u00e4us\u00b7lein", "im", "Nu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$.", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verrascheln in Mauer und Holze;", "tokens": ["Ver\u00b7ra\u00b7scheln", "in", "Mau\u00b7er", "und", "Hol\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nun l\u00e4\u00dft es dem M\u00e4gdlein auch l\u00e4nger nicht Ruh,", "tokens": ["Nun", "l\u00e4\u00dft", "es", "dem", "M\u00e4gd\u00b7lein", "auch", "l\u00e4n\u00b7ger", "nicht", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "PTKNEG", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie sch\u00fcttelt ihr Kleidchen, sie schn\u00fcrt sich die Schuh,", "tokens": ["Sie", "sch\u00fct\u00b7telt", "ihr", "Kleid\u00b7chen", ",", "sie", "schn\u00fcrt", "sich", "die", "Schuh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Dann tritt sie einher mit Stolze.", "tokens": ["Dann", "tritt", "sie", "ein\u00b7her", "mit", "Stol\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Es leuchtet ein Spiegel aus goldnem Gestell,", "tokens": ["Es", "leuch\u00b7tet", "ein", "Spie\u00b7gel", "aus", "gold\u00b7nem", "Ge\u00b7stell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da schaut sie hinein mit Lachen;", "tokens": ["Da", "schaut", "sie", "hin\u00b7ein", "mit", "La\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gleich schaut auch heraus ein M\u00e4gdelein hell,", "tokens": ["Gleich", "schaut", "auch", "he\u00b7raus", "ein", "M\u00e4g\u00b7del\u00b7ein", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das ist ihr einziger Spielgesell;", "tokens": ["Das", "ist", "ihr", "ein\u00b7zi\u00b7ger", "Spiel\u00b7ge\u00b7sell", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nun woll'n sie sich lustig machen.", "tokens": ["Nun", "woll'n", "sie", "sich", "lus\u00b7tig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie nickt voll Huld, ihr geh\u00f6rt ja das Reich;", "tokens": ["Sie", "nickt", "voll", "Huld", ",", "ihr", "ge\u00b7h\u00f6rt", "ja", "das", "Reich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da neigt sich das Spiegelkindlein,", "tokens": ["Da", "neigt", "sich", "das", "Spie\u00b7gel\u00b7kin\u00b7dlein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da neigt sich das Kind vor dem Spiegel zugleich,", "tokens": ["Da", "neigt", "sich", "das", "Kind", "vor", "dem", "Spie\u00b7gel", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da neigen sich beide gar anmuthreich,", "tokens": ["Da", "nei\u00b7gen", "sich", "bei\u00b7de", "gar", "an\u00b7muth\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIS", "ADV", "ADJD", "$,"], "meter": "-+--+--++-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da l\u00e4cheln die rosigen M\u00fcndlein.", "tokens": ["Da", "l\u00e4\u00b7cheln", "die", "ro\u00b7si\u00b7gen", "M\u00fcnd\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und wie sie l\u00e4cheln, so liebt sich der Fu\u00df,", "tokens": ["Und", "wie", "sie", "l\u00e4\u00b7cheln", ",", "so", "liebt", "sich", "der", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es rauschen die seidenen R\u00f6cklein,", "tokens": ["Es", "rau\u00b7schen", "die", "sei\u00b7de\u00b7nen", "R\u00f6ck\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--++", "measure": "prosodiakos"}, "line.3": {"text": "Die H\u00e4ndchen werfen sich Ku\u00df um Ku\u00df,", "tokens": ["Die", "H\u00e4nd\u00b7chen", "wer\u00b7fen", "sich", "Ku\u00df", "um", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Kind mit dem Kinde nun tanzen mu\u00df,", "tokens": ["Das", "Kind", "mit", "dem", "Kin\u00b7de", "nun", "tan\u00b7zen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Es tanzen im Nacken die L\u00f6cklein.", "tokens": ["Es", "tan\u00b7zen", "im", "Na\u00b7cken", "die", "L\u00f6c\u00b7klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Mond scheint voller und voller herein,", "tokens": ["Der", "Mond", "scheint", "vol\u00b7ler", "und", "vol\u00b7ler", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf dem Estrich gaukeln die Flimmer;", "tokens": ["Auf", "dem", "Est\u00b7rich", "gau\u00b7keln", "die", "Flim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Takte schweben die M\u00e4gdelein,", "tokens": ["Im", "Tak\u00b7te", "schwe\u00b7ben", "die", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bald tauchen sie tief in die Schatten hinein,", "tokens": ["Bald", "tau\u00b7chen", "sie", "tief", "in", "die", "Schat\u00b7ten", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Bald stehn sie in bl\u00e4ulichem Schimmer.", "tokens": ["Bald", "stehn", "sie", "in", "bl\u00e4u\u00b7li\u00b7chem", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Nun sinken die Glieder, nun halten sie an,", "tokens": ["Nun", "sin\u00b7ken", "die", "Glie\u00b7der", ",", "nun", "hal\u00b7ten", "sie", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und athmen aus Herzens Grunde;", "tokens": ["Und", "ath\u00b7men", "aus", "Her\u00b7zens", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie nahen sich sch\u00fcchtern, und beugen sich dann,", "tokens": ["Sie", "na\u00b7hen", "sich", "sch\u00fcch\u00b7tern", ",", "und", "beu\u00b7gen", "sich", "dann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVINF", "$,", "KON", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und knien vor einander, und r\u00fchren sich an", "tokens": ["Und", "kni\u00b7en", "vor", "ein\u00b7an\u00b7der", ",", "und", "r\u00fch\u00b7ren", "sich", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADV", "$,", "KON", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Mit dem zarten unschuldigen Munde.", "tokens": ["Mit", "dem", "zar\u00b7ten", "un\u00b7schul\u00b7di\u00b7gen", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.9": {"line.1": {"text": "Doch m\u00fcde werden die beiden allein", "tokens": ["Doch", "m\u00fc\u00b7de", "wer\u00b7den", "die", "bei\u00b7den", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "PIAT", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von all' der heimlichen Wonne;", "tokens": ["Von", "all'", "der", "heim\u00b7li\u00b7chen", "Won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sehns\u00fcchtig fl\u00fcstert das M\u00e4gdelein:", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "fl\u00fcs\u00b7tert", "das", "M\u00e4g\u00b7del\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "\u201eich mag nicht mehr tanzen im Mondenschein,", "tokens": ["\u201e", "ich", "mag", "nicht", "mehr", "tan\u00b7zen", "im", "Mon\u00b7den\u00b7schein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ach, k\u00e4me doch endlich die Sonne!\u201c", "tokens": ["Ach", ",", "k\u00e4\u00b7me", "doch", "end\u00b7lich", "die", "Son\u00b7ne", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Sie klettert hinunter ein Trepplein schief,", "tokens": ["Sie", "klet\u00b7tert", "hin\u00b7un\u00b7ter", "ein", "Trep\u00b7plein", "schief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und schleicht hinab in den Garten.", "tokens": ["Und", "schleicht", "hin\u00b7ab", "in", "den", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Sonne schlief und die Grille schlief:", "tokens": ["Die", "Son\u00b7ne", "schlief", "und", "die", "Gril\u00b7le", "schlief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u201ehier will ich sitzen im Grase tief,", "tokens": ["\u201e", "hier", "will", "ich", "sit\u00b7zen", "im", "Gra\u00b7se", "tief", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und der Sonne will ich warten.\u201c", "tokens": ["Und", "der", "Son\u00b7ne", "will", "ich", "war\u00b7ten", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch als nun Morgens um Busch und Gestein", "tokens": ["Doch", "als", "nun", "Mor\u00b7gens", "um", "Busch", "und", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verhuschet das D\u00e4mmergemunkel,", "tokens": ["Ver\u00b7hu\u00b7schet", "das", "D\u00e4m\u00b7mer\u00b7ge\u00b7mun\u00b7kel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da werden dem Kinde die Aeugelein klein;", "tokens": ["Da", "wer\u00b7den", "dem", "Kin\u00b7de", "die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie tanzte zu lange bei Mondenschein,", "tokens": ["Sie", "tanz\u00b7te", "zu", "lan\u00b7ge", "bei", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Nun schl\u00e4ft sie bei Sonnengefunkel.", "tokens": ["Nun", "schl\u00e4ft", "sie", "bei", "Son\u00b7nen\u00b7ge\u00b7fun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.12": {"line.1": {"text": "Nun liegt sie zwischen den Blumen dicht", "tokens": ["Nun", "liegt", "sie", "zwi\u00b7schen", "den", "Blu\u00b7men", "dicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf gr\u00fcnem, blitzendem Rasen;", "tokens": ["Auf", "gr\u00fc\u00b7nem", ",", "blit\u00b7zen\u00b7dem", "Ra\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und es schauen ihr in das s\u00fc\u00dfe Gesicht", "tokens": ["Und", "es", "schau\u00b7en", "ihr", "in", "das", "s\u00fc\u00b7\u00dfe", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die Nachtigall und das Sonnenlicht", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "und", "das", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und die kleinen neugierigen Hasen.", "tokens": ["Und", "die", "klei\u00b7nen", "neu\u00b7gie\u00b7ri\u00b7gen", "Ha\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}