{"dta.poem.21788": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n  Liebes-feuer/ ewige Flammen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.85", "fr:0.14"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Du liebst mich/ Schaz/ Rosille/", "tokens": ["Du", "liebst", "mich", "/", "Schaz", "/", "Ro\u00b7sil\u00b7le", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "NN", "$(", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mehr als dein eigen Herz/", "tokens": ["mehr", "als", "dein", "ei\u00b7gen", "Herz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPOSAT", "ADJA", "NN", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Mein Wollen ist dein Wille/", "tokens": ["Mein", "Wol\u00b7len", "ist", "dein", "Wil\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "mein Wiedersinn dein Schmerz", "tokens": ["mein", "Wie\u00b7der\u00b7sinn", "dein", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du schleust mich mit viel k\u00fcssen", "tokens": ["Du", "schleust", "mich", "mit", "viel", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fest in die Armen ein", "tokens": ["Fest", "in", "die", "Ar\u00b7men", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "und l\u00e4sset mich nicht missen", "tokens": ["und", "l\u00e4s\u00b7set", "mich", "nicht", "mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "was nur vergunnt mag sein.", "tokens": ["was", "nur", "ver\u00b7gunnt", "mag", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ist aber di\u00df die Flammen", "tokens": ["Ist", "a\u00b7ber", "di\u00df", "die", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PDS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "zuleschen gnug/ mein Kind/", "tokens": ["zu\u00b7le\u00b7schen", "gnug", "/", "mein", "Kind", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und lodern in den Wind.", "tokens": ["und", "lo\u00b7dern", "in", "den", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Flucht kan Feuer t\u00f6dten/", "tokens": ["Die", "Flucht", "kan", "Feu\u00b7er", "t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "lescht was die Gluht verlezzt:", "tokens": ["lescht", "was", "die", "Gluht", "ver\u00b7lezzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "ART", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Je mehr komm\u2019 ich in N\u00f6hten", "tokens": ["Je", "mehr", "komm'", "ich", "in", "N\u00f6h\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "ie mehr dein Mund mich nezzt.", "tokens": ["ie", "mehr", "dein", "Mund", "mich", "nezzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O dem betr\u00fcbtem Stande!", "tokens": ["O", "dem", "be\u00b7tr\u00fcb\u00b7tem", "Stan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das kr\u00e4nkt mich/ was mich s\u00fc\u00dft/", "tokens": ["das", "kr\u00e4nkt", "mich", "/", "was", "mich", "s\u00fc\u00dft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wird nu der Tau zum Brande", "tokens": ["wird", "nu", "der", "Tau", "zum", "Bran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der durch die Lippen flie\u00dft", "tokens": ["der", "durch", "die", "Lip\u00b7pen", "flie\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die heisse Donner-straalen/", "tokens": ["Die", "heis\u00b7se", "Don\u00b7ner\u00b7straa\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "kan man doch offtermahlen", "tokens": ["kan", "man", "doch", "off\u00b7ter\u00b7mah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "mit Wasser k\u00fchlen ein.", "tokens": ["mit", "Was\u00b7ser", "k\u00fch\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mein unau\u00dfleschlich Feuer", "tokens": ["Mein", "un\u00b7au\u00df\u00b7le\u00b7schlich", "Feu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "erkennet keine Wehr", "tokens": ["er\u00b7ken\u00b7net", "kei\u00b7ne", "Wehr"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "kehm Thetis mir zu steuer", "tokens": ["kehm", "The\u00b7tis", "mir", "zu", "steu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und g\u00f6\u00df\u2019 auff mich ihr Meer.", "tokens": ["und", "g\u00f6\u00df'", "auff", "mich", "ihr", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Jedoch w\u00fcrd\u2019 aus den Wellen", "tokens": ["Je\u00b7doch", "w\u00fcrd'", "aus", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "die Flamme schlagen f\u00fcr/", "tokens": ["die", "Flam\u00b7me", "schla\u00b7gen", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "es w\u00fcrden seine Quellen", "tokens": ["es", "w\u00fcr\u00b7den", "sei\u00b7ne", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "vertr\u00f6gen \u00fcber ihr.", "tokens": ["ver\u00b7tr\u00f6\u00b7gen", "\u00fc\u00b7ber", "ihr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du k\u00f6ntest mir noch mindern/", "tokens": ["Du", "k\u00f6n\u00b7test", "mir", "noch", "min\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mein Seelchen/ diese Brunst", "tokens": ["mein", "Seel\u00b7chen", "/", "die\u00b7se", "Brunst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und seine Gluhten lindern", "tokens": ["und", "sei\u00b7ne", "Gluh\u00b7ten", "lin\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "durch n\u00e4hre Liebes-gunst.", "tokens": ["durch", "n\u00e4h\u00b7re", "Lie\u00b7bes\u00b7gunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Was? n\u00e4her? nicht. Wir kennen", "tokens": ["Was", "?", "n\u00e4\u00b7her", "?", "nicht", ".", "Wir", "ken\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "$.", "ADJD", "$.", "PTKNEG", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der Ehr und Tugend Schein.", "tokens": ["der", "Ehr", "und", "Tu\u00b7gend", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eh wolt\u2019 ich ganz verbrennen/", "tokens": ["Eh", "wolt'", "ich", "ganz", "ver\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "als so geleschet sein.", "tokens": ["als", "so", "ge\u00b7le\u00b7schet", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}