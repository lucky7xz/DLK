{"textgrid.poem.57352": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wunderbar war's, war neu, es geschah, was nie noch geschehn ist!", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wunderbar war's, war neu, es geschah, was nie noch geschehn ist!", "tokens": ["Wun\u00b7der\u00b7bar", "wa\u00b7r's", ",", "war", "neu", ",", "es", "ge\u00b7schah", ",", "was", "nie", "noch", "ge\u00b7schehn", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "$,", "PRELS", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "+---+-+--+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ein Riese sank danieder, und starb;", "tokens": ["Ein", "Rie\u00b7se", "sank", "da\u00b7nie\u00b7der", ",", "und", "starb", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Aber er blieb nicht todt: denn es kam ein Geist, und belebte", "tokens": ["A\u00b7ber", "er", "blieb", "nicht", "todt", ":", "denn", "es", "kam", "ein", "Geist", ",", "und", "be\u00b7leb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADJD", "$.", "KON", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Den Todten wieder. Der richtet sich auf,", "tokens": ["Den", "Tod\u00b7ten", "wie\u00b7der", ".", "Der", "rich\u00b7tet", "sich", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "ART", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Steht, und schauet umher mit Feuerblicken. Die Seele,", "tokens": ["Steht", ",", "und", "schau\u00b7et", "um\u00b7her", "mit", "Feu\u00b7er\u00b7bli\u00b7cken", ".", "Die", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PTKVZ", "APPR", "NN", "$.", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Nun Schatten, umirret ihn, bebet vor ihm.", "tokens": ["Nun", "Schat\u00b7ten", ",", "um\u00b7ir\u00b7ret", "ihn", ",", "be\u00b7bet", "vor", "ihm", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Volk ist der Name des Riesen; des Schattens Namen ist K\u00f6nig;", "tokens": ["Volk", "ist", "der", "Na\u00b7me", "des", "Rie\u00b7sen", ";", "des", "Schat\u00b7tens", "Na\u00b7men", "ist", "K\u00f6\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$.", "ART", "NN", "NN", "VAFIN", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Des Geistes Nazionalassamblec.", "tokens": ["Des", "Geis\u00b7tes", "Na\u00b7zi\u00b7o\u00b7nal\u00b7as\u00b7sam\u00b7blec", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Aber du bist ja so wild! so sprach der Geist zu dem Riesen,", "tokens": ["A\u00b7ber", "du", "bist", "ja", "so", "wild", "!", "so", "sprach", "der", "Geist", "zu", "dem", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Dir siedet zu heiss in der Ader das Blut!", "tokens": ["Dir", "sie\u00b7det", "zu", "heiss", "in", "der", "A\u00b7der", "das", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Str\u00f6mt die Galle zu loh! Du musst mir gehorchen. So will es", "tokens": ["Str\u00f6mt", "die", "Gal\u00b7le", "zu", "loh", "!", "Du", "musst", "mir", "ge\u00b7hor\u00b7chen", ".", "So", "will", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$.", "ADV", "VMFIN", "PPER"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Die Weisheit, welche nur gl\u00fccklich uns macht;", "tokens": ["Die", "Weis\u00b7heit", ",", "wel\u00b7che", "nur", "gl\u00fcck\u00b7lich", "uns", "macht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Will es die Harmonie, so zwischen dem Geist' und dem Leib' ist,", "tokens": ["Will", "es", "die", "Har\u00b7mo\u00b7nie", ",", "so", "zwi\u00b7schen", "dem", "Geist'", "und", "dem", "Leib'", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "KON", "ART", "NN", "VAFIN", "$,"], "meter": "---+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und ohne die du zum zweytenmal stirbst!", "tokens": ["Und", "oh\u00b7ne", "die", "du", "zum", "zwey\u00b7ten\u00b7mal", "stirbst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PPER", "APPRART", "ADV", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Ach, und wer wird dann das zweytemal in das Leben", "tokens": ["Ach", ",", "und", "wer", "wird", "dann", "das", "zwey\u00b7te\u00b7mal", "in", "das", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "PWS", "VAFIN", "ADV", "ART", "ADV", "APPR", "ART", "NN"], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Dich rufen? von neuem Retter dir seyn?", "tokens": ["Dich", "ru\u00b7fen", "?", "von", "neu\u00b7em", "Ret\u00b7ter", "dir", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "APPR", "ADJA", "NN", "PPER", "VAINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "\u00bbgeist, gebeut! ich gehorche. Doch lass zuvor mich ein wenig", "tokens": ["\u00bb", "geist", ",", "ge\u00b7beut", "!", "ich", "ge\u00b7hor\u00b7che", ".", "Doch", "lass", "zu\u00b7vor", "mich", "ein", "we\u00b7nig"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVPP", "$,", "VVFIN", "$.", "PPER", "VVFIN", "$.", "KON", "VVFIN", "ADV", "PPER", "ART", "PIS"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Der Jugend mich freuen, die du mir gabst.", "tokens": ["Der", "Ju\u00b7gend", "mich", "freu\u00b7en", ",", "die", "du", "mir", "gabst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Wankt' ich nicht siech umher? lag schmachtend und bleich auf dem Strohe,", "tokens": ["Wankt'", "ich", "nicht", "siech", "um\u00b7her", "?", "lag", "schmach\u00b7tend", "und", "bleich", "auf", "dem", "Stro\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "PTKVZ", "$.", "VVFIN", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Und starb? Du hast den Jammer gesehn!", "tokens": ["Und", "starb", "?", "Du", "hast", "den", "Jam\u00b7mer", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Lass denn ein wenig mich taumeln beym Wollustmahle der Freyheit,", "tokens": ["Lass", "denn", "ein", "we\u00b7nig", "mich", "tau\u00b7meln", "beym", "Wol\u00b7lust\u00b7mah\u00b7le", "der", "Frey\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "PIS", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Mich kr\u00e4nzen mein Haar, und schw\u00f6ren beym Schwert!", "tokens": ["Mich", "kr\u00e4n\u00b7zen", "mein", "Haar", ",", "und", "schw\u00f6\u00b7ren", "beym", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Doch der verstummende Schatten, der einst mir Seele war, schwebet", "tokens": ["Doch", "der", "ver\u00b7stum\u00b7men\u00b7de", "Schat\u00b7ten", ",", "der", "einst", "mir", "See\u00b7le", "war", ",", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PPER", "NN", "VAFIN", "$,", "VVFIN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "So traurig vor mir, und tr\u00f6stet sich nicht!", "tokens": ["So", "trau\u00b7rig", "vor", "mir", ",", "und", "tr\u00f6s\u00b7tet", "sich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "$,", "KON", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Ginge wohl lieber hinab zu Elysiens Schatten, und sch\u00f6pfte", "tokens": ["Gin\u00b7ge", "wohl", "lie\u00b7ber", "hin\u00b7ab", "zu", "E\u00b7ly\u00b7si\u00b7ens", "Schat\u00b7ten", ",", "und", "sch\u00f6pf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "APPR", "NE", "NN", "$,", "KON", "VVFIN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Aus Lethe's Strome den labenden Trunk.", "tokens": ["Aus", "Lethe's", "Stro\u00b7me", "den", "la\u00b7ben\u00b7den", "Trunk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.27": {"text": "Sage, was soll ich thun, dass ich des W\u00fcnschenden Kummer", "tokens": ["Sa\u00b7ge", ",", "was", "soll", "ich", "thun", ",", "dass", "ich", "des", "W\u00fcn\u00b7schen\u00b7den", "Kum\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Bes\u00e4nftige? mindre des Zagenden Angst?\u00ab", "tokens": ["Be\u00b7s\u00e4nf\u00b7ti\u00b7ge", "?", "mind\u00b7re", "des", "Za\u00b7gen\u00b7den", "Angst", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.29": {"text": "Schweben muss er vor dir! so wills die Klugheit. Auch hat er", "tokens": ["Schwe\u00b7ben", "muss", "er", "vor", "dir", "!", "so", "wills", "die", "Klug\u00b7heit", ".", "Auch", "hat", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPER", "$.", "ADV", "VMFIN", "ART", "NN", "$.", "ADV", "VAFIN", "PPER"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Noch sonst wo ein grosses, ernstes Gesch\u00e4ft:", "tokens": ["Noch", "sonst", "wo", "ein", "gros\u00b7ses", ",", "erns\u00b7tes", "Ge\u00b7sch\u00e4ft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PWAV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Wandelt um Mitternacht in der K\u00f6nige Schl\u00f6ssern; dann wehklagt's,", "tokens": ["Wan\u00b7delt", "um", "Mit\u00b7ter\u00b7nacht", "in", "der", "K\u00f6\u00b7ni\u00b7ge", "Schl\u00f6s\u00b7sern", ";", "dann", "weh\u00b7klagt's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "NN", "$.", "ADV", "VVFIN", "$,"], "meter": "+--+-+--+--+-+-+", "measure": "iambic.septa.invert"}, "line.32": {"text": "Als fl\u00f6sse die blutige Thr\u00e4ne des Volks!", "tokens": ["Als", "fl\u00f6s\u00b7se", "die", "blu\u00b7ti\u00b7ge", "Thr\u00e4\u00b7ne", "des", "Volks", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.33": {"text": "Klingt's mit der Krone, als fiele sie ab! mit dem Zepter, als br\u00e4ch' er!", "tokens": ["Klingt's", "mit", "der", "Kro\u00b7ne", ",", "als", "fie\u00b7le", "sie", "ab", "!", "mit", "dem", "Zep\u00b7ter", ",", "als", "br\u00e4ch'", "er", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "(die horchenden, blassen H\u00f6flinge graut!)", "tokens": ["(", "die", "hor\u00b7chen\u00b7den", ",", "blas\u00b7sen", "H\u00f6f\u00b7lin\u00b7ge", "graut", "!", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.35": {"text": "Komt stets n\u00e4her! schliesst den grossen goldenen Saal auf,", "tokens": ["Komt", "stets", "n\u00e4\u00b7her", "!", "schliesst", "den", "gros\u00b7sen", "gol\u00b7de\u00b7nen", "Saal", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$.", "VVFIN", "ART", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.36": {"text": "Und r\u00fcttelt am Thron', ein warnend Gespenst!", "tokens": ["Und", "r\u00fct\u00b7telt", "am", "Thron'", ",", "ein", "war\u00b7nend", "Ge\u00b7spenst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "ART", "VVPP", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}