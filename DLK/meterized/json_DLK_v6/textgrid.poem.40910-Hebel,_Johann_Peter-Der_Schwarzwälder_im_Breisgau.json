{"textgrid.poem.40910": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Schwarzw\u00e4lder im Breisgau", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Z'M\u00fcllen an der Post,", "tokens": ["Z'\u00b7M\u00fcl\u00b7len", "an", "der", "Post", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Tausigsappermost!", "tokens": ["Tau\u00b7sigs\u00b7ap\u00b7per\u00b7most", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Trinkt me nit e gute Wi!", "tokens": ["Trinkt", "me", "nit", "e", "gu\u00b7te", "Wi", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PTKNEG", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Goht er nit wie Baum\u00f6l i,", "tokens": ["Goht", "er", "nit", "wie", "Bau\u00b7m\u00f6l", "i", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "KOKOM", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "z'M\u00fcllen an der Post!", "tokens": ["z'\u00b7M\u00fcl\u00b7len", "an", "der", "Post", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Z'B\u00fcrglen uf der H\u00f6h,", "tokens": ["Z'\u00b7B\u00fcr\u00b7glen", "uf", "der", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "nei, was cha me seh!", "tokens": ["nei", ",", "was", "cha", "me", "seh", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "FM", "FM", "FM", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "O, wie wechsle Berg und Tal,", "tokens": ["O", ",", "wie", "wechs\u00b7le", "Berg", "und", "Tal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Land und Wasser \u00fcberal,", "tokens": ["Land", "und", "Was\u00b7ser", "\u00fc\u00b7be\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "z'B\u00fcrglen uf der H\u00f6h!", "tokens": ["z'\u00b7B\u00fcr\u00b7glen", "uf", "der", "H\u00f6h", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Z'Staufen uffem M\u00e4rt", "tokens": ["Z'\u00b7Stau\u00b7fen", "uf\u00b7fem", "M\u00e4rt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "hen si, was me gert,", "tokens": ["hen", "si", ",", "was", "me", "gert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWS", "VVFIN", "VVPP", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.13": {"text": "Tanz und Wi und Lustberkeit,", "tokens": ["Tanz", "und", "Wi", "und", "Lust\u00b7ber\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "was eim numme 's Herz erfreut,", "tokens": ["was", "eim", "num\u00b7me", "'s", "Herz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "z'Staufen uffem M\u00e4rt!", "tokens": ["z'\u00b7Stau\u00b7fen", "uf\u00b7fem", "M\u00e4rt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Z'Friburg in der Stadt", "tokens": ["Z'\u00b7F\u00b7ri\u00b7burg", "in", "der", "Stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "sufer isch's und glatt,", "tokens": ["su\u00b7fer", "isch's", "und", "glatt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "richi Here, Geld und Gut,", "tokens": ["ri\u00b7chi", "He\u00b7re", ",", "Geld", "und", "Gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Jumpfere wie Milch und Blut,", "tokens": ["Jump\u00b7fe\u00b7re", "wie", "Milch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "z'Friburg in der Stadt.", "tokens": ["z'\u00b7F\u00b7ri\u00b7burg", "in", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Woni gang und stand,", "tokens": ["Wo\u00b7ni", "gang", "und", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "$,"], "meter": "----+", "measure": "unknown.measure.single"}, "line.22": {"text": "w\u00e4r's e lustig Land.", "tokens": ["w\u00e4r's", "e", "lus\u00b7tig", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Aber zeig mer, was de witt,", "tokens": ["A\u00b7ber", "zeig", "mer", ",", "was", "de", "witt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "numme n\u00e4umis findi nit", "tokens": ["num\u00b7me", "n\u00e4u\u00b7mis", "fin\u00b7di", "nit"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "in dem sch\u00f6ne Land.", "tokens": ["in", "dem", "sch\u00f6\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.26": {"text": "Minen Auge gfallt", "tokens": ["Mi\u00b7nen", "Au\u00b7ge", "gfallt"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.27": {"text": "Herischried im Wald.", "tokens": ["He\u00b7rischried", "im", "Wald", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Woni gang, se denki dra,", "tokens": ["Wo\u00b7ni", "gang", ",", "se", "den\u00b7ki", "dra", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "'s chunnt mer nit uf d'Gegnig a", "tokens": ["'s", "chunnt", "mer", "nit", "uf", "d'\u00b7Geg\u00b7nig", "a"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "NE", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "z'Herischried im Wald.", "tokens": ["z'\u00b7He\u00b7ri\u00b7schried", "im", "Wald", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Imme chleine Huus", "tokens": ["Im\u00b7me", "chlei\u00b7ne", "Huus"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.32": {"text": "wandelt i und us \u2013", "tokens": ["wan\u00b7delt", "i", "und", "us", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NE", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.33": {"text": "gelt, de meinsch, i sagder, wer?", "tokens": ["gelt", ",", "de", "meinsch", ",", "i", "sag\u00b7der", ",", "wer", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "NE", "ADJD", "$,", "FM", "FM", "$,", "PWS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "'s isch e Sie, es isch kei Er", "tokens": ["'s", "isch", "e", "Sie", ",", "es", "isch", "kei", "Er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN", "PPER", "$,", "PPER", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "imme chleine Huus.", "tokens": ["im\u00b7me", "chlei\u00b7ne", "Huus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}