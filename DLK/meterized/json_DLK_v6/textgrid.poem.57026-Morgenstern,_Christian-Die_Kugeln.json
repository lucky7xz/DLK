{"textgrid.poem.57026": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Kugeln", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmstr\u00f6m nimmt Papier aus seinem Schube.", "tokens": ["Palm\u00b7str\u00f6m", "nimmt", "Pa\u00b7pier", "aus", "sei\u00b7nem", "Schu\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und verteilt es kunstvoll in der Stube.", "tokens": ["Und", "ver\u00b7teilt", "es", "kunst\u00b7voll", "in", "der", "Stu\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und nachdem er Kugeln draus gemacht.", "tokens": ["Und", "nach\u00b7dem", "er", "Ku\u00b7geln", "draus", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "PAV", "VVPP", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Und verteilt es kunstvoll, und zur Nacht.", "tokens": ["Und", "ver\u00b7teilt", "es", "kunst\u00b7voll", ",", "und", "zur", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und verteilt die Kugeln so (zur Nacht),", "tokens": ["Und", "ver\u00b7teilt", "die", "Ku\u00b7geln", "so", "(", "zur", "Nacht", ")", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$(", "APPRART", "NN", "$(", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "da\u00df er, wenn er pl\u00f6tzlich nachts erwacht,", "tokens": ["da\u00df", "er", ",", "wenn", "er", "pl\u00f6tz\u00b7lich", "nachts", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "da\u00df er, wenn er nachts erwacht, die Kugeln", "tokens": ["da\u00df", "er", ",", "wenn", "er", "nachts", "er\u00b7wacht", ",", "die", "Ku\u00b7geln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "knistern h\u00f6rt und ihn ein heimlich Grugeln", "tokens": ["knis\u00b7tern", "h\u00f6rt", "und", "ihn", "ein", "heim\u00b7lich", "Gru\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "KON", "PPER", "ART", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "packt (da\u00df ihn dann nachts ein heimlich Grugeln", "tokens": ["packt", "(", "da\u00df", "ihn", "dann", "nachts", "ein", "heim\u00b7lich", "Gru\u00b7geln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "ADV", "APPR", "ART", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "packt) beim Spuk der packpapiernen Kugeln ...", "tokens": ["packt", ")", "beim", "Spuk", "der", "pack\u00b7pa\u00b7pier\u00b7nen", "Ku\u00b7geln", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}