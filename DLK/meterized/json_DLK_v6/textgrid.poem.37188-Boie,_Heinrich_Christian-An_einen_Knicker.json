{"textgrid.poem.37188": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "An einen Knicker", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wart, ich werde mich r\u00e4chen, Freund Lupercus,", "tokens": ["Wart", ",", "ich", "wer\u00b7de", "mich", "r\u00e4\u00b7chen", ",", "Freund", "Lu\u00b7per\u00b7cus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$,", "NN", "NE", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da\u00df Du ohne mich einzuladen schmausest!", "tokens": ["Da\u00df", "Du", "oh\u00b7ne", "mich", "ein\u00b7zu\u00b7la\u00b7den", "schmau\u00b7sest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVIZU", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "K\u00fcnftig n\u00f6thige, fleh und schicke neunmal,", "tokens": ["K\u00fcnf\u00b7tig", "n\u00f6\u00b7thi\u00b7ge", ",", "fleh", "und", "schi\u00b7cke", "neun\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Neunmal werd ich im Zorn \u2013 und was denn? \u2013 kommen!", "tokens": ["Neun\u00b7mal", "werd", "ich", "im", "Zorn", "\u2013", "und", "was", "denn", "?", "\u2013", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "$(", "KON", "PWS", "ADV", "$.", "$(", "VVINF", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}}}}