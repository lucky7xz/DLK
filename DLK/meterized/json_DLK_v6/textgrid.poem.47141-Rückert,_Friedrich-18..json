{"textgrid.poem.47141": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O ihr Nachtgestirn' am blauen Himmelszelt,", "tokens": ["O", "ihr", "Nacht\u00b7ge\u00b7stirn'", "am", "blau\u00b7en", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die ihr wandelt, ohn' euch zu verirren!", "tokens": ["Die", "ihr", "wan\u00b7delt", ",", "ohn'", "euch", "zu", "ver\u00b7ir\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nur dem Menschen ist's gegeben, Gottes Welt", "tokens": ["Nur", "dem", "Men\u00b7schen", "ist's", "ge\u00b7ge\u00b7ben", ",", "Got\u00b7tes", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "$,", "NN", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Liebend, hassend, strebend zu verwirren.", "tokens": ["Lie\u00b7bend", ",", "has\u00b7send", ",", "stre\u00b7bend", "zu", "ver\u00b7wir\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}