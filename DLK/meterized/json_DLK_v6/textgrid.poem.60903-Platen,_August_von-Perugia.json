{"textgrid.poem.60903": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Perugia", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchle verleiht in den Tagen der Sonne das steile Perugia;", "tokens": ["K\u00fch\u00b7le", "ver\u00b7leiht", "in", "den", "Ta\u00b7gen", "der", "Son\u00b7ne", "das", "stei\u00b7le", "Pe\u00b7ru\u00b7gia", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "Doch in den Tagen des Sturms scheint es des \u00c4olus Herd.", "tokens": ["Doch", "in", "den", "Ta\u00b7gen", "des", "Sturms", "scheint", "es", "des", "\u00c4\u00b7o\u00b7lus", "Herd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}}}}