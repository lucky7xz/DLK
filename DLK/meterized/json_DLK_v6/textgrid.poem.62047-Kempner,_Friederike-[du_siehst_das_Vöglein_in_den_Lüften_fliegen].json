{"textgrid.poem.62047": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[du siehst das V\u00f6glein in den L\u00fcften fliegen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du siehst das V\u00f6glein in den L\u00fcften fliegen,", "tokens": ["Du", "siehst", "das", "V\u00f6\u00b7glein", "in", "den", "L\u00fcf\u00b7ten", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die kleinen Bl\u00fcten auf dem Rasenland,", "tokens": ["Die", "klei\u00b7nen", "Bl\u00fc\u00b7ten", "auf", "dem", "Ra\u00b7sen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die K\u00e4fer schwirrend in den blauen L\u00fcften", "tokens": ["Die", "K\u00e4\u00b7fer", "schwir\u00b7rend", "in", "den", "blau\u00b7en", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mitten hin des Stromes Silberband!", "tokens": ["Und", "mit\u00b7ten", "hin", "des", "Stro\u00b7mes", "Sil\u00b7ber\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O freue Dich! denn nur durch Wunder", "tokens": ["O", "freu\u00b7e", "Dich", "!", "denn", "nur", "durch", "Wun\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schwebt in der Luft das V\u00f6gelein,", "tokens": ["Schwebt", "in", "der", "Luft", "das", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bl\u00fcten und der Strom sind Wunder!", "tokens": ["Die", "Bl\u00fc\u00b7ten", "und", "der", "Strom", "sind", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Wunder sind, mu\u00df Gottes Tempel sein! \u2013", "tokens": ["Wo", "Wun\u00b7der", "sind", ",", "mu\u00df", "Got\u00b7tes", "Tem\u00b7pel", "sein", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$,", "VMFIN", "NN", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}