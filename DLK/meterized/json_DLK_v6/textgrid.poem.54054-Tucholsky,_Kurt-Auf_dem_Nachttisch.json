{"textgrid.poem.54054": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Nachttisch", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was aber einmal gesagt und gefragt werden mu\u00df, ist dies:", "tokens": ["Was", "a\u00b7ber", "ein\u00b7mal", "ge\u00b7sagt", "und", "ge\u00b7fragt", "wer\u00b7den", "mu\u00df", ",", "ist", "dies", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVPP", "KON", "VVPP", "VAINF", "VMFIN", "$,", "VAFIN", "PDS", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wer ist der Amalthea-Verlag?", "tokens": ["Wer", "ist", "der", "A\u00b7malthe\u00b7a\u00b7Ver\u00b7lag", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch hat alles seine Grenzen, und ein Verlag ist f\u00fcr seine Autoren verantwortlich.", "tokens": ["Doch", "hat", "al\u00b7les", "sei\u00b7ne", "Gren\u00b7zen", ",", "und", "ein", "Ver\u00b7lag", "ist", "f\u00fcr", "sei\u00b7ne", "Au\u00b7to\u00b7ren", "ver\u00b7ant\u00b7wort\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "PPOSAT", "NN", "$,", "KON", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "--+-+-+--+-+--+--+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und wenn ein Verlag wagt, ein Buch zu drucken, das diesen Anwurf hier enth\u00e4lt:", "tokens": ["Und", "wenn", "ein", "Ver\u00b7lag", "wagt", ",", "ein", "Buch", "zu", "dru\u00b7cken", ",", "das", "die\u00b7sen", "An\u00b7wurf", "hier", "ent\u00b7h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+-+--+-+-+-+", "measure": "amphibrach.tri.plus"}}}}}