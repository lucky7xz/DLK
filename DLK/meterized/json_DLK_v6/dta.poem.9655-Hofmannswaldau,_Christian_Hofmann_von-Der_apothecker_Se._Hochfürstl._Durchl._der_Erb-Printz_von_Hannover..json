{"dta.poem.9655": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der apothecker/ Se. Hochf\u00fcrstl. Durchl. der  \n Erb-Printz von Hannover.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Printz/ so der schwadronen-orden", "tokens": ["Ein", "Printz", "/", "so", "der", "schwa\u00b7dro\u00b7nen\u00b7or\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gar offt gef\u00fchrt in kampff und feld/", "tokens": ["Gar", "offt", "ge\u00b7f\u00fchrt", "in", "kampff", "und", "feld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist heut ein apothecker worden/", "tokens": ["Ist", "heut", "ein", "a\u00b7pot\u00b7hec\u00b7ker", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil es das lo\u00df so angestellt.", "tokens": ["Weil", "es", "das", "lo\u00df", "so", "an\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was aber zeigt des gl\u00fcckes tand/", "tokens": ["Was", "a\u00b7ber", "zeigt", "des", "gl\u00fc\u00b7ckes", "tand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das sonst die menschen pflegt zu \u00e4ffen?", "tokens": ["Das", "sonst", "die", "men\u00b7schen", "pflegt", "zu", "\u00e4f\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als da\u00df des klugen Printzens hand", "tokens": ["Als", "da\u00df", "des", "klu\u00b7gen", "Print\u00b7zens", "hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In allem kan die dose treffen.", "tokens": ["In", "al\u00b7lem", "kan", "die", "do\u00b7se", "tref\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VMFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}