{"dta.poem.4414": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein klares Wasser.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich in Deinem sch\u00f6nen Kleide", "tokens": ["Da", "ich", "in", "Dei\u00b7nem", "sch\u00f6\u00b7nen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein bl\u00f6mourant, ein sterbend Blau,", "tokens": ["Ein", "bl\u00f6\u00b7mou\u00b7rant", ",", "ein", "ster\u00b7bend", "Blau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie man es nennet, schau;", "tokens": ["Wie", "man", "es", "nen\u00b7net", ",", "schau", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Gedenk\u2019 ich zwar ans Sterben: Doch dabey", "tokens": ["Ge\u00b7denk'", "ich", "zwar", "ans", "Ster\u00b7ben", ":", "Doch", "da\u00b7bey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$.", "KON", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Empfind\u2019 ich eine s\u00fcsse Freude,", "tokens": ["Emp\u00b7find'", "ich", "ei\u00b7ne", "s\u00fcs\u00b7se", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Indem so Herz als Auge findet,", "tokens": ["In\u00b7dem", "so", "Herz", "als", "Au\u00b7ge", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie so genau und liebreich sich", "tokens": ["Wie", "so", "ge\u00b7nau", "und", "lieb\u00b7reich", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit einem sterbenden und blassen Blauen,", "tokens": ["Mit", "ei\u00b7nem", "ster\u00b7ben\u00b7den", "und", "blas\u00b7sen", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das wir auf deinen Bl\u00e4ttern schauen,", "tokens": ["Das", "wir", "auf", "dei\u00b7nen", "Bl\u00e4t\u00b7tern", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Himmels sch\u00f6nes Blau zugleich verbindet.", "tokens": ["Des", "Him\u00b7mels", "sch\u00f6\u00b7nes", "Blau", "zu\u00b7gleich", "ver\u00b7bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}