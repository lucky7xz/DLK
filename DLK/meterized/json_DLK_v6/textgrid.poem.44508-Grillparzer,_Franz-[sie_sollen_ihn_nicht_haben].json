{"textgrid.poem.44508": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[sie sollen ihn nicht haben]", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sollen ihn nicht haben,", "tokens": ["Sie", "sol\u00b7len", "ihn", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den gr\u00fcnen Donaustrand,", "tokens": ["Den", "gr\u00fc\u00b7nen", "Do\u00b7naus\u00b7trand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da, wo die Ufer ragen", "tokens": ["Da", ",", "wo", "die", "U\u00b7fer", "ra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins Meer vom nahen Land.", "tokens": ["Ins", "Meer", "vom", "na\u00b7hen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sollen dort nicht horsten", "tokens": ["Sie", "sol\u00b7len", "dort", "nicht", "hors\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit langverhehltem Groll,", "tokens": ["Mit", "lang\u00b7ver\u00b7hehl\u00b7tem", "Groll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Flei\u00df und Bildung fordern", "tokens": ["Von", "Flei\u00df", "und", "Bil\u00b7dung", "for\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Roheit w\u00fcsten Zoll.", "tokens": ["Der", "Ro\u00b7heit", "w\u00fcs\u00b7ten", "Zoll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was soll der leichte Franke?", "tokens": ["Was", "soll", "der", "leich\u00b7te", "Fran\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er denkt und droht ja laut,", "tokens": ["Er", "denkt", "und", "droht", "ja", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eh Tat noch der Gedanke,", "tokens": ["Eh", "Tat", "noch", "der", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat l\u00e4ngst man vorgebaut.", "tokens": ["Hat", "l\u00e4ngst", "man", "vor\u00b7ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch jene d\u00fcstern Schergen,", "tokens": ["Doch", "je\u00b7ne", "d\u00fcs\u00b7tern", "Scher\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die unterm Kleid den Stahl,", "tokens": ["Die", "un\u00b7term", "Kleid", "den", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Ha\u00df im Busen bergen,", "tokens": ["Den", "Ha\u00df", "im", "Bu\u00b7sen", "ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die f\u00fcrchte du zumal,", "tokens": ["Die", "f\u00fcrch\u00b7te", "du", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die zwar mit Eisen k\u00e4mpfen,", "tokens": ["Die", "zwar", "mit", "Ei\u00b7sen", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch fr\u00fcher auch mit Gold,", "tokens": ["Doch", "fr\u00fc\u00b7her", "auch", "mit", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Wahrer deines Heiles", "tokens": ["Den", "Wah\u00b7rer", "dei\u00b7nes", "Hei\u00b7les"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Halten in ihrem Sold.", "tokens": ["Hal\u00b7ten", "in", "ih\u00b7rem", "Sold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Die seien erst begraben!", "tokens": ["Die", "sei\u00b7en", "erst", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Waffen in die Hand!", "tokens": ["Die", "Waf\u00b7fen", "in", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sollen ihn nicht haben,", "tokens": ["Sie", "sol\u00b7len", "ihn", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den gr\u00fcnen Donaustrand!", "tokens": ["Den", "gr\u00fc\u00b7nen", "Do\u00b7naus\u00b7trand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}