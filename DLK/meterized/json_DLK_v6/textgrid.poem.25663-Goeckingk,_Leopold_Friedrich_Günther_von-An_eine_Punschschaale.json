{"textgrid.poem.25663": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "An eine Punschschaale", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dir liegt manche Sp\u00f6tterey,", "tokens": ["In", "dir", "liegt", "man\u00b7che", "Sp\u00f6t\u00b7te\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und manche Schmeicheley verborgen.", "tokens": ["Und", "man\u00b7che", "Schmei\u00b7che\u00b7ley", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Zunge machst du Bl\u00f6den frei,", "tokens": ["Die", "Zun\u00b7ge", "machst", "du", "Bl\u00f6\u00b7den", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast Gegengift f\u00fcr alle Sorgen.", "tokens": ["Hast", "Ge\u00b7gen\u00b7gift", "f\u00fcr", "al\u00b7le", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gib mir durch erstes keinen Feind,", "tokens": ["Gib", "mir", "durch", "ers\u00b7tes", "kei\u00b7nen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durchs zweite, bitt' ich, keinen Freund,", "tokens": ["Durchs", "zwei\u00b7te", ",", "bitt'", "ich", ",", "kei\u00b7nen", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "VVFIN", "PPER", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durchs dritte, keines M\u00e4dchens Kranz,", "tokens": ["Durchs", "drit\u00b7te", ",", "kei\u00b7nes", "M\u00e4d\u00b7chens", "Kranz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So schenk' ich dir das vierte ganz.", "tokens": ["So", "schenk'", "ich", "dir", "das", "vier\u00b7te", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}