{"dta.poem.21274": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ptolom\u00e4us Evergetes  \n und Berenice.  \n 1765.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "O Berenice! sch\u00f6ner als der Morgen,", "tokens": ["O", "Be\u00b7re\u00b7ni\u00b7ce", "!", "sch\u00f6\u00b7ner", "als", "der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr mich geboren, lange mir verborgen,", "tokens": ["F\u00fcr", "mich", "ge\u00b7bo\u00b7ren", ",", "lan\u00b7ge", "mir", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sahe dich, ich liebte dich:", "tokens": ["Ich", "sa\u00b7he", "dich", ",", "ich", "lieb\u00b7te", "dich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ach! was f\u00fchltest du f\u00fcr mich?", "tokens": ["Doch", "ach", "!", "was", "f\u00fchl\u00b7test", "du", "f\u00fcr", "mich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWS", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchlte deine feuervollen Blicke,", "tokens": ["Ich", "f\u00fchl\u00b7te", "dei\u00b7ne", "feu\u00b7er\u00b7vol\u00b7len", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wandte schnell die meinigen zu-", "tokens": ["Und", "wand\u00b7te", "schnell", "die", "mei\u00b7ni\u00b7gen", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "PPOSS", "TRUNC"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Schon traut\u2019 ich ihnen selbst nicht mehr;", "tokens": ["Schon", "traut'", "ich", "ih\u00b7nen", "selbst", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn ach! sie liebten dich zu sehr.", "tokens": ["Denn", "ach", "!", "sie", "lieb\u00b7ten", "dich", "zu", "sehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "PPER", "PTKA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nach dir kann nichts hinfort mein Herz", "tokens": ["Nach", "dir", "kann", "nichts", "hin\u00b7fort", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach dir auch nicht die sch\u00f6nste der G\u00f6t-", "tokens": ["Nach", "dir", "auch", "nicht", "die", "sch\u00f6ns\u00b7te", "der", "G\u00f6t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "ART", "TRUNC"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Vergeblich b\u00f6te sie mir heut", "tokens": ["Ver\u00b7geb\u00b7lich", "b\u00f6\u00b7te", "sie", "mir", "heut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihrer Hand Unsterblichkeit.", "tokens": ["Mit", "ih\u00b7rer", "Hand", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Vor dir hat nichts mein junges Herz", "tokens": ["Vor", "dir", "hat", "nichts", "mein", "jun\u00b7ges", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun w\u00fcrde dirs durch keinen Gott ent-", "tokens": ["Nun", "w\u00fcr\u00b7de", "dirs", "durch", "kei\u00b7nen", "Gott", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und g\u00e4b\u2019 er mir mit seiner Hand", "tokens": ["Und", "g\u00e4b'", "er", "mir", "mit", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Gottheit \u00fcber Meer und Land.", "tokens": ["Die", "Got\u00b7theit", "\u00fc\u00b7ber", "Meer", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach! willst du mir nicht bald dein", "tokens": ["Ach", "!", "willst", "du", "mir", "nicht", "bald", "dein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Ebenbild in einer Tochter ge-", "tokens": ["Dein", "E\u00b7ben\u00b7bild", "in", "ei\u00b7ner", "Toch\u00b7ter", "ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht dieser Augen schlauen Witz?", "tokens": ["Nicht", "die\u00b7ser", "Au\u00b7gen", "schlau\u00b7en", "Witz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht diesen Mund, der Suada Sitz?", "tokens": ["Nicht", "die\u00b7sen", "Mund", ",", "der", "Sua\u00b7da", "Sitz", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dein sey das Ebenbild des ersten", "tokens": ["Dein", "sey", "das", "E\u00b7ben\u00b7bild", "des", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wann dich dereinst die Sorgen deines", "tokens": ["Wann", "dich", "de\u00b7reinst", "die", "Sor\u00b7gen", "dei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus meiner Arme Banden ziehn,", "tokens": ["Aus", "mei\u00b7ner", "Ar\u00b7me", "Ban\u00b7den", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umarm\u2019 ich doch, statt deiner, ihn.", "tokens": ["Um\u00b7arm'", "ich", "doch", ",", "statt", "dei\u00b7ner", ",", "ihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "KOUI", "PPOSAT", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn mich und dich die G\u00f6ttinn Isis", "tokens": ["Wenn", "mich", "und", "dich", "die", "G\u00f6t\u00b7tinn", "I\u00b7sis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KON", "PPER", "ART", "NN", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und mir dein Bild in einem Sohne", "tokens": ["Und", "mir", "dein", "Bild", "in", "ei\u00b7nem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So bring\u2019 ich diese Schal\u2019 ihr dar,", "tokens": ["So", "bring'", "ich", "die\u00b7se", "Schal'", "ihr", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeuginn unsres Bundes war.", "tokens": ["Die", "Zeu\u00b7ginn", "uns\u00b7res", "Bun\u00b7des", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und wenn die G\u00f6tter mir dein Bild", "tokens": ["Und", "wenn", "die", "G\u00f6t\u00b7ter", "mir", "dein", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So will ich ihnen diese Locke weihen.", "tokens": ["So", "will", "ich", "ih\u00b7nen", "die\u00b7se", "Lo\u00b7cke", "wei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die funfzehn oder sechzehn Jahr", "tokens": ["Die", "funf\u00b7zehn", "o\u00b7der", "sech\u00b7zehn", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "KON", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zierde meiner Scheitel war.", "tokens": ["Die", "Zier\u00b7de", "mei\u00b7ner", "Schei\u00b7tel", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach! soll ein Stal diess sch\u00f6ne Haar", "tokens": ["Ach", "!", "soll", "ein", "Stal", "diess", "sch\u00f6\u00b7ne", "Haar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VMFIN", "ART", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So muss ein Gott es an den Pol ver-", "tokens": ["So", "muss", "ein", "Gott", "es", "an", "den", "Pol", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dort ist der Raum noch nicht gef\u00fcllt,", "tokens": ["Dort", "ist", "der", "Raum", "noch", "nicht", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort flamm\u2019 es als ein Sternenbild.", "tokens": ["Dort", "flamm'", "es", "als", "ein", "Ster\u00b7nen\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Bis in den Himmel fliege deine", "tokens": ["Bis", "in", "den", "Him\u00b7mel", "flie\u00b7ge", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort werde sie, bey jedem Freuden-", "tokens": ["Dort", "wer\u00b7de", "sie", ",", "bey", "je\u00b7dem", "Freu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Voll Nektar, der die G\u00f6tter tr\u00e4nkt,", "tokens": ["Voll", "Nek\u00b7tar", ",", "der", "die", "G\u00f6t\u00b7ter", "tr\u00e4nkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und voll Unsterblichkeit geschenkt.", "tokens": ["Und", "voll", "U\u00b7nsterb\u00b7lich\u00b7keit", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Wann, sp\u00e4t nach mir, dich selbst der", "tokens": ["Wann", ",", "sp\u00e4t", "nach", "mir", ",", "dich", "selbst", "der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "ADJD", "APPR", "PPER", "$,", "PPER", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann thronest du wo deine Locke lo-", "tokens": ["Dann", "thro\u00b7nest", "du", "wo", "dei\u00b7ne", "Lo\u00b7cke", "lo"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PWAV", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ganze Norden ehret dich;", "tokens": ["Der", "gan\u00b7ze", "Nor\u00b7den", "eh\u00b7ret", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch lange nicht so sehr, als ich.", "tokens": ["Doch", "lan\u00b7ge", "nicht", "so", "sehr", ",", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "ADV", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Mit mir zugleich geneuss im Sternen-", "tokens": ["Mit", "mir", "zu\u00b7gleich", "ge\u00b7neuss", "im", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ADV", "APPRART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den G\u00f6ttertrank aus deiner goldnen", "tokens": ["Den", "G\u00f6t\u00b7ter\u00b7trank", "aus", "dei\u00b7ner", "gold\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Geliebter! kann er s\u00fcsser seyn,", "tokens": ["Ge\u00b7lieb\u00b7ter", "!", "kann", "er", "s\u00fcs\u00b7ser", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als dieser hochzeitliche Wein?", "tokens": ["Als", "die\u00b7ser", "hoch\u00b7zeit\u00b7li\u00b7che", "Wein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}}}}