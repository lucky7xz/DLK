{"textgrid.poem.35999": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Mariechen", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, der Lenz im Sonnenglanze", "tokens": ["Sieh", ",", "der", "Lenz", "im", "Son\u00b7nen\u00b7glan\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat die B\u00e4ume dir belaubt,", "tokens": ["Hat", "die", "B\u00e4u\u00b7me", "dir", "be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fcmchen gibt er dir zum Kranze", "tokens": ["Bl\u00fcm\u00b7chen", "gibt", "er", "dir", "zum", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr dein lockig Kinderhaupt.", "tokens": ["F\u00fcr", "dein", "lo\u00b7ckig", "Kin\u00b7der\u00b7haupt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh! Welch fr\u00f6hliches Get\u00fcmmel", "tokens": ["Sieh", "!", "Welch", "fr\u00f6h\u00b7li\u00b7ches", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der lieben, gr\u00fcnen Au!", "tokens": ["Auf", "der", "lie\u00b7ben", ",", "gr\u00fc\u00b7nen", "Au", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh den milden Fr\u00fchlingshimmel,", "tokens": ["Sieh", "den", "mil\u00b7den", "Fr\u00fch\u00b7lings\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell wie deiner Augen Blau!", "tokens": ["Hell", "wie", "dei\u00b7ner", "Au\u00b7gen", "Blau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O, wie traut die V\u00f6glein scherzen,", "tokens": ["O", ",", "wie", "traut", "die", "V\u00f6\u00b7glein", "scher\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O, wie wei\u00df die Bl\u00fcmchen stehn,", "tokens": ["O", ",", "wie", "wei\u00df", "die", "Bl\u00fcm\u00b7chen", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch \u2013 in deinem kleinen Herzen", "tokens": ["Doch", "\u2013", "in", "dei\u00b7nem", "klei\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht der Mai noch eins so sch\u00f6n.", "tokens": ["Bl\u00fcht", "der", "Mai", "noch", "eins", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PIS", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}