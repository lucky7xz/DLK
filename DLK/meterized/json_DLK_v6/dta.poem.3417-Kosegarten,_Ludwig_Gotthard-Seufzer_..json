{"dta.poem.3417": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Seufzer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sey mir gegr&#252;sst in meinem stillen Grame,", "tokens": ["Sey", "mir", "ge\u00b7gr", "&#252;", "sst", "in", "mei\u00b7nem", "stil\u00b7len", "Gra\u00b7me", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "$(", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Mein mondversilbertes Gemach!", "tokens": ["Mein", "mond\u00b7ver\u00b7sil\u00b7ber\u00b7tes", "Ge\u00b7mach", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dir belauschet mich kein Ohr, wenn Idens Name", "tokens": ["In", "dir", "be\u00b7lau\u00b7schet", "mich", "kein", "Ohr", ",", "wenn", "I\u00b7dens", "Na\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "KOUS", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der bl\u00f6den Lipp' entquillt, und niemand h\u00f6hnt", "tokens": ["Der", "bl\u00f6\u00b7den", "Lipp'", "ent\u00b7quillt", ",", "und", "nie\u00b7mand", "h\u00f6hnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "mein Ach.", "tokens": ["mein", "Ach", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Von deinen weissen mondbestrahlten Mauern", "tokens": ["Von", "dei\u00b7nen", "weis\u00b7sen", "mond\u00b7bes\u00b7trahl\u00b7ten", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Welt und dem Gew\u00fchl verhehlt,", "tokens": ["Der", "Welt", "und", "dem", "Ge\u00b7w\u00fchl", "ver\u00b7hehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will ich in Einsamkeit die \u00f6de Zeit vertrauern,", "tokens": ["Will", "ich", "in", "Ein\u00b7sam\u00b7keit", "die", "\u00f6\u00b7de", "Zeit", "ver\u00b7trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo Ida meinem Herzen fehlt.", "tokens": ["Wo", "I\u00b7da", "mei\u00b7nem", "Her\u00b7zen", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In dir will ich allein mit meinem Sehnen,", "tokens": ["In", "dir", "will", "ich", "al\u00b7lein", "mit", "mei\u00b7nem", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein mit meinem Kummer seyn;", "tokens": ["Al\u00b7lein", "mit", "mei\u00b7nem", "Kum\u00b7mer", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Iden, Iden soll mein Tr\u00e4umen und mein", "tokens": ["Und", "I\u00b7den", ",", "I\u00b7den", "soll", "mein", "Tr\u00e4u\u00b7men", "und", "mein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "VMFIN", "PPOSAT", "NN", "KON", "PPOSAT"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "W\u00e4hnen,", "tokens": ["W\u00e4h\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Mein Denken und mein Dichten heilig seyn.", "tokens": ["Mein", "Den\u00b7ken", "und", "mein", "Dich\u00b7ten", "hei\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}