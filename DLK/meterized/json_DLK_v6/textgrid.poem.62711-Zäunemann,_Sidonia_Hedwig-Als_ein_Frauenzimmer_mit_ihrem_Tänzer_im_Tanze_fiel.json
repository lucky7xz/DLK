{"textgrid.poem.62711": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Als ein Frauenzimmer mit ihrem T\u00e4nzer im Tanze fiel", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wird sonst ein Jungferbild durch List zum Fall gebracht;", "tokens": ["Wird", "sonst", "ein", "Jung\u00b7fer\u00b7bild", "durch", "List", "zum", "Fall", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So scheuet man zu jederzeit", "tokens": ["So", "scheu\u00b7et", "man", "zu", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Licht, und Gegenwart der Menschen:", "tokens": ["Das", "Licht", ",", "und", "Ge\u00b7gen\u00b7wart", "der", "Men\u00b7schen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man suchet Nacht und Einsamkeit;", "tokens": ["Man", "su\u00b7chet", "Nacht", "und", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du aber sch\u00e4mst dich nicht beym Tanz und Springen,", "tokens": ["Du", "a\u00b7ber", "sch\u00e4mst", "dich", "nicht", "beym", "Tanz", "und", "Sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Limenen \u00f6ffentlich zum Fall zu bringen.", "tokens": ["Li\u00b7me\u00b7nen", "\u00f6f\u00b7fent\u00b7lich", "zum", "Fall", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}