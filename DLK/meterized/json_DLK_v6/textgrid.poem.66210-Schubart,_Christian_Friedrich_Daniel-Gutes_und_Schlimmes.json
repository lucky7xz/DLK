{"textgrid.poem.66210": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Gutes und Schlimmes", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald rasselt Wodan nicht mehr auf eisernem Wagen,", "tokens": ["Bald", "ras\u00b7selt", "Wo\u00b7dan", "nicht", "mehr", "auf", "ei\u00b7ser\u00b7nem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von fliegenden wei\u00dfen Rossen getragen,", "tokens": ["Von", "flie\u00b7gen\u00b7den", "wei\u00b7\u00dfen", "Ros\u00b7sen", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sch\u00fcttelt zornig und wild", "tokens": ["Und", "sch\u00fct\u00b7telt", "zor\u00b7nig", "und", "wild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Den tausendj\u00e4hrigen Schild.", "tokens": ["Den", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gen", "Schild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Den blutigen Lorbeer \u00fcberschimmert die Palme,", "tokens": ["Den", "blu\u00b7ti\u00b7gen", "Lor\u00b7beer", "\u00fc\u00b7bers\u00b7chim\u00b7mert", "die", "Pal\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Gott des Friedens t\u00f6net im Psalme;", "tokens": ["Der", "Gott", "des", "Frie\u00b7dens", "t\u00f6\u00b7net", "im", "Psal\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn wie ein Thronenengel geschm\u00fcckt", "tokens": ["Denn", "wie", "ein", "Thro\u00b7nen\u00b7en\u00b7gel", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hat er den Frieden geschickt.", "tokens": ["Hat", "er", "den", "Frie\u00b7den", "ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Wilhelm, gesegnet vom Menschengeschlechte,", "tokens": ["Wil\u00b7helm", ",", "ge\u00b7seg\u00b7net", "vom", "Men\u00b7schen\u00b7ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Ergriff des weisen Leopold Rechte,", "tokens": ["Er\u00b7griff", "des", "wei\u00b7sen", "Leo\u00b7pold", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sprach: Ewiger Friede zwischen dir und mir!", "tokens": ["Sprach", ":", "E\u00b7wi\u00b7ger", "Frie\u00b7de", "zwi\u00b7schen", "dir", "und", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Friede, Friede zwischen Selim und Dir.", "tokens": ["Frie\u00b7de", ",", "Frie\u00b7de", "zwi\u00b7schen", "Se\u00b7lim", "und", "Dir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "NE", "KON", "PPER", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Dies h\u00f6rten von ferne die zornigen Schaaren,", "tokens": ["Dies", "h\u00f6r\u00b7ten", "von", "fer\u00b7ne", "die", "zor\u00b7ni\u00b7gen", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Streiften geronnenes Blut aus den Haaren;", "tokens": ["Streif\u00b7ten", "ge\u00b7ron\u00b7ne\u00b7nes", "Blut", "aus", "den", "Haa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Zu Koburg sprach der graue Wessir:", "tokens": ["Zu", "Ko\u00b7burg", "sprach", "der", "grau\u00b7e", "Wes\u00b7sir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Friede, Friede zwischen dir und mir!", "tokens": ["Frie\u00b7de", ",", "Frie\u00b7de", "zwi\u00b7schen", "dir", "und", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Gustav, gewaltig an Weisheit und Ehren,", "tokens": ["Gus\u00b7tav", ",", "ge\u00b7wal\u00b7tig", "an", "Weis\u00b7heit", "und", "Eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Der schwimmende Donn'rer in klippigten Scheeren,", "tokens": ["Der", "schwim\u00b7men\u00b7de", "Donn'\u00b7rer", "in", "klip\u00b7pig\u00b7ten", "Schee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bot, das Auge gen Himmel gewandt,", "tokens": ["Bot", ",", "das", "Au\u00b7ge", "gen", "Him\u00b7mel", "ge\u00b7wandt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Der gro\u00dfen Katharina die Hand.", "tokens": ["Der", "gro\u00b7\u00dfen", "Ka\u00b7tha\u00b7ri\u00b7na", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Potemkin, schleudre nicht ewig die Keule", "tokens": ["Po\u00b7tem\u00b7kin", ",", "schleud\u00b7re", "nicht", "e\u00b7wig", "die", "Keu\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "ADJD", "ART", "NN"], "meter": "+-----+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Des Todes unterm Schlachtengeheule \u2013", "tokens": ["Des", "To\u00b7des", "un\u00b7term", "Schlach\u00b7ten\u00b7ge\u00b7heu\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Eile dem Engel des Friedens in Schoo\u00df.", "tokens": ["Ei\u00b7le", "dem", "En\u00b7gel", "des", "Frie\u00b7dens", "in", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Menschlich \u2013 ist besser, als gro\u00df.", "tokens": ["Menschlich", "\u2013", "ist", "bes\u00b7ser", ",", "als", "gro\u00df", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "VAFIN", "ADJD", "$,", "KOUS", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Und so t\u00f6nt's vom Osten bis zum S\u00fcden,", "tokens": ["Und", "so", "t\u00f6nt's", "vom", "Os\u00b7ten", "bis", "zum", "S\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "So t\u00f6nt's vom Pol der Mitternacht:", "tokens": ["So", "t\u00f6nt's", "vom", "Pol", "der", "Mit\u00b7ter\u00b7nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frieden sollt ihr haben, gro\u00dfen Frieden,", "tokens": ["Frie\u00b7den", "sollt", "ihr", "ha\u00b7ben", ",", "gro\u00b7\u00dfen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nimmer donnern soll die W\u00fcrgeschlacht.", "tokens": ["Nim\u00b7mer", "don\u00b7nern", "soll", "die", "W\u00fcr\u00b7ge\u00b7schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Nimmer triefen soll das Schwert vom Blute,", "tokens": ["Nim\u00b7mer", "trie\u00b7fen", "soll", "das", "Schwert", "vom", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nimmer klebe Mark am Rossehuf.", "tokens": ["Nim\u00b7mer", "kle\u00b7be", "Mark", "am", "Ros\u00b7se\u00b7huf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wi\u00dft, ihr Menschen, da\u00df euch Gott, der Gute", "tokens": ["Wi\u00dft", ",", "ihr", "Men\u00b7schen", ",", "da\u00df", "euch", "Gott", ",", "der", "Gu\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht zur blutgen Fehde schuf.", "tokens": ["Nicht", "zur", "blut\u00b7gen", "Feh\u00b7de", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Friede soll es sein auf dieser Erde!", "tokens": ["Frie\u00b7de", "soll", "es", "sein", "auf", "die\u00b7ser", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Krieg, der H\u00f6llennachtgef\u00e4hrte,", "tokens": ["Und", "der", "Krieg", ",", "der", "H\u00f6l\u00b7len\u00b7nacht\u00b7ge\u00b7f\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit Zerst\u00f6rungswuth im Angesicht,", "tokens": ["Mit", "Zer\u00b7st\u00f6\u00b7rungs\u00b7wuth", "im", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Soll verheeren meine Erde nicht.", "tokens": ["Soll", "ver\u00b7hee\u00b7ren", "mei\u00b7ne", "Er\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}