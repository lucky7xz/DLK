{"textgrid.poem.50740": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Winter ist eine ehrliche Haut,", "tokens": ["Der", "Win\u00b7ter", "ist", "ei\u00b7ne", "ehr\u00b7li\u00b7che", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein alter Poldrian;", "tokens": ["Ein", "al\u00b7ter", "Pold\u00b7ri\u00b7an", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie zornig er mir ins Auge schaut,", "tokens": ["Wie", "zor\u00b7nig", "er", "mir", "ins", "Au\u00b7ge", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Blick ich ihn wiederum an!", "tokens": ["Blick", "ich", "ihn", "wie\u00b7de\u00b7rum", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Sein Blut ist k\u00fchl und starr wie Eis,", "tokens": ["Sein", "Blut", "ist", "k\u00fchl", "und", "starr", "wie", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nie seine Treue wankt;", "tokens": ["Doch", "nie", "sei\u00b7ne", "Treu\u00b7e", "wankt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie oft hab ich mich n\u00e4chtlicherweis", "tokens": ["Wie", "oft", "hab", "ich", "mich", "n\u00e4cht\u00b7li\u00b7cher\u00b7weis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PRF", "NE"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Mit ihm herumgezankt!", "tokens": ["Mit", "ihm", "her\u00b7um\u00b7ge\u00b7zankt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Da r\u00fcttelt er mir am Gartentor", "tokens": ["Da", "r\u00fct\u00b7telt", "er", "mir", "am", "Gar\u00b7ten\u00b7tor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und stampft auf den Beeten herum,", "tokens": ["Und", "stampft", "auf", "den", "Bee\u00b7ten", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er schimpft mich einen sanguinischen Tor,", "tokens": ["Er", "schimpft", "mich", "ei\u00b7nen", "san\u00b7gu\u00b7i\u00b7ni\u00b7schen", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Leichtgl\u00e4ubig und herzlich dumm!", "tokens": ["Leicht\u00b7gl\u00e4u\u00b7big", "und", "herz\u00b7lich", "dumm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Viel Hoffnungen zieh ich in Scherben auf", "tokens": ["Viel", "Hoff\u00b7nun\u00b7gen", "zieh", "ich", "in", "Scher\u00b7ben", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am kalten Sternenschein,", "tokens": ["Am", "kal\u00b7ten", "Ster\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ist er besonders versessen drauf", "tokens": ["Da", "ist", "er", "be\u00b7son\u00b7ders", "ver\u00b7ses\u00b7sen", "drauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "PAV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und st\u00fcrmt auf sie herein.", "tokens": ["Und", "st\u00fcrmt", "auf", "sie", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich balge mich immer, so gut ich kann,", "tokens": ["Ich", "bal\u00b7ge", "mich", "im\u00b7mer", ",", "so", "gut", "ich", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um jedes gr\u00fcne Reis;", "tokens": ["Um", "je\u00b7des", "gr\u00fc\u00b7ne", "Reis", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er aber entrupft sie, der harte Mann,", "tokens": ["Er", "a\u00b7ber", "en\u00b7trupft", "sie", ",", "der", "har\u00b7te", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Scherben b\u00fcschelweis.", "tokens": ["Den", "Scher\u00b7ben", "b\u00fc\u00b7schel\u00b7weis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch die mir der Alte stehenl\u00e4\u00dft,", "tokens": ["Doch", "die", "mir", "der", "Al\u00b7te", "ste\u00b7hen\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die sind erprobt und gefeit!", "tokens": ["Die", "sind", "er\u00b7probt", "und", "ge\u00b7feit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die sind gelenzet und fr\u00fchlingsfest", "tokens": ["Die", "sind", "ge\u00b7len\u00b7zet", "und", "fr\u00fch\u00b7lings\u00b7fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "VVPP", "KON", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und der Erf\u00fcllung geweiht!", "tokens": ["Und", "der", "Er\u00b7f\u00fcl\u00b7lung", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}