{"textgrid.poem.46703": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[als ich sah die Wolken weinen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich sah die Wolken weinen,", "tokens": ["Als", "ich", "sah", "die", "Wol\u00b7ken", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seufzt' ich: Sonne, wenn du mir", "tokens": ["Seufzt'", "ich", ":", "Son\u00b7ne", ",", "wenn", "du", "mir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "NN", "$,", "KOUS", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell nur einmal wolltest scheinen,", "tokens": ["Hell", "nur", "ein\u00b7mal", "woll\u00b7test", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freuen wollt' ich mich mit dir.", "tokens": ["Freu\u00b7en", "wollt'", "ich", "mich", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonne, dich nun seh' ich scheinen,", "tokens": ["Son\u00b7ne", ",", "dich", "nun", "seh'", "ich", "schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Freude fehlet mir;", "tokens": ["Doch", "die", "Freu\u00b7de", "feh\u00b7let", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Th\u00f6richt, da\u00df ich konnte meinen,", "tokens": ["Th\u00f6\u00b7richt", ",", "da\u00df", "ich", "konn\u00b7te", "mei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VMFIN", "PPOSAT", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kommen w\u00fcrde sie mit dir.", "tokens": ["Kom\u00b7men", "w\u00fcr\u00b7de", "sie", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sonne, so h\u00f6r' auf zu scheinen,", "tokens": ["Son\u00b7ne", ",", "so", "h\u00f6r'", "auf", "zu", "schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheinest nur zur Freude dir.", "tokens": ["Schei\u00b7nest", "nur", "zur", "Freu\u00b7de", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df die Wolken wieder weinen!", "tokens": ["La\u00df", "die", "Wol\u00b7ken", "wie\u00b7der", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie weinen doch mit mir.", "tokens": ["Denn", "sie", "wei\u00b7nen", "doch", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}