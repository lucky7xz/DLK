{"textgrid.poem.64228": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Epistel", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem friedlichen Dorf und dem Haus, von Reben umkleidet,", "tokens": ["Aus", "dem", "fried\u00b7li\u00b7chen", "Dorf", "und", "dem", "Haus", ",", "von", "Re\u00b7ben", "um\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Send' ich dir, trauter Genoss', einen bukolischen Gru\u00df.", "tokens": ["Send'", "ich", "dir", ",", "trau\u00b7ter", "Ge\u00b7noss'", ",", "ei\u00b7nen", "bu\u00b7ko\u00b7li\u00b7schen", "Gru\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Zwar nicht ist uns verg\u00f6nnt, so gewinnende Briefe zu schreiben,", "tokens": ["Zwar", "nicht", "ist", "uns", "ver\u00b7g\u00f6nnt", ",", "so", "ge\u00b7win\u00b7nen\u00b7de", "Brie\u00b7fe", "zu", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VAFIN", "PPER", "VVPP", "$,", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Wie sie die R\u00f6mer gesandt aus der Campagna zur Stadt,", "tokens": ["Wie", "sie", "die", "R\u00f6\u00b7mer", "ge\u00b7sandt", "aus", "der", "Cam\u00b7pag\u00b7na", "zur", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wie sie Horaz, der Schalk, an Bandusias Brunnen ersonnen", "tokens": ["Wie", "sie", "Ho\u00b7raz", ",", "der", "Schalk", ",", "an", "Ban\u00b7du\u00b7si\u00b7as", "Brun\u00b7nen", "er\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NE", "$,", "ART", "NN", "$,", "APPR", "NE", "NN", "VVINF"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "\u2013 Noch in seinem Gedicht rieselt ihr Silbergewog \u2013", "tokens": ["\u2013", "Noch", "in", "sei\u00b7nem", "Ge\u00b7dicht", "rie\u00b7selt", "ihr", "Sil\u00b7ber\u00b7ge\u00b7wog", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Oder von Mantua einst sie der edle Vergilius sandte,", "tokens": ["O\u00b7der", "von", "Man\u00b7tua", "einst", "sie", "der", "ed\u00b7le", "Ver\u00b7gi\u00b7lius", "sand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Reich mit dem h\u00f6chsten Pomp r\u00f6mischer Rede geschm\u00fcckt,", "tokens": ["Reich", "mit", "dem", "h\u00f6chs\u00b7ten", "Pomp", "r\u00f6\u00b7mi\u00b7scher", "Re\u00b7de", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Da\u00df, wenn August sie las, er verga\u00df der beherrschenden K\u00fcnste: \u2013", "tokens": ["Da\u00df", ",", "wenn", "Au\u00b7gust", "sie", "las", ",", "er", "ver\u00b7ga\u00df", "der", "be\u00b7herr\u00b7schen\u00b7den", "K\u00fcns\u00b7te", ":", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Solches zwar ist versagt \u2013: rauher ist Boden und Sinn.", "tokens": ["Sol\u00b7ches", "zwar", "ist", "ver\u00b7sagt", "\u2013", ":", "rau\u00b7her", "ist", "Bo\u00b7den", "und", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "VVPP", "$(", "$.", "ADJD", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Nicht ragt gl\u00e4nzend und rund mir von thrakischem Steine die Villa", "tokens": ["Nicht", "ragt", "gl\u00e4n\u00b7zend", "und", "rund", "mir", "von", "thr\u00b7a\u00b7ki\u00b7schem", "Stei\u00b7ne", "die", "Vil\u00b7la"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ADJD", "KON", "ADJD", "PPER", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+-+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Und die Charitinnen nicht stehen im Atrium mir,", "tokens": ["Und", "die", "Cha\u00b7ri\u00b7tin\u00b7nen", "nicht", "ste\u00b7hen", "im", "A\u00b7tri\u00b7um", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Nicht, von Platanen bedacht, dehnt weit sich die sand'ge Pal\u00e4stra:", "tokens": ["Nicht", ",", "von", "Pla\u00b7ta\u00b7nen", "be\u00b7dacht", ",", "dehnt", "weit", "sich", "die", "san\u00b7d'\u00b7ge", "Pa\u00b7l\u00e4st\u00b7ra", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "APPR", "NN", "VVPP", "$,", "VVFIN", "ADJD", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+-+-+-", "measure": "dactylic.di.plus"}, "line.14": {"text": "Nicht aus staubigem Schlauch wird mir Falerner geschenkt:", "tokens": ["Nicht", "aus", "stau\u00b7bi\u00b7gem", "Schlauch", "wird", "mir", "Fa\u00b7ler\u00b7ner", "ge\u00b7schenkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "+-+--+--++--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Nicht umsp\u00fclet das Haus mit der sanften ausonischen Welle,", "tokens": ["Nicht", "um\u00b7sp\u00fc\u00b7let", "das", "Haus", "mit", "der", "sanf\u00b7ten", "au\u00b7so\u00b7ni\u00b7schen", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Bis in das dienende Meer waglich gemauert, das Bad:", "tokens": ["Bis", "in", "das", "die\u00b7nen\u00b7de", "Meer", "wag\u00b7lich", "ge\u00b7mau\u00b7ert", ",", "das", "Bad", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ADJD", "VVPP", "$,", "PRELS", "NE", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Hart ist unser Geschlecht und die alabasterne Gl\u00e4tte", "tokens": ["Hart", "ist", "un\u00b7ser", "Ge\u00b7schlecht", "und", "die", "a\u00b7la\u00b7bas\u00b7ter\u00b7ne", "Gl\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Edelster Formengewalt weigert sich unserer Kunst:", "tokens": ["E\u00b7dels\u00b7ter", "For\u00b7men\u00b7ge\u00b7walt", "wei\u00b7gert", "sich", "un\u00b7se\u00b7rer", "Kunst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Horch, der Hexameter selbst, wie er seufzt in der Fessel des Deutschen!", "tokens": ["Horch", ",", "der", "He\u00b7xa\u00b7me\u00b7ter", "selbst", ",", "wie", "er", "seufzt", "in", "der", "Fes\u00b7sel", "des", "Deut\u00b7schen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Ach, der Verw\u00f6hnte verlangt reichere Tonmelodie.", "tokens": ["Ach", ",", "der", "Ver\u00b7w\u00f6hn\u00b7te", "ver\u00b7langt", "rei\u00b7che\u00b7re", "Ton\u00b7me\u00b7lo\u00b7die", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+--++--+-+-", "measure": "dactylic.di.plus"}, "line.21": {"text": "Aber ein Anderes ward den unstr\u00e4flichen S\u00f6hnen des Nordens:", "tokens": ["A\u00b7ber", "ein", "An\u00b7de\u00b7res", "ward", "den", "uns\u00b7tr\u00e4f\u00b7li\u00b7chen", "S\u00f6h\u00b7nen", "des", "Nor\u00b7dens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Ahnungsvollerer Reiz atmet in unsrer Natur:", "tokens": ["Ah\u00b7nungs\u00b7vol\u00b7le\u00b7rer", "Reiz", "at\u00b7met", "in", "uns\u00b7rer", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Wann sich, wie jetzt, die Sonne geneigt und die liebliche D\u00e4mm'rung", "tokens": ["Wann", "sich", ",", "wie", "jetzt", ",", "die", "Son\u00b7ne", "ge\u00b7neigt", "und", "die", "lieb\u00b7li\u00b7che", "D\u00e4m\u00b7m'\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "$,", "PWAV", "ADV", "$,", "ART", "NN", "VVPP", "KON", "ART", "ADJA", "NN"], "meter": "--+--+--+--+-+-+-", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Langsam den Schleier zieht \u00fcber das Abendgefild,", "tokens": ["Lang\u00b7sam", "den", "Schlei\u00b7er", "zieht", "\u00fc\u00b7ber", "das", "A\u00b7bend\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.25": {"text": "Wann der k\u00fchlere Wind an der Buchen Wipfel und Tannen", "tokens": ["Wann", "der", "k\u00fch\u00b7le\u00b7re", "Wind", "an", "der", "Bu\u00b7chen", "Wip\u00b7fel", "und", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.26": {"text": "Lieblichen Rauschens r\u00fchrt, wann aus dem Erlengeb\u00fcsch", "tokens": ["Lieb\u00b7li\u00b7chen", "Rau\u00b7schens", "r\u00fchrt", ",", "wann", "aus", "dem", "Er\u00b7len\u00b7ge\u00b7b\u00fcsch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Fl\u00f6tend der innige Ton der melodischen Amsel hervorklingt", "tokens": ["Fl\u00f6\u00b7tend", "der", "in\u00b7ni\u00b7ge", "Ton", "der", "me\u00b7lo\u00b7di\u00b7schen", "Am\u00b7sel", "her\u00b7vor\u00b7klingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.28": {"text": "Und vor dem braunen Geh\u00f6ft, unter dem Giebelgeb\u00e4lk,", "tokens": ["Und", "vor", "dem", "brau\u00b7nen", "Ge\u00b7h\u00f6ft", ",", "un\u00b7ter", "dem", "Gie\u00b7bel\u00b7ge\u00b7b\u00e4lk", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Vor der offenen T\u00fcr, auf der Holzbank, sitzen die Leute,", "tokens": ["Vor", "der", "of\u00b7fe\u00b7nen", "T\u00fcr", ",", "auf", "der", "Holz\u00b7bank", ",", "sit\u00b7zen", "die", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Plaudernd in Abendruh: hier der gebogene Greis,", "tokens": ["Plau\u00b7dernd", "in", "A\u00b7ben\u00b7druh", ":", "hier", "der", "ge\u00b7bo\u00b7ge\u00b7ne", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$.", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Dort das bl\u00fchende Weib, auf den kr\u00e4ftigen Armen den S\u00e4ugling,", "tokens": ["Dort", "das", "bl\u00fc\u00b7hen\u00b7de", "Weib", ",", "auf", "den", "kr\u00e4f\u00b7ti\u00b7gen", "Ar\u00b7men", "den", "S\u00e4ug\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.32": {"text": "W\u00e4hrend der blonde Bub schnitzelt am h\u00f6lzernen Schwert,", "tokens": ["W\u00e4h\u00b7rend", "der", "blon\u00b7de", "Bub", "schnit\u00b7zelt", "am", "h\u00f6l\u00b7zer\u00b7nen", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.33": {"text": "Frisch, krauslockig und froh, mit den blauen, den offenen Augen,", "tokens": ["Frisch", ",", "kraus\u00b7lo\u00b7ckig", "und", "froh", ",", "mit", "den", "blau\u00b7en", ",", "den", "of\u00b7fe\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Blau, wie dem R\u00f6mer sie einst kimbrische Schrecken geblitzt, \u2013", "tokens": ["Blau", ",", "wie", "dem", "R\u00f6\u00b7mer", "sie", "einst", "kim\u00b7bri\u00b7sche", "Schre\u00b7cken", "ge\u00b7blitzt", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "PPER", "ADV", "ADJA", "NN", "VVPP", "$,", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Aber am Brunnenrand dort, unter dem alten Holunder,", "tokens": ["A\u00b7ber", "am", "Brun\u00b7nen\u00b7rand", "dort", ",", "un\u00b7ter", "dem", "al\u00b7ten", "Ho\u00b7lun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Blickt in das \u00c4hrenfeld sinnend der Vater hinaus: \u2013 \u2013", "tokens": ["Blickt", "in", "das", "\u00c4h\u00b7ren\u00b7feld", "sin\u00b7nend", "der", "Va\u00b7ter", "hin\u00b7aus", ":", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "ART", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.37": {"text": "Freund, wer solches geschaut, nicht sch\u00e4mt er sich unseres Volkes", "tokens": ["Freund", ",", "wer", "sol\u00b7ches", "ge\u00b7schaut", ",", "nicht", "sch\u00e4mt", "er", "sich", "un\u00b7se\u00b7res", "Vol\u00b7kes"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "PIS", "VVPP", "$,", "PTKNEG", "VVFIN", "PPER", "PRF", "PPOSAT", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.38": {"text": "Und in bewegterem Gang schl\u00e4gt ihm gehoben das Herz.", "tokens": ["Und", "in", "be\u00b7weg\u00b7te\u00b7rem", "Gang", "schl\u00e4gt", "ihm"