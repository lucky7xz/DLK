{"textgrid.poem.64097": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Arabische Totenklage", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weithin ruht in Nacht die W\u00fcste,", "tokens": ["Weit\u00b7hin", "ruht", "in", "Nacht", "die", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterne flimmern sonder Zahl:", "tokens": ["Ster\u00b7ne", "flim\u00b7mern", "son\u00b7der", "Zahl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weithinweg vom lauten Lager", "tokens": ["Weit\u00b7hin\u00b7weg", "vom", "lau\u00b7ten", "La\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trag' ich meine stumme Qual.", "tokens": ["Trag'", "ich", "mei\u00b7ne", "stum\u00b7me", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei den Zelten kreist der Becher,", "tokens": ["Bei", "den", "Zel\u00b7ten", "kreist", "der", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sang erschallt und Saitenspiel: \u2013", "tokens": ["Sang", "er\u00b7schallt", "und", "Sai\u00b7ten\u00b7spiel", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach und noch sind's nicht drei Monde,", "tokens": ["Ach", "und", "noch", "sin\u00b7d's", "nicht", "drei", "Mon\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "ADV", "VAFIN", "PTKNEG", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mein tapfrer Bruder fiel!", "tokens": ["Da\u00df", "mein", "tapf\u00b7rer", "Bru\u00b7der", "fiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Abu Seid, du Stolz des Stammes,", "tokens": ["A\u00b7bu", "Seid", ",", "du", "Stolz", "des", "Stam\u00b7mes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAIMP", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stern des Rates, Sturm der Schlacht,", "tokens": ["Stern", "des", "Ra\u00b7tes", ",", "Sturm", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast gerettet Gut und Leben", "tokens": ["Hast", "ge\u00b7ret\u00b7tet", "Gut", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manchem, der dort singt und lacht.", "tokens": ["Man\u00b7chem", ",", "der", "dort", "singt", "und", "lacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Abu Seid, gazellenbr\u00e4unlich,", "tokens": ["A\u00b7bu", "Seid", ",", "ga\u00b7zel\u00b7len\u00b7br\u00e4un\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "VAIMP", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ner Frau'n geheimer Traum,", "tokens": ["Sch\u00f6\u00b7ner", "Frau'n", "ge\u00b7hei\u00b7mer", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinem Feind warst du ein L\u00f6we,", "tokens": ["Dei\u00b7nem", "Feind", "warst", "du", "ein", "L\u00f6\u00b7we", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NE", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Deinem Freund ein Palmenbaum.", "tokens": ["Dei\u00b7nem", "Freund", "ein", "Pal\u00b7men\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O was weilt' ich fern in Mekka,", "tokens": ["O", "was", "weilt'", "ich", "fern", "in", "Mek\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "ADJD", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als du sankst am Pa\u00df Al Irmt,", "tokens": ["Als", "du", "sankst", "am", "Pa\u00df", "Al", "Irmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo du, einer gegen vierzig,", "tokens": ["Wo", "du", ",", "ei\u00b7ner", "ge\u00b7gen", "vier\u00b7zig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "APPR", "CARD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsrer Herden Flucht beschirmt.", "tokens": ["Uns\u00b7rer", "Her\u00b7den", "Flucht", "be\u00b7schirmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dreizehn Lanzen schon im Schilde,", "tokens": ["Drei\u00b7zehn", "Lan\u00b7zen", "schon", "im", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieben Wunden in der Brust \u2013", "tokens": ["Sie\u00b7ben", "Wun\u00b7den", "in", "der", "Brust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer wollt'st du noch nicht fallen", "tokens": ["Im\u00b7mer", "wollt'st", "du", "noch", "nicht", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis du fallen doch gemu\u00dft.", "tokens": ["Bis", "du", "fal\u00b7len", "doch", "ge\u00b7mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ha, sie singen, weil die Geier,", "tokens": ["Ha", ",", "sie", "sin\u00b7gen", ",", "weil", "die", "Gei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zehren noch von deinem Leib \u2013", "tokens": ["Zeh\u00b7ren", "noch", "von", "dei\u00b7nem", "Leib", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein verga\u00df der Stamm, der Emir,", "tokens": ["Dein", "ver\u00b7ga\u00df", "der", "Stamm", ",", "der", "E\u00b7mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dein verga\u00df das eigne Weib.", "tokens": ["Dein", "ver\u00b7ga\u00df", "das", "eig\u00b7ne", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber ich will dein gedenken,", "tokens": ["A\u00b7ber", "ich", "will", "dein", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ner, tapfrer, junger Scheich:", "tokens": ["Sch\u00f6\u00b7ner", ",", "tapf\u00b7rer", ",", "jun\u00b7ger", "Scheich", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hilft kein Gott, kein Mensch dich r\u00e4chen \u2013", "tokens": ["Hilft", "kein", "Gott", ",", "kein", "Mensch", "dich", "r\u00e4\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So hilf du mir, H\u00f6llenreich!", "tokens": ["So", "hilf", "du", "mir", ",", "H\u00f6l\u00b7len\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "$,", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Meine Seele sollt ihr haben,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "sollt", "ihr", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00f6se Geister, immerdar,", "tokens": ["B\u00f6\u00b7se", "Geis\u00b7ter", ",", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Helft ihr das Geschlecht verderben,", "tokens": ["Helft", "ihr", "das", "Ge\u00b7schlecht", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das des Helden M\u00f6rder war.", "tokens": ["Das", "des", "Hel\u00b7den", "M\u00f6r\u00b7der", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ha, dann jauchz' ich durch die H\u00f6lle,", "tokens": ["Ha", ",", "dann", "jauch\u00b7z'", "ich", "durch", "die", "H\u00f6l\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Durch der Qualen Ewigkeit:", "tokens": ["Durch", "der", "Qua\u00b7len", "E\u00b7wig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbabu Seid, das war mein Bruder,", "tokens": ["\u00bb", "a\u00b7bu", "Seid", ",", "das", "war", "mein", "Bru\u00b7der", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich r\u00e4chte Abu Seid.\u00ab", "tokens": ["Und", "ich", "r\u00e4ch\u00b7te", "A\u00b7bu", "Seid", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}