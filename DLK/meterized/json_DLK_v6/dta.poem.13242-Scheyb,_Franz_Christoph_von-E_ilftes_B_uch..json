{"dta.poem.13242": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eerlaube K\u00f6niginn! da\u00df ich der Heer-F\u00fcrst seye,", "tokens": ["\u201e", "er\u00b7lau\u00b7be", "K\u00f6\u00b7ni\u00b7ginn", "!", "da\u00df", "ich", "der", "Heer\u00b7F\u00fcrst", "se\u00b7ye", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "\u201edem Volck befehl\u2019, und uns von diesem Feind befreye.", "tokens": ["\u201e", "dem", "Volck", "be\u00b7fehl'", ",", "und", "uns", "von", "die\u00b7sem", "Feind", "be\u00b7fre\u00b7ye", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PTKVZ", "$,", "KON", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "\u201eich spreche mir dadurch noch Thron noch Zepter zu;", "tokens": ["\u201e", "ich", "spre\u00b7che", "mir", "da\u00b7durch", "noch", "Thron", "noch", "Zep\u00b7ter", "zu", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PAV", "ADV", "NN", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "150\u201dDoch, wer mir in der Welt ihn geben kann, bist du;", "tokens": ["\"", "Doch", ",", "wer", "mir", "in", "der", "Welt", "ihn", "ge\u00b7ben", "kann", ",", "bist", "du", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "$,", "PWS", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201enein: die\u00df ist nicht mein Ziel, da\u00df ich den Thron betrete;", "tokens": ["\u201e", "nein", ":", "die\u00df", "ist", "nicht", "mein", "Ziel", ",", "da\u00df", "ich", "den", "Thron", "be\u00b7tre\u00b7te", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "PDS", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edas eben w\u00e4r, warum ich solchen Plaz verschm\u00e4hte.", "tokens": ["\u201e", "das", "e\u00b7ben", "w\u00e4r", ",", "wa\u00b7rum", "ich", "sol\u00b7chen", "Plaz", "ver\u00b7schm\u00e4h\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "ADV", "VAFIN", "$,", "PWAV", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edie W\u00fcrde, die ich mir dadurch von GOtt erbitt\u2019,", "tokens": ["\u201e", "die", "W\u00fcr\u00b7de", ",", "die", "ich", "mir", "da\u00b7durch", "von", "Gott", "er\u00b7bitt'", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "PAV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201e(Nach diesem richtet sich mein S\u00e4hnen und Gem\u00fcth)", "tokens": ["\u201e", "(", "Nach", "die\u00b7sem", "rich\u00b7tet", "sich", "mein", "S\u00e4h\u00b7nen", "und", "Ge\u00b7m\u00fcth", ")"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "APPR", "PDAT", "VVFIN", "PRF", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "155\u201dIst, GOtt mit uns, und dich mit deinem Feind vers\u00f6hnen,", "tokens": ["\"", "Ist", ",", "Gott", "mit", "uns", ",", "und", "dich", "mit", "dei\u00b7nem", "Feind", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "$,", "NN", "APPR", "PPER", "$,", "KON", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edein Lorber-reiches Haupt mit Friedens-Palmen kr\u00f6nen.", "tokens": ["\u201e", "dein", "Lor\u00b7ber\u00b7rei\u00b7ches", "Haupt", "mit", "Frie\u00b7dens\u00b7Pal\u00b7men", "kr\u00f6\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eja meiner Liebe Macht pr\u00e4gt mir den Eifer ein,", "tokens": ["\u201e", "ja", "mei\u00b7ner", "Lie\u00b7be", "Macht", "pr\u00e4gt", "mir", "den", "Ei\u00b7fer", "ein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201everfechter deines Rechts und deines Throns zu seyn.", "tokens": ["\u201e", "ver\u00b7fech\u00b7ter", "dei\u00b7nes", "Rechts", "und", "dei\u00b7nes", "Throns", "zu", "seyn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201emein Wunsch geht nur dahin, dem Vaterland zu n\u00fczen;", "tokens": ["\u201e", "mein", "Wunsch", "geht", "nur", "da\u00b7hin", ",", "dem", "Va\u00b7ter\u00b7land", "zu", "n\u00fc\u00b7zen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADV", "PAV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "160\u201dDein Volck, Theresia! dein Haus und dich zu sch\u00fczen.", "tokens": ["\"", "Dein", "Volck", ",", "The\u00b7re\u00b7sia", "!", "dein", "Haus", "und", "dich", "zu", "sch\u00fc\u00b7zen", "."], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "NE", "$.", "PPOSAT", "NN", "KON", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}