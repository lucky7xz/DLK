{"textgrid.poem.63541": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gedenkst du noch der Zeit,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedenkst du noch der Zeit,", "tokens": ["Ge\u00b7denkst", "du", "noch", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da wir uns alles waren?", "tokens": ["Da", "wir", "uns", "al\u00b7les", "wa\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PIS", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die liegt so weit, so weit!", "tokens": ["Die", "liegt", "so", "weit", ",", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich noch so unerfahren,", "tokens": ["Ich", "noch", "so", "un\u00b7er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du schon durch Leid gereift,", "tokens": ["Du", "schon", "durch", "Leid", "ge\u00b7reift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Todm\u00fcd in jungen Jahren.", "tokens": ["Tod\u00b7m\u00fcd", "in", "jun\u00b7gen", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Lang war ich umgeschweift,", "tokens": ["Lang", "war", "ich", "um\u00b7ge\u00b7schweift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch gleich in deinem Banne,", "tokens": ["Doch", "gleich", "in", "dei\u00b7nem", "Ban\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als mich dein Blick gestreift.", "tokens": ["Als", "mich", "dein", "Blick", "ge\u00b7streift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Lieb', in kurzer Spanne", "tokens": ["O", "Lieb'", ",", "in", "kur\u00b7zer", "Span\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schufst du das Weib zum Kind,", "tokens": ["Schufst", "du", "das", "Weib", "zum", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den jungen Fant zum Manne.", "tokens": ["Den", "jun\u00b7gen", "Fant", "zum", "Man\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es kam ein Wirbelwind", "tokens": ["Es", "kam", "ein", "Wir\u00b7bel\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und fuhr in unsre Flammen \u2013", "tokens": ["Und", "fuhr", "in", "uns\u00b7re", "Flam\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Wonnen kurz und blind!", "tokens": ["O", "Won\u00b7nen", "kurz", "und", "blind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So standen wir beisammen,", "tokens": ["So", "stan\u00b7den", "wir", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Reue nicht geschreckt,", "tokens": ["Von", "Reu\u00b7e", "nicht", "ge\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch von der Welt Verdammen.", "tokens": ["Noch", "von", "der", "Welt", "Ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was ward in uns geweckt,", "tokens": ["Was", "ward", "in", "uns", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das unsre Seelenbr\u00e4nde", "tokens": ["Das", "uns\u00b7re", "See\u00b7len\u00b7br\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit eis'gen Schauern deckt'?", "tokens": ["Mit", "eis'\u00b7gen", "Schau\u00b7ern", "deckt'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ist's m\u00f6glich? ", "tokens": ["Ist's", "m\u00f6g\u00b7lich", "?"], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Was kaum noch ", "tokens": ["Was", "kaum", "noch"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Kein Wort? kein Druck der H\u00e4nde?", "tokens": ["Kein", "Wort", "?", "kein", "Druck", "der", "H\u00e4n\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und Jahr um Jahr verrann", "tokens": ["Und", "Jahr", "um", "Jahr", "ver\u00b7rann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie unter Eisesh\u00fclle,", "tokens": ["Wie", "un\u00b7ter", "Ei\u00b7ses\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was auch die Parze spann.", "tokens": ["Was", "auch", "die", "Par\u00b7ze", "spann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wie hast du nur so stille", "tokens": ["Wie", "hast", "du", "nur", "so", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Zeiten durchgeharrt?", "tokens": ["Die", "Zei\u00b7ten", "durch\u00b7ge\u00b7harrt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War's Schicksal? war's dein Wille?", "tokens": ["Wa\u00b7r's", "Schick\u00b7sal", "?", "wa\u00b7r's", "dein", "Wil\u00b7le", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Kein Hauch der Gegenwart", "tokens": ["Kein", "Hauch", "der", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von mir zu dir, wenn selten", "tokens": ["Von", "mir", "zu", "dir", ",", "wenn", "sel\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "APPR", "PPER", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Genannt dein Name ward.", "tokens": ["Ge\u00b7nannt", "dein", "Na\u00b7me", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Zwei ferne, fremde Welten", "tokens": ["Zwei", "fer\u00b7ne", ",", "frem\u00b7de", "Wel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["CARD", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "All unser Freud' und Leid,", "tokens": ["All", "un\u00b7ser", "Freud'", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die einst so nah gesellten \u2013", "tokens": ["Die", "einst", "so", "nah", "ge\u00b7sell\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Gedenkst du noch der Zeit?", "tokens": ["Ge\u00b7denkst", "du", "noch", "der", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}