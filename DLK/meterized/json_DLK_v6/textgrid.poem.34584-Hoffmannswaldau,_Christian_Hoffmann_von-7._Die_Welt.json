{"textgrid.poem.34584": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "7. Die Welt", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Welt, und ihr ber\u00fchmtes gl\u00e4ntzen?", "tokens": ["Was", "ist", "die", "Welt", ",", "und", "ihr", "be\u00b7r\u00fchm\u00b7tes", "gl\u00e4nt\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "KON", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ist die Welt und ihre gantze Pracht?", "tokens": ["Was", "ist", "die", "Welt", "und", "ih\u00b7re", "gant\u00b7ze", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein schn\u00f6der Schein in kurtzgefasten Grentzen,", "tokens": ["Ein", "schn\u00f6\u00b7der", "Schein", "in", "kurtz\u00b7ge\u00b7fas\u00b7ten", "Grent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein schneller Blitz, bey schwarzgew\u00f6lckter Nacht;", "tokens": ["Ein", "schnel\u00b7ler", "Blitz", ",", "bey", "schwarz\u00b7ge\u00b7w\u00f6lck\u00b7ter", "Nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein bundtes Feld, da Kummerdisteln gr\u00fcnen;", "tokens": ["Ein", "bund\u00b7tes", "Feld", ",", "da", "Kum\u00b7mer\u00b7dis\u00b7teln", "gr\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein sch\u00f6n Spital, so voller Kranckheit steckt.", "tokens": ["Ein", "sch\u00f6n", "Spi\u00b7tal", ",", "so", "vol\u00b7ler", "Kran\u00b7ck\u00b7heit", "steckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Ein Sclavenhau\u00df, da alle Menschen dienen,", "tokens": ["Ein", "Scla\u00b7ven\u00b7hau\u00df", ",", "da", "al\u00b7le", "Men\u00b7schen", "die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein faules Grab, so Alabaster deckt.", "tokens": ["Ein", "fau\u00b7les", "Grab", ",", "so", "A\u00b7la\u00b7bas\u00b7ter", "deckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das ist der Grund, darauff wir Menschen bauen,", "tokens": ["Das", "ist", "der", "Grund", ",", "dar\u00b7auff", "wir", "Men\u00b7schen", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PAV", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und was das Fleisch f\u00fcr einen Abgott h\u00e4lt.", "tokens": ["Und", "was", "das", "Fleisch", "f\u00fcr", "ei\u00b7nen", "Ab\u00b7gott", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Komm Seele, komm, und lerne weiter schauen,", "tokens": ["Komm", "See\u00b7le", ",", "komm", ",", "und", "ler\u00b7ne", "wei\u00b7ter", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "$,", "KON", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Als sich erstreckt der Zirckel dieser Welt.", "tokens": ["Als", "sich", "er\u00b7streckt", "der", "Zir\u00b7ckel", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Streich ab von dir derselben kurtzes Prangen,", "tokens": ["Streich", "ab", "von", "dir", "der\u00b7sel\u00b7ben", "kurt\u00b7zes", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Halt ihre Lust f\u00fcr eine schwere Last.", "tokens": ["Halt", "ih\u00b7re", "Lust", "f\u00fcr", "ei\u00b7ne", "schwe\u00b7re", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "So wirst du leicht in diesen Port gelangen,", "tokens": ["So", "wirst", "du", "leicht", "in", "die\u00b7sen", "Port", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Da Ewigkeit und Sch\u00f6nheit sich umbfast.", "tokens": ["Da", "E\u00b7wig\u00b7keit", "und", "Sch\u00f6n\u00b7heit", "sich", "umb\u00b7fast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}