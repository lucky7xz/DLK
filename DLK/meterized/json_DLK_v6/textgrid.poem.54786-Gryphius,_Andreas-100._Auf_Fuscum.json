{"textgrid.poem.54786": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "100. Auf Fuscum", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Damit du nicht mehr d\u00f6rffst mein Werck vor dein ausgeben:", "tokens": ["Da\u00b7mit", "du", "nicht", "mehr", "d\u00f6rffst", "mein", "Werck", "vor", "dein", "aus\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df ich den Drucker dich der Arbeit \u00fcberheben.", "tokens": ["La\u00df", "ich", "den", "Dru\u00b7cker", "dich", "der", "Ar\u00b7beit", "\u00fc\u00b7ber\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich bin nicht mehr denn du/ ich bin was du gewesen/", "tokens": ["Ich", "bin", "nicht", "mehr", "denn", "du", "/", "ich", "bin", "was", "du", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "KON", "PPER", "$(", "PPER", "VAFIN", "PWS", "PPER", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald wirst du seyn was ich. Mein Wissen/ Thun und Lesen/", "tokens": ["Bald", "wirst", "du", "seyn", "was", "ich", ".", "Mein", "Wis\u00b7sen", "/", "Thun", "und", "Le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VAINF", "PWS", "PPER", "$.", "PPOSAT", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Nahme/ meine Zeitt/ mein Leben/ Ruhm u\u00f1 Stand", "tokens": ["Mein", "Nah\u00b7me", "/", "mei\u00b7ne", "Zeitt", "/", "mein", "Le\u00b7ben", "/", "Ruhm", "u\u00f1", "Stand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verschwunden als ein Rauch. Die leichte Hand voll Sand;", "tokens": ["Ver\u00b7schwun\u00b7den", "als", "ein", "Rauch", ".", "Die", "leich\u00b7te", "Hand", "voll", "Sand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$.", "ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verdeckt denselben Leib den vorhin vil geehret", "tokens": ["Ver\u00b7deckt", "den\u00b7sel\u00b7ben", "Leib", "den", "vor\u00b7hin", "vil", "ge\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "ART", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den nechst der Fiber Glutt itzt F\u00e4ull und Stanck zust\u00f6ret.", "tokens": ["Den", "nechst", "der", "Fi\u00b7ber", "Glutt", "itzt", "F\u00e4ull", "und", "Stanck", "zu\u00b7st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Beweine wer du bist/ nicht mich/ nur deine Noht.", "tokens": ["Be\u00b7wei\u00b7ne", "wer", "du", "bist", "/", "nicht", "mich", "/", "nur", "dei\u00b7ne", "Noht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VAFIN", "$(", "PTKNEG", "PPER", "$(", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du gehst in dem du gehst und stehst und ruhst zum Tod.", "tokens": ["Du", "gehst", "in", "dem", "du", "gehst", "und", "stehst", "und", "ruhst", "zum", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}