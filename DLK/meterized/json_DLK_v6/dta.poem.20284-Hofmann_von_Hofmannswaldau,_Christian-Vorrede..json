{"dta.poem.20284": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der mord-geist der stadt Rom stieg in den pful der h\u00f6llen/", "tokens": ["Der", "mord\u00b7geist", "der", "stadt", "Rom", "stieg", "in", "den", "pful", "der", "h\u00f6l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NE", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nahm die Furien ihm zu geh\u00fclffen an:", "tokens": ["Und", "nahm", "die", "Fu\u00b7ri\u00b7en", "ihm", "zu", "ge\u00b7h\u00fclf\u00b7fen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kommt/ sagt er/ helffet mir einst den Augustus f\u00e4llen;", "tokens": ["Kommt", "/", "sagt", "er", "/", "helf\u00b7fet", "mir", "einst", "den", "Au\u00b7gus\u00b7tus", "f\u00e4l\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Weil arglist/ gifft und stahl ihn nicht verletzen kan.", "tokens": ["Weil", "arg\u00b7list", "/", "gifft", "und", "stahl", "ihn", "nicht", "ver\u00b7let\u00b7zen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie aber wolten nicht sich dessen unterfangen;", "tokens": ["Sie", "a\u00b7ber", "wol\u00b7ten", "nicht", "sich", "des\u00b7sen", "un\u00b7ter\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PTKNEG", "PRF", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was Furien zu arg/ hat Livia begangen.", "tokens": ["Was", "Fu\u00b7ri\u00b7en", "zu", "arg", "/", "hat", "Li\u00b7via", "be\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKA", "ADJD", "$(", "VAFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}