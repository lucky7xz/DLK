{"dta.poem.6084": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie herrlich nehmen die, mit Licht und Schein", "tokens": ["Wie", "herr\u00b7lich", "neh\u00b7men", "die", ",", "mit", "Licht", "und", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wundersch\u00f6n bezogne, Himmels-Decken", "tokens": ["So", "wun\u00b7der\u00b7sch\u00f6n", "be\u00b7zog\u00b7ne", ",", "Him\u00b7mels\u00b7De\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In unsern Augen doch die Stellen ein", "tokens": ["In", "un\u00b7sern", "Au\u00b7gen", "doch", "die", "Stel\u00b7len", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der C\u00f6rper, welche uns die Schatten gantz verstecken!", "tokens": ["Der", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "uns", "die", "Schat\u00b7ten", "gantz", "ver\u00b7ste\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was f\u00fcr ein herrliches Speetacul! welche B\u00fchne", "tokens": ["Was", "f\u00fcr", "ein", "herr\u00b7li\u00b7ches", "Spee\u00b7ta\u00b7cul", "!", "wel\u00b7che", "B\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "$.", "PWAT", "NN"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Voll Majest\u00e4t und Pracht!", "tokens": ["Voll", "Ma\u00b7jes\u00b7t\u00e4t", "und", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was uns daselbst an C\u00f6rpern je erschiene,", "tokens": ["Was", "uns", "da\u00b7selbst", "an", "C\u00f6r\u00b7pern", "je", "er\u00b7schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "APPR", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nimmt alles seinen Rang und Ordnung stets in Acht.", "tokens": ["Nimmt", "al\u00b7les", "sei\u00b7nen", "Rang", "und", "Ord\u00b7nung", "stets", "in", "Acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "KON", "NN", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es schwindet kaum des hellem Tages Schein;", "tokens": ["Es", "schwin\u00b7det", "kaum", "des", "hel\u00b7lem", "Ta\u00b7ges", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So stellen sich die Stern\u2019 in hellen Schimmer ein.", "tokens": ["So", "stel\u00b7len", "sich", "die", "Stern'", "in", "hel\u00b7len", "Schim\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}