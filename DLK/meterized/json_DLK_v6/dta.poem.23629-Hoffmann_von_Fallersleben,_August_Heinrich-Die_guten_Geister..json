{"dta.poem.23629": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die guten Geister.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bist du auch hienieden", "tokens": ["Bist", "du", "auch", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gar gering und arm,", "tokens": ["Gar", "ge\u00b7ring", "und", "arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Herz, gieb dich zufrieden,", "tokens": ["Herz", ",", "gieb", "dich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Lass den Gram und Harm!", "tokens": ["Lass", "den", "Gram", "und", "Harm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Denn die h\u00f6chsten Gaben", "tokens": ["Denn", "die", "h\u00f6chs\u00b7ten", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sind auch dir nicht fern,", "tokens": ["Sind", "auch", "dir", "nicht", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Weil wir alle haben", "tokens": ["Weil", "wir", "al\u00b7le", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "VAFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Einen Gott und Herrn;", "tokens": ["Ei\u00b7nen", "Gott", "und", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Einen Herrn und Meister,", "tokens": ["Ei\u00b7nen", "Herrn", "und", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und ein Himmelreich \u2014", "tokens": ["Und", "ein", "Him\u00b7mel\u00b7reich"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alle guten Geister", "tokens": ["Al\u00b7le", "gu\u00b7ten", "Geis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sind auf Erden gleich.", "tokens": ["Sind", "auf", "Er\u00b7den", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}