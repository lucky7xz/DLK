{"textgrid.poem.45649": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Zu einer Biographie G\u00f6tz von Berlichingens", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Faustrecht gilt noch heut, die Faust bestimmt das Recht;", "tokens": ["Das", "Faus\u00b7trecht", "gilt", "noch", "heut", ",", "die", "Faust", "be\u00b7stimmt", "das", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch weil gebildet auch das Schmutzgeschlecht,", "tokens": ["Doch", "weil", "ge\u00b7bil\u00b7det", "auch", "das", "Schmutz\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVPP", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zog sich der Mut vom Herzen ins Gehirn,", "tokens": ["Zog", "sich", "der", "Mut", "vom", "Her\u00b7zen", "ins", "Ge\u00b7hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Statt eiserner Hand, die eiserne Stirn.", "tokens": ["Statt", "ei\u00b7ser\u00b7ner", "Hand", ",", "die", "ei\u00b7ser\u00b7ne", "Stirn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}