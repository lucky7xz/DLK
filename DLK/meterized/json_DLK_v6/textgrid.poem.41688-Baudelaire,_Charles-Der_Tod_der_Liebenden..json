{"textgrid.poem.41688": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod der Liebenden.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir werden Lager tief wie Gr\u00fcfte finden,", "tokens": ["Wir", "wer\u00b7den", "La\u00b7ger", "tief", "wie", "Gr\u00fcf\u00b7te", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADJD", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die leichte Wohlger\u00fcche \u00fcberspr\u00fchn,", "tokens": ["Die", "leich\u00b7te", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "\u00fc\u00b7bers\u00b7pr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seltne Blumen werden sich uns winden,", "tokens": ["Und", "selt\u00b7ne", "Blu\u00b7men", "wer\u00b7den", "sich", "uns", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PRF", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unter sch\u00f6nrem Himmel uns erbl\u00fchn.", "tokens": ["Die", "un\u00b7ter", "sch\u00f6n\u00b7rem", "Him\u00b7mel", "uns", "er\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die letzten Gluten hauchend, die entschwinden,", "tokens": ["Die", "letz\u00b7ten", "Glu\u00b7ten", "hau\u00b7chend", ",", "die", "ent\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind unsre Herzen Fackeln, licht und k\u00fchn,", "tokens": ["Sind", "uns\u00b7re", "Her\u00b7zen", "Fa\u00b7ckeln", ",", "licht", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und lassen Feuer, die sie hold verbinden,", "tokens": ["Und", "las\u00b7sen", "Feu\u00b7er", ",", "die", "sie", "hold", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus unsrer Geister Zwillingsspiegeln gl\u00fchn.", "tokens": ["Aus", "uns\u00b7rer", "Geis\u00b7ter", "Zwil\u00b7lings\u00b7spie\u00b7geln", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wann Blau und Rosig abends mystisch scheinen,", "tokens": ["Wann", "Blau", "und", "Ro\u00b7sig", "a\u00b7bends", "mys\u00b7tisch", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NE", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df tiefen Blick uns tauschen, wie ein Weinen,", "tokens": ["La\u00df", "tie\u00b7fen", "Blick", "uns", "tau\u00b7schen", ",", "wie", "ein", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Schluchzen, das nur Abschied atmen soll.", "tokens": ["Ein", "Schluch\u00b7zen", ",", "das", "nur", "Ab\u00b7schied", "at\u00b7men", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann schiebt ein Engel sacht zur\u00fcck die Riegel,", "tokens": ["Dann", "schiebt", "ein", "En\u00b7gel", "sacht", "zu\u00b7r\u00fcck", "die", "Rie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und neu belebt er, treu und liebevoll,", "tokens": ["Und", "neu", "be\u00b7lebt", "er", ",", "treu", "und", "lie\u00b7be\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die toten Flammen und die tr\u00fcben Spiegel.", "tokens": ["Die", "to\u00b7ten", "Flam\u00b7men", "und", "die", "tr\u00fc\u00b7ben", "Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}