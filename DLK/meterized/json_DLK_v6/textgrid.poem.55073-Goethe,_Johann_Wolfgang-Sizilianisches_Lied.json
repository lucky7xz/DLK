{"textgrid.poem.55073": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Sizilianisches Lied", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr schwarzen \u00c4ugelein!", "tokens": ["Ihr", "schwar\u00b7zen", "\u00c4u\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn ihr nur winket,", "tokens": ["Wenn", "ihr", "nur", "win\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Es fallen H\u00e4user ein,", "tokens": ["Es", "fal\u00b7len", "H\u00e4u\u00b7ser", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es fallen St\u00e4dte;", "tokens": ["Es", "fal\u00b7len", "St\u00e4d\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und diese Leimenwand", "tokens": ["Und", "die\u00b7se", "Lei\u00b7men\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vor meinem Herzen \u2013", "tokens": ["Vor", "mei\u00b7nem", "Her\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Bedenk doch nur einmal \u2013", "tokens": ["Be\u00b7denk", "doch", "nur", "ein\u00b7mal", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die sollt nicht fallen!", "tokens": ["Die", "sollt", "nicht", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}