{"textgrid.poem.61793": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An eine junge S\u00e4ngerin", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, noch einmal diese T\u00f6ne,", "tokens": ["Ach", ",", "noch", "ein\u00b7mal", "die\u00b7se", "T\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir Fl\u00fcgel in das sch\u00f6ne", "tokens": ["Die", "mir", "Fl\u00fc\u00b7gel", "in", "das", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zauberland der Jugend sind!", "tokens": ["Zau\u00b7ber\u00b7land", "der", "Ju\u00b7gend", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df sie schwellen voll und leise!", "tokens": ["La\u00df", "sie", "schwel\u00b7len", "voll", "und", "lei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese Weise", "tokens": ["Die\u00b7se", "Wei\u00b7se"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sang einst deine Mutter, Kind.", "tokens": ["Sang", "einst", "dei\u00b7ne", "Mut\u00b7ter", ",", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Klavier dort in der Nische", "tokens": ["Am", "Kla\u00b7vier", "dort", "in", "der", "Ni\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sa\u00df sie, wenn des Abends Frische", "tokens": ["Sa\u00df", "sie", ",", "wenn", "des", "A\u00b7bends", "Fri\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klar ins offne Fenster drang;", "tokens": ["Klar", "ins", "off\u00b7ne", "Fens\u00b7ter", "drang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Golden wob's um ihre Locken,", "tokens": ["Gol\u00b7den", "wob's", "um", "ih\u00b7re", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wie Glocken", "tokens": ["Und", "wie", "Glo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Schwebte wogend ihr Gesang.", "tokens": ["Schweb\u00b7te", "wo\u00b7gend", "ihr", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, das war vor langen Jahren,", "tokens": ["Ach", ",", "das", "war", "vor", "lan\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' ich in die Welt gefahren,", "tokens": ["Eh'", "ich", "in", "die", "Welt", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoch im Sturm noch trieb mein Herz;", "tokens": ["Hoch", "im", "Sturm", "noch", "trieb", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber stets bei ihrem Liede", "tokens": ["A\u00b7ber", "stets", "bei", "ih\u00b7rem", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kam ein Friede", "tokens": ["Kam", "ein", "Frie\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "In des J\u00fcnglings Lust und Schmerz.", "tokens": ["In", "des", "J\u00fcng\u00b7lings", "Lust", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Grau jetzt, mit ged\u00e4mpftem Feuer,", "tokens": ["Grau", "jetzt", ",", "mit", "ge\u00b7d\u00e4mpf\u00b7tem", "Feu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam kehr' ich; die mir teuer,", "tokens": ["Ein\u00b7sam", "kehr'", "ich", ";", "die", "mir", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "PRELS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gingen alle fast zur Ruh';", "tokens": ["Gin\u00b7gen", "al\u00b7le", "fast", "zur", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie auch schl\u00e4ft, die s\u00fc\u00dfe Rose,", "tokens": ["Sie", "auch", "schl\u00e4ft", ",", "die", "s\u00fc\u00b7\u00dfe", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unterm Moose,", "tokens": ["Un\u00b7term", "Moo\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Doch ihr Ebenbild bist du.", "tokens": ["Doch", "ihr", "E\u00b7ben\u00b7bild", "bist", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Singe, Kind, und in die blauen", "tokens": ["Sin\u00b7ge", ",", "Kind", ",", "und", "in", "die", "blau\u00b7en"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KON", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Augen la\u00df mich tief dir schauen!", "tokens": ["Au\u00b7gen", "la\u00df", "mich", "tief", "dir", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jugendheimw\u00e4rts tr\u00e4umt mein Sinn,", "tokens": ["Ju\u00b7gend\u00b7heim\u00b7w\u00e4rts", "tr\u00e4umt", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von l\u00e4ngst entschwundnen Lenzen", "tokens": ["Und", "von", "l\u00e4ngst", "ent\u00b7schwund\u00b7nen", "Len\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zieht ein Gl\u00e4nzen", "tokens": ["Zieht", "ein", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Durch die m\u00fcde Brust dahin.", "tokens": ["Durch", "die", "m\u00fc\u00b7de", "Brust", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}