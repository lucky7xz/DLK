{"textgrid.poem.60107": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bl\u00fcmchen, so wie diese,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00fcmchen, so wie diese,", "tokens": ["Bl\u00fcm\u00b7chen", ",", "so", "wie", "die\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "PDS", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Gibt uns Wald und Wiese,", "tokens": ["Gibt", "uns", "Wald", "und", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Berg und Thal;", "tokens": ["Berg", "und", "Thal", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Jeder kann sie finden,", "tokens": ["Je\u00b7der", "kann", "sie", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Kann sich Kr\u00e4nze winden", "tokens": ["Kann", "sich", "Kr\u00e4n\u00b7ze", "win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ohne Zahl.", "tokens": ["Oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "O wie viele Freuden,", "tokens": ["O", "wie", "vie\u00b7le", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn man sie bescheiden", "tokens": ["Wenn", "man", "sie", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht verschm\u00e4ht,", "tokens": ["Nicht", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Sind, wohin wir wallen,", "tokens": ["Sind", ",", "wo\u00b7hin", "wir", "wal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ueberall und Allen", "tokens": ["Ue\u00b7be\u00b7rall", "und", "Al\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hinges\u00e4t!", "tokens": ["Hin\u00b7ge\u00b7s\u00e4t", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Aber ach! der Wiese", "tokens": ["A\u00b7ber", "ach", "!", "der", "Wie\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$.", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bl\u00fcmchen, so wie diese,", "tokens": ["Bl\u00fcm\u00b7chen", ",", "so", "wie", "die\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "PDS", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sterben bald;", "tokens": ["Ster\u00b7ben", "bald", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ihrer sieht man keines,", "tokens": ["Ih\u00b7rer", "sieht", "man", "kei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PIS", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn im Herbst des Haines", "tokens": ["Wenn", "im", "Herbst", "des", "Hai\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Lied verhallt.", "tokens": ["Lied", "ver\u00b7hallt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Keines konnt' ich finden", "tokens": ["Kei\u00b7nes", "konnt'", "ich", "fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jetzt in \u00f6den Gr\u00fcnden;", "tokens": ["Jetzt", "in", "\u00f6\u00b7den", "Gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dennoch dir", "tokens": ["Den\u00b7noch", "dir"], "token_info": ["word", "word"], "pos": ["ADV", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Einen Strau\u00df zu geben,", "tokens": ["Ei\u00b7nen", "Strau\u00df", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schafft' ich neues Leben", "tokens": ["Schafft'", "ich", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Diesen hier.", "tokens": ["Die\u00b7sen", "hier", "."], "token_info": ["word", "word", "punct"], "pos": ["PDS", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Alle Menschenfreuden", "tokens": ["Al\u00b7le", "Men\u00b7schen\u00b7freu\u00b7den"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sind im fr\u00fchen Scheiden", "tokens": ["Sind", "im", "fr\u00fc\u00b7hen", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gleicher Art:", "tokens": ["Glei\u00b7cher", "Art", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Selig, wer, wie diese", "tokens": ["Se\u00b7lig", ",", "wer", ",", "wie", "die\u00b7se"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PWS", "$,", "PWAV", "PDS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Kinderchen der Wiese,", "tokens": ["Kin\u00b7der\u00b7chen", "der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Sie bewahrt!", "tokens": ["Sie", "be\u00b7wahrt", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}