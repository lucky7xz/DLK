{"textgrid.poem.40657": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um einen Fr\u00fchling ist mein Leben \u00e4rmer!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um einen Fr\u00fchling ist mein Leben \u00e4rmer!", "tokens": ["Um", "ei\u00b7nen", "Fr\u00fch\u00b7ling", "ist", "mein", "Le\u00b7ben", "\u00e4r\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Lenz verbl\u00fchte unbemerkt, verlassen,", "tokens": ["Ein", "Lenz", "ver\u00b7bl\u00fch\u00b7te", "un\u00b7be\u00b7merkt", ",", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umsonst lie\u00df er die Luft sein Gold verprassen,", "tokens": ["Um\u00b7sonst", "lie\u00df", "er", "die", "Luft", "sein", "Gold", "ver\u00b7pras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Im Wald sich heiser schmettern bunte L\u00e4rmer.", "tokens": ["Im", "Wald", "sich", "hei\u00b7ser", "schmet\u00b7tern", "bun\u00b7te", "L\u00e4r\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Traun, jenes Jahr hat keinen Fr\u00fchlingsschw\u00e4rmer;", "tokens": ["Traun", ",", "je\u00b7nes", "Jahr", "hat", "kei\u00b7nen", "Fr\u00fch\u00b7lings\u00b7schw\u00e4r\u00b7mer", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da stimmten vollern Chor die V\u00f6lkermassen,", "tokens": ["Da", "stimm\u00b7ten", "vol\u00b7lern", "Chor", "die", "V\u00f6l\u00b7ker\u00b7mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da bl\u00fchten schwarz-roth-golden selbst die Gassen,", "tokens": ["Da", "bl\u00fch\u00b7ten", "schwa\u00b7rz\u00b7ro\u00b7th\u00b7gol\u00b7den", "selbst", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Volksrath die Gestirne flammten w\u00e4rmer.", "tokens": ["Im", "Volks\u00b7rath", "die", "Ge\u00b7stir\u00b7ne", "flamm\u00b7ten", "w\u00e4r\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So ganz verga\u00df ich, da\u00df Natur auch bl\u00fchte!", "tokens": ["So", "ganz", "ver\u00b7ga\u00df", "ich", ",", "da\u00df", "Na\u00b7tur", "auch", "bl\u00fch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "KOUS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich frug um ihren Lenz erst, als schon Flocken", "tokens": ["Ich", "frug", "um", "ih\u00b7ren", "Lenz", "erst", ",", "als", "schon", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,", "KOUS", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Schneegew\u00f6lk auf d\u00fcrre Stoppeln spr\u00fchte. \u2013", "tokens": ["Das", "Schnee\u00b7ge\u00b7w\u00f6lk", "auf", "d\u00fcr\u00b7re", "Stop\u00b7peln", "spr\u00fch\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Lenz kam aufs Neu; ich aber f\u00fchl' erschrocken,", "tokens": ["Lenz", "kam", "aufs", "Neu", ";", "ich", "a\u00b7ber", "f\u00fchl'", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$.", "PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df Duft und Bl\u00fcthenspiel mich wieder locken,", "tokens": ["Da\u00df", "Duft", "und", "Bl\u00fc\u00b7then\u00b7spiel", "mich", "wie\u00b7der", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Waldstimmen wieder r\u00fchren mein Gem\u00fcthe!", "tokens": ["Wald\u00b7stim\u00b7men", "wie\u00b7der", "r\u00fch\u00b7ren", "mein", "Ge\u00b7m\u00fc\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}