{"textgrid.poem.42325": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Mittel gegen den Hochmut der Gro\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Klagen h\u00f6r' ich oft erheben", "tokens": ["Viel", "Kla\u00b7gen", "h\u00f6r'", "ich", "oft", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Hochmut, den der Gro\u00dfe \u00fcbt.", "tokens": ["Vom", "Hoch\u00b7mut", ",", "den", "der", "Gro\u00b7\u00dfe", "\u00fcbt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Gro\u00dfen Hochmut wird sich geben,", "tokens": ["Der", "Gro\u00b7\u00dfen", "Hoch\u00b7mut", "wird", "sich", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn unsre Kriecherei sich gibt.", "tokens": ["Wenn", "uns\u00b7re", "Krie\u00b7che\u00b7rei", "sich", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}