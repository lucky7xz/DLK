{"dta.poem.2233": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Menschen eigentlicher Gott ist Gold und Geld,", "tokens": ["Der", "Men\u00b7schen", "ei\u00b7gent\u00b7li\u00b7cher", "Gott", "ist", "Gold", "und", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sie beten zwar,", "tokens": ["sie", "be\u00b7ten", "zwar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In ihrem \u00e4u\u00dferlichen Anstand und Kirchen einen andern", "tokens": ["In", "ih\u00b7rem", "\u00e4u\u00b7\u00dfer\u00b7li\u00b7chen", "An\u00b7stand", "und", "Kir\u00b7chen", "ei\u00b7nen", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "an:", "tokens": ["an", ":"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Es meynens auch die meisten selbst, so wie es scheinet,", "tokens": ["Es", "mey\u00b7nens", "auch", "die", "meis\u00b7ten", "selbst", ",", "so", "wie", "es", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PIS", "ADV", "$,", "ADV", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "das ist wahr,", "tokens": ["das", "ist", "wahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Doch untersucht mans, merkt man leider! der Meisten Ab-", "tokens": ["Doch", "un\u00b7ter\u00b7sucht", "mans", ",", "merkt", "man", "lei\u00b7der", "!", "der", "Meis\u00b7ten", "Ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "$,", "VVFIN", "PIS", "ADV", "$.", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "sicht offenbar,", "tokens": ["sicht", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Sie ehren Gott, weil, da er reich, er ihnen Reichthum", "tokens": ["Sie", "eh\u00b7ren", "Gott", ",", "weil", ",", "da", "er", "reich", ",", "er", "ih\u00b7nen", "Reicht\u00b7hum"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "$,", "KOUS", "PPER", "ADJD", "$,", "PPER", "PPER", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "geben kann.", "tokens": ["ge\u00b7ben", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}