{"dta.poem.13261": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ezu gleicher Zeit geschah die schon erz\u00e4hlte Sach.", "tokens": ["\u201e", "zu", "glei\u00b7cher", "Zeit", "ge\u00b7schah", "die", "schon", "er\u00b7z\u00e4hl\u00b7te", "Sach", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "VVFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201enun forsch\u2019 ich der so wohl, als diesem Priester nach.", "tokens": ["\u201e", "nun", "for\u00b7sch'", "ich", "der", "so", "wohl", ",", "als", "die\u00b7sem", "Pries\u00b7ter", "nach", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "ADV", "ADV", "$,", "KOUS", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "375\u201dAllein was kann mein Geist bey solchem Fall gedencken?", "tokens": ["\"", "Al\u00b7lein", "was", "kann", "mein", "Geist", "bey", "sol\u00b7chem", "Fall", "ge\u00b7den\u00b7cken", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PWS", "VMFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eich mag desselben Kraft dort oder dahin lencken,", "tokens": ["\u201e", "ich", "mag", "des\u00b7sel\u00b7ben", "Kraft", "dort", "o\u00b7der", "da\u00b7hin", "len\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PDAT", "NN", "ADV", "KON", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.5": {"text": "\u201eso bin ich doch erstaunt. Ich hab ein Bild erblickt,", "tokens": ["\u201e", "so", "bin", "ich", "doch", "er\u00b7staunt", ".", "Ich", "hab", "ein", "Bild", "er\u00b7blickt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edas mir erst neuen Wahn in meine Sinnen dr\u00fcckt.", "tokens": ["\u201e", "das", "mir", "erst", "neu\u00b7en", "Wahn", "in", "mei\u00b7ne", "Sin\u00b7nen", "dr\u00fcckt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ees hangt in dem Gemach. Wann ich recht wahr genommen,", "tokens": ["\u201e", "es", "hangt", "in", "dem", "Ge\u00b7mach", ".", "Wann", "ich", "recht", "wahr", "ge\u00b7nom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "PWAV", "PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "380\u201dSo zeigt es die Gestallt, die mir ist vorgekommen:", "tokens": ["\"", "So", "zeigt", "es", "die", "Ge\u00b7stallt", ",", "die", "mir", "ist", "vor\u00b7ge\u00b7kom\u00b7men", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edas magere Gesicht; die Demuths-volle Mien;", "tokens": ["\u201e", "das", "ma\u00b7ge\u00b7re", "Ge\u00b7sicht", ";", "die", "De\u00b7muths\u00b7vol\u00b7le", "Mien", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eder Augen Fr\u00f6mmigkeit; das kraus-behaarte Kinn;", "tokens": ["\u201e", "der", "Au\u00b7gen", "Fr\u00f6m\u00b7mig\u00b7keit", ";", "das", "kraus\u00b7be\u00b7haar\u00b7te", "Kinn", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eder kurze graue Belz, der an den Achseln hienge,", "tokens": ["\u201e", "der", "kur\u00b7ze", "grau\u00b7e", "Belz", ",", "der", "an", "den", "Ach\u00b7seln", "hien\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eerkl\u00e4rten meinem Sinn, ich wei\u00df nicht, was vor Dinge.", "tokens": ["\u201e", "er\u00b7kl\u00e4r\u00b7ten", "mei\u00b7nem", "Sinn", ",", "ich", "wei\u00df", "nicht", ",", "was", "vor", "Din\u00b7ge", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "385\u201dNoch mehr: als er vor mir sich unversehns verlohr;", "tokens": ["\"", "Noch", "mehr", ":", "als", "er", "vor", "mir", "sich", "un\u00b7ver\u00b7sehns", "ver\u00b7lohr", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "KOUS", "PPER", "APPR", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eda kam sein Wesen mir als wie verhimmelt vor:", "tokens": ["\u201e", "da", "kam", "sein", "We\u00b7sen", "mir", "als", "wie", "ver\u00b7him\u00b7melt", "vor", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "KOUS", "KOKOM", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eich wollte mich von ihm, er sich von mir entfernen,", "tokens": ["\u201e", "ich", "woll\u00b7te", "mich", "von", "ihm", ",", "er", "sich", "von", "mir", "ent\u00b7fer\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PRF", "APPR", "PPER", "$,", "PPER", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eso blendte mich ein Schein von f\u00fcnf erleuchten Sternen.", "tokens": ["\u201e", "so", "blend\u00b7te", "mich", "ein", "Schein", "von", "f\u00fcnf", "er\u00b7leuch\u00b7ten", "Ster\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}