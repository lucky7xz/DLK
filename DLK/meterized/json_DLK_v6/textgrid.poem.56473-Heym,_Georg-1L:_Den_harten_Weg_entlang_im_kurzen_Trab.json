{"textgrid.poem.56473": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den harten Weg entlang im kurzen Trab", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den harten Weg entlang im kurzen Trab", "tokens": ["Den", "har\u00b7ten", "Weg", "ent\u00b7lang", "im", "kur\u00b7zen", "Trab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPO", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zieht sich der Str\u00e4flingstrupp, der heim marschiert", "tokens": ["Zieht", "sich", "der", "Str\u00e4f\u00b7lings\u00b7trupp", ",", "der", "heim", "mar\u00b7schiert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "PRELS", "PTKVZ", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch kahle Felder in das gro\u00dfe Grab,", "tokens": ["Durch", "kah\u00b7le", "Fel\u00b7der", "in", "das", "gro\u00b7\u00dfe", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das wie ein Schl\u00e4chterblock ins Graue stiert.", "tokens": ["Das", "wie", "ein", "Schl\u00e4ch\u00b7ter\u00b7block", "ins", "Grau\u00b7e", "stiert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sturm singt. Wind pfeift. Vor ihnen weht und irrt", "tokens": ["Sturm", "singt", ".", "Wind", "pfeift", ".", "Vor", "ih\u00b7nen", "weht", "und", "irrt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "NN", "VVFIN", "$.", "APPR", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ein Haufe alter Bl\u00e4tter kunterbunt.", "tokens": ["Ein", "Hau\u00b7fe", "al\u00b7ter", "Bl\u00e4t\u00b7ter", "kun\u00b7ter\u00b7bunt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die W\u00e4chter schlie\u00dfen ihren Zug. Es klirrt", "tokens": ["Die", "W\u00e4ch\u00b7ter", "schlie\u00b7\u00dfen", "ih\u00b7ren", "Zug", ".", "Es", "klirrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An ihrem Rock das gro\u00dfe Schl\u00fcsselbund.", "tokens": ["An", "ih\u00b7rem", "Rock", "das", "gro\u00b7\u00dfe", "Schl\u00fcs\u00b7sel\u00b7bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das breite Tor geht auf im Riesenbau", "tokens": ["Das", "brei\u00b7te", "Tor", "geht", "auf", "im", "Rie\u00b7sen\u00b7bau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wieder zu. Des Tages roter Rost", "tokens": ["Und", "wie\u00b7der", "zu", ".", "Des", "Ta\u00b7ges", "ro\u00b7ter", "Rost"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bedeckt den Westen. Tr\u00fcbe in dem Blau", "tokens": ["Be\u00b7deckt", "den", "Wes\u00b7ten", ".", "Tr\u00fc\u00b7be", "in", "dem", "Blau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zittert ein Stern im bittern Winterfrost.", "tokens": ["Zit\u00b7tert", "ein", "Stern", "im", "bit\u00b7tern", "Win\u00b7ter\u00b7frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Und ein paar B\u00e4ume stehn den Weg entlang", "tokens": ["Und", "ein", "paar", "B\u00e4u\u00b7me", "stehn", "den", "Weg", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "NN", "VVFIN", "ART", "NN", "APPO"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im halben Licht verkr\u00fcppelt und beleibt.", "tokens": ["Im", "hal\u00b7ben", "Licht", "ver\u00b7kr\u00fcp\u00b7pelt", "und", "be\u00b7leibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie schwarz aus einer Stirn gekr\u00fcmmt und krank", "tokens": ["Wie", "schwarz", "aus", "ei\u00b7ner", "Stirn", "ge\u00b7kr\u00fcmmt", "und", "krank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein starkes Horn steht und nach oben treibt.", "tokens": ["Ein", "star\u00b7kes", "Horn", "steht", "und", "nach", "o\u00b7ben", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}