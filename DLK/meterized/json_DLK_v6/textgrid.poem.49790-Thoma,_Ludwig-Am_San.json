{"textgrid.poem.49790": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Am San", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist die Heimat? Ach, so weit!", "tokens": ["Wo", "ist", "die", "Hei\u00b7mat", "?", "Ach", ",", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$.", "ITJ", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer \u00fcber hundert H\u00fcgel geht,", "tokens": ["Wer", "\u00fc\u00b7ber", "hun\u00b7dert", "H\u00fc\u00b7gel", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer auf dem h\u00f6chsten Berge steht,", "tokens": ["Wer", "auf", "dem", "h\u00f6chs\u00b7ten", "Ber\u00b7ge", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann sie noch nicht erschauen.", "tokens": ["Kann", "sie", "noch", "nicht", "er\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir h\u00f6ren 's wohl im frohen Mai,", "tokens": ["Wir", "h\u00f6\u00b7ren", "'s", "wohl", "im", "fro\u00b7hen", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es gr\u00fcne in der gleichen Welt", "tokens": ["Es", "gr\u00fc\u00b7ne", "in", "der", "glei\u00b7chen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der deutsche Wald, das deutsche Feld,", "tokens": ["Der", "deut\u00b7sche", "Wald", ",", "das", "deut\u00b7sche", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wollen schier nicht trauen.", "tokens": ["Und", "wol\u00b7len", "schier", "nicht", "trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo liegt die Heimat? Ach, so nah!", "tokens": ["Wo", "liegt", "die", "Hei\u00b7mat", "?", "Ach", ",", "so", "nah", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "ITJ", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df mit jedem Herzensschlag,", "tokens": ["Ich", "wei\u00df", "mit", "je\u00b7dem", "Her\u00b7zens\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df nichts von ihr mich scheiden mag,", "tokens": ["Da\u00df", "nichts", "von", "ihr", "mich", "schei\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Berg und Flu\u00df und Auen.", "tokens": ["Nicht", "Berg", "und", "Flu\u00df", "und", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}