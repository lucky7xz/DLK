{"textgrid.poem.28186": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1643, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zuvor ists auch geschehn und darff auch mehr geschehen,", "tokens": ["Zu\u00b7vor", "ists", "auch", "ge\u00b7schehn", "und", "darff", "auch", "mehr", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVPP", "KON", "VMFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich hab es selbst gesehen, begehrs nicht mehr zu sehen,", "tokens": ["Ich", "hab", "es", "selbst", "ge\u00b7se\u00b7hen", ",", "be\u00b7gehrs", "nicht", "mehr", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df auff gepfl\u00fcgtem Feld, in dem es Gerst empfangen,", "tokens": ["Da\u00df", "auff", "ge\u00b7pfl\u00fcg\u00b7tem", "Feld", ",", "in", "dem", "es", "Gerst", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind Pferde nachmals da in kurtzem auffgegangen.", "tokens": ["Sind", "Pfer\u00b7de", "nach\u00b7mals", "da", "in", "kurt\u00b7zem", "auff\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}