{"textgrid.poem.42523": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zuweilen lese ich die sch\u00f6nen Sachen,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zuweilen lese ich die sch\u00f6nen Sachen,", "tokens": ["Zu\u00b7wei\u00b7len", "le\u00b7se", "ich", "die", "sch\u00f6\u00b7nen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die feingekritzelt dir im Album stehn,", "tokens": ["Die", "fein\u00b7ge\u00b7krit\u00b7zelt", "dir", "im", "Al\u00b7bum", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mu\u00df, Verzeihung, \u00fcber manches lachen.", "tokens": ["Und", "mu\u00df", ",", "Ver\u00b7zei\u00b7hung", ",", "\u00fc\u00b7ber", "man\u00b7ches", "la\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "NN", "$,", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "All diese Spr\u00fcche werden bald vergehn;", "tokens": ["All", "die\u00b7se", "Spr\u00fc\u00b7che", "wer\u00b7den", "bald", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDAT", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Und alle Namen, die sich unterschrieben,", "tokens": ["Und", "al\u00b7le", "Na\u00b7men", ",", "die", "sich", "un\u00b7ter\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie werden wie das Laub im Herbst verwehn", "tokens": ["Sie", "wer\u00b7den", "wie", "das", "Laub", "im", "Herbst", "ver\u00b7wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und rasch verwirbeln, alle deine Lieben", "tokens": ["Und", "rasch", "ver\u00b7wir\u00b7beln", ",", "al\u00b7le", "dei\u00b7ne", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVINF", "$,", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Herbst des Lebens schnell zum finstern Grabe", "tokens": ["Vom", "Herbst", "des", "Le\u00b7bens", "schnell", "zum", "fins\u00b7tern", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Enttaumeln und wie Spreu im Wind zerstieben.", "tokens": ["Ent\u00b7tau\u00b7meln", "und", "wie", "Spreu", "im", "Wind", "zer\u00b7stie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PWAV", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbzum Frohgedenken\u00ab mancher lustige Knabe", "tokens": ["\u00bb", "zum", "Froh\u00b7ge\u00b7den\u00b7ken", "\u00ab", "man\u00b7cher", "lus\u00b7ti\u00b7ge", "Kna\u00b7be"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "$(", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schrieb sich hier ein, seis Liebster oder Bruder;", "tokens": ["Schrieb", "sich", "hier", "ein", ",", "seis", "Liebs\u00b7ter", "o\u00b7der", "Bru\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es kr\u00e4chzt nach ihnen auch der alte Rabe,", "tokens": ["Es", "kr\u00e4chzt", "nach", "ih\u00b7nen", "auch", "der", "al\u00b7te", "Ra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der gute Vetter Tod, des tr\u00e4ges Ruder", "tokens": ["Der", "gu\u00b7te", "Vet\u00b7ter", "Tod", ",", "des", "tr\u00e4\u00b7ges", "Ru\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie langsam steuert durch des Hades Fluten,", "tokens": ["Sie", "lang\u00b7sam", "steu\u00b7ert", "durch", "des", "Ha\u00b7des", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Nimmerwiedersehn, so Mann wie Bruder.", "tokens": ["Auf", "Nim\u00b7mer\u00b7wie\u00b7der\u00b7sehn", ",", "so", "Mann", "wie", "Bru\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADV", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "In weiter Ferne, tief in Abendgluten,", "tokens": ["In", "wei\u00b7ter", "Fer\u00b7ne", ",", "tief", "in", "A\u00b7bend\u00b7glu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ersiehst du einmal noch die l\u00e4ngst schon bleichen", "tokens": ["Er\u00b7siehst", "du", "ein\u00b7mal", "noch", "die", "l\u00e4ngst", "schon", "blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In morschen S\u00e4rgen, und dein Herz wird bluten.", "tokens": ["In", "mor\u00b7schen", "S\u00e4r\u00b7gen", ",", "und", "dein", "Herz", "wird", "blu\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich kanns verstehn, da\u00df diese Liebeszeichen", "tokens": ["Ich", "kanns", "ver\u00b7stehn", ",", "da\u00df", "die\u00b7se", "Lie\u00b7bes\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir wert sind. Aber la\u00df sie nicht von andern,", "tokens": ["Dir", "wert", "sind", ".", "A\u00b7ber", "la\u00df", "sie", "nicht", "von", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "$.", "KON", "VVIMP", "PPER", "PTKNEG", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir gleichg\u00fcltigen Menschen je erreichen.", "tokens": ["Dir", "gleich\u00b7g\u00fcl\u00b7ti\u00b7gen", "Men\u00b7schen", "je", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Ein Spott ists, wenn von Hand zu Hand sie wandern.", "tokens": ["Ein", "Spott", "ists", ",", "wenn", "von", "Hand", "zu", "Hand", "sie", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "APPR", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}