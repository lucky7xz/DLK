{"dta.poem.5948": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.84", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ainsi differens Assemblages", "tokens": ["Ain\u00b7si", "dif\u00b7fe\u00b7rens", "As\u00b7sem\u00b7bla\u00b7ges"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "De l\u2019 air subtil, & des Corps \u00e9therez,", "tokens": ["De", "l'", "air", "sub\u00b7til", ",", "&", "des", "Corps", "\u00e9the\u00b7rez", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,", "$(", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Se ramassant dans leurs partages", "tokens": ["Se", "ra\u00b7mas\u00b7sant", "dans", "leurs", "par\u00b7ta\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Autour des Centres s\u00e9parez,", "tokens": ["Au\u00b7tour", "des", "Cen\u00b7tres", "s\u00e9pa\u00b7rez", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Ont fait ces Tourbillons, qui tous sont m\u00fbs de m\u00eame,", "tokens": ["Ont", "fait", "ces", "Tour\u00b7bil\u00b7lons", ",", "qui", "tous", "sont", "m\u00fbs", "de", "m\u00ea\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sur leurs Cercles tournans d\u2019 une vitesse extr\u00eame,", "tokens": ["Sur", "leurs", "Cer\u00b7cles", "tour\u00b7nans", "d'", "u\u00b7ne", "vi\u00b7tes\u00b7se", "ex\u00b7tr\u00ea\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "En divers sens, en diff\u00e9rens degrez.", "tokens": ["En", "di\u00b7vers", "sens", ",", "en", "diff\u00e9rens", "de\u00b7grez", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "$,", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}