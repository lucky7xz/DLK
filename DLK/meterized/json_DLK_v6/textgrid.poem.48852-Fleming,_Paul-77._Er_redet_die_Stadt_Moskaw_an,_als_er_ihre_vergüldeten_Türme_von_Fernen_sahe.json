{"textgrid.poem.48852": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "77. Er redet die Stadt Moskaw an, als er ihre verg\u00fcldeten T\u00fcrme von Fernen sahe", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du edle Kaiserin der St\u00e4dte der Ruthenen,", "tokens": ["Du", "ed\u00b7le", "Kai\u00b7se\u00b7rin", "der", "St\u00e4d\u00b7te", "der", "Ru\u00b7the\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gro\u00df, herlich, sch\u00f6ne, reich; seh' ich auf dich dorthin,", "tokens": ["gro\u00df", ",", "her\u00b7lich", ",", "sch\u00f6\u00b7ne", ",", "reich", ";", "seh'", "ich", "auf", "dich", "dor\u00b7thin", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJA", "$,", "ADJD", "$.", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auf dein verg\u00fcldtes Haupt, so k\u00f6mt mir in den Sinn", "tokens": ["auf", "dein", "ver\u00b7g\u00fcld\u00b7tes", "Haupt", ",", "so", "k\u00f6mt", "mir", "in", "den", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "was G\u00fcldners noch als Gold, nach dem ich mich mu\u00df sehnen.", "tokens": ["was", "G\u00fcld\u00b7ners", "noch", "als", "Gold", ",", "nach", "dem", "ich", "mich", "mu\u00df", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "KOUS", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist das hohe Haar der sch\u00f6nen Basilenen,", "tokens": ["Es", "ist", "das", "ho\u00b7he", "Haar", "der", "sch\u00f6\u00b7nen", "Ba\u00b7si\u00b7le\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "durch welcher Treflichkeit ich eingenommen bin.", "tokens": ["durch", "wel\u00b7cher", "Tref\u00b7lich\u00b7keit", "ich", "ein\u00b7ge\u00b7nom\u00b7men", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie, ganz Ich, sie mein All, sie meine Herscherin,", "tokens": ["Sie", ",", "ganz", "Ich", ",", "sie", "mein", "All", ",", "sie", "mei\u00b7ne", "Her\u00b7sche\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "PPER", "$,", "PPER", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "hat bei mir allen Preis der Sch\u00f6nsten unter Sch\u00f6nen.", "tokens": ["hat", "bei", "mir", "al\u00b7len", "Preis", "der", "Sch\u00f6ns\u00b7ten", "un\u00b7ter", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PIAT", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich r\u00fchme billich dich, du Hauptstadt deiner Welt,", "tokens": ["Ich", "r\u00fch\u00b7me", "bil\u00b7lich", "dich", ",", "du", "Haupt\u00b7stadt", "dei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weil deiner G\u00f6tlichkeit hier nichts die Wage h\u00e4lt", "tokens": ["weil", "dei\u00b7ner", "G\u00f6t\u00b7lich\u00b7keit", "hier", "nichts", "die", "Wa\u00b7ge", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PIS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und du der Auszug bist von Tausenden der Reussen.", "tokens": ["und", "du", "der", "Aus\u00b7zug", "bist", "von", "Tau\u00b7sen\u00b7den", "der", "Reus\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VAFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mehr aber r\u00fchm' ich dich, weil, was dich himlisch preist,", "tokens": ["Mehr", "a\u00b7ber", "r\u00fchm'", "ich", "dich", ",", "weil", ",", "was", "dich", "him\u00b7lisch", "preist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mich an ein g\u00f6ttlichs Mensch bei dir gedenken hei\u00dft,", "tokens": ["mich", "an", "ein", "g\u00f6tt\u00b7lichs", "Mensch", "bei", "dir", "ge\u00b7den\u00b7ken", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in welcher Alles ist, was treflich wird gehei\u00dfen.", "tokens": ["in", "wel\u00b7cher", "Al\u00b7les", "ist", ",", "was", "tref\u00b7lich", "wird", "ge\u00b7hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "VAFIN", "$,", "PRELS", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}