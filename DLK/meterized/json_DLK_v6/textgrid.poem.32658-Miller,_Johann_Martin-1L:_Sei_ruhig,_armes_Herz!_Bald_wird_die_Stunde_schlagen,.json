{"textgrid.poem.32658": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei ruhig, armes Herz! Bald wird die Stunde schlagen,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei ruhig, armes Herz! Bald wird die Stunde schlagen,", "tokens": ["Sei", "ru\u00b7hig", ",", "ar\u00b7mes", "Herz", "!", "Bald", "wird", "die", "Stun\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dir Erl\u00f6sung prophezeit,", "tokens": ["Die", "dir", "Er\u00b7l\u00f6\u00b7sung", "pro\u00b7phe\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nach so vielen Trauertagen", "tokens": ["Und", "nach", "so", "vie\u00b7len", "Trau\u00b7er\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu kommen deiner Nacht gebeut.", "tokens": ["Zu", "kom\u00b7men", "dei\u00b7ner", "Nacht", "ge\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Siehst du, die Sch\u00f6pfung trau'rt! Dem nahen Tod entgegen", "tokens": ["Siehst", "du", ",", "die", "Sch\u00f6p\u00b7fung", "trau'rt", "!", "Dem", "na\u00b7hen", "Tod", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welkt schon die sinkende Natur;", "tokens": ["Welkt", "schon", "die", "sin\u00b7ken\u00b7de", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ihr Pulsschlag klopft in schw\u00e4chern Schl\u00e4gen,", "tokens": ["Ihr", "Puls\u00b7schlag", "klopft", "in", "schw\u00e4\u00b7chern", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bald erlischt des Lebens Spur.", "tokens": ["Und", "bald", "er\u00b7lischt", "des", "Le\u00b7bens", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon sinkt der Blumen Schmuck; und keine neuen bl\u00fchen", "tokens": ["Schon", "sinkt", "der", "Blu\u00b7men", "Schmuck", ";", "und", "kei\u00b7ne", "neu\u00b7en", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$.", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An den verwaisten St\u00e4tten auf,", "tokens": ["An", "den", "ver\u00b7wais\u00b7ten", "St\u00e4t\u00b7ten", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und totenfarbe Nebel ziehen", "tokens": ["Und", "to\u00b7ten\u00b7far\u00b7be", "Ne\u00b7bel", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich aus dem tr\u00fcben Strom herauf.", "tokens": ["Sich", "aus", "dem", "tr\u00fc\u00b7ben", "Strom", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der braune Wald, entbl\u00f6\u00dft des farbichten Gewandes,", "tokens": ["Der", "brau\u00b7ne", "Wald", ",", "ent\u00b7bl\u00f6\u00dft", "des", "far\u00b7bich\u00b7ten", "Ge\u00b7wan\u00b7des", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "N\u00e4hrt keine Lieders\u00e4nger mehr,", "tokens": ["N\u00e4hrt", "kei\u00b7ne", "Lie\u00b7der\u00b7s\u00e4n\u00b7ger", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jede Weidetrift des Landes", "tokens": ["Und", "je\u00b7de", "Wei\u00b7de\u00b7trift", "des", "Lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist herdenlos und freudenleer.", "tokens": ["Ist", "her\u00b7den\u00b7los", "und", "freu\u00b7den\u00b7leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bald wird des Winters Flor die Flur umher umweben,", "tokens": ["Bald", "wird", "des", "Win\u00b7ters", "Flor", "die", "Flur", "um\u00b7her", "um\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alles sterben; \u2013 und auch mich", "tokens": ["Und", "al\u00b7les", "ster\u00b7ben", ";", "\u2013", "und", "auch", "mich"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVINF", "$.", "$(", "KON", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirst du, o Todesnacht, umgeben,", "tokens": ["Wirst", "du", ",", "o", "To\u00b7des\u00b7nacht", ",", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sterben, sterben werd' auch ich! \u2013", "tokens": ["Und", "ster\u00b7ben", ",", "ster\u00b7ben", "werd'", "auch", "ich", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVINF", "$,", "VVINF", "VAFIN", "ADV", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O sei willkommen mir, du sch\u00f6nster Tag des Lebens,", "tokens": ["O", "sei", "will\u00b7kom\u00b7men", "mir", ",", "du", "sch\u00f6ns\u00b7ter", "Tag", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "PPER", "$,", "PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ich \u2013 von dir, Amynt, bedau'rt,", "tokens": ["Das", "ich", "\u2013", "von", "dir", ",", "A\u00b7mynt", ",", "be\u00b7dau'rt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "$(", "APPR", "PPER", "$,", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der du mein Leid gekannt \u2013 vergebens", "tokens": ["Der", "du", "mein", "Leid", "ge\u00b7kannt", "\u2013", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$(", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um meine Chloe durchgetrau'rt.", "tokens": ["Um", "mei\u00b7ne", "Chloe", "durch\u00b7ge\u00b7trau'rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Du kanntest sie, Amynt! Von allen unsern Leiden", "tokens": ["Du", "kann\u00b7test", "sie", ",", "A\u00b7mynt", "!", "Von", "al\u00b7len", "un\u00b7sern", "Lei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NE", "$.", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warst du Vertrauter, littest mit;", "tokens": ["Warst", "du", "Ver\u00b7trau\u00b7ter", ",", "lit\u00b7test", "mit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du sahst die Thr\u00e4ne, die beim Scheiden", "tokens": ["Du", "sahst", "die", "Thr\u00e4\u00b7ne", ",", "die", "beim", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von unsern bleichen Wangen glitt.", "tokens": ["Von", "un\u00b7sern", "blei\u00b7chen", "Wan\u00b7gen", "glitt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "O Lieber! wenn noch Trost zu finden ist auf Erden,", "tokens": ["O", "Lie\u00b7ber", "!", "wenn", "noch", "Trost", "zu", "fin\u00b7den", "ist", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "KOUS", "ADV", "NN", "PTKZU", "VVINF", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So tr\u00f6ste sie, bis wir, vereint,", "tokens": ["So", "tr\u00f6s\u00b7te", "sie", ",", "bis", "wir", ",", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "$,", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Uns wieder da umarmen werden,", "tokens": ["Uns", "wie\u00b7der", "da", "um\u00b7ar\u00b7men", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo nicht verfolgte Liebe weint!", "tokens": ["Wo", "nicht", "ver\u00b7folg\u00b7te", "Lie\u00b7be", "weint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}