{"textgrid.poem.56003": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: So erschrocken, wie sie nie erschraken,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So erschrocken, wie sie nie erschraken,", "tokens": ["So", "er\u00b7schro\u00b7cken", ",", "wie", "sie", "nie", "er\u00b7schra\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "ohne Ordnung, oft durchlocht und locker,", "tokens": ["oh\u00b7ne", "Ord\u00b7nung", ",", "oft", "durch\u00b7locht", "und", "lo\u00b7cker", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "hocken sie in dem geborstnen Ocker", "tokens": ["ho\u00b7cken", "sie", "in", "dem", "ge\u00b7borst\u00b7nen", "O\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "ihres Ackers, nicht von ihren Laken", "tokens": ["ih\u00b7res", "A\u00b7ckers", ",", "nicht", "von", "ih\u00b7ren", "La\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "abzubringen, die sie liebgewannen.", "tokens": ["ab\u00b7zu\u00b7brin\u00b7gen", ",", "die", "sie", "lieb\u00b7ge\u00b7wan\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIZU", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber Engel kommen an, um \u00d6le", "tokens": ["A\u00b7ber", "En\u00b7gel", "kom\u00b7men", "an", ",", "um", "\u00d6\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PTKVZ", "$,", "KOUI", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "einzutr\u00e4ufeln in die trocknen Pfannen", "tokens": ["ein\u00b7zu\u00b7tr\u00e4u\u00b7feln", "in", "die", "trock\u00b7nen", "Pfan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und um jedem in die Achselh\u00f6hle", "tokens": ["und", "um", "je\u00b7dem", "in", "die", "Ach\u00b7sel\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "das zu legen, was er in dem L\u00e4rme", "tokens": ["das", "zu", "le\u00b7gen", ",", "was", "er", "in", "dem", "L\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "PTKZU", "VVINF", "$,", "PWS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "damals seines Lebens nicht entweihte;", "tokens": ["da\u00b7mals", "sei\u00b7nes", "Le\u00b7bens", "nicht", "ent\u00b7weih\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "denn dort hat es noch ein wenig W\u00e4rme,", "tokens": ["denn", "dort", "hat", "es", "noch", "ein", "we\u00b7nig", "W\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "da\u00df es nicht des Herren Hand erk\u00e4lte", "tokens": ["da\u00df", "es", "nicht", "des", "Her\u00b7ren", "Hand", "er\u00b7k\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "oben, wenn er es aus jeder Seite", "tokens": ["o\u00b7ben", ",", "wenn", "er", "es", "aus", "je\u00b7der", "Sei\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "leise greift, zu f\u00fchlen, ob es g\u00e4lte.", "tokens": ["lei\u00b7se", "greift", ",", "zu", "f\u00fch\u00b7len", ",", "ob", "es", "g\u00e4l\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}