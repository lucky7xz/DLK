{"textgrid.poem.60322": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die n\u00e4chtliche Fahrt", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu \u00f6d und traurig selbst den Heidewinden", "tokens": ["Zu", "\u00f6d", "und", "trau\u00b7rig", "selbst", "den", "Hei\u00b7de\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "KON", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind diese winterlichen Einsamkeiten,", "tokens": ["Sind", "die\u00b7se", "win\u00b7ter\u00b7li\u00b7chen", "Ein\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur Schnee und Schnee ringsaus in alle Weiten,", "tokens": ["Nur", "Schnee", "und", "Schnee", "rings\u00b7aus", "in", "al\u00b7le", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur stiller, keuscher, kalter Tod zu finden.", "tokens": ["Nur", "stil\u00b7ler", ",", "keu\u00b7scher", ",", "kal\u00b7ter", "Tod", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier ists umsonst, nach frohem Ton zu lauschen,", "tokens": ["Hier", "ists", "um\u00b7sonst", ",", "nach", "fro\u00b7hem", "Ton", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Singv\u00f6gel sind geflohn von diesem Grabe,", "tokens": ["Sing\u00b7v\u00f6\u00b7gel", "sind", "ge\u00b7flohn", "von", "die\u00b7sem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Schnabel in die Federn h\u00fcllt der Rabe,", "tokens": ["Der", "Schna\u00b7bel", "in", "die", "Fe\u00b7dern", "h\u00fcllt", "der", "Ra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eingefroren ist der B\u00e4che Rauschen.", "tokens": ["Und", "ein\u00b7ge\u00b7fro\u00b7ren", "ist", "der", "B\u00e4\u00b7che", "Rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sieht man den Wald so tief in Tod versunken,", "tokens": ["Sieht", "man", "den", "Wald", "so", "tief", "in", "Tod", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will mans nicht glauben, da\u00df er jemals wieder", "tokens": ["Will", "mans", "nicht", "glau\u00b7ben", ",", "da\u00df", "er", "je\u00b7mals", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufgr\u00fcnt im Lenz, da\u00df je hier seine Lieder", "tokens": ["Auf\u00b7gr\u00fcnt", "im", "Lenz", ",", "da\u00df", "je", "hier", "sei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "KOUS", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Vogel singt, vom Fr\u00fchlingshauche trunken.", "tokens": ["Ein", "Vo\u00b7gel", "singt", ",", "vom", "Fr\u00fch\u00b7lings\u00b7hau\u00b7che", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es gl\u00e4nzt der Eichenwald in Eisesklammern;", "tokens": ["Es", "gl\u00e4nzt", "der", "Ei\u00b7chen\u00b7wald", "in", "Ei\u00b7ses\u00b7klam\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt W\u00f6lfe heulen am verschneiten Grunde,", "tokens": ["Jetzt", "W\u00f6l\u00b7fe", "heu\u00b7len", "am", "ver\u00b7schnei\u00b7ten", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Bettler, hungerwach, in n\u00e4chtger Stunde", "tokens": ["Wie", "Bett\u00b7ler", ",", "hun\u00b7ger\u00b7wach", ",", "in", "n\u00e4cht\u00b7ger", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Grabe eines milden K\u00f6nigs jammern.", "tokens": ["Am", "Gra\u00b7be", "ei\u00b7nes", "mil\u00b7den", "K\u00f6\u00b7nigs", "jam\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dort f\u00e4hrt ein Schlitten auf der blanken W\u00fcste,", "tokens": ["Dort", "f\u00e4hrt", "ein", "Schlit\u00b7ten", "auf", "der", "blan\u00b7ken", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Kutscher treibt die ausgestreckten Pferde,", "tokens": ["Der", "Kut\u00b7scher", "treibt", "die", "aus\u00b7ge\u00b7streck\u00b7ten", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als ob mit seinem Fuhrwerk er die Erde", "tokens": ["Als", "ob", "mit", "sei\u00b7nem", "Fuhr\u00b7werk", "er", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor Sonnenaufgang noch umrennen m\u00fc\u00dfte.", "tokens": ["Vor", "Son\u00b7nen\u00b7auf\u00b7gang", "noch", "um\u00b7ren\u00b7nen", "m\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Drei Hengste sinds, rasch wie des Nordens L\u00fcfte,", "tokens": ["Drei", "Hengs\u00b7te", "sinds", ",", "rasch", "wie", "des", "Nor\u00b7dens", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein jeder tr\u00e4gt das werte Probezeichen", "tokens": ["Ein", "je\u00b7der", "tr\u00e4gt", "das", "wer\u00b7te", "Pro\u00b7be\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Schnelligkeit im r\u00fcstigen Entweichen,", "tokens": ["Der", "Schnel\u00b7lig\u00b7keit", "im", "r\u00fcs\u00b7ti\u00b7gen", "Ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Narbe des Wolfsbisses an der H\u00fcfte.", "tokens": ["Die", "Nar\u00b7be", "des", "Wolfs\u00b7bis\u00b7ses", "an", "der", "H\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein Gl\u00f6cklein tr\u00e4gt das Mittelro\u00df der Gabel,", "tokens": ["Ein", "Gl\u00f6c\u00b7klein", "tr\u00e4gt", "das", "Mit\u00b7tel\u00b7ro\u00df", "der", "Ga\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Gl\u00f6cklein tanzend fliehn vorbei die B\u00e4ume", "tokens": ["Zum", "Gl\u00f6c\u00b7klein", "tan\u00b7zend", "fliehn", "vor\u00b7bei", "die", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Schlitten, tr\u00fcb, wie schnellverge\u00dfne Tr\u00e4ume,", "tokens": ["Am", "Schlit\u00b7ten", ",", "tr\u00fcb", ",", "wie", "schnell\u00b7ver\u00b7ge\u00df\u00b7ne", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Wald entflieht wie eine bleiche Fabel.", "tokens": ["Der", "Wald", "ent\u00b7flieht", "wie", "ei\u00b7ne", "blei\u00b7che", "Fa\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die schnellen Renner sind mit Eis behangen,", "tokens": ["Die", "schnel\u00b7len", "Ren\u00b7ner", "sind", "mit", "Eis", "be\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das klirrend an den schwarzen M\u00e4hnen zittert,", "tokens": ["Das", "klir\u00b7rend", "an", "den", "schwar\u00b7zen", "M\u00e4h\u00b7nen", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Rosse R\u00fccken ist mit Reif umgittert:", "tokens": ["Der", "Ros\u00b7se", "R\u00fc\u00b7cken", "ist", "mit", "Reif", "um\u00b7git\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Tod will sie mit kaltem Netze fangen.", "tokens": ["Der", "Tod", "will", "sie", "mit", "kal\u00b7tem", "Net\u00b7ze", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Gekauert sitzt, geh\u00fcllt vom B\u00e4renkragen,", "tokens": ["Ge\u00b7kau\u00b7ert", "sitzt", ",", "ge\u00b7h\u00fcllt", "vom", "B\u00e4\u00b7ren\u00b7kra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wojewod im Schlittenkorbgeflechte", "tokens": ["Der", "Wo\u00b7je\u00b7wod", "im", "Schlit\u00b7ten\u00b7korb\u00b7ge\u00b7flech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Still hinter seinem pelzverh\u00fcllten Knechte,", "tokens": ["Still", "hin\u00b7ter", "sei\u00b7nem", "pelz\u00b7ver\u00b7h\u00fcll\u00b7ten", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der manchmal pfeift, die Pferde anzujagen.", "tokens": ["Der", "manch\u00b7mal", "pfeift", ",", "die", "Pfer\u00b7de", "an\u00b7zu\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Dem Schlitten folgt in klarer Mondeshelle", "tokens": ["Dem", "Schlit\u00b7ten", "folgt", "in", "kla\u00b7rer", "Mon\u00b7des\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein zweiter nach, mit gleichgeschwinden Rennern,", "tokens": ["Ein", "zwei\u00b7ter", "nach", ",", "mit", "gleich\u00b7ge\u00b7schwin\u00b7den", "Ren\u00b7nern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Befrachtet auch mit zwei verh\u00fcllten M\u00e4nnern,", "tokens": ["Be\u00b7frach\u00b7tet", "auch", "mit", "zwei", "ver\u00b7h\u00fcll\u00b7ten", "M\u00e4n\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auf der Heide klingelt seine Schelle.", "tokens": ["Und", "auf", "der", "Hei\u00b7de", "klin\u00b7gelt", "sei\u00b7ne", "Schel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Die Nacht ist grimmig kalt; o Wandrer meide", "tokens": ["Die", "Nacht", "ist", "grim\u00b7mig", "kalt", ";", "o", "Wand\u00b7rer", "mei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$.", "FM", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Schlaf; h\u00f6rst du das Gl\u00f6cklein nicht mehr schlagen,", "tokens": ["Den", "Schlaf", ";", "h\u00f6rst", "du", "das", "Gl\u00f6c\u00b7klein", "nicht", "mehr", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wirds vom Rosse dir vorangetragen,", "tokens": ["So", "wirds", "vom", "Ros\u00b7se", "dir", "vor\u00b7an\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein wandernd Sterbegl\u00f6cklein auf der Heide.", "tokens": ["Dein", "wan\u00b7dernd", "Ster\u00b7be\u00b7gl\u00f6c\u00b7klein", "auf", "der", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Der B\u00e4ume Leben floh zum Grund hinunter;", "tokens": ["Der", "B\u00e4u\u00b7me", "Le\u00b7ben", "floh", "zum", "Grund", "hin\u00b7un\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gib, Wandrer, acht, da\u00df nicht auch deine Seele", "tokens": ["Gib", ",", "Wand\u00b7rer", ",", "acht", ",", "da\u00df", "nicht", "auch", "dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "CARD", "$,", "KOUS", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu ihrem Grunde sich hinunterstehle,", "tokens": ["Zu", "ih\u00b7rem", "Grun\u00b7de", "sich", 