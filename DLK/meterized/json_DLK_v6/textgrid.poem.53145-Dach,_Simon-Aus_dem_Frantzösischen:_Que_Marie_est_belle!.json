{"textgrid.poem.53145": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem Frantz\u00f6sischen: Que Marie est belle!", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trefflich hoch zu halten", "tokens": ["Treff\u00b7lich", "hoch", "zu", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist Rosettchen Zier!", "tokens": ["Ist", "Ro\u00b7sett\u00b7chen", "Zier", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie heist mich erkalten,", "tokens": ["Sie", "heist", "mich", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ich verschweig' es ihr,", "tokens": ["Ich", "ver\u00b7schweig'", "es", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Also hefftig trag' ich schew", "tokens": ["Al\u00b7so", "heff\u00b7tig", "trag'", "ich", "schew"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu empfinden Gram vnd Rew.", "tokens": ["Zu", "emp\u00b7fin\u00b7den", "Gram", "vnd", "Rew", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich thar jhr nicht klagen,", "tokens": ["Ich", "thar", "jhr", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was mein Leiden wil,", "tokens": ["Was", "mein", "Lei\u00b7den", "wil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Vnd in solchen Plagen", "tokens": ["Vnd", "in", "sol\u00b7chen", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Seufftz' ich, aber still;", "tokens": ["Seufft\u00b7z'", "ich", ",", "a\u00b7ber", "still", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Also hefftig trag' ich schew", "tokens": ["Al\u00b7so", "heff\u00b7tig", "trag'", "ich", "schew"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu empfinden Gram vnd Rew.", "tokens": ["Zu", "emp\u00b7fin\u00b7den", "Gram", "vnd", "Rew", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich verheel' als m\u00fcglich", "tokens": ["Ich", "ver\u00b7heel'", "als", "m\u00fcg\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Meine liechte Glut,", "tokens": ["Mei\u00b7ne", "liech\u00b7te", "Glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Welche mir betrieglich", "tokens": ["Wel\u00b7che", "mir", "be\u00b7trieg\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Auffzehrt Seel' vnd Muth,", "tokens": ["Auff\u00b7zehrt", "Seel'", "vnd", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Also hefftig trag' ich schew", "tokens": ["Al\u00b7so", "heff\u00b7tig", "trag'", "ich", "schew"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu empfinden Gram vnd Rew.", "tokens": ["Zu", "emp\u00b7fin\u00b7den", "Gram", "vnd", "Rew", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So verdrieslich schweigen,", "tokens": ["So", "ver\u00b7dries\u00b7lich", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hilfft es meiner Noht?", "tokens": ["Hilfft", "es", "mei\u00b7ner", "Noht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hie sol sich er\u00e4ugen", "tokens": ["Hie", "sol", "sich", "er\u00b7\u00e4u\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kein Lohn als der Todt,", "tokens": ["Kein", "Lohn", "als", "der", "Todt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Also hefftig trag' ich schew", "tokens": ["Al\u00b7so", "heff\u00b7tig", "trag'", "ich", "schew"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu empfinden Gram vnd Rew.", "tokens": ["Zu", "emp\u00b7fin\u00b7den", "Gram", "vnd", "Rew", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Edles Mensch von Sitten,", "tokens": ["Ed\u00b7les", "Mensch", "von", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Du machst mir den Krieg,", "tokens": ["Du", "machst", "mir", "den", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Amorn wil ich bitten,", "tokens": ["A\u00b7morn", "wil", "ich", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df er sey mein Sieg,", "tokens": ["Da\u00df", "er", "sey", "mein", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Denn in Liebe trag' ich schew", "tokens": ["Denn", "in", "Lie\u00b7be", "trag'", "ich", "schew"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr geringstem Gram vnd Rew.", "tokens": ["F\u00fcr", "ge\u00b7rings\u00b7tem", "Gram", "vnd", "Rew", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}