{"textgrid.poem.60151": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir zur Last f\u00fchl ich mein Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir zur Last f\u00fchl ich mein Leben,", "tokens": ["Mir", "zur", "Last", "f\u00fchl", "ich", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam finden meine Tage mich,", "tokens": ["Ein\u00b7sam", "fin\u00b7den", "mei\u00b7ne", "Ta\u00b7ge", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die mit Wolken sind umgeben;", "tokens": ["Die", "mit", "Wol\u00b7ken", "sind", "um\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner h\u00fcllt aus seinem Nebel sich.", "tokens": ["Kei\u00b7ner", "h\u00fcllt", "aus", "sei\u00b7nem", "Ne\u00b7bel", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Alles mein Vergn\u00fcgen", "tokens": ["Al\u00b7les", "mein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mu\u00df im Staube liegen!", "tokens": ["Mu\u00df", "im", "Stau\u00b7be", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ach wie ganz hat mich der Tod beraubt!", "tokens": ["Ach", "wie", "ganz", "hat", "mich", "der", "Tod", "be\u00b7raubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wie der kalte Herbst den Garten,", "tokens": ["Wie", "der", "kal\u00b7te", "Herbst", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Den er ganz entlaubt.", "tokens": ["Den", "er", "ganz", "ent\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Todtenbl\u00e4sse \u00fcberziehet", "tokens": ["Tod\u00b7ten\u00b7bl\u00e4s\u00b7se", "\u00fc\u00b7ber\u00b7zie\u00b7het"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein von Thr\u00e4nen nasses Angesicht,", "tokens": ["Mein", "von", "Thr\u00e4\u00b7nen", "nas\u00b7ses", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn mein Herz, das mir entfliehet,", "tokens": ["Wenn", "mein", "Herz", ",", "das", "mir", "ent\u00b7flie\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Bewohnern kalter Gr\u00e4ber spricht.", "tokens": ["Mit", "Be\u00b7woh\u00b7nern", "kal\u00b7ter", "Gr\u00e4\u00b7ber", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Auf dem Leichensteine,", "tokens": ["Auf", "dem", "Lei\u00b7chen\u00b7stei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sitz ich dann und weine", "tokens": ["Sitz", "ich", "dann", "und", "wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Meinen Jammer in den d\u00fcrren Sand,", "tokens": ["Mei\u00b7nen", "Jam\u00b7mer", "in", "den", "d\u00fcr\u00b7ren", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Der das beste Herz bedecket,", "tokens": ["Der", "das", "bes\u00b7te", "Herz", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Das f\u00fcr mich empfand!", "tokens": ["Das", "f\u00fcr", "mich", "emp\u00b7fand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dunkler sind mir meine N\u00e4chte", "tokens": ["Dunk\u00b7ler", "sind", "mir", "mei\u00b7ne", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als Egyptens dicke Mitternacht.", "tokens": ["Als", "E\u00b7gyp\u00b7tens", "di\u00b7cke", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn der Tag den C\u00f6rper schw\u00e4chte,", "tokens": ["Wenn", "der", "Tag", "den", "C\u00f6r\u00b7per", "schw\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird die Nacht mit tr\u00fcbem Gram durchwacht!", "tokens": ["Wird", "die", "Nacht", "mit", "tr\u00fc\u00b7bem", "Gram", "durch\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Vor mir hin verbreiten", "tokens": ["Vor", "mir", "hin", "ver\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sich verflo\u00dfne Zeiten!", "tokens": ["Sich", "ver\u00b7flo\u00df\u00b7ne", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Als mein Freund mir an der Seite lag,", "tokens": ["Als", "mein", "Freund", "mir", "an", "der", "Sei\u00b7te", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ach da fand im Arm der Freude", "tokens": ["Ach", "da", "fand", "im", "Arm", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mich der junge Tag!", "tokens": ["Mich", "der", "jun\u00b7ge", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Unter dem Tumult der Sorgen", "tokens": ["Un\u00b7ter", "dem", "Tu\u00b7mult", "der", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Werd ich jezt die Sonne nicht gewahr!", "tokens": ["Werd", "ich", "jezt", "die", "Son\u00b7ne", "nicht", "ge\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mir erscheint kein heitrer Morgen", "tokens": ["Mir", "er\u00b7scheint", "kein", "hei\u00b7trer", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und f\u00fcr mich becr\u00e4nzt sich nicht das Jahr!", "tokens": ["Und", "f\u00fcr", "mich", "be\u00b7cr\u00b7\u00e4nzt", "sich", "nicht", "das", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Blumen, Lenz und Lieder", "tokens": ["Blu\u00b7men", ",", "Lenz", "und", "Lie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sind mir nur zuwieder,", "tokens": ["Sind", "mir", "nur", "zu\u00b7wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und das gr\u00fcne Thal erg\u00f6zt mich nie,", "tokens": ["Und", "das", "gr\u00fc\u00b7ne", "Thal", "er\u00b7g\u00f6zt", "mich", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Selbst die Nachtigallen singen", "tokens": ["Selbst", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mir Melancholie!", "tokens": ["Mir", "Me\u00b7lan\u00b7cho\u00b7lie", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Rauscht ihr silberklaren B\u00e4che!", "tokens": ["Rauscht", "ihr", "sil\u00b7ber\u00b7kla\u00b7ren", "B\u00e4\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rausche st\u00e4rker, du zu stille Spree!", "tokens": ["Rau\u00b7sche", "st\u00e4r\u00b7ker", ",", "du", "zu", "stil\u00b7le", "Spree", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wiederhohle was ich spreche,", "tokens": ["Wie\u00b7der\u00b7hoh\u00b7le", "was", "ich", "spre\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich um dein Ufer wankend geh.", "tokens": ["Wenn", "ich", "um", "dein", "U\u00b7fer", "wan\u00b7kend", "geh."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr verschwiegnen Linden,", "tokens": ["Ihr", "ver\u00b7schwieg\u00b7nen", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mein betr\u00fcbt Empfinden", "tokens": ["Mein", "be\u00b7tr\u00fcbt", "Emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "VVPP", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Grab ich tief in eure St\u00e4mme ein,", "tokens": ["Grab", "ich", "tief", "in", "eu\u00b7re", "St\u00e4m\u00b7me", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und ihr sollt von meinem Jammer,", "tokens": ["Und", "ihr", "sollt", "von", "mei\u00b7nem", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Das Geschichtbuch seyn.", "tokens": ["Das", "Ge\u00b7schicht\u00b7buch", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Du, o Mond mit voller Wange,", "tokens": ["Du", ",", "o", "Mond", "mit", "vol\u00b7ler", "Wan\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey ein Zeuge, wie betr\u00fcbt ich bin!", "tokens": ["Sey", "ein", "Zeu\u00b7ge", ",", "wie", "be\u00b7tr\u00fcbt", "ich", "bin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und wenn ich noch Trost verlange", "tokens": ["Und", "wenn", "ich", "noch", "Trost", "ver\u00b7lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blickt auf mich, ihr Sterne! Mitleid hin.", "tokens": ["Blickt", "auf", "mich", ",", "ihr", "Ster\u00b7ne", "!", "Mit\u00b7leid", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$.", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Seht die Thr\u00e4nen rollen", "tokens": ["Seht", "die", "Thr\u00e4\u00b7nen", "rol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Die euch sagen sollen,", "tokens": ["Die", "euch", "sa\u00b7gen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df mein Schicksal hart mit mir verfuhr.", "tokens": ["Da\u00df", "mein", "Schick\u00b7sal", "hart", "mit", "mir", "ver\u00b7fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ach, ich bin noch Freuden-loser,", "tokens": ["Ach", ",", "ich", "bin", "noch", "Freu\u00b7den\u00b7lo\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Als die \u00f6de Flur!", "tokens": ["Als", "die", "\u00f6\u00b7de", "Flur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "O, ihr B\u00fcrger jener Welten", "tokens": ["O", ",", "ihr", "B\u00fcr\u00b7ger", "je\u00b7ner", "Wel\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr \u00fcber meinem Haupte wohnt!", "tokens": ["Die", "ihr", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", "wohnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "H\u00f6rt, wie ich den Tod mu\u00df schelten", "tokens": ["H\u00f6rt", ",", "wie", "ich", "den", "Tod", "mu\u00df", "schel\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er unbarmherzig mich verschont.", "tokens": ["Da\u00df", "er", "un\u00b7barm\u00b7her\u00b7zig", "mich", "ver\u00b7schont", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aber nein, vernehmet!", "tokens": ["A\u00b7ber", "nein", ",", "ver\u00b7neh\u00b7met", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wie mein Herz sich sch\u00e4met,", "tokens": ["Wie", "mein", "Herz", "sich", "sch\u00e4\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df es ungeduldig sich emp\u00f6rt,", "tokens": ["Da\u00df", "es", "un\u00b7ge\u00b7dul\u00b7dig", "sich", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und den Willen eures Sch\u00f6pfers", "tokens": ["Und", "den", "Wil\u00b7len", "eu\u00b7res", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Murrend hat entehrt!", "tokens": ["Mur\u00b7rend", "hat", "ent\u00b7ehrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Nie will ich dem Leben fluchen", "tokens": ["Nie", "will", "ich", "dem", "Le\u00b7ben", "flu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst mein Kummer soll mir heilig seyn.", "tokens": ["Selbst", "mein", "Kum\u00b7mer", "soll", "mir", "hei\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oft will ich den Staub besuchen,", "tokens": ["Oft", "will", "ich", "den", "Staub", "be\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und ihm eine stille Thr\u00e4ne weyhn.", "tokens": ["Und", "ihm", "ei\u00b7ne", "stil\u00b7le", "Thr\u00e4\u00b7ne", "weyhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Der entflogne Schatten", "tokens": ["Der", "ent\u00b7flog\u00b7ne", "Schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Meines theuren Gatten,", "tokens": ["Mei\u00b7nes", "theu\u00b7ren", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "L\u00e4chelt dann mit euch auf mich herab,", "tokens": ["L\u00e4\u00b7chelt", "dann", "mit", "euch", "auf", "mich", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und behorcht die frommen Seufzer", "tokens": ["Und", "be\u00b7horcht", "die", "from\u00b7men", "Seuf\u00b7zer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Hingest\u00f6hnt aufs Grab!", "tokens": ["Hin\u00b7ge\u00b7st\u00f6hnt", "aufs", "Grab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}