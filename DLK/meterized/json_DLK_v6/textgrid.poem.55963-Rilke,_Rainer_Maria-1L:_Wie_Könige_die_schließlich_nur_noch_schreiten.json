{"textgrid.poem.55963": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie K\u00f6nige die schlie\u00dflich nur noch schreiten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie K\u00f6nige die schlie\u00dflich nur noch schreiten", "tokens": ["Wie", "K\u00f6\u00b7ni\u00b7ge", "die", "schlie\u00df\u00b7lich", "nur", "noch", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "ADJD", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "fast ohne Ziel, nur um von Zeit zu Zeit", "tokens": ["fast", "oh\u00b7ne", "Ziel", ",", "nur", "um", "von", "Zeit", "zu", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sich den Verneigenden auf beiden Seiten", "tokens": ["sich", "den", "Ver\u00b7nei\u00b7gen\u00b7den", "auf", "bei\u00b7den", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "zu zeigen in des Mantels Einsamkeit \u2013:", "tokens": ["zu", "zei\u00b7gen", "in", "des", "Man\u00b7tels", "Ein\u00b7sam\u00b7keit", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "so steigt, allein zwischen den Balustraden,", "tokens": ["so", "steigt", ",", "al\u00b7lein", "zwi\u00b7schen", "den", "Ba\u00b7lust\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die sich verneigen schon seit Anbeginn,", "tokens": ["die", "sich", "ver\u00b7nei\u00b7gen", "schon", "seit", "An\u00b7be\u00b7ginn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die Treppe: langsam und von Gottes Gnaden", "tokens": ["die", "Trep\u00b7pe", ":", "lang\u00b7sam", "und", "von", "Got\u00b7tes", "Gna\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADJD", "KON", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und auf den Himmel zu und nirgends hin;", "tokens": ["und", "auf", "den", "Him\u00b7mel", "zu", "und", "nir\u00b7gends", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "als ob sie allen Folgenden befahl", "tokens": ["als", "ob", "sie", "al\u00b7len", "Fol\u00b7gen\u00b7den", "be\u00b7fahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zur\u00fcckzubleiben, \u2013 so da\u00df sie nicht wagen", "tokens": ["zu\u00b7r\u00fcck\u00b7zu\u00b7blei\u00b7ben", ",", "\u2013", "so", "da\u00df", "sie", "nicht", "wa\u00b7gen"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "$(", "ADV", "KOUS", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "von ferne nachzugehen; nicht einmal", "tokens": ["von", "fer\u00b7ne", "nach\u00b7zu\u00b7ge\u00b7hen", ";", "nicht", "ein\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "VVIZU", "$.", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die schwere Schleppe durfte einer tragen.", "tokens": ["die", "schwe\u00b7re", "Schlep\u00b7pe", "durf\u00b7te", "ei\u00b7ner", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}