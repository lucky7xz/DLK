{"textgrid.poem.43446": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus d\u00fcrrem Stube wird das Laub noch brechen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus d\u00fcrrem Stube wird das Laub noch brechen", "tokens": ["Aus", "d\u00fcr\u00b7rem", "Stu\u00b7be", "wird", "das", "Laub", "noch", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auch der nackte Felsen wird noch gr\u00fcn,", "tokens": ["Und", "auch", "der", "nack\u00b7te", "Fel\u00b7sen", "wird", "noch", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du darfst ein Wort, ein einzig Wort nur sprechen,", "tokens": ["Du", "darfst", "ein", "Wort", ",", "ein", "ein\u00b7zig", "Wort", "nur", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "ART", "ADJD", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und unsre ganze Hoffnung wird erbl\u00fchn.", "tokens": ["Und", "uns\u00b7re", "gan\u00b7ze", "Hoff\u00b7nung", "wird", "er\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur in der Hoffnung ruht das sch\u00f6nre Leben,", "tokens": ["Nur", "in", "der", "Hoff\u00b7nung", "ruht", "das", "sch\u00f6n\u00b7re", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Hoffnung ist auch unser Heil und Hort;", "tokens": ["Die", "Hoff\u00b7nung", "ist", "auch", "un\u00b7ser", "Heil", "und", "Hort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du giebst uns Alles, willst du Hoffnung geben,", "tokens": ["Du", "giebst", "uns", "Al\u00b7les", ",", "willst", "du", "Hoff\u00b7nung", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und unser ganzes Hoffen ist Ein Wort.", "tokens": ["Und", "un\u00b7ser", "gan\u00b7zes", "Hof\u00b7fen", "ist", "Ein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O sprich Ein Wort in diesen tr\u00fcben Tagen,", "tokens": ["O", "sprich", "Ein", "Wort", "in", "die\u00b7sen", "tr\u00fc\u00b7ben", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Trug und Knechtssinn, L\u00fcg' und Schmeichelei", "tokens": ["Wo", "Trug", "und", "Knechts\u00b7sinn", ",", "L\u00fcg'", "und", "Schmei\u00b7che\u00b7lei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Wahrheit gern in Fesseln m\u00f6chte schlagen,", "tokens": ["Die", "Wahr\u00b7heit", "gern", "in", "Fes\u00b7seln", "m\u00f6ch\u00b7te", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein K\u00f6nig, sprich das Wort: ", "tokens": ["Mein", "K\u00f6\u00b7nig", ",", "sprich", "das", "Wort", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}