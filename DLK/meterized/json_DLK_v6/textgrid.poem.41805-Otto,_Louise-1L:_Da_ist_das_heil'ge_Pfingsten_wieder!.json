{"textgrid.poem.41805": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da ist das heil'ge Pfingsten wieder!", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ist das heil'ge Pfingsten wieder!", "tokens": ["Da", "ist", "das", "heil'\u00b7ge", "Pfings\u00b7ten", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lenzes und des Geistes Gru\u00df", "tokens": ["Des", "Len\u00b7zes", "und", "des", "Geis\u00b7tes", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Senkt sich mit ihm zur Erde nieder", "tokens": ["Senkt", "sich", "mit", "ihm", "zur", "Er\u00b7de", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "APPRART", "NN", "PTKVZ"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Im Sonnenglanz und Bl\u00fctenku\u00df.", "tokens": ["Im", "Son\u00b7nen\u00b7glanz", "und", "Bl\u00fc\u00b7ten\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Halb wie der Maien leises Zittern", "tokens": ["Halb", "wie", "der", "Mai\u00b7en", "lei\u00b7ses", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im hellen Morgensonnenstrahl,", "tokens": ["Im", "hel\u00b7len", "Mor\u00b7gen\u00b7son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Halb wie ein dr\u00f6hnendes Gewittern", "tokens": ["Halb", "wie", "ein", "dr\u00f6h\u00b7nen\u00b7des", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erklingt der Andacht Pfingstchoral.", "tokens": ["Er\u00b7klingt", "der", "An\u00b7dacht", "Pfingst\u00b7cho\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbo heil'ger Geist, dereinst gesendet", "tokens": ["\u00bb", "o", "heil'\u00b7ger", "Geist", ",", "de\u00b7reinst", "ge\u00b7sen\u00b7det"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Kampf der J\u00fcnger Schar zu weihn,", "tokens": ["Zum", "Kampf", "der", "J\u00fcn\u00b7ger", "Schar", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie vom Niedern abgewendet", "tokens": ["Da\u00df", "sie", "vom", "Nie\u00b7dern", "ab\u00b7ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem H\u00f6chsten diene nur allein. \u2013", "tokens": ["Dem", "H\u00f6chs\u00b7ten", "die\u00b7ne", "nur", "al\u00b7lein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O heil'ger Geist, komm zu uns allen,", "tokens": ["O", "heil'\u00b7ger", "Geist", ",", "komm", "zu", "uns", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die wir auch heute kampfbereit", "tokens": ["Die", "wir", "auch", "heu\u00b7te", "kampf\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Ziele der Erl\u00f6sung wallen,", "tokens": ["Zum", "Zie\u00b7le", "der", "Er\u00b7l\u00f6\u00b7sung", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von dem die Menschheit noch so weit.\u00ab", "tokens": ["Von", "dem", "die", "Menschheit", "noch", "so", "weit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Sich Deinem Dienste recht ergeben", "tokens": ["Sich", "Dei\u00b7nem", "Diens\u00b7te", "recht", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hei\u00dft: nur f\u00fcr Freiheit, Lieb' und Licht", "tokens": ["Hei\u00dft", ":", "nur", "f\u00fcr", "Frei\u00b7heit", ",", "Lieb'", "und", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schwert des Geistes aufzuheben,", "tokens": ["Das", "Schwert", "des", "Geis\u00b7tes", "auf\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das blutlos selbst den Sieg erficht.", "tokens": ["Das", "blut\u00b7los", "selbst", "den", "Sieg", "er\u00b7ficht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch thut es not dies Schwert zu schwingen", "tokens": ["Doch", "thut", "es", "not", "dies", "Schwert", "zu", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "PDS", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gen Sclaventum und Ha\u00df und Nacht.", "tokens": ["Gen", "Scla\u00b7ven\u00b7tum", "und", "Ha\u00df", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis Volk um Volk aus ihren Schlingen", "tokens": ["Bis", "Volk", "um", "Volk", "aus", "ih\u00b7ren", "Schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum selgen Freiheitstag erwacht.", "tokens": ["Zum", "sel\u00b7gen", "Frei\u00b7heits\u00b7tag", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Heil ist nur auf des Lichtes Bahnen,", "tokens": ["Heil", "ist", "nur", "auf", "des", "Lich\u00b7tes", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nur sie zu wandeln Ehr und Kuhm,", "tokens": ["Nur", "sie", "zu", "wan\u00b7deln", "Ehr", "und", "Kuhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum w\u00e4hlt gehorsam seinem Mahnen", "tokens": ["Drum", "w\u00e4hlt", "ge\u00b7hor\u00b7sam", "sei\u00b7nem", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des heil'gen Geistes Kittertum!", "tokens": ["Des", "heil'\u00b7gen", "Geis\u00b7tes", "Kit\u00b7ter\u00b7tum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00dft hoch und stolz sein Banner wehen", "tokens": ["La\u00dft", "hoch", "und", "stolz", "sein", "Ban\u00b7ner", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit frischem Maienlaub umkr\u00e4nzt,", "tokens": ["Mit", "fri\u00b7schem", "Mai\u00b7en\u00b7laub", "um\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis wir es siegreich flattern sehen", "tokens": ["Bis", "wir", "es", "sieg\u00b7reich", "flat\u00b7tern", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PRF", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Geistesflammen \u00fcbergl\u00e4nzt.", "tokens": ["Von", "Geis\u00b7tes\u00b7flam\u00b7men", "\u00fc\u00b7ber\u00b7gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Bis alles Volk, selbst geistdurchdrungen,", "tokens": ["Bis", "al\u00b7les", "Volk", ",", "selbst", "geist\u00b7durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vereint, erleuchtet, und befreit", "tokens": ["Ver\u00b7eint", ",", "er\u00b7leuch\u00b7tet", ",", "und", "be\u00b7freit"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVPP", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein ew'ges Pfingsten sich errungen,", "tokens": ["Ein", "ew'\u00b7ges", "Pfings\u00b7ten", "sich", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Lieb' und Licht und Herrlichkeit.", "tokens": ["Voll", "Lieb'", "und", "Licht", "und", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}