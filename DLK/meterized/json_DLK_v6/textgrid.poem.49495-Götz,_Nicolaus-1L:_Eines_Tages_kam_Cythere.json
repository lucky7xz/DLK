{"textgrid.poem.49495": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eines Tages kam Cythere", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eines Tages kam Cythere", "tokens": ["Ei\u00b7nes", "Ta\u00b7ges", "kam", "Cy\u00b7the\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem Fuse des Parna\u00dfes", "tokens": ["An", "dem", "Fu\u00b7se", "des", "Par\u00b7na\u00b7\u00dfes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Und ersucht ihn, ihren Knaben,", "tokens": ["Und", "er\u00b7sucht", "ihn", ",", "ih\u00b7ren", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der so wild zu unterrichten.", "tokens": ["Der", "so", "wild", "zu", "un\u00b7ter\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich nahm er ihn in die Lehre;", "tokens": ["Gleich", "nahm", "er", "ihn", "in", "die", "Leh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Lehrt ihn der Cam\u00f6nen K\u00fcnste;", "tokens": ["Lehrt", "ihn", "der", "Ca\u00b7m\u00f6\u00b7nen", "K\u00fcns\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Macht ihn sittsam und gehorsam", "tokens": ["Macht", "ihn", "sitt\u00b7sam", "und", "ge\u00b7hor\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gegen seine sch\u00f6ne Lehren;", "tokens": ["Ge\u00b7gen", "sei\u00b7ne", "sch\u00f6\u00b7ne", "Leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und gew\u00f6hnt ihn, vor den Musen", "tokens": ["Und", "ge\u00b7w\u00f6hnt", "ihn", ",", "vor", "den", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Stets gekleidet zu erscheinen.", "tokens": ["Stets", "ge\u00b7klei\u00b7det", "zu", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lange nachher kam sie wieder.", "tokens": ["Lan\u00b7ge", "nach\u00b7her", "kam", "sie", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weiser, und geliebter Dichter,", "tokens": ["Wei\u00b7ser", ",", "und", "ge\u00b7lieb\u00b7ter", "Dich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprach sie, was kann ich dir geben,", "tokens": ["Sprach", "sie", ",", "was", "kann", "ich", "dir", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Deinen Flei\u00df an meinem Kleinen", "tokens": ["Dei\u00b7nen", "Flei\u00df", "an", "mei\u00b7nem", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach Verdienste zu belohnen?", "tokens": ["Nach", "Ver\u00b7diens\u00b7te", "zu", "be\u00b7loh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du erzogest ihn so sittsam,", "tokens": ["Du", "er\u00b7zo\u00b7gest", "ihn", "so", "sitt\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ihn alle Pierinnen,", "tokens": ["Da\u00df", "ihn", "al\u00b7le", "Pie\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Da\u00df ihn alle Menschen lieben.", "tokens": ["Da\u00df", "ihn", "al\u00b7le", "Men\u00b7schen", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00f6chtestu doch selber sagen,", "tokens": ["M\u00f6ch\u00b7tes\u00b7tu", "doch", "sel\u00b7ber", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie ich dich belohnen k\u00f6nne!", "tokens": ["Wie", "ich", "dich", "be\u00b7loh\u00b7nen", "k\u00f6n\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Soll ich von den Charitinnen", "tokens": ["Soll", "ich", "von", "den", "Cha\u00b7ri\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dir die Artigste verm\u00e4hlen?", "tokens": ["Dir", "die", "Ar\u00b7tigs\u00b7te", "ver\u00b7m\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.13": {"text": "Oder willstu eine andre?", "tokens": ["O\u00b7der", "will\u00b7stu", "ei\u00b7ne", "and\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Er erwiederte bescheiden,", "tokens": ["Er", "er\u00b7wie\u00b7der\u00b7te", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit gro\u00dfer Ehrerbietung:", "tokens": ["Und", "mit", "gro\u00b7\u00dfer", "Ehr\u00b7er\u00b7bie\u00b7tung", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbach! ", "tokens": ["\u00bb", "ach", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ITJ", "$."], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Sie verstund ihn, und verm\u00e4hlte", "tokens": ["Sie", "ver\u00b7stund", "ihn", ",", "und", "ver\u00b7m\u00e4hl\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich in des Parna\u00dfes G\u00e4rten", "tokens": ["Sich", "in", "des", "Par\u00b7na\u00b7\u00dfes", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit ihm, in geheimer Stille.", "tokens": ["Mit", "ihm", ",", "in", "ge\u00b7hei\u00b7mer", "Stil\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie badete, so hielt er", "tokens": ["Wenn", "sie", "ba\u00b7de\u00b7te", ",", "so", "hielt", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihren G\u00fcrtel in Verwahrung;", "tokens": ["Ih\u00b7ren", "G\u00fcr\u00b7tel", "in", "Ver\u00b7wah\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn er dichtete, so schrieben", "tokens": ["Wenn", "er", "dich\u00b7te\u00b7te", ",", "so", "schrie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihre Gratien die Lieder,", "tokens": ["Ih\u00b7re", "Gra\u00b7ti\u00b7en", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die sie ihn verbessern lehrte.", "tokens": ["Die", "sie", "ihn", "ver\u00b7bes\u00b7sern", "lehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Amor selbst mu\u00dft ihn bedienen:", "tokens": ["A\u00b7mor", "selbst", "mu\u00dft", "ihn", "be\u00b7die\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ihm den alten Bart von Silber,", "tokens": ["Ihm", "den", "al\u00b7ten", "Bart", "von", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ihm die alten Locken salben,", "tokens": ["Ihm", "die", "al\u00b7ten", "Lo\u00b7cken", "sal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ihn bey holdem Sonnenscheine", "tokens": ["Ihn", "bey", "hol\u00b7dem", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "An der Hand spazieren f\u00fchren,", "tokens": ["An", "der", "Hand", "spa\u00b7zie\u00b7ren", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ihm die goldne Leyer tragen,", "tokens": ["Ihm", "die", "gold\u00b7ne", "Le\u00b7yer", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ihm, mit jedem neuen Morgen,", "tokens": ["Ihm", ",", "mit", "je\u00b7dem", "neu\u00b7en", "Mor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Neue Rosenkr\u00e4nze binden,", "tokens": ["Neu\u00b7e", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7ze", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und um seine Schl\u00e4fe winden;", "tokens": ["Und", "um", "sei\u00b7ne", "Schl\u00e4\u00b7fe", "win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und ihn immer: ", "tokens": ["Und", "ihn", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Und ihn immer: ", "tokens": ["Und", "ihn", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Niemand wolle sich verwundern,", "tokens": ["Nie\u00b7mand", "wol\u00b7le", "sich", "ver\u00b7wun\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Da\u00df man seine Kleinigkeiten", "tokens": ["Da\u00df", "man", "sei\u00b7ne", "Klei\u00b7nig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Annoch liest, und \u00fcbersetzet.", "tokens": ["An\u00b7noch", "liest", ",", "und", "\u00fc\u00b7bers\u00b7et\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}