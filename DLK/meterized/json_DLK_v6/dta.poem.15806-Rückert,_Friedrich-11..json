{"dta.poem.15806": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Arbeitsam willst du seyn, doch nicht Erholung missen,", "tokens": ["Ar\u00b7beit\u00b7sam", "willst", "du", "seyn", ",", "doch", "nicht", "Er\u00b7ho\u00b7lung", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAINF", "$,", "ADV", "PTKNEG", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und beides m\u00f6chtest du recht auszugleichen wissen.", "tokens": ["Und", "bei\u00b7des", "m\u00f6ch\u00b7test", "du", "recht", "aus\u00b7zu\u00b7glei\u00b7chen", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "La\u00df dir empfehlen, was Erfahrung mir empfohlen:", "tokens": ["La\u00df", "dir", "emp\u00b7feh\u00b7len", ",", "was", "Er\u00b7fah\u00b7rung", "mir", "emp\u00b7foh\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "PWS", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einer Arbeit dient die andre zum Erholen.", "tokens": ["Von", "ei\u00b7ner", "Ar\u00b7beit", "dient", "die", "and\u00b7re", "zum", "Er\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Ausruh' bester Art ist Wechselth\u00e4tigkeit,", "tokens": ["Die", "Aus\u00b7ruh'", "bes\u00b7ter", "Art", "ist", "Wech\u00b7selt\u00b7h\u00e4\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo gleich im Wechsel bleibt des Strebens St\u00e4tigkeit.", "tokens": ["Wo", "gleich", "im", "Wech\u00b7sel", "bleibt", "des", "Stre\u00b7bens", "St\u00e4\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}