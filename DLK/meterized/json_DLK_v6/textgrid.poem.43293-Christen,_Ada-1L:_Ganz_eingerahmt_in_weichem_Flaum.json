{"textgrid.poem.43293": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ganz eingerahmt in weichem Flaum", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz eingerahmt in weichem Flaum", "tokens": ["Ganz", "ein\u00b7ge\u00b7rahmt", "in", "wei\u00b7chem", "Flaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind heute unsre Scheiben,", "tokens": ["Sind", "heu\u00b7te", "uns\u00b7re", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sehe durch die L\u00fccken kaum", "tokens": ["Ich", "se\u00b7he", "durch", "die", "L\u00fc\u00b7cken", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wirre Flockentreiben.", "tokens": ["Das", "wir\u00b7re", "Flo\u00b7cken\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Thurm hat eine M\u00fctze auf", "tokens": ["Der", "Thurm", "hat", "ei\u00b7ne", "M\u00fct\u00b7ze", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schneewei\u00df, und Edelsteine", "tokens": ["Schnee\u00b7wei\u00df", ",", "und", "E\u00b7del\u00b7stei\u00b7ne"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Umglitzern ihn bis an den Knauf", "tokens": ["Um\u00b7glit\u00b7zern", "ihn", "bis", "an", "den", "Knauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Wintersonnenscheine.", "tokens": ["Im", "Win\u00b7ter\u00b7son\u00b7nen\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So guckt er freundlich aus der Fern'", "tokens": ["So", "guckt", "er", "freund\u00b7lich", "aus", "der", "Fern'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unser Nest das warme,", "tokens": ["In", "un\u00b7ser", "Nest", "das", "war\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als freute auch den alten Herrn", "tokens": ["Als", "freu\u00b7te", "auch", "den", "al\u00b7ten", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Kind in deinem Arme.", "tokens": ["Das", "Kind", "in", "dei\u00b7nem", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}