{"textgrid.poem.65038": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Baum erhebt sich hier", "tokens": ["Kein", "Baum", "er\u00b7hebt", "sich", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf sand'ger D\u00fcnenfl\u00e4che,", "tokens": ["Auf", "san\u00b7d'\u00b7ger", "D\u00fc\u00b7nen\u00b7fl\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der von dem Lenze dir", "tokens": ["Der", "von", "dem", "Len\u00b7ze", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mit S\u00e4usellauten spr\u00e4che.", "tokens": ["Mit", "S\u00e4u\u00b7sel\u00b7lau\u00b7ten", "spr\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vergebens lauscht dein Ohr", "tokens": ["Ver\u00b7ge\u00b7bens", "lauscht", "dein", "Ohr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem Lied der Nachtigallen,", "tokens": ["Dem", "Lied", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es flieht ihr s\u00fc\u00dfer Chor", "tokens": ["Es", "flieht", "ihr", "s\u00fc\u00b7\u00dfer", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die \u00f6den Meereshallen.", "tokens": ["Die", "\u00f6\u00b7den", "Mee\u00b7res\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier h\u00f6rst du nur den Sang", "tokens": ["Hier", "h\u00f6rst", "du", "nur", "den", "Sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der zwei gewalt'gen Stimmen,", "tokens": ["Der", "zwei", "ge\u00b7walt'\u00b7gen", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die todesfroh und bang", "tokens": ["Die", "to\u00b7des\u00b7froh", "und", "bang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Wenn sich in grimmer Wuth", "tokens": ["Wenn", "sich", "in", "grim\u00b7mer", "Wuth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Am Fels die Wogen brechen,", "tokens": ["Am", "Fels", "die", "Wo\u00b7gen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mit der dunkeln Fluth", "tokens": ["Und", "mit", "der", "dun\u00b7keln", "Fluth"], "toke