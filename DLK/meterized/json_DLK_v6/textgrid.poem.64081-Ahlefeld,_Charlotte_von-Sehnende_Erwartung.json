{"textgrid.poem.64081": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Sehnende Erwartung", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es l\u00e4rmt der Markt \u2013 Ger\u00e4usch erf\u00fcllt die Strassen,", "tokens": ["Es", "l\u00e4rmt", "der", "Markt", "\u2013", "Ge\u00b7r\u00e4usch", "er\u00b7f\u00fcllt", "die", "Stras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Glocke klingt, die Th\u00fcr geht auf und zu,", "tokens": ["Die", "Glo\u00b7cke", "klingt", ",", "die", "Th\u00fcr", "geht", "auf", "und", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Dem lauschenden, get\u00e4uschten Ohr entgegen,", "tokens": ["Dem", "lau\u00b7schen\u00b7den", ",", "ge\u00b7t\u00e4uschten", "Ohr", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das jedem Selbstbetruge freudig glaubt.", "tokens": ["Das", "je\u00b7dem", "Selbst\u00b7be\u00b7tru\u00b7ge", "freu\u00b7dig", "glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch ach umsonst! es regt sich frohes Leben,", "tokens": ["Doch", "ach", "um\u00b7sonst", "!", "es", "regt", "sich", "fro\u00b7hes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "PPER", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Th\u00e4tigkeit im tosenden Gedr\u00e4nge", "tokens": ["Und", "Th\u00e4\u00b7tig\u00b7keit", "im", "to\u00b7sen\u00b7den", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der lauten Stadt, die \u2013 wie ein wogend Meer", "tokens": ["Der", "lau\u00b7ten", "Stadt", ",", "die", "\u2013", "wie", "ein", "wo\u00b7gend", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "$(", "KOKOM", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den isolirten Felsen rings umsp\u00fchlt \u2013", "tokens": ["Den", "i\u00b7sol\u00b7ir\u00b7ten", "Fel\u00b7sen", "rings", "um\u00b7sp\u00fchlt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mich Einsame umgiebt. \u2013 Ach ", "tokens": ["Mich", "Ein\u00b7sa\u00b7me", "um\u00b7giebt", ".", "\u2013", "Ach"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["PPER", "NN", "VVPP", "$.", "$(", "ITJ"], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Vernehm' ich nicht \u2013 harmonisch w\u00fcrde dann", "tokens": ["Ver\u00b7nehm'", "ich", "nicht", "\u2013", "har\u00b7mo\u00b7nisch", "w\u00fcr\u00b7de", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "$(", "ADJD", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das wild verworrene Ger\u00e4usch mich gr\u00fcssen,", "tokens": ["Das", "wild", "ver\u00b7wor\u00b7re\u00b7ne", "Ge\u00b7r\u00e4usch", "mich", "gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das jetzt bet\u00e4ubend mir die Brust beklemmt.", "tokens": ["Das", "jetzt", "be\u00b7t\u00e4u\u00b7bend", "mir", "die", "Brust", "be\u00b7klemmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zerstreuung m\u00f6cht' ich im Gew\u00fchle suchen,", "tokens": ["Zer\u00b7streu\u00b7ung", "m\u00f6cht'", "ich", "im", "Ge\u00b7w\u00fch\u00b7le", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Doch mitten unter Menschen f\u00fchl' ich mich allein", "tokens": ["Doch", "mit\u00b7ten", "un\u00b7ter", "Men\u00b7schen", "f\u00fchl'", "ich", "mich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit Deinem Bilde, das in meiner Seele", "tokens": ["Mit", "Dei\u00b7nem", "Bil\u00b7de", ",", "das", "in", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mild wie der Mond in ew'ger Klarheit strahlet.", "tokens": ["Mild", "wie", "der", "Mond", "in", "ew'\u00b7ger", "Klar\u00b7heit", "strah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ja, immer stehst Du vor mir, rein und liebend,", "tokens": ["Ja", ",", "im\u00b7mer", "stehst", "Du", "vor", "mir", ",", "rein", "und", "lie\u00b7bend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "F\u00fcr mich der Inbegriff des h\u00f6chsten Gl\u00fccks.", "tokens": ["F\u00fcr", "mich", "der", "In\u00b7be\u00b7griff", "des", "h\u00f6chs\u00b7ten", "Gl\u00fccks", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Aus Deinem L\u00e4cheln nehm' ich meine Freude,", "tokens": ["Aus", "Dei\u00b7nem", "L\u00e4\u00b7cheln", "nehm'", "ich", "mei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Aus Deinem Ernste saug' ich meinen Schmerz,", "tokens": ["Aus", "Dei\u00b7nem", "Erns\u00b7te", "saug'", "ich", "mei\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Begeistrung weht Dein Athem mir entgegen", "tokens": ["Be\u00b7geis\u00b7trung", "weht", "Dein", "A\u00b7them", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und neuen Muth erweckt in mir Dein Blick.", "tokens": ["Und", "neu\u00b7en", "Muth", "er\u00b7weckt", "in", "mir", "Dein", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O weile nicht \u2013 der Trennung finstre Wolken", "tokens": ["O", "wei\u00b7le", "nicht", "\u2013", "der", "Tren\u00b7nung", "finst\u00b7re", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$(", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Umziehen bald den Horizont des Lebens", "tokens": ["Um\u00b7zie\u00b7hen", "bald", "den", "Ho\u00b7ri\u00b7zont", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und weite Ferne dr\u00e4ngt sich zwischen uns.", "tokens": ["Und", "wei\u00b7te", "Fer\u00b7ne", "dr\u00e4ngt", "sich", "zwi\u00b7schen", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So g\u00f6nne mir die letzten, goldnen Strahlen,", "tokens": ["So", "g\u00f6n\u00b7ne", "mir", "die", "letz\u00b7ten", ",", "gold\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die meine dunkle Bahn mir noch erhellen.", "tokens": ["Die", "mei\u00b7ne", "dunk\u00b7le", "Bahn", "mir", "noch", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn schnell entflieht die Zeit \u2013 auf ihren Schwingen", "tokens": ["Denn", "schnell", "ent\u00b7flieht", "die", "Zeit", "\u2013", "auf", "ih\u00b7ren", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nimmt sie die Bl\u00fcthen unsers Daseyns mit,", "tokens": ["Nimmt", "sie", "die", "Bl\u00fc\u00b7then", "un\u00b7sers", "Da\u00b7seyns", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und nur die Reue bleibt, die um vers\u00e4umte Stunden", "tokens": ["Und", "nur", "die", "Reu\u00b7e", "bleibt", ",", "die", "um", "ver\u00b7s\u00e4um\u00b7te", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Trauerflor vergebner Wehmuth breitet.", "tokens": ["Den", "Trau\u00b7er\u00b7flor", "ver\u00b7geb\u00b7ner", "Weh\u00b7muth", "brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O lass ihr keinen Augenblick verh\u00fcllen,", "tokens": ["O", "lass", "ihr", "kei\u00b7nen", "Au\u00b7gen\u00b7blick", "ver\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den wir dem Schicksal abgewinnen d\u00fcrfen,", "tokens": ["Den", "wir", "dem", "Schick\u00b7sal", "ab\u00b7ge\u00b7win\u00b7nen", "d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und eile sehnend, wie ich Dich erwarte,", "tokens": ["Und", "ei\u00b7le", "seh\u00b7nend", ",", "wie", "ich", "Dich", "er\u00b7war\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dem Herzen zu, das Dir entgegen schl\u00e4gt.", "tokens": ["Dem", "Her\u00b7zen", "zu", ",", "das", "Dir", "ent\u00b7ge\u00b7gen", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}