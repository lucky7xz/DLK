{"dta.poem.18373": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Schertz-gedancken/ als er sie  \n hincken sahe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verzeihe/ sch\u00f6nstes bild/ wann meine reimen hincken/", "tokens": ["Ver\u00b7zei\u00b7he", "/", "sch\u00f6ns\u00b7tes", "bild", "/", "wann", "mei\u00b7ne", "rei\u00b7men", "hin\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "PWAV", "PPOSAT", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn alle syllben nicht in gleichen schrancken stehn/", "tokens": ["Wenn", "al\u00b7le", "syll\u00b7ben", "nicht", "in", "glei\u00b7chen", "schran\u00b7cken", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PTKNEG", "APPR", "ADJA", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann offt die w\u00f6rter selbst aus dem gewichte sincken/", "tokens": ["Wann", "offt", "die", "w\u00f6r\u00b7ter", "selbst", "aus", "dem", "ge\u00b7wich\u00b7te", "sin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nicht/ wie sichs geb\u00fchrt/ in steiffer ordnung gehn;", "tokens": ["Und", "nicht", "/", "wie", "sichs", "ge\u00b7b\u00fchrt", "/", "in", "steif\u00b7fer", "ord\u00b7nung", "gehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$(", "PWAV", "PIS", "VVPP", "$(", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie lernen es von dir: denn als ich dich erblickte/", "tokens": ["Sie", "ler\u00b7nen", "es", "von", "dir", ":", "denn", "als", "ich", "dich", "er\u00b7blick\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$.", "KON", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie sich dein einer fu\u00df nicht gar zu wol bequemt/", "tokens": ["Wie", "sich", "dein", "ei\u00b7ner", "fu\u00df", "nicht", "gar", "zu", "wol", "be\u00b7quemt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "ART", "NN", "PTKNEG", "ADV", "PTKA", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und dich bey jedem tritt fast zu der erden dr\u00fcckte/", "tokens": ["Und", "dich", "bey", "je\u00b7dem", "tritt", "fast", "zu", "der", "er\u00b7den", "dr\u00fcck\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ward hand und feder mir von stunden an gel\u00e4hmt.", "tokens": ["Ward", "hand", "und", "fe\u00b7der", "mir", "von", "stun\u00b7den", "an", "ge\u00b7l\u00e4hmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PPER", "APPR", "ADJA", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kan hand und feder nun nicht eben zierlich schertzen/", "tokens": ["Kan", "hand", "und", "fe\u00b7der", "nun", "nicht", "e\u00b7ben", "zier\u00b7lich", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "ADJD", "ADV", "PTKNEG", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Steckt keine lieblichkeit in dieser engen schrifft;", "tokens": ["Steckt", "kei\u00b7ne", "lieb\u00b7lich\u00b7keit", "in", "die\u00b7ser", "en\u00b7gen", "schrifft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PDAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So liegt die schuld an dir/ weil dein zu strenger schmertzen", "tokens": ["So", "liegt", "die", "schuld", "an", "dir", "/", "weil", "dein", "zu", "stren\u00b7ger", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "APPR", "PPER", "$(", "KOUS", "PPOSAT", "PTKA", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mir alle sehnen hemmt und durchs ge\u00e4der trifft.", "tokens": ["Mir", "al\u00b7le", "seh\u00b7nen", "hemmt", "und", "durchs", "ge\u00b7\u00e4\u00b7der", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVINF", "VVFIN", "KON", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wiewol mein brief will sich auch hinckend zu dir f\u00fcgen/", "tokens": ["Wie\u00b7wol", "mein", "brief", "will", "sich", "auch", "hin\u00b7ckend", "zu", "dir", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "VMFIN", "PRF", "ADV", "VVPP", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er stellt sich eben so/ wie du/ erb\u00e4rmlich an/", "tokens": ["Er", "stellt", "sich", "e\u00b7ben", "so", "/", "wie", "du", "/", "er\u00b7b\u00e4rm\u00b7lich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "$(", "PWAV", "PPER", "$(", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und kan man rath und trost aus frembdem kummer kriegen/", "tokens": ["Und", "kan", "man", "rath", "und", "trost", "aus", "fremb\u00b7dem", "kum\u00b7mer", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So glaub ich/ da\u00df er dich gewi\u00df auch tr\u00f6sten kan.", "tokens": ["So", "glaub", "ich", "/", "da\u00df", "er", "dich", "ge\u00b7wi\u00df", "auch", "tr\u00f6s\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Schau nur wie kl\u00e4glich er wil hin und wieder sincken/", "tokens": ["Schau", "nur", "wie", "kl\u00e4g\u00b7lich", "er", "wil", "hin", "und", "wie\u00b7der", "sin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOKOM", "ADJD", "PPER", "VMFIN", "ADV", "KON", "ADV", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.18": {"text": "Wie alle syllben fast auf krancken f\u00fcssen gehn/", "tokens": ["Wie", "al\u00b7le", "syll\u00b7ben", "fast", "auf", "kran\u00b7cken", "f\u00fcs\u00b7sen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADV", "APPR", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und wie die w\u00f6rter selbst von allen seiten hincken/", "tokens": ["Und", "wie", "die", "w\u00f6r\u00b7ter", "selbst", "von", "al\u00b7len", "sei\u00b7ten", "hin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was letzt soll/ geht voran/ was erst/ willetzlich stehn.", "tokens": ["Was", "letzt", "soll", "/", "geht", "vo\u00b7ran", "/", "was", "erst", "/", "wil\u00b7letz\u00b7lich", "stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "$(", "VVFIN", "PTKVZ", "$(", "PWS", "ADV", "$(", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nicht lache/ sch\u00f6nstes bild/ es taumlen hand und sinnen/", "tokens": ["Nicht", "la\u00b7che", "/", "sch\u00f6ns\u00b7tes", "bild", "/", "es", "taum\u00b7len", "hand", "und", "sin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$(", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du schaust allhier die frucht verwirrter traurigkeit;", "tokens": ["Du", "schaust", "all\u00b7hier", "die", "frucht", "ver\u00b7wirr\u00b7ter", "trau\u00b7rig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und dencke selber nach/ was kan man wol beginnen/", "tokens": ["Und", "den\u00b7cke", "sel\u00b7ber", "nach", "/", "was", "kan", "man", "wol", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "$(", "PWS", "VMFIN", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wann feder/ hand und hertz mit kummer \u00fcberstreut?", "tokens": ["Wann", "fe\u00b7der", "/", "hand", "und", "hertz", "mit", "kum\u00b7mer", "\u00fc\u00b7bers\u00b7treut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$(", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jedoch ich weis/ du wirst die reime nicht verachten/", "tokens": ["Je\u00b7doch", "ich", "weis", "/", "du", "wirst", "die", "rei\u00b7me", "nicht", "ver\u00b7ach\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKVZ", "$(", "PPER", "VAFIN", "ART", "VVFIN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ob sie schon noch so schwach mit lahmen schritten gehn/", "tokens": ["Ob", "sie", "schon", "noch", "so", "schwach", "mit", "lah\u00b7men", "schrit\u00b7ten", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "APPR", "VVINF", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du wirst vielmehr durch sie den ordenitzt betrachten/", "tokens": ["Du", "wirst", "viel\u00b7mehr", "durch", "sie", "den", "or\u00b7de\u00b7nitzt", "be\u00b7trach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "ART", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Darinn du selbst nun must als mit-geno\u00dfin stehn.", "tokens": ["Da\u00b7rinn", "du", "selbst", "nun", "must", "als", "mit\u00b7ge\u00b7no\u00b7\u00dfin", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADV", "VMFIN", "KOKOM", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du darfst gewi\u00dflich dich des hinckens gar nicht sch\u00e4men/", "tokens": ["Du", "darfst", "ge\u00b7wi\u00df\u00b7lich", "dich", "des", "hin\u00b7ckens", "gar", "nicht", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "ART", "ADV", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es ist von alters her fast hoch und werth geacht.", "tokens": ["Es", "ist", "von", "al\u00b7ters", "her", "fast", "hoch", "und", "werth", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mann must\u2019 offt hinckende zum dienst der g\u00f6tter nehmen/", "tokens": ["Mann", "must'", "offt", "hin\u00b7cken\u00b7de", "zum", "dienst", "der", "g\u00f6t\u00b7ter", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "VVFIN", "APPRART", "ADV", "ART", "ADJA", "VVINF", "$("], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.32": {"text": "Wann ihnen etwan ward ein opfer-fest gebracht;", "tokens": ["Wann", "ih\u00b7nen", "et\u00b7wan", "ward", "ein", "op\u00b7fer\u00b7fest", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Was war wol lieblicher und sch\u00f6ner anzusehen/", "tokens": ["Was", "war", "wol", "lieb\u00b7li\u00b7cher", "und", "sch\u00f6\u00b7ner", "an\u00b7zu\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Als da ein scharffer dorn der Venus fu\u00df verletzt/", "tokens": ["Als", "da", "ein", "scharf\u00b7fer", "dorn", "der", "Ve\u00b7nus", "fu\u00df", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und sie deswegenlahm und hinckend muste gehen?", "tokens": ["Und", "sie", "des\u00b7we\u00b7gen\u00b7lahm", "und", "hin\u00b7ckend", "mus\u00b7te", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVPP", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und doch ward sie deshalb nicht minder hoch-gesch\u00e4tzt,", "tokens": ["Und", "doch", "ward", "sie", "des\u00b7halb", "nicht", "min\u00b7der", "hoch\u00b7ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PAV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.37": {"text": "Das tantzen an sich selbst ist nur ein zierlichs hincken/", "tokens": ["Das", "tant\u00b7zen", "an", "sich", "selbst", "ist", "nur", "ein", "zier\u00b7lichs", "hin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PRF", "ADV", "VAFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Man hincket ebenfals/ nur da\u00df mans k\u00fcnstlich macht/", "tokens": ["Man", "hin\u00b7cket", "e\u00b7ben\u00b7fals", "/", "nur", "da\u00df", "mans", "k\u00fcnst\u00b7lich", "macht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$(", "ADV", "KOUS", "PIS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bald hebt der fus sich auf/ bald mus er wieder sincken/", "tokens": ["Bald", "hebt", "der", "fus", "sich", "auf", "/", "bald", "mus", "er", "wie\u00b7der", "sin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "$(", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So wie er durch die kunst in ordnung wird gebracht.", "tokens": ["So", "wie", "er", "durch", "die", "kunst", "in", "ord\u00b7nung", "wird", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "APPR", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ja selbst Vulcanus ist deshalb in himmel kommen/", "tokens": ["Ja", "selbst", "Vul\u00b7ca\u00b7nus", "ist", "des\u00b7halb", "in", "him\u00b7mel", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "NE", "VAFIN", "PAV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Man trug ihm willig auff der grossen g\u00f6tter rath/", "tokens": ["Man", "trug", "ihm", "wil\u00b7lig", "auff", "der", "gros\u00b7sen", "g\u00f6t\u00b7ter", "rath", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die ihn in ihre zahl und freundschafft aufgenommen/", "tokens": ["Die", "ihn", "in", "ih\u00b7re", "zahl", "und", "freund\u00b7schafft", "auf\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ob er schon noch so grob und starck gehincket hat;", "tokens": ["Ob", "er", "schon", "noch", "so", "grob", "und", "starck", "ge\u00b7hin\u00b7cket", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ist nun Vulcanus gott/ so mustu g\u00f6ttin werden/", "tokens": ["Ist", "nun", "Vul\u00b7ca\u00b7nus", "gott", "/", "so", "mus\u00b7tu", "g\u00f6t\u00b7tin", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "NN", "$(", "ADV", "VMFIN", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Weil ihr euch beyde gut und wol zusammen schickt;", "tokens": ["Weil", "ihr", "euch", "bey\u00b7de", "gut", "und", "wol", "zu\u00b7sam\u00b7men", "schickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "ADJD", "KON", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Befe\u00dfle du demnach die freyheit dieser erden/", "tokens": ["Be\u00b7fe\u00df\u00b7le", "du", "dem\u00b7nach", "die", "frey\u00b7heit", "die\u00b7ser", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Er h\u00e4lt den himmels- creys dort droben schon bestrickt.", "tokens": ["Er", "h\u00e4lt", "den", "him\u00b7mels", "creys", "dort", "dro\u00b7ben", "schon", "be\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "TRUNC", "NN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}