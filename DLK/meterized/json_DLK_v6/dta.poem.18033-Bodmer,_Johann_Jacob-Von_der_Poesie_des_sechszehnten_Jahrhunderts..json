{"dta.poem.18033": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der Poesie des sechszehnten  \n Jahrhunderts.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lieb der Mensch lebendig sey,", "tokens": ["Wie", "lieb", "der", "Mensch", "le\u00b7ben\u00b7dig", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist nach Tod Beywohnung frey.", "tokens": ["Er", "ist", "nach", "Tod", "Bey\u00b7woh\u00b7nung", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Bauch ist gar ein b&#339;ser Sack", "tokens": ["Der", "Bauch", "ist", "gar", "ein", "b", "&#339;", "ser", "Sack"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "XY", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er verh&#339;nt aller W&#252;rtze Geschmack.", "tokens": ["Er", "verh", "&#339;", "nt", "al\u00b7ler", "W", "&#252;", "rt\u00b7ze", "Ge\u00b7schmack", "."], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "FM.la", "FM.la", "FM.la", "$(", "ADJA", "NN", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Wer Muscatnuss nimt in den Mund", "tokens": ["Wer", "Mus\u00b7cat\u00b7nuss", "nimt", "in", "den", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und es wieder austhet zu Stund;", "tokens": ["Und", "es", "wie\u00b7der", "aus\u00b7thet", "zu", "Stund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Sie waer ihm darnach ungezaem,", "tokens": ["Sie", "waer", "ihm", "dar\u00b7nach", "un\u00b7ge\u00b7zaem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "PPER", "PAV", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df er sie in Mund wieder naem.", "tokens": ["Da\u00df", "er", "sie", "in", "Mund", "wie\u00b7der", "naem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Seit wir uns selber widerstahn,", "tokens": ["Seit", "wir", "uns", "sel\u00b7ber", "wi\u00b7der\u00b7stahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wer soll uns dann vor sauber han?", "tokens": ["Wer", "soll", "uns", "dann", "vor", "sau\u00b7ber", "han", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}