{"textgrid.poem.39893": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still in des Herzogs Hause", "tokens": ["Still", "in", "des", "Her\u00b7zogs", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ward's, mitten in Madrid,", "tokens": ["Ward's", ",", "mit\u00b7ten", "in", "Mad\u00b7rid", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPR", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es hallt in seiner Klause", "tokens": ["Es", "hallt", "in", "sei\u00b7ner", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht mehr des Henkers Tritt.", "tokens": ["Nicht", "mehr", "des", "Hen\u00b7kers", "Tritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vom Baume seines Lebens", "tokens": ["Vom", "Bau\u00b7me", "sei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fiel Frucht und Blatt schon ab,", "tokens": ["Fiel", "Frucht", "und", "Blatt", "schon", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hin ist der Mut des Strebens,", "tokens": ["Hin", "ist", "der", "Mut", "des", "Stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerknickt sein Feldherrnstab.", "tokens": ["Zer\u00b7knickt", "sein", "Feld\u00b7herrns\u00b7tab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Leib ist morsch, die Schmerzen", "tokens": ["Der", "Leib", "ist", "morsch", ",", "die", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verzehrten seine Kraft;", "tokens": ["Ver\u00b7zehr\u00b7ten", "sei\u00b7ne", "Kraft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In dem verwelkten Herzen", "tokens": ["In", "dem", "ver\u00b7welk\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dorrt selbst die Leidenschaft.", "tokens": ["Dorrt", "selbst", "die", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sein Haupt liegt auf dem Kissen,", "tokens": ["Sein", "Haupt", "liegt", "auf", "dem", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er lechzt nicht mehr nach Blut,", "tokens": ["Er", "lechzt", "nicht", "mehr", "nach", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das nackende Gewissen", "tokens": ["Das", "na\u00b7cken\u00b7de", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Ist all sein Hab' und Gut.", "tokens": ["Ist", "all", "sein", "Hab'", "und", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum klammert er sich zagend", "tokens": ["Drum", "klam\u00b7mert", "er", "sich", "za\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An's kahle Leben an,", "tokens": ["An's", "kah\u00b7le", "Le\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Blicken \u00e4ngstlich fragend,", "tokens": ["Mit", "Bli\u00b7cken", "\u00e4ngst\u00b7lich", "fra\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob Niemand fristen kann.", "tokens": ["Ob", "Nie\u00b7mand", "fris\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch nichts erquickt den Armen,", "tokens": ["Doch", "nichts", "er\u00b7quickt", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stumpf ist der Aerzte Witz;", "tokens": ["Stumpf", "ist", "der", "A\u00b7erz\u00b7te", "Witz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur einen will's erbarmen,", "tokens": ["Nur", "ei\u00b7nen", "will's", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den mahnt es, wie ein Blitz.", "tokens": ["Den", "mahnt", "es", ",", "wie", "ein", "Blitz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbden Tod ihm fern zu halten,", "tokens": ["\u00bb", "den", "Tod", "ihm", "fern", "zu", "hal\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist eins mir noch bewu\u00dft:", "tokens": ["Ist", "eins", "mir", "noch", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Legt den ersch\u00f6pften Alten", "tokens": ["Legt", "den", "er\u00b7sch\u00f6pf\u00b7ten", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An eines Weibes Brust!\u00ab", "tokens": ["An", "ei\u00b7nes", "Wei\u00b7bes", "Brust", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Arzt sprach sorgsam, leise,", "tokens": ["Der", "Arzt", "sprach", "sorg\u00b7sam", ",", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Diener es vernahm;", "tokens": ["Der", "Die\u00b7ner", "es", "ver\u00b7nahm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald stehet vor dem Greise", "tokens": ["Bald", "ste\u00b7het", "vor", "dem", "Grei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein s\u00e4ugend Weib voll Scham.", "tokens": ["Ein", "s\u00e4u\u00b7gend", "Weib", "voll", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Mutteraugen lenken", "tokens": ["Die", "Mut\u00b7ter\u00b7au\u00b7gen", "len\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mitleidig sich auf ihn,", "tokens": ["Mit\u00b7lei\u00b7dig", "sich", "auf", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPER", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Den d\u00fcrren Mund zu tr\u00e4nken,", "tokens": ["Den", "d\u00fcr\u00b7ren", "Mund", "zu", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Reicht sie die Brust ihm hin.", "tokens": ["Reicht", "sie", "die", "Brust", "ihm", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Mit innigem Vergn\u00fcgen \u2013", "tokens": ["Mit", "in\u00b7ni\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Er wei\u00df nicht, was er thut \u2013", "tokens": ["Er", "wei\u00df", "nicht", ",", "was", "er", "thut", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trinkt er in langen Z\u00fcgen,", "tokens": ["Trinkt", "er", "in", "lan\u00b7gen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch ihm wird Milch zu Blut.", "tokens": ["Doch", "ihm", "wird", "Milch", "zu", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Bald graus't ihm vor dem Tranke,", "tokens": ["Bald", "graus't", "ihm", "vor", "dem", "Tran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er kehrt sich weg entsetzt.", "tokens": ["Er", "kehrt", "sich", "weg", "ent\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf blickt der Schwache, Kranke,", "tokens": ["Auf", "blickt", "der", "Schwa\u00b7che", ",", "Kran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und todbla\u00df ruft er jetzt:", "tokens": ["Und", "tod\u00b7bla\u00df", "ruft", "er", "jetzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbo, ich will nicht mehr morden,", "tokens": ["\u00bb", "o", ",", "ich", "will", "nicht", "mehr", "mor\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich hab's versprochen, Weib!", "tokens": ["Ich", "hab's", "ver\u00b7spro\u00b7chen", ",", "Weib", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin dein S\u00e4ugling worden.", "tokens": ["Ich", "bin", "dein", "S\u00e4ug\u00b7ling", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verschone meinen Leib!\u00ab", "tokens": ["Ver\u00b7scho\u00b7ne", "mei\u00b7nen", "Leib", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Aerzte stehn und staunen,", "tokens": ["Die", "A\u00b7erz\u00b7te", "stehn", "und", "stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Wahnsinn bricht hervor,", "tokens": ["Der", "Wahn\u00b7sinn", "bricht", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die alten Diener raunen", "tokens": ["Die", "al\u00b7ten", "Die\u00b7ner", "rau\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erinnrung sich in's Ohr.", "tokens": ["E\u00b7rinn\u00b7rung", "sich", "in's", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Es beut die Brust vergebens", "tokens": ["Es", "beut", "die", "Brust", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das junge Weib dem Greis,", "tokens": ["Das", "jun\u00b7ge", "Weib", "dem", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am warmen Quell des Lebens", "tokens": ["Am", "war\u00b7men", "Quell", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Liegt er wie Stein und Eis.", "tokens": ["Liegt", "er", "wie", "Stein", "und", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Die Lippen regt er zitternd;", "tokens": ["Die", "Lip\u00b7pen", "regt", "er", "zit\u00b7ternd", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Auge das Gericht,", "tokens": ["Im", "Au\u00b7ge", "das", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verlischt, die H\u00f6lle witternd,", "tokens": ["Ver\u00b7lischt", ",", "die", "H\u00f6l\u00b7le", "wit\u00b7ternd", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein bleiches Angesicht.", "tokens": ["Sein", "blei\u00b7ches", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}