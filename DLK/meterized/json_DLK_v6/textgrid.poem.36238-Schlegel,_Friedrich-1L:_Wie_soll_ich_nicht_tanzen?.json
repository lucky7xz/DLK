{"textgrid.poem.36238": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie soll ich nicht tanzen?", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie soll ich nicht tanzen?", "tokens": ["Wie", "soll", "ich", "nicht", "tan\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Es macht keine M\u00fche;", "tokens": ["Es", "macht", "kei\u00b7ne", "M\u00fc\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und reizende Farben", "tokens": ["Und", "rei\u00b7zen\u00b7de", "Far\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schimmern hier im Gr\u00fcnen.", "tokens": ["Schim\u00b7mern", "hier", "im", "Gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Immer sch\u00f6ner gl\u00e4nzen", "tokens": ["Im\u00b7mer", "sch\u00f6\u00b7ner", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Meine bunten Fl\u00fcgel,", "tokens": ["Mei\u00b7ne", "bun\u00b7ten", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Immer s\u00fc\u00dfer hauchen", "tokens": ["Im\u00b7mer", "s\u00fc\u00b7\u00dfer", "hau\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Alle kleinen Bl\u00fcten.", "tokens": ["Al\u00b7le", "klei\u00b7nen", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Ich nasche die Bl\u00fcten,", "tokens": ["Ich", "na\u00b7sche", "die", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Ihr k\u00f6nnt sie nicht h\u00fcten.", "tokens": ["Ihr", "k\u00f6nnt", "sie", "nicht", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie gro\u00df ist die Freude,", "tokens": ["Wie", "gro\u00df", "ist", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sei's sp\u00e4t oder fr\u00fche,", "tokens": ["Sei's", "sp\u00e4t", "o\u00b7der", "fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Leichtsinnig zu schweben", "tokens": ["Leicht\u00b7sin\u00b7nig", "zu", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "\u00dcber Tal und H\u00fcgel.", "tokens": ["\u00dc\u00b7ber", "Tal", "und", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn der Abend s\u00e4uselt", "tokens": ["Wenn", "der", "A\u00b7bend", "s\u00e4u\u00b7selt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Seht ihr Wolken gl\u00fchen;", "tokens": ["Seht", "ihr", "Wol\u00b7ken", "gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn die L\u00fcfte golden,", "tokens": ["Wenn", "die", "L\u00fcf\u00b7te", "gol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Scheint die Wiese gr\u00fcner.", "tokens": ["Scheint", "die", "Wie\u00b7se", "gr\u00fc\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Ich nasche die Bl\u00fcten,", "tokens": ["Ich", "na\u00b7sche", "die", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Ihr k\u00f6nnt sie nicht h\u00fcten.", "tokens": ["Ihr", "k\u00f6nnt", "sie", "nicht", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}