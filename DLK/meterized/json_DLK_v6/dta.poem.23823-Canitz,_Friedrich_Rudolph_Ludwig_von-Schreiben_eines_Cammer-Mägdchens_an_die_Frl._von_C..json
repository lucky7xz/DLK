{"dta.poem.23823": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Schreiben eines Cammer-M\u00e4gdchens  \n an die Frl. von C.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Weil sich doch keine Magd darf in ihr Zimmer wagen/", "tokens": ["Weil", "sich", "doch", "kei\u00b7ne", "Magd", "darf", "in", "ihr", "Zim\u00b7mer", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "PIAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihre Blicke nicht auf schlechte Leute gehn/", "tokens": ["Und", "ih\u00b7re", "Bli\u00b7cke", "nicht", "auf", "schlech\u00b7te", "Leu\u00b7te", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df ich durch dis Blat mich \u00fcber sie beklagen/", "tokens": ["So", "mu\u00df", "ich", "durch", "dis", "Blat", "mich", "\u00fc\u00b7ber", "sie", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PDS", "NN", "PRF", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nachdem mir ohne Schuld so grosse Schmach ge-", "tokens": ["Nach\u00b7dem", "mir", "oh\u00b7ne", "Schuld", "so", "gros\u00b7se", "Schmach", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erinnert sie sich noch/ wie gestern bey dem Tantze/", "tokens": ["E\u00b7rin\u00b7nert", "sie", "sich", "noch", "/", "wie", "ge\u00b7stern", "bey", "dem", "Tant\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$(", "KOKOM", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+----+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ihr ungerechter Spruch mich aus der Reye stie\u00df/", "tokens": ["Ihr", "un\u00b7ge\u00b7rech\u00b7ter", "Spruch", "mich", "aus", "der", "Re\u00b7ye", "stie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja aus der Cammer selbst/ (als wenn ich ihrem Glantze", "tokens": ["Ja", "aus", "der", "Cam\u00b7mer", "selbst", "/", "(", "als", "wenn", "ich", "ih\u00b7rem", "Glant\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "ART", "NN", "ADV", "$(", "$(", "KOKOM", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Ansto\u00df w\u00fcrde seyn/) ins Elend wandern hie\u00df.", "tokens": ["Ein", "An\u00b7sto\u00df", "w\u00fcr\u00b7de", "seyn", "/", ")", "ins", "E\u00b7lend", "wan\u00b7dern", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VAINF", "$(", "$(", "APPRART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den Schwager welcher mich zu seinem Ungl\u00fcck wehlte/", "tokens": ["Den", "Schwa\u00b7ger", "wel\u00b7cher", "mich", "zu", "sei\u00b7nem", "Un\u00b7gl\u00fcck", "wehl\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den traf mit mir zugleich ihr hartes Donner-Wort/", "tokens": ["Den", "traf", "mit", "mir", "zu\u00b7gleich", "ihr", "har\u00b7tes", "Don\u00b7ner\u00b7Wort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und weil mir ein Geschlecht von sechszehn Ahnen fehlte/", "tokens": ["Und", "weil", "mir", "ein", "Ge\u00b7schlecht", "von", "sechs\u00b7zehn", "Ah\u00b7nen", "fehl\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So muste Coridon mit samt der Nymphe fort.", "tokens": ["So", "mus\u00b7te", "Co\u00b7ri\u00b7don", "mit", "samt", "der", "Nym\u00b7phe", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich glaube da\u00df es nicht der Juno mehr verdrossen/", "tokens": ["Ich", "glau\u00b7be", "da\u00df", "es", "nicht", "der", "Ju\u00b7no", "mehr", "ver\u00b7dros\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PTKNEG", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als Paris ihren Grimm durch seine Wahl erweckt;", "tokens": ["Als", "Pa\u00b7ris", "ih\u00b7ren", "Grimm", "durch", "sei\u00b7ne", "Wahl", "er\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NE", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich schwere/ da\u00df vor Angst ich wenig Ruh genossen/", "tokens": ["Ich", "schwe\u00b7re", "/", "da\u00df", "vor", "Angst", "ich", "we\u00b7nig", "Ruh", "ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "APPR", "NN", "PPER", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ihr zornig Angesicht hat mich im Schlaff erschreckt.", "tokens": ["Ihr", "zor\u00b7nig", "An\u00b7ge\u00b7sicht", "hat", "mich", "im", "Schlaff", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Hochzeit ist wol recht mein Trauer-Fest geworden/", "tokens": ["Die", "Hoch\u00b7zeit", "ist", "wol", "recht", "mein", "Trau\u00b7e\u00b7rFest", "ge\u00b7wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was andre fr\u00f6lich macht/ ist Ursach meiner Pein/", "tokens": ["Was", "and\u00b7re", "fr\u00f6\u00b7lich", "macht", "/", "ist", "Ur\u00b7sach", "mei\u00b7ner", "Pein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVFIN", "$(", "VAFIN", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Braut ist eine Magd noch in geringerm Orden/", "tokens": ["Die", "Braut", "ist", "ei\u00b7ne", "Magd", "noch", "in", "ge\u00b7rin\u00b7germ", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch wird sie hoch geachtt/ ich mu\u00df verh\u00f6hnet seyn.", "tokens": ["Doch", "wird", "sie", "hoch", "ge\u00b7achtt", "/", "ich", "mu\u00df", "ver\u00b7h\u00f6h\u00b7net", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVPP", "$(", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die gantze M\u00e4gde-Zunfft wird meiner sp\u00f6ttisch lachen/", "tokens": ["Die", "gant\u00b7ze", "M\u00e4g\u00b7de\u00b7Zunfft", "wird", "mei\u00b7ner", "sp\u00f6t\u00b7tisch", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Fama tr\u00e4gt es schon bi\u00df auf den Fisch-Marckt hin/", "tokens": ["Die", "Fa\u00b7ma", "tr\u00e4gt", "es", "schon", "bi\u00df", "auf", "den", "FischMa\u00b7rckt", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Da\u00df mein Verh\u00e4ngni\u00df mir den Schand-Fleck wollen", "tokens": ["Da\u00df", "mein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "mir", "den", "Schan\u00b7dFleck", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und was ich vor ein Ball des falschen Gl\u00fcckes bin.", "tokens": ["Und", "was", "ich", "vor", "ein", "Ball", "des", "fal\u00b7schen", "Gl\u00fc\u00b7ckes", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich kan mich/ Fr\u00e4ulein/ nicht an ihrem Hochmuth r\u00e4chen/", "tokens": ["Ich", "kan", "mich", "/", "Fr\u00e4u\u00b7lein", "/", "nicht", "an", "ih\u00b7rem", "Hoch\u00b7muth", "r\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$(", "NN", "$(", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Doch hoff ich da\u00df es ihr soll nach Verdienst ergehn/", "tokens": ["Doch", "hoff", "ich", "da\u00df", "es", "ihr", "soll", "nach", "Ver\u00b7dienst", "er\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "PPER", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df noch ein --- ihr wird den Starr-Kopff brechen/", "tokens": ["Da\u00df", "noch", "ein", "ihr", "wird", "den", "Starr\u00b7Kopff", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "$(", "PPER", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.28": {"text": "Denn werd\u2019 ich Freud und Lust an meiner Feindin", "tokens": ["Denn", "werd'", "ich", "Freud", "und", "Lust", "an", "mei\u00b7ner", "Fein\u00b7din"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}