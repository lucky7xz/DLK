{"dta.poem.361": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Talestris  stehet viel vor der Th\u00fcre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Talestris die nunmehr nach drey\u00dfig Jahren gehet/", "tokens": ["Ta\u00b7le\u00b7stris", "die", "nun\u00b7mehr", "nach", "drey\u00b7\u00dfig", "Jah\u00b7ren", "ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADV", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und ihre Kinder-Schuh bereits vertreten hat/", "tokens": ["Und", "ih\u00b7re", "Kin\u00b7der\u00b7Schuh", "be\u00b7reits", "ver\u00b7tre\u00b7ten", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die gr\u00e4mt sich/ da\u00df ihr Mund nicht mehr voll Rosen stehet", "tokens": ["Die", "gr\u00e4mt", "sich", "/", "da\u00df", "ihr", "Mund", "nicht", "mehr", "voll", "Ro\u00b7sen", "ste\u00b7het"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "$(", "KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie er ihr vor der Zeit in jungen Jahren that.", "tokens": ["Wie", "er", "ihr", "vor", "der", "Zeit", "in", "jun\u00b7gen", "Jah\u00b7ren", "that", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch weil sie noch nicht gantz auf Wang\u2019 und Mund verbl\u00fchet/", "tokens": ["Doch", "weil", "sie", "noch", "nicht", "gantz", "auf", "Wang'", "und", "Mund", "ver\u00b7bl\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So stellt das grosse Kind sich t\u00e4glich vor die Th\u00fcr/", "tokens": ["So", "stellt", "das", "gros\u00b7se", "Kind", "sich", "t\u00e4g\u00b7lich", "vor", "die", "Th\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das stumme Wesen spricht/ wie! da\u00df denn keiner siehet", "tokens": ["Das", "stum\u00b7me", "We\u00b7sen", "spricht", "/", "wie", "!", "da\u00df", "denn", "kei\u00b7ner", "sie\u00b7het"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KOKOM", "$.", "KOUS", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df ich so artig bin/ ist denn kein K\u00e4uffer hier?", "tokens": ["Da\u00df", "ich", "so", "ar\u00b7tig", "bin", "/", "ist", "denn", "kein", "K\u00e4uf\u00b7fer", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$(", "VAFIN", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie l\u00e4st wol hundert mahl sich an die Gassen schauen/", "tokens": ["Sie", "l\u00e4st", "wol", "hun\u00b7dert", "mahl", "sich", "an", "die", "Gas\u00b7sen", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "ADV", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und danckt den Gr\u00fcssenden mit grosser Freundlichkeit;", "tokens": ["Und", "danckt", "den", "Gr\u00fcs\u00b7sen\u00b7den", "mit", "gros\u00b7ser", "Freund\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein dis alles kan ihr Gl\u00fccke nicht erbauen/", "tokens": ["Al\u00b7lein", "dis", "al\u00b7les", "kan", "ihr", "Gl\u00fc\u00b7cke", "nicht", "er\u00b7bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PIS", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kein Freyer findt sich ein/ es ist niemand der beut.", "tokens": ["Kein", "Frey\u00b7er", "findt", "sich", "ein", "/", "es", "ist", "nie\u00b7mand", "der", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ART", "$(", "PPER", "VAFIN", "PIS", "ART", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Das stehen n\u00fctzt ihr nichts/ umsonst l\u00e4st sie sich sehen/", "tokens": ["Das", "ste\u00b7hen", "n\u00fctzt", "ihr", "nichts", "/", "um\u00b7sonst", "l\u00e4st", "sie", "sich", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PIS", "$(", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es beist kein K\u00e4uffer an/ die Waare will man nicht;", "tokens": ["Es", "beist", "kein", "K\u00e4uf\u00b7fer", "an", "/", "die", "Waa\u00b7re", "will", "man", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$(", "ART", "NN", "VMFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch h\u00f6rt ihr losen Leut/ man mu\u00df wol draussen stehen/", "tokens": ["Doch", "h\u00f6rt", "ihr", "lo\u00b7sen", "Leut", "/", "man", "mu\u00df", "wol", "draus\u00b7sen", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$(", "PIS", "VMFIN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und vor der Th\u00fcren seyn wenn Raum und Platz gebricht.", "tokens": ["Und", "vor", "der", "Th\u00fc\u00b7ren", "seyn", "wenn", "Raum", "und", "Platz", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAINF", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}