{"dta.poem.11343": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bey dem seeligen hintritt herrn Michael  \n Ludolffs, K\u00f6nigl. Preu\u00dfischen land-  \n schafftlichen  Secretarii.  \n B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich, seeligster! dein hertz,", "tokens": ["Wenn", "ich", ",", "see\u00b7ligs\u00b7ter", "!", "dein", "hertz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wenn ich dein bekannt gem\u00fcthe,", "tokens": ["Wenn", "ich", "dein", "be\u00b7kannt", "ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine sanfftmuth, deine g\u00fcte,", "tokens": ["Dei\u00b7ne", "sanfft\u00b7muth", ",", "dei\u00b7ne", "g\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich deiner freunde schmertz", "tokens": ["Wenn", "ich", "dei\u00b7ner", "freun\u00b7de", "schmertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich aufs h\u00f6chste wolte treiben;", "tokens": ["Gleich", "aufs", "h\u00f6chs\u00b7te", "wol\u00b7te", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcrd\u2019 ich doch die wahrheit schreiben:", "tokens": ["W\u00fcrd'", "ich", "doch", "die", "wahr\u00b7heit", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles seufftzt und klagt um dich;", "tokens": ["Al\u00b7les", "seufftzt", "und", "klagt", "um", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es bleibt nicht bey den deinen", "tokens": ["Und", "es", "bleibt", "nicht", "bey", "den", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "APPR", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Man sieht auch die kirche weinen:", "tokens": ["Man", "sieht", "auch", "die", "kir\u00b7che", "wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Land und stadt betr\u00fcbet sich,", "tokens": ["Land", "und", "stadt", "be\u00b7tr\u00fc\u00b7bet", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df mit dir heut in der erden", "tokens": ["Da\u00df", "mit", "dir", "heut", "in", "der", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "So viel soll begraben werden.", "tokens": ["So", "viel", "soll", "be\u00b7gra\u00b7ben", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Selbst der Preussen grosser held", "tokens": ["Selbst", "der", "Preus\u00b7sen", "gros\u00b7ser", "held"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der bey diesen strengen zeiten", "tokens": ["Der", "bey", "die\u00b7sen", "stren\u00b7gen", "zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehr viel von ge\u00fcbten leuten,", "tokens": ["Sehr", "viel", "von", "ge\u00b7\u00fcb\u00b7ten", "leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und noch mehr von treuen, h\u00e4lt,", "tokens": ["Und", "noch", "mehr", "von", "treu\u00b7en", ",", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcrde, w\u00fcst\u2019 er dein erblassen,", "tokens": ["W\u00fcr\u00b7de", ",", "w\u00fcst'", "er", "dein", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich nicht unbedauert lassen;", "tokens": ["Dich", "nicht", "un\u00b7be\u00b7dau\u00b7ert", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}