{"dta.poem.16349": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "10.  Ich bin wie Gott/ und Gott wie ich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin so gro\u00df als GOtt/ Er ist als ich so klein:", "tokens": ["Ich", "bin", "so", "gro\u00df", "als", "Gott", "/", "Er", "ist", "als", "ich", "so", "klein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "NN", "$(", "PPER", "VAFIN", "KOUS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er kan nicht \u00fcber mich/ ich unter Jhm nicht seyn.", "tokens": ["Er", "kan", "nicht", "\u00fc\u00b7ber", "mich", "/", "ich", "un\u00b7ter", "Jhm", "nicht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "$(", "PPER", "APPR", "PPER", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}