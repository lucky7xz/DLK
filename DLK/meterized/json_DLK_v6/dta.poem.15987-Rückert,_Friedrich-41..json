{"dta.poem.15987": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Abendr\u00f6the kam, und sah zum Tod ermattet", "tokens": ["Die", "A\u00b7bend\u00b7r\u00f6\u00b7the", "kam", ",", "und", "sah", "zum", "Tod", "er\u00b7mat\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Leben, Schlummer half, und sanft ward es bestattet.", "tokens": ["Das", "Le\u00b7ben", ",", "Schlum\u00b7mer", "half", ",", "und", "sanft", "ward", "es", "be\u00b7stat\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVFIN", "$,", "KON", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Nacht im Trauerflor, die dunkle Klagefrau,", "tokens": ["Die", "Nacht", "im", "Trau\u00b7er\u00b7flor", ",", "die", "dunk\u00b7le", "Kla\u00b7ge\u00b7frau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gieng hinterdrein, und weint' aus Sternen kalten Thau.", "tokens": ["Gieng", "hin\u00b7ter\u00b7drein", ",", "und", "weint'", "aus", "Ster\u00b7nen", "kal\u00b7ten", "Thau", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch Morgenr\u00f6the kam heran mit gl\u00fchnden Wangen,", "tokens": ["Doch", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "kam", "he\u00b7ran", "mit", "gl\u00fchn\u00b7den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und rief: Wo ist mein Kind? ich gl\u00fch' es zu umfangen.", "tokens": ["Und", "rief", ":", "Wo", "ist", "mein", "Kind", "?", "ich", "gl\u00fch'", "es", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gestorben! rief die Nacht mit letztem Thr\u00e4nengu\u00df.", "tokens": ["Ge\u00b7stor\u00b7ben", "!", "rief", "die", "Nacht", "mit", "letz\u00b7tem", "Thr\u00e4\u00b7nen\u00b7gu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da weckt' es rasch vom Schlaf der Morgenr\u00f6the Ku\u00df.", "tokens": ["Da", "weckt'", "es", "rasch", "vom", "Schlaf", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die holde Mutter sprach: O d\u00fcrft' ich bei dir bleiben!", "tokens": ["Die", "hol\u00b7de", "Mut\u00b7ter", "sprach", ":", "O", "d\u00fcrft'", "ich", "bei", "dir", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "NE", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch schon die Sonne flammt, von dir mich zu vertreiben.", "tokens": ["Doch", "schon", "die", "Son\u00b7ne", "flammt", ",", "von", "dir", "mich", "zu", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Leb' wohl! auch diesen Tag und jeden mu\u00dft du sterben,", "tokens": ["Leb'", "wohl", "!", "auch", "die\u00b7sen", "Tag", "und", "je\u00b7den", "mu\u00dft", "du", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "ADV", "PDAT", "NN", "KON", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch neues Leben stets von meinem Hauch erwerben.", "tokens": ["Doch", "neu\u00b7es", "Le\u00b7ben", "stets", "von", "mei\u00b7nem", "Hauch", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}