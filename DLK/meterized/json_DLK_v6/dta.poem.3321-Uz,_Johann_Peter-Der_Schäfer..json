{"dta.poem.3321": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Sch\u00e4fer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.71", "af:0.28"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Arkadien! sey mir gegr\u00fcsst!", "tokens": ["Ar\u00b7ka\u00b7di\u00b7en", "!", "sey", "mir", "ge\u00b7gr\u00fcs\u00b7st", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Du Land begl\u00fcckter Hirten,", "tokens": ["Du", "Land", "be\u00b7gl\u00fcck\u00b7ter", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo unter unentweihten Myrthen", "tokens": ["Wo", "un\u00b7ter", "un\u00b7ent\u00b7weih\u00b7ten", "Myr\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein z\u00e4rtlich Herz allein noch r\u00fchmlich ist!", "tokens": ["Ein", "z\u00e4rt\u00b7lich", "Herz", "al\u00b7lein", "noch", "r\u00fchm\u00b7lich", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich will mit sanftem Hirtenstab", "tokens": ["Ich", "will", "mit", "sanf\u00b7tem", "Hir\u00b7ten\u00b7stab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier meine Schafe weiden.", "tokens": ["Hier", "mei\u00b7ne", "Scha\u00b7fe", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier, Liebe! schenke mir die Freuden,", "tokens": ["Hier", ",", "Lie\u00b7be", "!", "schen\u00b7ke", "mir", "die", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mir die Stadt, die stolze Stadt nicht gab.", "tokens": ["Die", "mir", "die", "Stadt", ",", "die", "stol\u00b7ze", "Stadt", "nicht", "gab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie sch\u00e4ferm\u00e4ssig, wie getreu", "tokens": ["Wie", "sch\u00e4\u00b7fer\u00b7m\u00e4s\u00b7sig", ",", "wie", "ge\u00b7treu"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich Climenen lieben,", "tokens": ["Will", "ich", "Cli\u00b7me\u00b7nen", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Bis meinen ehrfurchtvollen Trieben", "tokens": ["Bis", "mei\u00b7nen", "ehr\u00b7furcht\u00b7vol\u00b7len", "Trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr Mund erlaubt, da\u00df ich ihr Sch\u00e4fer sey!", "tokens": ["Ihr", "Mund", "er\u00b7laubt", ",", "da\u00df", "ich", "ihr", "Sch\u00e4\u00b7fer", "sey", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Welch s\u00fcssem Traume geb ich Raum,", "tokens": ["Welch", "s\u00fcs\u00b7sem", "Trau\u00b7me", "geb", "ich", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mich zum Sch\u00e4fer machet!", "tokens": ["Der", "mich", "zum", "Sch\u00e4\u00b7fer", "ma\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die traurige Vernunft erwachet:", "tokens": ["Die", "trau\u00b7ri\u00b7ge", "Ver\u00b7nunft", "er\u00b7wa\u00b7chet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz tr\u00e4umt fort und liebet seinen Traum.", "tokens": ["Das", "Herz", "tr\u00e4umt", "fort", "und", "lie\u00b7bet", "sei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}