{"textgrid.poem.33448": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Schw\u00e4rmerei", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Paul sagte:", "tokens": ["Paul", "sag\u00b7te", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ach, wer doch ewig Auto fahren k\u00f6nnte \u2013", "tokens": ["Ach", ",", "wer", "doch", "e\u00b7wig", "Au\u00b7to", "fah\u00b7ren", "k\u00f6nn\u00b7te", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "ADV", "ADJD", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir bohren uns durch hochgestielte W\u00e4lder,", "tokens": ["Wir", "boh\u00b7ren", "uns", "durch", "hoch\u00b7ges\u00b7tiel\u00b7te", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir \u00fcberholen Fl\u00e4chen, die sich endlos schienen.", "tokens": ["Wir", "\u00fc\u00b7berh\u00b7o\u00b7len", "Fl\u00e4\u00b7chen", ",", "die", "sich", "end\u00b7los", "schie\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir \u00fcberfahren den Wind und \u00fcberfallen die D\u00f6rfer, die flinken.", "tokens": ["Wir", "\u00fc\u00b7berf\u00b7ah\u00b7ren", "den", "Wind", "und", "\u00fc\u00b7berf\u00b7al\u00b7len", "die", "D\u00f6r\u00b7fer", ",", "die", "flin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+--+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Aber verha\u00dft sind uns die Ger\u00fcche der langsamen St\u00e4dte \u2013", "tokens": ["A\u00b7ber", "ver\u00b7ha\u00dft", "sind", "uns", "die", "Ge\u00b7r\u00fc\u00b7che", "der", "lang\u00b7sa\u00b7men", "St\u00e4d\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Hei, wie wir fliegen! Immer den Tod entlang ...", "tokens": ["Hei", ",", "wie", "wir", "flie\u00b7gen", "!", "Im\u00b7mer", "den", "Tod", "ent\u00b7lang", "..."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VVINF", "$.", "ADV", "ART", "NN", "APPO", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie wir ihn h\u00f6hnen und ihn verspotten, der uns am Leben sitzt!", "tokens": ["Wie", "wir", "ihn", "h\u00f6h\u00b7nen", "und", "ihn", "ver\u00b7spot\u00b7ten", ",", "der", "uns", "am", "Le\u00b7ben", "sitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "KON", "PPER", "VVFIN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Der uns die Gr\u00e4ben legt und alle Stra\u00dfen kr\u00fcmmt \u2013 ha, wir verlachen ihn", "tokens": ["Der", "uns", "die", "Gr\u00e4\u00b7ben", "legt", "und", "al\u00b7le", "Stra\u00b7\u00dfen", "kr\u00fcmmt", "\u2013", "ha", ",", "wir", "ver\u00b7la\u00b7chen", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "KON", "PIAT", "NN", "VVFIN", "$(", "ITJ", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und die Wege, die \u00fcberwundenen, vergehen vor uns \u2013", "tokens": ["Und", "die", "We\u00b7ge", ",", "die", "\u00fc\u00b7berw\u00b7un\u00b7de\u00b7nen", ",", "ver\u00b7ge\u00b7hen", "vor", "uns", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVINF", "$,", "VVFIN", "APPR", "PPER", "$("], "meter": "--+--+-+---+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "So werden wir die ganze Welt durchauteln ...", "tokens": ["So", "wer\u00b7den", "wir", "die", "gan\u00b7ze", "Welt", "durch\u00b7au\u00b7teln", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis wir einmal an einem heitern Abend", "tokens": ["Bis", "wir", "ein\u00b7mal", "an", "ei\u00b7nem", "hei\u00b7tern", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An einem starken Baum ein kr\u00e4ftges Ende finden.", "tokens": ["An", "ei\u00b7nem", "star\u00b7ken", "Baum", "ein", "kr\u00e4ft\u00b7ges", "En\u00b7de", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}