{"textgrid.poem.46208": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Antwortschreiben, in eben denselbigen reimen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wiewol ich weit und breit den g\u00f6ttern dieser orten", "tokens": ["Wie\u00b7wol", "ich", "weit", "und", "breit", "den", "g\u00f6t\u00b7tern", "die\u00b7ser", "or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "ART", "ADJA", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mit aller sorg und treu nach ihrer eignen worten", "tokens": ["mit", "al\u00b7ler", "sorg", "und", "treu", "nach", "ih\u00b7rer", "eig\u00b7nen", "wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "inhalt gern dienen wolt; wiewol ich schreib in eil", "tokens": ["in\u00b7halt", "gern", "die\u00b7nen", "wolt", ";", "wie\u00b7wol", "ich", "schreib", "in", "eil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$.", "KOUS", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "oder (tiefsinniger) zu dichten mich verweil,", "tokens": ["o\u00b7der", "(", "tief\u00b7sin\u00b7ni\u00b7ger", ")", "zu", "dich\u00b7ten", "mich", "ver\u00b7weil", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADJA", "$(", "APPR", "ADJA", "PPER", "ADJD", "$,"], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "kan ihnen leider doch, die sich, blut zu vergie\u00dfen,", "tokens": ["kan", "ih\u00b7nen", "lei\u00b7der", "doch", ",", "die", "sich", ",", "blut", "zu", "ver\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "$,", "PRELS", "PRF", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "erfreuen, nicht nach lust die schwarze farb herflie\u00dfen.", "tokens": ["er\u00b7freu\u00b7en", ",", "nicht", "nach", "lust", "die", "schwar\u00b7ze", "farb", "her\u00b7flie\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "PTKNEG", "APPR", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ja solt mir Ph\u00f6bus selbs und Hermes ihre list", "tokens": ["Ja", "solt", "mir", "Ph\u00f6\u00b7bus", "selbs", "und", "Her\u00b7mes", "ih\u00b7re", "list"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "VMFIN", "PPER", "NE", "ADV", "KON", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und ihre ganze kunst verleihen alle frist", "tokens": ["und", "ih\u00b7re", "gan\u00b7ze", "kunst", "ver\u00b7lei\u00b7hen", "al\u00b7le", "frist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "mich in dem sch\u00f6nen feld der tugend und der ehren", "tokens": ["mich", "in", "dem", "sch\u00f6\u00b7nen", "feld", "der", "tu\u00b7gend", "und", "der", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "KON", "ART", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "erquickend, andre leut der freiheit bahn zu lehren;", "tokens": ["er\u00b7qui\u00b7ckend", ",", "and\u00b7re", "leut", "der", "frei\u00b7heit", "bahn", "zu", "leh\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PIS", "VVFIN", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja wan ich schon alsbald den panzer selbs anleg", "tokens": ["Ja", "wan", "ich", "schon", "als\u00b7bald", "den", "pan\u00b7zer", "selbs", "an\u00b7leg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PWAV", "PPER", "ADV", "ADV", "ART", "NN", "ADV", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und mich, als ob ich jung wie Phaeton, beweg,", "tokens": ["und", "mich", ",", "als", "ob", "ich", "jung", "wie", "Phae\u00b7ton", ",", "be\u00b7weg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "KOKOM", "NE", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "das doch zu torecht ist, solt ich sie nicht ergetzen,", "tokens": ["das", "doch", "zu", "to\u00b7recht", "ist", ",", "solt", "ich", "sie", "nicht", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKA", "ADJD", "VAFIN", "$,", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "sondern vilmehr sie freind, dan ihre feind verletzen.", "tokens": ["son\u00b7dern", "vil\u00b7mehr", "sie", "freind", ",", "dan", "ih\u00b7re", "feind", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJD", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Drum der goldh\u00e4rig got m\u00f6cht sparen sein geschenk,", "tokens": ["Drum", "der", "gold\u00b7h\u00e4\u00b7rig", "got", "m\u00f6cht", "spa\u00b7ren", "sein", "ge\u00b7schenk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJD", "ADJD", "VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "weil ohn dasselbig ich niemals nicht eingedenk", "tokens": ["weil", "ohn", "das\u00b7sel\u00b7big", "ich", "nie\u00b7mals", "nicht", "ein\u00b7ge\u00b7denk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "PPER", "ADV", "PTKNEG", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "wie dankbar ich mich doch dem schenkern m\u00f6g erweisen.", "tokens": ["wie", "dank\u00b7bar", "ich", "mich", "doch", "dem", "schen\u00b7kern", "m\u00f6g", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "ADV", "ART", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der m\u00fcde Pegasus solt auch nicht ohn hufeisen", "tokens": ["Der", "m\u00fc\u00b7de", "Pe\u00b7ga\u00b7sus", "solt", "auch", "nicht", "ohn", "huf\u00b7ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "von meinetwegen sein, auf da\u00df, so er lahm wird,", "tokens": ["von", "mei\u00b7net\u00b7we\u00b7gen", "sein", ",", "auf", "da\u00df", ",", "so", "er", "lahm", "wird", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAINF", "$,", "APPR", "KOUS", "$,", "ADV", "PPER", "PTKVZ", "VAFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "ihn nicht mit anderm vih wegtreibt ein loser hirt,", "tokens": ["ihn", "nicht", "mit", "an\u00b7derm", "vih", "weg\u00b7treibt", "ein", "lo\u00b7ser", "hirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "PIS", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "und Pallas gebend mir, als ihrem underthanen,", "tokens": ["und", "Pal\u00b7las", "ge\u00b7bend", "mir", ",", "als", "ih\u00b7rem", "un\u00b7der\u00b7tha\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "PPER", "$,", "KOUS", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "ein leibgeding, solt mich zu weiterm dienst nicht mahnen.", "tokens": ["ein", "leib\u00b7ge\u00b7ding", ",", "solt", "mich", "zu", "wei\u00b7term", "dienst", "nicht", "mah\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "VMFIN", "PRF", "APPR", "PIS", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Solt mir auch das Pocal, leer schwer gnug an gewicht,", "tokens": ["Solt", "mir", "auch", "das", "Po\u00b7cal", ",", "leer", "schwer", "gnug", "an", "ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "$,", "ADJD", "ADJD", "ADV", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "die harte Ph\u00f6be selbs und zarte Hebe nicht", "tokens": ["die", "har\u00b7te", "Ph\u00f6\u00b7be", "selbs", "und", "zar\u00b7te", "He\u00b7be", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "KON", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "mit jung und geilem wein und Nectar voll einschenken,", "tokens": ["mit", "jung", "und", "gei\u00b7lem", "wein", "und", "Nec\u00b7tar", "voll", "ein\u00b7schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN", "KON", "NN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "will ich des schenkers doch in ewigkeit gedenken.", "tokens": ["will", "ich", "des", "schen\u00b7kers", "doch", "in", "e\u00b7wig\u00b7keit", "ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}}}}