{"dta.poem.7880": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Untreue .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Dir ist die Herrschaft l\u00e4ngst gegeben", "tokens": ["Dir", "ist", "die", "Herr\u00b7schaft", "l\u00e4ngst", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meinem Liede, meinem Leben,", "tokens": ["In", "mei\u00b7nem", "Lie\u00b7de", ",", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur diese Nacht, o welch ein Traum!", "tokens": ["Nur", "die\u00b7se", "Nacht", ",", "o", "welch", "ein", "Traum", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "FM", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O la\u00df das schwere Herz mich l\u00f6sen!", "tokens": ["O", "la\u00df", "das", "schwe\u00b7re", "Herz", "mich", "l\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es sa\u00df ein fremd, verschleiert Wesen", "tokens": ["Es", "sa\u00df", "ein", "fremd", ",", "ver\u00b7schlei\u00b7ert", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dort unter unsrer Liebe Baum.", "tokens": ["Dort", "un\u00b7ter", "uns\u00b7rer", "Lie\u00b7be", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie h\u00e4lt sie meinen Sinn gefangen!", "tokens": ["Wie", "h\u00e4lt", "sie", "mei\u00b7nen", "Sinn", "ge\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich nahe mich mit s\u00fc\u00dfem Bangen,", "tokens": ["Ich", "na\u00b7he", "mich", "mit", "s\u00fc\u00b7\u00dfem", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie aber hebt den Schleier leicht;", "tokens": ["Sie", "a\u00b7ber", "hebt", "den", "Schlei\u00b7er", "leicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da seh\u2019 ich \u2014 deine lieben Augen,", "tokens": ["Da", "seh'", "ich", "dei\u00b7ne", "lie\u00b7ben", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! deine blauen, trauten Augen,", "tokens": ["Ach", "!", "dei\u00b7ne", "blau\u00b7en", ",", "trau\u00b7ten", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und jeder fremde Schein entweicht.", "tokens": ["Und", "je\u00b7der", "frem\u00b7de", "Schein", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}