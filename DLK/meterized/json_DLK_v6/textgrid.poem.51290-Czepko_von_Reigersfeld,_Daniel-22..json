{"textgrid.poem.51290": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt h\u00e4ngt wie ein Ball in der erhabnen Lufft,", "tokens": ["Die", "Welt", "h\u00e4ngt", "wie", "ein", "Ball", "in", "der", "er\u00b7hab\u00b7nen", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die lichte Lufft mu\u00df die, die alles tr\u00e4get, tragen:", "tokens": ["Die", "lich\u00b7te", "Lufft", "mu\u00df", "die", ",", "die", "al\u00b7les", "tr\u00e4\u00b7get", ",", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus ihrem Lauffe kommt die Lufft nach Wolbehagen,", "tokens": ["Aus", "ih\u00b7rem", "Lauf\u00b7fe", "kommt", "die", "Lufft", "nach", "Wol\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Lufft macht, da\u00df sie lebt, durch Land und Berg und Klufft.", "tokens": ["Die", "Lufft", "macht", ",", "da\u00df", "sie", "lebt", ",", "durch", "Land", "und", "Berg", "und", "Klufft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Mensch befindets selbst. Lufft ist sein Auffenthalt,", "tokens": ["Der", "Mensch", "be\u00b7fin\u00b7dets", "selbst", ".", "Lufft", "ist", "sein", "Auf\u00b7fent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Lufft zeucht er an, Lufft bl\u00e4st er von sich nach Gen\u00fcgen:", "tokens": ["Lufft", "zeucht", "er", "an", ",", "Lufft", "bl\u00e4st", "er", "von", "sich", "nach", "Ge\u00b7n\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "NN", "VVFIN", "PPER", "APPR", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir sehen in der Lufft sein gantzes Leben liegen.", "tokens": ["Wir", "se\u00b7hen", "in", "der", "Lufft", "sein", "gant\u00b7zes", "Le\u00b7ben", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verl\u00e4\u00dfet ihn die Lufft, stracks ist er todt und kalt.", "tokens": ["Ver\u00b7l\u00e4\u00b7\u00dfet", "ihn", "die", "Lufft", ",", "stracks", "ist", "er", "todt", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weil nun der Bau der Welt in eitler Lufft besteht", "tokens": ["Weil", "nun", "der", "Bau", "der", "Welt", "in", "eit\u00b7ler", "Lufft", "be\u00b7steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sol nicht in der Lufft ihr Thun und Werck zustieben?", "tokens": ["Wie", "sol", "nicht", "in", "der", "Lufft", "ihr", "Thun", "und", "Werck", "zu\u00b7stie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Mensch auch, weil er ist und wird von Lufft getrieben,", "tokens": ["Der", "Mensch", "auch", ",", "weil", "er", "ist", "und", "wird", "von", "Lufft", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "PPER", "VAFIN", "KON", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ists Wunder, da\u00df sein Wunsch hin in die L\u00fcffte geht.", "tokens": ["Ists", "Wun\u00b7der", ",", "da\u00df", "sein", "Wunsch", "hin", "in", "die", "L\u00fcff\u00b7te", "geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}