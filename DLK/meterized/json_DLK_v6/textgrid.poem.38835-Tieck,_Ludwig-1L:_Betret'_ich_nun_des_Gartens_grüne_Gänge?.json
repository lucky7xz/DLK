{"textgrid.poem.38835": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Betret' ich nun des Gartens gr\u00fcne G\u00e4nge?", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Betret' ich nun des Gartens gr\u00fcne G\u00e4nge?", "tokens": ["Be\u00b7tret'", "ich", "nun", "des", "Gar\u00b7tens", "gr\u00fc\u00b7ne", "G\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie frisch und lieblich dort die tiefen Gr\u00fcnde!", "tokens": ["Wie", "frisch", "und", "lieb\u00b7lich", "dort", "die", "tie\u00b7fen", "Gr\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Einsamkeit holdseelig und gelinde,", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "hold\u00b7see\u00b7lig", "und", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Chorgesang rauscht hier das Baumgedr\u00e4nge.", "tokens": ["Wie", "Chor\u00b7ge\u00b7sang", "rauscht", "hier", "das", "Baum\u00b7ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was find' ich an dem bl\u00fchenden Geh\u00e4nge?", "tokens": ["Was", "find'", "ich", "an", "dem", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7h\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie! Thr\u00e4nen an so manchem bunten Kinde?", "tokens": ["Wie", "!", "Thr\u00e4\u00b7nen", "an", "so", "man\u00b7chem", "bun\u00b7ten", "Kin\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "APPR", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was seufzen denn so bang die Abendwinde?", "tokens": ["Was", "seuf\u00b7zen", "denn", "so", "bang", "die", "Ab\u00b7end\u00b7win\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo t\u00f6nen her so zauberhaft Ges\u00e4nge?", "tokens": ["Wo", "t\u00f6\u00b7nen", "her", "so", "zau\u00b7ber\u00b7haft", "Ge\u00b7s\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sind wohl so sp\u00e4t in Wandrung noch die Bienen?", "tokens": ["Sind", "wohl", "so", "sp\u00e4t", "in", "Wand\u00b7rung", "noch", "die", "Bie\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlummern hier Lieder aufgeweckt von Sternen?", "tokens": ["Schlum\u00b7mern", "hier", "Lie\u00b7der", "auf\u00b7ge\u00b7weckt", "von", "Ster\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "VVPP", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Des Waldes Geister, in der B\u00e4ume Kronen? \u2013", "tokens": ["Des", "Wal\u00b7des", "Geis\u00b7ter", ",", "in", "der", "B\u00e4u\u00b7me", "Kro\u00b7nen", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gesangs-G\u00f6ttinnen, die den Hain bewohnen,", "tokens": ["Ge\u00b7sangs\u00b7G\u00f6t\u00b7tin\u00b7nen", ",", "die", "den", "Hain", "be\u00b7woh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind jetzt, herdenkend, weit in andern Fernen,", "tokens": ["Sind", "jetzt", ",", "her\u00b7den\u00b7kend", ",", "weit", "in", "an\u00b7dern", "Fer\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "VVPP", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum klagt so Wind, wie Staud', und Baum im Gr\u00fcnen.", "tokens": ["Drum", "klagt", "so", "Wind", ",", "wie", "Staud'", ",", "und", "Baum", "im", "Gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NN", "$,", "PWAV", "NN", "$,", "KON", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Thal, Wald mu\u00df ihnen dienen,", "tokens": ["Thal", ",", "Wald", "mu\u00df", "ih\u00b7nen", "die\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie sind Gesang, und welchen Baum sie denken,", "tokens": ["Sie", "sind", "Ge\u00b7sang", ",", "und", "wel\u00b7chen", "Baum", "sie", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "KON", "PWAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der mu\u00df s\u00fc\u00dfklingend seine Zweige senken.", "tokens": ["Der", "mu\u00df", "s\u00fc\u00df\u00b7klin\u00b7gend", "sei\u00b7ne", "Zwei\u00b7ge", "sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}