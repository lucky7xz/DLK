{"textgrid.poem.46780": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[die Gestalten meiner Lieben]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Gestalten meiner Lieben,", "tokens": ["Die", "Ge\u00b7stal\u00b7ten", "mei\u00b7ner", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufgel\u00f6st in goldnen Duft,", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "in", "gold\u00b7nen", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil in Staub die Leiber stieben,", "tokens": ["Weil", "in", "Staub", "die", "Lei\u00b7ber", "stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Gestalten sind geblieben", "tokens": ["Die", "Ge\u00b7stal\u00b7ten", "sind", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Um mich schwebend in der Luft.", "tokens": ["Um", "mich", "schwe\u00b7bend", "in", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Gestalten meiner Lieben", "tokens": ["Die", "Ge\u00b7stal\u00b7ten", "mei\u00b7ner", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lassen sich vom Hauch der Luft", "tokens": ["Las\u00b7sen", "sich", "vom", "Hauch", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In Gebilde nach Belieben", "tokens": ["In", "Ge\u00b7bil\u00b7de", "nach", "Be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wechselnd so zusammenschieben,", "tokens": ["Wech\u00b7selnd", "so", "zu\u00b7sam\u00b7men\u00b7schie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der Nebel aus der Schluft.", "tokens": ["Wie", "der", "Ne\u00b7bel", "aus", "der", "Schluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Gestalten meiner Lieben,", "tokens": ["Die", "Ge\u00b7stal\u00b7ten", "mei\u00b7ner", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebend \u00fcber ihrer Gruft,", "tokens": ["Schwe\u00b7bend", "\u00fc\u00b7ber", "ih\u00b7rer", "Gruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von des Windes Hauch getrieben", "tokens": ["Von", "des", "Win\u00b7des", "Hauch", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nirgend sind sie fern geblieben,", "tokens": ["Nir\u00b7gend", "sind", "sie", "fern", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo sie meine Sehnsucht ruft.", "tokens": ["Wo", "sie", "mei\u00b7ne", "Sehn\u00b7sucht", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}