{"textgrid.poem.33126": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "15. Soliloquium zu Weyhnachten", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rath, Kraft, und Held und Wunderbar!", "tokens": ["Rath", ",", "Kraft", ",", "und", "Held", "und", "Wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Nam ist meiner Seelen klar", "tokens": ["Dein", "Nam", "ist", "mei\u00b7ner", "See\u00b7len", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Du mit Deinem Blut erkauft,", "tokens": ["Die", "Du", "mit", "Dei\u00b7nem", "Blut", "er\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit der Liebes-Gluht getauft,", "tokens": ["Und", "mit", "der", "Lie\u00b7bes\u00b7Gluht", "ge\u00b7tauft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Br\u00e4utigam, an meiner Stirne brennt", "tokens": ["Mein", "Br\u00e4u\u00b7ti\u00b7gam", ",", "an", "mei\u00b7ner", "Stir\u00b7ne", "brennt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Nam und Creutz, seitdem ich Dich erkennt.", "tokens": ["Dein", "Nam", "und", "Creutz", ",", "seit\u00b7dem", "ich", "Dich", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn ich, mit allem meinem Flei\u00df,", "tokens": ["Wenn", "ich", ",", "mit", "al\u00b7lem", "mei\u00b7nem", "Flei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir nimmermehr zu rathen wei\u00df,", "tokens": ["Mir", "nim\u00b7mer\u00b7mehr", "zu", "ra\u00b7then", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine Ohnmacht, Unverstand", "tokens": ["Und", "mei\u00b7ne", "Ohn\u00b7macht", ",", "Un\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Schwachheit kr\u00e4ftiglich erkant;", "tokens": ["Und", "Schwach\u00b7heit", "kr\u00e4f\u00b7tig\u00b7lich", "er\u00b7kant", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bist Du ja der unerforschte Mann,", "tokens": ["So", "bist", "Du", "ja", "der", "un\u00b7er\u00b7forschte", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Der allen meinen Sachen rathen kan.", "tokens": ["Der", "al\u00b7len", "mei\u00b7nen", "Sa\u00b7chen", "ra\u00b7then", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Fehlt mirs an aller Lebens-Kraft,", "tokens": ["Fehlt", "mirs", "an", "al\u00b7ler", "Le\u00b7bens\u00b7Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat meine Rebe keinen Saft,", "tokens": ["Hat", "mei\u00b7ne", "Re\u00b7be", "kei\u00b7nen", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sinke ich vor Mattigkeit", "tokens": ["Und", "sin\u00b7ke", "ich", "vor", "Mat\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beynahe hin zu mancher Zeit;", "tokens": ["Bey\u00b7na\u00b7he", "hin", "zu", "man\u00b7cher", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ist Dein kr\u00e4ftiges Gef\u00fchl in mir,", "tokens": ["So", "ist", "Dein", "kr\u00e4f\u00b7ti\u00b7ges", "Ge\u00b7f\u00fchl", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das h\u00e4lt mir starke Helden-Kr\u00e4fte f\u00fcr.", "tokens": ["Das", "h\u00e4lt", "mir", "star\u00b7ke", "Hel\u00b7den\u00b7Kr\u00e4f\u00b7te", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn ich im schweren Glaubens-Kampf", "tokens": ["Wenn", "ich", "im", "schwe\u00b7ren", "Glau\u00b7bens\u00b7Kampf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch manchen dikken Rauch und Dampf,", "tokens": ["Durch", "man\u00b7chen", "dik\u00b7ken", "Rauch", "und", "Dampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch manche Leibs- und Geists-Gefahr,", "tokens": ["Durch", "man\u00b7che", "Leibs", "und", "Geist\u00b7s\u00b7Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "TRUNC", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mich dr\u00e4nge zu der Sieges-Schaar;", "tokens": ["Mich", "dr\u00e4n\u00b7ge", "zu", "der", "Sie\u00b7ges\u00b7Schaar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bist Dus, unbezwungner Wunder-Held,", "tokens": ["So", "bist", "Dus", ",", "un\u00b7be\u00b7zwung\u00b7ner", "Wun\u00b7der\u00b7Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der meinetwegen alle Feinde f\u00e4llt.", "tokens": ["Der", "mei\u00b7net\u00b7we\u00b7gen", "al\u00b7le", "Fein\u00b7de", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wenn sich mein Senf-Korns-Glaube regt,", "tokens": ["Wenn", "sich", "mein", "Sen\u00b7fKorns\u00b7Glau\u00b7be", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kindlich Dir zu F\u00fcssen legt,", "tokens": ["Und", "kind\u00b7lich", "Dir", "zu", "F\u00fcs\u00b7sen", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mag der Feinde Hohn-Geschrey", "tokens": ["So", "mag", "der", "Fein\u00b7de", "Hohn\u00b7Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ert\u00f6nen: da\u00df ich th\u00f6richt sey.", "tokens": ["Er\u00b7t\u00f6\u00b7nen", ":", "da\u00df", "ich", "th\u00f6\u00b7richt", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich f\u00fcrchte mich deswegen doch kein Haar:", "tokens": ["Ich", "f\u00fcrch\u00b7te", "mich", "des\u00b7we\u00b7gen", "doch", "kein", "Haar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein Glaub ist Sieg, mein Zwek ist Wunderbar.", "tokens": ["Mein", "Glaub", "ist", "Sieg", ",", "mein", "Zwek", "ist", "Wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mein Alles! mehr als alle Welt,", "tokens": ["Mein", "Al\u00b7les", "!", "mehr", "als", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "$.", "PIAT", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Freund! der ewig Treue h\u00e4lt,", "tokens": ["Mein", "Freund", "!", "der", "e\u00b7wig", "Treu\u00b7e", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein wei\u00df- und rother Br\u00e4utigam!", "tokens": ["Mein", "wei\u00df", "und", "ro\u00b7ther", "Br\u00e4u\u00b7ti\u00b7gam", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein immerw\u00e4hrend Oster-Lamm!", "tokens": ["Mein", "im\u00b7mer\u00b7w\u00e4h\u00b7rend", "Os\u00b7ter\u00b7Lamm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Leit-Stern! meine Liebe! meine Zier!", "tokens": ["Mein", "Leit\u00b7S\u00b7tern", "!", "mei\u00b7ne", "Lie\u00b7be", "!", "mei\u00b7ne", "Zier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sey ewiglich mein Steinritz, mein Panier!", "tokens": ["Sey", "e\u00b7wig\u00b7lich", "mein", "Stein\u00b7ritz", ",", "mein", "Pa\u00b7nier", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Hast Du mich in der Zeit gewolt,", "tokens": ["Hast", "Du", "mich", "in", "der", "Zeit", "ge\u00b7wolt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die R\u00e4der-schnell von dannen rollt;", "tokens": ["Die", "R\u00e4\u00b7der\u00b7schnell", "von", "dan\u00b7nen", "rollt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mi\u00df mir selbst die Stunden ab!", "tokens": ["So", "mi\u00df", "mir", "selbst", "die", "Stun\u00b7den", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sey meiner Reise Wander-Stab!", "tokens": ["Sey", "mei\u00b7ner", "Rei\u00b7se", "Wan\u00b7der\u00b7Stab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sey meines Thuns sein Sch\u00f6pfer! f\u00fchre mich", "tokens": ["Sey", "mei\u00b7nes", "Thuns", "sein", "Sch\u00f6p\u00b7fer", "!", "f\u00fch\u00b7re", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In allem Dir zu wandeln w\u00fcrdiglich!", "tokens": ["In", "al\u00b7lem", "Dir", "zu", "wan\u00b7deln", "w\u00fcr\u00b7dig\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "PTKZU", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Soll ich viel Jahr im Karren fort;", "tokens": ["Soll", "ich", "viel", "Jahr", "im", "Kar\u00b7ren", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So zeige mir den Ruhe-Port,", "tokens": ["So", "zei\u00b7ge", "mir", "den", "Ru\u00b7he\u00b7Port", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von ferne zeige mir die Stadt,", "tokens": ["Von", "fer\u00b7ne", "zei\u00b7ge", "mir", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Deine Hand bereitet hat,", "tokens": ["Die", "Dei\u00b7ne", "Hand", "be\u00b7rei\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das g\u00fcldne Seraphinen Liebes-Licht:", "tokens": ["Das", "g\u00fcld\u00b7ne", "Se\u00b7ra\u00b7phi\u00b7nen", "Lie\u00b7bes\u00b7Licht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So schrekket mich die lange Reise nicht.", "tokens": ["So", "schrek\u00b7ket", "mich", "die", "lan\u00b7ge", "Rei\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und wenn ich meiner Br\u00fcder Zahl", "tokens": ["Und", "wenn", "ich", "mei\u00b7ner", "Br\u00fc\u00b7der", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Deiner holden Gnaden-Wahl", "tokens": ["Nach", "Dei\u00b7ner", "hol\u00b7den", "Gna\u00b7den\u00b7Wahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An meinem Theile auch erf\u00fcllt;", "tokens": ["An", "mei\u00b7nem", "Thei\u00b7le", "auch", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenns endlich auch Belohnens gilt:", "tokens": ["Wenns", "end\u00b7lich", "auch", "Be\u00b7loh\u00b7nens", "gilt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wei\u00dfst Du, da\u00df mein Lohn, mein Licht und Ruh", "tokens": ["So", "wei\u00dfst", "Du", ",", "da\u00df", "mein", "Lohn", ",", "mein", "Licht", "und", "Ruh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur Du alleine werden solst, Nur Du.", "tokens": ["Nur", "Du", "al\u00b7lei\u00b7ne", "wer\u00b7den", "solst", ",", "Nur", "Du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VAINF", "VMFIN", "$,", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}