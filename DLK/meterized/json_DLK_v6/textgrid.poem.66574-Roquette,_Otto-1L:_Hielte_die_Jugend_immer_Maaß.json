{"textgrid.poem.66574": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hielte die Jugend immer Maa\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hielte die Jugend immer Maa\u00df", "tokens": ["Hiel\u00b7te", "die", "Ju\u00b7gend", "im\u00b7mer", "Maa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und verst\u00fcnden die Philister Spa\u00df,", "tokens": ["Und", "ver\u00b7st\u00fcn\u00b7den", "die", "Phi\u00b7lis\u00b7ter", "Spa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gerieth' in jedem Jahre der Wein,", "tokens": ["Ge\u00b7ri\u00b7e\u00b7th'", "in", "je\u00b7dem", "Jah\u00b7re", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Oder th\u00e4t's Gold vom Himmel schnei'n;", "tokens": ["O\u00b7der", "th\u00e4t's", "Gold", "vom", "Him\u00b7mel", "schnei'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "W\u00e4ren die Weiber durch die Bank", "tokens": ["W\u00e4\u00b7ren", "die", "Wei\u00b7ber", "durch", "die", "Bank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sch\u00f6n und gef\u00e4llig und ohne Wank;", "tokens": ["Sch\u00f6n", "und", "ge\u00b7f\u00e4l\u00b7lig", "und", "oh\u00b7ne", "Wank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "APPR", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "G\u00e4b's vor der Wahrheit keine Scheu,", "tokens": ["G\u00e4b's", "vor", "der", "Wahr\u00b7heit", "kei\u00b7ne", "Scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und keine Thorheit und keine Reu;", "tokens": ["Und", "kei\u00b7ne", "Thor\u00b7heit", "und", "kei\u00b7ne", "Reu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "K\u00f6nnte man f\u00fcrder ungeprellt", "tokens": ["K\u00f6nn\u00b7te", "man", "f\u00fcr\u00b7der", "un\u00b7ge\u00b7prellt"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Ueber Tag und Herz und Willen schalten,", "tokens": ["Ue\u00b7ber", "Tag", "und", "Herz", "und", "Wil\u00b7len", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "W\u00e4r's in so hochvollkomm'ner Welt", "tokens": ["W\u00e4r's", "in", "so", "hoch\u00b7voll\u00b7kom\u00b7m'\u00b7ner", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Nicht l\u00e4nger auszuhalten!", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "aus\u00b7zu\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}