{"textgrid.poem.38618": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Befreiung", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Jubelton begr\u00fc\u00df' ich Feld und Himmel,", "tokens": ["Mit", "Ju\u00b7bel\u00b7ton", "be\u00b7gr\u00fc\u00df'", "ich", "Feld", "und", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gebirg' und See", "tokens": ["Ge\u00b7bir\u00b7g'", "und", "See"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und Wies' und Hain, entronnen dem Get\u00fcmmel", "tokens": ["Und", "Wies'", "und", "Hain", ",", "ent\u00b7ron\u00b7nen", "dem", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Assemblee.", "tokens": ["Der", "As\u00b7sem\u00b7blee", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wo ich, so z\u00fcrnt' Apollon mir! geplagter", "tokens": ["Wo", "ich", ",", "so", "z\u00fcrnt'", "A\u00b7pol\u00b7lon", "mir", "!", "ge\u00b7plag\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "$,", "ADV", "VVFIN", "NE", "PPER", "$.", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Yoricks Staar,", "tokens": ["Als", "Y\u00b7o\u00b7ricks", "Staar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Im Kreise junger Stuzer und betagter", "tokens": ["Im", "Krei\u00b7se", "jun\u00b7ger", "Stu\u00b7zer", "und", "be\u00b7tag\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Koketten war.", "tokens": ["Ko\u00b7ket\u00b7ten", "war", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dort w\u00f6lkt sich d\u00fcnstend, bei des F\u00e4chers Wehen,", "tokens": ["Dort", "w\u00f6lkt", "sich", "d\u00fcns\u00b7tend", ",", "bei", "des", "F\u00e4\u00b7chers", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Pomadenduft;", "tokens": ["Po\u00b7ma\u00b7den\u00b7duft", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hier str\u00f6mt der Hauch bebl\u00fcteter Alleen", "tokens": ["Hier", "str\u00f6mt", "der", "Hauch", "be\u00b7bl\u00fc\u00b7te\u00b7ter", "Al\u00b7leen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In reiner Luft.", "tokens": ["In", "rei\u00b7ner", "Luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Die Kunst erschuf dort ganze Blumenbeete", "tokens": ["Die", "Kunst", "er\u00b7schuf", "dort", "gan\u00b7ze", "Blu\u00b7men\u00b7bee\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Seid' und Flor;", "tokens": ["Aus", "Seid'", "und", "Flor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hier hebt der Mohn, in frischer Jugendr\u00f6the,", "tokens": ["Hier", "hebt", "der", "Mohn", ",", "in", "fri\u00b7scher", "Ju\u00b7gend\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Haupt empor.", "tokens": ["Sein", "Haupt", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Dort f\u00e4rbt Karmin die l\u00e4ngstverblichne Wange", "tokens": ["Dort", "f\u00e4rbt", "Kar\u00b7min", "die", "l\u00e4ngst\u00b7ver\u00b7blich\u00b7ne", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gn\u00e4dgen Frau;", "tokens": ["Der", "gn\u00e4d\u00b7gen", "Frau", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hier r\u00f6thet sich beim Sonnenuntergange", "tokens": ["Hier", "r\u00f6\u00b7thet", "sich", "beim", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Himmels Blau.", "tokens": ["Des", "Him\u00b7mels", "Blau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Vom schwarzen Fittig thaut der Langenweile", "tokens": ["Vom", "schwar\u00b7zen", "Fit\u00b7tig", "thaut", "der", "Lan\u00b7gen\u00b7wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort Schl\u00e4frigkeit;", "tokens": ["Dort", "Schl\u00e4f\u00b7rig\u00b7keit", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hier fleucht, befl\u00fcgelt mit des Sturmwinds Eile,", "tokens": ["Hier", "fleucht", ",", "be\u00b7fl\u00fc\u00b7gelt", "mit", "des", "Sturm\u00b7winds", "Ei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die goldne Zeit.", "tokens": ["Die", "gold\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Hier, Freiheit, bl\u00fcht dein m\u00fctterlicher Boden,", "tokens": ["Hier", ",", "Frei\u00b7heit", ",", "bl\u00fcht", "dein", "m\u00fct\u00b7ter\u00b7li\u00b7cher", "Bo\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier weilest du!", "tokens": ["Hier", "wei\u00b7lest", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hier wohnt Zufriedenheit! hier weht der Oden", "tokens": ["Hier", "wohnt", "Zu\u00b7frie\u00b7den\u00b7heit", "!", "hier", "weht", "der", "O\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Seelenruh!", "tokens": ["Der", "See\u00b7len\u00b7ruh", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Hier tr\u00e4uft ein steter Himmelsthau von Freuden", "tokens": ["Hier", "tr\u00e4uft", "ein", "ste\u00b7ter", "Him\u00b7mel\u00b7sthau", "von", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Hain und Flur!", "tokens": ["Auf", "Hain", "und", "Flur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So lang' ich bin, soll nichts von dir mich scheiden,", "tokens": ["So", "lang'", "ich", "bin", ",", "soll", "nichts", "von", "dir", "mich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VAFIN", "$,", "VMFIN", "PIS", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Natur! Natur!", "tokens": ["Na\u00b7tur", "!", "Na\u00b7tur", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}