ab viel Ritter, Grafen,", "tokens": ["Ich", "hab", "viel", "Rit\u00b7ter", ",", "Gra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meines Vaters Land;", "tokens": ["In", "mei\u00b7nes", "Va\u00b7ters", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch sind viel Ritter, Grafen,", "tokens": ["Auch", "sind", "viel", "Rit\u00b7ter", ",", "Gra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An meines Vaters Hof,", "tokens": ["An", "mei\u00b7nes", "Va\u00b7ters", "Hof", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was ich nicht lernet hab,", "tokens": ["Was", "ich", "nicht", "ler\u00b7net", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das lern' ich heute noch.\u00ab", "tokens": ["Das", "lern'", "ich", "heu\u00b7te", "noch", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er nahm ihn in der Mitte,", "tokens": ["Er", "nahm", "ihn", "in", "der", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da er am schw\u00e4chsten war,", "tokens": ["Da", "er", "am", "schw\u00e4chs\u00b7ten", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schwang ihn dann zur\u00fccke,", "tokens": ["Und", "schwang", "ihn", "dann", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl in das gr\u00fcne Gras.", "tokens": ["Wohl", "in", "das", "gr\u00fc\u00b7ne", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbnun sage mir, viel Junger,", "tokens": ["\u00bb", "nun", "sa\u00b7ge", "mir", ",", "viel", "Jun\u00b7ger", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Beichtvater will ich seyn,", "tokens": ["Dein", "Beicht\u00b7va\u00b7ter", "will", "ich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bist du ein junger Wolfinger,", "tokens": ["Bist", "du", "ein", "jun\u00b7ger", "Wol\u00b7fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Von mir sollt du genesen.", "tokens": ["Von", "mir", "sollt", "du", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER