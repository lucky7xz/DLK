{"textgrid.poem.37849": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Lied vom alten Hildebrandt", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich will zu Land ausreiten,\u00ab", "tokens": ["\u00bb", "ich", "will", "zu", "Land", "aus\u00b7rei\u00b7ten", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "APPR", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach Meister Hildebrandt,", "tokens": ["Sprach", "Meis\u00b7ter", "Hil\u00b7de\u00b7brandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwer wird die Weg mir weisen", "tokens": ["\u00bb", "wer", "wird", "die", "Weg", "mir", "wei\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gen Bern wohl in das Land?", "tokens": ["Gen", "Bern", "wohl", "in", "das", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Unkund sind sie geworden", "tokens": ["Un\u00b7kund", "sind", "sie", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir manchen lieben Tag,", "tokens": ["Mir", "man\u00b7chen", "lie\u00b7ben", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In zwey und drey\u00dfig Jahren", "tokens": ["In", "zwey", "und", "drey\u00b7\u00dfig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "KON", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Frau Utten ich nicht sah.\u00ab", "tokens": ["Frau", "Ut\u00b7ten", "ich", "nicht", "sah", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbwillt du zu Land ausreiten,\u00ab", "tokens": ["\u00bb", "willt", "du", "zu", "Land", "aus\u00b7rei\u00b7ten", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "APPR", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach Herzog Amelung,", "tokens": ["Sprach", "Her\u00b7zog", "A\u00b7me\u00b7lung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00bbwas begegnet dir auf der Heiden?", "tokens": ["\u00bb", "was", "be\u00b7geg\u00b7net", "dir", "auf", "der", "Hei\u00b7den", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ein stolzer Degen jung.", "tokens": ["Ein", "stol\u00b7zer", "De\u00b7gen", "jung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was begegnet dir in der Marke?", "tokens": ["Was", "be\u00b7geg\u00b7net", "dir", "in", "der", "Mar\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Der junge Hildebrandt,", "tokens": ["Der", "jun\u00b7ge", "Hil\u00b7de\u00b7brandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ja rittest du selb zw\u00f6lfe,", "tokens": ["Ja", "rit\u00b7test", "du", "selb", "zw\u00f6l\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von ihm w\u00fcrdst angerannt.\u00ab", "tokens": ["Von", "ihm", "w\u00fcrdst", "an\u00b7ge\u00b7rannt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbund rennet er mich an,", "tokens": ["\u00bb", "und", "ren\u00b7net", "er", "mich", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In seinem Uebermuth,", "tokens": ["In", "sei\u00b7nem", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zerhau ich seinen gr\u00fcnen Schild,", "tokens": ["Zer\u00b7hau", "ich", "sei\u00b7nen", "gr\u00fc\u00b7nen", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das thut ihm nimmer gut,", "tokens": ["Das", "thut", "ihm", "nim\u00b7mer", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zerhau ihm seine Bande,", "tokens": ["Zer\u00b7hau", "ihm", "sei\u00b7ne", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit einem Schriemenschlag,", "tokens": ["Mit", "ei\u00b7nem", "Schrie\u00b7mensc\u00b7hlag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df er's ein ganzes Jahr", "tokens": ["Da\u00df", "er's", "ein", "gan\u00b7zes", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Mutter klagen mag.\u00ab", "tokens": ["Der", "Mut\u00b7ter", "kla\u00b7gen", "mag.", "\u00ab"], "token_info": ["word", "word", "word", "abbreviation", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "\u00bbund das sollt du nicht thun!\u00ab", "tokens": ["\u00bb", "und", "das", "sollt", "du", "nicht", "thun", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Herr Dieterich wohl spricht,", "tokens": ["Herr", "Die\u00b7te\u00b7rich", "wohl", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdenn dieser junge Hildebrandt", "tokens": ["\u00bb", "denn", "die\u00b7ser", "jun\u00b7ge", "Hil\u00b7de\u00b7brandt"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist mir von Herzen lieb.", "tokens": ["Ist", "mir", "von", "Her\u00b7zen", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu ihm sollst freundlich sprechen,", "tokens": ["Zu", "ihm", "sollst", "freund\u00b7lich", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohl durch den Willen mein,", "tokens": ["Wohl", "durch", "den", "Wil\u00b7len", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df er dich lasse reiten,", "tokens": ["Da\u00df", "er", "dich", "las\u00b7se", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So lieb ich ihm mag seyn.\u00ab", "tokens": ["So", "lieb", "ich", "ihm", "mag", "seyn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VMFIN", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da er zum Rosengarten reit,", "tokens": ["Da", "er", "zum", "Ro\u00b7sen\u00b7gar\u00b7ten", "reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl in der Berner Mark,", "tokens": ["Wohl", "in", "der", "Ber\u00b7ner", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kam in viel Arbeit;", "tokens": ["Er", "kam", "in", "viel", "Ar\u00b7beit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Von einem Helden stark,", "tokens": ["Von", "ei\u00b7nem", "Hel\u00b7den", "stark", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von einem Helden jung,", "tokens": ["Von", "ei\u00b7nem", "Hel\u00b7den", "jung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ward er da angerannt.", "tokens": ["Ward", "er", "da", "an\u00b7ge\u00b7rannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbnun sage mir, viel Alter,", "tokens": ["\u00bb", "nun", "sa\u00b7ge", "mir", ",", "viel", "Al\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was suchst in Vaters Land?", "tokens": ["Was", "suchst", "in", "Va\u00b7ters", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du f\u00fchrst den Harnisch eben,", "tokens": ["Du", "f\u00fchrst", "den", "Har\u00b7nisch", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie eines K\u00f6nigs Kind,", "tokens": ["Wie", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du machst mich jungen Helden", "tokens": ["Du", "machst", "mich", "jun\u00b7gen", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit sehnden Augen blind;", "tokens": ["Mit", "sehn\u00b7den", "Au\u00b7gen", "blind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du sollst daheime bleiben,", "tokens": ["Du", "sollst", "da\u00b7hei\u00b7me", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Beym guten Hausgemach,", "tokens": ["Beym", "gu\u00b7ten", "Haus\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bey einer hei\u00dfen Glute.\u00ab", "tokens": ["Bey", "ei\u00b7ner", "hei\u00b7\u00dfen", "Glu\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Alte lacht und sprach:", "tokens": ["Der", "Al\u00b7te", "lacht", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbsollt ich daheime bleiben", "tokens": ["\u00bb", "sollt", "ich", "da\u00b7hei\u00b7me", "blei\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey gutem Hausgemach?", "tokens": ["Bey", "gu\u00b7tem", "Haus\u00b7ge\u00b7mach", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin in allen Tagen", "tokens": ["Ich", "bin", "in", "al\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu reisen aufgesezt,", "tokens": ["Zu", "rei\u00b7sen", "auf\u00b7ge\u00b7sezt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu reisen und zu fechten", "tokens": ["Zu", "rei\u00b7sen", "und", "zu", "fech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bis auf mein Heimefahrt;", "tokens": ["Bis", "auf", "mein", "Hei\u00b7me\u00b7fahrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das sag ich dir, viel Junger,", "tokens": ["Das", "sag", "ich", "dir", ",", "viel", "Jun\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Drauf grauet mir der Bart.\u00ab", "tokens": ["Drauf", "grau\u00b7et", "mir", "der", "Bart", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbdein Bart will ich ausraufen,", "tokens": ["\u00bb", "dein", "Bart", "will", "ich", "aus\u00b7rau\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das sag ich, alter Mann,", "tokens": ["Das", "sag", "ich", ",", "al\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df dir dein rosenfarbnes Blut", "tokens": ["Da\u00df", "dir", "dein", "ro\u00b7sen\u00b7farb\u00b7nes", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wangen \u00fcberl\u00e4uft;", "tokens": ["Die", "Wan\u00b7gen", "\u00fc\u00b7berl\u00b7\u00e4uft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein Harnisch und dein gr\u00fcnes Schild", "tokens": ["Dein", "Har\u00b7nisch", "und", "dein", "gr\u00fc\u00b7nes", "Schild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00dft du mir hierauf geben,", "tokens": ["Mu\u00dft", "du", "mir", "hier\u00b7auf", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PAV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Dazu auch mein Gefangner seyn,", "tokens": ["Da\u00b7zu", "auch", "mein", "Ge\u00b7fang\u00b7ner", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Willt du behalten Leben.\u00ab", "tokens": ["Willt", "du", "be\u00b7hal\u00b7ten", "Le\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbmein Harnisch und mein gr\u00fcnes Schild", "tokens": ["\u00bb", "mein", "Har\u00b7nisch", "und", "mein", "gr\u00fc\u00b7nes", "Schild"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich haben oft ern\u00e4hrt;", "tokens": ["Mich", "ha\u00b7ben", "oft", "er\u00b7n\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich traue Christ vom Himmel wohl,", "tokens": ["Ich", "trau\u00b7e", "Christ", "vom", "Him\u00b7mel", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will mich deiner wehren!\u00ab", "tokens": ["Ich", "will", "mich", "dei\u00b7ner", "weh\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie lie\u00dfen von den Worten,", "tokens": ["Sie", "lie\u00b7\u00dfen", "von", "den", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und zogen scharfe Schwerdt,", "tokens": ["Und", "zo\u00b7gen", "schar\u00b7fe", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was diese zwey begehrten,", "tokens": ["Was", "die\u00b7se", "zwey", "be\u00b7gehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "CARD", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des wurden sie gew\u00e4hrt.", "tokens": ["Des", "wur\u00b7den", "sie", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ich wei\u00df nicht, wie der Junge", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "der", "Jun\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Alten gab ein'n Schlag,", "tokens": ["Dem", "Al\u00b7ten", "gab", "ein'n", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "De\u00df sich der alte Hildebrandt", "tokens": ["De\u00df", "sich", "der", "al\u00b7te", "Hil\u00b7de\u00b7brandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Herzen sehr erschrack,", "tokens": ["Von", "Her\u00b7zen", "sehr", "er\u00b7schrack", ","], "token_info"