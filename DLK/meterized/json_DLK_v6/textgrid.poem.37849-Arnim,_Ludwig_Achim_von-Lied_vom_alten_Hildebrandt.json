 "measure": "iambic.tri"}, "line.8": {"text": "Du junger Helde k\u00fchn.\u00ab", "tokens": ["Du", "jun\u00b7ger", "Hel\u00b7de", "k\u00fchn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "\u00bbdu sagst mir viel von W\u00f6lfen,", "tokens": ["\u00bb", "du", "sagst", "mir", "viel", "von", "W\u00f6l\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die laufen in das Holz,", "tokens": ["Die", "lau\u00b7fen", "in", "das", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin ein edler Degen", "tokens": ["Ich", "bin", "ein", "ed\u00b7ler", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus deutschem Lande stolz.", "tokens": ["Aus", "deut\u00b7schem", "Lan\u00b7de", "stolz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Mutter hei\u00dft Frau Utte,", "tokens": ["Mein", "Mut\u00b7ter", "hei\u00dft", "Frau", "Ut\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die edle Herzogin,", "tokens": ["Die", "ed\u00b7le", "Her\u00b7zo\u00b7gin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "l