{"dta.poem.2590": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. G. E. v. S. u. J. M. C. v. B. Hoch-  \n Adel. Hochzeit-Fest. 22. Oct. 1680.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Adonis sah' das Jahr mit Frucht barkeit erf\u00fcllt.", "tokens": ["A\u00b7do\u00b7nis", "sah'", "das", "Jahr", "mit", "Frucht", "bar\u00b7keit", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie da ein reicher Herbst mit Purpur-Trauben prangte/", "tokens": ["Wie", "da", "ein", "rei\u00b7cher", "Herbst", "mit", "Pur\u00b7pur\u00b7Trau\u00b7ben", "prang\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJD", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Pomona Sch\u00fcsseln dort voll sch\u00f6ner Aepffel langte/", "tokens": ["Po\u00b7mo\u00b7na", "Sch\u00fcs\u00b7seln", "dort", "voll", "sch\u00f6\u00b7ner", "A\u00b7e\u00b7pf\u00b7fel", "lang\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADJD", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und wie die Quitten sich in Woll\u2019 und Gold geh\u00fcllt.", "tokens": ["Und", "wie", "die", "Quit\u00b7ten", "sich", "in", "Woll'", "und", "Gold", "ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "APPR", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Mispeln dicke Reyh stund zeitig nur zum brechen/", "tokens": ["Der", "Mis\u00b7peln", "di\u00b7cke", "Reyh", "stund", "zei\u00b7tig", "nur", "zum", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ADJD", "ADV", "APPRART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Fr\u00fcchte wolten fast der Zweige Demuth schw\u00e4chen.", "tokens": ["Die", "Fr\u00fcch\u00b7te", "wol\u00b7ten", "fast", "der", "Zwei\u00b7ge", "De\u00b7muth", "schw\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er rief au\u00df frohem Muth: O mehr als g\u00fcldne Zeit!", "tokens": ["Er", "rief", "au\u00df", "fro\u00b7hem", "Muth", ":", "O", "mehr", "als", "g\u00fcld\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "NE", "PIAT", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schenckt uns der Garten-Mann Priapus volle Schalen?", "tokens": ["Schenckt", "uns", "der", "Gar\u00b7ten\u00b7Mann", "Pria\u00b7pus", "vol\u00b7le", "Scha\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Und reicht der Bachus uns den Nectar-Safft zu mahlen?", "tokens": ["Und", "reicht", "der", "Ba\u00b7chus", "uns", "den", "Nec\u00b7ta\u00b7r\u00b7S\u00b7afft", "zu", "mah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Kr\u00f6nt Wachsthum so das Jahr/ ist Vieh und Mensch erfreut?", "tokens": ["Kr\u00f6nt", "Wach\u00b7sthum", "so", "das", "Jahr", "/", "ist", "Vieh", "und", "Mensch", "er\u00b7freut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "ART", "NN", "$(", "VAFIN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hat so manch hohes Wild Diana schon gef\u00e4llet", "tokens": ["Hat", "so", "manch", "ho\u00b7hes", "Wild", "Di\u00b7a\u00b7na", "schon", "ge\u00b7f\u00e4l\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "ADJA", "NE", "NE", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und zu der Tafeln Zier den V\u00f6geln nachgestellet?", "tokens": ["Und", "zu", "der", "Ta\u00b7feln", "Zier", "den", "V\u00f6\u00b7geln", "nach\u00b7ge\u00b7stel\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gewehrt die Nais uns darzu ihr Schuppen-Heer/", "tokens": ["Ge\u00b7wehrt", "die", "Nais", "uns", "dar\u00b7zu", "ihr", "Schup\u00b7pen\u00b7Heer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "PPER", "PAV", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und machen G\u00f6tter uns so angenehme Stunden/", "tokens": ["Und", "ma\u00b7chen", "G\u00f6t\u00b7ter", "uns", "so", "an\u00b7ge\u00b7neh\u00b7me", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So hat gewi\u00df bey mir sich eine Regung funden/", "tokens": ["So", "hat", "ge\u00b7wi\u00df", "bey", "mir", "sich", "ei\u00b7ne", "Re\u00b7gung", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PPER", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die ich nicht nennen kan; mein Hertz wallt als ein Meer/", "tokens": ["Die", "ich", "nicht", "nen\u00b7nen", "kan", ";", "mein", "Hertz", "wallt", "als", "ein", "Meer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VMFIN", "$.", "PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Es kocht das heisse Blut/ der Adern Quellen sieden/", "tokens": ["Es", "kocht", "das", "heis\u00b7se", "Blut", "/", "der", "A\u00b7dern", "Quel\u00b7len", "sie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und Marck und Beine gl\u00fchn wie Eisen in den Schmiden.", "tokens": ["Und", "Marck", "und", "Bei\u00b7ne", "gl\u00fchn", "wie", "Ei\u00b7sen", "in", "den", "Schmi\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "KOKOM", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wer hilfft mir? Venus nahm Adonis in die Scho\u00df.", "tokens": ["Wer", "hilfft", "mir", "?", "Ve\u00b7nus", "nahm", "A\u00b7do\u00b7nis", "in", "die", "Scho\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "VVFIN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Leander der entschlief in seiner Hero Armen.", "tokens": ["Le\u00b7an\u00b7der", "der", "ent\u00b7schlief", "in", "sei\u00b7ner", "He\u00b7ro", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "VVFIN", "APPR", "PPOSAT", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den Paris hat erfreut der Helenen Erbarmen.", "tokens": ["Den", "Pa\u00b7ris", "hat", "er\u00b7freut", "der", "He\u00b7le\u00b7nen", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Man wei\u00df was Hercules bey Omphalen geno\u00df.", "tokens": ["Man", "wei\u00df", "was", "Her\u00b7cu\u00b7les", "bey", "Om\u00b7pha\u00b7len", "ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Soll ich Adonis seyn/ so w\u00fcnsch\u2019 ich gleiches Gl\u00fccke/", "tokens": ["Soll", "ich", "A\u00b7do\u00b7nis", "seyn", "/", "so", "w\u00fcn\u00b7sch'", "ich", "glei\u00b7ches", "Gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "VAINF", "$(", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+---+-+-+-", "measure": "unknown.measure.hexa"}, "line.24": {"text": "Da\u00df eine Venus mich in meiner Noth erquicke.", "tokens": ["Da\u00df", "ei\u00b7ne", "Ve\u00b7nus", "mich", "in", "mei\u00b7ner", "Noth", "er\u00b7qui\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wo aber such ich sie/ in einem Myrten Wald?", "tokens": ["Wo", "a\u00b7ber", "such", "ich", "sie", "/", "in", "ei\u00b7nem", "Myr\u00b7ten", "Wald", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPER", "$(", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nein/ hat ein wildes Schwein nicht den Adon zerrissen.", "tokens": ["Nein", "/", "hat", "ein", "wil\u00b7des", "Schwein", "nicht", "den", "A\u00b7don", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie/ oder find ich sie bey Crystallinen Fl\u00fcssen?", "tokens": ["Wie", "/", "o\u00b7der", "find", "ich", "sie", "bey", "Crys\u00b7tal\u00b7li\u00b7nen", "Fl\u00fcs\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KON", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Hier schreckt uns Proteus offt in he\u00dflicher Gestalt.", "tokens": ["Hier", "schreckt", "uns", "Pro\u00b7teus", "offt", "in", "he\u00df\u00b7li\u00b7cher", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Soll ich auf hohen Berg und Felsen mich umbschauen/", "tokens": ["Soll", "ich", "auf", "ho\u00b7hen", "Berg", "und", "Fel\u00b7sen", "mich", "umbsc\u00b7hau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "KON", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da find ich nichts als Stein als bange Furcht und Grauen.", "tokens": ["Da", "find", "ich", "nichts", "als", "Stein", "als", "ban\u00b7ge", "Furcht", "und", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "KOKOM", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Erwehl\u2019 ich denn das Feld? die Gegend lacht mich an/", "tokens": ["Er\u00b7wehl'", "ich", "denn", "das", "Feld", "?", "die", "Ge\u00b7gend", "lacht", "mich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Vulcanus aber hat/ der alles nur verzehret/", "tokens": ["Vul\u00b7ca\u00b7nus", "a\u00b7ber", "hat", "/", "der", "al\u00b7les", "nur", "ver\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "$(", "ART", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Zweymal die Sch\u00e4ferey in Asch nnd ", "tokens": ["Zwey\u00b7mal", "die", "Sch\u00e4\u00b7fe\u00b7rey", "in", "Asch", "nnd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NE", "KON"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Ich zweiffle/ da\u00df ich da die Nymfe finden kan/", "tokens": ["Ich", "zweiff\u00b7le", "/", "da\u00df", "ich", "da", "die", "Nym\u00b7fe", "fin\u00b7den", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So mich vergn\u00fcgen wird/ doch ich will mich bem\u00fchen/", "tokens": ["So", "mich", "ver\u00b7gn\u00fc\u00b7gen", "wird", "/", "doch", "ich", "will", "mich", "be\u00b7m\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VAFIN", "$(", "KON", "PPER", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Pflegt doch der Rosen Zier auf Heck und Dorn zu bl\u00fchen.", "tokens": ["Pflegt", "doch", "der", "Ro\u00b7sen", "Zier", "auf", "Heck", "und", "Dorn", "zu", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "APPR", "NE", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er sucht/ weil Echo nach den holen Seufftzer spricht.", "tokens": ["Er", "sucht", "/", "weil", "E\u00b7cho", "nach", "den", "ho\u00b7len", "Seufft\u00b7zer", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Er eilt begierig fort bi\u00df da\u00df er im Gefilde/", "tokens": ["Er", "eilt", "be\u00b7gie\u00b7rig", "fort", "bi\u00df", "da\u00df", "er", "im", "Ge\u00b7fil\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "KON", "KOUS", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Die Nymfe hat erblickt/ so einem G\u00f6tter-Bilde", "tokens": ["Die", "Nym\u00b7fe", "hat", "er\u00b7blickt", "/", "so", "ei\u00b7nem", "G\u00f6t\u00b7ter\u00b7Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Vollkommen \u00e4hnlich war/ der Augen Sonnenlicht", "tokens": ["Voll\u00b7kom\u00b7men", "\u00e4hn\u00b7lich", "war", "/", "der", "Au\u00b7gen", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "VAFIN", "$(", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Warff tausend Strahlen au\u00df/ als wie wenn Feuer-Ballen", "tokens": ["Warff", "tau\u00b7send", "Strah\u00b7len", "au\u00df", "/", "als", "wie", 