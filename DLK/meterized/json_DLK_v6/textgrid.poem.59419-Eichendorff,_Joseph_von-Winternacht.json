{"textgrid.poem.59419": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Winternacht", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschneit liegt rings die ganze Welt,", "tokens": ["Ver\u00b7schneit", "liegt", "rings", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab nichts, was mich freuet,", "tokens": ["Ich", "hab", "nichts", ",", "was", "mich", "freu\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verlassen steht der Baum im Feld,", "tokens": ["Ver\u00b7las\u00b7sen", "steht", "der", "Baum", "im", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat l\u00e4ngst sein Laub verstreuet.", "tokens": ["Hat", "l\u00e4ngst", "sein", "Laub", "ver\u00b7streu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Wind nur geht bei stiller Nacht", "tokens": ["Der", "Wind", "nur", "geht", "bei", "stil\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und r\u00fcttelt an dem Baume,", "tokens": ["Und", "r\u00fct\u00b7telt", "an", "dem", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da r\u00fchrt er seinen Wipfel sacht", "tokens": ["Da", "r\u00fchrt", "er", "sei\u00b7nen", "Wip\u00b7fel", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und redet wie im Traume.", "tokens": ["Und", "re\u00b7det", "wie", "im", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er tr\u00e4umt von k\u00fcnft'ger Fr\u00fchlingszeit,", "tokens": ["Er", "tr\u00e4umt", "von", "k\u00fcnft'\u00b7ger", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gr\u00fcn und Quellenrauschen,", "tokens": ["Von", "Gr\u00fcn", "und", "Quel\u00b7len\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo er im neuen Bl\u00fctenkleid", "tokens": ["Wo", "er", "im", "neu\u00b7en", "Bl\u00fc\u00b7ten\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Gottes Lob wird rauschen.", "tokens": ["Zu", "Got\u00b7tes", "Lob", "wird", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}