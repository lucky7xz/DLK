{"textgrid.poem.45788": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Historiker", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Geschichtschreiber waren sonst Befangene,", "tokens": ["Die", "Ge\u00b7schicht\u00b7schrei\u00b7ber", "wa\u00b7ren", "sonst", "Be\u00b7fan\u00b7ge\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die neue Zeit gab neue Richte,", "tokens": ["Die", "neu\u00b7e", "Zeit", "gab", "neu\u00b7e", "Rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir schreiben nicht mehr die vergangene,", "tokens": ["Wir", "schrei\u00b7ben", "nicht", "mehr", "die", "ver\u00b7gan\u00b7ge\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Wir schreiben k\u00fcnftige Geschichte.", "tokens": ["Wir", "schrei\u00b7ben", "k\u00fcnf\u00b7ti\u00b7ge", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}