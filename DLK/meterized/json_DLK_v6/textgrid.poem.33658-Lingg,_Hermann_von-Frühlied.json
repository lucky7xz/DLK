{"textgrid.poem.33658": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlied", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht mir ein hohes Alter!", "tokens": ["Nicht", "mir", "ein", "ho\u00b7hes", "Al\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht mir im Abendrot", "tokens": ["Nicht", "mir", "im", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Lebens letzten Psalter,", "tokens": ["Des", "Le\u00b7bens", "letz\u00b7ten", "Psal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht mir den Greisentod!", "tokens": ["Nicht", "mir", "den", "Grei\u00b7sen\u00b7tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Blume meiner Freuden", "tokens": ["Die", "Blu\u00b7me", "mei\u00b7ner", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War irdisch ja, ich trank", "tokens": ["War", "ir\u00b7disch", "ja", ",", "ich", "trank"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "$,", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom goldnen Kelch der Heiden", "tokens": ["Vom", "gold\u00b7nen", "Kelch", "der", "Hei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und trotzte, bis ich sank.", "tokens": ["Und", "trotz\u00b7te", ",", "bis", "ich", "sank", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich war ein wilder J\u00e4ger;", "tokens": ["Ich", "war", "ein", "wil\u00b7der", "J\u00e4\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Haupt, von Schuld nicht rein,", "tokens": ["Mein", "Haupt", ",", "von", "Schuld", "nicht", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll nie ein W\u00fcrdetr\u00e4ger", "tokens": ["Soll", "nie", "ein", "W\u00fcr\u00b7de\u00b7tr\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Silberlocke sein.", "tokens": ["Der", "Sil\u00b7berl\u00b7o\u00b7cke", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr Schatten, h\u00f6rt mein Bitten:", "tokens": ["Ihr", "Schat\u00b7ten", ",", "h\u00f6rt", "mein", "Bit\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht heimlich, hinterr\u00fccks,", "tokens": ["Nicht", "heim\u00b7lich", ",", "hin\u00b7ter\u00b7r\u00fccks", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf meiner Bahn inmitten", "tokens": ["Auf", "mei\u00b7ner", "Bahn", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "St\u00fcrzt mich im Flug des Gl\u00fccks.", "tokens": ["St\u00fcrzt", "mich", "im", "Flug", "des", "Gl\u00fccks", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Noch eh' das Lied verklungen,", "tokens": ["Noch", "eh'", "das", "Lied", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eh' Lust und Leid verweht,", "tokens": ["Eh'", "Lust", "und", "Leid", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang das Schwert geschwungen", "tokens": ["So", "lang", "das", "Schwert", "ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hoch das Banner weht!", "tokens": ["Und", "hoch", "das", "Ban\u00b7ner", "weht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}