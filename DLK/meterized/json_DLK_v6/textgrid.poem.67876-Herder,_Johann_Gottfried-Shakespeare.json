{"textgrid.poem.67876": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Shakespeare", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie s\u00fc\u00df das Mondlicht auf dem H\u00fcgel schl\u00e4ft!", "tokens": ["Wie", "s\u00fc\u00df", "das", "Mond\u00b7licht", "auf", "dem", "H\u00fc\u00b7gel", "schl\u00e4ft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier woll'n wir sizen, und den s\u00fcssen Schall", "tokens": ["Hier", "woll'n", "wir", "si\u00b7zen", ",", "und", "den", "s\u00fcs\u00b7sen", "Schall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Ohre lassen schl\u00fcpfen. Sanfte Stille", "tokens": ["Zum", "Oh\u00b7re", "las\u00b7sen", "schl\u00fcp\u00b7fen", ".", "Sanf\u00b7te", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "VVINF", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Nacht wird Taste s\u00fcsser Harmonie.", "tokens": ["Und", "Nacht", "wird", "Tas\u00b7te", "s\u00fcs\u00b7ser", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Siz, Jessika, sieh, wie die Himmelsflur", "tokens": ["Siz", ",", "Jes\u00b7si\u00b7ka", ",", "sieh", ",", "wie", "die", "Him\u00b7mels\u00b7flur"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist eingelegt mit St\u00fccken reichen Goldes!", "tokens": ["Ist", "ein\u00b7ge\u00b7legt", "mit", "St\u00fc\u00b7cken", "rei\u00b7chen", "Gol\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da ist kein kleiner Kreis, den du da siehst,", "tokens": ["Da", "ist", "kein", "klei\u00b7ner", "Kreis", ",", "den", "du", "da", "siehst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der nicht in seinem Lauf wie'n Engel singt,", "tokens": ["Der", "nicht", "in", "sei\u00b7nem", "Lauf", "wie'n", "En\u00b7gel", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PPOSAT", "NN", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Stimmt ein ins Chor der jungen Cherubim.", "tokens": ["Stimmt", "ein", "ins", "Chor", "der", "jun\u00b7gen", "Che\u00b7ru\u00b7bim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPRART", "NN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Harmonie ist in den ew'gen T\u00f6nen;", "tokens": ["Die", "Har\u00b7mo\u00b7nie", "ist", "in", "den", "ew'\u00b7gen", "T\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nur wir, so lang dies Kothkleid Sterblichkeit", "tokens": ["Nur", "wir", ",", "so", "lang", "dies", "Koth\u00b7kleid", "Sterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "ADV", "ADJD", "PDS", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Uns grob einh\u00fcllet, k\u00f6nnen sie nicht h\u00f6ren. \u2013", "tokens": ["Uns", "grob", "ein\u00b7h\u00fcl\u00b7let", ",", "k\u00f6n\u00b7nen", "sie", "nicht", "h\u00f6\u00b7ren", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Mann, der nicht Musik hat in ihm selbst,", "tokens": ["Der", "Mann", ",", "der", "nicht", "Mu\u00b7sik", "hat", "in", "ihm", "selbst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "NN", "VAFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ger\u00fchrt nicht wird vom Einklang s\u00fcsser T\u00f6ne,", "tokens": ["Ge\u00b7r\u00fchrt", "nicht", "wird", "vom", "Ein\u00b7klang", "s\u00fcs\u00b7ser", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "VAFIN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu R\u00e4nken, Raub, Verrath ist der gemacht;", "tokens": ["Zu", "R\u00e4n\u00b7ken", ",", "Raub", ",", "Ver\u00b7rath", "ist", "der", "ge\u00b7macht", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "VAFIN", "ART", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Triebe seines Geistes sind wie Nacht,", "tokens": ["Die", "Trie\u00b7be", "sei\u00b7nes", "Geis\u00b7tes", "sind", "wie", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sein Herz ist schwarz, wie Erebus \u2013", "tokens": ["Sein", "Herz", "ist", "schwarz", ",", "wie", "E\u00b7re\u00b7bus", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PWAV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trau nicht dem Manne!", "tokens": ["Trau", "nicht", "dem", "Man\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}