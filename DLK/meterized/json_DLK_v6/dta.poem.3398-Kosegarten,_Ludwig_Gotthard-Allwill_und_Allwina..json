{"dta.poem.3398": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Allwill und Allwina.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schon r\u00f6thete die H&#252;gel", "tokens": ["Schon", "r\u00f6\u00b7the\u00b7te", "die", "H", "&#252;", "gel"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["ADV", "VVFIN", "ART", "XY", "$(", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sonne letzter Strahl.", "tokens": ["Der", "Son\u00b7ne", "letz\u00b7ter", "Strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es k\u00fchlten Zephyrfl\u00fcgel", "tokens": ["Es", "k\u00fchl\u00b7ten", "Ze\u00b7phyr\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ausgesengte Thal.", "tokens": ["Das", "aus\u00b7ge\u00b7seng\u00b7te", "Thal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf Blumen, Saaten, Kr\u00e4uter,", "tokens": ["Auf", "Blu\u00b7men", ",", "Saa\u00b7ten", ",", "Kr\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sank Gottes Abendthau.", "tokens": ["Sank", "Got\u00b7tes", "A\u00b7bendt\u00b7hau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Halbmond gl\u00e4nzte heiter", "tokens": ["Der", "Halb\u00b7mond", "gl\u00e4nz\u00b7te", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im wolkenlosen Blau.", "tokens": ["Im", "wol\u00b7ken\u00b7lo\u00b7sen", "Blau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}