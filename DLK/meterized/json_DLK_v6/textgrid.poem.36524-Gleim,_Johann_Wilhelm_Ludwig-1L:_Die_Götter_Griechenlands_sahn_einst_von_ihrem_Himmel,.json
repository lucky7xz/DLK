{"textgrid.poem.36524": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die G\u00f6tter Griechenlands sahn einst von ihrem Himmel,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die G\u00f6tter Griechenlands sahn einst von ihrem Himmel,", "tokens": ["Die", "G\u00f6t\u00b7ter", "Grie\u00b7chen\u00b7lands", "sahn", "einst", "von", "ih\u00b7rem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf ihre Menschenwelt, mit ", "tokens": ["Auf", "ih\u00b7re", "Men\u00b7schen\u00b7welt", ",", "mit"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auf einmal alle sahn des Lebenden Get\u00fcmmel,", "tokens": ["Auf", "ein\u00b7mal", "al\u00b7le", "sahn", "des", "Le\u00b7ben\u00b7den", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Todten stilles Grab.", "tokens": ["Des", "Tod\u00b7ten", "stil\u00b7les", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Lebt! sagte Vater Zeus, und alle Todten lebten;", "tokens": ["Lebt", "!", "sag\u00b7te", "Va\u00b7ter", "Zeus", ",", "und", "al\u00b7le", "Tod\u00b7ten", "leb\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "NN", "NE", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle G\u00f6tter sahn sie leben; freuten sich", "tokens": ["Und", "al\u00b7le", "G\u00f6t\u00b7ter", "sahn", "sie", "le\u00b7ben", ";", "freu\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "VVINF", "$.", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Worts der Allmacht sehr, und l\u00e4chelten, und schwebten", "tokens": ["Des", "Worts", "der", "All\u00b7macht", "sehr", ",", "und", "l\u00e4\u00b7chel\u00b7ten", ",", "und", "schweb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Auf ihren Wolken sichtbarlich.", "tokens": ["Auf", "ih\u00b7ren", "Wol\u00b7ken", "sicht\u00b7bar\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Frau Juno ganz allein fand wenig Wohlgefallen", "tokens": ["Frau", "Ju\u00b7no", "ganz", "al\u00b7lein", "fand", "we\u00b7nig", "Wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "ADV", "ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An ihres Herrn Gemahls fast gro\u00dfer Vaterthat,", "tokens": ["An", "ih\u00b7res", "Herrn", "Ge\u00b7mahls", "fast", "gro\u00b7\u00dfer", "Va\u00b7tert\u00b7hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie runzelte die Stirn, sie z\u00fcrnte stolz, sie bat,", "tokens": ["Sie", "run\u00b7zel\u00b7te", "die", "Stirn", ",", "sie", "z\u00fcrn\u00b7te", "stolz", ",", "sie", "bat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "(mit Bitten herrscht das Weib im Himmel und Sanct-Gallen!)", "tokens": ["(", "mit", "Bit\u00b7ten", "herrscht", "das", "Weib", "im", "Him\u00b7mel", "und", "Sanc\u00b7tGal\u00b7len", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Aufgelebten alle gleich", "tokens": ["Die", "Auf\u00b7ge\u00b7leb\u00b7ten", "al\u00b7le", "gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur\u00fcck zu senden doch ins stille Schattenreich!", "tokens": ["Zu\u00b7r\u00fcck", "zu", "sen\u00b7den", "doch", "ins", "stil\u00b7le", "Schat\u00b7ten\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "KON", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bescheiden bat sie das, und schlug die Augen nieder", "tokens": ["Be\u00b7schei\u00b7den", "bat", "sie", "das", ",", "und", "schlug", "die", "Au\u00b7gen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PDS", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hielt zum Vater Zeus ein h\u00f6rbegierig Ohr.", "tokens": ["Und", "hielt", "zum", "Va\u00b7ter", "Zeus", "ein", "h\u00f6r\u00b7be\u00b7gie\u00b7rig", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "NE", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sterbt! sagte Vater Zeus, und alle starben wieder,", "tokens": ["Sterbt", "!", "sag\u00b7te", "Va\u00b7ter", "Zeus", ",", "und", "al\u00b7le", "star\u00b7ben", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "NN", "NE", "$,", "KON", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf der Erde war's so leer, als wie zuvor.", "tokens": ["Und", "auf", "der", "Er\u00b7de", "wa\u00b7r's", "so", "leer", ",", "als", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "KOKOM", "ADV", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Mit Bitten herrscht das Weib im Himmel und Sanct-Gallen!", "tokens": ["Mit", "Bit\u00b7ten", "herrscht", "das", "Weib", "im", "Him\u00b7mel", "und", "Sanc\u00b7tGal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Himmel stiftet es, mit Bitten, Zank und Streit,", "tokens": ["Im", "Him\u00b7mel", "stif\u00b7tet", "es", ",", "mit", "Bit\u00b7ten", ",", "Zank", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und in Sanct-Gallen Einigkeit!", "tokens": ["Und", "in", "Sanc\u00b7tGal\u00b7len", "Ei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gottlob! das ist ja sch\u00f6n! Wir leben in Sanct-Gallen!", "tokens": ["Gott\u00b7lob", "!", "das", "ist", "ja", "sch\u00f6n", "!", "Wir", "le\u00b7ben", "in", "Sanc\u00b7tGal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unsre Weiber lassen sich,", "tokens": ["Und", "uns\u00b7re", "Wei\u00b7ber", "las\u00b7sen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was uns gef\u00e4llt, gefallen,", "tokens": ["Was", "uns", "ge\u00b7f\u00e4llt", ",", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die kleinen und die gro\u00dfen Schnallen,", "tokens": ["Die", "klei\u00b7nen", "und", "die", "gro\u00b7\u00dfen", "Schnal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Perlen, die Korallen,", "tokens": ["Die", "Per\u00b7len", ",", "die", "Ko\u00b7ral\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Hut \u2013 und den Gedankenstrich!", "tokens": ["Den", "Hut", "\u2013", "und", "den", "Ge\u00b7dan\u00b7ken\u00b7strich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}