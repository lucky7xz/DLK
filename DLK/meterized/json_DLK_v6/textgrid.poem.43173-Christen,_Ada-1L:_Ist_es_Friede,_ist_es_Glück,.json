{"textgrid.poem.43173": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist es Friede, ist es Gl\u00fcck,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist es Friede, ist es Gl\u00fcck,", "tokens": ["Ist", "es", "Frie\u00b7de", ",", "ist", "es", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was durch meine Tr\u00e4ume zieht,", "tokens": ["Was", "durch", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsichtbar, wie Blumenduft,", "tokens": ["Un\u00b7sicht\u00b7bar", ",", "wie", "Blu\u00b7men\u00b7duft", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise, wie ein Kindeslied?", "tokens": ["Lei\u00b7se", ",", "wie", "ein", "Kin\u00b7des\u00b7lied", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kehrt die Jugend mir zur\u00fcck,", "tokens": ["Kehrt", "die", "Ju\u00b7gend", "mir", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jene Sehnsucht, die mich mied,", "tokens": ["Je\u00b7ne", "Sehn\u00b7sucht", ",", "die", "mich", "mied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit des Lebens kalte Luft", "tokens": ["Seit", "des", "Le\u00b7bens", "kal\u00b7te", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich und meine Seele schied?", "tokens": ["Mich", "und", "mei\u00b7ne", "See\u00b7le", "schied", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}