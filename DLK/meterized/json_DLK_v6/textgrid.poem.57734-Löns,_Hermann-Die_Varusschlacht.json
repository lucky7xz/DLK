{"textgrid.poem.57734": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Varusschlacht", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herdrauch zog die Diele entlang,", "tokens": ["Der", "Her\u00b7drauch", "zog", "die", "Die\u00b7le", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Im Feuer schwelte der Torf,", "tokens": ["Im", "Feu\u00b7er", "schwel\u00b7te", "der", "Torf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und um das Feuer, da sa\u00df im Kreis", "tokens": ["Und", "um", "das", "Feu\u00b7er", ",", "da", "sa\u00df", "im", "Kreis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die ganze Jungmannschaft vom Dorf;", "tokens": ["Die", "gan\u00b7ze", "Jung\u00b7mann\u00b7schaft", "vom", "Dorf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hatten es alle vernommen,", "tokens": ["Sie", "hat\u00b7ten", "es", "al\u00b7le", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Es ging von Haus zu Haus:", "tokens": ["Es", "ging", "von", "Haus", "zu", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbw\u00fclfke ist wiedergekommen,", "tokens": ["\u00bb", "w\u00fclf\u00b7ke", "ist", "wie\u00b7der\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Er gibt heute abend eins aus!\u00ab", "tokens": ["Er", "gibt", "heu\u00b7te", "a\u00b7bend", "eins", "aus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS", "PTKVZ", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Und W\u00fclfke, der griente und legte dann los:", "tokens": ["Und", "W\u00fclf\u00b7ke", ",", "der", "grien\u00b7te", "und", "leg\u00b7te", "dann", "los", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "ADJA", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u00bbna ja, das war schon ein Spa\u00df,", "tokens": ["\u00bb", "na", "ja", ",", "das", "war", "schon", "ein", "Spa\u00df", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "ADV", "$,", "PDS", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Wenn's auch den r\u00f6mischen Kerls nicht so schien,", "tokens": ["Wenn's", "auch", "den", "r\u00f6\u00b7mi\u00b7schen", "Kerls", "nicht", "so", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn scheu\u00dflich bekam ihnen das;", "tokens": ["Denn", "scheu\u00df\u00b7lich", "be\u00b7kam", "ih\u00b7nen", "das", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PDS", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Was machten sie sich auch so mausig,", "tokens": ["Was", "mach\u00b7ten", "sie", "sich", "auch", "so", "mau\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uns ging die Geduld schlie\u00dflich aus,", "tokens": ["Uns", "ging", "die", "Ge\u00b7duld", "schlie\u00df\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Na, und da, da ging's ihnen lausig,", "tokens": ["Na", ",", "und", "da", ",", "da", "ging's", "ih\u00b7nen", "lau\u00b7sig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Kaum einer fand wieder nach Haus.", "tokens": ["Kaum", "ei\u00b7ner", "fand", "wie\u00b7der", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Na ja, wie das nun gekommen ist,", "tokens": ["Na", "ja", ",", "wie", "das", "nun", "ge\u00b7kom\u00b7men", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "PWAV", "PDS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Das wei\u00df ich nicht mehr so genau,", "tokens": ["Das", "wei\u00df", "ich", "nicht", "mehr", "so", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich trank am Abend gef\u00e4hrlich viel Bier", "tokens": ["Ich", "trank", "am", "A\u00b7bend", "ge\u00b7f\u00e4hr\u00b7lich", "viel", "Bier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "PIAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und war am Morgen noch blau;", "tokens": ["Und", "war", "am", "Mor\u00b7gen", "noch", "blau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Es sa\u00df da auf so 'nem Dinge", "tokens": ["Es", "sa\u00df", "da", "auf", "so", "'nem", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein Kerl und machte sich breit,", "tokens": ["Ein", "Kerl", "und", "mach\u00b7te", "sich", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Er hatte die Finger voll Ringe", "tokens": ["Er", "hat\u00b7te", "die", "Fin\u00b7ger", "voll", "Rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Vor lauter Hoff\u00e4rtigkeit.", "tokens": ["Vor", "lau\u00b7ter", "Hof\u00b7f\u00e4r\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Na ja, der prahlte in einem weg,", "tokens": ["Na", "ja", ",", "der", "prahl\u00b7te", "in", "ei\u00b7nem", "weg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "PRELS", "VVFIN", "APPR", "ART", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Blo\u00df da\u00df ich kein Wort von verstand,", "tokens": ["Blo\u00df", "da\u00df", "ich", "kein", "Wort", "von", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PIAT", "NN", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Mann aus der Gegend, der meinte zu mir,", "tokens": ["Ein", "Mann", "aus", "der", "Ge\u00b7gend", ",", "der", "mein\u00b7te", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er h\u00e4tt' uns Gesindel genannt;", "tokens": ["Er", "h\u00e4tt'", "uns", "Ge\u00b7sin\u00b7del", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Das wollt' mir denn doch nicht recht passen,", "tokens": ["Das", "wollt'", "mir", "denn", "doch", "nicht", "recht", "pas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor den Augen wurd's mir ganz rot,", "tokens": ["Vor", "den", "Au\u00b7gen", "wurd's", "mir", "ganz", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Ich konnt' es wahrhaftig nicht lassen", "tokens": ["Ich", "konnt'", "es", "wahr\u00b7haf\u00b7tig", "nicht", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "PTKNEG", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und b\u00f6lkte wie unklug: schlah' dot!", "tokens": ["Und", "b\u00f6lk\u00b7te", "wie", "un\u00b7klug", ":", "schlah'", "dot", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ADJD", "$.", "VVFIN", "NE", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Na ja, da ging die Geschichte denn los,", "tokens": ["Na", "ja", ",", "da", "ging", "die", "Ge\u00b7schich\u00b7te", "denn", "los", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Mir wurde davon erst ganz dumm;", "tokens": ["Mir", "wur\u00b7de", "da\u00b7von", "erst", "ganz", "dumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohin ich auch sah, fiel ein r\u00f6mischer Kerl,", "tokens": ["Wo\u00b7hin", "ich", "auch", "sah", ",", "fiel", "ein", "r\u00f6\u00b7mi\u00b7scher", "Kerl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie'n Schwein beim Schlachtefest um;", "tokens": ["Wie'n", "Schwein", "beim", "Schlach\u00b7te\u00b7fest", "um", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.5": {"text": "Da spuckt' ich denn auch in die H\u00e4nde", "tokens": ["Da", "spuckt'", "ich", "denn", "auch", "in", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und kriegte mein Messer her,", "tokens": ["Und", "krieg\u00b7te", "mein", "Mes\u00b7ser", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ein ganzes langes Ende", "tokens": ["Ein", "gan\u00b7zes", "lan\u00b7ges", "En\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wei\u00df ich nun aber gar nichts mehr.", "tokens": ["Wei\u00df", "ich", "nun", "a\u00b7ber", "gar", "nichts", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Na ja, denn \u00fcber- und \u00fcberall", "tokens": ["Na", "ja", ",", "denn", "\u00fc\u00b7ber", "und", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "$,", "KON", "TRUNC", "KON", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da ging's: schlah' dot, schlah' dot!", "tokens": ["Da", "ging's", ":", "schlah'", "dot", ",", "schlah'", "dot", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "VVFIN", "NE", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und als ich wieder zu mir kam,", "tokens": ["Und", "als", "ich", "wie\u00b7der", "zu", "mir", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Arm war bis obenhin rot;", "tokens": ["Mein", "Arm", "war", "bis", "o\u00b7ben\u00b7hin", "rot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Zum Kuckuck war mein Messer,", "tokens": ["Zum", "Ku\u00b7ckuck", "war", "mein", "Mes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da langte ich mir ein Schwert,", "tokens": ["Da", "lang\u00b7te", "ich", "mir", "ein", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das flutschte denn doch noch besser;", "tokens": ["Das", "flutschte", "denn", "doch", "noch", "bes\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es h\u00e4ngt da hinter dem Herd.", "tokens": ["Es", "h\u00e4ngt", "da", "hin\u00b7ter", "dem", "Herd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Na ja, so ging's denn in einem fort,", "tokens": ["Na", "ja", ",", "so", "ging's", "denn", "in", "ei\u00b7nem", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ADV", "VVFIN", "ADV", "APPR", "ART", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir brachten sie sch\u00f6n auf den Trab,", "tokens": ["Wir", "brach\u00b7ten", "sie", "sch\u00f6n", "auf", "den", "Trab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich habe die Nase noch voll von dem Kerl,", "tokens": ["Ich", "ha\u00b7be", "die", "Na\u00b7se", "noch", "voll", "von", "dem", "Kerl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dem ich den K\u00e4lberfang gab;", "tokens": ["Dem", "ich", "den", "K\u00e4l\u00b7ber\u00b7fang", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Er tat man so blitzen und blinken", "tokens": ["Er", "tat", "man", "so", "bl