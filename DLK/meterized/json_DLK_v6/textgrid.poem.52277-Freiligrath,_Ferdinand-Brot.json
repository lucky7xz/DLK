{"textgrid.poem.52277": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Brot", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn am Gestad' und in den L\u00fcften", "tokens": ["Wenn", "am", "Ge\u00b7stad'", "und", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich keine M\u00fchle mehr bewegt;", "tokens": ["Sich", "kei\u00b7ne", "M\u00fch\u00b7le", "mehr", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn, m\u00fc\u00dfig weidend auf den Triften,", "tokens": ["Wenn", ",", "m\u00fc\u00b7\u00dfig", "wei\u00b7dend", "auf", "den", "Trif\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Esel keinen Sack mehr tr\u00e4gt:", "tokens": ["Der", "E\u00b7sel", "kei\u00b7nen", "Sack", "mehr", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann, wie ein Volk am hellen Tage", "tokens": ["Dann", ",", "wie", "ein", "Volk", "am", "hel\u00b7len", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "K\u00fchn tritt der Hunger in das Haus;", "tokens": ["K\u00fchn", "tritt", "der", "Hun\u00b7ger", "in", "das", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Wetter r\u00fcstet sich zum Schlage,", "tokens": ["Ein", "Wet\u00b7ter", "r\u00fcs\u00b7tet", "sich", "zum", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und durch die Luft geht ein Gebraus:", "tokens": ["Und", "durch", "die", "Luft", "geht", "ein", "Ge\u00b7braus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr d\u00e4mpft den Zornruf, o Despoten,", "tokens": ["Ihr", "d\u00e4mpft", "den", "Zorn\u00b7ruf", ",", "o", "Des\u00b7po\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Volkes nicht, das hungernd droht!", "tokens": ["Des", "Vol\u00b7kes", "nicht", ",", "das", "hun\u00b7gernd", "droht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Denn die Natur hat ihn geboten,", "tokens": ["Denn", "die", "Na\u00b7tur", "hat", "ihn", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den Schrei: Brot! Brot! Brot tut uns not!", "tokens": ["Den", "Schrei", ":", "Brot", "!", "Brot", "!", "Brot", "tut", "uns", "not", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "$.", "NN", "$.", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Hunger kommt vom Dorf gegangen,", "tokens": ["Der", "Hun\u00b7ger", "kommt", "vom", "Dorf", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Einzieht er durch der St\u00e4dte Tor;", "tokens": ["Ein\u00b7zieht", "er", "durch", "der", "St\u00e4d\u00b7te", "Tor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So haltet ihm doch eure Stangen", "tokens": ["So", "hal\u00b7tet", "ihm", "doch", "eu\u00b7re", "Stan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und eure Trommelst\u00f6cke vor!", "tokens": ["Und", "eu\u00b7re", "Trom\u00b7mel\u00b7st\u00f6\u00b7cke", "vor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz Pulver und Kart\u00e4tschenschauer", "tokens": ["Trotz", "Pul\u00b7ver", "und", "Kar\u00b7t\u00e4t\u00b7schen\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Rasch wie ein Vogel ist sein Lauf,", "tokens": ["Rasch", "wie", "ein", "Vo\u00b7gel", "ist", "sein", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und auf der allerh\u00f6chsten Mauer", "tokens": ["Und", "auf", "der", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Pflanzt er sein schwarzes Banner auf.", "tokens": ["Pflanzt", "er", "sein", "schwar\u00b7zes", "Ban\u00b7ner", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Ihr d\u00e4mpft den Zornruf, o Despoten,", "tokens": ["Ihr", "d\u00e4mpft", "den", "Zorn\u00b7ruf", ",", "o", "Des\u00b7po\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Volkes nicht, das hungernd droht!", "tokens": ["Des", "Vol\u00b7kes", "nicht", ",", "das", "hun\u00b7gernd", "droht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Denn die Natur hat ihn geboten,", "tokens": ["Denn", "die", "Na\u00b7tur", "hat", "ihn", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den Schrei: Brot! Brot! Brot tut uns not!", "tokens": ["Den", "Schrei", ":", "Brot", "!", "Brot", "!", "Brot", "tut", "uns", "not", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "$.", "NN", "$.", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00dft eure S\u00f6ldnerhaufen kommen", "tokens": ["La\u00dft", "eu\u00b7re", "S\u00f6ld\u00b7ner\u00b7hau\u00b7fen", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In gleichem Schritt, mit gleicher Wehr!", "tokens": ["In", "glei\u00b7chem", "Schritt", ",", "mit", "glei\u00b7cher", "Wehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Scheuer und der Flur genommen,", "tokens": ["Der", "Scheu\u00b7er", "und", "der", "Flur", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat Waffen auch des Hungers Heer;", "tokens": ["Hat", "Waf\u00b7fen", "auch", "des", "Hun\u00b7gers", "Heer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es rei\u00dft die Schaufel aus der Scholle,", "tokens": ["Es", "rei\u00dft", "die", "Schau\u00b7fel", "aus", "der", "Schol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sense rei\u00dft es aus dem Korn;", "tokens": ["Die", "Sen\u00b7se", "rei\u00dft", "es", "aus", "dem", "Korn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sogar des M\u00e4dchens Brust, die volle,", "tokens": ["So\u00b7gar", "des", "M\u00e4d\u00b7chens", "Brust", ",", "die", "vol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr d\u00e4mpft den Zornruf, o Despoten,", "tokens": ["Ihr", "d\u00e4mpft", "den", "Zorn\u00b7ruf", ",", "o", "Des\u00b7po\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Des Volkes nicht, das hungernd droht!", "tokens": ["Des", "Vol\u00b7kes", "nicht", ",", "das", "hun\u00b7gernd", "droht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Denn die Natur hat ihn geboten,", "tokens": ["Denn", "die", "Na\u00b7tur", "hat", "ihn", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Den Schrei: Brot! Brot! Brot tut uns not!", "tokens": ["Den", "Schrei", ":", "Brot", "!", "Brot", "!", "Brot", "tut", "uns", "not", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "$.", "NN", "$.", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Packt, in des Volkes mut'gen Reihen,", "tokens": ["Packt", ",", "in", "des", "Vol\u00b7kes", "mut'\u00b7gen", "Rei\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wer Sichel oder Flinte tr\u00e4gt!", "tokens": ["Wer", "Si\u00b7chel", "o\u00b7der", "Flin\u00b7te", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00dft immer das Ger\u00fcst uns dr\u00e4uen,", "tokens": ["La\u00dft", "im\u00b7mer", "das", "Ge\u00b7r\u00fcst", "uns", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dem das Beil den Kopf abschl\u00e4gt!", "tokens": ["Auf", "dem", "das", "Beil", "den", "Kopf", "ab\u00b7schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat es, in finstrer Schauer Mitten,", "tokens": ["Hat", "es", ",", "in", "finst\u00b7rer", "Schau\u00b7er", "Mit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat es, die Luft durchzuckend scheu,", "tokens": ["Hat", "es", ",", "die", "Luft", "durch\u00b7zu