{"textgrid.poem.54853": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "67. Uber den wunderlichen Fall/ welcher sich zu Leiden in Holland", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sein Leichnam ward ins Grab vergangen Tag gebracht/", "tokens": ["Sein", "Leich\u00b7nam", "ward", "ins", "Grab", "ver\u00b7gan\u00b7gen", "Tag", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "VVPP", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der grei\u00dfe ", "tokens": ["Der", "grei\u00b7\u00dfe"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und folgt der Leichen nach/ als die nun hingetragen:", "tokens": ["Und", "folgt", "der", "Lei\u00b7chen", "nach", "/", "als", "die", "nun", "hin\u00b7ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "$(", "KOUS", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Trinckt er nach Landes Brauch in des verscharten Hau\u00df/", "tokens": ["Trinckt", "er", "nach", "Lan\u00b7des", "Brauch", "in", "des", "ver\u00b7schar\u00b7ten", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bi\u00df sich das Licht verlohr/ da geht er truncken aus", "tokens": ["Bi\u00df", "sich", "das", "Licht", "ver\u00b7lohr", "/", 