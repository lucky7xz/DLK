{"textgrid.poem.55745": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich k\u00f6nnt' Dich erw\u00fcrgen, Lucia,", "tokens": ["Ich", "k\u00f6nnt'", "Dich", "er\u00b7w\u00fcr\u00b7gen", ",", "Lu\u00b7cia", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wie giftige Vipernbrut,", "tokens": ["Wie", "gif\u00b7ti\u00b7ge", "Vi\u00b7pern\u00b7brut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich m\u00f6chte Dein Antlitz zerfetzen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "Dein", "Ant\u00b7litz", "zer\u00b7fet\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zerstampfen in rasender Wut.", "tokens": ["Zer\u00b7stamp\u00b7fen", "in", "ra\u00b7sen\u00b7der", "Wut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Was stachelst Du wieder und wieder", "tokens": ["Was", "sta\u00b7chelst", "Du", "wie\u00b7der", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "KON", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Erl\u00f6schende Sinnengier,", "tokens": ["Er\u00b7l\u00f6\u00b7schen\u00b7de", "Sin\u00b7nen\u00b7gier", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hinweg mir aus den Augen,", "tokens": ["Hin\u00b7weg", "mir", "aus", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir ekelt, ekelt vor Dir.", "tokens": ["Mir", "e\u00b7kelt", ",", "e\u00b7kelt", "vor", "Dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}