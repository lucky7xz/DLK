{"textgrid.poem.35840": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdie Eine nur dem Einen\u00ab \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdie Eine nur dem Einen\u00ab \u2013", "tokens": ["\u00bb", "die", "Ei\u00b7ne", "nur", "dem", "Ei\u00b7nen", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ART", "ADV", "ART", "ART", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer d\u00e4chte noch an Wahl?", "tokens": ["Wer", "d\u00e4ch\u00b7te", "noch", "an", "Wahl", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel unverstandnes Weinen,", "tokens": ["Viel", "un\u00b7ver\u00b7stand\u00b7nes", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel selbstgeschaffne Qual", "tokens": ["Viel", "selbst\u00b7ge\u00b7schaff\u00b7ne", "Qual"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Trug ich im jungen Herzen \u2013", "tokens": ["Trug", "ich", "im", "jun\u00b7gen", "Her\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach! nur der Erde Schmerzen.", "tokens": ["Ach", "!", "nur", "der", "Er\u00b7de", "Schmer\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bis ich verstand Dein Werben", "tokens": ["Bis", "ich", "ver\u00b7stand", "Dein", "Wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um dieses arme Herz,", "tokens": ["Um", "die\u00b7ses", "ar\u00b7me", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis ich erkannt Dein Sterben", "tokens": ["Bis", "ich", "er\u00b7kannt", "Dein", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im tiefsten Liebesschmerz;", "tokens": ["Im", "tiefs\u00b7ten", "Lie\u00b7bes\u00b7schmerz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da mu\u00dft' ich Dich umfassen,", "tokens": ["Da", "mu\u00dft'", "ich", "Dich", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da mu\u00dft' ich Dir mich lassen.", "tokens": ["Da", "mu\u00dft'", "ich", "Dir", "mich", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du hast dies Erdenleben,", "tokens": ["Du", "hast", "dies", "Er\u00b7den\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du hast ein ewig Sein", "tokens": ["Du", "hast", "ein", "e\u00b7wig", "Sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir darum nur gegeben,", "tokens": ["Mir", "da\u00b7rum", "nur", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ganz ich werde Dein;", "tokens": ["Da\u00df", "ganz", "ich", "wer\u00b7de", "Dein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du willst Dich mir verm\u00e4hlen,", "tokens": ["Du", "willst", "Dich", "mir", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie k\u00f6nnt' ", "tokens": ["Wie", "k\u00f6nnt'"], "token_info": ["word", "word"], "pos": ["PWAV", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Wollt' ich dem Feinde geben,", "tokens": ["Wollt'", "ich", "dem", "Fein\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was mir der Freund geschenkt,", "tokens": ["Was", "mir", "der", "Freund", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So w\u00e4r' mein ewig Leben", "tokens": ["So", "w\u00e4r'", "mein", "e\u00b7wig", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In ew'gen Tod versenkt,", "tokens": ["In", "ew'\u00b7gen", "Tod", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und nur durch mich verloren", "tokens": ["Und", "nur", "durch", "mich", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Ziel, dem ich geboren.", "tokens": ["Das", "Ziel", ",", "dem", "ich", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbdem Einen denn die Eine\u00ab", "tokens": ["\u00bb", "dem", "Ei\u00b7nen", "denn", "die", "Ei\u00b7ne", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ART", "ADV", "ART", "ART", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr Zeit und Ewigkeit!", "tokens": ["F\u00fcr", "Zeit", "und", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O, halte fest das Deine", "tokens": ["O", ",", "hal\u00b7te", "fest", "das", "Dei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADJD", "ART", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im hei\u00dfen Lebensstreit!", "tokens": ["Im", "hei\u00b7\u00dfen", "Le\u00b7bens\u00b7streit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Einst, von der Welt geschieden,", "tokens": ["Einst", ",", "von", "der", "Welt", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Find' ich in Dir den Frieden. Amen.", "tokens": ["Find'", "ich", "in", "Dir", "den", "Frie\u00b7den", ".", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}