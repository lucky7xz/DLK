{"textgrid.poem.60898": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Gerechte Rache", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rache gew\u00e4hrt mir der Tag, wann blo\u00df mein Name zur\u00fcckbleibt:", "tokens": ["Ra\u00b7che", "ge\u00b7w\u00e4hrt", "mir", "der", "Tag", ",", "wann", "blo\u00df", "mein", "Na\u00b7me", "zu\u00b7r\u00fcck\u00b7bleibt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+-+++", "measure": "dactylic.di.plus"}, "line.2": {"text": "S\u00e4ng er noch itzt, ruft dann mancher vergebliche Wunsch.", "tokens": ["S\u00e4ng", "er", "noch", "itzt", ",", "ruft", "dann", "man\u00b7cher", "ver\u00b7geb\u00b7li\u00b7che", "Wunsch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "$,", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ach, wir lauschen umsonst, wie seine Hexameter wogen,", "tokens": ["Ach", ",", "wir", "lau\u00b7schen", "um\u00b7sonst", ",", "wie", "sei\u00b7ne", "He\u00b7xa\u00b7me\u00b7ter", "wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wie sein m\u00e4nnlicher Geist auf dem Pentameter schwebt!", "tokens": ["Wie", "sein", "m\u00e4nn\u00b7li\u00b7cher", "Geist", "auf", "dem", "Pen\u00b7ta\u00b7me\u00b7ter", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}