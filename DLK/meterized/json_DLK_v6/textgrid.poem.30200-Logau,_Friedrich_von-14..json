{"textgrid.poem.30200": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Jungfern, wenn ich solte,", "tokens": ["Ihr", "Jung\u00b7fern", ",", "wenn", "ich", "sol\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wie ich gerne wolte,", "tokens": ["So", "wie", "ich", "ger\u00b7ne", "wol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr w\u00fcrdet sehn, ich wolte", "tokens": ["Ihr", "w\u00fcr\u00b7det", "sehn", ",", "ich", "wol\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht anders, als ich solte;", "tokens": ["Nicht", "an\u00b7ders", ",", "als", "ich", "sol\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn di\u00df wer, was ich solte,", "tokens": ["Denn", "di\u00df", "wer", ",", "was", "ich", "sol\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PWS", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was Euer Wille wolte.", "tokens": ["Was", "Eu\u00b7er", "Wil\u00b7le", "wol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}