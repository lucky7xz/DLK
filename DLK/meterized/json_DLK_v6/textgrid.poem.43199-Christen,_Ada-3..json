{"textgrid.poem.43199": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie tragen doch ein besseres Gepr\u00e4ge,", "tokens": ["Sie", "tra\u00b7gen", "doch", "ein", "bes\u00b7se\u00b7res", "Ge\u00b7pr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Barkenf\u00fchrer und der Gondolier',", "tokens": ["Der", "Bar\u00b7ken\u00b7f\u00fch\u00b7rer", "und", "der", "Gon\u00b7do\u00b7lier'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit welcher Treue lieben sie das Meer,", "tokens": ["Mit", "wel\u00b7cher", "Treu\u00b7e", "lie\u00b7ben", "sie", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als ob ihr Gl\u00fcck in seinen Tiefen l\u00e4ge.", "tokens": ["Als", "ob", "ihr", "Gl\u00fcck", "in", "sei\u00b7nen", "Tie\u00b7fen", "l\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie weisen noch mit seltsam stolzer Miene", "tokens": ["Sie", "wei\u00b7sen", "noch", "mit", "selt\u00b7sam", "stol\u00b7zer", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Dogenm\u00fctze auf dem Prachtpalast,", "tokens": ["Die", "Do\u00b7gen\u00b7m\u00fct\u00b7ze", "auf", "dem", "Pracht\u00b7pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie nennen seufzend und mit dumpfer Hast", "tokens": ["Sie", "nen\u00b7nen", "seuf\u00b7zend", "und", "mit", "dum\u00b7pfer", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Namen jeder berstenden Ruine.", "tokens": ["Den", "Na\u00b7men", "je\u00b7der", "bers\u00b7ten\u00b7den", "Ru\u00b7i\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie sch\u00fcchtern-z\u00e4rtlich kosen ihre H\u00e4nde", "tokens": ["Wie", "sch\u00fcch\u00b7tern\u00b7z\u00e4rt\u00b7lich", "ko\u00b7sen", "ih\u00b7re", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kunstgebilde einer gro\u00dfen Zeit,", "tokens": ["Die", "Kunst\u00b7ge\u00b7bil\u00b7de", "ei\u00b7ner", "gro\u00b7\u00dfen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ob den Untergang der Herrlichkeit", "tokens": ["Als", "ob", "den", "Un\u00b7ter\u00b7gang", "der", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Kind des Volkes nur allein empf\u00e4nde.", "tokens": ["Das", "Kind", "des", "Vol\u00b7kes", "nur", "al\u00b7lein", "em\u00b7pf\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}