{"textgrid.poem.41314": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Dat veniam corvis, vexat censura columbas", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der schwarzen Locken Glanz wird, fast ohn' Unterscheid,", "tokens": ["Der", "schwar\u00b7zen", "Lo\u00b7cken", "Glanz", "wird", ",", "fast", "ohn'", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei dir der Sch\u00f6nen Rang entscheiden.", "tokens": ["Bei", "dir", "der", "Sch\u00f6\u00b7nen", "Rang", "ent\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Blonde stichelst du. Mich d\u00e4ucht, du gehst zu weit:", "tokens": ["Auf", "Blon\u00b7de", "sti\u00b7chelst", "du", ".", "Mich", "d\u00e4ucht", ",", "du", "gehst", "zu", "weit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sei kl\u00fcger, Freund, und halt's mit beiden.", "tokens": ["Sei", "kl\u00fc\u00b7ger", ",", "Freund", ",", "und", "halt's", "mit", "bei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$,", "KON", "VVFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-", "measure