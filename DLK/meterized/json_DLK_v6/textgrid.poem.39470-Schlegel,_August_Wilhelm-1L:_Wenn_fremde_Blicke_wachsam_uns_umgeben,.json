{"textgrid.poem.39470": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn fremde Blicke wachsam uns umgeben,", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn fremde Blicke wachsam uns umgeben,", "tokens": ["Wenn", "frem\u00b7de", "Bli\u00b7cke", "wach\u00b7sam", "uns", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und unsre tiefe Sehnsucht, ungestillt,", "tokens": ["Und", "uns\u00b7re", "tie\u00b7fe", "Sehn\u00b7sucht", ",", "un\u00b7ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich in der Heiterkeit Geberde h\u00fcllt,", "tokens": ["Sich", "in", "der", "Hei\u00b7ter\u00b7keit", "Ge\u00b7ber\u00b7de", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und leise kaum den Busen wagt zu heben:", "tokens": ["Und", "lei\u00b7se", "kaum", "den", "Bu\u00b7sen", "wagt", "zu", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann ist nur eins, o mein geliebtes Leben!", "tokens": ["Dann", "ist", "nur", "eins", ",", "o", "mein", "ge\u00b7lieb\u00b7tes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "$,", "FM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was mein Gem\u00fcth mit Wonn' und Ahndung f\u00fcllt:", "tokens": ["Was", "mein", "Ge\u00b7m\u00fcth", "mit", "Wonn'", "und", "Ahn\u00b7dung", "f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Melodie, so deinem Mund' entquillt,", "tokens": ["Die", "Me\u00b7lo\u00b7die", ",", "so", "dei\u00b7nem", "Mund'", "ent\u00b7quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der seelenvollen T\u00f6ne sanftes Schweben.", "tokens": ["Der", "see\u00b7len\u00b7vol\u00b7len", "T\u00f6\u00b7ne", "sanf\u00b7tes", "Schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie Liebesodem f\u00fchl' ich den Gesang", "tokens": ["Wie", "Lie\u00b7be\u00b7so\u00b7dem", "f\u00fchl'", "ich", "den", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf diesen Lippen, die vergebens gl\u00fchen;", "tokens": ["Auf", "die\u00b7sen", "Lip\u00b7pen", ",", "die", "ver\u00b7ge\u00b7bens", "gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Ku\u00dfe wird mir jeder zarte Klang.", "tokens": ["Zum", "Ku\u00b7\u00dfe", "wird", "mir", "je\u00b7der", "zar\u00b7te", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und nenne die\u00df nicht eitle Phantasieen.", "tokens": ["Und", "nen\u00b7ne", "die\u00df", "nicht", "eit\u00b7le", "Phan\u00b7ta\u00b7si\u00b7e\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "PTKNEG", "ADJA", "NN", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Vernehm' ich nicht im schweigenden Umfang", "tokens": ["Ver\u00b7nehm'", "ich", "nicht", "im", "schwei\u00b7gen\u00b7den", "Um\u00b7fang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--++", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auch deines Herzens sch\u00f6ne Harmonieen?", "tokens": ["Auch", "dei\u00b7nes", "Her\u00b7zens", "sch\u00f6\u00b7ne", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}