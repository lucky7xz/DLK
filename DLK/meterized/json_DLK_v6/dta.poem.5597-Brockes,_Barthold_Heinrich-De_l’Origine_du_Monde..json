{"dta.poem.5597": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "De l\u2019Origine du Monde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was Geister seyn, was Wesen, welche dencken,", "tokens": ["Was", "Geis\u00b7ter", "seyn", ",", "was", "We\u00b7sen", ",", "wel\u00b7che", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "VAINF", "$,", "PWS", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie ein Vorwurf kann die Sinne lencken,", "tokens": ["Und", "wie", "ein", "Vor\u00b7wurf", "kann", "die", "Sin\u00b7ne", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch welche Handlungen, bald kr\u00e4fftig und bald zart,", "tokens": ["Durch", "wel\u00b7che", "Hand\u00b7lun\u00b7gen", ",", "bald", "kr\u00e4ff\u00b7tig", "und", "bald", "zart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Auf so verschiedne Art,", "tokens": ["Auf", "so", "ver\u00b7schied\u00b7ne", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Seele sich beweget, f\u00fchlet.", "tokens": ["Die", "See\u00b7le", "sich", "be\u00b7we\u00b7get", ",", "f\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dieses ist worauf mein Dichten zielet;", "tokens": ["Und", "die\u00b7ses", "ist", "wo\u00b7rauf", "mein", "Dich\u00b7ten", "zie\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der w\u00fcrckenden Natur verborgne K\u00fcnste,", "tokens": ["Der", "w\u00fcr\u00b7cken\u00b7den", "Na\u00b7tur", "ver\u00b7borg\u00b7ne", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Geheimes, z\u00e4rtliches Gespinste", "tokens": ["Ge\u00b7hei\u00b7mes", ",", "z\u00e4rt\u00b7li\u00b7ches", "Ge\u00b7spins\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Zu zeigen, damit ein Gem\u00fcht", "tokens": ["Zu", "zei\u00b7gen", ",", "da\u00b7mit", "ein", "Ge\u00b7m\u00fcht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das sehe, was kein Auge sieht.", "tokens": ["Das", "se\u00b7he", ",", "was", "kein", "Au\u00b7ge", "sieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}