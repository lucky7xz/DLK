{"textgrid.poem.60340": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Traurige Wege", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin mit dir im Wald gegangen;", "tokens": ["Bin", "mit", "dir", "im", "Wald", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, wie war der Wald so froh!", "tokens": ["Ach", ",", "wie", "war", "der", "Wald", "so", "froh", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles gr\u00fcn, die V\u00f6gel sangen,", "tokens": ["Al\u00b7les", "gr\u00fcn", ",", "die", "V\u00f6\u00b7gel", "san\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das scheue Wild entfloh.", "tokens": ["Und", "das", "scheu\u00b7e", "Wild", "ent\u00b7floh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo die Liebe frei und offen", "tokens": ["Wo", "die", "Lie\u00b7be", "frei", "und", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings von allen Zweigen schallt,", "tokens": ["Rings", "von", "al\u00b7len", "Zwei\u00b7gen", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ging die Liebe ohne Hoffen", "tokens": ["Ging", "die", "Lie\u00b7be", "oh\u00b7ne", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traurig durch den gr\u00fcnen Wald. \u2013", "tokens": ["Trau\u00b7rig", "durch", "den", "gr\u00fc\u00b7nen", "Wald", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bin mit dir am Flu\u00df gefahren;", "tokens": ["Bin", "mit", "dir", "am", "Flu\u00df", "ge\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, wie war die Nacht so mild!", "tokens": ["Ach", ",", "wie", "war", "die", "Nacht", "so", "mild", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Flut, der sanften, klaren,", "tokens": ["Auf", "der", "Flut", ",", "der", "sanf\u00b7ten", ",", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiegte sich des Mondes Bild.", "tokens": ["Wieg\u00b7te", "sich", "des", "Mon\u00b7des", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lustig scherzten die Gesellen;", "tokens": ["Lus\u00b7tig", "scherz\u00b7ten", "die", "Ge\u00b7sel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Liebe schwieg und sann,", "tokens": ["Uns\u00b7re", "Lie\u00b7be", "schwieg", "und", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie mit jedem Schlag der Wellen", "tokens": ["Wie", "mit", "je\u00b7dem", "Schlag", "der", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeit und Gl\u00fcck vor\u00fcberrann. \u2013", "tokens": ["Zeit", "und", "Gl\u00fcck", "vor\u00b7\u00fc\u00b7ber\u00b7rann", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Graue Wolken niederhingen,", "tokens": ["Grau\u00b7e", "Wol\u00b7ken", "nie\u00b7der\u00b7hin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Kreuze strich der West,", "tokens": ["Durch", "die", "Kreu\u00b7ze", "strich", "der", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wir einst am Kirchhof gingen;", "tokens": ["Als", "wir", "einst", "am", "Kirch\u00b7hof", "gin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach wie schliefen sie so fest!", "tokens": ["Ach", "wie", "schlie\u00b7fen", "sie", "so", "fest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "An den Kreuzen, an den Steinen", "tokens": ["An", "den", "Kreu\u00b7zen", ",", "an", "den", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fand die Liebe keinen Halt;", "tokens": ["Fand", "die", "Lie\u00b7be", "kei\u00b7nen", "Halt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sahen uns die Toten weinen,", "tokens": ["Sa\u00b7hen", "uns", "die", "To\u00b7ten", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wir dort vorbeigewallt?", "tokens": ["Als", "wir", "dort", "vor\u00b7bei\u00b7ge\u00b7wallt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}