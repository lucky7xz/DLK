{"textgrid.poem.40357": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hurrra, zum ersten Mal:", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hurrra, zum ersten Mal:", "tokens": ["Hurr\u00b7ra", ",", "zum", "ers\u00b7ten", "Mal", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Mutter, der Peter,", "tokens": ["Mut\u00b7ter", ",", "der", "Pe\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NE", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "hurra, jetzt geht er!", "tokens": ["hur\u00b7ra", ",", "jetzt", "geht", "er", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kuck, ganz alleinechen", "tokens": ["Kuck", ",", "ganz", "al\u00b7lei\u00b7ne\u00b7chen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "setzt er die Beinechen,", "tokens": ["setzt", "er", "die", "Bei\u00b7ne\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.6": {"text": "ganz wie zur Reichstagswahl,", "tokens": ["ganz", "wie", "zur", "Reichs\u00b7tags\u00b7wahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "wie Onkel Wackelpfahl!", "tokens": ["wie", "On\u00b7kel", "Wa\u00b7ckel\u00b7pfahl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Aua, Geschrei:", "tokens": ["Au\u00b7a", ",", "Ge\u00b7schrei", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "bautz, vorbei!", "tokens": ["bautz", ",", "vor\u00b7bei", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}