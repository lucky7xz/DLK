{"textgrid.poem.63433": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Denn helpt dat nich", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fch, wenn du mi nich magst un wist", "tokens": ["S\u00fch", ",", "wenn", "du", "mi", "nich", "magst", "un", "wist"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "NE", "PTKNEG", "FM", "FM", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "un mi nich wist un magst", "tokens": ["un", "mi", "nich", "wist", "un", "magst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "un achter Hans un Krischan b\u00fcst", "tokens": ["un", "ach\u00b7ter", "Hans", "un", "Krisc\u00b7han", "b\u00fcst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "un \u00fcmmer ut mi lachst,", "tokens": ["un", "\u00fcm\u00b7mer", "ut", "mi", "lachst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "as ob ick d\u0153sig wir un mall", "tokens": ["as", "ob", "ick", "d\u0153\u00b7sig", "wir", "un", "mall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "ADJD", "PPER", "FM", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "as Hinrich sin oll Vo\u00df in'n Stall,", "tokens": ["as", "Hin\u00b7rich", "sin", "oll", "Vo\u00df", "in'n", "Stall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "na \u2013 je denn helpt dat nich!", "tokens": ["na", "\u2013", "je", "denn", "helpt", "dat", "nich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "ADV", "ADV", "VVFIN", "ART", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Un wenn du meenst, wat ick man spa\u00df,", "tokens": ["Un", "wenn", "du", "me\u00b7enst", ",", "wat", "ick", "man", "spa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "$,", "PWS", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "un dat's doch all min Irnst \u2013", "tokens": ["un", "dat's", "doch", "all", "min", "Irnst", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ADV", "PIAT", "PPOSAT", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "s\u00fch, denn b\u00fcst du am Enn de Klas,", "tokens": ["s\u00fch", ",", "denn", "b\u00fcst", "du", "am", "Enn", "de", "Klas", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "VVFIN", "PPER", "APPRART", "NN", "NE", "NE", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "man dat to lat du't lirnst.", "tokens": ["man", "dat", "to", "lat", "du't", "lirnst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Un wenn, ihr du di dat vers\u00fchst,", "tokens": ["Un", "wenn", ",", "ihr", "du", "di", "dat", "ver\u00b7s\u00fchst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "$,", "PPER", "PPER", "NE", "NN", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "du nahst mit sonn lang N\u00e4s' aft\u00fchst,", "tokens": ["du", "nahst", "mit", "sonn", "lang", "N\u00e4s'", "af\u00b7t\u00fchst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "na \u2013 je denn helpt dat nich!", "tokens": ["na", "\u2013", "je", "denn", "helpt", "dat", "nich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "ADV", "ADV", "VVFIN", "ART", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Un wenn ick Liesch nu frigen do", "tokens": ["Un", "wenn", "ick", "Liesch", "nu", "fri\u00b7gen", "do"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "NN", "ADV", "ADJA", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "un swaps di sitten lat", "tokens": ["un", "swaps", "di", "sit\u00b7ten", "lat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "samst din oll Petersill, s\u00fch so,", "tokens": ["samst", "din", "oll", "Pe\u00b7ter\u00b7sill", ",", "s\u00fch", "so", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADJD", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "wen weet, ob mi dat schadt.", "tokens": ["wen", "weet", ",", "ob", "mi", "dat", "schadt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "NE", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn du di v\u00f6r din'n Kopp denn sleihst", "tokens": ["Wenn", "du", "di", "v\u00f6r", "din'n", "Kopp", "denn", "sleihst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "FM", "FM", "FM", "FM", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "un nahsten di vers\u00f6pen deist,", "tokens": ["un", "nahs\u00b7ten", "di", "ver\u00b7s\u00f6\u00b7pen", "de\u00b7ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "na \u2013 je denn helpt dat nich!", "tokens": ["na", "\u2013", "je", "denn", "helpt", "dat", "nich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "ADV", "ADV", "VVFIN", "ART", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}