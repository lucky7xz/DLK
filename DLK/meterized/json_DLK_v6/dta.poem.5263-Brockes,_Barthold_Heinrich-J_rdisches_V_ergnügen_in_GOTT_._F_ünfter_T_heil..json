{"dta.poem.5263": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und wovon, in den sch\u00f6nen Wercken,", "tokens": ["Und", "wo\u00b7von", ",", "in", "den", "sch\u00f6\u00b7nen", "Wer\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man sie mit Vernunft erblickt,", "tokens": ["Wenn", "man", "sie", "mit", "Ver\u00b7nunft", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So Macht als Lieb und Weisheit zu bemercken.", "tokens": ["So", "Macht", "als", "Lieb", "und", "Weis\u00b7heit", "zu", "be\u00b7mer\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann nun in kurtzer Zeit die gr\u00fcnen Zweige schiessen,", "tokens": ["Wann", "nun", "in", "kurt\u00b7zer", "Zeit", "die", "gr\u00fc\u00b7nen", "Zwei\u00b7ge", "schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Bl\u00e4tter sich vergr\u00f6ssern und sich schliessen,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "sich", "ver\u00b7gr\u00f6s\u00b7sern", "und", "sich", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So l\u00e4\u00dft an jedem Baum, nicht minder sch\u00f6n,", "tokens": ["So", "l\u00e4\u00dft", "an", "je\u00b7dem", "Baum", ",", "nicht", "min\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich eine neue Art von Sch\u00f6nheit sehn,", "tokens": ["Sich", "ei\u00b7ne", "neu\u00b7e", "Art", "von", "Sch\u00f6n\u00b7heit", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Indem, da sich das Heer der Bl\u00e4tter fast vereint,", "tokens": ["In\u00b7dem", ",", "da", "sich", "das", "Heer", "der", "Bl\u00e4t\u00b7ter", "fast", "ver\u00b7eint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PRF", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Baum ein gr\u00fcn Gew\u00f6lck, ein gr\u00fcn Gew\u00f6lbe scheint.", "tokens": ["Der", "Baum", "ein", "gr\u00fcn", "Ge\u00b7w\u00f6lck", ",", "ein", "gr\u00fcn", "Ge\u00b7w\u00f6l\u00b7be", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es l\u00e4\u00dft als tr\u00fcge jeder Ast", "tokens": ["Es", "l\u00e4\u00dft", "als", "tr\u00fc\u00b7ge", "je\u00b7der", "Ast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nicht ohne M\u00fch der Bl\u00e4tter gr\u00fcne Last.", "tokens": ["Nicht", "oh\u00b7ne", "M\u00fch", "der", "Bl\u00e4t\u00b7ter", "gr\u00fc\u00b7ne", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die schwancken Zweige h\u00e4ngen nieder,", "tokens": ["Die", "schwan\u00b7cken", "Zwei\u00b7ge", "h\u00e4n\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und gleichen, da sie abwerts hangen,", "tokens": ["Und", "glei\u00b7chen", ",", "da", "sie", "ab\u00b7werts", "han\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und in sanft-wallenden und gr\u00fcnen Schimmer prangen,", "tokens": ["Und", "in", "sanft\u00b7wal\u00b7len\u00b7den", "und", "gr\u00fc\u00b7nen", "Schim\u00b7mer", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dem abwerts h\u00e4ngenden Gefieder", "tokens": ["Dem", "ab\u00b7werts", "h\u00e4n\u00b7gen\u00b7den", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Von gr\u00fcn-beaugten Pfauen-Schw\u00e4ntzen,", "tokens": ["Von", "gr\u00fcn\u00b7be\u00b7aug\u00b7ten", "Pfau\u00b7en\u00b7Schw\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Nur mit dem Unterscheid, da\u00df die mit blauen,", "tokens": ["Nur", "mit", "dem", "Un\u00b7ter\u00b7scheid", ",", "da\u00df", "die", "mit", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KOUS", "ART", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Da diese hier mit gr\u00fcnen Augen, gl\u00e4ntzen.", "tokens": ["Da", "die\u00b7se", "hier", "mit", "gr\u00fc\u00b7nen", "Au\u00b7gen", ",", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wenn wir demnach, im lauen Lentzen,", "tokens": ["Wenn", "wir", "dem\u00b7nach", ",", "im", "lau\u00b7en", "Lent\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der gr\u00fcnen B\u00e4ume Pracht beschauen;", "tokens": ["Der", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7me", "Pracht", "be\u00b7schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "So la\u00dft es ohne Danck und Andacht nie geschehn!", "tokens": ["So", "la\u00dft", "es", "oh\u00b7ne", "Danck", "und", "An\u00b7dacht", "nie", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "La\u00dft uns in unsrer Lust denjenigen erh\u00f6hn,", "tokens": ["La\u00dft", "uns", "in", "uns\u00b7rer", "Lust", "den\u00b7je\u00b7ni\u00b7gen", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Durch dessen Lieb\u2019 und weise Macht,", "tokens": ["Durch", "des\u00b7sen", "Lieb'", "und", "wei\u00b7se", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Was sch\u00f6n und herrlich ist allein hervorgebracht!", "tokens": ["Was", "sch\u00f6n", "und", "herr\u00b7lich", "ist", "al\u00b7lein", "her\u00b7vor\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}