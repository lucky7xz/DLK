{"textgrid.poem.46042": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Am dreiundzwanzigsten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh! Jairus naht gequ\u00e4let,", "tokens": ["Sieh", "!", "Jai\u00b7rus", "naht", "ge\u00b7qu\u00e4\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beuget vor dem Herrn sein Knie:", "tokens": ["Beu\u00b7get", "vor", "dem", "Herrn", "sein", "Knie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbmeine Tochter, jetzt entseelet,", "tokens": ["\u00bb", "mei\u00b7ne", "Toch\u00b7ter", ",", "jetzt", "ent\u00b7see\u00b7let", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebt, legst du die Hand auf sie.\u00ab", "tokens": ["Lebt", ",", "legst", "du", "die", "Hand", "auf", "sie", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Jesus folgt; der Glaube f\u00fchrte", "tokens": ["Je\u00b7sus", "folgt", ";", "der", "Glau\u00b7be", "f\u00fchr\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Her ein schamhaft krankes Weib.", "tokens": ["Her", "ein", "scham\u00b7haft", "kran\u00b7kes", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wenn ich seinen Saum ber\u00fchrte,", "tokens": ["Wenn", "ich", "sei\u00b7nen", "Saum", "be\u00b7r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaubt sie, dann genest mein Leib.", "tokens": ["Glaubt", "sie", ",", "dann", "ge\u00b7nest", "mein", "Leib", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie ber\u00fchrt ihn, \u2013 er sich wendet:", "tokens": ["Sie", "be\u00b7r\u00fchrt", "ihn", ",", "\u2013", "er", "sich", "wen\u00b7det", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "$(", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbtochter, habe Trost, zur Stund'", "tokens": ["\u00bb", "toch\u00b7ter", ",", "ha\u00b7be", "Trost", ",", "zur", "Stund'"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "$,", "VAFIN", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Half dein Glaube dir.\u00ab Geendet", "tokens": ["Half", "dein", "Glau\u00b7be", "dir", ".", "\u00ab", "Ge\u00b7en\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "$.", "$(", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War ihr Weh, sie war gesund.", "tokens": ["War", "ihr", "Weh", ",", "sie", "war", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu den Pfeifern, dem Gedr\u00e4nge,", "tokens": ["Zu", "den", "Pfei\u00b7fern", ",", "dem", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt er: \u00bbWeicht vom Sterbehaus,", "tokens": ["Sagt", "er", ":", "\u00bb", "Weicht", "vom", "Ster\u00b7be\u00b7haus", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie schl\u00e4ft nur.\u00ab Und die Menge", "tokens": ["Denn", "sie", "schl\u00e4ft", "nur", ".", "\u00ab", "Und", "die", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$.", "$(", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lachte unsern Herrn da aus.", "tokens": ["Lach\u00b7te", "un\u00b7sern", "Herrn", "da", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Als das Volk hinweggegangen,", "tokens": ["Als", "das", "Volk", "hin\u00b7weg\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ging er ein, nahm bei der Hand", "tokens": ["Ging", "er", "ein", ",", "nahm", "bei", "der", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Verstorbne, Heil empfangen", "tokens": ["Die", "Ver\u00b7storb\u00b7ne", ",", "Heil", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat sie, die da auferstand.", "tokens": ["Hat", "sie", ",", "die", "da", "auf\u00b7er\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und verk\u00fcndet ward das Wunder:", "tokens": ["Und", "ver\u00b7k\u00fcn\u00b7det", "ward", "das", "Wun\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbheil macht seines Mantels Saum!\u00ab", "tokens": ["\u00bb", "heil", "macht", "sei\u00b7nes", "Man\u00b7tels", "Saum", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr, mach unsern Glauben munter!", "tokens": ["Herr", ",", "mach", "un\u00b7sern", "Glau\u00b7ben", "mun\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glauben macht den Tod zum Traum.", "tokens": ["Glau\u00b7ben", "macht", "den", "Tod", "zum", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}