{"textgrid.poem.61559": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die fernen Fl\u00f6ten h\u00f6r' ich schallen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die fernen Fl\u00f6ten h\u00f6r' ich schallen,", "tokens": ["Die", "fer\u00b7nen", "Fl\u00f6\u00b7ten", "h\u00f6r'", "ich", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Feierhymnus wogt darein;", "tokens": ["Der", "Fei\u00b7er\u00b7hym\u00b7nus", "wogt", "da\u00b7rein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es w\u00e4lzt sich zu des Tempels Hallen", "tokens": ["Es", "w\u00e4lzt", "sich", "zu", "des", "Tem\u00b7pels", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Volkes Strom im Morgenschein.", "tokens": ["Des", "Vol\u00b7kes", "Strom", "im", "Mor\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Knaben rote Fackeln strahlen", "tokens": ["Der", "Kna\u00b7ben", "ro\u00b7te", "Fa\u00b7ckeln", "strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf wei\u00dfer Festgewandung Zier;", "tokens": ["Auf", "wei\u00b7\u00dfer", "Fest\u00b7ge\u00b7wan\u00b7dung", "Zier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Priester tragen goldne Schalen", "tokens": ["Die", "Pries\u00b7ter", "tra\u00b7gen", "gold\u00b7ne", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und f\u00fchren den bekr\u00e4nzten Stier.", "tokens": ["Und", "f\u00fch\u00b7ren", "den", "be\u00b7kr\u00e4nz\u00b7ten", "Stier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl m\u00f6cht' ich mit den andern ziehen", "tokens": ["Wohl", "m\u00f6cht'", "ich", "mit", "den", "an\u00b7dern", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jubeln in des Opfers Rauch;", "tokens": ["Und", "ju\u00b7beln", "in", "des", "Op\u00b7fers", "Rauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch auf den Stufen, da sie knieen,", "tokens": ["Doch", "auf", "den", "Stu\u00b7fen", ",", "da", "sie", "kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ums\u00e4uselt mich kein Lebenshauch.", "tokens": ["Um\u00b7s\u00e4u\u00b7selt", "mich", "kein", "Le\u00b7bens\u00b7hauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Kindheit milde Schleier sanken,", "tokens": ["Der", "Kind\u00b7heit", "mil\u00b7de", "Schlei\u00b7er", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die mich umfangen lieb und eng,", "tokens": ["Die", "mich", "um\u00b7fan\u00b7gen", "lieb", "und", "eng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und vor dem siegenden Gedanken", "tokens": ["Und", "vor", "dem", "sie\u00b7gen\u00b7den", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erlag der G\u00f6tter bunt Gedr\u00e4ng'.", "tokens": ["Er\u00b7lag", "der", "G\u00f6t\u00b7ter", "bunt", "Ge\u00b7dr\u00e4ng'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wie sich des Olymps Gestalten", "tokens": ["Doch", "wie", "sich", "des", "O\u00b7lymps", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PRF", "ART", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gleich Tr\u00e4umen l\u00f6sten nebelhaft,", "tokens": ["Gleich", "Tr\u00e4u\u00b7men", "l\u00f6s\u00b7ten", "ne\u00b7bel\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Da war es mir, als fl\u00f6ss' ihr Walten", "tokens": ["Da", "war", "es", "mir", ",", "als", "fl\u00f6ss'", "ihr", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KOUS", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur\u00fcck in ", "tokens": ["Zu\u00b7r\u00fcck", "in"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Aus allem, was der Tag vollendet,", "tokens": ["Aus", "al\u00b7lem", ",", "was", "der", "Tag", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Spricht g\u00f6ttlich hoch ein ", "tokens": ["Spricht", "g\u00f6tt\u00b7lich", "hoch", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADJD", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und meine Seele st\u00fcrzt geblendet", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "st\u00fcrzt", "ge\u00b7blen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor dieses Reichtums F\u00fcllen hin.", "tokens": ["Vor", "die\u00b7ses", "Reich\u00b7tums", "F\u00fcl\u00b7len", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O du, den ich zu nennen zage,", "tokens": ["O", "du", ",", "den", "ich", "zu", "nen\u00b7nen", "za\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du ew'ger Geist, des reines Licht", "tokens": ["Du", "ew'\u00b7ger", "Geist", ",", "des", "rei\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch durch den Dunst der G\u00f6ttersage", "tokens": ["Noch", "durch", "den", "Dunst", "der", "G\u00f6t\u00b7ter\u00b7sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In tausend Farben spielend bricht;", "tokens": ["In", "tau\u00b7send", "Far\u00b7ben", "spie\u00b7lend", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den sie in tausend Bildern ehren,", "tokens": ["Den", "sie", "in", "tau\u00b7send", "Bil\u00b7dern", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dem doch nie ein Bildnis glich,", "tokens": ["Und", "dem", "doch", "nie", "ein", "Bild\u00b7nis", "glich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du, den ich nimmer kann entbehren,", "tokens": ["Du", ",", "den", "ich", "nim\u00b7mer", "kann", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du Einziger, wie fass' ich dich!", "tokens": ["Du", "Ein\u00b7zi\u00b7ger", ",", "wie", "fass'", "ich", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Im Weltall sucht' ich ohn' Ermatten", "tokens": ["Im", "Wel\u00b7tall", "sucht'", "ich", "ohn'", "Er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich zu ergr\u00fcnden voll und ganz;", "tokens": ["Dich", "zu", "er\u00b7gr\u00fcn\u00b7den", "voll", "und", "ganz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nachts verh\u00fcllst du dich in Schatten", "tokens": ["Doch", "nachts", "ver\u00b7h\u00fcllst", "du", "dich", "in", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und birgst am Tage dich im Glanz.", "tokens": ["Und", "birgst", "am", "Ta\u00b7ge", "dich", "im", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn das Morgenrot mich weckte,", "tokens": ["Und", "wenn", "das", "Mor\u00b7gen\u00b7rot", "mich", "weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00fcbergl\u00fcht aus meinem Traum", "tokens": ["Und", "\u00fc\u00b7berg\u00b7l\u00fcht", "aus", "mei\u00b7nem", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Hand ich tastend darnach streckte:", "tokens": ["Die", "Hand", "ich", "tas\u00b7tend", "dar\u00b7nach", "streck\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es war nur deines Kleides Saum.", "tokens": ["Es", "war", "nur", "dei\u00b7nes", "Klei\u00b7des", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohl ruft der Donner deinen Namen,", "tokens": ["Wohl", "ruft", "der", "Don\u00b7ner", "dei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl zeigt der Blitz uns deine Spur;", "tokens": ["Wohl", "zeigt", "der", "Blitz", "uns", "dei\u00b7ne", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, ob sie deine Boten kamen,", "tokens": ["Doch", ",", "ob", "sie", "dei\u00b7ne", "Bo\u00b7ten", "ka\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie bringen ", "tokens": ["Sie", "brin\u00b7gen"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "O, was von dir die Dinge stammeln", "tokens": ["O", ",", "was", "von", "dir", "die", "Din\u00b7ge", "stam\u00b7meln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dunkelm Deuten fort und fort.", "tokens": ["Mit", "dun\u00b7kelm", "Deu\u00b7ten", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wirst du's, Erhabner, nie versammeln", "tokens": ["Wirst", "du's", ",", "Er\u00b7hab\u00b7ner", ",", "nie", "ver\u00b7sam\u00b7meln"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In ein lebendig klares Wort?", "tokens": ["In", "ein", "le\u00b7ben\u00b7dig", "kla\u00b7res", "Wort", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wird nie dein liebender Gedanke", "tokens": ["Wird", "nie", "dein", "lie\u00b7ben\u00b7der", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Wehmut \u00fcber unser Leid", "tokens": ["Voll", "Weh\u00b7mut", "\u00fc\u00b7ber", "un\u00b7ser", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herab sich neigen in die Schranke", "tokens": ["Her\u00b7ab", "sich", "nei\u00b7gen", "in", "die", "Schran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sehnsuchtbangen Sterblichkeit?", "tokens": ["Der", "sehn\u00b7sucht\u00b7ban\u00b7gen", "Sterb\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wirst nie dein blendend Licht du lassen,", "tokens": ["Wirst", "nie", "dein", "blen\u00b7dend", "Licht", "du", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJD", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich nah und menschlich kundzutun,", "tokens": ["Dich", "nah", "und", "menschlich", "kund\u00b7zu\u00b7tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Da\u00df wir mit Armen dich umfassen", "tokens": ["Da\u00df", "wir", "mit", "Ar\u00b7men", "dich", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und fromm an deinem Busen ruhn?", "tokens": ["Und", "fromm", "an", "dei\u00b7nem", "Bu\u00b7sen", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach, tief in meiner Seele Grunde,", "tokens": ["Ach", ",", "tief", "in", "mei\u00b7ner", "See\u00b7le", "Grun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da schl\u00e4ft ein Ahnen wundervoll:", "tokens": ["Da", "schl\u00e4ft", "ein", "Ah\u00b7nen", "wun\u00b7der\u00b7voll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Lauf der Zeiten bringt die Stunde,", "tokens": ["Der", "Lauf", "der", "Zei\u00b7ten", "bringt", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da solches Heil geschehen soll.", "tokens": ["Da", "sol\u00b7ches", "Heil", "ge\u00b7sche\u00b7hen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O selig, denen du dein Wesen", "tokens": ["O", "se\u00b7lig", ",", "de\u00b7nen", "du", "dein", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Dann sichtbar hold entgegensenkst,", "tokens": ["Dann", "sicht\u00b7bar", "hold", "ent\u00b7ge\u00b7gen\u00b7senkst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die du zu himmlischem Genesen", "tokens": ["Die", "du", "zu", "himm\u00b7li\u00b7schem", "Ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus deines Lebens Adern tr\u00e4nkst!", "tokens": ["Aus", "dei\u00b7nes", "Le\u00b7bens", "A\u00b7dern", "tr\u00e4nkst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dann wird der Baum der Menschheit gr\u00fcnen;", "tokens": ["Dann", "wird", "der", "Baum", "der", "Menschheit", "gr\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dann werden ihren alten Zwist", "tokens": ["Dann", "wer\u00b7den", "ih\u00b7ren", "al\u00b7ten", "Zwist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel und die Erde s\u00fchnen", "tokens": ["Der", "Him\u00b7mel", "und", "die", "Er\u00b7de", "s\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch den, der beider teilhaft ist.", "tokens": ["Durch", "den", ",", "der", "bei\u00b7der", "teil\u00b7haft", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein sanftes Leuchten wird durchdringen", "tokens": ["Ein", "sanf\u00b7tes", "Leuch\u00b7ten", "wird", "durch\u00b7drin\u00b7gen"], "tok