{"textgrid.poem.31186": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbadam Mensch\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob eine Wurst, die nachts im Rauchfang h\u00e4ngt", "tokens": ["Ob", "ei\u00b7ne", "Wurst", ",", "die", "nachts", "im", "Rauch\u00b7fang", "h\u00e4ngt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich noch Gedanken \u00fcber einen Stern macht,", "tokens": ["Sich", "noch", "Ge\u00b7dan\u00b7ken", "\u00fc\u00b7ber", "ei\u00b7nen", "Stern", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der golden \u00fcber Ihrem Zipfel brennt?", "tokens": ["Der", "gol\u00b7den", "\u00fc\u00b7ber", "Ih\u00b7rem", "Zip\u00b7fel", "brennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In dies Problem sich wie ein Maulwurf gr\u00fcbelnd,", "tokens": ["In", "dies", "Prob\u00b7lem", "sich", "wie", "ein", "Maul\u00b7wurf", "gr\u00fc\u00b7belnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "PRF", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Bepinselte er seine Nase sich", "tokens": ["Be\u00b7pin\u00b7sel\u00b7te", "er", "sei\u00b7ne", "Na\u00b7se", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vor seinem Spiegel kunstvoll mit Zinober,", "tokens": ["Vor", "sei\u00b7nem", "Spie\u00b7gel", "kunst\u00b7voll", "mit", "Zi\u00b7no\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Schrie Kikriki, frass siebzehn saure Gurken,", "tokens": ["Schrie", "Kik\u00b7ri\u00b7ki", ",", "frass", "sieb\u00b7zehn", "sau\u00b7re", "Gur\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Soff dann diverse K\u00fcbel Buttermilch", "tokens": ["Soff", "dann", "di\u00b7ver\u00b7se", "K\u00fc\u00b7bel", "But\u00b7ter\u00b7milch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und starb zuletzt als \u2013 Sultan von Marokko.", "tokens": ["Und", "starb", "zu\u00b7letzt", "als", "\u2013", "Sul\u00b7tan", "von", "Ma\u00b7rok\u00b7ko", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KOUS", "$(", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}