{"textgrid.poem.40097": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur kein th\u00f6richtes Ereifern,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur kein th\u00f6richtes Ereifern,", "tokens": ["Nur", "kein", "th\u00f6\u00b7rich\u00b7tes", "Er\u00b7ei\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "wenn die Wichte dich begeifern!", "tokens": ["wenn", "die", "Wich\u00b7te", "dich", "be\u00b7gei\u00b7fern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Kautschukseelen fliegen", "tokens": ["Die\u00b7se", "Kaut\u00b7schuk\u00b7see\u00b7len", "flie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "nur so h\u00f6her vor Vergn\u00fcgen,", "tokens": ["nur", "so", "h\u00f6\u00b7her", "vor", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "um so mehr sie Hiebe kriegen.", "tokens": ["um", "so", "mehr", "sie", "Hie\u00b7be", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}