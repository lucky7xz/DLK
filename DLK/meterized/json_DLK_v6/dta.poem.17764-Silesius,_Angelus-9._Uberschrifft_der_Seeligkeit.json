{"dta.poem.17764": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "9.  Uberschrifft der Seeligkeit", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ist es jmmer Tag/ hier scheint die Ewge Sonne/", "tokens": ["Hier", "ist", "es", "jm\u00b7mer", "Tag", "/", "hier", "scheint", "die", "Ew\u00b7ge", "Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier wei\u00df man nicht von Weh/ von Kummer Angst", "tokens": ["Hier", "wei\u00df", "man", "nicht", "von", "Weh", "/", "von", "Kum\u00b7mer", "Angst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "APPR", "NN", "$(", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und Leid.", "tokens": ["und", "Leid", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Man lebt in gantzer Lust und gantzer Seeligkeit.", "tokens": ["Man", "lebt", "in", "gant\u00b7zer", "Lust", "und", "gant\u00b7zer", "See\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man sicht unnd h\u00f6ret nichts als lauter Freud ", "tokens": ["Man", "sicht", "unnd", "h\u00f6\u00b7ret", "nichts", "als", "lau\u00b7ter", "Freud"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD", "VVFIN", "PIS", "KOKOM", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wonne.", "tokens": ["Won\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Man trinkt sich satt und Voll beym s\u00fcssen JEsus-", "tokens": ["Man", "trinkt", "sich", "satt", "und", "Voll", "beym", "s\u00fcs\u00b7sen", "Je\u00b7sus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bronne.", "tokens": ["Bron\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Man sitzt in stoltzer Ruh/ man d\u00e4nkt an keine Zeit/", "tokens": ["Man", "sitzt", "in", "stolt\u00b7zer", "Ruh", "/", "man", "d\u00e4nkt", "an", "kei\u00b7ne", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$(", "PIS", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man leget niemals ab da\u00df Kleid der Herrlichkeit.", "tokens": ["Man", "le\u00b7get", "nie\u00b7mals", "ab", "da\u00df", "Kleid", "der", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "KOUS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hier rauschet wie ein Strom was vor nur tropffwei\u00df", "tokens": ["Hier", "rau\u00b7schet", "wie", "ein", "Strom", "was", "vor", "nur", "tropf\u00b7fwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "PWS", "APPR", "ADV", "NN"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.12": {"text": "ronne.", "tokens": ["ron\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Hier schaut man GOttes glantz und s\u00fcsses Angesicht/", "tokens": ["Hier", "schaut", "man", "Got\u00b7tes", "glantz", "und", "s\u00fcs\u00b7ses", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hier wird man \u00fcberformt mit seiner GOttheit Licht.", "tokens": ["Hier", "wird", "man", "\u00fc\u00b7ber\u00b7formt", "mit", "sei\u00b7ner", "Got\u00b7theit", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hier senkt man sich in Jhn/ und giebt jhm tausendk\u00fcsse.", "tokens": ["Hier", "senkt", "man", "sich", "in", "Jhn", "/", "und", "giebt", "jhm", "tau\u00b7send\u00b7k\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPR", "PPER", "$(", "KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man liebt und wird geliebt/ man schmekt jhn wie er ist.", "tokens": ["Man", "liebt", "und", "wird", "ge\u00b7liebt", "/", "man", "schmekt", "jhn", "wie", "er", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VAFIN", "VVPP", "$(", "PIS", "VVFIN", "PPER", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Man singt sein Lob und alls worzu man ist erkiest.", "tokens": ["Man", "singt", "sein", "Lob", "und", "alls", "wor\u00b7zu", "man", "ist", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "KON", "ADV", "PWAV", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ach JEsu hilff mir doch damit auch ichs geniesse!", "tokens": ["Ach", "Je\u00b7su", "hilff", "mir", "doch", "da\u00b7mit", "auch", "ichs", "ge\u00b7nies\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "PPER", "ADV", "PAV", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}