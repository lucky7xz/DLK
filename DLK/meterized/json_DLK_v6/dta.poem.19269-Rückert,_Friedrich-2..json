{"dta.poem.19269": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom niedern H\u00fcgel sah ich auf mein Heimatsthal,", "tokens": ["Vom", "nie\u00b7dern", "H\u00fc\u00b7gel", "sah", "ich", "auf", "mein", "Hei\u00b7mat\u00b7sthal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alles lag vor mir verkl\u00e4rt im Sonnenstral.", "tokens": ["Und", "al\u00b7les", "lag", "vor", "mir", "ver\u00b7kl\u00e4rt", "im", "Son\u00b7nen\u00b7stral", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich sah das Einzelste mit Liebesblick, das kleinste,", "tokens": ["Ich", "sah", "das", "Ein\u00b7zels\u00b7te", "mit", "Lie\u00b7bes\u00b7blick", ",", "das", "kleins\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und jeder Unterschied ward mir vertraut, der feinste.", "tokens": ["Und", "je\u00b7der", "Un\u00b7ter\u00b7schied", "ward", "mir", "ver\u00b7traut", ",", "der", "feins\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich sah mich satt daran, viel liebe lange Stunden;", "tokens": ["Ich", "sah", "mich", "satt", "da\u00b7ran", ",", "viel", "lie\u00b7be", "lan\u00b7ge", "Stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PAV", "$,", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann stieg ich h\u00f6her an, als jene Lust geschwunden.", "tokens": ["Dann", "stieg", "ich", "h\u00f6\u00b7her", "an", ",", "als", "je\u00b7ne", "Lust", "ge\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KOUS", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich stieg auf einen Berg, der sich vor mir erhoben;", "tokens": ["Ich", "stieg", "auf", "ei\u00b7nen", "Berg", ",", "der", "sich", "vor", "mir", "er\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und wieder auf mein Thal schau' ich herab von oben.", "tokens": ["Und", "wie\u00b7der", "auf", "mein", "Thal", "schau'", "ich", "her\u00b7ab", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es ist dasselbe noch, und ist ein andres doch,", "tokens": ["Es", "ist", "das\u00b7sel\u00b7be", "noch", ",", "und", "ist", "ein", "and\u00b7res", "doch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "ADV", "$,", "KON", "VAFIN", "ART", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich seh es ganz, und seh dazu viel andres noch", "tokens": ["Ich", "seh", "es", "ganz", ",", "und", "seh", "da\u00b7zu", "viel", "and\u00b7res", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "PAV", "ADV", "PIS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nun laben will ich mich am neuen Blick mit Schweigen,", "tokens": ["Nun", "la\u00b7ben", "will", "ich", "mich", "am", "neu\u00b7en", "Blick", "mit", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eine Stufe dann vielleicht noch h\u00f6her steigen.", "tokens": ["Und", "ei\u00b7ne", "Stu\u00b7fe", "dann", "viel\u00b7leicht", "noch", "h\u00f6\u00b7her", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}