{"textgrid.poem.50237": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "15. Auff Diophantes", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil sein grossm\u00fcthig Hertz, das allen stoltzen Pracht", "tokens": ["Weil", "sein", "gross\u00b7m\u00fct\u00b7hig", "Hertz", ",", "das", "al\u00b7len", "stolt\u00b7zen", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJD", "NN", "$,", "PRELS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Erden nichtig h\u00e4lt, ein jeder nam in acht';", "tokens": ["Der", "Er\u00b7den", "nich\u00b7tig", "h\u00e4lt", ",", "ein", "je\u00b7der", "nam", "in", "acht'", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "ART", "PIS", "VVFIN", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So liess man ihn ", "tokens": ["So", "liess", "man", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Da er ", "tokens": ["Da", "er"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}}}}}