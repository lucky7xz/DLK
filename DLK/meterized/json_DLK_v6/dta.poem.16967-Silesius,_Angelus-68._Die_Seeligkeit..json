{"dta.poem.16967": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "68.  Die Seeligkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Seeligkeit? Ein zuflu\u00df aller Freuden;", "tokens": ["Was", "ist", "die", "See\u00b7lig\u00b7keit", "?", "Ein", "zu\u00b7flu\u00df", "al\u00b7ler", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "PIAT", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ein st\u00e4tes anschaun Gotts; Ein lieben ohn Verdru\u00df;", "tokens": ["Ein", "st\u00e4\u00b7tes", "an\u00b7schaun", "Gotts", ";", "Ein", "lie\u00b7ben", "ohn", "Ver\u00b7dru\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$.", "ART", "ADJA", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Leben ohne Tod; Ein s\u00fcsser ", "tokens": ["Ein", "Le\u00b7ben", "oh\u00b7ne", "Tod", ";", "Ein", "s\u00fcs\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht einen Augenblik vom Br\u00e4utigam seyn gescheiden.", "tokens": ["Nicht", "ei\u00b7nen", "Au\u00b7gen\u00b7blik", "vom", "Br\u00e4u\u00b7ti\u00b7gam", "seyn", "ge\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPRART", "NE", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}