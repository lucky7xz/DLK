{"textgrid.poem.46518": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[in Gesichten und Gedichten]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Gesichten und Gedichten", "tokens": ["In", "Ge\u00b7sich\u00b7ten", "und", "Ge\u00b7dich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mir Sch\u00f6nstes je erschienen,", "tokens": ["Was", "mir", "Sch\u00f6ns\u00b7tes", "je", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Habt ihr alles \u00fcbersch\u00f6net;", "tokens": ["Habt", "ihr", "al\u00b7les", "\u00fc\u00b7bersc\u00b7h\u00f6\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich staunte, da\u00df ich lebend", "tokens": ["Und", "ich", "staun\u00b7te", ",", "da\u00df", "ich", "le\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollt' in euch vor Augen sehen,", "tokens": ["Sollt'", "in", "euch", "vor", "Au\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich nur geglaubt, es lebe", "tokens": ["Was", "ich", "nur", "ge\u00b7glaubt", ",", "es", "le\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In Gedichten und Gesichten.", "tokens": ["In", "Ge\u00b7dich\u00b7ten", "und", "Ge\u00b7sich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus Gedichten und Gesichten", "tokens": ["Aus", "Ge\u00b7dich\u00b7ten", "und", "Ge\u00b7sich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df wie Bilder aus dem Rahmen", "tokens": ["Da\u00df", "wie", "Bil\u00b7der", "aus", "dem", "Rah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr heraus ins Leben tretet,", "tokens": ["Ihr", "he\u00b7raus", "ins", "Le\u00b7ben", "tre\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat mich immer so gewundert,", "tokens": ["Hat", "mich", "im\u00b7mer", "so", "ge\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df es nun mich wundert minder,", "tokens": ["Da\u00df", "es", "nun", "mich", "wun\u00b7dert", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ich sehn mu\u00df, da\u00df den Augen", "tokens": ["Wie", "ich", "sehn", "mu\u00df", ",", "da\u00df", "den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwindend ihr zur\u00fcck euch wandelt", "tokens": ["Schwin\u00b7dend", "ihr", "zu\u00b7r\u00fcck", "euch", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "PTKVZ", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu Gesichten und Gedichten.", "tokens": ["Zu", "Ge\u00b7sich\u00b7ten", "und", "Ge\u00b7dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}