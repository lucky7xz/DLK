{"textgrid.poem.52718": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Eleonore", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grausamer Tod!", "tokens": ["Grau\u00b7sa\u00b7mer", "Tod", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "In deine gl\u00fchenden Arme", "tokens": ["In", "dei\u00b7ne", "gl\u00fc\u00b7hen\u00b7den", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "St\u00fcrz ich ein verlorenes Weib.", "tokens": ["St\u00fcrz", "ich", "ein", "ver\u00b7lo\u00b7re\u00b7nes", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Aber noch hei\u00dfer,", "tokens": ["A\u00b7ber", "noch", "hei\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Aber noch brennender", "tokens": ["A\u00b7ber", "noch", "bren\u00b7nen\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADJA"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ist der Schmerz um den entseelten Gatten,", "tokens": ["Ist", "der", "Schmerz", "um", "den", "ent\u00b7seel\u00b7ten", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Als du,", "tokens": ["Als", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Der aus Flammen lodert.", "tokens": ["Der", "aus", "Flam\u00b7men", "lo\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "So sprach des Hindu lieberf\u00fcllte", "tokens": ["So", "sprach", "des", "Hin\u00b7du", "lie\u00b7ber\u00b7f\u00fcll\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gattin.", "tokens": ["Gat\u00b7tin", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ich sage: sie kannte Liebe.", "tokens": ["Ich", "sa\u00b7ge", ":", "sie", "kann\u00b7te", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie f\u00fchlte,", "tokens": ["Sie", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und ihr Gef\u00fchl", "tokens": ["Und", "ihr", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "War keine S\u00fcnde.", "tokens": ["War", "kei\u00b7ne", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}