{"dta.poem.8495": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Meine Steckenpferde, die Uhren.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O die Bev\u00f6lkerung &#252;berall!", "tokens": ["O", "die", "Be\u00b7v\u00f6l\u00b7ke\u00b7rung", "&#252;", "be\u00b7rall", "!"], "token_info": ["word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "ART", "NN", "$(", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "O unsre gesegneten Zeiten!", "tokens": ["O", "uns\u00b7re", "ge\u00b7se\u00b7gne\u00b7ten", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "In Ro\u00dfpall\u00e4sten und Menschenstall", "tokens": ["In", "Ro\u00df\u00b7pal\u00b7l\u00e4s\u00b7ten", "und", "Men\u00b7schen\u00b7stall"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie Flocken sieht man es gleiten;", "tokens": ["Wie", "Flo\u00b7cken", "sieht", "man", "es", "glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Von Bettlern wimmelt das ganze Land,", "tokens": ["Von", "Bett\u00b7lern", "wim\u00b7melt", "das", "gan\u00b7ze", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von K\u00fcnstlergesindel die Erde;", "tokens": ["Von", "K\u00fcnst\u00b7ler\u00b7ge\u00b7sin\u00b7del", "die", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Doch keine Race nahm \u00fcberhand,", "tokens": ["Doch", "kei\u00b7ne", "Ra\u00b7ce", "nahm", "\u00fc\u00b7ber\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wie jene der Steckenpferde.", "tokens": ["Wie", "je\u00b7ne", "der", "Ste\u00b7cken\u00b7pfer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Eine reitet den Zernebock,", "tokens": ["Der", "Ei\u00b7ne", "rei\u00b7tet", "den", "Zer\u00b7ne\u00b7bock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Andre, Himmel! den G\u00f6the,", "tokens": ["Der", "And\u00b7re", ",", "Him\u00b7mel", "!", "den", "G\u00f6\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Jener sprengt \u00fcber Stein und Stock", "tokens": ["Und", "Je\u00b7ner", "sprengt", "\u00fc\u00b7ber", "Stein", "und", "Stock"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf einer alten Muskete.", "tokens": ["Auf", "ei\u00b7ner", "al\u00b7ten", "Mus\u00b7ke\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Ein Tonnenbacher rutscht dieser mit", "tokens": ["Ein", "Ton\u00b7nen\u00b7ba\u00b7cher", "rutscht", "die\u00b7ser", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PDAT", "APPR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auf hochgetriebnem Pokale,", "tokens": ["Auf", "hoch\u00b7ge\u00b7trieb\u00b7nem", "Po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und Jener macht den bedenklichen Ritt", "tokens": ["Und", "Je\u00b7ner", "macht", "den", "be\u00b7denk\u00b7li\u00b7chen", "Ritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Auf einem elektrischen Aale.", "tokens": ["Auf", "ei\u00b7nem", "e\u00b7lekt\u00b7ri\u00b7schen", "Aa\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das war vor Zeiten ein anderes Ding:", "tokens": ["Das", "war", "vor", "Zei\u00b7ten", "ein", "an\u00b7de\u00b7res", "Ding", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kam mal \u2019ne M\u00f6ve geflogen,", "tokens": ["Kam", "mal", "'ne", "M\u00f6\u00b7ve", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Fing einer im Flor den Schmetterling,", "tokens": ["Fing", "ei\u00b7ner", "im", "Flor", "den", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schier h\u00e4tt\u2019 man die Glocken gezogen,", "tokens": ["Schier", "h\u00e4tt'", "man", "die", "Glo\u00b7cken", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und wer vom Pegasus nur getr\u00e4umt,", "tokens": ["Und", "wer", "vom", "Pe\u00b7ga\u00b7sus", "nur", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "De\u00df staunten Freund\u2019 und Verwandte;", "tokens": ["De\u00df", "staun\u00b7ten", "Freund'", "und", "Ver\u00b7wand\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Jetzt steht im Narrenstalle gez\u00e4umt", "tokens": ["Jetzt", "steht", "im", "Nar\u00b7ren\u00b7stal\u00b7le", "ge\u00b7z\u00e4umt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "F\u00fcr Jeden die Rozinante.", "tokens": ["F\u00fcr", "Je\u00b7den", "die", "Ro\u00b7zi\u00b7nan\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Meine Steckenpferde sind glatt und rund,", "tokens": ["Mei\u00b7ne", "Ste\u00b7cken\u00b7pfer\u00b7de", "sind", "glatt", "und", "rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sind blank gef\u00fctterte Schimmel,", "tokens": ["Sind", "blank", "ge\u00b7f\u00fct\u00b7ter\u00b7te", "Schim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr Trab ein Fl\u00fcstern von Frauenmund,", "tokens": ["Ihr", "Trab", "ein", "Fl\u00fcs\u00b7tern", "von", "Frau\u00b7en\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr Wiehern ein zartes Gebimmel.", "tokens": ["Ihr", "Wie\u00b7hern", "ein", "zar\u00b7tes", "Ge\u00b7bim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Dort sprangen sie an der Longe hinaus!", "tokens": ["Dort", "spran\u00b7gen", "sie", "an", "der", "Lon\u00b7ge", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Meine Silbergrauen und Fahlen,", "tokens": ["Mei\u00b7ne", "Sil\u00b7ber\u00b7grau\u00b7en", "und", "Fah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Sechs Kreuzer dem, der sie lobt zu Haus,", "tokens": ["Sechs", "Kreu\u00b7zer", "dem", ",", "der", "sie", "lobt", "zu", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und zw\u00f6lf, der sie lobt in Journalen!", "tokens": ["Und", "zw\u00f6lf", ",", "der", "sie", "lobt", "in", "Jour\u00b7na\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}