{"dta.poem.17572": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "194.  Aller Heiligen Werke sind nur ein  \n Wert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Was alle Heilgen thun/ das kan ein Mensch allein:", "tokens": ["Was", "al\u00b7le", "Heil\u00b7gen", "thun", "/", "das", "kan", "ein", "Mensch", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVINF", "$(", "PDS", "VMFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja? schau sie thun sonst nichts als GOtt gelassen seyn.", "tokens": ["Ja", "?", "schau", "sie", "thun", "sonst", "nichts", "als", "Gott", "ge\u00b7las\u00b7sen", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PPER", "VVFIN", "ADV", "PIS", "KOKOM", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}