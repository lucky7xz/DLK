{"textgrid.poem.32436": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "115. Der Schwur", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schw\u00f6re Lalagen, da\u00df sonder ihre K\u00fcsse,", "tokens": ["Ich", "schw\u00f6\u00b7re", "La\u00b7la\u00b7gen", ",", "da\u00df", "son\u00b7der", "ih\u00b7re", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein k\u00f6nigliches Gl\u00fcck mein Leben mir vers\u00fc\u00dfe.", "tokens": ["Kein", "k\u00f6\u00b7nig\u00b7li\u00b7ches", "Gl\u00fcck", "mein", "Le\u00b7ben", "mir", "ver\u00b7s\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies schw\u00f6r' ich ihr im Ernst, wofern sie sich ergibt;", "tokens": ["Dies", "schw\u00f6r'", "ich", "ihr", "im", "Ernst", ",", "wo\u00b7fern", "sie", "sich", "er\u00b7gibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schw\u00f6r' es ihr im Scherz, wofern sie mich nicht liebt.", "tokens": ["Und", "schw\u00f6r'", "es", "ihr", "im", "Scherz", ",", "wo\u00b7fern", "sie", "mich", "nicht", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}