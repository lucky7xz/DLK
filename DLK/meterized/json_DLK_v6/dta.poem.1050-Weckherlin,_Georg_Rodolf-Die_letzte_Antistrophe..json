{"dta.poem.1050": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die letzte Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hingegen will au\u00df sondern gnaden", "tokens": ["Hin\u00b7ge\u00b7gen", "will", "au\u00df", "son\u00b7dern", "gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der himmel der erden zugut/", "tokens": ["Der", "him\u00b7mel", "der", "er\u00b7den", "zu\u00b7gut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Au\u00df dises F\u00fcrsten hohem blut", "tokens": ["Au\u00df", "di\u00b7ses", "F\u00fcrs\u00b7ten", "ho\u00b7hem", "blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich mit s\u00fcssen fr\u00fcchten beladen:", "tokens": ["Dich", "mit", "s\u00fcs\u00b7sen", "fr\u00fcch\u00b7ten", "be\u00b7la\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "In dem au\u00df deinem edlen leib", "tokens": ["In", "dem", "au\u00df", "dei\u00b7nem", "ed\u00b7len", "leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entspringen sollen grosse Printzen", "tokens": ["Ent\u00b7sprin\u00b7gen", "sol\u00b7len", "gros\u00b7se", "Print\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Deren tugent mancher Provintzen", "tokens": ["De\u00b7ren", "tu\u00b7gent", "man\u00b7cher", "Pro\u00b7vint\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Erl\u00f6sung vnd besch\u00fctzung pleib:", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "vnd", "be\u00b7sch\u00fct\u00b7zung", "pleib", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der gro\u00df adler sich zu er g\u00f6tzen", "tokens": ["Der", "gro\u00df", "ad\u00b7ler", "sich", "zu", "er", "g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "PRF", "APPR", "PPER", "VVINF"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.10": {"text": "W\u00fcrt sich gern auf jhren schilt setzen.", "tokens": ["W\u00fcrt", "sich", "gern", "auf", "jhren", "schilt", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Alsdan soll jhr gerechter Zorn", "tokens": ["Als\u00b7dan", "soll", "jhr", "ge\u00b7rech\u00b7ter", "Zorn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Des Monds zwayspitzig stoltzes horn/", "tokens": ["Des", "Monds", "zway\u00b7spit\u00b7zig", "stolt\u00b7zes", "horn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wie auch den getriplierten Cronen", "tokens": ["Wie", "auch", "den", "ge\u00b7tri\u00b7plier\u00b7ten", "Cro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.14": {"text": "In beedem C", "tokens": ["In", "bee\u00b7dem", "C"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Zu des H\u00f6chsten ehr nicht verschonen", "tokens": ["Zu", "des", "H\u00f6chs\u00b7ten", "ehr", "nicht", "ver\u00b7scho\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKNEG", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Mit jhrer w\u00f6hr vnd lantzen spitz.", "tokens": ["Mit", "jhrer", "w\u00f6hr", "vnd", "lant\u00b7zen", "spitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}