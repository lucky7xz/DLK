{"textgrid.poem.45157": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in das Stammbuch des Fr\u00e4uleins Emilie Baronin von Schlechta]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wardst als Braut dereinst mir zugesagt,", "tokens": ["Du", "wardst", "als", "Braut", "de\u00b7reinst", "mir", "zu\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch ward die Hochzeit etwas noch vertagt,", "tokens": ["Doch", "ward", "die", "Hoch\u00b7zeit", "et\u00b7was", "noch", "ver\u00b7tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil ich nicht alt zwar, aber du viel j\u00fcnger,", "tokens": ["Weil", "ich", "nicht", "alt", "zwar", ",", "a\u00b7ber", "du", "viel", "j\u00fcn\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "ADV", "$,", "KON", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht gr\u00f6\u00dfer kaum, als jetzt dein Zeigefinger.", "tokens": ["Nicht", "gr\u00f6\u00b7\u00dfer", "kaum", ",", "als", "jetzt", "dein", "Zei\u00b7ge\u00b7fin\u00b7ger", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "$,", "KOUS", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch ists ein seltsam Ding mit der Grammatik,", "tokens": ["Doch", "ists", "ein", "selt\u00b7sam", "Ding", "mit", "der", "Gram\u00b7ma\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sie schl\u00e4gt oft um, besonders in der Praktik:", "tokens": ["Sie", "schl\u00e4gt", "oft", "um", ",", "be\u00b7son\u00b7ders", "in", "der", "Prak\u00b7tik", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Aus meinem alt ward \u00e4lter, wie die Regel lehrt,", "tokens": ["Aus", "mei\u00b7nem", "alt", "ward", "\u00e4l\u00b7ter", ",", "wie", "die", "Re\u00b7gel", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "VAFIN", "ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du wardst aus j\u00fcnger jung \u2013 gerade umgekehrt;", "tokens": ["Du", "wardst", "aus", "j\u00fcn\u00b7ger", "jung", "\u2013", "ge\u00b7ra\u00b7de", "um\u00b7ge\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und w\u00e4hrend du ein holder Positiv,", "tokens": ["Und", "w\u00e4h\u00b7rend", "du", "ein", "hol\u00b7der", "Po\u00b7si\u00b7tiv", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Scheints, da\u00df die letzte Staffel mir schon rief:", "tokens": ["Scheints", ",", "da\u00df", "die", "letz\u00b7te", "Staf\u00b7fel", "mir", "schon", "rief", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wir wollen drum nicht steigern noch vergleichen,", "tokens": ["Wir", "wol\u00b7len", "drum", "nicht", "stei\u00b7gern", "noch", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "PTKNEG", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und statt dem Bindewort sei uns ein \u2013 Trennungszeichen.", "tokens": ["Und", "statt", "dem", "Bin\u00b7de\u00b7wort", "sei", "uns", "ein", "\u2013", "Tren\u00b7nungs\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ART", "$(", "NN", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}}}}