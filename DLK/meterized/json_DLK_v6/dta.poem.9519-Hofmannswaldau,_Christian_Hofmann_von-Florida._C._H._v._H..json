{"dta.poem.9519": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Florida.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein schiff treibt lufft und wind/ mich treibet lieb und", "tokens": ["Mein", "schiff", "treibt", "lufft", "und", "wind", "/", "mich", "trei\u00b7bet", "lieb", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "VVFIN", "NN", "KON", "NE", "$(", "PPER", "VVFIN", "ADJD", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "brunst/", "tokens": ["brunst", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ich mu\u00df in Florida den steiffen ancker sencken/", "tokens": ["Ich", "mu\u00df", "in", "Flo\u00b7ri\u00b7da", "den", "steif\u00b7fen", "an\u00b7cker", "sen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NE", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beseegel ich die see vergebens und umsonst/", "tokens": ["Be\u00b7see\u00b7gel", "ich", "die", "see", "ver\u00b7ge\u00b7bens", "und", "um\u00b7sonst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Soll ich denn ohne frucht das schwere ruder lencken?", "tokens": ["Soll", "ich", "denn", "oh\u00b7ne", "frucht", "das", "schwe\u00b7re", "ru\u00b7der", "len\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gold/ perlen/ helffenbein begehrt mein hertze nicht/", "tokens": ["Gold", "/", "per\u00b7len", "/", "helf\u00b7fen\u00b7bein", "be\u00b7gehrt", "mein", "hert\u00b7ze", "nicht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVINF", "$(", "ADJD", "VVPP", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das leere Florida soll mir die augen f\u00fcllen/", "tokens": ["Das", "lee\u00b7re", "Flo\u00b7ri\u00b7da", "soll", "mir", "die", "au\u00b7gen", "f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ob dem lande gleich der diamant gebricht/", "tokens": ["Und", "ob", "dem", "lan\u00b7de", "gleich", "der", "di\u00b7a\u00b7mant", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So ist es doch genung mir meine brunst zu stillen.", "tokens": ["So", "ist", "es", "doch", "ge\u00b7nung", "mir", "mei\u00b7ne", "brunst", "zu", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da soll mein wohnhau\u00df seyn/ da sollen leib und geist", "tokens": ["Da", "soll", "mein", "wohn\u00b7hau\u00df", "seyn", "/", "da", "sol\u00b7len", "leib", "und", "geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJD", "VAINF", "$(", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In h\u00f6chster freundlichkeit zusammen sich erg\u00f6tzen/", "tokens": ["In", "h\u00f6chs\u00b7ter", "freund\u00b7lich\u00b7keit", "zu\u00b7sam\u00b7men", "sich", "er\u00b7g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da will ich/ wann und wie es das verh\u00e4ngn\u00fc\u00df heist/", "tokens": ["Da", "will", "ich", "/", "wann", "und", "wie", "es", "das", "ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "heist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$(", "PWAV", "KON", "PWAV", "PPER", "PDS", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mich in die grosse zahl der todten lassen setzen.", "tokens": ["Mich", "in", "die", "gros\u00b7se", "zahl", "der", "tod\u00b7ten", "las\u00b7sen", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch weil so manches schiff auff dieser reise bleibt/", "tokens": ["Doch", "weil", "so", "man\u00b7ches", "schiff", "auff", "die\u00b7ser", "rei\u00b7se", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "VVFIN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da alles ist umzirckt mit klippen und mit steinen/", "tokens": ["Da", "al\u00b7les", "ist", "um\u00b7zirckt", "mit", "klip\u00b7pen", "und", "mit", "stei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "VVPP", "APPR", "VVINF", "KON", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So ruff ich Venus an/ da\u00df sie die wellen treibt/", "tokens": ["So", "ruff", "ich", "Ve\u00b7nus", "an", "/", "da\u00df", "sie", "die", "wel\u00b7len", "treibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$(", "KOUS", "PPER", "ART", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und vor den steuermann mir sendet ihren kleinen.", "tokens": ["Und", "vor", "den", "steu\u00b7er\u00b7mann", "mir", "sen\u00b7det", "ih\u00b7ren", "klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bringt Venus mich an port/ und setzet mich ans land/", "tokens": ["Bringt", "Ve\u00b7nus", "mich", "an", "port", "/", "und", "set\u00b7zet", "mich", "ans", "land", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "APPR", "NE", "$(", "KON", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So will ich t\u00e4glich mich zu ihrem tempel f\u00fcgen/", "tokens": ["So", "will", "ich", "t\u00e4g\u00b7lich", "mich", "zu", "ih\u00b7rem", "tem\u00b7pel", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und ich verspreche ihr mit sinnen/ hertz und hand/", "tokens": ["Und", "ich", "ver\u00b7spre\u00b7che", "ihr", "mit", "sin\u00b7nen", "/", "hertz", "und", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "VVINF", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df ich ins k\u00fcnfftig will auff blo\u00dfer erde liegen.", "tokens": ["Da\u00df", "ich", "ins", "k\u00fcnff\u00b7tig", "will", "auff", "blo\u00b7\u00dfer", "er\u00b7de", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJD", "VMFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}