{"textgrid.poem.40564": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Turnierplatz einer Tenne,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Turnierplatz einer Tenne,", "tokens": ["Im", "Tur\u00b7nier\u00b7platz", "ei\u00b7ner", "Ten\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf dem Thron von Schobern, Scheitern,", "tokens": ["Auf", "dem", "Thron", "von", "Scho\u00b7bern", ",", "Schei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzt in Anmut Jungfrau Henne,", "tokens": ["Sitzt", "in", "An\u00b7mut", "Jung\u00b7frau", "Hen\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Richtend zwischen zweien Streitern.", "tokens": ["Rich\u00b7tend", "zwi\u00b7schen", "zwei\u00b7en", "Strei\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, es hat ihr sittsam Gackern,", "tokens": ["Ach", ",", "es", "hat", "ihr", "sitt\u00b7sam", "Ga\u00b7ckern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr jungfr\u00e4ulich sittsam Schreiten", "tokens": ["Ihr", "jung\u00b7fr\u00e4u\u00b7lich", "sitt\u00b7sam", "Schrei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJD", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Liebentflammt die beiden Wackern,", "tokens": ["Lie\u00b7bent\u00b7flammt", "die", "bei\u00b7den", "Wa\u00b7ckern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die um ihren Preis nun streiten.", "tokens": ["Die", "um", "ih\u00b7ren", "Preis", "nun", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Welcher ist's, den sie erkoren,", "tokens": ["Wel\u00b7cher", "ist's", ",", "den", "sie", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem sie weiht die gleiche Flamme?", "tokens": ["Dem", "sie", "weiht", "die", "glei\u00b7che", "Flam\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldhahn mit den schmucken Sporen?", "tokens": ["Gold\u00b7hahn", "mit", "den", "schmu\u00b7cken", "Spo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwarzhahn mit dem sch\u00f6nen Kamme?", "tokens": ["Schwarz\u00b7hahn", "mit", "dem", "sch\u00f6\u00b7nen", "Kam\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Goldhahn ist ein stolzer Ritter,", "tokens": ["Gold\u00b7hahn", "ist", "ein", "stol\u00b7zer", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt ein Wamms orangenfarben,", "tokens": ["Tr\u00e4gt", "ein", "Wamms", "o\u00b7ran\u00b7gen\u00b7far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldnen Panzer, bunte Flitter,", "tokens": ["Gold\u00b7nen", "Pan\u00b7zer", ",", "bun\u00b7te", "Flit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fcner Federn voll Garben!", "tokens": ["Gr\u00fc\u00b7ner", "Fe\u00b7dern", "voll", "Gar\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Siegbewu\u00dft im Selbstgefallen", "tokens": ["Sieg\u00b7be\u00b7wu\u00dft", "im", "Selbst\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht der Stutzer ganz verloren,", "tokens": ["Steht", "der", "Stut\u00b7zer", "ganz", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der Maid zumeist vor Allem", "tokens": ["Doch", "der", "Maid", "zu\u00b7meist", "vor", "Al\u00b7lem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traun, behagen seine Sporen.", "tokens": ["Traun", ",", "be\u00b7ha\u00b7gen", "sei\u00b7ne", "Spo\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schwarzhahn prunkt nicht also eitel!", "tokens": ["Schwarz\u00b7hahn", "prunkt", "nicht", "al\u00b7so", "ei\u00b7tel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Melancholikus von Hause,", "tokens": ["Me\u00b7lan\u00b7cho\u00b7li\u00b7kus", "von", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einfach schwarz vom Fu\u00df zum Scheitel", "tokens": ["Ein\u00b7fach", "schwarz", "vom", "Fu\u00df", "zum", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4gt er Mantel, R\u00fcstung, Krause.", "tokens": ["Tr\u00e4gt", "er", "Man\u00b7tel", ",", "R\u00fcs\u00b7tung", ",", "Krau\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Seufzend mit gesenkten Blicken", "tokens": ["Seuf\u00b7zend", "mit", "ge\u00b7senk\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Birgt er in sich seine Flamme,", "tokens": ["Birgt", "er", "in", "sich", "sei\u00b7ne", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Dame fand Entz\u00fccken", "tokens": ["Doch", "die", "Da\u00b7me", "fand", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem sch\u00f6nen rothen Kamme.", "tokens": ["An", "dem", "sch\u00f6\u00b7nen", "ro\u00b7then", "Kam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Horch, Trompetenst\u00f6\u00dfe kr\u00e4hen!", "tokens": ["Horch", ",", "Trom\u00b7pe\u00b7ten\u00b7st\u00f6\u00b7\u00dfe", "kr\u00e4\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf zum Kampf, ihr tapfern Ritter!", "tokens": ["Auf", "zum", "Kampf", ",", "ihr", "tap\u00b7fern", "Rit\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4ubend in den L\u00fcften wehen", "tokens": ["St\u00e4u\u00b7bend", "in", "den", "L\u00fcf\u00b7ten", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Federn statt der Lanzensplitter.", "tokens": ["Fe\u00b7dern", "statt", "der", "Lan\u00b7zen\u00b7split\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie sie an einander springen,", "tokens": ["Wie", "sie", "an", "ein\u00b7an\u00b7der", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grimmig mit den Fl\u00fcgeln schlagen,", "tokens": ["Grim\u00b7mig", "mit", "den", "Fl\u00fc\u00b7geln", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Blick und Kralle ringen,", "tokens": ["Und", "mit", "Blick", "und", "Kral\u00b7le", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Degengleich die Schn\u00e4bel tragen!", "tokens": ["De\u00b7gen\u00b7gleich", "die", "Schn\u00e4\u00b7bel", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Weh', ein Kleinod hat verloren", "tokens": ["Weh'", ",", "ein", "Klei\u00b7nod", "hat", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder in des Kampfes Flamme,", "tokens": ["Je\u00b7der", "in", "des", "Kamp\u00b7fes", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldhahn seine sch\u00f6nen Sporen,", "tokens": ["Gold\u00b7hahn", "sei\u00b7ne", "sch\u00f6\u00b7nen", "Spo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwarzhahn ein gut St\u00fcck vom Kamme!", "tokens": ["Schwarz\u00b7hahn", "ein", "gut", "St\u00fcck", "vom", "Kam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und die Dame steht unschl\u00fcssig,", "tokens": ["Und", "die", "Da\u00b7me", "steht", "un\u00b7schl\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer zum Siegespreis zu w\u00e4hlen?", "tokens": ["Wer", "zum", "Sie\u00b7ge\u00b7spreis", "zu", "w\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwarzhahn, der des Kammes m\u00fcssig?", "tokens": ["Schwarz\u00b7hahn", ",", "der", "des", "Kam\u00b7mes", "m\u00fcs\u00b7sig", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Goldhahn, dem die Sporen fehlen?", "tokens": ["Gold\u00b7hahn", ",", "dem", "die", "Spo\u00b7ren", "feh\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}