{"textgrid.poem.64905": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn du dich nimmer willst von Gott verlassen w\u00e4hnen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du dich nimmer willst von Gott verlassen w\u00e4hnen,", "tokens": ["Wenn", "du", "dich", "nim\u00b7mer", "willst", "von", "Gott", "ver\u00b7las\u00b7sen", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VMFIN", "APPR", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So zwing' ihn zu dir her, durch deines Herzens Sehnen.", "tokens": ["So", "zwing'", "ihn", "zu", "dir", "her", ",", "durch", "dei\u00b7nes", "Her\u00b7zens", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verschm\u00e4h der Erde Gl\u00fcck, lern ihre Qual besiegen,", "tokens": ["Ver\u00b7schm\u00e4h", "der", "Er\u00b7de", "Gl\u00fcck", ",", "lern", "ih\u00b7re", "Qual", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kannst du wandellos im Arme Gottes liegen.", "tokens": ["So", "kannst", "du", "wan\u00b7del\u00b7los", "im", "Ar\u00b7me", "Got\u00b7tes", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O wirf in diesen Brand dein Denken, Trachten, Sinnen,", "tokens": ["O", "wirf", "in", "die\u00b7sen", "Brand", "dein", "Den\u00b7ken", ",", "Trach\u00b7ten", ",", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PDAT", "NN", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wolle f\u00fcr den Schein die Wesenheit gewinnen.", "tokens": ["Und", "wol\u00b7le", "f\u00fcr", "den", "Schein", "die", "We\u00b7sen\u00b7heit", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}