{"dta.poem.18461": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der breit- und schmale weg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein breiter steg", "tokens": ["Es", "ist", "ein", "brei\u00b7ter", "steg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und wohlgebahnter weg/", "tokens": ["Und", "wohl\u00b7ge\u00b7bahn\u00b7ter", "weg", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der zum verderben f\u00fchret/", "tokens": ["Der", "zum", "ver\u00b7der\u00b7ben", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den einer/ so doch blind/", "tokens": ["Den", "ei\u00b7ner", "/", "so", "doch", "blind", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$(", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ohn allen ansto\u00df sindt/", "tokens": ["Ohn", "al\u00b7len", "an\u00b7sto\u00df", "sindt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ohn alles f\u00fchlen sp\u00fchret.", "tokens": ["Ohn", "al\u00b7les", "f\u00fch\u00b7len", "sp\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der den die tugend kennt/", "tokens": ["Der", "den", "die", "tu\u00b7gend", "kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wird allzeit schmal genennt/", "tokens": ["Wird", "all\u00b7zeit", "schmal", "ge\u00b7nennt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Kein mensch kan selben finden/", "tokens": ["Kein", "mensch", "kan", "sel\u00b7ben", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Als der der tugend freund/", "tokens": ["Als", "der", "der", "tu\u00b7gend", "freund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der laster \u00e4rgster feind", "tokens": ["Der", "las\u00b7ter", "\u00e4rgs\u00b7ter", "feind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und nicht geblendt von s\u00fcnden.", "tokens": ["Und", "nicht", "ge\u00b7blendt", "von", "s\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}