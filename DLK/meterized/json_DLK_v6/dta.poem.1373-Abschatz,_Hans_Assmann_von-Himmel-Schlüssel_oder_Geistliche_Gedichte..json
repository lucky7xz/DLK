{"dta.poem.1373": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit l\u00e4st ihre Flucht uns t\u00e4g- und st\u00fcndlich schauen/", "tokens": ["Die", "Zeit", "l\u00e4st", "ih\u00b7re", "Flucht", "uns", "t\u00e4g", "und", "st\u00fcnd\u00b7lich", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "TRUNC", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dennoch wollen wir auff ihre L\u00e4nge bauen:", "tokens": ["Und", "den\u00b7noch", "wol\u00b7len", "wir", "auff", "ih\u00b7re", "L\u00e4n\u00b7ge", "bau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Sclave fremder Guust stirbt/ eh er ihm gelebt/", "tokens": ["Ein", "Scla\u00b7ve", "frem\u00b7der", "Guust", "stirbt", "/", "eh", "er", "ihm", "ge\u00b7lebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$(", "KOUS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Knecht der blinden Lieb/ ein Diener seiner G\u00fctter/", "tokens": ["Ein", "Knecht", "der", "blin\u00b7den", "Lieb", "/", "ein", "Die\u00b7ner", "sei\u00b7ner", "G\u00fct\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Macht ihm sein Leben selbst durch Furcht und Hoffnung bit-", "tokens": ["Macht", "ihm", "sein", "Le\u00b7ben", "selbst", "durch", "Furcht", "und", "Hoff\u00b7nung", "bit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "ter.", "tokens": ["ter", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Wer k\u00fchner Hitze voll/ nach fremden Blutte strebt/", "tokens": ["Wer", "k\u00fch\u00b7ner", "Hit\u00b7ze", "voll", "/", "nach", "frem\u00b7den", "Blut\u00b7te", "strebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ADJD", "$(", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sucht offters vor der Zeit sein eignes zu verlieren/", "tokens": ["Sucht", "off\u00b7ters", "vor", "der", "Zeit", "sein", "eig\u00b7nes", "zu", "ver\u00b7lie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PPOSAT", "ADJA", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer sich den schn\u00f6den Dunst der Ehre l\u00e4st verf\u00fchren/", "tokens": ["Wer", "sich", "den", "schn\u00f6\u00b7den", "Dunst", "der", "Eh\u00b7re", "l\u00e4st", "ver\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dient vor ein Gauckel-Spiel des Gl\u00fcckes und der Zeit.", "tokens": ["Dient", "vor", "ein", "Gau\u00b7ckel\u00b7Spiel", "des", "Gl\u00fc\u00b7ckes", "und", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was trachtet ihr denn viel/ ihr B\u00fcrger dieser Erden/", "tokens": ["Was", "trach\u00b7tet", "ihr", "denn", "viel", "/", "ihr", "B\u00fcr\u00b7ger", "die\u00b7ser", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$(", "PPOSAT", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Beg\u00fcnstigt/ reich/ geliebt/ gef\u00fcrcht und hoch zu werden?", "tokens": ["Be\u00b7g\u00fcns\u00b7tigt", "/", "reich", "/", "ge\u00b7liebt", "/", "ge\u00b7f\u00fcrcht", "und", "hoch", "zu", "wer\u00b7den", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "ADJD", "$(", "VVPP", "$(", "VVPP", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn ihr doch Erd aus Erd und Staub und Asche seyd.", "tokens": ["Wenn", "ihr", "doch", "Erd", "aus", "Erd", "und", "Staub", "und", "A\u00b7sche", "seyd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}