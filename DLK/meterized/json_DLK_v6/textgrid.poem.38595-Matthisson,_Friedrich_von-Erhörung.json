{"textgrid.poem.38595": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erh\u00f6rung", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich bl\u00fchte der Knospenkranz der Hoffnung", "tokens": ["End\u00b7lich", "bl\u00fch\u00b7te", "der", "Knos\u00b7pen\u00b7kranz", "der", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und es gl\u00e4nzte die Wolk', auf der sie schwebte,", "tokens": ["Und", "es", "gl\u00e4nz\u00b7te", "die", "Wol\u00b7k'", ",", "auf", "der", "sie", "schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gleich dem Schleier Aurorens, bei des Maitags", "tokens": ["Gleich", "dem", "Schlei\u00b7er", "Au\u00b7ro\u00b7rens", ",", "bei", "des", "Mai\u00b7tags"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NE", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Leisem Erwachen.", "tokens": ["Lei\u00b7sem", "Er\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Endlich sank ich ans Herz der Auserkohrnen!", "tokens": ["End\u00b7lich", "sank", "ich", "ans", "Herz", "der", "Au\u00b7ser\u00b7kohr\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da, da schwanden die Eumeniden-Bilder", "tokens": ["Da", ",", "da", "schwan\u00b7den", "die", "Eu\u00b7me\u00b7ni\u00b7den\u00b7Bil\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aus der Trennungen Graun in des Vergessens", "tokens": ["Aus", "der", "Tren\u00b7nun\u00b7gen", "Graun", "in", "des", "Ver\u00b7ges\u00b7sens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ewige Nebel.", "tokens": ["E\u00b7wi\u00b7ge", "Ne\u00b7bel", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Nur des scheidenden Grabes Dunkel schreckt mich!", "tokens": ["Nur", "des", "schei\u00b7den\u00b7den", "Gra\u00b7bes", "Dun\u00b7kel", "schreckt", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "O da\u00df unser Gebein, zu gleicher Stunde,", "tokens": ["O", "da\u00df", "un\u00b7ser", "Ge\u00b7bein", ",", "zu", "glei\u00b7cher", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Erd', Allg\u00fctige, du mit Einem Rasen", "tokens": ["Erd'", ",", "All\u00b7g\u00fc\u00b7ti\u00b7ge", ",", "du", "mit", "Ei\u00b7nem", "Ra\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "$,", "PPER", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "M\u00fctterlich decktest!", "tokens": ["M\u00fct\u00b7ter\u00b7lich", "deck\u00b7test", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}