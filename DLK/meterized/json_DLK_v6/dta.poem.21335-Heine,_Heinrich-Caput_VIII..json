{"dta.poem.21335": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kaiser ist auferstanden seitdem,", "tokens": ["Der", "Kai\u00b7ser", "ist", "auf\u00b7er\u00b7stan\u00b7den", "seit\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch die englischen W\u00fcrmer haben", "tokens": ["Doch", "die", "eng\u00b7li\u00b7schen", "W\u00fcr\u00b7mer", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aus ihm einen stillen Mann gemacht,", "tokens": ["Aus", "ihm", "ei\u00b7nen", "stil\u00b7len", "Mann", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und er lie\u00df sich wieder begraben.", "tokens": ["Und", "er", "lie\u00df", "sich", "wie\u00b7der", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Hab\u2019 selber sein Leichenbeg\u00e4ngni\u00df gesehn,", "tokens": ["Hab'", "sel\u00b7ber", "sein", "Lei\u00b7chen\u00b7be\u00b7g\u00e4ng\u00b7ni\u00df", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich sah den goldenen Wagen", "tokens": ["Ich", "sah", "den", "gol\u00b7de\u00b7nen", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die goldenen Siegesg\u00f6ttinnen drauf,", "tokens": ["Und", "die", "gol\u00b7de\u00b7nen", "Sie\u00b7ges\u00b7g\u00f6t\u00b7tin\u00b7nen", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die den goldenen Sarg getragen.", "tokens": ["Die", "den", "gol\u00b7de\u00b7nen", "Sarg", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Den Elis\u00e4ischen Feldern entlang,", "tokens": ["Den", "E\u00b7li\u00b7s\u00e4\u00b7i\u00b7schen", "Fel\u00b7dern", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch des Triumphes Bogen,", "tokens": ["Durch", "des", "Tri\u00b7um\u00b7phes", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wohl durch den Nebel, wohl \u00fcber den Schnee,", "tokens": ["Wohl", "durch", "den", "Ne\u00b7bel", ",", "wohl", "\u00fc\u00b7ber", "den", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Kam langsam der Zug gezogen.", "tokens": ["Kam", "lang\u00b7sam", "der", "Zug", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Mi\u00dft\u00f6nend schauerlich war die Musik.", "tokens": ["Mi\u00df\u00b7t\u00f6\u00b7nend", "schau\u00b7er\u00b7lich", "war", "die", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Die Musikanten starrten", "tokens": ["Die", "Mu\u00b7si\u00b7kan\u00b7ten", "starr\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor K\u00e4lte. Wehm\u00fcthig gr\u00fc\u00dften mich", "tokens": ["Vor", "K\u00e4l\u00b7te", ".", "Weh\u00b7m\u00fct\u00b7hig", "gr\u00fc\u00df\u00b7ten", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "NE", "VVFIN", "PPER"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die Adler der Standarten.", "tokens": ["Die", "Ad\u00b7ler", "der", "Stand\u00b7ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}