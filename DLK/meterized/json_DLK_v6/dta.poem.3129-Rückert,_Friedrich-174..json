{"dta.poem.3129": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "174.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Aberglauben auch, den ich durchaus nicht preise,", "tokens": ["Den", "A\u00b7berg\u00b7lau\u00b7ben", "auch", ",", "den", "ich", "durc\u00b7haus", "nicht", "prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ehr' ich als einer Zeit und Stufe Glaubensweise.", "tokens": ["Ehr'", "ich", "als", "ei\u00b7ner", "Zeit", "und", "Stu\u00b7fe", "Glau\u00b7bens\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOUS", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo er unschuldig galt, da will ich ihn nicht schelten;", "tokens": ["Wo", "er", "un\u00b7schul\u00b7dig", "galt", ",", "da", "will", "ich", "ihn", "nicht", "schel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In der Erkenntnis Licht kann er nunmehr nicht gelten.", "tokens": ["In", "der", "Er\u00b7kennt\u00b7nis", "Licht", "kann", "er", "nun\u00b7mehr", "nicht", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein ahnungsreicher Traum aus unsrer Kindheit Nacht,", "tokens": ["Ein", "ah\u00b7nungs\u00b7rei\u00b7cher", "Traum", "aus", "uns\u00b7rer", "Kind\u00b7heit", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Mann unbrauchbar, der zum Denken ist erwacht.", "tokens": ["Dem", "Mann", "un\u00b7brauch\u00b7bar", ",", "der", "zum", "Den\u00b7ken", "ist", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Vorzeit M\u00e4rchenstyl, der Phantasie zum Spiel", "tokens": ["Der", "Vor\u00b7zeit", "M\u00e4r\u00b7chens\u00b7tyl", ",", "der", "Phan\u00b7ta\u00b7sie", "zum", "Spiel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu g\u00f6nnen, doch f\u00fcr Geist und Herz kein w\u00fcrdig Ziel,", "tokens": ["Zu", "g\u00f6n\u00b7nen", ",", "doch", "f\u00fcr", "Geist", "und", "Herz", "kein", "w\u00fcr\u00b7dig", "Ziel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "APPR", "NN", "KON", "NN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Scherzhafte Dichtungsart, die wol zum Scherz gef\u00e4llt,", "tokens": ["Scherz\u00b7haf\u00b7te", "Dich\u00b7tungs\u00b7art", ",", "die", "wol", "zum", "Scherz", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch l\u00e4cherlich sich macht, wenn sie sich ernsthaft stellt.", "tokens": ["Doch", "l\u00e4\u00b7cher\u00b7lich", "sich", "macht", ",", "wenn", "sie", "sich", "ernst\u00b7haft", "stellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}