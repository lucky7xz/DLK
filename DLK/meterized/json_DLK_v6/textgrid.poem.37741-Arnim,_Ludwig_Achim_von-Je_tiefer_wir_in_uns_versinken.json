{"textgrid.poem.37741": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Je tiefer wir in uns versinken", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Je tiefer wir in uns versinken,", "tokens": ["Je", "tie\u00b7fer", "wir", "in", "uns", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Je n\u00e4her dringen wir zur H\u00f6lle,", "tokens": ["Je", "n\u00e4\u00b7her", "drin\u00b7gen", "wir", "zur", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald f\u00fchlen wir des Gluthstroms Welle,", "tokens": ["Bald", "f\u00fch\u00b7len", "wir", "des", "Gluth\u00b7stroms", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und m\u00fcssen bald darin vertrinken;", "tokens": ["Und", "m\u00fcs\u00b7sen", "bald", "da\u00b7rin", "ver\u00b7trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er zehrt das Fleisch von unserm Leibe,", "tokens": ["Er", "zehrt", "das", "Fleisch", "von", "un\u00b7serm", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00f6de wird's im Zeitvertreibe,", "tokens": ["Und", "\u00f6\u00b7de", "wird's", "im", "Zeit\u00b7ver\u00b7trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In uns ist Tod!", "tokens": ["In", "uns", "ist", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die Welt ist Gott!", "tokens": ["Die", "Welt", "ist", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "O Mensch, la\u00df nicht vom Menschen los,", "tokens": ["O", "Mensch", ",", "la\u00df", "nicht", "vom", "Men\u00b7schen", "los", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PTKNEG", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ist deine S\u00fcnde noch so gro\u00df,", "tokens": ["Ist", "dei\u00b7ne", "S\u00fcn\u00b7de", "noch", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Meid nur die Sehnsucht nach den S\u00fcnden,", "tokens": ["Meid", "nur", "die", "Sehn\u00b7sucht", "nach", "den", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So kannst du noch viel Gnade finden;", "tokens": ["So", "kannst", "du", "noch", "viel", "Gna\u00b7de", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wer hat die Gnade noch ermessen?", "tokens": ["Wer", "hat", "die", "Gna\u00b7de", "noch", "er\u00b7mes\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Es kann der Mensch so viel vergessen!", "tokens": ["Es", "kann", "der", "Mensch", "so", "viel", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}