{"dta.poem.5312": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hans und Mops.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hans stund des Morgens auf, und Mops sein Hund,", "tokens": ["Hans", "stund", "des", "Mor\u00b7gens", "auf", ",", "und", "Mops", "sein", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADV", "PTKVZ", "$,", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zugleich;", "tokens": ["zu\u00b7gleich", ";"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Hans zog die Kleider an, reckt\u2019 seinen Arm, und g\u00e4hnte;", "tokens": ["Hans", "zog", "die", "Klei\u00b7der", "an", ",", "reckt'", "sei\u00b7nen", "Arm", ",", "und", "g\u00e4hn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mops reckte, sch\u00fcttelt\u2019 sich, und dehnte", "tokens": ["Mops", "reck\u00b7te", ",", "sch\u00fct\u00b7telt'", "sich", ",", "und", "dehn\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PRF", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht minder alle vier; gebacknen weissen Teig", "tokens": ["Nicht", "min\u00b7der", "al\u00b7le", "vier", ";", "ge\u00b7back\u00b7nen", "weis\u00b7sen", "Teig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PIAT", "CARD", "$.", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "A\u00df Hans; da Mops nur blos vom schwartzem Brodte fra\u00df.", "tokens": ["A\u00df", "Hans", ";", "da", "Mops", "nur", "blos", "vom", "schwart\u00b7zem", "Brod\u00b7te", "fra\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "KOUS", "NN", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mops tranck das Wasser roh, und Hans gekochtes Na\u00df.", "tokens": ["Mops", "tranck", "das", "Was\u00b7ser", "roh", ",", "und", "Hans", "ge\u00b7koch\u00b7tes", "Na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "$,", "KON", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hans ging darauf ins Feld; Mops gleichfals. Hans be-", "tokens": ["Hans", "ging", "da\u00b7rauf", "ins", "Feld", ";", "Mops", "gleich\u00b7fals", ".", "Hans", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PAV", "APPRART", "NN", "$.", "NN", "ADV", "$.", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "schritte", "tokens": ["schrit\u00b7te"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Ein Pferd; Mops aber nicht, er lief, und jener ritte,", "tokens": ["Ein", "Pferd", ";", "Mops", "a\u00b7ber", "nicht", ",", "er", "lief", ",", "und", "je\u00b7ner", "rit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "ADV", "PTKNEG", "$,", "PPER", "VVFIN", "$,", "KON", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bi\u00df da\u00df der Mittag sie nach Hause wieder rief.", "tokens": ["Bi\u00df", "da\u00df", "der", "Mit\u00b7tag", "sie", "nach", "Hau\u00b7se", "wie\u00b7der", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "PPER", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hans a\u00df; Mops ebenfals. Wie Hans ein wenig schlief,", "tokens": ["Hans", "a\u00df", ";", "Mops", "e\u00b7ben\u00b7fals", ".", "Wie", "Hans", "ein", "we\u00b7nig", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "NN", "ADV", "$.", "PWAV", "NE", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Schlief Mops nicht weniger. Das sch\u00f6ne Sonnen-Licht", "tokens": ["Schlief", "Mops", "nicht", "we\u00b7ni\u00b7ger", ".", "Das", "sch\u00f6\u00b7ne", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "PTKNEG", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ward nicht von Hans beschaut, von Mops imgleichen", "tokens": ["Ward", "nicht", "von", "Hans", "be\u00b7schaut", ",", "von", "Mops", "im\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "NE", "VVPP", "$,", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "nicht.", "tokens": ["nicht", "."], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$."], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Da\u00df in der Fr\u00fchlings-Zeit die Creatur so sch\u00f6n,", "tokens": ["Da\u00df", "in", "der", "Fr\u00fch\u00b7lings\u00b7Zeit", "die", "Crea\u00b7tur", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Hat weder Hans noch Mops bemerckt und angesehn.", "tokens": ["Hat", "we\u00b7der", "Hans", "noch", "Mops", "be\u00b7merckt", "und", "an\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "NE", "ADV", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sie machten sich daraus nicht die geringste Freude.", "tokens": ["Sie", "mach\u00b7ten", "sich", "da\u00b7raus", "nicht", "die", "ge\u00b7rings\u00b7te", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durch wenig viel gesagt: ", "tokens": ["Durch", "we\u00b7nig", "viel", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Sie trancken beide Na\u00df; sie assen beide Brodt:", "tokens": ["Sie", "tran\u00b7cken", "bei\u00b7de", "Na\u00df", ";", "sie", "as\u00b7sen", "bei\u00b7de", "Brodt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es lebten Hans und Mops; jetzt sind sie beide todt.", "tokens": ["Es", "leb\u00b7ten", "Hans", "und", "Mops", ";", "jetzt", "sind", "sie", "bei\u00b7de", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "$.", "ADV", "VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}