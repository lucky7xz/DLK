{"textgrid.poem.55409": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Olympos, der Kissavos,", "tokens": ["Der", "O\u00b7lym\u00b7pos", ",", "der", "Kis\u00b7sa\u00b7vos", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die zwei Berge haderten;", "tokens": ["Die", "zwei", "Ber\u00b7ge", "ha\u00b7der\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da entgegnend sprach Olympos", "tokens": ["Da", "ent\u00b7geg\u00b7nend", "sprach", "O\u00b7lym\u00b7pos"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also zu dem Kissavos:", "tokens": ["Al\u00b7so", "zu", "dem", "Kis\u00b7sa\u00b7vos", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbnicht erhebe dich, Kissave,", "tokens": ["\u00bb", "nicht", "er\u00b7he\u00b7be", "dich", ",", "Kis\u00b7sa\u00b7ve", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PTKNEG", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "T\u00fcrken\u2013 du Getretener.", "tokens": ["T\u00fcr\u00b7ken", "\u2013", "du", "Ge\u00b7tre\u00b7te\u00b7ner", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Bin ich doch der Greis Olympos,", "tokens": ["Bin", "ich", "doch", "der", "Greis", "O\u00b7lym\u00b7pos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den die ganze Welt vernahm.", "tokens": ["Den", "die", "gan\u00b7ze", "Welt", "ver\u00b7nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Zweiundsechzig Gipfel z\u00e4hl ich", "tokens": ["Zwei\u00b7und\u00b7sech\u00b7zig", "Gip\u00b7fel", "z\u00e4hl", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und zweitausend Quellen klar,", "tokens": ["Und", "zweit\u00b7au\u00b7send", "Quel\u00b7len", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Jeder Brunn hat seinen Wimpel,", "tokens": ["Je\u00b7der", "Brunn", "hat", "sei\u00b7nen", "Wim\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Seinen K\u00e4mpfer jeder Zweig.", "tokens": ["Sei\u00b7nen", "K\u00e4mp\u00b7fer", "je\u00b7der", "Zweig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Auf den h\u00f6chsten Gipfel hat sich", "tokens": ["Auf", "den", "h\u00f6chs\u00b7ten", "Gip\u00b7fel", "hat", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Mir ein Adler aufgesetzt,", "tokens": ["Mir", "ein", "Ad\u00b7ler", "auf\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Fa\u00dft in seinen m\u00e4cht'gen Klauen", "tokens": ["Fa\u00dft", "in", "sei\u00b7nen", "m\u00e4cht'\u00b7gen", "Klau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Eines Helden blutend Haupt.", "tokens": ["Ei\u00b7nes", "Hel\u00b7den", "blu\u00b7tend", "Haupt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "\u203asage, Haupt! wie ist's ergangen?", "tokens": ["\u203a", "sa\u00b7ge", ",", "Haupt", "!", "wie", "ist's", "er\u00b7gan\u00b7gen", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$.", "PWAV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Fielest du verbrecherisch?\u2039", "tokens": ["Fie\u00b7lest", "du", "ver\u00b7bre\u00b7che\u00b7risch", "?", "\u2039"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "\u203aspeise, Vogel, meine Jugend,", "tokens": ["\u203a", "spei\u00b7se", ",", "Vo\u00b7gel", ",", "mei\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Meine Mannheit speise nur!", "tokens": ["Mei\u00b7ne", "Mann\u00b7heit", "spei\u00b7se", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Ellenl\u00e4nger w\u00e4chst dein Fl\u00fcgel,", "tokens": ["El\u00b7len\u00b7l\u00e4n\u00b7ger", "w\u00e4chst", "dein", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Deine Klaue spannenlang.", "tokens": ["Dei\u00b7ne", "Klau\u00b7e", "span\u00b7nen\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Bei Louron, in Xeromeron,", "tokens": ["Bei", "Lou\u00b7ron", ",", "in", "Xe\u00b7ro\u00b7me\u00b7ron", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Lebt ich in dem Kriegerstand,", "tokens": ["Lebt", "ich", "in", "dem", "Krie\u00b7ger\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "So in Chasia, auf 'm Olympos", "tokens": ["So", "in", "Cha\u00b7sia", ",", "auf", "'m", "O\u00b7lym\u00b7pos"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "$,", "APPR", "NE", "NE"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "K\u00e4mpft ich bis ins zw\u00f6lfte Jahr.", "tokens": ["K\u00e4mpft", "ich", "bis", "ins", "zw\u00f6lf\u00b7te", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Sechzig Agas, ich erschlug sie,", "tokens": ["Sech\u00b7zig", "A\u00b7gas", ",", "ich", "er\u00b7schlug", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Ihr Gefild verbrannt ich dann;", "tokens": ["Ihr", "Ge\u00b7fild", "ver\u00b7brannt", "ich", "dann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Die ich sonst noch niederstreckte,", "tokens": ["Die", "ich", "sonst", "noch", "nie\u00b7der\u00b7streck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "T\u00fcrken, Albaneser auch,", "tokens": ["T\u00fcr\u00b7ken", ",", "Al\u00b7ba\u00b7ne\u00b7ser", "auch", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Sind zu viele, gar zu viele,", "tokens": ["Sind", "zu", "vie\u00b7le", ",", "gar", "zu", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "$,", "ADV", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Da\u00df ich sie nicht z\u00e4hlen mag;", "tokens": ["Da\u00df", "ich", "sie", "nicht", "z\u00e4h\u00b7len", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Nun ist meine Reihe kommen,", "tokens": ["Nun", "ist", "mei\u00b7ne", "Rei\u00b7he", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Im Gefechte fiel ich brav.\u2039\u00ab", "tokens": ["Im", "Ge\u00b7fech\u00b7te", "fiel", "ich", "brav", ".", "\u2039", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}