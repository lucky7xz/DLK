{"textgrid.poem.35554": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit verrinnt. O s\u00fc\u00dfer Helios!", "tokens": ["Die", "Zeit", "ver\u00b7rinnt", ".", "O", "s\u00fc\u00b7\u00dfer", "He\u00b7li\u00b7os", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Bild im Kr\u00f6tent\u00fcmpel s\u00fc\u00df und klar;", "tokens": ["O", "Bild", "im", "Kr\u00f6\u00b7ten\u00b7t\u00fcm\u00b7pel", "s\u00fc\u00df", "und", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Sand versinkt ein Eden wunderbar.", "tokens": ["Im", "Sand", "ver\u00b7sinkt", "ein", "E\u00b7den", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Goldammern wiegt ein Busch in seinem Scho\u00df.", "tokens": ["Gold\u00b7am\u00b7mern", "wiegt", "ein", "Busch", "in", "sei\u00b7nem", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Bruder stirbt dir in verwunschnem Land", "tokens": ["Ein", "Bru\u00b7der", "stirbt", "dir", "in", "ver\u00b7wun\u00b7schnem", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und st\u00e4hlern schaun dich deine Augen an.", "tokens": ["Und", "st\u00e4h\u00b7lern", "schaun", "dich", "dei\u00b7ne", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Goldnem dort ein Duft von Thymian.", "tokens": ["In", "Gold\u00b7nem", "dort", "ein", "Duft", "von", "Thy\u00b7mi\u00b7an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Knabe legt am Weiler einen Brand.", "tokens": ["Ein", "Kna\u00b7be", "legt", "am", "Wei\u00b7ler", "ei\u00b7nen", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Liebenden in Faltern neu ergl\u00fchn", "tokens": ["Die", "Lie\u00b7ben\u00b7den", "in", "Fal\u00b7tern", "neu", "er\u00b7gl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schaukeln heiter hin um Stein und Zahl.", "tokens": ["Und", "schau\u00b7keln", "hei\u00b7ter", "hin", "um", "Stein", "und", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aufflattern Kr\u00e4hen um ein ekles Mahl", "tokens": ["Auf\u00b7flat\u00b7tern", "Kr\u00e4\u00b7hen", "um", "ein", "ek\u00b7les", "Mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und deine Stirne tost durchs sanfte Gr\u00fcn.", "tokens": ["Und", "dei\u00b7ne", "Stir\u00b7ne", "tost", "durchs", "sanf\u00b7te", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Dornenstrauch verendet weich ein Wild.", "tokens": ["Im", "Dor\u00b7nen\u00b7strauch", "ver\u00b7en\u00b7det", "weich", "ein", "Wild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nachgleitet dir ein heller Kindertag,", "tokens": ["Nach\u00b7glei\u00b7tet", "dir", "ein", "hel\u00b7ler", "Kin\u00b7der\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der graue Wind, der flatterhaft und vag", "tokens": ["Der", "grau\u00b7e", "Wind", ",", "der", "flat\u00b7ter\u00b7haft", "und", "vag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verfallne D\u00fcfte durch die D\u00e4mmerung sp\u00fclt.", "tokens": ["Ver\u00b7fall\u00b7ne", "D\u00fcf\u00b7te", "durch", "die", "D\u00e4m\u00b7me\u00b7rung", "sp\u00fclt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", 