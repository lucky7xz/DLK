{"textgrid.poem.66542": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df, ich bin an Tadlern reich,", "tokens": ["Ich", "wei\u00df", ",", "ich", "bin", "an", "Tad\u00b7lern", "reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die schwer das Zetern lassen,", "tokens": ["Die", "schwer", "das", "Ze\u00b7tern", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mir auf jeden dummen Streich", "tokens": ["Und", "mir", "auf", "je\u00b7den", "dum\u00b7men", "Streich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Argusaugen passen.", "tokens": ["Mit", "Ar\u00b7gus\u00b7au\u00b7gen", "pas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und th\u00e4t' ich es den Kl\u00fcgsten gleich,", "tokens": ["Und", "th\u00e4t'", "ich", "es", "den", "Kl\u00fcgs\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wissen sich zu fassen,", "tokens": ["Sie", "wis\u00b7sen", "sich", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und zetern, da\u00df den dummen Streich", "tokens": ["Und", "ze\u00b7tern", ",", "da\u00df", "den", "dum\u00b7men", "Streich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kl\u00fcglich unterlassen.", "tokens": ["Ich", "kl\u00fcg\u00b7lich", "un\u00b7ter\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}