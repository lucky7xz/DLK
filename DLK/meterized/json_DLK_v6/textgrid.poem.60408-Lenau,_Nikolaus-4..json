{"textgrid.poem.60408": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da liegt der Feinde gestreckte Schar,", "tokens": ["Da", "liegt", "der", "Fein\u00b7de", "ge\u00b7streck\u00b7te", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie liegt in ihrem blutroten Blut;", "tokens": ["Sie", "liegt", "in", "ih\u00b7rem", "blut\u00b7ro\u00b7ten", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Wie haut er so scharf, wie haut er so gut,", "tokens": ["Wie", "haut", "er", "so", "scharf", ",", "wie", "haut", "er", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der flinke Husar!", "tokens": ["Der", "flin\u00b7ke", "Hu\u00b7sar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Da liegen sie, ha! so bleich und rot,", "tokens": ["Da", "lie\u00b7gen", "sie", ",", "ha", "!", "so", "bleich", "und", "rot", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ITJ", "$.", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es zittern und wanken noch husch! husch!", "tokens": ["Es", "zit\u00b7tern", "und", "wan\u00b7ken", "noch", "husch", "!", "husch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$.", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihre Seelen auf seinem Federbusch,", "tokens": ["Ih\u00b7re", "See\u00b7len", "auf", "sei\u00b7nem", "Fe\u00b7der\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Da liegen sie tot.", "tokens": ["Da", "lie\u00b7gen", "sie", "tot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und weiter ruft der Trompetenruf,", "tokens": ["Und", "wei\u00b7ter", "ruft", "der", "Trom\u00b7pe\u00b7ten\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er wischt an die M\u00e4hne sein nasses Schwert,", "tokens": ["Er", "wischt", "an", "die", "M\u00e4h\u00b7ne", "sein", "nas\u00b7ses", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und weiter springt sein lustiges Pferd", "tokens": ["Und", "wei\u00b7ter", "springt", "sein", "lus\u00b7ti\u00b7ges", "Pferd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit rotem Huf.", "tokens": ["Mit", "ro\u00b7tem", "Huf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}