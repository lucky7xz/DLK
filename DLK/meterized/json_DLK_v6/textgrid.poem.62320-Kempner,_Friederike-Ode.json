{"textgrid.poem.62320": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Ode", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem kleinsten der Weltenr\u00e4ume,", "tokens": ["Aus", "dem", "kleins\u00b7ten", "der", "Wel\u00b7ten\u00b7r\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Niedriger Mensch, erhebst du dich", "tokens": ["Nied\u00b7ri\u00b7ger", "Mensch", ",", "er\u00b7hebst", "du", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PRF"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zu dem Gedanken an Gott,", "tokens": ["Zu", "dem", "Ge\u00b7dan\u00b7ken", "an", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Du wagst es.", "tokens": ["Du", "wagst", "es", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wei\u00dft du, ahnst du auch", "tokens": ["Wei\u00dft", "du", ",", "ahnst", "du", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nur den Begriff seiner Gr\u00f6\u00dfe?", "tokens": ["Nur", "den", "Be\u00b7griff", "sei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Gro\u00df ist Gott, gn\u00e4dig ist nur er", "tokens": ["Gro\u00df", "ist", "Gott", ",", "gn\u00e4\u00b7dig", "ist", "nur", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "NN", "$,", "ADJD", "VAFIN", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Millionen Wesen, Millionen Seelen,", "tokens": ["Mil\u00b7lion\u00b7en", "We\u00b7sen", ",", "Mil\u00b7lion\u00b7en", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}, "line.9": {"text": "Die einst hier gelebt.", "tokens": ["Die", "einst", "hier", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Und Millionen Wesen, die in h\u00f6heren Welten leben,", "tokens": ["Und", "Mil\u00b7lion\u00b7en", "We\u00b7sen", ",", "die", "in", "h\u00f6\u00b7he\u00b7ren", "Wel\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Millionen von Engeln", "tokens": ["Und", "Mil\u00b7lion\u00b7en", "von", "En\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und h\u00f6heren Geistern", "tokens": ["Und", "h\u00f6\u00b7he\u00b7ren", "Geis\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Erfassen nicht Gottes Gr\u00f6\u00dfe;", "tokens": ["Er\u00b7fas\u00b7sen", "nicht", "Got\u00b7tes", "Gr\u00f6\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie rufen Alle: Gott ist gro\u00df.", "tokens": ["Sie", "ru\u00b7fen", "Al\u00b7le", ":", "Gott", "ist", "gro\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mehr wissen sie nichts von Gott,", "tokens": ["Mehr", "wis\u00b7sen", "sie", "nichts", "von", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Geschweige du Mensch,", "tokens": ["Ge\u00b7schwei\u00b7ge", "du", "Mensch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Bescheide dich, freue dich, da\u00df Gott gro\u00df und allgn\u00e4dig ist.", "tokens": ["Be\u00b7schei\u00b7de", "dich", ",", "freu\u00b7e", "dich", ",", "da\u00df", "Gott", "gro\u00df", "und", "allg\u00b7n\u00e4\u00b7dig", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "KOUS", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Dies Bewu\u00dftsein begl\u00fccke dich,", "tokens": ["Dies", "Be\u00b7wu\u00df\u00b7tsein", "be\u00b7gl\u00fc\u00b7cke", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erfreue dich \u2013", "tokens": ["Er\u00b7freu\u00b7e", "dich", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Es sei deine Hoffnung, dein Halt;", "tokens": ["Es", "sei", "dei\u00b7ne", "Hoff\u00b7nung", ",", "dein", "Halt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "Und freue dich jedes Tages,", "tokens": ["Und", "freu\u00b7e", "dich", "je\u00b7des", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Und freue dich jedes Gr\u00e4sleins,", "tokens": ["Und", "freu\u00b7e", "dich", "je\u00b7des", "Gr\u00e4s\u00b7leins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Der Schwalbe und Lerche und des Vergi\u00dfmeinnichts.", "tokens": ["Der", "Schwal\u00b7be", "und", "Ler\u00b7che", "und", "des", "Ver\u00b7gi\u00df\u00b7mein\u00b7nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Und wisse, da\u00df Gott dich liebt,", "tokens": ["Und", "wis\u00b7se", ",", "da\u00df", "Gott", "dich", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Solange du lebst, und du lebst ewig.", "tokens": ["So\u00b7lan\u00b7ge", "du", "lebst", ",", "und", "du", "lebst", "e\u00b7wig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Eins aber wissen alle, da\u00df er", "tokens": ["Eins", "a\u00b7ber", "wis\u00b7sen", "al\u00b7le", ",", "da\u00df", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PIS", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ihnen geholfen hat und hilft.", "tokens": ["Ih\u00b7nen", "ge\u00b7hol\u00b7fen", "hat", "und", "hilft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "KON", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}