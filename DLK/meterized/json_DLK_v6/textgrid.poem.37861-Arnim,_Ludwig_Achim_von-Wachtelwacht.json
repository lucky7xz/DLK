{"textgrid.poem.37861": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wachtelwacht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt wie die Wachtel im Gr\u00fcnen sch\u00f6n schlagt,", "tokens": ["H\u00f6rt", "wie", "die", "Wach\u00b7tel", "im", "Gr\u00fc\u00b7nen", "sch\u00f6n", "schlagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Lobet Gott, lobet Gott!", "tokens": ["Lo\u00b7bet", "Gott", ",", "lo\u00b7bet", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mir kommt kein Schauder, sie sagt,", "tokens": ["Mir", "kommt", "kein", "Schau\u00b7der", ",", "sie", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Fliehet von einem ins andre gr\u00fcn Feld,", "tokens": ["Flie\u00b7het", "von", "ei\u00b7nem", "ins", "and\u00b7re", "gr\u00fcn", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Und uns den Wachsthum der Fr\u00fcchte vermeldt,", "tokens": ["Und", "uns", "den", "Wach\u00b7sthum", "der", "Fr\u00fcch\u00b7te", "ver\u00b7meldt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Rufet zu allen mit Lust und mit Freud:", "tokens": ["Ru\u00b7fet", "zu", "al\u00b7len", "mit", "Lust", "und", "mit", "Freud", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Danke Gott, danke Gott!", "tokens": ["Dan\u00b7ke", "Gott", ",", "dan\u00b7ke", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Der du mir geben die Zeit.", "tokens": ["Der", "du", "mir", "ge\u00b7ben", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Morgens sie ruft, eh der Tag noch anbricht:", "tokens": ["Mor\u00b7gens", "sie", "ruft", ",", "eh", "der", "Tag", "noch", "an\u00b7bricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Guten Tag, guten Tag!", "tokens": ["Gu\u00b7ten", "Tag", ",", "gu\u00b7ten", "Tag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wartet der Sonnen ihr Licht;", "tokens": ["War\u00b7tet", "der", "Son\u00b7nen", "ihr", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Ist sie aufgangen, so jauchzt sie vor Freud,", "tokens": ["Ist", "sie", "auf\u00b7gan\u00b7gen", ",", "so", "jauchzt", "sie", "vor", "Freud", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVIZU", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Sch\u00fcttert die Federn, und strecket den Leib,", "tokens": ["Sch\u00fct\u00b7tert", "die", "Fe\u00b7dern", ",", "und", "stre\u00b7cket", "den", "Leib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Wendet die Augen dem Himmel hinzu,", "tokens": ["Wen\u00b7det", "die", "Au\u00b7gen", "dem", "Him\u00b7mel", "hin\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Dank sey Gott, dank sey Gott!", "tokens": ["Dank", "sey", "Gott", ",", "dank", "sey", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "NN", "$,", "APPR", "VAFIN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Der du mir geben die Ruh.", "tokens": ["Der", "du", "mir", "ge\u00b7ben", "die", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Blinket der k\u00fchlende Thau auf der Heid,", "tokens": ["Blin\u00b7ket", "der", "k\u00fch\u00b7len\u00b7de", "Thau", "auf", "der", "Heid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Werd ich na\u00df, werd ich na\u00df!", "tokens": ["Werd", "ich", "na\u00df", ",", "werd", "ich", "na\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Zitternd sie balde ausschreit,", "tokens": ["Zit\u00b7ternd", "sie", "bal\u00b7de", "aus\u00b7schreit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "ADV", "ADJD", "$,"], "meter": "+--+---", "measure": "dactylic.di.plus"}, "line.4": {"text": "Fliehet der Sonne entgegen und bitt,", "tokens": ["Flie\u00b7het", "der", "Son\u00b7ne", "ent\u00b7ge\u00b7gen", "und", "bitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Da\u00df sie ihr theile die W\u00e4rme auch mit,", "tokens": ["Da\u00df", "sie", "ihr", "thei\u00b7le", "die", "W\u00e4r\u00b7me", "auch", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Laufet zum Sande und scharret sich ein,", "tokens": ["Lau\u00b7fet", "zum", "San\u00b7de", "und", "schar\u00b7ret", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Sagt sie, und legt sich darein.", "tokens": ["Sagt", "sie", ",", "und", "legt", "sich", "da\u00b7rein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Kommt nun der Waidmann mit Hund und mit Bley,", "tokens": ["Kommt", "nun", "der", "Waid\u00b7mann", "mit", "Hund", "und", "mit", "Bley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcrcht mich nicht, f\u00fcrcht mich nicht!", "tokens": ["F\u00fcrcht", "mich", "nicht", ",", "f\u00fcrcht", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Liegend ich beyde nicht scheu,", "tokens": ["Lie\u00b7gend", "ich", "bey\u00b7de", "nicht", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PIS", "PTKNEG", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Steht nur der Waizen, und gr\u00fcnet das Laub,", "tokens": ["Steht", "nur", "der", "Wai\u00b7zen", ",", "und", "gr\u00fc\u00b7net", "das", "Laub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich meinen Feinden nicht werde zum Raub,", "tokens": ["Ich", "mei\u00b7nen", "Fein\u00b7den", "nicht", "wer\u00b7de", "zum", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKNEG", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Aber die Schnitter die machen mich arm,", "tokens": ["A\u00b7ber", "die", "Schnit\u00b7ter", "die", "ma\u00b7chen", "mich", "arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Wehe mir, wehe mir!", "tokens": ["We\u00b7he", "mir", ",", "we\u00b7he", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Da\u00df sich der Himmel erbarm.", "tokens": ["Da\u00df", "sich", "der", "Him\u00b7mel", "er\u00b7barm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Kommen die Schnitter, so ruft sie ganz keck:", "tokens": ["Kom\u00b7men", "die", "Schnit\u00b7ter", ",", "so", "ruft", "sie", "ganz", "keck", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Tritt mich nicht, tritt mich nicht!", "tokens": ["Tritt", "mich", "nicht", ",", "tritt", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Liegend zur Erde gestreckt.", "tokens": ["Lie\u00b7gend", "zur", "Er\u00b7de", "ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Flieht von geschnittenen Feldern hindann,", "tokens": ["Flieht", "von", "ge\u00b7schnit\u00b7te\u00b7nen", "Fel\u00b7dern", "hin\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Weil sie sich nirgend verbergen mehr kann,", "tokens": ["Weil", "sie", "sich", "nir\u00b7gend", "ver\u00b7ber\u00b7gen", "mehr", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "ADV", "VMFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Klaget, ich finde kein K\u00f6rnlein darin,", "tokens": ["Kla\u00b7get", ",", "ich", "fin\u00b7de", "kein", "K\u00f6rn\u00b7lein", "da\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "PAV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Ist mir leid, ist mir leid!", "tokens": ["Ist", "mir", "leid", ",", "ist", "mir", "leid", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Flieht zu den Saaten dahin.", "tokens": ["Flieht", "zu", "den", "Saa\u00b7ten", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PAV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Ist nun das Schneiden der Fr\u00fcchte vorbey,", "tokens": ["Ist", "nun", "das", "Schnei\u00b7den", "der", "Fr\u00fcch\u00b7te", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Harte Zeit! harte Zeit!", "tokens": ["Har\u00b7te", "Zeit", "!", "har\u00b7te", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Schon kommt der Winter herbey.", "tokens": ["Schon", "kommt", "der", "Win\u00b7ter", "her\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Hebt sich zum Lande zu wandern nun fort", "tokens": ["Hebt", "sich", "zum", "Lan\u00b7de", "zu", "wan\u00b7dern", "nun", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "PTKZU", "VVFIN", "ADV", "PTKVZ"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Hin zu dem andern weit fr\u00f6hlichern Ort", "tokens": ["Hin", "zu", "dem", "an\u00b7dern", "weit", "fr\u00f6h\u00b7li\u00b7chern", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJD", "ADJA", "NN"], "meter": "+--+-++--+", "measure": "iambic.penta.invert"}, "line.6": {"text": "W\u00fcnschet indessen dem Lande noch an:", "tokens": ["W\u00fcn\u00b7schet", "in\u00b7des\u00b7sen", "dem", "Lan\u00b7de", "noch", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "H\u00fct dich Gott, h\u00fct dich Gott!", "tokens": ["H\u00fct", "dich", "Gott", ",", "h\u00fct", "dich", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Fliehet in Frieden bergan.", "tokens": ["Flie\u00b7het", "in", "Frie\u00b7den", "ber\u00b7gan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}