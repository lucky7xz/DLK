{"textgrid.poem.42243": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Gru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df ein allerliebstes Kind,", "tokens": ["Ich", "wei\u00df", "ein", "al\u00b7ler\u00b7liebs\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kind, wie selten Kinder sind,", "tokens": ["Ein", "Kind", ",", "wie", "sel\u00b7ten", "Kin\u00b7der", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit schwarzem Auge, schwarzem Haar,", "tokens": ["Mit", "schwar\u00b7zem", "Au\u00b7ge", ",", "schwar\u00b7zem", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Wuchs und Haltung wunderbar!", "tokens": ["An", "Wuchs", "und", "Hal\u00b7tung", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "'s ist nicht zu gro\u00df und nicht zu klein,", "tokens": ["'s", "ist", "nicht", "zu", "gro\u00df", "und", "nicht", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKA", "ADJD", "KON", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "'s ist nicht zu dick und nicht zu fein,", "tokens": ["'s", "ist", "nicht", "zu", "dick", "und", "nicht", "zu", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKA", "ADJD", "KON", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Es singt und springt und tanzt und lacht,", "tokens": ["Es", "singt", "und", "springt", "und", "tanzt", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat manchen schon verr\u00fcckt gemacht.", "tokens": ["Hat", "man\u00b7chen", "schon", "ver\u00b7r\u00fcckt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}