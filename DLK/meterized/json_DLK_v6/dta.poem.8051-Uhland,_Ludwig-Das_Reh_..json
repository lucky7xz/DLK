{"dta.poem.8051": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Reh .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.85", "et:0.14"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es jagt\u2019 ein J\u00e4ger fr\u00fch am Tag", "tokens": ["Es", "jagt'", "ein", "J\u00e4\u00b7ger", "fr\u00fch", "am", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Reh durch W\u00e4lder und Auen,", "tokens": ["Ein", "Reh", "durch", "W\u00e4l\u00b7der", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da sah er aus dem Gartenhag", "tokens": ["Da", "sah", "er", "aus", "dem", "Gar\u00b7ten\u00b7hag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein rosig M\u00e4gdlein schauen.", "tokens": ["Ein", "ro\u00b7sig", "M\u00e4gd\u00b7lein", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was ist geschehn dem guten Pferd?", "tokens": ["Was", "ist", "ge\u00b7schehn", "dem", "gu\u00b7ten", "Pferd", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat es den Fu\u00df verletzet?", "tokens": ["Hat", "es", "den", "Fu\u00df", "ver\u00b7let\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ist geschehn dem J\u00e4ger werth,", "tokens": ["Was", "ist", "ge\u00b7schehn", "dem", "J\u00e4\u00b7ger", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "VVPP", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er nicht mehr ruft und hetzet?", "tokens": ["Da\u00df", "er", "nicht", "mehr", "ruft", "und", "het\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Rehlein rennet immer noch", "tokens": ["Das", "Reh\u00b7lein", "ren\u00b7net", "im\u00b7mer", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ueber Berg und Thal so bange.", "tokens": ["Ue\u00b7ber", "Berg", "und", "Thal", "so", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Halt an, du seltsam Thierlein, doch!", "tokens": ["Halt", "an", ",", "du", "selt\u00b7sam", "Thier\u00b7lein", ",", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJD", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der J\u00e4ger verga\u00df dich lange.", "tokens": ["Der", "J\u00e4\u00b7ger", "ver\u00b7ga\u00df", "dich", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}