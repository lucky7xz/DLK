{"dta.poem.18899": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Fahne .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Fetch Ali Schah der T\u00fcrk ist Dschemschid gleich,", "tokens": ["Fetch", "A\u00b7li", "Schah", "der", "T\u00fcrk", "ist", "Dschem\u00b7schid", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "VAFIN", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weltlicht, und Irans Herr, der Erden Sonne.", "tokens": ["Welt\u00b7licht", ",", "und", "I\u00b7rans", "Herr", ",", "der", "Er\u00b7den", "Son\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NE", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Schirm wirft auf die Weltflur weiten Schatten,", "tokens": ["Sein", "Schirm", "wirft", "auf", "die", "Welt\u00b7flur", "wei\u00b7ten", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Gurt haucht Muskus in Saturns Gehirn.", "tokens": ["Sein", "Gurt", "haucht", "Mus\u00b7kus", "in", "Sa\u00b7turns", "Ge\u00b7hirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Iran ist L\u00f6wenschlucht, sein F\u00fcrst die Sonne;", "tokens": ["I\u00b7ran", "ist", "L\u00f6\u00b7wen\u00b7schlucht", ",", "sein", "F\u00fcrst", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Drum prangen Leu und Sonn\u2019 in Daras Banner.", "tokens": ["Drum", "pran\u00b7gen", "Leu", "und", "Sonn'", "in", "Da\u00b7ras", "Ban\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das Haupt des Boten Abul Hassan Chan", "tokens": ["Das", "Haupt", "des", "Bo\u00b7ten", "A\u00b7bul", "Has\u00b7san", "Chan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NE", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Erhebt zum Himmelsdom das seidne Banner.", "tokens": ["Er\u00b7hebt", "zum", "Him\u00b7mels\u00b7dom", "das", "seid\u00b7ne", "Ban\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Aus Liebe ward nach London er gesandt", "tokens": ["Aus", "Lie\u00b7be", "ward", "nach", "Lon\u00b7don", "er", "ge\u00b7sandt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "APPR", "NE", "PPER", "VVPP"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.10": {"text": "Und brachte Gl\u00fcck und Heil dem Christenherrn.", "tokens": ["Und", "brach\u00b7te", "Gl\u00fcck", "und", "Heil", "dem", "Chris\u00b7ten\u00b7herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}