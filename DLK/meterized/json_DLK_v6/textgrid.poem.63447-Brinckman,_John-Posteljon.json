{"textgrid.poem.63447": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Posteljon", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Klock de st\u00f6tt, de K\u00f6ster", "tokens": ["De", "Klock", "de", "st\u00f6tt", ",", "de", "K\u00f6s\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "NE", "VVFIN", "$,", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "de beiert af de Woch;", "tokens": ["de", "bei\u00b7ert", "af", "de", "Woch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.da", "FM.da", "FM.da", "FM.da", "FM.da", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "de Kl\u0153s' dor v\u00f6r ehr Nester", "tokens": ["de", "Kl\u0153s'", "dor", "v\u00f6r", "ehr", "Nes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "de karken luder noch.", "tokens": ["de", "kar\u00b7ken", "lu\u00b7der", "noch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wat is dat f\u00f6r'n Gekl\u0153ter?", "tokens": ["Wat", "is", "dat", "f\u00f6r'n", "Ge\u00b7kl\u0153ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.nl", "FM.nl", "FM.nl", "FM.nl", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dat geiht jo klack un klick!", "tokens": ["Dat", "geiht", "jo", "klack", "un", "klick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u2013 Dat is in't D\u00f6rp een K\u0153ter,", "tokens": ["\u2013", "Dat", "is", "in't", "D\u00f6rp", "e\u00b7en", "K\u0153ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "sin Sei\u00df de hoort he sick.", "tokens": ["sin", "Sei\u00df", "de", "ho\u00b7ort", "he", "sick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Klack, klick! klick, klack! so sleiht he", "tokens": ["Klack", ",", "klick", "!", "klick", ",", "klack", "!", "so", "sleiht", "he"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ITJ", "$.", "VVFIN", "$,", "VVFIN", "$.", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "sin'n Hamer r\u00fcnn un rup,", "tokens": ["sin'n", "Ha\u00b7mer", "r\u00fcnn", "un", "rup", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "un dorto pippt un fl\u00e4ut't he", "tokens": ["un", "dor\u00b7to", "pippt", "un", "fl\u00e4ut't", "he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "sick s\u00fclm een St\u00fcckschen up.", "tokens": ["sick", "s\u00fclm", "e\u00b7en", "St\u00fcck\u00b7schen", "up", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wat heet dat Panken, Pinken?", "tokens": ["Wat", "heet", "dat", "Pan\u00b7ken", ",", "Pin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u2013 Dat is dor v\u00f6r de Sm\u00e4r,", "tokens": ["\u2013", "Dat", "is", "dor", "v\u00f6r", "de", "Sm\u00e4r", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dor leggt de Smitt ni Tinken", "tokens": ["dor", "leggt", "de", "Smitt", "ni", "Tin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "de isern \u00c4gen v\u00f6r.", "tokens": ["de", "i\u00b7sern", "\u00c4\u00b7gen", "v\u00f6r", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "As nachts de Fl\u00e4mmstirn blunken", "tokens": ["As", "nachts", "de", "Fl\u00e4mms\u00b7tirn", "blun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NE", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mank Durn un Fleeder mank,", "tokens": ["mank", "Durn", "un", "Flee\u00b7der", "mank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "so h\u00fcppt un spreuht de Funken,", "tokens": ["so", "h\u00fcppt", "un", "spreuht", "de", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "un dat geiht pinke pank!", "tokens": ["un", "dat", "geiht", "pin\u00b7ke", "pank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Wat bl\u00f6\u00dft so sch\u00f6n dor achter?", "tokens": ["Wat", "bl\u00f6\u00dft", "so", "sch\u00f6n", "dor", "ach\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wat's dat f\u00f6r'n hellen Ton,", "tokens": ["Wat's", "dat", "f\u00f6r'n", "hel\u00b7len", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "denn lut eens, denn eens sachter?", "tokens": ["denn", "lut", "e\u00b7ens", ",", "denn", "e\u00b7ens", "sach\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "$,", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 Dat is een Posteljon.", "tokens": ["\u2013", "Dat", "is", "e\u00b7en", "Pos\u00b7tel\u00b7jon", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "De bl\u00f6\u00dft dat Leed vun Sleswig", "tokens": ["De", "bl\u00f6\u00dft", "dat", "Leed", "vun", "Sles\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "un Holsteen blotsverwandt,", "tokens": ["un", "Hols\u00b7te\u00b7en", "blots\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "de bl\u00f6\u00dft dor vun de Gefjon", "tokens": ["de", "bl\u00f6\u00dft", "dor", "vun", "de", "Gef\u00b7jon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "un vun de D\u00fcppler Schant;", "tokens": ["un", "vun", "de", "D\u00fcpp\u00b7ler", "Schant", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "vun Meld\u00f6rp un Bornh\u00f6ved", "tokens": ["vun", "Mel\u00b7d\u00f6rp", "un", "Born\u00b7h\u00f6\u00b7ved"], "token_info": ["word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "NE"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "un Hinrich vun Swerin", "tokens": ["un", "Hin\u00b7rich", "vun", "Swe\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "un vun de Grotm\u00fcl bl\u00f6\u00dft he,", "tokens": ["un", "vun", "de", "Grot\u00b7m\u00fcl", "bl\u00f6\u00dft", "he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "de achter Flensborg schri'n;", "tokens": ["de", "ach\u00b7ter", "Flens\u00b7borg", "schri'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "de sick vor Wallen hollen", "tokens": ["de", "sick", "vor", "Wal\u00b7len", "hol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "un s\u00fcnd doch man Grash\u00e4kt,", "tokens": ["un", "s\u00fcnd", "doch", "man", "Gras\u00b7h\u00e4kt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "KON", "PIS", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "un wenn dat fluscht, sick achter", "tokens": ["un", "wenn", "dat", "fluscht", ",", "sick", "ach\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM", "KOUS", "ART", "NN", "$,", "XY", "XY"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "ehr groten Br\u00f6der st\u00e4kt.", "tokens": ["ehr", "gro\u00b7ten", "Br\u00f6\u00b7der", "st\u00e4kt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}