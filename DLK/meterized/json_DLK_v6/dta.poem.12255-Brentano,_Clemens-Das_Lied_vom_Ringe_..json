{"dta.poem.12255": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Ringe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es waren drey Soldaten,               ", "tokens": ["Es", "wa\u00b7ren", "drey", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dabey ein junges Blut,", "tokens": ["Da\u00b7bey", "ein", "jun\u00b7ges", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hatten sich vergangen,", "tokens": ["Sie", "hat\u00b7ten", "sich", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Graf nahm sie gefangen,", "tokens": ["Der", "Graf", "nahm", "sie", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Setzt sie bis auf den Tod.", "tokens": ["Setzt", "sie", "bis", "auf", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Es war ein wackres M\u00e4delein", "tokens": ["Es", "war", "ein", "wack\u00b7res", "M\u00e4\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dazu aus fremdem Land,", "tokens": ["Da\u00b7zu", "aus", "frem\u00b7dem", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie lief in aller Eilen", "tokens": ["Sie", "lief", "in", "al\u00b7ler", "Ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Tags wohl zehen Meilen", "tokens": ["Des", "Tags", "wohl", "ze\u00b7hen", "Mei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis zu dem Grafen hin.", "tokens": ["Bis", "zu", "dem", "Gra\u00b7fen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201egott gr\u00fc\u00df Euch, edler Herre mein,", "tokens": ["\u201e", "gott", "gr\u00fc\u00df", "Euch", ",", "ed\u00b7ler", "Her\u00b7re", "mein", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eich w\u00fcnsch Euch guten Tag,", "tokens": ["\u201e", "ich", "w\u00fcnsch", "Euch", "gu\u00b7ten", "Tag", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201each! wolt Ihr mein gedenken", "tokens": ["\u201e", "ach", "!", "wolt", "Ihr", "mein", "ge\u00b7den\u00b7ken"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$.", "VMFIN", "PPER", "PPOSAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eden Gefangnen mir zu schenken", "tokens": ["\u201e", "den", "Ge\u00b7fang\u00b7nen", "mir", "zu", "schen\u00b7ken"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "\u201eja schenken zu der Eh.\u201c", "tokens": ["\u201e", "ja", "schen\u00b7ken", "zu", "der", "Eh", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201each nein, mein liebes M\u00e4delein,", "tokens": ["\u201e", "ach", "nein", ",", "mein", "lie\u00b7bes", "M\u00e4\u00b7de\u00b7lein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKANT", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edas kann und mag nicht sein,", "tokens": ["\u201e", "das", "kann", "und", "mag", "nicht", "sein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "KON", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eder Gefangne der mu\u00df sterben,", "tokens": ["\u201e", "der", "Ge\u00b7fang\u00b7ne", "der", "mu\u00df", "ster\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201egott's Gnad mu\u00df er ererben", "tokens": ["\u201e", "gott's", "Gnad", "mu\u00df", "er", "er\u00b7er\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201ewie er verdienet hat.\u201c", "tokens": ["\u201e", "wie", "er", "ver\u00b7die\u00b7net", "hat", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PPER", "VVFIN", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das M\u00e4del drehet sich herum", "tokens": ["Das", "M\u00e4\u00b7del", "dre\u00b7het", "sich", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weinet bitterlich,", "tokens": ["Und", "wei\u00b7net", "bit\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie lief in aller Eilen", "tokens": ["Sie", "lief", "in", "al\u00b7ler", "Ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Tags wohl zwanzig Meilen,", "tokens": ["Des", "Tags", "wohl", "zwan\u00b7zig", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis zu dem tiefen Thurm.", "tokens": ["Bis", "zu", "dem", "tie\u00b7fen", "Thurm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201egott gr\u00fc\u00df Euch ihr Gefangnen mein,", "tokens": ["\u201e", "gott", "gr\u00fc\u00df", "Euch", "ihr", "Ge\u00b7fang\u00b7nen", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eich w\u00fcnsch Euch guten Tag!", "tokens": ["\u201e", "ich", "w\u00fcnsch", "Euch", "gu\u00b7ten", "Tag", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eich hab f\u00fcr Euch gebeten,", "tokens": ["\u201e", "ich", "hab", "f\u00fcr", "Euch", "ge\u00b7be\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eich kann Euch nicht erretten,", "tokens": ["\u201e", "ich", "kann", "Euch", "nicht", "er\u00b7ret\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201ees hilft nicht Gut noch Geld.\u201c", "tokens": ["\u201e", "es", "hilft", "nicht", "Gut", "noch", "Geld", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "ADJD", "ADV", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was hat sie unter ihrem Sch\u00fcrzelein?", "tokens": ["Was", "hat", "sie", "un\u00b7ter", "ih\u00b7rem", "Sch\u00fcr\u00b7zel\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Hemdlein war schneewei\u00df,", "tokens": ["Ein", "Hemd\u00b7lein", "war", "schnee\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edas nimm du Allerliebster mein,", "tokens": ["\u201e", "das", "nimm", "du", "Al\u00b7ler\u00b7liebs\u00b7ter", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ees soll von mir dein Brauthemd sein,", "tokens": ["\u201e", "es", "soll", "von", "mir", "dein", "Brau\u00b7themd", "sein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "APPR", "PPER", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201edarin lieg du im Tod.\u201c", "tokens": ["\u201e", "da\u00b7rin", "lieg", "du", "im", "Tod", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was zog er von dem Finger sein?", "tokens": ["Was", "zog", "er", "von", "dem", "Fin\u00b7ger", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ringlein, war von Gold,", "tokens": ["Ein", "Rin\u00b7glein", ",", "war", "von", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edas nimm du H\u00fcbsche, du Feine,", "tokens": ["\u201e", "das", "nimm", "du", "H\u00fcb\u00b7sche", ",", "du", "Fei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u201edu Allerliebste meine,", "tokens": ["\u201e", "du", "Al\u00b7ler\u00b7liebs\u00b7te", "mei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201edas soll dein Trauring sein.\u201c", "tokens": ["\u201e", "das", "soll", "dein", "Trau\u00b7ring", "sein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u201ewas soll ich mit dem Ringlein thun,", "tokens": ["\u201e", "was", "soll", "ich", "mit", "dem", "Rin\u00b7glein", "thun", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewenn ichs nicht tragen kan?\u201c", "tokens": ["\u201e", "wenn", "ichs", "nicht", "tra\u00b7gen", "kan", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eleg es in Kisten und Kasten,", "tokens": ["\u201e", "leg", "es", "in", "Kis\u00b7ten", "und", "Kas\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "\u201eund la\u00df es ruhen und rasten", "tokens": ["\u201e", "und", "la\u00df", "es", "ru\u00b7hen", "und", "ras\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VVIMP", "PPER", "VVINF", "KON", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u201ebis an den j\u00fcngsten Tag.\u201c", "tokens": ["\u201e", "bis", "an", "den", "j\u00fcng\u00b7sten", "Tag", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u201eund wenn ich \u00fcber Kisten und Kasten komm,", "tokens": ["\u201e", "und", "wenn", "ich", "\u00fc\u00b7ber", "Kis\u00b7ten", "und", "Kas\u00b7ten", "komm", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201eund sehe das Ringlein an,", "tokens": ["\u201e", "und", "se\u00b7he", "das", "Rin\u00b7glein", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201eda darfs ichs nicht anstecken,", "tokens": ["\u201e", "da", "darfs", "ichs", "nicht", "an\u00b7ste\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201edas Herz m\u00f6cht mir zerbrechen,", "tokens": ["\u201e", "das", "Herz", "m\u00f6cht", "mir", "zer\u00b7bre\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eweil ichs nicht \u00e4ndern kann.\u201c", "tokens": ["\u201e", "weil", "ichs", "nicht", "\u00e4n\u00b7dern", "kann", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}