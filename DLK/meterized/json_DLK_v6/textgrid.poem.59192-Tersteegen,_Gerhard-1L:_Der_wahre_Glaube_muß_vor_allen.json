{"textgrid.poem.59192": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der wahre Glaube mu\u00df vor allen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der wahre Glaube mu\u00df vor allen", "tokens": ["Der", "wah\u00b7re", "Glau\u00b7be", "mu\u00df", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Grunds\u00e4ul' sein, soll dieses Haus nicht fallen;", "tokens": ["Die", "Grund\u00b7s\u00e4ul'", "sein", ",", "soll", "die\u00b7ses", "Haus", "nicht", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$,", "VMFIN", "PDAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf ihn sich alle Tugend gr\u00fcnd't,", "tokens": ["Auf", "ihn", "sich", "al\u00b7le", "Tu\u00b7gend", "gr\u00fcnd't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihm sie all' in eins gefasset sind.", "tokens": ["In", "ihm", "sie", "all'", "in", "eins", "ge\u00b7fas\u00b7set", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "PIS", "APPR", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}