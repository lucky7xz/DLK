{"textgrid.poem.32592": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "9. Als die junge Am\u00f6ne nicht wolte Stand halten", "genre": "verse", "period": "N.A.", "pub_year": 1675, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am\u00f6ne darff ich mich erk\u00fchnen", "tokens": ["A\u00b7m\u00f6\u00b7ne", "darff", "ich", "mich", "er\u00b7k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meiner schlechten H\u00f6fflichkeit", "tokens": ["Mit", "mei\u00b7ner", "schlech\u00b7ten", "H\u00f6ff\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die s\u00fcsse Sch\u00f6nheit zu bedienen", "tokens": ["Die", "s\u00fcs\u00b7se", "Sch\u00f6n\u00b7heit", "zu", "be\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sich au\u00df ihren Augen streut?", "tokens": ["So", "sich", "au\u00df", "ih\u00b7ren", "Au\u00b7gen", "streut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach soll ich allersch\u00f6nstes Bild?", "tokens": ["Ach", "soll", "ich", "al\u00b7ler\u00b7sch\u00f6ns\u00b7tes", "Bild", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}