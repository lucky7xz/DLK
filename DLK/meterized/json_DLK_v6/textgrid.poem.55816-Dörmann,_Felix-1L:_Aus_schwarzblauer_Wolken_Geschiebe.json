{"textgrid.poem.55816": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus schwarzblauer Wolken Geschiebe", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus schwarzblauer Wolken Geschiebe", "tokens": ["Aus", "schwarz\u00b7blau\u00b7er", "Wol\u00b7ken", "Ge\u00b7schie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Funkelstern der Liebe", "tokens": ["Der", "Fun\u00b7kels\u00b7tern", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Glut\u00e4ugig zu lodern beginnt,", "tokens": ["Glu\u00b7t\u00e4u\u00b7gig", "zu", "lo\u00b7dern", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Zerfetzte Nebelstreifen", "tokens": ["Zer\u00b7fetz\u00b7te", "Ne\u00b7bel\u00b7strei\u00b7fen"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Grausilbern schwimmen und schweifen", "tokens": ["Grau\u00b7sil\u00b7bern", "schwim\u00b7men", "und", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Und schaukeln im Abendwind;", "tokens": ["Und", "schau\u00b7keln", "im", "Ab\u00b7end\u00b7wind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Aus m\u00fcdgebeugten Weiden", "tokens": ["Aus", "m\u00fcd\u00b7ge\u00b7beug\u00b7ten", "Wei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Eint\u00f6nig rauscht und singt", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", "rauscht", "und", "singt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Ein Wasser durch die Haiden,", "tokens": ["Ein", "Was\u00b7ser", "durch", "die", "Hai\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Von nickenden Gr\u00e4sern umringt;", "tokens": ["Von", "ni\u00b7cken\u00b7den", "Gr\u00e4\u00b7sern", "um\u00b7ringt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "In L\u00fcften, rosig-feuchten,", "tokens": ["In", "L\u00fcf\u00b7ten", ",", "ro\u00b7sig\u00b7feuch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Kastaniend\u00fcfte zieh'n,", "tokens": ["Kas\u00b7ta\u00b7ni\u00b7en\u00b7d\u00fcf\u00b7te", "zieh'n", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.13": {"text": "Johannisw\u00fcrmer leuchten,", "tokens": ["Jo\u00b7han\u00b7nis\u00b7w\u00fcr\u00b7mer", "leuch\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Nachtv\u00f6gel huschen und flieh'n.", "tokens": ["Nacht\u00b7v\u00f6\u00b7gel", "hu\u00b7schen", "und", "flieh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NN", "VVINF", "KON", "VVFIN", "NE"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Und reich und immer reicher", "tokens": ["Und", "reich", "und", "im\u00b7mer", "rei\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entflammt der Sterne Pracht,", "tokens": ["Ent\u00b7flammt", "der", "Ster\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weich und immer weicher", "tokens": ["Und", "weich", "und", "im\u00b7mer", "wei\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schw\u00fcler athmet die Nacht.", "tokens": ["Und", "schw\u00fc\u00b7ler", "ath\u00b7met", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die Wolken sind versunken", "tokens": ["Die", "Wol\u00b7ken", "sind", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am fernen Horizont;", "tokens": ["Am", "fer\u00b7nen", "Ho\u00b7ri\u00b7zont", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Fl\u00fcssigen Silbers trunken,", "tokens": ["Fl\u00fcs\u00b7si\u00b7gen", "Sil\u00b7bers", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Wei\u00dfgl\u00fchend naht der Mond ...", "tokens": ["Wei\u00df\u00b7gl\u00fc\u00b7hend", "naht", "der", "Mond", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die jugendfrischen Mienen", "tokens": ["Die", "ju\u00b7gend\u00b7fri\u00b7schen", "Mie\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Lichte weich umschienen,", "tokens": ["Vom", "Lich\u00b7te", "weich", "um\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Knab' die Stra\u00dfe zog,", "tokens": ["Ein", "Knab'", "die", "Stra\u00b7\u00dfe", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie rothe Haidebl\u00fcthen", "tokens": ["Wie", "ro\u00b7the", "Hai\u00b7de\u00b7bl\u00fc\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Wangen ihm ergl\u00fchten,", "tokens": ["Die", "Wan\u00b7gen", "ihm", "er\u00b7gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Lied vom Mund ihm flog.", "tokens": ["Ein", "Lied", "vom", "Mund", "ihm", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hinaus in die strahlenden, hellen", "tokens": ["Hin\u00b7aus", "in", "die", "strah\u00b7len\u00b7den", ",", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Gefilde, selig und frei,", "tokens": ["Ge\u00b7fil\u00b7de", ",", "se\u00b7lig", "und", "frei", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Wie Lerchengeschmetter schwellen", "tokens": ["Wie", "Ler\u00b7chen\u00b7ge\u00b7schmet\u00b7ter", "schwel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Die T\u00f6ne und verquellen", "tokens": ["Die", "T\u00f6\u00b7ne", "und", "ver\u00b7quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "In einen Jubelschrei.", "tokens": ["In", "ei\u00b7nen", "Ju\u00b7bel\u00b7schrei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Im \u00dcberschwang der Gef\u00fchle", "tokens": ["Im", "\u00dc\u00b7bersc\u00b7hwang", "der", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "In's Haidekraut er springt,", "tokens": ["In's", "Hai\u00b7de\u00b7kraut", "er", "springt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und Gr\u00e4ser, thauig-k\u00fchle,", "tokens": ["Und", "Gr\u00e4\u00b7ser", ",", "thau\u00b7ig\u00b7k\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Sein bebender Arm umschlingt.", "tokens": ["Sein", "be\u00b7ben\u00b7der", "Arm", "um\u00b7schlingt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Er k\u00fcsst die schimmernden Kronen", "tokens": ["Er", "k\u00fcsst", "die", "schim\u00b7mern\u00b7den", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Am Boden, leben-geschwellt:", "tokens": ["Am", "Bo\u00b7den", ",", "le\u00b7ben\u00b7ge\u00b7schwellt", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "\u00bbm\u00f6cht' immer auf dir wohnen,", "tokens": ["\u00bb", "m\u00f6cht'", "im\u00b7mer", "auf", "dir", "woh\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du liebe, s\u00fc\u00dfe Welt.", "tokens": ["Du", "lie\u00b7be", ",", "s\u00fc\u00b7\u00dfe", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Gott, wie ist das Leben", "tokens": ["O", "Gott", ",", "wie", "ist", "das", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sch\u00f6n, so wundersch\u00f6n,", "tokens": ["So", "sch\u00f6n", ",", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "M\u00f6cht' wie der Vogel schweben", "tokens": ["M\u00f6cht'", "wie", "der", "Vo\u00b7gel", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "KOKOM", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Leicht \u00fcber Thal und H\u00f6h'n!", "tokens": ["Leicht", "\u00fc\u00b7ber", "Thal", "und", "H\u00f6h'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann w\u00e4r' die Welt mein eigen,", "tokens": ["Dann", "w\u00e4r'", "die", "Welt", "mein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die ganze reiche Welt,", "tokens": ["Die", "gan\u00b7ze", "rei\u00b7che", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dann wollt' ich niedersteigen,", "tokens": ["Dann", "wollt'", "ich", "nie\u00b7ders\u00b7tei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wo's eben mir gef\u00e4llt;", "tokens": ["Wo's", "e\u00b7ben", "mir", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und jauchzend wollt ich umwinden", "tokens": ["Und", "jauch\u00b7zend", "wollt", "ich", "um\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Und k\u00fcssen ein liebliches Kind.", "tokens": ["Und", "k\u00fcs\u00b7sen", "ein", "lieb\u00b7li\u00b7ches", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "O sagt mir wo eines zu finden,", "tokens": ["O", "sagt", "mir", "wo", "ei\u00b7nes", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PWAV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "O sagt es geschwind.\u00ab ...", "tokens": ["O", "sagt", "es", "ge\u00b7schwind", ".", "\u00ab", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "$.", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Da schauert leise, leise", "tokens": ["Da", "schau\u00b7ert", "lei\u00b7se", ",", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In's Ohr ihm eine Weise,", "tokens": ["In's", "Ohr", "ihm", "ei\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So traurig, sehnsuchtsvoll,", "tokens": ["So", "trau\u00b7rig", ",", "sehn\u00b7suchts\u00b7voll", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und schmeichelnde Stimmen erwidern", "tokens": ["Und", "schmei\u00b7cheln\u00b7de", "Stim\u00b7men", "er\u00b7wi\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Mit hei\u00df-durchhauchten Liedern \u2013", "tokens": ["Mit", "hei\u00df\u00b7durch\u00b7hauch\u00b7ten", "Lie\u00b7dern", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wildes Schluchzen schwoll.", "tokens": ["Und", "wil\u00b7des", "Schluch\u00b7zen", "schwoll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbdas sind der Willis' Schaaren! \u2013", "tokens": ["\u00bb", "das", "sind", "der", "Wil\u00b7lis'", "Schaa\u00b7ren", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jetzt soll mich Gott bewahren.\u00ab", "tokens": ["Jetzt", "soll", "mich", "Gott", "be\u00b7wah\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nah und immer n\u00e4her", "tokens": ["Und", "nah", "und", "im\u00b7mer", "n\u00e4\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein blasser Reigen schwebt,", "tokens": ["Ein", "blas\u00b7ser", "Rei\u00b7gen", "schwebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und weh' und immer weher", "tokens": ["Und", "weh'", "und", "im\u00b7mer", "we\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein klagendes Singen bebt:", "tokens": ["Ein", "kla\u00b7gen\u00b7des", "Sin\u00b7gen", "bebt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbwir sind verstorbene Br\u00e4ute.", "tokens": ["\u00bb", "wir", "sind", "ver\u00b7stor\u00b7be\u00b7ne", "Br\u00e4u\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hochzeitliches Gel\u00e4ute", "tokens": ["Hoch\u00b7zeit\u00b7li\u00b7ches", "Ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Umklang uns nie, ach nie!", "tokens": ["Um\u00b7klang", "uns", "nie", ",", "ach", "nie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ITJ", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Uns gr\u00fc\u00dfte statt jauchzender Reigen", "tokens": ["Uns", "gr\u00fc\u00df\u00b7te", "statt", "jauch\u00b7zen\u00b7der", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Nur weinender Todesgeigen", "tokens": ["Nur", "wei\u00b7nen\u00b7der", "To\u00b7des\u00b7gei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Grabdunkle Melodie;", "tokens": ["Grab\u00b7dunk\u00b7le", "Me\u00b7lo\u00b7die", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir haben hei\u00df empfunden,", "tokens": ["Wir", "ha\u00b7ben", "hei\u00df", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch nie dem Geliebten verbunden,", "tokens": ["Doch", "nie", "dem", "Ge\u00b7lieb\u00b7ten", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Hat jede nur Liebe getr\u00e4umt,", "tokens": ["Hat", "je\u00b7de", "nur", "Lie\u00b7be", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADV", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Uns gl\u00e4ubig hinzugeben", "tokens": ["Uns", "gl\u00e4u\u00b7big", "hin\u00b7zu\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Auf Sterben und auf Leben \u2013", "tokens": ["Auf", "Ster\u00b7ben", "und", "auf", "Le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wir habens verschm\u00e4ht und vers\u00e4umt.", "tokens": ["Wir", "ha\u00b7bens", "ver\u00b7schm\u00e4ht", "und", "ver\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Nun sind wir schlafen 'gangen,", "tokens": ["Nun", "sind", "wir", "schla\u00b7fen", "'gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Doch loderndes Verlangen", "tokens": ["Doch", "lo\u00b7dern\u00b7des", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Aus Grab und Gruft uns hebt,", "tokens": ["Aus", "Grab", "und", "Gruft", "uns", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Nach K\u00fcssen, selig bangen,", "tokens": ["Nach", "K\u00fcs\u00b7sen", ",", "se\u00b7lig", "ban\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Nach z\u00e4rtlichem Umfangen", "tokens": ["Nach", "z\u00e4rt\u00b7li\u00b7chem", "Um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Die Seele st\u00f6hnt und bebt.", "tokens": ["Die", "See\u00b7le", "st\u00f6hnt", "und", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "O Du sollst selig werden,", "tokens": ["O", "Du", "sollst", "se\u00b7lig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.20": {"text": "Wie keiner noch auf Erden.", "tokens": ["Wie", "kei\u00b7ner", "noch", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "O komm' in unsern Kreis,", "tokens": ["O", "komm'", "in", "un\u00b7sern", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Wir wollen Dich umschlie\u00dfen,", "tokens": ["Wir", "wol\u00b7len", "Dich", "um\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Und jauchzen und genie\u00dfen \u2013", "tokens": ["Und", "jauch\u00b7zen", "und", "ge\u00b7nie\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Und k\u00fcssen \u2013 schwer und hei\u00df.\u00ab", "tokens": ["Und", "k\u00fcs\u00b7sen", "\u2013", "schwer", "und", "hei\u00df", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVINF", "$(", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Willis, blass vor Sehnen,", "tokens": ["Die", "Wil\u00b7lis", ",", "blass", "vor", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich ihm entgegen dehnen,", "tokens": ["Sich", "ihm", "ent\u00b7ge\u00b7gen", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "PTKVZ", "PDS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die dunklen Feueraugen", "tokens": ["Die", "dunk\u00b7len", "Feu\u00b7er\u00b7au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seine sich senken und saugen", "tokens": ["In", "sei\u00b7ne", "sich", "sen\u00b7ken", "und", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "PRF", "VVINF", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "So z\u00e4rtlich, s\u00fc\u00df und wild,", "tokens": ["So", "z\u00e4rt\u00b7lich", ",", "s\u00fc\u00df", "und", "wild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Lippen Purpurrosen", "tokens": ["Der", "Lip\u00b7pen", "Pur\u00b7pur\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die seinen weich umkosen,", "tokens": ["Die", "sei\u00b7nen", "weich", "um\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Hauch ihn warm umquillt \u2013", "tokens": ["Ihr", "Hauch", "ihn", "warm", "um\u00b7quillt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Da schn\u00fcrt auch ihm die Kehle", "tokens": ["Da", "schn\u00fcrt", "auch", "ihm", "die", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"t