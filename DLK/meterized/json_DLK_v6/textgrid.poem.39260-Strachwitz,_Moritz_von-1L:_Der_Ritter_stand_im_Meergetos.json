{"textgrid.poem.39260": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Ritter stand im Meergetos", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ritter stand im Meergetos", "tokens": ["Der", "Rit\u00b7ter", "stand", "im", "Meer\u00b7ge\u00b7tos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl hoch auf schwankem Mast,", "tokens": ["Wohl", "hoch", "auf", "schwan\u00b7kem", "Mast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es heult der Sturm, es schl\u00e4gt die See", "tokens": ["Es", "heult", "der", "Sturm", ",", "es", "schl\u00e4gt", "die", "See"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wild an die Heldenrast.", "tokens": ["Wild", "an", "die", "Hel\u00b7den\u00b7rast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Im Winde weht der Helmbusch wei\u00df,", "tokens": ["Im", "Win\u00b7de", "weht", "der", "Helm\u00b7busch", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Panzer blitzt im Strahl,", "tokens": ["Der", "Pan\u00b7zer", "blitzt", "im", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie fliegt des L\u00f6wen Auge hei\u00df", "tokens": ["Wie", "fliegt", "des", "L\u00f6\u00b7wen", "Au\u00b7ge", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs blaue Wogental.", "tokens": ["Durchs", "blau\u00b7e", "Wo\u00b7gen\u00b7tal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Banner rauscht in Sturmesnacht", "tokens": ["Das", "Ban\u00b7ner", "rauscht", "in", "Stur\u00b7mes\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Frankreich seinen Gru\u00df,", "tokens": ["Nach", "Fran\u00b7kreich", "sei\u00b7nen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Ritter auf der Segelwacht", "tokens": ["Der", "Rit\u00b7ter", "auf", "der", "Se\u00b7gel\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Steht stolz im Windesku\u00df.", "tokens": ["Steht", "stolz", "im", "Win\u00b7des\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da rauscht es dumpf, da klingt's empor", "tokens": ["Da", "rauscht", "es", "dumpf", ",", "da", "klingt's", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "NE", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl aus der Tiefe Nacht,", "tokens": ["Wohl", "aus", "der", "Tie\u00b7fe", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schl\u00e4gt es an des Ritters Ohr", "tokens": ["Da", "schl\u00e4gt", "es", "an", "des", "Rit\u00b7ters", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In wundervoller Macht.", "tokens": ["In", "wun\u00b7der\u00b7vol\u00b7ler", "Macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Aus tiefem Grund der Feye Mund", "tokens": ["Aus", "tie\u00b7fem", "Grund", "der", "Fe\u00b7ye", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erschallt im Zauberland,", "tokens": ["Er\u00b7schallt", "im", "Zau\u00b7ber\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welle ruht, es horcht der Sturm", "tokens": ["Die", "Wel\u00b7le", "ruht", ",", "es", "horcht", "der", "Sturm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kl\u00e4ngen wundertraut.", "tokens": ["Den", "Kl\u00e4n\u00b7gen", "wun\u00b7der\u00b7traut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbwir tragen Dich, wir heben Dich,", "tokens": ["\u00bb", "wir", "tra\u00b7gen", "Dich", ",", "wir", "he\u00b7ben", "Dich", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Leu! auf laue Flut,", "tokens": ["Du", "Leu", "!", "auf", "lau\u00b7e", "Flut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir k\u00fcssen, wir umschweben Dich", "tokens": ["Wir", "k\u00fcs\u00b7sen", ",", "wir", "um\u00b7schwe\u00b7ben", "Dich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In himmelsblauer Glut.\u00ab", "tokens": ["In", "him\u00b7mels\u00b7blau\u00b7er", "Glut", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbdu bist der Sohn der Meeresfey,", "tokens": ["\u00bb", "du", "bist", "der", "Sohn", "der", "Mee\u00b7res\u00b7fey", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Englands Lippe kost;", "tokens": ["Die", "En\u00b7glands", "Lip\u00b7pe", "kost", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum braust das Meer so hoch und frei,", "tokens": ["Drum", "braust", "das", "Meer", "so", "hoch", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das an die Klippe tost.\u00ab", "tokens": ["Das", "an", "die", "Klip\u00b7pe", "tost", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbdu Meereswoge, wild und kalt!", "tokens": ["\u00bb", "du", "Mee\u00b7res\u00b7wo\u00b7ge", ",", "wild", "und", "kalt", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie tobst Du raschen Grimms!", "tokens": ["Wie", "tobst", "Du", "ra\u00b7schen", "Grimms", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie schleuderst Du die Schaumesflut", "tokens": ["Wie", "schleu\u00b7derst", "Du", "die", "Schau\u00b7mes\u00b7flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Englands Felsensims!\u00ab", "tokens": ["An", "En\u00b7glands", "Fel\u00b7sen\u00b7sims", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbwie klammerst Du Dich inniglich", "tokens": ["\u00bb", "wie", "klam\u00b7merst", "Du", "Dich", "in\u00b7nig\u00b7lich"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An seines Busens Schnee,", "tokens": ["An", "sei\u00b7nes", "Bu\u00b7sens", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie rauschest Du so minniglich,", "tokens": ["Wie", "rau\u00b7schest", "Du", "so", "min\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du dunkelblaue See.\u00ab", "tokens": ["Du", "dun\u00b7kel\u00b7blau\u00b7e", "See", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbsiehst Du den L\u00f6wen flattern hoch,", "tokens": ["\u00bb", "siehst", "Du", "den", "L\u00f6\u00b7wen", "flat\u00b7tern", "hoch", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hei\u00df nach Frankreich schaut,", "tokens": ["Der", "hei\u00df", "nach", "Fran\u00b7kreich", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den L\u00f6wen von Britannien,", "tokens": ["Den", "L\u00f6\u00b7wen", "von", "Bri\u00b7tan\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Den Meeresbuhlen traut?\u00ab", "tokens": ["Den", "Mee\u00b7res\u00b7buh\u00b7len", "traut", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbdu L\u00f6wenbruder wundersch\u00f6n!", "tokens": ["\u00bb", "du", "L\u00f6\u00b7wen\u00b7bru\u00b7der", "wun\u00b7der\u00b7sch\u00f6n", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich gr\u00fc\u00dft der Wogen Lied,", "tokens": ["Dich", "gr\u00fc\u00dft", "der", "Wo\u00b7gen", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir klingt des Sturmes Schlachtget\u00f6n,", "tokens": ["Dir", "klingt", "des", "Stur\u00b7mes", "Schlacht\u00b7ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der rasche Bogen zieht.\u00ab", "tokens": ["Der", "ra\u00b7sche", "Bo\u00b7gen", "zieht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbzerrei\u00dfe Du des Franzmanns Stolz", "tokens": ["\u00bb", "zer\u00b7rei\u00b7\u00dfe", "Du", "des", "Franz\u00b7manns", "Stolz"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Deiner Klauen Wucht,", "tokens": ["Mit", "Dei\u00b7ner", "Klau\u00b7en", "Wucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir schaukeln Dich im Segelholz", "tokens": ["Wir", "schau\u00b7keln", "Dich", "im", "Se\u00b7gel\u00b7holz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur meeresblauen Bucht.\u00ab", "tokens": ["Zur", "mee\u00b7res\u00b7blau\u00b7en", "Bucht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbwir sch\u00fctzen Dir Dein England gr\u00fcn", "tokens": ["\u00bb", "wir", "sch\u00fct\u00b7zen", "Dir", "Dein", "En\u00b7gland", "gr\u00fcn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Sturm und Wogennacht,", "tokens": ["Mit", "Sturm", "und", "Wo\u00b7gen\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du ziehe hin, mein L\u00f6we k\u00fchn,", "tokens": ["Du", "zie\u00b7he", "hin", ",", "mein", "L\u00f6\u00b7we", "k\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In wetterschneller Macht.\u00ab", "tokens": ["In", "wet\u00b7ter\u00b7schnel\u00b7ler", "Macht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der Ritter horcht und staunt und sinnt.", "tokens": ["Der", "Rit\u00b7ter", "horcht", "und", "staunt", "und", "sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verklungen ist das Lied,", "tokens": ["Ver\u00b7klun\u00b7gen", "ist", "das", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ist die wei\u00dfe Lilie", "tokens": ["Da", "ist", "die", "wei\u00b7\u00dfe", "Li\u00b7lie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Todesnacht verbl\u00fcht.", "tokens": ["In", "To\u00b7des\u00b7nacht", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}