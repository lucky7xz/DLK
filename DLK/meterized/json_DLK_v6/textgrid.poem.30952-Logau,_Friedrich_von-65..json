{"textgrid.poem.30952": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r freyen Dienstbarkeit, w\u00e4r nicht was freyes dran,", "tokens": ["W\u00e4r", "frey\u00b7en", "Dienst\u00b7bar\u00b7keit", ",", "w\u00e4r", "nicht", "was", "frey\u00b7es", "dran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "VAFIN", "PTKNEG", "PWS", "ADJA", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es gienge keine Frau, kein Mann mehr diese Bahn.", "tokens": ["Es", "gien\u00b7ge", "kei\u00b7ne", "Frau", ",", "kein", "Mann", "mehr", "die\u00b7se", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie gehen aber drauff offt mehr als zweene G\u00e4nge;", "tokens": ["Sie", "ge\u00b7hen", "a\u00b7ber", "drauff", "offt", "mehr", "als", "zwee\u00b7ne", "G\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "ADV", "PIAT", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "W\u00e4r nun nichts gutes dran, man miede ja die Menge.", "tokens": ["W\u00e4r", "nun", "nichts", "gu\u00b7tes", "dran", ",", "man", "mie\u00b7de", "ja", "die", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "ADJA", "PAV", "$,", "PIS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}