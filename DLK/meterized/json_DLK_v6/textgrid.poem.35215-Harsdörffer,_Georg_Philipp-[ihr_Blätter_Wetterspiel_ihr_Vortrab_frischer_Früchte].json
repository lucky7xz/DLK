{"textgrid.poem.35215": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[ihr Bl\u00e4tter/ Wetterspiel/ ihr Vortrab frischer Fr\u00fcchte]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Bl\u00e4tter/ Wetterspiel/ ihr Vortrab frischer Fr\u00fcchte/", "tokens": ["Ihr", "Bl\u00e4t\u00b7ter", "/", "Wet\u00b7ter\u00b7spiel", "/", "ihr", "Vor\u00b7trab", "fri\u00b7scher", "Fr\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "$(", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Zephyrs Buhlgew\u00e4chs/ ihr leichte Lentzenbruht/", "tokens": ["Des", "Ze\u00b7phyrs", "Buhl\u00b7ge\u00b7w\u00e4chs", "/", "ihr", "leich\u00b7te", "Lent\u00b7zen\u00b7bruht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr macht/ da\u00df unsre Ruh im Schatt\u0113 pr\u00e4chtig ruht/", "tokens": ["Ihr", "macht", "/", "da\u00df", "uns\u00b7re", "Ruh", "im", "Schat\u00b7t\u0113", "pr\u00e4ch\u00b7tig", "ruht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie solten wir dann nicht euch dichten ein Gedichte.", "tokens": ["Wie", "sol\u00b7ten", "wir", "dann", "nicht", "euch", "dich\u00b7ten", "ein", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PTKNEG", "PPER", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Zweige Zittern rauscht/ und weidet das Gesichte/", "tokens": ["Der", "Zwei\u00b7ge", "Zit\u00b7tern", "rauscht", "/", "und", "wei\u00b7det", "das", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von euch k\u00f6mmt/ da\u00df wir oft aufkl\u00e4ren Sinn und Muht.", "tokens": ["Von", "euch", "k\u00f6mmt", "/", "da\u00df", "wir", "oft", "auf\u00b7kl\u00e4\u00b7ren", "Sinn", "und", "Muht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Nun Flora sch\u00fctz euch fort f\u00fcr harter Hagels-Wut/", "tokens": ["Nun", "Flo\u00b7ra", "sch\u00fctz", "euch", "fort", "f\u00fcr", "har\u00b7ter", "Ha\u00b7gels\u00b7Wut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es mache sp\u00e4tlich euch der Bl\u00e4ste Brast zu nichte/", "tokens": ["Es", "ma\u00b7che", "sp\u00e4t\u00b7lich", "euch", "der", "Bl\u00e4s\u00b7te", "Brast", "zu", "nich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "APPR", "PIS", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du aber/ fettes Gras/ Smaragden nahgesipt/", "tokens": ["Du", "a\u00b7ber", "/", "fet\u00b7tes", "Gras", "/", "Sma\u00b7rag\u00b7den", "nah\u00b7ge\u00b7sipt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ADJA", "NN", "$(", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du Blumenbunter Rokk der W\u00f6chnerinn der Erden/", "tokens": ["Du", "Blu\u00b7men\u00b7bun\u00b7ter", "Rokk", "der", "W\u00f6ch\u00b7ne\u00b7rinn", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wir zimmern ober dir oft unsrer Sorgen Sarg/", "tokens": ["Wir", "zim\u00b7mern", "o\u00b7ber", "dir", "oft", "uns\u00b7rer", "Sor\u00b7gen", "Sarg", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ADV", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Heerden speisest du/ und bist uns nimmer karg.", "tokens": ["Die", "Heer\u00b7den", "spei\u00b7sest", "du", "/", "und", "bist", "uns", "nim\u00b7mer", "karg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "KON", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wohlan/ du m\u00fcssest auch spat abgestreiffet werden/", "tokens": ["Wo\u00b7hlan", "/", "du", "m\u00fcs\u00b7sest", "auch", "spat", "ab\u00b7ge\u00b7streif\u00b7fet", "wer\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VMFIN", "ADV", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Spat werde deine Zier vom Nordwind abgeknipt.", "tokens": ["Spat", "wer\u00b7de", "dei\u00b7ne", "Zier", "vom", "Nord\u00b7wind", "ab\u00b7ge\u00b7knipt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}