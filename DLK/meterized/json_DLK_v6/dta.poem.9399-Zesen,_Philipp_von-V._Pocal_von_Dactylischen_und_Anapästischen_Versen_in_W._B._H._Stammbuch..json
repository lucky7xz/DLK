{"dta.poem.9399": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n  Pocal  \n von Dactylischen und Anap\u00e4stischen  \n Versen/  \n  in W. B. H. Stammbuch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jugend", "tokens": ["Ju\u00b7gend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "und tugend", "tokens": ["und", "tu\u00b7gend"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "steht artig beysam\u0303en/", "tokens": ["steht", "ar\u00b7tig", "bey\u00b7sam\u0303en", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "jugend", "tokens": ["ju\u00b7gend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "und Tugend", "tokens": ["und", "Tu\u00b7gend"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "in eyfrigen flam\u0303en", "tokens": ["in", "ey\u00b7fri\u00b7gen", "flam\u0303en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "leider! gar selten man\u0303 findt", "tokens": ["lei\u00b7der", "!", "gar", "sel\u00b7ten", "ma\u00f1", "findt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "ADJA", "NN", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Jtzo zu unseren zeiten entz\u00fcndt;", "tokens": ["Jt\u00b7zo", "zu", "un\u00b7se\u00b7ren", "zei\u00b7ten", "ent\u00b7z\u00fcndt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00fc\u00dfiggang/ laster und eitele sachen/", "tokens": ["M\u00fc\u00b7\u00dfig\u00b7gang", "/", "las\u00b7ter", "und", "ei\u00b7te\u00b7le", "sa\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "KON", "VVFIN", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "jtzund an Tugend vu\u0303 k\u00fcnste-stat wache\u0303/", "tokens": ["jt\u00b7zund", "an", "Tu\u00b7gend", "v\u0169", "k\u00fcns\u00b7te\u00b7stat", "wach\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "l\u00e4\u00dfer sich einer zur tugend scho\u0303 an/", "tokens": ["l\u00e4\u00b7\u00dfer", "sich", "ei\u00b7ner", "zur", "tu\u00b7gend", "sch\u00f5", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "APPRART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "folget d' tuge\u0303d un\u0303 findet die ban/", "tokens": ["fol\u00b7get", "d'", "tug\u1ebdd", "u\u00f1", "fin\u00b7det", "die", "ban", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "KON", "VVFIN", "ART", "NE", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "wird er geneidet in allen/", "tokens": ["wird", "er", "ge\u00b7nei\u00b7det", "in", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "PIAT", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "kan\u0303 keinem gefallen:", "tokens": ["ka\u00f1", "kei\u00b7nem", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "k\u00fcnste vergehen/", "tokens": ["k\u00fcns\u00b7te", "ver\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVINF", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "laster entstehe\u0303;", "tokens": ["las\u00b7ter", "ent\u00b7steh\u1ebd", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "bi\u00df alles vergehet/", "tokens": ["bi\u00df", "al\u00b7les", "ver\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "was stichelt un\u0303 schmehet/", "tokens": ["was", "sti\u00b7chelt", "u\u00f1", "schme\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "das unsrige vollend/ zerrin\u0303t", "tokens": ["das", "uns\u00b7ri\u00b7ge", "vol\u00b7lend", "/", "zer\u00b7ri\u00f1t"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "VVPP", "$(", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "und steubet wie wellen un\u0303 wind:", "tokens": ["und", "steu\u00b7bet", "wie", "wel\u00b7len", "u\u00f1", "wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "VVINF", "KON", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Achtestu dieses so bistu ja blind!", "tokens": ["Ach\u00b7tes\u00b7tu", "die\u00b7ses", "so", "bis\u00b7tu", "ja", "blind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}