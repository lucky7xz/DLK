{"textgrid.poem.31215": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Itzo/ da der Winter meist", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Itzo/ da der Winter meist", "tokens": ["It\u00b7zo", "/", "da", "der", "Win\u00b7ter", "meist"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "nichts wie Schnee und Hagel schmei\u00dft/", "tokens": ["nichts", "wie", "Schnee", "und", "Ha\u00b7gel", "schmei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "draut man sich au\u00df seinem Hau\u00df", "tokens": ["draut", "man", "sich", "au\u00df", "sei\u00b7nem", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "kaum mit halber Nase rau\u00df;", "tokens": ["kaum", "mit", "hal\u00b7ber", "Na\u00b7se", "rau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "denn es sind uns sonst die Ohren", "tokens": ["denn", "es", "sind", "uns", "sonst", "die", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "gleich gantz dikk mit Ey\u00df befroren.", "tokens": ["gleich", "gantz", "dikk", "mit", "Ey\u00df", "be\u00b7fro\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dr\u00fcmb so sezzt man seinen Sinn", "tokens": ["Dr\u00fcmb", "so", "sezzt", "man", "sei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "auff ein volles W\u00e4mbstrichin.", "tokens": ["auff", "ein", "vol\u00b7les", "W\u00e4mbs\u00b7tri\u00b7chin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eyer-Muh\u00df mit Amber dreyn", "tokens": ["E\u00b7yer\u00b7\u00b7M\u00b7uh\u00df", "mit", "Am\u00b7ber", "dreyn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "schlingert man in sich hinein/", "tokens": ["schlin\u00b7gert", "man", "in", "sich", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PRF", "APZR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und wie s\u00fc\u00df zum Koffe schm\u00e4kken", "tokens": ["und", "wie", "s\u00fc\u00df", "zum", "Kof\u00b7fe", "schm\u00e4k\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "morgens itzt die Botter-W\u00e4kken!", "tokens": ["mor\u00b7gens", "itzt", "die", "Bot\u00b7ter\u00b7W\u00e4k\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Karpen/ Stintckens/ Pl\u00f6tzckens/ H\u00e4cht/", "tokens": ["Kar\u00b7pen", "/", "Stint\u00b7ckens", "/", "Pl\u00f6tz\u00b7ckens", "/", "H\u00e4cht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NE", "$(", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "alles k\u00f6mbt uns itzo r\u00e4cht/", "tokens": ["al\u00b7les", "k\u00f6mbt", "uns", "it\u00b7zo", "r\u00e4cht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fcncken/ W\u00f6rste/ Sauer-Kraut", "tokens": ["Sch\u00fcn\u00b7cken", "/", "W\u00f6rs\u00b7te", "/", "Sau\u00b7e\u00b7rKraut"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$(", "NE", "$(", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und wa\u00df man noch sonst verdaut.", "tokens": ["und", "wa\u00df", "man", "noch", "sonst", "ver\u00b7daut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ingwergens und Citronaten", "tokens": ["Ing\u00b7wer\u00b7gens", "und", "Cit\u00b7ro\u00b7na\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sind itzt gleichfalls wohl-gerathen.", "tokens": ["sind", "itzt", "gleich\u00b7falls", "wohl\u00b7ge\u00b7rat\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hat man dan genug gebappt/", "tokens": ["Hat", "man", "dan", "ge\u00b7nug", "ge\u00b7bappt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00fchlt man/ da\u00df man kaum mehr jappt/", "tokens": ["f\u00fchlt", "man", "/", "da\u00df", "man", "kaum", "mehr", "jappt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$(", "KOUS", "PIS", "ADV", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "zihmbt ein Schl\u00fcckgen Aqwa vit/", "tokens": ["zihmbt", "ein", "Schl\u00fcck\u00b7gen", "A\u00b7qwa", "vit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "weil man nicht den Kirch-Thurm siht.", "tokens": ["weil", "man", "nicht", "den", "Kirch\u00b7Thurm", "siht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch man wei\u00df/ es ragt derselbe", "tokens": ["Doch", "man", "wei\u00df", "/", "es", "ragt", "der\u00b7sel\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$(", "PPER", "VVFIN", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "noch ins obre Blau-Gewelbe.", "tokens": ["noch", "ins", "ob\u00b7re", "Blau\u00b7Ge\u00b7wel\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Drauff so drukkt man Dorime", "tokens": ["Drauff", "so", "drukkt", "man", "Do\u00b7ri\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "PIS", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "z\u00e4hrtlig auff das Canape/", "tokens": ["z\u00e4hrt\u00b7lig", "auff", "das", "Ca\u00b7na\u00b7pe", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "butzt ihr Schnuhtzgen und enth\u00fcllt", "tokens": ["butzt", "ihr", "Schnuhtz\u00b7gen", "und", "ent\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wa\u00df ihr brall das Mihder f\u00fcllt;", "tokens": ["wa\u00df", "ihr", "brall", "das", "Mih\u00b7der", "f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "denn man mu\u00df nach solchen Sachen", "tokens": ["denn", "man", "mu\u00df", "nach", "sol\u00b7chen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sich ein Mouvementgen machen.", "tokens": ["sich", "ein", "Mou\u00b7ve\u00b7ment\u00b7gen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihrer \u00c4uglein flincker Lauff", "tokens": ["Ih\u00b7rer", "\u00c4ug\u00b7lein", "flin\u00b7cker", "Lauff"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "fordert uns zum Spihlen auff/", "tokens": ["for\u00b7dert", "uns", "zum", "Spih\u00b7len", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und sie kikkert und sie lacht/", "tokens": ["und", "sie", "kik\u00b7kert", "und", "sie", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "bi\u00df ihr pumps/ das B\u00e4ltzgen kracht.", "tokens": ["bi\u00df", "ihr", "pumps", "/", "das", "B\u00e4ltz\u00b7gen", "kracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So nur kan man mit Behagen", "tokens": ["So", "nur", "kan", "man", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}