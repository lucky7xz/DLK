{"dta.poem.2682": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber den seeligen Todt  \n  Hn. J. G. E.   M. D.   den 7. May  1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer/ Vetter/ deinen Tod mit trucknen Augen schauet/", "tokens": ["Wer", "/", "Vet\u00b7ter", "/", "dei\u00b7nen", "Tod", "mit", "truck\u00b7nen", "Au\u00b7gen", "schau\u00b7et", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$(", "NE", "$(", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ist ein harter Fels und unempfindlich Stein.", "tokens": ["Der", "ist", "ein", "har\u00b7ter", "Fels", "und", "un\u00b7emp\u00b7find\u00b7lich", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer deinem Grabe nicht ein Ehrenmahl auffbauet/", "tokens": ["Wer", "dei\u00b7nem", "Gra\u00b7be", "nicht", "ein", "Eh\u00b7ren\u00b7mahl", "auff\u00b7bau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mu\u00df im Helicon ein blosser Frembdling seyn.", "tokens": ["Der", "mu\u00df", "im", "He\u00b7li\u00b7con", "ein", "blos\u00b7ser", "Frembd\u00b7ling", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Kunst und Artzney Gott Apollo steht betr\u00fcbet/", "tokens": ["Der", "Kunst", "und", "Artz\u00b7ney", "Gott", "A\u00b7pol\u00b7lo", "steht", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "NE", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und reist vor Hertzeleid die Lorbern aus dem Haar/", "tokens": ["Und", "reist", "vor", "Hert\u00b7ze\u00b7leid", "die", "Lor\u00b7bern", "aus", "dem", "Haar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Giebt traurig an den Tag/ wie hoch er dich geliebet/", "tokens": ["Giebt", "trau\u00b7rig", "an", "den", "Tag", "/", "wie", "hoch", "er", "dich", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$(", "PWAV", "ADJD", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wie sein Meister Ruhm mit dir liegt auff der Baar.", "tokens": ["Und", "wie", "sein", "Meis\u00b7ter", "Ruhm", "mit", "dir", "liegt", "auff", "der", "Baar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "NN", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die keusche Daphne seufftzt und Flora schwimmt in Thr\u00e4nen/", "tokens": ["Die", "keu\u00b7sche", "Daph\u00b7ne", "seufftzt", "und", "Flo\u00b7ra", "schwimmt", "in", "Thr\u00e4\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die sie ohn unterla\u00df auff Blum und Kr\u00e4uter setzt.", "tokens": ["Die", "sie", "ohn", "un\u00b7ter\u00b7la\u00df", "auff", "Blum", "und", "Kr\u00e4u\u00b7ter", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Mutter aller Ding er\u00f6ffnet frey ihr Sehnen", "tokens": ["Die", "Mut\u00b7ter", "al\u00b7ler", "Ding", "er\u00b7\u00f6ff\u00b7net", "frey", "ihr", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil sie dich jederzeit als Rath und Trost gesch\u00e4tzt;", "tokens": ["Weil", "sie", "dich", "je\u00b7der\u00b7zeit", "als", "Rath", "und", "Trost", "ge\u00b7sch\u00e4tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "KOUS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hygea tritt hervor in einem weissen Kleide/", "tokens": ["Hy\u00b7gea", "tritt", "her\u00b7vor", "in", "ei\u00b7nem", "weis\u00b7sen", "Klei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Siht nicht so lebhafft sch\u00f6n als wie sie vormals pflag/", "tokens": ["Siht", "nicht", "so", "leb\u00b7hafft", "sch\u00f6n", "als", "wie", "sie", "vor\u00b7mals", "pflag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "VVFIN", "ADJD", "KOKOM", "PWAV", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Klagt deinen Untergang und geht in tieffem Leide/", "tokens": ["Klagt", "dei\u00b7nen", "Un\u00b7ter\u00b7gang", "und", "geht", "in", "tief\u00b7fem", "Lei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nun ihr Gesundheit Kelch dir nicht mehr nutzen mag.", "tokens": ["Nun", "ihr", "Ge\u00b7sund\u00b7heit", "Kelch", "dir", "nicht", "mehr", "nut\u00b7zen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "PPOSAT", "NN", "NN", "PPER", "PTKNEG", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Jhr Vater Aesculap durchforschet in Metallen", "tokens": ["Ihr", "Va\u00b7ter", "A\u00b7e\u00b7scu\u00b7lap", "durch\u00b7for\u00b7schet", "in", "Me\u00b7tal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Ob irgend eintzig Geist/ dir auffzuhelffen/ sey?", "tokens": ["Ob", "ir\u00b7gend", "eint\u00b7zig", "Geist", "/", "dir", "auff\u00b7zu\u00b7hel\u00b7ffen", "/", "sey", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "NN", "$(", "PPER", "VVINF", "$(", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach Eisenharter Schlu\u00df! du solst und must verfallen;", "tokens": ["Ach", "Ei\u00b7sen\u00b7har\u00b7ter", "Schlu\u00df", "!", "du", "solst", "und", "must", "ver\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "NN", "$.", "PPER", "VMFIN", "KON", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Hier macht noch Artzt noch Kunst dir vom Verwesen frey.", "tokens": ["Hier", "macht", "noch", "Artzt", "noch", "Kunst", "dir", "vom", "Ver\u00b7we\u00b7sen", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "ADV", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die H\u00e4upter voller Witz/ so alles auffgeschlossen", "tokens": ["Die", "H\u00e4up\u00b7ter", "vol\u00b7ler", "Witz", "/", "so", "al\u00b7les", "auff\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was in der Erden Scho\u00df hat die Natur versteckt/", "tokens": ["Was", "in", "der", "Er\u00b7den", "Scho\u00df", "hat", "die", "Na\u00b7tur", "ver\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Vermissen nur zu fr\u00fch dich ihren Mitgenossen/", "tokens": ["Ver\u00b7mis\u00b7sen", "nur", "zu", "fr\u00fch", "dich", "ih\u00b7ren", "Mit\u00b7ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKA", "ADJD", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und sind mit gleichem Weh der Traurigkeit bedeckt.", "tokens": ["Und", "sind", "mit", "glei\u00b7chem", "Weh", "der", "Trau\u00b7rig\u00b7keit", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du bist auch Thr\u00e4nen werth/ Apollens Aug und Hertze/", "tokens": ["Du", "bist", "auch", "Thr\u00e4\u00b7nen", "werth", "/", "A\u00b7pol\u00b7lens", "Aug", "und", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ADJD", "$(", "NE", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und gro\u00df ist der Verlust den man empfinden mu\u00df.", "tokens": ["Und", "gro\u00df", "ist", "der", "Ver\u00b7lust", "den", "man", "emp\u00b7fin\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dein edle Wissenschafft hat gleich der Sonnen Kertze", "tokens": ["Dein", "ed\u00b7le", "Wis\u00b7sen\u00b7schafft", "hat", "gleich", "der", "Son\u00b7nen", "Kert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit ihrer Stralen Krafft beherrscht der Lethe Flu\u00df.", "tokens": ["Mit", "ih\u00b7rer", "Stra\u00b7len", "Krafft", "be\u00b7herrscht", "der", "Le\u00b7the", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Acheron erschrack/ Coeytus flo\u00df zur\u00fccke/", "tokens": ["Der", "A\u00b7che\u00b7ron", "er\u00b7schrack", "/", "Coe\u00b7ytus", "flo\u00df", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "NE", "VVFIN", "PTKVZ", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Und Charon nehm nicht mehr so h\u00e4uffig F\u00e4hrgeld ein.", "tokens": ["Und", "Cha\u00b7ron", "nehm", "nicht", "mehr", "so", "h\u00e4uf\u00b7fig", "F\u00e4hr\u00b7geld", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PTKNEG", "ADV", "ADV", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Parcen starrten gar ob diesem Meisterst\u00fccke.", "tokens": ["Die", "Par\u00b7cen", "starr\u00b7ten", "gar", "ob", "die\u00b7sem", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KOUS", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn Krancke deine Cur hie\u00df frisch und munter seyn.", "tokens": ["Wenn", "Kran\u00b7cke", "dei\u00b7ne", "Cur", "hie\u00df", "frisch", "und", "mun\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was hast du nicht f\u00fcr Lust von Jugend auff getragen", "tokens": ["Was", "hast", "du", "nicht", "f\u00fcr", "Lust", "von", "Ju\u00b7gend", "auff", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "APPR", "NN", "APPR", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dem grossen Vater gleich an Ruhm und Kunst zu gehn;", "tokens": ["Dem", "gros\u00b7sen", "Va\u00b7ter", "gleich", "an", "Ruhm", "und", "Kunst", "zu", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie eyfrig war dein Flei\u00df im Lesen/ Forschen/ Fragen/", "tokens": ["Wie", "ey\u00b7frig", "war", "dein", "Flei\u00df", "im", "Le\u00b7sen", "/", "For\u00b7schen", "/", "Fra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und wie die alte Welt und neue zu verstehn.", "tokens": ["Und", "wie", "die", "al\u00b7te", "Welt", "und", "neu\u00b7e", "zu", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "KON", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Gantz Jehna r\u00fchmt dich noch/ da du den Grund geleget/", "tokens": ["Gantz", "Jeh\u00b7na", "r\u00fchmt", "dich", "noch", "/", "da", "du", "den", "Grund", "ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Worauff ein Podalir das Schlo\u00df der Wei\u00dfheit baut.", "tokens": ["Wo\u00b7rauff", "ein", "Po\u00b7da\u00b7lir", "das", "Schlo\u00df", "der", "Wei\u00df\u00b7heit", "baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wo du mit grossem Ruhm Streit-Fragen hast erreget/", "tokens": ["Wo", "du", "mit", "gros\u00b7sem", "Ruhm", "Streit\u00b7Fra\u00b7gen", "hast", "er\u00b7re\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und solche mit Vernunfft zu l\u00f6sen dir getraut.", "tokens": ["Und", "sol\u00b7che", "mit", "Ver\u00b7nunfft", "zu", "l\u00f6\u00b7sen", "dir", "ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "NN", "PTKZU", "VVINF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Drauff nahm dich Leiden an und wie\u00df dir seine Sch\u00e4tze", "tokens": ["Drauff", "nahm", "dich", "Lei\u00b7den", "an", "und", "wie\u00df", "dir", "sei\u00b7ne", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "w