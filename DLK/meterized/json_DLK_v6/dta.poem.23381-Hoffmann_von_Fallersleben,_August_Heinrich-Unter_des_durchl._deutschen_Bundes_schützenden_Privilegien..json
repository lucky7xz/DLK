{"dta.poem.23381": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unter des durchl. deutschen Bundes sch\u00fctzenden  \n Privilegien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo kann der Dichter froher sein", "tokens": ["Wo", "kann", "der", "Dich\u00b7ter", "fro\u00b7her", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singen so von allerhand,", "tokens": ["Und", "sin\u00b7gen", "so", "von", "al\u00b7ler\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Tugend, Freundschaft, Lieb' und Wein,", "tokens": ["Von", "Tu\u00b7gend", ",", "Freund\u00b7schaft", ",", "Lieb'", "und", "Wein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von K\u00f6nig, Gott und Vaterland,", "tokens": ["Von", "K\u00f6\u00b7nig", ",", "Gott", "und", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Unter des deutschen Bundes sch\u00fctzenden Privilegien?", "tokens": ["Un\u00b7ter", "des", "deut\u00b7schen", "Bun\u00b7des", "sch\u00fct\u00b7zen\u00b7den", "Pri\u00b7vi\u00b7le\u00b7gi\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "---+-+-+-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "Wo ist ein Land doch weit und breit,", "tokens": ["Wo", "ist", "ein", "Land", "doch", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Ihm Hab' und Gut und Ruhm vermehrt,", "tokens": ["Ihm", "Hab'", "und", "Gut", "und", "Ruhm", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "ADJD", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als wir es sehn, o Wunder!", "tokens": ["Als", "wir", "es", "sehn", ",", "o", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "$,", "FM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als wir es sehn jetzunder", "tokens": ["Als", "wir", "es", "sehn", "je\u00b7tzun\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Unter des deutschen Bundes sch\u00fctzenden Privilegien?", "tokens": ["Un\u00b7ter", "des", "deut\u00b7schen", "Bun\u00b7des", "sch\u00fct\u00b7zen\u00b7den", "Pri\u00b7vi\u00b7le\u00b7gi\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "---+-+-+-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "In Luft und Wasser, Wald und Feld", "tokens": ["In", "Luft", "und", "Was\u00b7ser", ",", "Wald", "und", "Feld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nirgend freier doch ein Thier,", "tokens": ["Ist", "nir\u00b7gend", "frei\u00b7er", "doch", "ein", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch singt kein Vogel in der Welt", "tokens": ["Auch", "singt", "kein", "Vo\u00b7gel", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch jemals freier noch als wir!", "tokens": ["Doch", "je\u00b7mals", "frei\u00b7er", "noch", "als", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie bin ich guter Dinge!", "tokens": ["Wie", "bin", "ich", "gu\u00b7ter", "Din\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich trinke, spring' und singe", "tokens": ["Ich", "trin\u00b7ke", ",", "spring'", "und", "sin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Unter des deutschen Bundes sch\u00fctzenden Privilegien.", "tokens": ["Un\u00b7ter", "des", "deut\u00b7schen", "Bun\u00b7des", "sch\u00fct\u00b7zen\u00b7den", "Pri\u00b7vi\u00b7le\u00b7gi\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "---+-+-+-+-+-+-", "measure": "unknown.measure.hexa"}}}}}