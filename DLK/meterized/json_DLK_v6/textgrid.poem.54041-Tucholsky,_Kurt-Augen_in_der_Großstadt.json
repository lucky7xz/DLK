{"textgrid.poem.54041": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Augen in der Gro\u00dfstadt", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du zur Arbeit gehst", "tokens": ["Wenn", "du", "zur", "Ar\u00b7beit", "gehst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "am fr\u00fchen Morgen,", "tokens": ["am", "fr\u00fc\u00b7hen", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "wenn du am Bahnhof stehst", "tokens": ["wenn", "du", "am", "Bahn\u00b7hof", "stehst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "mit deinen Sorgen:", "tokens": ["mit", "dei\u00b7nen", "Sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "da zeigt die Stadt", "tokens": ["da", "zeigt", "die", "Stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "dir asphaltglatt", "tokens": ["dir", "as\u00b7phalt\u00b7glatt"], "token_info": ["word", "word"], "pos": ["PPER", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "im Menschentrichter", "tokens": ["im", "Men\u00b7schen\u00b7trich\u00b7ter"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Millionen Gesichter:", "tokens": ["Mil\u00b7lion\u00b7en", "Ge\u00b7sich\u00b7ter", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Zwei fremde Augen, ein kurzer Blick,", "tokens": ["Zwei", "frem\u00b7de", "Au\u00b7gen", ",", "ein", "kur\u00b7zer", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "die Braue, Pupillen, die Lider \u2013", "tokens": ["die", "Brau\u00b7e", ",", "Pu\u00b7pil\u00b7len", ",", "die", "Li\u00b7der", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Was war das? vielleicht dein Lebensgl\u00fcck . . .", "tokens": ["Was", "war", "das", "?", "viel\u00b7leicht", "dein", "Le\u00b7bens\u00b7gl\u00fcck", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$.", "ADV", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "vorbei, verweht, nie wieder.", "tokens": ["vor\u00b7bei", ",", "ver\u00b7weht", ",", "nie", "wie\u00b7der", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "VVFIN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du gehst dein Leben lang", "tokens": ["Du", "gehst", "dein", "Le\u00b7ben", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "auf tausend Stra\u00dfen;", "tokens": ["auf", "tau\u00b7send", "Stra\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "du siehst auf deinem Gang,", "tokens": ["du", "siehst", "auf", "dei\u00b7nem", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "die dich verga\u00dfen.", "tokens": ["die", "dich", "ver\u00b7ga\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ein Auge winkt,", "tokens": ["Ein", "Au\u00b7ge", "winkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "die Seele klingt;", "tokens": ["die", "See\u00b7le", "klingt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "du hasts gefunden,", "tokens": ["du", "hasts", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "nur f\u00fcr Sekunden . . .", "tokens": ["nur", "f\u00fcr", "Se\u00b7kun\u00b7den", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "$.", "$.", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Zwei fremde Augen, ein kurzer Blick,", "tokens": ["Zwei", "frem\u00b7de", "Au\u00b7gen", ",", "ein", "kur\u00b7zer", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "die Braue, Pupillen, die Lider;", "tokens": ["die", "Brau\u00b7e", ",", "Pu\u00b7pil\u00b7len", 