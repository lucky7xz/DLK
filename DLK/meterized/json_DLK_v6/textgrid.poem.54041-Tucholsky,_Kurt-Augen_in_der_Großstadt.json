oken_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Was war das?", "tokens": ["Was", "war", "das", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Von der gro\u00dfen Menschheit ein St\u00fcck!", "tokens": ["Von", "der", "gro\u00b7\u00dfen", "Menschheit", "ein", "St\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Vorbei, verweht, nie wieder.", "tokens": ["Vor\u00b7bei", ",", "ver\u00b7weht", ",", "nie", "wie\u00b7der", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}