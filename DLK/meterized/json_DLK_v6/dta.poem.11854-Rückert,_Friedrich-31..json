{"dta.poem.11854": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch eine Kunst du lernst, solang du lernend bist,", "tokens": ["Welch", "ei\u00b7ne", "Kunst", "du", "lernst", ",", "so\u00b7lang", "du", "ler\u00b7nend", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird halbgelungnes selbst dich freuen lange Frist.", "tokens": ["Wird", "halb\u00b7ge\u00b7lung\u00b7nes", "selbst", "dich", "freu\u00b7en", "lan\u00b7ge", "Frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "ADV", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jemehr dann Meisterschaft sich wird dem Werk verbinden,", "tokens": ["Je\u00b7mehr", "dann", "Meis\u00b7ter\u00b7schaft", "sich", "wird", "dem", "Werk", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "PRF", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je k\u00fcrzer wird die Lust daran zusammenschwinden.", "tokens": ["Je", "k\u00fcr\u00b7zer", "wird", "die", "Lust", "da\u00b7ran", "zu\u00b7sam\u00b7men\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was erst auf Wochen hielt, h\u00e4lt bald nur noch auf Tage,", "tokens": ["Was", "erst", "auf", "Wo\u00b7chen", "hielt", ",", "h\u00e4lt", "bald", "nur", "noch", "auf", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald, was auf Tage, kaum noch Stunden in der Wage.", "tokens": ["Bald", ",", "was", "auf", "Ta\u00b7ge", ",", "kaum", "noch", "Stun\u00b7den", "in", "der", "Wa\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "APPR", "NN", "$,", "ADV", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Am Ende f\u00fchlest du ein Gl\u00fcck das so entspringt,", "tokens": ["Am", "En\u00b7de", "f\u00fch\u00b7lest", "du", "ein", "Gl\u00fcck", "das", "so", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur noch im Augenblick, wo dir das Werk gelingt.", "tokens": ["Nur", "noch", "im", "Au\u00b7gen\u00b7blick", ",", "wo", "dir", "das", "Werk", "ge\u00b7lingt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dann bleibt kein andrer Rath, als Arbeit fr\u00fch und spat,", "tokens": ["Dann", "bleibt", "kein", "an\u00b7drer", "Rath", ",", "als", "Ar\u00b7beit", "fr\u00fch", "und", "spat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,", "KOUS", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil nur das Thun dich freut, nicht die gethane That.", "tokens": ["Weil", "nur", "das", "Thun", "dich", "freut", ",", "nicht", "die", "ge\u00b7tha\u00b7ne", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Darum nicht klage du, und schaff nur immer zu!", "tokens": ["Da\u00b7rum", "nicht", "kla\u00b7ge", "du", ",", "und", "schaff", "nur", "im\u00b7mer", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sch\u00f6pfung selber schafft deswegen spat und fruh.", "tokens": ["Die", "Sch\u00f6p\u00b7fung", "sel\u00b7ber", "schafft", "des\u00b7we\u00b7gen", "spat", "und", "fruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PAV", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}