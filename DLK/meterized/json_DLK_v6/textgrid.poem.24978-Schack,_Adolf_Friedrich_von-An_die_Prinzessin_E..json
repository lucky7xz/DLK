{"textgrid.poem.24978": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An die Prinzessin E.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du l\u00e4chelst hold beim Morgengru\u00dfe,", "tokens": ["Du", "l\u00e4\u00b7chelst", "hold", "beim", "Mor\u00b7gen\u00b7gru\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als ob kein Gram auf Erden sei;", "tokens": ["Als", "ob", "kein", "Gram", "auf", "Er\u00b7den", "sei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIAT", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hold l\u00e4chelnd schwebst mit leichtem Fu\u00dfe", "tokens": ["Hold", "l\u00e4\u00b7chelnd", "schwebst", "mit", "leich\u00b7tem", "Fu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du abends mir im Tanz vorbei.", "tokens": ["Du", "a\u00b7bends", "mir", "im", "Tanz", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und doch \u2013 die Schwermut ahnen alle,", "tokens": ["Und", "doch", "\u2013", "die", "Schwer\u00b7mut", "ah\u00b7nen", "al\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ART", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die hin durch deine Seele schleicht;", "tokens": ["Die", "hin", "durch", "dei\u00b7ne", "See\u00b7le", "schleicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn fr\u00fch den Schwamm voll bittrer Galle", "tokens": ["Denn", "fr\u00fch", "den", "Schwamm", "voll", "bit\u00b7trer", "Gal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat dir die arge Welt gereicht.", "tokens": ["Hat", "dir", "die", "ar\u00b7ge", "Welt", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "An Herzen, die verzweifelnd brachen,", "tokens": ["An", "Her\u00b7zen", ",", "die", "ver\u00b7zwei\u00b7felnd", "bra\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lag deines, bis zum Tod betr\u00fcbt;", "tokens": ["Lag", "dei\u00b7nes", ",", "bis", "zum", "Tod", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "KOUS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So viel die Menschen dir versprachen", "tokens": ["So", "viel", "die", "Men\u00b7schen", "dir", "ver\u00b7spra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trug haben sie an dir ver\u00fcbt.", "tokens": ["Trug", "ha\u00b7ben", "sie", "an", "dir", "ver\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So la\u00df die falsche Maske sinken", "tokens": ["So", "la\u00df", "die", "fal\u00b7sche", "Mas\u00b7ke", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nimm den Festkranz aus dem Haar;", "tokens": ["Und", "nimm", "den", "Fest\u00b7kranz", "aus", "dem", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mag sich das laute Leben schminken,", "tokens": ["Mag", "sich", "das", "lau\u00b7te", "Le\u00b7ben", "schmin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PDS", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Einsamkeit ist ewig wahr.", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "ist", "e\u00b7wig", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gleich gilt vor ihr des Armen Kammer,", "tokens": ["Gleich", "gilt", "vor", "ihr", "des", "Ar\u00b7men", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das prachtgeschm\u00fcckte F\u00fcrstenhaus. \u2013", "tokens": ["Das", "pracht\u00b7ge\u00b7schm\u00fcck\u00b7te", "F\u00fcrs\u00b7ten\u00b7haus", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geh denn und weine deinen Jammer", "tokens": ["Geh", "denn", "und", "wei\u00b7ne", "dei\u00b7nen", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im dunklen St\u00fcbchen einsam aus.", "tokens": ["Im", "dunk\u00b7len", "St\u00fcb\u00b7chen", "ein\u00b7sam", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}