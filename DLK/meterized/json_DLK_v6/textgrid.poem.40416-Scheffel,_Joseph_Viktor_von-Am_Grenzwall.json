{"textgrid.poem.40416": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Am Grenzwall", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein R\u00f6mer stand in finstrer Nacht", "tokens": ["Ein", "R\u00f6\u00b7mer", "stand", "in", "finst\u00b7rer", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am deutschen Grenzwall Posten,", "tokens": ["Am", "deut\u00b7schen", "Grenz\u00b7wall", "Pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fern vom Kastell war seine Wacht,", "tokens": ["Fern", "vom", "Kas\u00b7tell", "war", "sei\u00b7ne", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Das Antlitz gegen Osten ...", "tokens": ["Das", "Ant\u00b7litz", "ge\u00b7gen", "Os\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da regt sich feindlich was am Flu\u00df,", "tokens": ["Da", "regt", "sich", "feind\u00b7lich", "was", "am", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da schleicht und hallt was leise ...", "tokens": ["Da", "schleicht", "und", "hallt", "was", "lei\u00b7se", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PWS", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kein P\u00e4an von Horazius,", "tokens": ["Kein", "P\u00e4\u00b7an", "von", "Ho\u00b7ra\u00b7zi\u00b7us", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Ganz wildfremd war die Weise:", "tokens": ["Ganz", "wild\u00b7fremd", "war", "die", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbha' ... hamm' ... hammer dich emol, emol, emol", "tokens": ["\u00bb", "ha'", "...", "hamm'", "...", "ham\u00b7mer", "dich", "e\u00b7mol", ",", "e\u00b7mol", ",", "e\u00b7mol"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["$(", "VVIMP", "$(", "VVIMP", "$(", "VVIMP", "PPER", "ADJD", "$,", "ADJD", "$,", "XY"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "An dei'm verrissene' Kamisol,", "tokens": ["An", "dei'm", "ver\u00b7ris\u00b7se\u00b7ne'", "Ka\u00b7mi\u00b7sol", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Du schlechter Kerl!\u00ab", "tokens": ["Du", "schlech\u00b7ter", "Kerl", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "An eine Jungfrau Chattenstamms", "tokens": ["An", "ei\u00b7ne", "Jung\u00b7frau", "Chat\u00b7ten\u00b7stamms"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hatt' er sein Herz vertandelt", "tokens": ["Hatt'", "er", "sein", "Herz", "ver\u00b7tan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und war ihr oft im Lederwams", "tokens": ["Und", "war", "ihr", "oft", "im", "Le\u00b7der\u00b7wams"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Kaufmann zugewandelt.", "tokens": ["Als", "Kauf\u00b7mann", "zu\u00b7ge\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt kam die Rache ... eins, zwei, drei!", "tokens": ["Jetzt", "kam", "die", "Ra\u00b7che", "...", "eins", ",", "zwei", ",", "drei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "PIS", "$,", "CARD", "$,", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jetzt war der Damm erklettert ...", "tokens": ["Jetzt", "war", "der", "Damm", "er\u00b7klet\u00b7tert", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt kam's wie wilder Katzen Schrei", "tokens": ["Jetzt", "kam's", "wie", "wil\u00b7der", "Kat\u00b7zen", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Keulenschlag geschmettert:", "tokens": ["Und", "Keu\u00b7len\u00b7schlag", "ge\u00b7schmet\u00b7tert", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbha' ... hamm' ... hammer dich emol, emol, emol", "tokens": ["\u00bb", "ha'", "...", "hamm'", "...", "ham\u00b7mer", "dich", "e\u00b7mol", ",", "e\u00b7mol", ",", "e\u00b7mol"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["$(", "VVIMP", "$(", "VVIMP", "$(", "VVIMP", "PPER", "ADJD", "$,", "ADJD", "$,", "XY"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "An dei'm verrissene' Kamisol,", "tokens": ["An", "dei'm", "ver\u00b7ris\u00b7se\u00b7ne'", "Ka\u00b7mi\u00b7sol", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Du schlechter Kerl!\u00ab", "tokens": ["Du", "schlech\u00b7ter", "Kerl", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Er zog sein Schwert, er blies sein Horn,", "tokens": ["Er", "zog", "sein", "Schwert", ",", "er", "blies", "sein", "Horn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Focht als geschulter Krieger,", "tokens": ["Focht", "als", "ge\u00b7schul\u00b7ter", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fruchtlos war Mut und R\u00f6merzorn,", "tokens": ["Frucht\u00b7los", "war", "Mut", "und", "R\u00f6\u00b7mer\u00b7zorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die Wilden blieben Sieger.", "tokens": ["Die", "Wil\u00b7den", "blie\u00b7ben", "Sie\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie banden ihn und trugen ihn", "tokens": ["Sie", "ban\u00b7den", "ihn", "und", "tru\u00b7gen", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie einen Sack von dannen;", "tokens": ["Wie", "ei\u00b7nen", "Sack", "von", "dan\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als die Kohort' am Platz erschien,", "tokens": ["Als", "die", "Ko\u00b7hort'", "am", "Platz", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Scholl's fern schon durch die Tannen:", "tokens": ["Scholl's", "fern", "schon", "durch", "die", "Tan\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "\u00bbha'... hamm'... hammer dich emol, emol, emol", "tokens": ["\u00bb", "ha'", "...", "hamm'", "...", "ham\u00b7mer", "dich", "e\u00b7mol", ",", "e\u00b7mol", ",", "e\u00b7mol"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["$(", "VVIMP", "$(", "VVIMP", "$(", "VVIMP", "PPER", "ADJD", "$,", "ADJD", "$,", "XY"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "An dei'm verrissene' Kamisol,", "tokens": ["An", "dei'm", "ver\u00b7ris\u00b7se\u00b7ne'", "Ka\u00b7mi\u00b7sol", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Du schlechter Kerl!\u00ab", "tokens": ["Du", "schlech\u00b7ter", "Kerl", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Versammelt war im heiligen Hain", "tokens": ["Ver\u00b7sam\u00b7melt", "war", "im", "hei\u00b7li\u00b7gen", "Hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Chatten Landsgemeinde,", "tokens": ["Der", "Chat\u00b7ten", "Lands\u00b7ge\u00b7mein\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Odinsjulfest einzuweihn", "tokens": ["Ihr", "O\u00b7dins\u00b7jul\u00b7fest", "ein\u00b7zu\u00b7weihn"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Opferblut vom Feinde.", "tokens": ["Mit", "Op\u00b7fer\u00b7blut", "vom", "Fein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der f\u00fchlt' sich schon als Bratenschmor", "tokens": ["Der", "f\u00fchlt'", "sich", "schon", "als", "Bra\u00b7ten\u00b7schmor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In der Barbaren Z\u00e4hnen,", "tokens": ["In", "der", "Bar\u00b7ba\u00b7ren", "Z\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Da sprang sein blonder Schatz hervor", "tokens": ["Da", "sprang", "sein", "blon\u00b7der", "Schatz", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und rief mit hei\u00dfen Tr\u00e4nen:", "tokens": ["Und", "rief", "mit", "hei\u00b7\u00dfen", "Tr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbha'... hamm'... hammer dich emol, emol, emol", "tokens": ["\u00bb", "ha'", "...", "hamm'", "...", "ham\u00b7mer", "dich", "e\u00b7mol", ",", "e\u00b7mol", ",", "e\u00b7mol"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["$(", "VVIMP", "$(", "VVIMP", "$(", "VVIMP", "PPER", "ADJD", "$,", "ADJD", "$,", "XY"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "An dei'm verrissene' Kamisol,", "tokens": ["An", "dei'm", "ver\u00b7ris\u00b7se\u00b7ne'", "Ka\u00b7mi\u00b7sol", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Du schlechter Kerl!\u00ab", "tokens": ["Du", "schlech\u00b7ter", "Kerl", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und alles Volk sprach tiefger\u00fchrt", "tokens": ["Und", "al\u00b7les", "Volk", "sprach", "tief\u00b7ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob solcher Wiederfindung:", "tokens": ["Ob", "sol\u00b7cher", "Wie\u00b7der\u00b7fin\u00b7dung", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbman geb' ihn frei und losgeschn\u00fcrt", "tokens": ["\u00bb", "man", "geb'", "ihn", "frei", "und", "los\u00b7ge\u00b7schn\u00fcrt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PIS", "VVFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Freundin zur Verbindung!", "tokens": ["Der", "Freun\u00b7din", "zur", "Ver\u00b7bin\u00b7dung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nimmt sie ihn hier vom Fleck als Frau,", "tokens": ["Nimmt", "sie", "ihn", "hier", "vom", "Fleck", "als", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sei alle Schuld verziehen.", "tokens": ["Sei", "al\u00b7le", "Schuld", "ver\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und heut noch wird im ganzen Gau", "tokens": ["Und", "heut", "noch", "wird", "im", "gan\u00b7zen", "Gau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als Festbardit geschrien:", "tokens": ["Als", "Fest\u00b7bar\u00b7dit", "ge\u00b7schri\u00b7en", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ha'... hamm'... hammer dich emol, emol, emol", "tokens": ["Ha'", "...", "hamm'", "...", "ham\u00b7mer", "dich", "e\u00b7mol", ",", "e\u00b7mol", ",", "e\u00b7mol"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$(", "VVIMP", "$(", "VVIMP", "PPER", "ADJD", "$,", "ADJD", "$,", "XY"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "An dei'm verrissene' Kamisol,", "tokens": ["An", "dei'm", "ver\u00b7ris\u00b7se\u00b7ne'", "Ka\u00b7mi\u00b7sol", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Du schlechter Kerl!\u00ab", "tokens": ["Du", "schlech\u00b7ter", "Kerl", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}