{"textgrid.poem.33351": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Stutzerlied", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4rrchen, sey nicht spr\u00f6de,", "tokens": ["N\u00e4rr\u00b7chen", ",", "sey", "nicht", "spr\u00f6\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Komm, und k\u00fcsse mich!", "tokens": ["Komm", ",", "und", "k\u00fcs\u00b7se", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "J\u00fcnger, warst du bl\u00f6de,", "tokens": ["J\u00fcn\u00b7ger", ",", "warst", "du", "bl\u00f6\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Aelter, zierst du dich.", "tokens": ["A\u00b7el\u00b7ter", ",", "zierst", "du", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wisse, nur ein Weilchen", "tokens": ["Wis\u00b7se", ",", "nur", "ein", "Weil\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sind die M\u00e4dchen sch\u00f6n,", "tokens": ["Sind", "die", "M\u00e4d\u00b7chen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "M\u00fcssen, wie die Veilchen,", "tokens": ["M\u00fcs\u00b7sen", ",", "wie", "die", "Veil\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Welken und vergeh'n.", "tokens": ["Wel\u00b7ken", "und", "ver\u00b7geh'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "VVFIN", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Jetzt nur sind, wie Seide,", "tokens": ["Jetzt", "nur", "sind", ",", "wie", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$,", "PWAV", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Deine H\u00e4ndchen weich;", "tokens": ["Dei\u00b7ne", "H\u00e4nd\u00b7chen", "weich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aber bald sind beide", "tokens": ["A\u00b7ber", "bald", "sind", "bei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Deinem Handschuh gleich.", "tokens": ["Dei\u00b7nem", "Hand\u00b7schuh", "gleich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt nur zeigt dein Schmunzeln", "tokens": ["Jetzt", "nur", "zeigt", "dein", "Schmun\u00b7zeln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Holde Gr\u00fcbchen mir:", "tokens": ["Hol\u00b7de", "Gr\u00fcb\u00b7chen", "mir", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bald umziehen Runzeln", "tokens": ["Bald", "um\u00b7zie\u00b7hen", "Run\u00b7zeln"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mund und Wange dir", "tokens": ["Mund", "und", "Wan\u00b7ge", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Jetzt nur, kleines N\u00e4rrchen,", "tokens": ["Jetzt", "nur", ",", "klei\u00b7nes", "N\u00e4rr\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist dein Busen voll,", "tokens": ["Ist", "dein", "Bu\u00b7sen", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und in wenig J\u00e4hrchen", "tokens": ["Und", "in", "we\u00b7nig", "J\u00e4hr\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist er schlapp und hohl.", "tokens": ["Ist", "er", "schlapp", "und", "hohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Jetzt nur sieht dein Leibchen", "tokens": ["Jetzt", "nur", "sieht", "dein", "Leib\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zirkelf\u00f6rmig aus;", "tokens": ["Zir\u00b7kel\u00b7f\u00f6r\u00b7mig", "aus", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bist du einst ein Weibchen,", "tokens": ["Bist", "du", "einst", "ein", "Weib\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wird ein Viereck d'raus.", "tokens": ["Wird", "ein", "Vier\u00b7eck", "d'raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Deine Augen funkeln", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "fun\u00b7keln"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jetzt nur, wei\u00dft du das?", "tokens": ["Jetzt", "nur", ",", "wei\u00dft", "du", "das", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "PPER", "PDS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wisse, bald verdunkeln", "tokens": ["Wis\u00b7se", ",", "bald", "ver\u00b7dun\u00b7keln"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sie, wie tr\u00fcbes Glas.", "tokens": ["Sie", ",", "wie", "tr\u00fc\u00b7bes", "Glas", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Jetzt nur dir zu F\u00fcssen", "tokens": ["Jetzt", "nur", "dir", "zu", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Siehst du Herrchen fleh'n;", "tokens": ["Siehst", "du", "Herr\u00b7chen", "fleh'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aelter wirst du m\u00fcssen", "tokens": ["A\u00b7el\u00b7ter", "wirst", "du", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Liebebetteln geh'n.", "tokens": ["Lie\u00b7be\u00b7bet\u00b7teln", "geh'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["NN", "VVFIN", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Jetzo gieb, und labe", "tokens": ["Jet\u00b7zo", "gieb", ",", "und", "la\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVIMP", "$,", "KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Freundlich jeden Gast,", "tokens": ["Freund\u00b7lich", "je\u00b7den", "Gast", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Spar' nicht deine Habe,", "tokens": ["Spa\u00b7r'", "nicht", "dei\u00b7ne", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis du nichts mehr hast.", "tokens": ["Bis", "du", "nichts", "mehr", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "K\u00fcsse, weil dein M\u00fcndchen", "tokens": ["K\u00fcs\u00b7se", ",", "weil", "dein", "M\u00fcnd\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Roth und k\u00fc\u00dflich ist;", "tokens": ["Roth", "und", "k\u00fc\u00df\u00b7lich", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denk', es kommt ein St\u00fcndchen,", "tokens": ["Denk'", ",", "es", "kommt", "ein", "St\u00fcnd\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wo dir's Niemand k\u00fc\u00dft.", "tokens": ["Wo", "dir's", "Nie\u00b7mand", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}