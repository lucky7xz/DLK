{"textgrid.poem.60536": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Stille See", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Felsen rings bewahren den stillen dunkeln See,", "tokens": ["Die", "Fel\u00b7sen", "rings", "be\u00b7wah\u00b7ren", "den", "stil\u00b7len", "dun\u00b7keln", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und auf den Gipfeln schimmert der zarte Sommerschnee.", "tokens": ["Und", "auf", "den", "Gip\u00b7feln", "schim\u00b7mert", "der", "zar\u00b7te", "Som\u00b7mer\u00b7schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der stille See getreulich l\u00e4\u00dft jedes Rlatt erscheinen,", "tokens": ["Der", "stil\u00b7le", "See", "ge\u00b7treu\u00b7lich", "l\u00e4\u00dft", "je\u00b7des", "Rlatt", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Treue ist zu schauen im Friedlichen und Reinen.", "tokens": ["Die", "Treu\u00b7e", "ist", "zu", "schau\u00b7en", "im", "Fried\u00b7li\u00b7chen", "und", "Rei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKZU", "VVINF", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}