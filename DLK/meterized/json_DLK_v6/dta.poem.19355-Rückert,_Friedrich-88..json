{"dta.poem.19355": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Ganzen offenbar gereicht es nicht zum Heil,", "tokens": ["Dem", "Gan\u00b7zen", "of\u00b7fen\u00b7bar", "ge\u00b7reicht", "es", "nicht", "zum", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn es beg\u00fcnstiget vor andern einen Theil;", "tokens": ["Wenn", "es", "be\u00b7g\u00fcns\u00b7ti\u00b7get", "vor", "an\u00b7dern", "ei\u00b7nen", "Theil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch auch dem Theile wird es nicht zum Heil gereichen,", "tokens": ["Doch", "auch", "dem", "Thei\u00b7le", "wird", "es", "nicht", "zum", "Heil", "ge\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sich beg\u00fcnstigt sieht vor allen seines gleichen:", "tokens": ["Der", "sich", "be\u00b7g\u00fcns\u00b7tigt", "sieht", "vor", "al\u00b7len", "sei\u00b7nes", "glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "VVFIN", "APPR", "PIAT", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der unbeg\u00fcnstigte wird zwar am Mangel sterben,", "tokens": ["Der", "un\u00b7be\u00b7g\u00fcns\u00b7tig\u00b7te", "wird", "zwar", "am", "Man\u00b7gel", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch der beg\u00fcnstigte vor Ueberflu\u00df verderben.", "tokens": ["Doch", "der", "be\u00b7g\u00fcns\u00b7tig\u00b7te", "vor", "Ue\u00b7berf\u00b7lu\u00df", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}