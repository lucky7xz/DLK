{"dta.poem.6811": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was hat ein weiser Mann auf dieser Welt", "tokens": ["Was", "hat", "ein", "wei\u00b7ser", "Mann", "auf", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr Wunder-Dinge nicht ans Licht gestellt!", "tokens": ["F\u00fcr", "Wun\u00b7der\u00b7Din\u00b7ge", "nicht", "ans", "Licht", "ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er hat ihm Augen selbst, indem er Glas polirt,", "tokens": ["Er", "hat", "ihm", "Au\u00b7gen", "selbst", ",", "in\u00b7dem", "er", "Glas", "po\u00b7lirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "ADV", "$,", "KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die anders, als die seinigen, formirt:", "tokens": ["Die", "an\u00b7ders", ",", "als", "die", "sei\u00b7ni\u00b7gen", ",", "for\u00b7mirt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "ART", "PPOSS", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da er der Erden Rund weit unter sich gelassen,", "tokens": ["Da", "er", "der", "Er\u00b7den", "Rund", "weit", "un\u00b7ter", "sich", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "ADJD", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wei\u00df ihn derselben Bau und Klumpen nicht zu fassen,", "tokens": ["Wei\u00df", "ihn", "der\u00b7sel\u00b7ben", "Bau", "und", "Klum\u00b7pen", "nicht", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "KON", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er hat im weiten All, nunmehr fast keine Schrancken.", "tokens": ["Er", "hat", "im", "wei\u00b7ten", "All", ",", "nun\u00b7mehr", "fast", "kei\u00b7ne", "Schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es kennen die Natur die forschende Gedancken,", "tokens": ["Es", "ken\u00b7nen", "die", "Na\u00b7tur", "die", "for\u00b7schen\u00b7de", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Er ahmt ihr nach, und wei\u00df sich ihrer zu bemeistern.", "tokens": ["Er", "ahmt", "ihr", "nach", ",", "und", "wei\u00df", "sich", "ih\u00b7rer", "zu", "be\u00b7meis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es ist nunmehr ja den erhabnen Geistern,", "tokens": ["Es", "ist", "nun\u00b7mehr", "ja", "den", "er\u00b7hab\u00b7nen", "Geis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die das, was wahr, erg\u00f6tzt, wenns ihnen sich entdeckt,", "tokens": ["Die", "das", ",", "was", "wahr", ",", "er\u00b7g\u00f6tzt", ",", "wenns", "ih\u00b7nen", "sich", "ent\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "$,", "PWS", "ADJD", "$,", "VVPP", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da sie von GOTTES Wunderwercken", "tokens": ["Da", "sie", "von", "GoT\u00b7TES", "Wun\u00b7der\u00b7wer\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die ungez\u00e4hlte Zahl und Herrlichkeit bemercken;", "tokens": ["Die", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Zahl", "und", "Herr\u00b7lich\u00b7keit", "be\u00b7mer\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Fast kein Geheimni\u00df mehr versteckt.", "tokens": ["Fast", "kein", "Ge\u00b7heim\u00b7ni\u00df", "mehr", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}