{"textgrid.poem.42680": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gerne h\u00e4tt' ich Hymens Flitterwochen,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gerne h\u00e4tt' ich Hymens Flitterwochen,", "tokens": ["Ger\u00b7ne", "h\u00e4tt'", "ich", "Hy\u00b7mens", "Flit\u00b7ter\u00b7wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo, o Freund! sich Freud' an Freude dr\u00e4ngt,", "tokens": ["Wo", ",", "o", "Freund", "!", "sich", "Freud'", "an", "Freu\u00b7de", "dr\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "NN", "$.", "PRF", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Himmel voller Geigen h\u00e4ngt,", "tokens": ["Und", "der", "Him\u00b7mel", "vol\u00b7ler", "Gei\u00b7gen", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hie und da ein St\u00fcndchen abgebrochen,", "tokens": ["Hie", "und", "da", "ein", "St\u00fcnd\u00b7chen", "ab\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Gerne manchen spr\u00f6den Reim gehascht,", "tokens": ["Ger\u00b7ne", "man\u00b7chen", "spr\u00f6\u00b7den", "Reim", "ge\u00b7hascht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und dich f\u00fcr dein stattlich Hochzeitkarmen", "tokens": ["Und", "dich", "f\u00fcr", "dein", "statt\u00b7lich", "Hoch\u00b7zeit\u00b7kar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Pl\u00f6tzlich durch ein Danklied \u00fcberrascht.", "tokens": ["Pl\u00f6tz\u00b7lich", "durch", "ein", "Dank\u00b7lied", "\u00fc\u00b7berr\u00b7ascht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Aber konnt' ich aus geliebten Armen", "tokens": ["A\u00b7ber", "konnt'", "ich", "aus", "ge\u00b7lieb\u00b7ten", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Stoisch fliehn, dem Zaubernetz der Lust", "tokens": ["Sto\u00b7isch", "fliehn", ",", "dem", "Zau\u00b7ber\u00b7netz", "der", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Trotzig mich entwinden, und durch Reimen,", "tokens": ["Trot\u00b7zig", "mich", "ent\u00b7win\u00b7den", ",", "und", "durch", "Rei\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "$,", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Kalt an einer jungen Gattinn Brust,", "tokens": ["Kalt", "an", "ei\u00b7ner", "jun\u00b7gen", "Gat\u00b7tinn", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Hymens s\u00fcssen Opferdienst vers\u00e4umen?", "tokens": ["Hy\u00b7mens", "s\u00fcs\u00b7sen", "Op\u00b7fer\u00b7dienst", "ver\u00b7s\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Lieber! solch ein Kriminalvergehn", "tokens": ["Lie\u00b7ber", "!", "solch", "ein", "Kri\u00b7mi\u00b7nal\u00b7ver\u00b7gehn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "PIAT", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "W\u00fcrde mir, der nun in seinem neuen", "tokens": ["W\u00fcr\u00b7de", "mir", ",", "der", "nun", "in", "sei\u00b7nem", "neu\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "\u00c4mtchen w\u00fcnscht mit Ehren zu bestehn,", "tokens": ["\u00c4mt\u00b7chen", "w\u00fcnscht", "mit", "Eh\u00b7ren", "zu", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Mein geliebtes Weibchen nie verzeihen.", "tokens": ["Mein", "ge\u00b7lieb\u00b7tes", "Weib\u00b7chen", "nie", "ver\u00b7zei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn dein Freund nun noch dich str\u00e4flich d\u00e4ucht,", "tokens": ["Wenn", "dein", "Freund", "nun", "noch", "dich", "str\u00e4f\u00b7lich", "d\u00e4ucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "So bedenk', es machte wohl vielleicht", "tokens": ["So", "be\u00b7denk'", ",", "es", "mach\u00b7te", "wohl", "viel\u00b7leicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eines Weibes Kuss auch dich zum S\u00fcnder.", "tokens": ["Ei\u00b7nes", "Wei\u00b7bes", "Kuss", "auch", "dich", "zum", "S\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Drum vergieb, mein Bester! Sp\u00e4t erscheint", "tokens": ["Drum", "ver\u00b7gieb", ",", "mein", "Bes\u00b7ter", "!", "Sp\u00e4t", "er\u00b7scheint"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "PPOSAT", "NN", "$.", "ADJD", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Zwar mein Dank: doch ist er drum nicht minder", "tokens": ["Zwar", "mein", "Dank", ":", "doch", "ist", "er", "drum", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "PAV", "PTKNEG", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Liebevoll, nicht minder gut gemeint.", "tokens": ["Lie\u00b7be\u00b7voll", ",", "nicht", "min\u00b7der", "gut", "ge\u00b7meint", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PTKNEG", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Herzlich gr\u00fcss' ich dich in meinem, Freund!", "tokens": ["Herz\u00b7lich", "gr\u00fcss'", "ich", "dich", "in", "mei\u00b7nem", ",", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und in meiner trauten Gattinn Namen.", "tokens": ["Und", "in", "mei\u00b7ner", "trau\u00b7ten", "Gat\u00b7tinn", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Freudig las ich ihr dein Liedchen vor:", "tokens": ["Freu\u00b7dig", "las", "ich", "ihr", "dein", "Lied\u00b7chen", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Sieh! sie horchte mit entz\u00fccktem Ohr,", "tokens": ["Sieh", "!", "sie", "horch\u00b7te", "mit", "ent\u00b7z\u00fcck\u00b7tem", "Ohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Und als wir zur Katastrophe kamen,", "tokens": ["Und", "als", "wir", "zur", "Ka\u00b7tast\u00b7ro\u00b7phe", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Die den wonniglichen Wunsch enth\u00e4lt,", "tokens": ["Die", "den", "won\u00b7nig\u00b7li\u00b7chen", "Wunsch", "ent\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Dass ein Sohn, erzeugt aus meinem Samen,", "tokens": ["Dass", "ein", "Sohn", ",", "er\u00b7zeugt", "aus", "mei\u00b7nem", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Mir erwachse, den die Afterwelt", "tokens": ["Mir", "er\u00b7wach\u00b7se", ",", "den", "die", "Af\u00b7ter\u00b7welt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Einst den Biederm\u00e4nnern zugesellt,", "tokens": ["Einst", "den", "Bie\u00b7der\u00b7m\u00e4n\u00b7nern", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Fiel sie um den Hals mir, und rief: Amen!", "tokens": ["Fiel", "sie", "um", "den", "Hals", "mir", ",", "und", "rief", ":", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "$,", "KON", "VVFIN", "$.", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nun wohlan! ihr Wille soll geschehn!", "tokens": ["Nun", "wo\u00b7hlan", "!", "ihr", "Wil\u00b7le", "soll", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ungeschw\u00e4cht von Amors Dienst, durch den", "tokens": ["Un\u00b7ge\u00b7schw\u00e4cht", "von", "A\u00b7mors", "Dienst", ",", "durch", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NE", "NN", "$,", "APPR", "ART"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Junge Schwelger vor der Zeit zu schlaffen", "tokens": ["Jun\u00b7ge", "Schwel\u00b7ger", "vor", "der", "Zeit", "zu", "schlaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Greisen werden, will ich wohlgemuth", "tokens": ["Grei\u00b7sen", "wer\u00b7den", ",", "will", "ich", "wohl\u00b7ge\u00b7muth"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAINF", "$,", "VMFIN", "PPER", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Eine kleine Welt um mich erschaffen,", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", "Welt", "um", "mich", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und, in meinem frohen Lilliput", "tokens": ["Und", ",", "in", "mei\u00b7nem", "fro\u00b7hen", "Lil\u00b7li\u00b7put"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Eingebannt, mich meines Lebens freuen.", "tokens": ["Ein\u00b7ge\u00b7bannt", ",", "mich", "mei\u00b7nes", "Le\u00b7bens", "freu\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie ein Schiff, das in dem Hafen ruht,", "tokens": ["Wie", "ein", "Schiff", ",", "das", "in", "dem", "Ha\u00b7fen", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Trotz' ich sorglos dann des ungetreuen", "tokens": ["Trotz'", "ich", "sorg\u00b7los", "dann", "des", "un\u00b7ge\u00b7treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Gl\u00fcckes wandelbarer Ebb' und Flut,", "tokens": ["Gl\u00fc\u00b7ckes", "wan\u00b7del\u00b7ba\u00b7rer", "Ebb'", "und", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Und hat einst auf ihrem schnellen R\u00e4dchen", "tokens": ["Und", "hat", "einst", "auf", "ih\u00b7rem", "schnel\u00b7len", "R\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Klotho mir das letzte Lebensf\u00e4dchen", "tokens": ["Klo\u00b7tho", "mir", "das", "letz\u00b7te", "Le\u00b7bens\u00b7f\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "ADJA", "NN"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.13": {"text": "Abgesponnen, rafft mit kalter Hand", "tokens": ["Ab\u00b7ge\u00b7spon\u00b7nen", ",", "rafft", "mit", "kal\u00b7ter", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Mich der Tod von meiner Gattinn Seite", "tokens": ["Mich", "der", "Tod", "von", "mei\u00b7ner", "Gat\u00b7tinn", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Hin nach jenem ew'gen Otaheite,", "tokens": ["Hin", "nach", "je\u00b7nem", "ew'\u00b7gen", "O\u00b7ta\u00b7hei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Das bisher kein k\u00fchner Cook noch fand,", "tokens": ["Das", "bis\u00b7her", "kein", "k\u00fch\u00b7ner", "Cook", "noch", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und kein Mann, wie Forster, je beschrieben,", "tokens": ["Und", "kein", "Mann", ",", "wie", "Fors\u00b7ter", ",", "je", "be\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PWAV", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "O so dr\u00fccke meiner guten lieben", "tokens": ["O", "so", "dr\u00fc\u00b7cke", "mei\u00b7ner", "gu\u00b7ten", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPOSAT", "ADJA", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Kinder Erstling mir die Augen zu!", "tokens": ["Kin\u00b7der", "Erst\u00b7ling", "mir", "die", "Au\u00b7gen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Ein bew\u00e4hrter treuer Freund, wie du,", "tokens": ["Ein", "be\u00b7w\u00e4hr\u00b7ter", "treu\u00b7er", "Freund", ",", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PWAV", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Folge meinem stillen Leichenzuge,", "tokens": ["Fol\u00b7ge", "mei\u00b7nem", "stil\u00b7len", "Lei\u00b7chen\u00b7zu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Und es schall' an meinem Aschenkruge", "tokens": ["Und", "es", "schall'", "an", "mei\u00b7nem", "A\u00b7schen\u00b7kru\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Laut das Zeugniss: dieser Leichenstein", "tokens": ["Laut", "das", "Zeug\u00b7niss", ":", "die\u00b7ser", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "PDAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Decket eines braven Manns Gebein!", "tokens": ["De\u00b7cket", "ei\u00b7nes", "bra\u00b7ven", "Manns", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}