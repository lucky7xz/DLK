{"dta.poem.19933": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Rosenstock .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Siehe! die Wurzel, sie liegt im schweigenden Dunkel be-", "tokens": ["Sie\u00b7he", "!", "die", "Wur\u00b7zel", ",", "sie", "liegt", "im", "schwei\u00b7gen\u00b7den", "Dun\u00b7kel", "be"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "graben,", "tokens": ["gra\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Einsam und finster, geh\u00f6rt diese der ewigen Nacht.", "tokens": ["Ein\u00b7sam", "und", "fins\u00b7ter", ",", "ge\u00b7h\u00f6rt", "die\u00b7se", "der", "e\u00b7wi\u00b7gen", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "VVFIN", "PDS", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Oben entfalten sich drauf die gr\u00fcnen Bl\u00e4tter, die Dorne,", "tokens": ["O\u00b7ben", "ent\u00b7fal\u00b7ten", "sich", "drauf", "die", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", ",", "die", "Dor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PAV", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Bild der Erde sind sie, deuten auf Hoffnung und Schmerz.", "tokens": ["Bild", "der", "Er\u00b7de", "sind", "sie", ",", "deu\u00b7ten", "auf", "Hoff\u00b7nung", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Ob der Wurzel voll Nacht, ob gr\u00fcnen Bl\u00e4ttern und Dornen", "tokens": ["Ob", "der", "Wur\u00b7zel", "voll", "Nacht", ",", "ob", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7tern", "und", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "NN", "$,", "KOUS", "ADJA", "NN", "KON", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Stehet ein jugendlich Roth, bl\u00fchet die Rose voll Glut.", "tokens": ["Ste\u00b7het", "ein", "ju\u00b7gend\u00b7lich", "Roth", ",", "bl\u00fc\u00b7het", "die", "Ro\u00b7se", "voll", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}