{"dta.poem.4286": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das wilde Pferd.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bebt und zittert nicht der Boden? Welch ein Strampfen", "tokens": ["Bebt", "und", "zit\u00b7tert", "nicht", "der", "Bo\u00b7den", "?", "Welch", "ein", "Stramp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$.", "PIAT", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "h\u00f6rt man hier?", "tokens": ["h\u00f6rt", "man", "hier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Welch ein Schnauben f\u00fcllt die Luft! Weicht! ein wild-doch", "tokens": ["Welch", "ein", "Schnau\u00b7ben", "f\u00fcllt", "die", "Luft", "!", "Weicht", "!", "ein", "wild\u00b7doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "NN", "VVFIN", "ART", "NN", "$.", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "sch\u00f6nes Thier", "tokens": ["sch\u00f6\u00b7nes", "Thier"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Sprengt daher in vollem Rennen! Halt! es stutzt, es b\u00e4umet", "tokens": ["Sprengt", "da\u00b7her", "in", "vol\u00b7lem", "Ren\u00b7nen", "!", "Halt", "!", "es", "stutzt", ",", "es", "b\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PAV", "APPR", "ADJA", "NN", "$.", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "sich.", "tokens": ["sich", "."], "token_info": ["word", "punct"], "pos": ["PRF", "$."], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Scheint es doch sich aufzuhalten, da\u00df man recht und eigent-", "tokens": ["Scheint", "es", "doch", "sich", "auf\u00b7zu\u00b7hal\u00b7ten", ",", "da\u00df", "man", "recht", "und", "ei\u00b7gent"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "VVIZU", "$,", "KOUS", "PIS", "ADJD", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "lich", "tokens": ["lich"], "token_info": ["word"], "pos": ["ADJD"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Seines C\u00f6rpers sch\u00f6nen Bau, seines Hufs und Schenkels", "tokens": ["Sei\u00b7nes", "C\u00f6r\u00b7pers", "sch\u00f6\u00b7nen", "Bau", ",", "sei\u00b7nes", "Hufs", "und", "Schen\u00b7kels"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Zier,", "tokens": ["Zier", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Seinen edelm\u00fcht\u2019gen Anstand, seinen Wuchs betrachten", "tokens": ["Sei\u00b7nen", "e\u00b7del\u00b7m\u00fcht'\u00b7gen", "An\u00b7stand", ",", "sei\u00b7nen", "Wuchs", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "k\u00f6nne,", "tokens": ["k\u00f6n\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Und geb\u00fchrende Bewundrung Dem, der es erschaffen,", "tokens": ["Und", "ge\u00b7b\u00fch\u00b7ren\u00b7de", "Be\u00b7wund\u00b7rung", "Dem", ",", "der", "es", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+---+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.14": {"text": "g\u00f6nne.", "tokens": ["g\u00f6n\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Welch ein hell- und ernster Strahl blitzt aus seinem Aug\u2019", "tokens": ["Welch", "ein", "hell", "und", "erns\u00b7ter", "Strahl", "blitzt", "aus", "sei\u00b7nem", "Aug'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "TRUNC", "KON", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "herf\u00fcr!", "tokens": ["her\u00b7f\u00fcr", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Die verworrne M\u00e4hne flieget. Seht, wie spielt sein", "tokens": ["Die", "ver\u00b7worr\u00b7ne", "M\u00e4h\u00b7ne", "flie\u00b7get", ".", "Seht", ",", "wie", "spielt", "sein"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "VVFIN", "$,", "PWAV", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "reges Ohr!", "tokens": ["re\u00b7ges", "Ohr", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Seht, es peitscht sein Schweif die Luft! Seht, wie an ver-", "tokens": ["Seht", ",", "es", "peitscht", "sein", "Schweif", "die", "Luft", "!", "Seht", ",", "wie", "an", "ver"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$.", "VVFIN", "$,", "PWAV", "APPR", "TRUNC"], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "schiednen Stellen,", "tokens": ["schied\u00b7nen", "Stel\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Von dem wallenden Gebl\u00fct, die gedehnten Adern schwellen.", "tokens": ["Von", "dem", "wal\u00b7len\u00b7den", "Ge\u00b7bl\u00fct", ",", "die", "ge\u00b7dehn\u00b7ten", "A\u00b7dern", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Bricht doch aus geschw\u00e4rzten Wolken kaum so hell ein Stern", "tokens": ["Bricht", "doch", "aus", "ge\u00b7schw\u00e4rz\u00b7ten", "Wol\u00b7ken", "kaum", "so", "hell", "ein", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "ADV", "ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "hervor,", "tokens": ["her\u00b7vor", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Als der Stern an seiner Stirn, von dem dunklen Haar", "tokens": ["Als", "der", "Stern", "an", "sei\u00b7ner", "Stirn", ",", "von", "dem", "dunk\u00b7len", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "begrenzet,", "tokens": ["be\u00b7gren\u00b7zet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Mitten an dem netten Kopf, zwischen beyden Augen, gl\u00e4nzet.", "tokens": ["Mit\u00b7ten", "an", "dem", "net\u00b7ten", "Kopf", ",", "zwi\u00b7schen", "bey\u00b7den", "Au\u00b7gen", ",", "gl\u00e4n\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie die\u00df Thier ein Meisterst\u00fcck von der bildenden Natur;", "tokens": ["Wie", "die\u00df", "Thier", "ein", "Meis\u00b7ter\u00b7st\u00fcck", "von", "der", "bil\u00b7den\u00b7den", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "So ist die durch wenig Striche hier gebildete Figur", "tokens": ["So", "ist", "die", "durch", "we\u00b7nig", "Stri\u00b7che", "hier", "ge\u00b7bil\u00b7de\u00b7te", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "APPR", "PIAT", "NN", "ADV", "ADJA", "NN"], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Auch des Meisters Meisterst\u00fcck. La\u00df die\u00df Bild das letzte", "tokens": ["Auch", "des", "Meis\u00b7ters", "Meis\u00b7ter\u00b7st\u00fcck", ".", "La\u00df", "die\u00df", "Bild", "das", "letz\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "$.", "VVIMP", "PDS", "NN", "ART", "ADJA"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "seyn,", "tokens": ["seyn", ","], "token_info": ["word", "punct"], "pos": ["VAINF", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ich kann dir nicht weiter folgen, ", "tokens": ["Ich", "kann", "dir", "nicht", "wei\u00b7ter", "fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "allein.", "tokens": ["al\u00b7lein", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}}}}}