{"textgrid.poem.40142": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Abendgang", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Flur will ruhn:", "tokens": ["Die", "Flur", "will", "ruhn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "in Halmen, Zweigen", "tokens": ["in", "Hal\u00b7men", ",", "Zwei\u00b7gen"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "ein leises Neigen.", "tokens": ["ein", "lei\u00b7ses", "Nei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Aus Wiesen nun", "tokens": ["Aus", "Wie\u00b7sen", "nun"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "die Nebel steigen:", "tokens": ["die", "Ne\u00b7bel", "stei\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ob's wohl zu h\u00f6ren?", "tokens": ["Ob's", "wohl", "zu", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "lauschen ich will ...", "tokens": ["lau\u00b7schen", "ich", "will", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Still, Liebchen, still:", "tokens": ["Still", ",", "Lieb\u00b7chen", ",", "still", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "wir st\u00f6ren", "tokens": ["wir", "st\u00f6\u00b7ren"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "dies sel'ge Schweigen!", "tokens": ["dies", "sel'\u00b7ge", "Schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}