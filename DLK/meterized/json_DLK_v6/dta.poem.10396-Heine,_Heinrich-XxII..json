{"dta.poem.10396": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Und w\u00fc\u00dften's die Blumen, die kleinen,", "tokens": ["Und", "w\u00fc\u00df\u00b7ten's", "die", "Blu\u00b7men", ",", "die", "klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie tief verwundet mein Herz,", "tokens": ["Wie", "tief", "ver\u00b7wun\u00b7det", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie w\u00fcrden mit mir weinen,", "tokens": ["Sie", "w\u00fcr\u00b7den", "mit", "mir", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu heilen meinen Schmerz.", "tokens": ["Zu", "hei\u00b7len", "mei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und w\u00fc\u00dften's die Nachtigallen", "tokens": ["Und", "w\u00fc\u00df\u00b7ten's", "die", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ich so traurig und krank,", "tokens": ["Wie", "ich", "so", "trau\u00b7rig", "und", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie lie\u00dfen fr\u00f6hlich erschallen", "tokens": ["Sie", "lie\u00b7\u00dfen", "fr\u00f6h\u00b7lich", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Erquickenden Gesang.", "tokens": ["Er\u00b7qui\u00b7cken\u00b7den", "Ge\u00b7sang", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und w\u00fc\u00dften sie mein Wehe,", "tokens": ["Und", "w\u00fc\u00df\u00b7ten", "sie", "mein", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die goldnen Sternelein,", "tokens": ["Die", "gold\u00b7nen", "Ster\u00b7ne\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie k\u00e4men aus ihrer H\u00f6he,", "tokens": ["Sie", "k\u00e4\u00b7men", "aus", "ih\u00b7rer", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und spr\u00e4chen Trost mir ein.", "tokens": ["Und", "spr\u00e4\u00b7chen", "Trost", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die alle k\u00f6nnen's nicht wissen,", "tokens": ["Die", "al\u00b7le", "k\u00f6n\u00b7nen's", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nur Eine kennt meinen Schmerz;", "tokens": ["Nur", "Ei\u00b7ne", "kennt", "mei\u00b7nen", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie hat ja selbst zerrissen,", "tokens": ["Sie", "hat", "ja", "selbst", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerrissen mir das Herz.", "tokens": ["Zer\u00b7ris\u00b7sen", "mir", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}