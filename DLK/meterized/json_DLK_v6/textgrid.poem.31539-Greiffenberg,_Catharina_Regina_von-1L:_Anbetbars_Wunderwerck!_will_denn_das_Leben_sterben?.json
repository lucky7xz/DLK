{"textgrid.poem.31539": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Anbetbars Wunderwerck! will denn das Leben sterben?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anbetbars Wunderwerck! will denn das Leben sterben?", "tokens": ["An\u00b7bet\u00b7bars", "Wun\u00b7der\u00b7werck", "!", "will", "denn", "das", "Le\u00b7ben", "ster\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "verseucht die Lebensquell? verlischt das Ewig Liecht?", "tokens": ["ver\u00b7seucht", "die", "Le\u00b7bens\u00b7quell", "?", "ver\u00b7lischt", "das", "E\u00b7wig", "Liecht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "hat Saffts und Kraffts Vrsprung / kein Safft und Krafft mehr nicht?", "tokens": ["hat", "Saffts", "und", "Kraffts", "Vr\u00b7sprung", "/", "kein", "Safft", "und", "Krafft", "mehr", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "NN", "$(", "PIAT", "NN", "KON", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "will Ertz-Erhaltungs-St\u00e4rck / selbselbsten hie verderben?", "tokens": ["will", "Ertz\u00b7Er\u00b7hal\u00b7tungs\u00b7St\u00e4rck", "/", "selb\u00b7selbs\u00b7ten", "hie", "ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$(", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "das Ewig Leben wir von Christus Sterben erben.", "tokens": ["das", "E\u00b7wig", "Le\u00b7ben", "wir", "von", "Chris\u00b7tus", "Ster\u00b7ben", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die \u00e4usserst' Eusserung der Gottheits-Krafft geschicht /", "tokens": ["Die", "\u00e4us\u00b7ser\u00b7st'", "Eus\u00b7se\u00b7rung", "der", "Gott\u00b7heits\u00b7Krafft", "ge\u00b7schicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "die jetzt im H\u00f6llen-Reich Zerst\u00f6rungs-Werk verricht.", "tokens": ["die", "jetzt", "im", "H\u00f6l\u00b7len\u00b7Reich", "Zer\u00b7st\u00f6\u00b7rungs\u00b7Werk", "ver\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der unsterbliche kan unsterblichkeit erwerben", "tokens": ["Der", "uns\u00b7terb\u00b7li\u00b7che", "kan", "uns\u00b7terb\u00b7lich\u00b7keit", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VMFIN", "NN", "VVINF"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "im Tod; der hat sich mit dem Leben selbst verschlungen.", "tokens": ["im", "Tod", ";", "der", "hat", "sich", "mit", "dem", "Le\u00b7ben", "selbst", "ver\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ART", "VAFIN", "PRF", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aus Jesu End / erfolgt mein Gl\u00fccks-Vnendlichkeit.", "tokens": ["Aus", "Je\u00b7su", "End", "/", "er\u00b7folgt", "mein", "Gl\u00fccks\u00b7\u00b7V\u00b7nend\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es hat die selbste St\u00e4rck durch Schwachheit \u00fcberrungen", "tokens": ["Es", "hat", "die", "selbs\u00b7te", "St\u00e4rck", "durch", "Schwach\u00b7heit", "\u00fc\u00b7ber\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "die st\u00e4rcksten Menschen-Feind. Der / so des Tods befreyt /", "tokens": ["die", "st\u00e4rcks\u00b7ten", "Men\u00b7schen\u00b7Feind", ".", "Der", "/", "so", "des", "Tods", "be\u00b7freyt", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "$(", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "wolt sterben / da\u00df dadurch das Sterblich' Ewig lebet.", "tokens": ["wolt", "ster\u00b7ben", "/", "da\u00df", "da\u00b7durch", "das", "Sterb\u00b7lich'", "E\u00b7wig", "le\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$(", "KOUS", "PAV", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Vrst\u00e4nd-Geister er der Erd im Grab einwebet.", "tokens": ["Die", "Vr\u00b7st\u00e4n\u00b7dGeis\u00b7ter", "er", "der", "Erd", "im", "Grab", "ein\u00b7we\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}