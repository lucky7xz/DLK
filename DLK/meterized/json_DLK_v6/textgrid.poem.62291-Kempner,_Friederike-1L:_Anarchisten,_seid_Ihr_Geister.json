{"textgrid.poem.62291": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Anarchisten, seid Ihr Geister", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anarchisten, seid Ihr Geister", "tokens": ["An\u00b7ar\u00b7chis\u00b7ten", ",", "seid", "Ihr", "Geis\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der H\u00f6lle tiefsten Gr\u00fcnden?", "tokens": ["Aus", "der", "H\u00f6l\u00b7le", "tiefs\u00b7ten", "Gr\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist der B\u00f6se Euer Meister,", "tokens": ["Ist", "der", "B\u00f6\u00b7se", "Eu\u00b7er", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt die Menschheit Ihr anz\u00fcnden?", "tokens": ["Wollt", "die", "Menschheit", "Ihr", "an\u00b7z\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVIZU", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Bringt Ihr eine Feuerflut?", "tokens": ["Bringt", "Ihr", "ei\u00b7ne", "Feu\u00b7er\u00b7flut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, Ihr wi\u00dft nicht, was Ihr tut!", "tokens": ["Ach", ",", "Ihr", "wi\u00dft", "nicht", ",", "was", "Ihr", "tut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kehret in Euch \u2013 Recht und Ehre", "tokens": ["Keh\u00b7ret", "in", "Euch", "\u2013", "Recht", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind des Weltalls gro\u00dfe Lehre,", "tokens": ["Sind", "des", "Welt\u00b7alls", "gro\u00b7\u00dfe", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie der Wahn Euch auch bet\u00f6re,", "tokens": ["Wie", "der", "Wahn", "Euch", "auch", "be\u00b7t\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehrt zur\u00fcck zu Recht und Ehre!", "tokens": ["Kehrt", "zu\u00b7r\u00fcck", "zu", "Recht", "und", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheu\u00dflich ist der Meuchelmord,", "tokens": ["Scheu\u00df\u00b7lich", "ist", "der", "Meu\u00b7chel\u00b7mord", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Furien weilen an dem Ort!", "tokens": ["Fu\u00b7ri\u00b7en", "wei\u00b7len", "an", "dem", "Ort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Scheu\u00dflicher ist: Feuer z\u00fcnden,", "tokens": ["Scheu\u00df\u00b7li\u00b7cher", "ist", ":", "Feu\u00b7er", "z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die \u00e4rgste aller S\u00fcnden \u2013", "tokens": ["Ist", "die", "\u00e4rgs\u00b7te", "al\u00b7ler", "S\u00fcn\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6llenstrafen zu verk\u00fcnden,", "tokens": ["H\u00f6l\u00b7len\u00b7stra\u00b7fen", "zu", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Konnte man nichts Schlimmres finden!", "tokens": ["Konn\u00b7te", "man", "nichts", "Schlimm\u00b7res", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PIS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gift und Mord und Feuerbrand", "tokens": ["Gift", "und", "Mord", "und", "Feu\u00b7er\u00b7brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind verdammt von Land zu Land!", "tokens": ["Sind", "ver\u00b7dammt", "von", "Land", "zu", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die Leidenschaft auch meinet,", "tokens": ["Was", "die", "Lei\u00b7den\u00b7schaft", "auch", "mei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dem Wahnwitz richtig scheinet.", "tokens": ["Was", "dem", "Wahn\u00b7witz", "rich\u00b7tig", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kehrt zur\u00fcck zu Recht und Ehre,", "tokens": ["Kehrt", "zu\u00b7r\u00fcck", "zu", "Recht", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Merkt Euch der Geschichte Lehre:", "tokens": ["Merkt", "Euch", "der", "Ge\u00b7schich\u00b7te", "Leh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Niemals n\u00fctzlich war der Mord;", "tokens": ["Nie\u00b7mals", "n\u00fctz\u00b7lich", "war", "der", "Mord", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es gibt ein ew'ges Dort!", "tokens": ["Und", "es", "gibt", "ein", "ew'\u00b7ges", "Dort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}