{"dta.poem.10824": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das Geld ein allgemeiner Ab-  \n gott der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die V\u00f6lker die der Wahn ber\u00fckt, des\nJrthums Nebelrauch verh\u00fcllt,", "tokens": ["Die", "V\u00f6l\u00b7ker", "die", "der", "Wahn", "be\u00b7r\u00fckt", ",", "des", "Jr\u00b7thums", "Ne\u00b7bel\u00b7rauch", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und die der arge Seelenfeind, mit Aber-", "tokens": ["Und", "die", "der", "ar\u00b7ge", "See\u00b7len\u00b7feind", ",", "mit", "A\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verliessen ihren wahren ", "tokens": ["Ver\u00b7lies\u00b7sen", "ih\u00b7ren", "wah\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00e4hlten sich an dessen Stat, aus Wahnwiz", "tokens": ["Und", "w\u00e4hl\u00b7ten", "sich", "an", "des\u00b7sen", "Stat", ",", "aus", "Wahn\u00b7wiz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PRELAT", "NN", "$,", "APPR", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Zuerst ist woll des Himmels Licht, das grosse Au-", "tokens": ["Zu\u00b7erst", "ist", "woll", "des", "Him\u00b7mels", "Licht", ",", "das", "gros\u00b7se", "Au"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Sonne die so herrlich strahlt, die durch die", "tokens": ["Die", "Son\u00b7ne", "die", "so", "herr\u00b7lich", "strahlt", ",", "die", "durch", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADV", "ADJD", "VVFIN", "$,", "PRELS", "APPR", "ART"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Was in Naturreich w\u00e4chst und bl\u00fcht, von denen un-", "tokens": ["Was", "in", "Na\u00b7tur\u00b7reich", "w\u00e4chst", "und", "bl\u00fcht", ",", "von", "de\u00b7nen", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPR", "NE", "VVFIN", "KON", "VVFIN", "$,", "APPR", "PRELS", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vor einen ", "tokens": ["Vor", "ei\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Allein nachdem hat jedes Land sich einen Abgott aus-", "tokens": ["Al\u00b7lein", "nach\u00b7dem", "hat", "je\u00b7des", "Land", "sich", "ei\u00b7nen", "Ab\u00b7gott", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "VAFIN", "PIAT", "NN", "PRF", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Der als ein tapfrer Held dem Volk etwan zu Vor-", "tokens": ["Der", "als", "ein", "tapf\u00b7rer", "Held", "dem", "Volk", "et\u00b7wan", "zu", "Vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sieht man in jene tr\u00fcbe Zeit, da Finsternis die Erd", "tokens": ["Sieht", "man", "in", "je\u00b7ne", "tr\u00fc\u00b7be", "Zeit", ",", "da", "Fins\u00b7ter\u00b7nis", "die", "Erd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "PDAT", "ADJA", "NN", "$,", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Der Aberglaube gr\u00f6stentheils sich \u00fcber alle Welt er-", "tokens": ["Der", "A\u00b7berg\u00b7lau\u00b7be", "gr\u00f6s\u00b7ten\u00b7theils", "sich", "\u00fc\u00b7ber", "al\u00b7le", "Welt", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PRF", "APPR", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Da hatt ein jeder seinen ", "tokens": ["Da", "hatt", "ein", "je\u00b7der", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "PIAT", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Wenn man in andre Grenzen kam, da ward ein", "tokens": ["Wenn", "man", "in", "and\u00b7re", "Gren\u00b7zen", "kam", ",", "da", "ward", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.15": {"text": "Der dieses Landes G\u00f6zze war. Und so fand man", "tokens": ["Der", "die\u00b7ses", "Lan\u00b7des", "G\u00f6z\u00b7ze", "war", ".", "Und", "so", "fand", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PDAT", "NN", "NN", "VAFIN", "$.", "KON", "ADV", "VVFIN", "PIS"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Stets andre G\u00f6tter, welchen man pflegt s\u00fcssen Wei-", "tokens": ["Stets", "and\u00b7re", "G\u00f6t\u00b7ter", ",", "wel\u00b7chen", "man", "pflegt", "s\u00fcs\u00b7sen", "Wei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PWAT", "PIS", "VVFIN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Allein jezt da der G\u00f6zzendienst, in vielen L\u00e4ndern", "tokens": ["Al\u00b7lein", "jezt", "da", "der", "G\u00f6z\u00b7zen\u00b7dienst", ",", "in", "vie\u00b7len", "L\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bleibt noch ein b\u00f6ser Abgott da, den ehret noch die", "tokens": ["Bleibt", "noch", "ein", "b\u00f6\u00b7ser", "Ab\u00b7gott", "da", ",", "den", "eh\u00b7ret", "noch", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,", "ART", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.19": {"text": "Die Christen die im klaren Licht der Offenbahrung", "tokens": ["Die", "Chris\u00b7ten", "die", "im", "kla\u00b7ren", "Licht", "der", "Of\u00b7fen\u00b7bah\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es sey ein einzig wahrer ", "tokens": ["Es", "sey", "ein", "ein\u00b7zig", "wah\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Die lieben diesen \u00f6fters mehr, als den, der alles hat", "tokens": ["Die", "lie\u00b7ben", "die\u00b7sen", "\u00f6f\u00b7ters", "mehr", ",", "als", "den", ",", "der", "al\u00b7les", "hat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "PDAT", "ADV", "ADV", "$,", "KOUS", "ART", "$,", "PRELS", "PIS", "VAFIN"], "meter": "-+-+-+-++--+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "Und Himmel, Erde, Luft und Meer aus einem Nichts", "tokens": ["Und", "Him\u00b7mel", ",", "Er\u00b7de", ",", "Luft", "und", "Meer", "aus", "ei\u00b7nem", "Nichts"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein jeder wird begierig seyn den Abgott aller Welt", "tokens": ["Ein", "je\u00b7der", "wird", "be\u00b7gie\u00b7rig", "seyn", "den", "Ab\u00b7gott", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "ADJD", "VAINF", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "Jedoch er ist genug bekandt, man pfleget ihn das", "tokens": ["Je\u00b7doch", "er", "ist", "ge\u00b7nug", "be\u00b7kandt", ",", "man", "pfle\u00b7get", "ihn", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PIS", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}