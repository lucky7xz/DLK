{"textgrid.poem.57571": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Juni", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein schlechter Fr\u00fchling \u2013 alle B\u00e4ume", "tokens": ["Ein", "schlech\u00b7ter", "Fr\u00fch\u00b7ling", "\u2013", "al\u00b7le", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Raupen und Gew\u00fcrm zerpfl\u00fcckt \u2013", "tokens": ["Von", "Rau\u00b7pen", "und", "Ge\u00b7w\u00fcrm", "zer\u00b7pfl\u00fcckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die besten meiner Lebenstr\u00e4ume", "tokens": ["Die", "bes\u00b7ten", "mei\u00b7ner", "Le\u00b7ben\u00b7str\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind auch in ihrem Mai geknickt.", "tokens": ["Sind", "auch", "in", "ih\u00b7rem", "Mai", "ge\u00b7knickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Frost verdarb die sch\u00f6nsten Bl\u00fcten,", "tokens": ["Der", "Frost", "ver\u00b7darb", "die", "sch\u00f6ns\u00b7ten", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Made fra\u00df sich bis ins Mark \u2013", "tokens": ["Die", "Ma\u00b7de", "fra\u00df", "sich", "bis", "ins", "Mark", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel gute Vors\u00e4tze vergl\u00fchten,...", "tokens": ["Viel", "gu\u00b7te", "Vor\u00b7s\u00e4t\u00b7ze", "ver\u00b7gl\u00fch\u00b7ten", ",", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fcr Tod und Gift ist nichts zu stark.", "tokens": ["F\u00fcr", "Tod", "und", "Gift", "ist", "nichts", "zu", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PIS", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und doch \u2013 es schie\u00dfen frische Triebe,", "tokens": ["Und", "doch", "\u2013", "es", "schie\u00b7\u00dfen", "fri\u00b7sche", "Trie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gr\u00fcngoldnes Laub schm\u00fcckt jeden Zweig \u2013", "tokens": ["Gr\u00fcn\u00b7gold\u00b7nes", "Laub", "schm\u00fcckt", "je\u00b7den", "Zweig", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Un\u00fcberwindbar ist die Liebe", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7bar", "ist", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und abzuwehren jeder Streich.", "tokens": ["Und", "ab\u00b7zu\u00b7weh\u00b7ren", "je\u00b7der", "Streich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIZU", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Kein Baum f\u00e4llt bei dem ersten Streiche,", "tokens": ["Kein", "Baum", "f\u00e4llt", "bei", "dem", "ers\u00b7ten", "Strei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Herz bricht von dem tiefsten Hieb \u2013", "tokens": ["Kein", "Herz", "bricht", "von", "dem", "tiefs\u00b7ten", "Hieb", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es bleibt dem Herzen wie der Eiche", "tokens": ["Es", "bleibt", "dem", "Her\u00b7zen", "wie", "der", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Rettung der Johannistrieb.", "tokens": ["Als", "Ret\u00b7tung", "der", "Jo\u00b7han\u00b7nis\u00b7trieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum still, mein Herz, und la\u00df das Wimmern", "tokens": ["Drum", "still", ",", "mein", "Herz", ",", "und", "la\u00df", "das", "Wim\u00b7mern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schrei \u00bbProst Rest\u00ab des Wehgest\u00f6hns!", "tokens": ["Und", "schrei", "\u00bb", "Prost", "Rest", "\u00ab", "des", "Weh\u00b7ge\u00b7st\u00f6hns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "NN", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "St\u00fcrzt auch die ganze Welt in Tr\u00fcmmern,", "tokens": ["St\u00fcrzt", "auch", "die", "gan\u00b7ze", "Welt", "in", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bleib' ja doch der Hermann L\u00f6ns.", "tokens": ["Ich", "bleib'", "ja", "doch", "der", "Her\u00b7mann", "L\u00f6ns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}