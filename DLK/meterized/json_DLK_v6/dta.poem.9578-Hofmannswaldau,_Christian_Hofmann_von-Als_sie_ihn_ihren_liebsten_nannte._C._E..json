{"dta.poem.9578": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihn ihren liebsten nannte.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ljbore/ deine gunst ist fast zu hoch gestiegen:", "tokens": ["Lj\u00b7bo\u00b7re", "/", "dei\u00b7ne", "gunst", "ist", "fast", "zu", "hoch", "ge\u00b7stie\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du weist ja/ wer ich bin/ und soll dein liebster seyn?", "tokens": ["Du", "weist", "ja", "/", "wer", "ich", "bin", "/", "und", "soll", "dein", "liebs\u00b7ter", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWS", "PPER", "VAFIN", "$(", "KON", "VMFIN", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Solch ungemeines gl\u00fcck will mir durchaus nicht ein/", "tokens": ["Solch", "un\u00b7ge\u00b7mei\u00b7nes", "gl\u00fcck", "will", "mir", "durc\u00b7haus", "nicht", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und heist vernunfft und witz bey mir zu boden liegen.", "tokens": ["Und", "heist", "ver\u00b7nunfft", "und", "witz", "bey", "mir", "zu", "bo\u00b7den", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "KON", "NN", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach! deine h\u00f6fftigkeit will mich zu starck besiegen!", "tokens": ["Ach", "!", "dei\u00b7ne", "h\u00f6ff\u00b7tig\u00b7keit", "will", "mich", "zu", "starck", "be\u00b7sie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VMFIN", "PRF", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du gehst verschwendrisch um mit deinem gnaden-schein.", "tokens": ["Du", "gehst", "ver\u00b7schwend\u00b7risch", "um", "mit", "dei\u00b7nem", "gna\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn dencke/ was du thust: Ein schlechter kiesel-stein", "tokens": ["Denn", "den\u00b7cke", "/", "was", "du", "thust", ":", "Ein", "schlech\u00b7ter", "kie\u00b7sel\u00b7stein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "PWS", "PPER", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Soll sich auff dein gehei\u00df zum diamant verf\u00fcgen!", "tokens": ["Soll", "sich", "auff", "dein", "ge\u00b7hei\u00df", "zum", "di\u00b7a\u00b7mant", "ver\u00b7f\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu perlen legt man sonst nicht schwartzen ziegel-grau\u00df/", "tokens": ["Zu", "per\u00b7len", "legt", "man", "sonst", "nicht", "schwart\u00b7zen", "zie\u00b7gel\u00b7grau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PIS", "ADV", "PTKNEG", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und ne\u00dfeln bindet man in keinen blumen-strau\u00df;", "tokens": ["Und", "ne\u00b7\u00df\u00b7eln", "bin\u00b7det", "man", "in", "kei\u00b7nen", "blu\u00b7men\u00b7strau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PIS", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Wie schickt sich dann dein glantz zu meinem duncklen wesen!", "tokens": ["Wie", "schickt", "sich", "dann", "dein", "glantz", "zu", "mei\u00b7nem", "dunck\u00b7len", "we\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Libore/ glaube mir/ du thust mir wohl und recht/", "tokens": ["Li\u00b7bo\u00b7re", "/", "glau\u00b7be", "mir", "/", "du", "thust", "mir", "wohl", "und", "recht", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "ADV", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn du ins k\u00fcnfft\u2019ge mich nur nennest deinen knecht/", "tokens": ["Wenn", "du", "ins", "k\u00fcnf\u00b7ft'\u00b7ge", "mich", "nur", "nen\u00b7nest", "dei\u00b7nen", "knecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Zu deinem liebsten hat das gl\u00fcck mich nicht erlesen.", "tokens": ["Zu", "dei\u00b7nem", "liebs\u00b7ten", "hat", "das", "gl\u00fcck", "mich", "nicht", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}