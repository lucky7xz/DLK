{"textgrid.poem.52232": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Wallenstein", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei, wie man doch in unsern Tagen", "tokens": ["Ei", ",", "wie", "man", "doch", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PIS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nachahmt den Wallenstein!", "tokens": ["Nac\u00b7hahmt", "den", "Wal\u00b7len\u00b7stein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der konnte, sagt man, nicht vertragen", "tokens": ["Der", "konn\u00b7te", ",", "sagt", "man", ",", "nicht", "ver\u00b7tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VMFIN", "$,", "VVFIN", "PIS", "$,", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Hahnes mutig Schrein!", "tokens": ["Des", "Hah\u00b7nes", "mu\u00b7tig", "Schrein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Sterne grollend Strahlenwerfen", "tokens": ["Der", "Ster\u00b7ne", "grol\u00b7lend", "Strah\u00b7len\u00b7wer\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kaltbl\u00fctig mocht' er schaun;", "tokens": ["Kalt\u00b7bl\u00fc\u00b7tig", "mocht'", "er", "schaun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Allein \u2013 es kam wohl von den Nerven! \u2013", "tokens": ["Al\u00b7lein", "\u2013", "es", "kam", "wohl", "von", "den", "Ner\u00b7ven", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$(", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Kr\u00e4hen macht' ihm Graun!", "tokens": ["Ein", "Kr\u00e4\u00b7hen", "macht'", "ihm", "Graun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Furcht des Hahnen, wie wir sehen,", "tokens": ["Die", "Furcht", "des", "Hah\u00b7nen", ",", "wie", "wir", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward heuer allgemein:", "tokens": ["Ward", "heu\u00b7er", "all\u00b7ge\u00b7mein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man bebt vor einem dreisten Kr\u00e4hen,", "tokens": ["Man", "bebt", "vor", "ei\u00b7nem", "dreis\u00b7ten", "Kr\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz wie der Wallenstein!", "tokens": ["Ganz", "wie", "der", "Wal\u00b7len\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich meine nicht den roten Hahnen,", "tokens": ["Ich", "mei\u00b7ne", "nicht", "den", "ro\u00b7ten", "Hah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch den von Frankreich nicht \u2013", "tokens": ["Auch", "den", "von", "Fran\u00b7kreich", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "NE", "PTKNEG", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ich meine den nur, dessen Mahnen", "tokens": ["Ich", "mei\u00b7ne", "den", "nur", ",", "des\u00b7sen", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADV", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sagt, da\u00df der Tag anbricht!", "tokens": ["Sagt", ",", "da\u00df", "der", "Tag", "an\u00b7bricht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}