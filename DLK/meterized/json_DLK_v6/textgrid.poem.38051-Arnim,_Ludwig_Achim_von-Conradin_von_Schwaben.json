{"textgrid.poem.38051": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Conradin von Schwaben", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Conradin zu Jahren kam,", "tokens": ["Als", "Con\u00b7ra\u00b7din", "zu", "Jah\u00b7ren", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schnelle Sach sich bald vernahm,", "tokens": ["Ein", "schnel\u00b7le", "Sach", "sich", "bald", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wollt sich m\u00e4nnlich halten,", "tokens": ["Er", "wollt", "sich", "m\u00e4nn\u00b7lich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Alle Erbl\u00e4nder nehmen ein,", "tokens": ["Al\u00b7le", "Er\u00b7bl\u00e4n\u00b7der", "neh\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Die von den Aeltern eigen sein,", "tokens": ["Die", "von", "den", "A\u00b7el\u00b7tern", "ei\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die wollt er frey verwalten.", "tokens": ["Die", "wollt", "er", "frey", "ver\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df er sie frey und eigen h\u00e4tt", "tokens": ["Da\u00df", "er", "sie", "frey", "und", "ei\u00b7gen", "h\u00e4tt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "KON", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um Kriegsvolk th\u00e4t er schreiben", "tokens": ["Um", "Kriegs\u00b7volk", "th\u00e4t", "er", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Im K\u00f6nigreich, F\u00fcrstenth\u00fcm und St\u00e4dt,", "tokens": ["Im", "K\u00f6\u00b7nig\u00b7reich", ",", "F\u00fcrs\u00b7tent\u00b7h\u00fcm", "und", "St\u00e4dt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Da sollt niemand ausbleiben,", "tokens": ["Da", "sollt", "nie\u00b7mand", "aus\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Sondern ihm treuen Beystand thun,", "tokens": ["Son\u00b7dern", "ihm", "treu\u00b7en", "Beys\u00b7tand", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Bis er ein Heer zusammenbracht,", "tokens": ["Bis", "er", "ein", "Heer", "zu\u00b7sam\u00b7men\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hat er kein Rast und konnt nicht ruhn.", "tokens": ["Hat", "er", "kein", "Rast", "und", "konnt", "nicht", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "KON", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als nun Papst Clemens solches vernahm,", "tokens": ["Als", "nun", "Papst", "Cle\u00b7mens", "sol\u00b7ches", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "NE", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Sache bald zuvor auch kam,", "tokens": ["Der", "Sa\u00b7che", "bald", "zu\u00b7vor", "auch", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Th\u00e4t auch ein Kriegsherr verschreiben.", "tokens": ["Th\u00e4t", "auch", "ein", "Kriegs\u00b7herr", "ver\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und schrieb dem Grafen Karl gleich,", "tokens": ["Und", "schrieb", "dem", "Gra\u00b7fen", "Karl", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Bruder des K\u00f6nigs in Frankenreich,", "tokens": ["Dem", "Bru\u00b7der", "des", "K\u00f6\u00b7nigs", "in", "Fran\u00b7ken\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Er sollte nicht ausbleiben,", "tokens": ["Er", "soll\u00b7te", "nicht", "aus\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sondern Konrad wehren thun,", "tokens": ["Son\u00b7dern", "Kon\u00b7rad", "weh\u00b7ren", "thun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und alle P\u00e4\u00df verlegen.", "tokens": ["Und", "al\u00b7le", "P\u00e4\u00df", "ver\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Graf Karl th\u00e4ts alsbald nun,", "tokens": ["Graf", "Karl", "th\u00e4ts", "als\u00b7bald", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Er zog ihm straks entgegen,", "tokens": ["Er", "zog", "ihm", "straks", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und machte durch Verr\u00e4therey,", "tokens": ["Und", "mach\u00b7te", "durch", "Ver\u00b7r\u00e4\u00b7the\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df er Neapel genommen ein,", "tokens": ["Da\u00df", "er", "Nea\u00b7pel", "ge\u00b7nom\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVPP", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Eh Conradin noch kam herbey.", "tokens": ["Eh", "Con\u00b7ra\u00b7din", "noch", "kam", "her\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Karl der schicket aus gar viel", "tokens": ["Karl", "der", "schi\u00b7cket", "aus", "gar", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "VVFIN", "APPR", "ADV", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Verr\u00e4ther in geheimer Still,", "tokens": ["Ver\u00b7r\u00e4\u00b7ther", "in", "ge\u00b7hei\u00b7mer", "Still", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sollten Sperl einnehmen,", "tokens": ["Sie", "soll\u00b7ten", "Sperl", "ein\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn Karl lie\u00df gar viel darauf gehn,", "tokens": ["Denn", "Karl", "lie\u00df", "gar", "viel", "da\u00b7rauf", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADV", "ADV", "PAV", "VVINF", "$,"], "meter": "-+--+-++", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "In Papstes Namen ists geschehn,", "tokens": ["In", "Paps\u00b7tes", "Na\u00b7men", "ists", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Conradin zu d\u00e4mmen,", "tokens": ["Den", "Con\u00b7ra\u00b7din", "zu", "d\u00e4m\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Papst verhie\u00df ihm grosses Gut,", "tokens": ["Der", "Papst", "ver\u00b7hie\u00df", "ihm", "gros\u00b7ses", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn er ihn m\u00f6cht bestreiten,", "tokens": ["Wenn", "er", "ihn", "m\u00f6cht", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Derhalben hielt er gute Hut,", "tokens": ["Der\u00b7hal\u00b7ben", "hielt", "er", "gu\u00b7te", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er lie\u00df gro\u00df Gut erbiethen,", "tokens": ["Er", "lie\u00df", "gro\u00df", "Gut", "er\u00b7bie\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So die Verr\u00e4therey gemacht,", "tokens": ["So", "die", "Ver\u00b7r\u00e4\u00b7the\u00b7rey", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Steg und Weg daselbst er wu\u00dft,", "tokens": ["Die", "Steg", "und", "Weg", "da\u00b7selbst", "er", "wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da r\u00fcckt er bey in tiefer Nacht.", "tokens": ["Da", "r\u00fcckt", "er", "bey", "in", "tie\u00b7fer", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Conradin mit seinem Heer", "tokens": ["Con\u00b7ra\u00b7din", "mit", "sei\u00b7nem", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die Nacht da einkehrt.", "tokens": ["Auf", "die", "Nacht", "da", "ein\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zu Morgens wollt er r\u00fccken", "tokens": ["Zu", "Mor\u00b7gens", "wollt", "er", "r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja ins K\u00f6nigreich Neapel ein!", "tokens": ["Ja", "ins", "K\u00f6\u00b7nig\u00b7reich", "Nea\u00b7pel", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPRART", "NN", "NE", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Lie\u00df ausrufen mit heller Stimm,", "tokens": ["Lie\u00df", "aus\u00b7ru\u00b7fen", "mit", "hel\u00b7ler", "Stimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Sein Red wollt er nicht zucken,", "tokens": ["Sein", "Red", "wollt", "er", "nicht", "zu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Eh m\u00fc\u00df ihm drauf gehn Leib und Gut,", "tokens": ["Eh", "m\u00fc\u00df", "ihm", "drauf", "gehn", "Leib", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PAV", "VVFIN", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er wolle es drauf setzen! \u2013", "tokens": ["Er", "wol\u00b7le", "es", "drauf", "set\u00b7zen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PAV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Landsknecht sind nun wohlgemuth:", "tokens": ["Die", "Lands\u00b7knecht", "sind", "nun", "wohl\u00b7ge\u00b7muth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Reis' soll uns erg\u00f6tzen!", "tokens": ["Die", "Reis'", "soll", "uns", "er\u00b7g\u00f6t\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Sie konnten sich nicht r\u00fcsten mehr,", "tokens": ["Sie", "konn\u00b7ten", "sich", "nicht", "r\u00fcs\u00b7ten", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hineinzur\u00fccken in das Land,", "tokens": ["Hin\u00b7ein\u00b7zu\u00b7r\u00fc\u00b7cken", "in", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als schon der Feind vorhanden w\u00e4r.", "tokens": ["Als", "schon", "der", "Feind", "vor\u00b7han\u00b7den", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun h\u00f6ret zu, wie es ergieng,", "tokens": ["Nun", "h\u00f6\u00b7ret", "zu", ",", "wie", "es", "er\u00b7gieng", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Als sich der Schimpf mit Ernst anfieng,", "tokens": ["Als", "sich", "der", "Schimpf", "mit", "Ernst", "an\u00b7fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Schanz ward hastlich \u00fcbersehen.", "tokens": ["Die", "Schanz", "ward", "hast\u00b7lich", "\u00fc\u00b7ber\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Conradin hat gesiegt im Anfang,", "tokens": ["Con\u00b7ra\u00b7din", "hat", "ge\u00b7siegt", "im", "An\u00b7fang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da \u00fcber die Beut die Ordnung sank,", "tokens": ["Da", "\u00fc\u00b7ber", "die", "Beut", "die", "Ord\u00b7nung", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"