{"textgrid.poem.28226": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es m\u00fchet sich der Mensch, auff da\u00df er was erwerbe,", "tokens": ["Es", "m\u00fc\u00b7het", "sich", "der", "Mensch", ",", "auff", "da\u00df", "er", "was", "er\u00b7wer\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "APPR", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was er dann erwirbt, soll, da\u00df er wo nicht sterbe,", "tokens": ["Und", "was", "er", "dann", "er\u00b7wirbt", ",", "soll", ",", "da\u00df", "er", "wo", "nicht", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "VMFIN", "$,", "KOUS", "PPER", "PWAV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wann er nun nicht stirbt, so sol er drum nur leben,", "tokens": ["Und", "wann", "er", "nun", "nicht", "stirbt", ",", "so", "sol", "er", "drum", "nur", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "PAV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auff da\u00df er, was er wirbt, zur Steuer m\u00fcsse geben.", "tokens": ["Auff", "da\u00df", "er", ",", "was", "er", "wirbt", ",", "zur", "Steu\u00b7er", "m\u00fcs\u00b7se", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann bringt ihm weiter nichts das m\u00fchen und erwerben", "tokens": ["Dann", "bringt", "ihm", "wei\u00b7ter", "nichts", "das", "m\u00fc\u00b7hen", "und", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "ART", "ADJA", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und alles, was er gibt, als so nur eher sterben.", "tokens": ["Und", "al\u00b7les", ",", "was", "er", "gibt", ",", "als", "so", "nur", "e\u00b7her", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}