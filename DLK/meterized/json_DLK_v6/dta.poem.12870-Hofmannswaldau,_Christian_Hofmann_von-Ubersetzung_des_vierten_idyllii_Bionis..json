{"dta.poem.12870": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersetzung des vierten   idyllii  \n Bionis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Musen f\u00fcrchten sich nicht vor der holden Liebe,", "tokens": ["Die", "Mu\u00b7sen", "f\u00fcrch\u00b7ten", "sich", "nicht", "vor", "der", "hol\u00b7den", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ehren sie vielmehr, und folgen ihrem triebe", "tokens": ["Sie", "eh\u00b7ren", "sie", "viel\u00b7mehr", ",", "und", "fol\u00b7gen", "ih\u00b7rem", "trie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "PPOSAT", "VVFIN"], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Mit h\u00f6chster wohllust nach. Wer ihre gunst begehrt,", "tokens": ["Mit", "h\u00f6chs\u00b7ter", "wohl\u00b7lust", "nach", ".", "Wer", "ih\u00b7re", "gunst", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "PWS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und unempfindlich ist, den achten sie kaum werth", "tokens": ["Und", "un\u00b7emp\u00b7find\u00b7lich", "ist", ",", "den", "ach\u00b7ten", "sie", "kaum", "werth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "ART", "ADJA", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur einmal anzusehn. Doch wer von liebe singen", "tokens": ["Nur", "ein\u00b7mal", "an\u00b7zu\u00b7sehn", ".", "Doch", "wer", "von", "lie\u00b7be", "sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVIZU", "$.", "KON", "PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und seuffzer dichten will, dem wird es bald gelingen.", "tokens": ["Und", "seuff\u00b7zer", "dich\u00b7ten", "will", ",", "dem", "wird", "es", "bald", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "VMFIN", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die verse stellen sich fast ohne dencken ein,", "tokens": ["Die", "ver\u00b7se", "stel\u00b7len", "sich", "fast", "oh\u00b7ne", "den\u00b7cken", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und was sein kopff beginnt, mu\u00df sch\u00f6n und sinnreich seyn.", "tokens": ["Und", "was", "sein", "kopff", "be\u00b7ginnt", ",", "mu\u00df", "sch\u00f6n", "und", "sinn\u00b7reich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVFIN", "$,", "VMFIN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dis ist kein falscher satz, ich kan es selbst bezeugen,", "tokens": ["Dis", "ist", "kein", "fal\u00b7scher", "satz", ",", "ich", "kan", "es", "selbst", "be\u00b7zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn wenn ich mich bem\u00fch den himmel zu ersteigen,", "tokens": ["Denn", "wenn", "ich", "mich", "be\u00b7m\u00fch", "den", "him\u00b7mel", "zu", "er\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und einen hohen GOtt daselbst besingen will,", "tokens": ["Und", "ei\u00b7nen", "ho\u00b7hen", "Gott", "da\u00b7selbst", "be\u00b7sin\u00b7gen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PAV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So starrt die bl\u00f6de zung\u2019 und zittert als ein kiel,", "tokens": ["So", "starrt", "die", "bl\u00f6\u00b7de", "zung'", "und", "zit\u00b7tert", "als", "ein", "kiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn ihn der wind ber\u00fchrt. Doch wenn ich dich ergreiffe,", "tokens": ["Wenn", "ihn", "der", "wind", "be\u00b7r\u00fchrt", ".", "Doch", "wenn", "ich", "dich", "er\u00b7greif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$.", "KON", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mein steter zeit-vertreib, mein lust-spiel, meine pfeiffe!", "tokens": ["Mein", "ste\u00b7ter", "zeit\u00b7ver\u00b7treib", ",", "mein", "lust\u00b7spiel", ",", "mei\u00b7ne", "pfeif\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und Daphnen, oder ja der Liebe s\u00fc\u00dfe macht,", "tokens": ["Und", "Daph\u00b7nen", ",", "o\u00b7der", "ja", "der", "Lie\u00b7be", "s\u00fc\u00b7\u00dfe", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "ADV", "ART", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie ich vorhin gethan, zu r\u00fchmen bin bedacht,", "tokens": ["Wie", "ich", "vor\u00b7hin", "ge\u00b7than", ",", "zu", "r\u00fch\u00b7men", "bin", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,", "PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So giebt ein stiller trieb mir alle kr\u00e4ffte wieder,", "tokens": ["So", "giebt", "ein", "stil\u00b7ler", "trieb", "mir", "al\u00b7le", "kr\u00e4ff\u00b7te", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "VVFIN", "PPER", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und meine fl\u00f6te spielt die angenehmsten lieder.", "tokens": ["Und", "mei\u00b7ne", "fl\u00f6\u00b7te", "spielt", "die", "an\u00b7ge\u00b7nehms\u00b7ten", "lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}