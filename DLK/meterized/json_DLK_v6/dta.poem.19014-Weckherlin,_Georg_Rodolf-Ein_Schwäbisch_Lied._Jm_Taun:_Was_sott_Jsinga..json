{"dta.poem.19014": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ein Schw\u00e4bisch Lied.  \n Jm Taun: Was sott Jsinga.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.71", "sw:0.28"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Yer hipscha Meatza luogat zuo", "tokens": ["Yer", "hip\u00b7scha", "Meat\u00b7za", "luo\u00b7gat", "zuo"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mar wettat gaun mit Vy a kuo", "tokens": ["Mar", "wet\u00b7tat", "gaun", "mit", "Vy", "a", "kuo"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "(mit gaunst vnd guotan ayhro)", "tokens": ["(", "mit", "gaunst", "vnd", "guo\u00b7tan", "ay\u00b7hro", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "KON", "XY", "XY", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mar dumlat as reacht/", "tokens": ["Mar", "dum\u00b7lat", "as", "reacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "As depfere kneacht/", "tokens": ["As", "dep\u00b7fe\u00b7re", "kneacht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mit diena graussa hayra jo hayro.", "tokens": ["Mit", "die\u00b7na", "graus\u00b7sa", "hay\u00b7ra", "jo", "hay\u00b7ro", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wem mara wenga bsoffa send/", "tokens": ["Wem", "ma\u00b7ra", "wen\u00b7ga", "bsof\u00b7fa", "send", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vnd das mar aunsarn Spilman hend/", "tokens": ["Vnd", "das", "mar", "aun\u00b7sarn", "Spil\u00b7man", "hend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "NE", "NN", "VVPP", "$("], "meter": "--+-+++-", "measure": "anapaest.init"}, "line.3": {"text": "Dear a\u00df kan zdanza pfeiffo:", "tokens": ["De\u00b7ar", "a\u00df", "kan", "zdan\u00b7za", "pfeif\u00b7fo", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sem mar schaun fraw/", "tokens": ["So", "sem", "mar", "schaun", "fraw", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "FM", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd kennat gaun aw", "tokens": ["Vnd", "ken\u00b7nat", "gaun", "aw"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Dmedla depfar angreiffa jo greiffo.", "tokens": ["Dmed\u00b7la", "de\u00b7pfar", "an\u00b7greif\u00b7fa", "jo", "greif\u00b7fo", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was derfs vil weasas do/ botz bleach/", "tokens": ["Was", "derfs", "vil", "we\u00b7a\u00b7sas", "do", "/", "botz", "bleach", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "NN", "VVFIN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Lend a\u00df sien wear verduot duy zeach/", "tokens": ["Lend", "a\u00df", "sien", "wear", "ver\u00b7duot", "duy", "zeach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lend a\u00df gaund\u2019stanga leyho:", "tokens": ["Lend", "a\u00df", "gaun\u00b7d'\u00b7stan\u00b7ga", "ley\u00b7ho", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "So muo\u00df a\u00df gaun sain", "tokens": ["So", "muo\u00df", "a\u00df", "gaun", "sain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "FM.fr", "FM.fr", "FM.fr", "FM.fr"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Vm mar st\u00f6ckats nain", "tokens": ["Vm", "mar", "st\u00f6c\u00b7ka\u00b7ts", "nain"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Sotts schaun dia gsella kheya jo kheyo.", "tokens": ["Sotts", "schaun", "dia", "gsel\u00b7la", "khey\u00b7a", "jo", "khey\u00b7o", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.it", "FM.it", "FM.it", "FM.it", "FM.it", "FM.it", "FM.it", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nan so gend a\u00df an healla blick", "tokens": ["Nan", "so", "gend", "a\u00df", "an", "he\u00b7al\u00b7la", "blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVPP", "VVFIN", "APPR", "NE", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Den Vyar gsicht ischt lauttar glick/", "tokens": ["Den", "Vy\u00b7ar", "gsicht", "ischt", "laut\u00b7tar", "glick", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd macht a\u00df dstang erschittlo:", "tokens": ["Vnd", "macht", "a\u00df", "ds\u00b7tang", "er\u00b7schitt\u00b7lo", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Vyr hor des ischt aw", "tokens": ["Vyr", "hor", "des", "ischt", "aw"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "As glitzig a\u00df schtraw/", "tokens": ["As", "glit\u00b7zig", "a\u00df", "scht\u00b7raw", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie raun send uyra ditla jo ditlo.", "tokens": ["Wie", "raun", "send", "u\u00b7yra", "dit\u00b7la", "jo", "dit\u00b7lo", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "FM.it", "FM.it", "FM.it", "FM.it", "FM.it", "FM.it", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vnd vyra goscha send so rawt/", "tokens": ["Vnd", "vy\u00b7ra", "go\u00b7scha", "send", "so", "rawt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "ADV", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Aw s\u00fcssar wedar k\u00e4\u00df at brawt", "tokens": ["Aw", "s\u00fcs\u00b7sar", "we\u00b7dar", "k\u00e4\u00df", "at", "brawt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wen ar dz\u00e4n froindly bl\u00f6ckot:", "tokens": ["Wen", "ar", "dz\u00e4n", "froind\u00b7ly", "bl\u00f6\u00b7ckot", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ar frayata gaun", "tokens": ["Ar", "fray\u00b7a\u00b7ta", "gaun"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "A\u00df wem mar dschtang schaun", "tokens": ["A\u00df", "wem", "mar", "dschtang", "schaun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Hettat mitta nein gscht\u00f6ckat/ jo gscht\u00f6ckot.", "tokens": ["Het\u00b7tat", "mit\u00b7ta", "nein", "gscht\u00f6c\u00b7kat", "/", "jo", "gscht\u00f6\u00b7ckot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}