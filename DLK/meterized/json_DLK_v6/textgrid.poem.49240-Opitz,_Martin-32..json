{"textgrid.poem.49240": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist mein Lieb gestalt, so ist ihr Angesicht,", "tokens": ["So", "ist", "mein", "Lieb", "ge\u00b7stalt", ",", "so", "ist", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Hals, ihr rother Mund und ihrer Augen Licht;", "tokens": ["Ihr", "Hals", ",", "ihr", "ro\u00b7ther", "Mund", "und", "ih\u00b7rer", "Au\u00b7gen", "Licht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "K\u00f6nt' jetzt der Maler auch entwerfen ihre Sinnen,", "tokens": ["K\u00f6nt'", "jetzt", "der", "Ma\u00b7ler", "auch", "ent\u00b7wer\u00b7fen", "ih\u00b7re", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nichts sch\u00f6ners w\u00fcrde man auf Erden finden k\u00f6nnen.", "tokens": ["Nichts", "sch\u00f6\u00b7ners", "w\u00fcr\u00b7de", "man", "auf", "Er\u00b7den", "fin\u00b7den", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "PIS", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "