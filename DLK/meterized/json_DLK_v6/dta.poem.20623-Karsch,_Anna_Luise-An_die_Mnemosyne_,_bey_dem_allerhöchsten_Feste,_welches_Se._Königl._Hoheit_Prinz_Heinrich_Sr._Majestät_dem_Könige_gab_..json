{"dta.poem.20623": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An die Mnemosyne ,  \n bey dem  \n allerh\u00f6chsten Feste, welches Se. K\u00f6nigl. Hoheit  \n  Prinz Heinrich  \n  Sr. Majest\u00e4t dem K\u00f6nige gab .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O G\u00f6ttin, die, vom h\u00f6chsten Jupiter geliebt,                 ", "tokens": ["O", "G\u00f6t\u00b7tin", ",", "die", ",", "vom", "h\u00f6chs\u00b7ten", "Ju\u00b7pi\u00b7ter", "ge\u00b7liebt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die keusche Musen hat gebohren,", "tokens": ["Die", "keu\u00b7sche", "Mu\u00b7sen", "hat", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wen dir der Fama Lob zur Aufbewahrung giebt,", "tokens": ["Wen", "dir", "der", "Fa\u00b7ma", "Lob", "zur", "Auf\u00b7be\u00b7wah\u00b7rung", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Derselbe Held bleibt unverlohren.", "tokens": ["Der\u00b7sel\u00b7be", "Held", "bleibt", "un\u00b7ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch deiner T\u00f6chter hohe Lobges\u00e4nge lebt", "tokens": ["Durch", "dei\u00b7ner", "T\u00f6ch\u00b7ter", "ho\u00b7he", "Lob\u00b7ge\u00b7s\u00e4n\u00b7ge", "lebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einst Friedrich nach zehn tausend Sonnen,", "tokens": ["Einst", "Fried\u00b7rich", "nach", "zehn", "tau\u00b7send", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So lebet ", "tokens": ["So", "le\u00b7bet"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Von welchen Er den Sieg gewonnen.", "tokens": ["Von", "wel\u00b7chen", "Er", "den", "Sieg", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihm dampfet Weihrauch, we\u00f1 Er durch die L\u00e4nder reist,", "tokens": ["Ihm", "damp\u00b7fet", "Weih\u00b7rauch", ",", "we\u00f1", "Er", "durch", "die", "L\u00e4n\u00b7der", "reist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von andern Adlern \u00fcberschattet;", "tokens": ["Von", "an\u00b7dern", "Ad\u00b7lern", "\u00fc\u00b7bersc\u00b7hat\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und in dem Lande, wo ihr, Musen, Seinen Geist", "tokens": ["Und", "in", "dem", "Lan\u00b7de", ",", "wo", "ihr", ",", "Mu\u00b7sen", ",", "Sei\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu eurer Unterweisung hattet.", "tokens": ["Zu", "eu\u00b7rer", "Un\u00b7ter\u00b7wei\u00b7sung", "hat\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er wird verehrt, und mit Bewundrung angeblickt,", "tokens": ["Er", "wird", "ver\u00b7ehrt", ",", "und", "mit", "Be\u00b7wund\u00b7rung", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sey, da\u00df Er die Legionen", "tokens": ["Es", "sey", ",", "da\u00df", "Er", "die", "Le\u00b7gi\u00b7o\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Treffen ordnet, oder Freudenfeste schm\u00fcckt,", "tokens": ["Im", "Tref\u00b7fen", "ord\u00b7net", ",", "o\u00b7der", "Freu\u00b7den\u00b7fes\u00b7te", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier, wo ihr euch entschlo\u00dft, zu wohnen.", "tokens": ["Hier", ",", "wo", "ihr", "euch", "ent\u00b7schlo\u00dft", ",", "zu", "woh\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es sey, da\u00df sich um Ihn ein l\u00e4ndlich M\u00e4dchenchor", "tokens": ["Es", "sey", ",", "da\u00df", "sich", "um", "Ihn", "ein", "l\u00e4nd\u00b7lich", "M\u00e4d\u00b7chen\u00b7chor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PRF", "APPR", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit vollen Blumenk\u00f6rben dr\u00e4nget;", "tokens": ["Mit", "vol\u00b7len", "Blu\u00b7men\u00b7k\u00f6r\u00b7ben", "dr\u00e4n\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es sey auch, da\u00df Er hier in T\u00e4nzen strahlt hervor,", "tokens": ["Es", "sey", "auch", ",", "da\u00df", "Er", "hier", "in", "T\u00e4n\u00b7zen", "strahlt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Scherz in Seine Blicke menget.", "tokens": ["Und", "Scherz", "in", "Sei\u00b7ne", "Bli\u00b7cke", "men\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihm mag gleich Purpur, oder weiches W\u00e4ldermoo\u00df", "tokens": ["Ihm", "mag", "gleich", "Pur\u00b7pur", ",", "o\u00b7der", "wei\u00b7ches", "W\u00e4l\u00b7der\u00b7moo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Sessel dienen; Ihn empfange", "tokens": ["Zum", "Ses\u00b7sel", "die\u00b7nen", ";", "Ihn", "emp\u00b7fan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Pallast, oder auch Sein Landhaus: Er ist gro\u00df,", "tokens": ["Sein", "Pal\u00b7last", ",", "o\u00b7der", "auch", "Sein", "Land\u00b7haus", ":", "Er", "ist", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "ADV", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Menschenfreund, zum Wettgesange", "tokens": ["Als", "Men\u00b7schen\u00b7freund", ",", "zum", "Wett\u00b7ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "F\u00fcr deine Kinder, du Ged\u00e4chtni\u00dfg\u00f6ttin! Er", "tokens": ["F\u00fcr", "dei\u00b7ne", "Kin\u00b7der", ",", "du", "Ge\u00b7d\u00e4cht\u00b7ni\u00df\u00b7g\u00f6t\u00b7tin", "!", "Er"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "NN", "$.", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird mehr geliebt von Seinem Bruder,", "tokens": ["Wird", "mehr", "ge\u00b7liebt", "von", "Sei\u00b7nem", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem K\u00f6nige, als selbst Sein sieggewohntes Heer,", "tokens": ["Dem", "K\u00f6\u00b7ni\u00b7ge", ",", "als", "selbst", "Sein", "sieg\u00b7ge\u00b7wohn\u00b7tes", "Heer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein Lorbeer, und Sein Staatenruder.", "tokens": ["Sein", "Lor\u00b7beer", ",", "und", "Sein", "Staa\u00b7ten\u00b7ru\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}