{"textgrid.poem.43222": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kam einst zur\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kam einst zur\u00fcck", "tokens": ["Kam", "einst", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "In sp\u00e4ter Nacht", "tokens": ["In", "sp\u00e4\u00b7ter", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und sah zum Fenster hinaus,", "tokens": ["Und", "sah", "zum", "Fens\u00b7ter", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Kein lieber Blick", "tokens": ["Kein", "lie\u00b7ber", "Blick"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Her\u00fcberlacht,", "tokens": ["Her\u00b7\u00fc\u00b7berl\u00b7acht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Im Dunkel stand Dein Haus.", "tokens": ["Im", "Dun\u00b7kel", "stand", "Dein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf meinem Tisch", "tokens": ["Auf", "mei\u00b7nem", "Tisch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ein Brieflein lag,", "tokens": ["Ein", "Brie\u00b7flein", "lag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Geschrieben von fremder Hand,", "tokens": ["Ge\u00b7schrie\u00b7ben", "von", "frem\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Ich las den Wisch", "tokens": ["Ich", "las", "den", "Wisch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Am n\u00e4chsten Tag", "tokens": ["Am", "n\u00e4chs\u00b7ten", "Tag"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Und fluchte, da\u00df ich ihn fand.", "tokens": ["Und", "fluch\u00b7te", ",", "da\u00df", "ich", "ihn", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Von Thr\u00e4nen war", "tokens": ["Von", "Thr\u00e4\u00b7nen", "war"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Das Brieflein na\u00df,", "tokens": ["Das", "Brie\u00b7flein", "na\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Vielleicht auch feuchtgek\u00fc\u00dft \u2013", "tokens": ["Viel\u00b7leicht", "auch", "feucht\u00b7ge\u00b7k\u00fc\u00dft", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Mir wurde klar,", "tokens": ["Mir", "wur\u00b7de", "klar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Als ich so las,", "tokens": ["Als", "ich", "so", "las", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Leer war die Welt und w\u00fcst.", "tokens": ["Leer", "war", "die", "Welt", "und", "w\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "\u00bbich gehe fort,", "tokens": ["\u00bb", "ich", "ge\u00b7he", "fort", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Du bleibe hier;", "tokens": ["Du", "blei\u00b7be", "hier", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "F\u00fcr uns taugt nicht Ein Weg!", "tokens": ["F\u00fcr", "uns", "taugt", "nicht", "Ein", "Weg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Dies letzte Wort", "tokens": ["Dies", "letz\u00b7te", "Wort"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Ich bittend Dir", "tokens": ["Ich", "bit\u00b7tend", "Dir"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVPP", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Auf Deine Seele leg'.", "tokens": ["Auf", "Dei\u00b7ne", "See\u00b7le", "leg'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Du bist so klug", "tokens": ["Du", "bist", "so", "klug"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Und ich bin dumm", "tokens": ["Und", "ich", "bin", "dumm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Und traurig oft und arm;", "tokens": ["Und", "trau\u00b7rig", "oft", "und", "arm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Wei\u00df doch genug,", "tokens": ["Wei\u00df", "doch", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.29": {"text": "Ich wei\u00df, warum", "tokens": ["Ich", "wei\u00df", ",", "wa\u00b7rum"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Ich liebte treu und warm.", "tokens": ["Ich", "lieb\u00b7te", "treu", "und", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Ich wei\u00df genau,", "tokens": ["Ich", "wei\u00df", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Da\u00df meine Art", "tokens": ["Da\u00df", "mei\u00b7ne", "Art"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Zu Deiner Art nicht pa\u00dft \u2013", "tokens": ["Zu", "Dei\u00b7ner", "Art", "nicht", "pa\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Das hab' ich l\u00e4ngst erfa\u00dft.", "tokens": ["Das", "hab'", "ich", "l\u00e4ngst", "er\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Warum ich geh'?", "tokens": ["Wa\u00b7rum", "ich", "geh'", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Ich wei\u00df es wohl,", "tokens": ["Ich", "wei\u00df", "es", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Du wirst es einst versteh'n,", "tokens": ["Du", "wirst", "es", "einst", "ver\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Und wirst das Weh", "tokens": ["Und", "wirst", "das", "Weh"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.39": {"text": "Mir mitleidsvoll", "tokens": ["Mir", "mit\u00b7le\u00b7ids\u00b7voll"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "++-+-", "measure": "iambic.di"}, "line.40": {"text": "Verzeih'n beim Wiederseh'n. \u2013", "tokens": ["Ver\u00b7zeih'n", "beim", "Wie\u00b7der\u00b7seh'", "n.", "\u2013"], "token_info": ["word", "word", "word", "abbreviation", "punct"], "pos": ["NN", "APPRART", "NN", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Nicht schreiben kann", "tokens": ["Nicht", "schrei\u00b7ben", "kann"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.42": {"text": "Ich selbst das Wort,", "tokens": ["Ich", "selbst", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "Den Brief, der zu Dir spricht.", "tokens": ["Den", "Brief", ",", "der", "zu", "Dir", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Sei5 gl\u00fccklich, Mann!", "tokens": ["Sei5", "gl\u00fcck\u00b7lich", ",", "Mann", "!"], "token_info": ["number_compound", "word", "punct", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.45": {"text": "Oh \u2013 ich mu\u00df fort.", "tokens": ["Oh", "\u2013", "ich", "mu\u00df", "fort", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["XY", "$(", "PPER", "VMFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "Der Herr verla\u00df' Dich nicht!\u00ab ...", "tokens": ["Der", "Herr", "ver\u00b7la\u00df'", "Dich", "nicht", "!", "\u00ab", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.48": {"text": "Vor einem Jahr", "tokens": ["Vor", "ei\u00b7nem", "Jahr"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.49": {"text": "Schriebst Du mir so,", "tokens": ["Schriebst", "Du", "mir", "so", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.50": {"text": "Und jetzt lebst Du in Lust;", "tokens": ["Und", "jetzt", "lebst", "Du", "in", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.51": {"text": "Was ist nun wahr? \u2013", "tokens": ["Was", "ist", "nun", "wahr", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.52": {"text": "Heut' lacht' ich froh,", "tokens": ["Heut'", "lacht'", "ich", "froh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.53": {"text": "Jetzt g\u00e4hrt es in meiner Brust. \u2013", "tokens": ["Jetzt", "g\u00e4hrt", "es", "in", "mei\u00b7ner", "Brust", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}