{"dta.poem.1065": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lob ich dein wissenheit vnd lehr/", "tokens": ["Lob", "ich", "dein", "wis\u00b7sen\u00b7heit", "vnd", "lehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was du gesehen vnd er fahren;", "tokens": ["Was", "du", "ge\u00b7se\u00b7hen", "vnd", "er", "fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Spricht dein verstand/ das seine ehr", "tokens": ["Spricht", "dein", "ver\u00b7stand", "/", "das", "sei\u00b7ne", "ehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "$(", "PDS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd treflichkeit hie nicht zuspahren:", "tokens": ["Vnd", "tref\u00b7lich\u00b7keit", "hie", "nicht", "zu\u00b7spah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wan ich den gl\u00fccklichen fortgang", "tokens": ["Wan", "ich", "den", "gl\u00fcck\u00b7li\u00b7chen", "fort\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Deiner werck begehr zuerklingen/", "tokens": ["Dei\u00b7ner", "werck", "be\u00b7gehr", "zu\u00b7er\u00b7klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Will deine wei\u00dfheit/ mein gesang", "tokens": ["Will", "dei\u00b7ne", "wei\u00df\u00b7heit", "/", "mein", "ge\u00b7sang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Soll sie des Gl\u00fccks vrsach f\u00fcrbringen/", "tokens": ["Soll", "sie", "des", "Gl\u00fccks", "vr\u00b7sach", "f\u00fcr\u00b7brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Welche erwegend aigentlich", "tokens": ["Wel\u00b7che", "er\u00b7we\u00b7gend", "ai\u00b7gent\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "VVPP", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "All Ort vnd Zeit belaitet dich/", "tokens": ["All", "Ort", "vnd", "Zeit", "be\u00b7lai\u00b7tet", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Vnd das Gl\u00fcck/ solches zuregieren/", "tokens": ["Vnd", "das", "Gl\u00fcck", "/", "sol\u00b7ches", "zu\u00b7re\u00b7gie\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "PIS", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Pfleget in dem haupt zu bosieren.", "tokens": ["Pfle\u00b7get", "in", "dem", "haupt", "zu", "bo\u00b7sie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}}}}