{"textgrid.poem.37035": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Leander und Hero,", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon ehmahls sang der Leyermann", "tokens": ["Schon", "eh\u00b7mahls", "sang", "der", "Le\u00b7yer\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Musaeus die Geschichte,", "tokens": ["Mu\u00b7sa\u00b7eus", "die", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Die ich euch jetzt, so gut ich kan,", "tokens": ["Die", "ich", "euch", "jetzt", ",", "so", "gut", "ich", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erz\u00e4hle und berichte. \u2013", "tokens": ["Er\u00b7z\u00e4h\u00b7le", "und", "be\u00b7rich\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein J\u00fcngling, der Leander hie\u00df,", "tokens": ["Ein", "J\u00fcng\u00b7ling", ",", "der", "Le\u00b7an\u00b7der", "hie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kam einstmahls in ein St\u00e4dtchen,", "tokens": ["Kam", "einst\u00b7mahls", "in", "ein", "St\u00e4dt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das seinem Blick die Hero wies,", "tokens": ["Das", "sei\u00b7nem", "Blick", "die", "He\u00b7ro", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Krone aller M\u00e4dchen.", "tokens": ["Die", "Kro\u00b7ne", "al\u00b7ler", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er machte einen Reverenz,", "tokens": ["Er", "mach\u00b7te", "ei\u00b7nen", "Re\u00b7ve\u00b7renz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ihn zur Erde dr\u00fcckte,", "tokens": ["Der", "ihn", "zur", "Er\u00b7de", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als er die Mi\u00df, im jungen Lenz,", "tokens": ["Als", "er", "die", "Mi\u00df", ",", "im", "jun\u00b7gen", "Lenz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum erstenmahl erblickte.", "tokens": ["Zum", "ers\u00b7ten\u00b7mahl", "er\u00b7blick\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von nun an schwebt' ihr G\u00f6tterbild,", "tokens": ["Von", "nun", "an", "schwebt'", "ihr", "G\u00f6t\u00b7ter\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APZR", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im labyrintschen Tanze,", "tokens": ["Im", "la\u00b7by\u00b7rint\u00b7schen", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Um seinen Blick, das Haupt umh\u00fcllt", "tokens": ["Um", "sei\u00b7nen", "Blick", ",", "das", "Haupt", "um\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit einem Blumenkranze.", "tokens": ["Mit", "ei\u00b7nem", "Blu\u00b7men\u00b7kran\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bald schwatzt er ihr von Liebe vor,", "tokens": ["Bald", "schwatzt", "er", "ihr", "von", "Lie\u00b7be", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Martern, und von Schmerzen.", "tokens": ["Von", "Mar\u00b7tern", ",", "und", "von", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sie? sie widmet ihm ihr Ohr,", "tokens": ["Und", "sie", "?", "sie", "wid\u00b7met", "ihm", "ihr", "Ohr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nebst einem Platz im Herzen.", "tokens": ["Nebst", "ei\u00b7nem", "Platz", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun f\u00fchlt der J\u00fcngling sich, und brennt,", "tokens": ["Nun", "f\u00fchlt", "der", "J\u00fcng\u00b7ling", "sich", ",", "und", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sch\u00f6ne gl\u00fcht nicht minder,", "tokens": ["Die", "Sch\u00f6\u00b7ne", "gl\u00fcht", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch, ach, das Meer der Helle trennt", "tokens": ["Doch", ",", "ach", ",", "das", "Meer", "der", "Hel\u00b7le", "trennt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$,", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die beyden armen Kinder.", "tokens": ["Die", "bey\u00b7den", "ar\u00b7men", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er hatte, leider, keinen Kahn,", "tokens": ["Er", "hat\u00b7te", ",", "lei\u00b7der", ",", "kei\u00b7nen", "Kahn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum schwamm er durch die Fluthen,", "tokens": ["Drum", "schwamm", "er", "durch", "die", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was noch kein Amadis gethan,", "tokens": ["Was", "noch", "kein", "A\u00b7ma\u00b7dis", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Hayn und Fluren ruhten.", "tokens": ["Wenn", "Hayn", "und", "Flu\u00b7ren", "ruh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein schattenvoller Myrthenhayn", "tokens": ["Ein", "schat\u00b7ten\u00b7vol\u00b7ler", "Myr\u00b7then\u00b7hayn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verh\u00fcllte ihre K\u00fc\u00dfe,", "tokens": ["Ver\u00b7h\u00fcll\u00b7te", "ih\u00b7re", "K\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und tausend andre T\u00e4ndeleyn", "tokens": ["Und", "tau\u00b7send", "and\u00b7re", "T\u00e4n\u00b7de\u00b7leyn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In gr\u00fcne Finsterni\u00dfe.", "tokens": ["In", "gr\u00fc\u00b7ne", "Fins\u00b7ter\u00b7ni\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was sie sich Z\u00e4rtliches gesagt,", "tokens": ["Was", "sie", "sich", "Z\u00e4rt\u00b7li\u00b7ches", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das wi\u00dfen nur die Pl\u00e4tze,", "tokens": ["Das", "wi\u00b7\u00dfen", "nur", "die", "Pl\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo sie manch St\u00fcndchen zugebracht,", "tokens": ["Wo", "sie", "manch", "St\u00fcnd\u00b7chen", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am fl\u00fcsternden Geschw\u00e4tze", "tokens": ["Am", "fl\u00fcs\u00b7tern\u00b7den", "Ge\u00b7schw\u00e4t\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Bachs. Sie f\u00fchlten Cypris Sohn,", "tokens": ["Des", "Bachs", ".", "Sie", "f\u00fchl\u00b7ten", "Cyp\u00b7ris", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Inde\u00df die Gegend lauschte,", "tokens": ["In\u00b7de\u00df", "die", "Ge\u00b7gend", "lauschte", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ihrer K\u00fc\u00dfe Silberton", "tokens": ["Und", "ih\u00b7rer", "K\u00fc\u00b7\u00dfe", "Sil\u00b7ber\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Schattenwald durchrauschte.", "tokens": ["Den", "Schat\u00b7ten\u00b7wald", "durc\u00b7hrauschte", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Kurz, sie beschlo\u00dfen dieses Spiel,", "tokens": ["Kurz", ",", "sie", "be\u00b7schlo\u00b7\u00dfen", "die\u00b7ses", "Spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschaffen zum Erg\u00f6tzen,", "tokens": ["Ge\u00b7schaf\u00b7fen", "zum", "Er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ihnen ziemlich wohl gefiel,", "tokens": ["Das", "ih\u00b7nen", "ziem\u00b7lich", "wohl", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinf\u00fchro fortzusetzen.", "tokens": ["Hin\u00b7f\u00fch\u00b7ro", "fort\u00b7zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVIZU", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Leander schwamm, die Sch\u00f6ne sa\u00df", "tokens": ["Le\u00b7an\u00b7der", "schwamm", ",", "die", "Sch\u00f6\u00b7ne", "sa\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Ufer, voll Verlangen,", "tokens": ["Am", "U\u00b7fer", ",", "voll", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Liebling, w\u00e4r er noch so na\u00df,", "tokens": ["Den", "Lieb\u00b7ling", ",", "w\u00e4r", "er", "noch", "so", "na\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu k\u00fc\u00dfen, zu umfangen.", "tokens": ["Zu", "k\u00fc\u00b7\u00dfen", ",", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie wies ihm, mit erhobner Hand,", "tokens": ["Sie", "wies", "ihm", ",", "mit", "er\u00b7hob\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lichtgen in der Ferne,", "tokens": ["Ein", "Licht\u00b7gen", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Nacht sich um das Mondlicht wand,", "tokens": ["Wenn", "Nacht", "sich", "um", "das", "Mond\u00b7licht", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und um den Glanz der Sterne.", "tokens": ["Und", "um", "den", "Glanz", "der", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er folgte dann dem Lichtstral nach. \u2013", "tokens": ["Er", "folg\u00b7te", "dann", "dem", "Licht\u00b7stral", "nach", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch Aeols H\u00f6hlen senden", "tokens": ["Doch", "A\u00b7e\u00b7ols", "H\u00f6h\u00b7len", "sen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Einst St\u00fcrme, und die rei\u00dfen, ach,", "tokens": ["Einst", "St\u00fcr\u00b7me", ",", "und", "die", "rei\u00b7\u00dfen", ",", "ach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "KON", "ART", "ADJA", "$,", "ITJ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Licht ihr aus den H\u00e4nden.", "tokens": ["Das", "Licht", "ihr", "aus", "den", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nun \u00f6fnet sie den Rosenmund", "tokens": ["Nun", "\u00f6f\u00b7net", "sie", "den", "Ro\u00b7sen\u00b7mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Seufzern und zu Klagen,", "tokens": ["Zu", "Seuf\u00b7zern", "und", "zu", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3