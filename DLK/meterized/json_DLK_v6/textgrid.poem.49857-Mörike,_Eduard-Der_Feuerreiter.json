{"textgrid.poem.49857": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der Feuerreiter", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehet ihr am Fensterlein", "tokens": ["Se\u00b7het", "ihr", "am", "Fens\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort die rote M\u00fctze wieder?", "tokens": ["Dort", "die", "ro\u00b7te", "M\u00fct\u00b7ze", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht geheuer mu\u00df es sein,", "tokens": ["Nicht", "ge\u00b7heu\u00b7er", "mu\u00df", "es", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn er geht schon auf und nieder.", "tokens": ["Denn", "er", "geht", "schon", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und auf einmal welch Gew\u00fchle", "tokens": ["Und", "auf", "ein\u00b7mal", "welch", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bei der Br\u00fccke, nach dem Feld!", "tokens": ["Bei", "der", "Br\u00fc\u00b7cke", ",", "nach", "dem", "Feld", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Horch! das Feuergl\u00f6cklein gellt:", "tokens": ["Horch", "!", "das", "Feu\u00b7er\u00b7gl\u00f6c\u00b7klein", "gellt", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hinterm Berg,", "tokens": ["Hin\u00b7term", "Berg", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Hinterm Berg", "tokens": ["Hin\u00b7term", "Berg"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Brennt es in der M\u00fchle!", "tokens": ["Brennt", "es", "in", "der", "M\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Schaut! da sprengt er w\u00fctend schier", "tokens": ["Schaut", "!", "da", "sprengt", "er", "w\u00fc\u00b7tend", "schier"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "ADJA"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch das Tor, der Feuerreiter,", "tokens": ["Durch", "das", "Tor", ",", "der", "Feu\u00b7er\u00b7rei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem rippend\u00fcrren Tier,", "tokens": ["Auf", "dem", "rip\u00b7pen\u00b7d\u00fcr\u00b7ren", "Tier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als auf einer Feuerleiter!", "tokens": ["Als", "auf", "ei\u00b7ner", "Feu\u00b7er\u00b7lei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Querfeldein! Durch Qualm und Schw\u00fcle", "tokens": ["Quer\u00b7fel\u00b7dein", "!", "Durch", "Qualm", "und", "Schw\u00fc\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rennt er schon, und ist am Ort!", "tokens": ["Rennt", "er", "schon", ",", "und", "ist", "am", "Ort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dr\u00fcben schallt es fort und fort:", "tokens": ["Dr\u00fc\u00b7ben", "schallt", "es", "fort", "und", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hinterm Berg,", "tokens": ["Hin\u00b7term", "Berg", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Hinterm Berg", "tokens": ["Hin\u00b7term", "Berg"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Brennt es in der M\u00fchle!", "tokens": ["Brennt", "es", "in", "der", "M\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Der so oft den roten Hahn", "tokens": ["Der", "so", "oft", "den", "ro\u00b7ten", "Hahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meilenweit von fern gerochen,", "tokens": ["Mei\u00b7len\u00b7weit", "von", "fern", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit des heilgen Kreuzes Span", "tokens": ["Mit", "des", "heil\u00b7gen", "Kreu\u00b7zes", "Span"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freventlich die Glut besprochen \u2013", "tokens": ["Fre\u00b7vent\u00b7lich", "die", "Glut", "be\u00b7spro\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$("], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Weh! dir grinst vom Dachgest\u00fchle", "tokens": ["Weh", "!", "dir", "grinst", "vom", "Dach\u00b7ge\u00b7st\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dort der Feind im H\u00f6llenschein.", "tokens": ["Dort", "der", "Feind", "im", "H\u00f6l\u00b7len\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gnade Gott der Seele dein!", "tokens": ["Gna\u00b7de", "Gott", "der", "See\u00b7le", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hinterm Berg,", "tokens": ["Hin\u00b7term", "Berg", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Hinterm Berg", "tokens": ["Hin\u00b7term", "Berg"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Rast er in der M\u00fchle!", "tokens": ["Rast", "er", "in", "der", "M\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Keine Stunde hielt es an,", "tokens": ["Kei\u00b7ne", "Stun\u00b7de", "hielt", "es", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis die M\u00fchle borst in Tr\u00fcmmer;", "tokens": ["Bis", "die", "M\u00fch\u00b7le", "borst", "in", "Tr\u00fcm\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch den kecken Reitersmann", "tokens": ["Doch", "den", "ke\u00b7cken", "Rei\u00b7ters\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah man von der Stunde nimmer.", "tokens": ["Sah", "man", "von", "der", "Stun\u00b7de", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Volk und Wagen im Gew\u00fchle", "tokens": ["Volk", "und", "Wa\u00b7gen", "im", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kehren heim von all dem Graus;", "tokens": ["Keh\u00b7ren", "heim", "von", "all", "dem", "Graus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch das Gl\u00f6cklein klinget aus:", "tokens": ["Auch", "das", "Gl\u00f6c\u00b7klein", "klin\u00b7get", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Hinterm Berg,", "tokens": ["Hin\u00b7term", "Berg", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Hinterm Berg", "tokens": ["Hin\u00b7term", "Berg"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.12": {"line.1": {"text": "Brennt's! \u2013", "tokens": ["Brennt's", "!", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["NE", "$.", "$("], "meter": "+", "measure": "single.up"}}, "stanza.13": {"line.1": {"text": "Nach der Zeit ein M\u00fcller fand", "tokens": ["Nach", "der", "Zeit", "ein", "M\u00fcl\u00b7ler", "fand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein Gerippe samt der M\u00fctzen", "tokens": ["Ein", "Ge\u00b7rip\u00b7pe", "samt", "der", "M\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aufrecht an der Kellerwand", "tokens": ["Auf\u00b7recht", "an", "der", "Kel\u00b7ler\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der beinern M\u00e4hre sitzen:", "tokens": ["Auf", "der", "bei\u00b7nern", "M\u00e4h\u00b7re", "sit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Feuerreiter, wie so k\u00fchle", "tokens": ["Feu\u00b7er\u00b7rei\u00b7ter", ",", "wie", "so", "k\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reitest du in deinem Grab!", "tokens": ["Rei\u00b7test", "du", "in", "dei\u00b7nem", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Husch! da f\u00e4llt's in Asche ab.", "tokens": ["Husch", "!", "da", "f\u00e4llt's", "in", "A\u00b7sche", "ab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ruhe wohl,", "tokens": ["Ru\u00b7he", "wohl", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ruhe wohl", "tokens": ["Ru\u00b7he", "wohl"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.15": {"line.1": {"text": "Drunten in der M\u00fchle!", "tokens": ["Drun\u00b7ten", "in", "der", "M\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}