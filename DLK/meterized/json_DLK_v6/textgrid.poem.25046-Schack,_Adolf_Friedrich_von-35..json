{"textgrid.poem.25046": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, da\u00df wir diese Stunde Arm in Arme", "tokens": ["Komm", ",", "da\u00df", "wir", "die\u00b7se", "Stun\u00b7de", "Arm", "in", "Ar\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PDAT", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur seligsten des Lebens weihn!", "tokens": ["Zur", "se\u00b7ligs\u00b7ten", "des", "Le\u00b7bens", "weihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Vergessen soll die Welt mit ihrem Harme", "tokens": ["Ver\u00b7ges\u00b7sen", "soll", "die", "Welt", "mit", "ih\u00b7rem", "Har\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Vollgenu\u00df der Liebe sein!", "tokens": ["Im", "Voll\u00b7ge\u00b7nu\u00df", "der", "Lie\u00b7be", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fernab ist die Vergangenheit versunken;", "tokens": ["Fern\u00b7ab", "ist", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "ver\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, ob ein Tag dereinst uns trennt,", "tokens": ["Und", ",", "ob", "ein", "Tag", "de\u00b7reinst", "uns", "trennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht denk' ich's, w\u00e4hrend meine Seele trunken", "tokens": ["Nicht", "denk'", "ich's", ",", "w\u00e4h\u00b7rend", "mei\u00b7ne", "See\u00b7le", "trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PIS", "$,", "KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Ku\u00df auf deinem Munde brennt.", "tokens": ["Im", "Ku\u00df", "auf", "dei\u00b7nem", "Mun\u00b7de", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verwehn, in der Gef\u00fchle Sturm gebrochen,", "tokens": ["Ver\u00b7wehn", ",", "in", "der", "Ge\u00b7f\u00fch\u00b7le", "Sturm", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mag auf den Lippen uns das Wort,", "tokens": ["Mag", "auf", "den", "Lip\u00b7pen", "uns", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Pulse doch, die aneinander pochen,", "tokens": ["Die", "Pul\u00b7se", "doch", ",", "die", "an\u00b7ein\u00b7an\u00b7der", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die beiden Herzen reden fort.", "tokens": ["Die", "bei\u00b7den", "Her\u00b7zen", "re\u00b7den", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wird das finstre Thor vor uns erschlossen:", "tokens": ["Und", "wird", "das", "finst\u00b7re", "Thor", "vor", "uns", "er\u00b7schlos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie scheuten wir den letzten Pfad,", "tokens": ["Wie", "scheu\u00b7ten", "wir", "den", "letz\u00b7ten", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wir in ", "tokens": ["Die", "wir", "in"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Was Herrlichstes das Leben hat?", "tokens": ["Was", "Herr\u00b7lichs\u00b7tes", "das", "Le\u00b7ben", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}