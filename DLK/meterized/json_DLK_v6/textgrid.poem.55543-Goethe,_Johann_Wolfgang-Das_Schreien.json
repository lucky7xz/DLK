{"textgrid.poem.55543": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Das Schreien", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst ging ich meinem M\u00e4dchen nach", "tokens": ["Einst", "ging", "ich", "mei\u00b7nem", "M\u00e4d\u00b7chen", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tief in den Wald hinein", "tokens": ["Tief", "in", "den", "Wald", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und fiel ihr um den Hals und \u2013 \u00bbAch!\u00ab", "tokens": ["Und", "fiel", "ihr", "um", "den", "Hals", "und", "\u2013", "\u00bb", "Ach", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "$(", "$(", "ITJ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Droht sie, \u00bbich werde schrein.\u00ab", "tokens": ["Droht", "sie", ",", "\u00bb", "ich", "wer\u00b7de", "schrein", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "PPER", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da rief ich trotzig: \u00bbHa! ich will", "tokens": ["Da", "rief", "ich", "trot\u00b7zig", ":", "\u00bb", "Ha", "!", "ich", "will"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "$(", "ITJ", "$.", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den t\u00f6ten, der uns st\u00f6rt!\u00ab \u2013", "tokens": ["Den", "t\u00f6\u00b7ten", ",", "der", "uns", "st\u00f6rt", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbstill\u00ab, lispelt sie, \u00bbGeliebter, still!", "tokens": ["\u00bb", "still", "\u00ab", ",", "lis\u00b7pelt", "sie", ",", "\u00bb", "Ge\u00b7lieb\u00b7ter", ",", "still", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ja dich niemand h\u00f6rt.\u00ab", "tokens": ["Da\u00df", "ja", "dich", "nie\u00b7mand", "h\u00f6rt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "PPER", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}