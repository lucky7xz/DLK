{"dta.poem.8395": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  N\u00fc\u00dfler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Hier wo das klare quell mitt einfalt war vmbringet/\nDas seiner adern krafft in vnsern adern regt/\nVndt beydes sinnen trost vndt leibes wolfarth hegt/\nHier wo jhr Naja des in schlechter einfalt gienget/", "tokens": ["Hier", "wo", "das", "kla\u00b7re", "quell", "mitt", "ein\u00b7falt", "war", "vm\u00b7brin\u00b7get", "/", "Das", "sei\u00b7ner", "a\u00b7dern", "krafft", "in", "vn\u00b7sern", "a\u00b7dern", "regt", "/", "Vndt", "bey\u00b7des", "sin\u00b7nen", "trost", "vndt", "lei\u00b7bes", "wolf\u00b7arth", "hegt", "/", "Hier", "wo", "jhr", "Na\u00b7ja", "des", "in", "schlech\u00b7ter", "ein\u00b7falt", "gien\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$(", "PDS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PIS", "VVINF", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$(", "ADV", "PWAV", "PPOSAT", "NN", "ART", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+--+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Vndt ewren j\u00e4gerzeug an faule w\u00e4nde hienget/\nIst worden vmb euch her ein newer grundt gelegt/\nDer jetzt das edle hau\u00df zue ewren ehren tregt/\nVndt der Natur auch selbst nicht wenig sch\u00f6nheit bringet.", "tokens": ["Vndt", "ew\u00b7ren", "j\u00e4\u00b7ger\u00b7zeug", "an", "fau\u00b7le", "w\u00e4n\u00b7de", "hien\u00b7get", "/", "Ist", "wor\u00b7den", "vmb", "euch", "her", "ein", "ne\u00b7wer", "grundt", "ge\u00b7legt", "/", "Der", "jetzt", "das", "ed\u00b7le", "hau\u00df", "zue", "ew\u00b7ren", "eh\u00b7ren", "tregt", "/", "Vndt", "der", "Na\u00b7tur", "auch", "selbst", "nicht", "we\u00b7nig", "sch\u00f6n\u00b7heit", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$(", "VAFIN", "VAPP", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$(", "ART", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "VVINF", "VVFIN", "$(", "KON", "ART", "NN", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+--+--+--+-+-+-+-+-+-+--+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Di\u00df hatt der heldt gethan dem dieses ort geh\u00f6ret/\nDer seinen namen zwar mitt gro\u00dfen thaten mehret/\nDoch gleichwol wirdt von jhm nicht minder auff die noth", "tokens": ["Di\u00df", "hatt", "der", "heldt", "ge\u00b7than", "dem", "die\u00b7ses", "ort", "ge\u00b7h\u00f6\u00b7ret", "/", "Der", "sei\u00b7nen", "na\u00b7men", "zwar", "mitt", "gro\u00b7\u00dfen", "tha\u00b7ten", "meh\u00b7ret", "/", "Doch", "gleich\u00b7wol", "wirdt", "von", "jhm", "nicht", "min\u00b7der", "auff", "die", "noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "ART", "ADJA", "NN", "VVFIN", "$(", "ART", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "VVFIN", "VVFIN", "$(", "KON", "ADV", "VAFIN", "APPR", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+--+-+-+-+-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Vndt lust der lebens zeit durch dieses werck geschawet.\nFragt jhr/ warumb er es nach tempelsart gebawet?\nEr meint gosundtheit sey der siechen leute Gott.", "tokens": ["Vndt", "lust", "der", "le\u00b7bens", "zeit", "durch", "die\u00b7ses", "werck", "ge\u00b7scha\u00b7wet", ".", "Fragt", "jhr", "/", "wa\u00b7rumb", "er", "es", "nach", "tem\u00b7pel\u00b7sart", "ge\u00b7ba\u00b7wet", "?", "Er", "meint", "go\u00b7sund\u00b7theit", "sey", "der", "sie\u00b7chen", "leu\u00b7te", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$.", "VVFIN", "PPER", "$(", "PWAV", "PPER", "PPER", "APPR", "NE", "VVPP", "$.", "PPER", "VVFIN", "NN", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "----+--+-+-+-+--+--+--+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}