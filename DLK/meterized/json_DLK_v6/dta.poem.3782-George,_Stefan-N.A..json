{"dta.poem.3782": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Deine wimper spricht da dein mund sich schloss \u00b7", "tokens": ["Dei\u00b7ne", "wim\u00b7per", "spricht", "da", "dein", "mund", "sich", "schloss"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "PRF", "VVFIN", "XY"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dass ich von ihr den feuchten schleier sp\u00fcle", "tokens": ["Dass", "ich", "von", "ihr", "den", "feuch\u00b7ten", "schlei\u00b7er", "sp\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was soll ich o mein bruder mein genoss?", "tokens": ["Was", "soll", "ich", "o", "mein", "bru\u00b7der", "mein", "ge\u00b7noss", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "FM", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn es den \u00fcber-leuchtenden adern", "tokens": ["Wenn", "es", "den", "\u00fc\u00b7ber\u00b7leuch\u00b7ten\u00b7den", "a\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor staub und den rauhen winden graut", "tokens": ["Vor", "staub", "und", "den", "rau\u00b7hen", "win\u00b7den", "graut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "So sollst du mit dem himmel nicht hadern", "tokens": ["So", "sollst", "du", "mit", "dem", "him\u00b7mel", "nicht", "ha\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der an dem hehren spiel sich erbaut", "tokens": ["Der", "an", "dem", "heh\u00b7ren", "spiel", "sich", "er\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Nimm als lohn dass vor dir nur kranken", "tokens": ["Nimm", "als", "lohn", "dass", "vor", "dir", "nur", "kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "NN", "KOUS", "APPR", "PPER", "ADV", "VVINF"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die stolzen glieder zur urne gar", "tokens": ["Die", "stol\u00b7zen", "glie\u00b7der", "zur", "ur\u00b7ne", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es ziemt nicht in irdischer klage zu wanken", "tokens": ["Es", "ziemt", "nicht", "in", "ir\u00b7di\u00b7scher", "kla\u00b7ge", "zu", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Uns die das los f\u00fcr den purpur gebar", "tokens": ["Uns", "die", "das", "los", "f\u00fcr", "den", "pur\u00b7pur", "ge\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ART", "NN", "APPR", "ART", "ADJD", "ADJD"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}}}}}