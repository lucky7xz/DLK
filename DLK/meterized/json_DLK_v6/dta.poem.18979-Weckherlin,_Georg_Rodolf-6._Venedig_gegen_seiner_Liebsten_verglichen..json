{"dta.poem.18979": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "6.  \n  Venedig gegen seiner Liebsten  \n verglichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Wjtzlo\u00df war die f\u00fcrwitz/ aufs\u00e4tzig der f\u00fcrsatz/", "tokens": ["Wjtz\u00b7lo\u00df", "war", "die", "f\u00fcr\u00b7witz", "/", "auf\u00b7s\u00e4t\u00b7zig", "der", "f\u00fcr\u00b7satz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "$(", "ADJD", "ART", "NN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Creutz-geitzig der ehrgeitz/ die mich so sehr be-", "tokens": ["Creutz\u00b7geit\u00b7zig", "der", "ehr\u00b7geitz", "/", "die", "mich", "so", "sehr", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$(", "PRELS", "PPER", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "th\u00f6ret/", "tokens": ["th\u00f6\u00b7ret", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Da\u00df eines F\u00fcrsten will/ der Sch\u00f6n vnd Liebgesatz", "tokens": ["Da\u00df", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "will", "/", "der", "Sch\u00f6n", "vnd", "Lieb\u00b7ge\u00b7satz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VMFIN", "$(", "ART", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zuwider/ mich gleichwol gehorsamen gelehret.", "tokens": ["Zu\u00b7wi\u00b7der", "/", "mich", "gleich\u00b7wol", "ge\u00b7hor\u00b7sa\u00b7men", "ge\u00b7leh\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Dan wz seind doch die Brent/ Galleen/ Marxenplatz/", "tokens": ["Dan", "wz", "seind", "doch", "die", "Brent", "/", "Gal\u00b7le\u00b7en", "/", "Mar\u00b7xen\u00b7platz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$(", "NN", "$(", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die statliche pall\u00e4st/ der schatz so weit vermehret/", "tokens": ["Die", "stat\u00b7li\u00b7che", "pal\u00b7l\u00e4st", "/", "der", "schatz", "so", "weit", "ver\u00b7meh\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gegen der haaren strom v\u00f5 purem gold bewehret/", "tokens": ["Ge\u00b7gen", "der", "haa\u00b7ren", "strom", "v\u00f5", "pu\u00b7rem", "gold", "be\u00b7weh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "FM", "FM", "FM", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd gegen der Sch\u00f6nheit vnd tugend gr\u00f6sserm", "tokens": ["Vnd", "ge\u00b7gen", "der", "Sch\u00f6n\u00b7heit", "vnd", "tu\u00b7gend", "gr\u00f6s\u00b7serm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "schatz?", "tokens": ["schatz", "?"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Was ist des Hertzogs/ Rahts/ vn\u0303 Curtisanen prange\u0304", "tokens": ["Was", "ist", "des", "Hert\u00b7zogs", "/", "Rahts", "/", "v\u00f1", "Cur\u00b7ti\u00b7sa\u00b7nen", "prang\u0113"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$(", "NN", "$(", "XY", "XY", "XY"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In purpur/ scharlach/ gold/ in bestem saal vnnd", "tokens": ["In", "pur\u00b7pur", "/", "schar\u00b7lach", "/", "gold", "/", "in", "bes\u00b7tem", "saal", "vnnd"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "$(", "VVFIN", "$(", "NN", "$(", "APPR", "ADJA", "NN", "KON"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "mahl/", "tokens": ["mahl", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Verglichen mit dem schmuck der lippen vnd der", "tokens": ["Ver\u00b7gli\u00b7chen", "mit", "dem", "schmuck", "der", "lip\u00b7pen", "vnd", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "KON", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "wangen:", "tokens": ["wan\u00b7gen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Was seind die M\u00fcntz/ Zeughau\u00df/ gesch\u00fctz vnd Ar-", "tokens": ["Was", "seind", "die", "M\u00fcntz", "/", "Zeug\u00b7hau\u00df", "/", "ge\u00b7sch\u00fctz", "vnd", "Ar"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$(", "NE", "$(", "ADJD", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "senal/", "tokens": ["se\u00b7nal", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "--", "measure": "unknown.measure.zero"}, "line.3": {"text": "Gegen dem sch\u00f6nen aug/ dz billich (mein verlang\u1ebd", "tokens": ["Ge\u00b7gen", "dem", "sch\u00f6\u00b7nen", "aug", "/", "dz", "bil\u00b7lich", "(", "mein", "ver\u00b7lang\u1ebd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "KOUS", "ADJD", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zustrafen) so weit ab mich t\u00f6det wie ein strahl?", "tokens": ["Zu\u00b7stra\u00b7fen", ")", "so", "weit", "ab", "mich", "t\u00f6\u00b7det", "wie", "ein", "strahl", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}