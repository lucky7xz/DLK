{"dta.poem.9184": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n Die unverrostete Liebe.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Die erste liebe rostet nicht/", "tokens": ["Die", "ers\u00b7te", "lie\u00b7be", "ros\u00b7tet", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann zwey verliebte seelen", "tokens": ["Wann", "zwey", "ver\u00b7lieb\u00b7te", "see\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "CARD", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein ander in getreuer pflicht", "tokens": ["Ein", "an\u00b7der", "in", "ge\u00b7treu\u00b7er", "pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dieser zeit erw\u00e4hlen/", "tokens": ["Zu", "die\u00b7ser", "zeit", "er\u00b7w\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da noch die zarte gluth vielleicht", "tokens": ["Da", "noch", "die", "zar\u00b7te", "gluth", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach keinen fremden feuer r\u00e4ucht.", "tokens": ["Nach", "kei\u00b7nen", "frem\u00b7den", "feu\u00b7er", "r\u00e4ucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Bald erstlich hat erkohren/", "tokens": ["Bald", "erst\u00b7lich", "hat", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ist ihr liebstes vatterland", "tokens": ["Da", "ist", "ihr", "liebs\u00b7tes", "vat\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da wird sie neu gebohren/", "tokens": ["Da", "wird", "sie", "neu", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da trifft sie lust und nahrung an/", "tokens": ["Da", "trifft", "sie", "lust", "und", "nah\u00b7rung", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dadurch sie sich vermehren kan.", "tokens": ["Da\u00b7durch", "sie", "sich", "ver\u00b7meh\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. So bald sie aber anderwerts", "tokens": ["So", "bald", "sie", "a\u00b7ber", "an\u00b7der\u00b7werts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die heissen strahlen lencket/", "tokens": ["Die", "heis\u00b7sen", "strah\u00b7len", "len\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und an ein unbekanntes hertz", "tokens": ["Und", "an", "ein", "un\u00b7be\u00b7kann\u00b7tes", "hertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die lust-begierden hencket/", "tokens": ["Die", "lust\u00b7be\u00b7gier\u00b7den", "hen\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So k\u00f6mmt sie in ein fremdes land", "tokens": ["So", "k\u00f6mmt", "sie", "in", "ein", "frem\u00b7des", "land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und setzt den grundstein auff den sand.", "tokens": ["Und", "setzt", "den", "grund\u00b7stein", "auff", "den", "sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Es darff ein schlechter wirbelwind", "tokens": ["Es", "darff", "ein", "schlech\u00b7ter", "wir\u00b7bel\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein kleines l\u00fcfftgen rasen/", "tokens": ["Ein", "klei\u00b7nes", "l\u00fcfft\u00b7gen", "ra\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wird das wesen gar geschwind", "tokens": ["So", "wird", "das", "we\u00b7sen", "gar", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PDS", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einen klump geblasen/", "tokens": ["In", "ei\u00b7nen", "klump", "ge\u00b7bla\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Alsdenn so mu\u00df der st\u00e4rckste wein", "tokens": ["Als\u00b7denn", "so", "mu\u00df", "der", "st\u00e4rcks\u00b7te", "wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des sch\u00e4rffsten e\u00dfigs vater seyn.", "tokens": ["Des", "sch\u00e4rffs\u00b7ten", "e\u00b7\u00dfigs", "va\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Dem ersten liebsten bleib man gut/", "tokens": ["Dem", "ers\u00b7ten", "liebs\u00b7ten", "bleib", "man", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wann man unterweilen", "tokens": ["Und", "wann", "man", "un\u00b7ter\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleich anders redt und anders thut/", "tokens": ["Gleich", "an\u00b7ders", "redt", "und", "an\u00b7ders", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will doch der schmertz nicht heilen/", "tokens": ["Will", "doch", "der", "schmertz", "nicht", "hei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es bleibt ein kleiner auffenthalt/", "tokens": ["Es", "bleibt", "ein", "klei\u00b7ner", "auf\u00b7fent\u00b7halt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der immer in dem hertzen wallt.", "tokens": ["Der", "im\u00b7mer", "in", "dem", "hert\u00b7zen", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Es m\u00fcst ein schlechtes m\u00e4dgen seyn", "tokens": ["Es", "m\u00fcst", "ein", "schlech\u00b7tes", "m\u00e4d\u00b7gen", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das wir nicht solten lieben/", "tokens": ["Das", "wir", "nicht", "sol\u00b7ten", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wann sie uns zu der s\u00fcssen pein", "tokens": ["Wann", "sie", "uns", "zu", "der", "s\u00fcs\u00b7sen", "pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat erstlich angetrieben/", "tokens": ["Hat", "erst\u00b7lich", "an\u00b7ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wann es bi\u00dfweilen anders scheint/", "tokens": ["Wann", "es", "bi\u00df\u00b7wei\u00b7len", "an\u00b7ders", "scheint", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind die gedancken doch ihr freund.", "tokens": ["Sind", "die", "ge\u00b7dan\u00b7cken", "doch", "ihr", "freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Ich f\u00fchle meinen ersten pfeil", "tokens": ["Ich", "f\u00fch\u00b7le", "mei\u00b7nen", "ers\u00b7ten", "pfeil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch immer in den hertzen/", "tokens": ["Noch", "im\u00b7mer", "in", "den", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der nimmt noch sein bescheiden theil", "tokens": ["Der", "nimmt", "noch", "sein", "be\u00b7schei\u00b7den", "theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von meinen zarten schmettzen/", "tokens": ["Von", "mei\u00b7nen", "zar\u00b7ten", "schmett\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ob mirs gleich nicht werden kan/", "tokens": ["Und", "ob", "mirs", "gleich", "nicht", "wer\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "ADV", "PTKNEG", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So denck ich doch mit lust daran.", "tokens": ["So", "denck", "ich", "doch", "mit", "lust", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "VVFIN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "8. Ich kan mich endlich scharff verliebt", "tokens": ["Ich", "kan", "mich", "end\u00b7lich", "scharff", "ver\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey allen m\u00e4dgen stellen/", "tokens": ["Bey", "al\u00b7len", "m\u00e4d\u00b7gen", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und die ein bi\u00dfgen anla\u00df gibt/", "tokens": ["Und", "die", "ein", "bi\u00df\u00b7gen", "an\u00b7la\u00df", "gibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan leicht mein hertze f\u00e4llen/", "tokens": ["Kan", "leicht", "mein", "hert\u00b7ze", "f\u00e4l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch keine blickt mich s\u00fcsser an/", "tokens": ["Doch", "kei\u00b7ne", "blickt", "mich", "s\u00fcs\u00b7ser", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als die ich erstlich lieb gewan.", "tokens": ["Als", "die", "ich", "erst\u00b7lich", "lieb", "ge\u00b7wan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}