{"dta.poem.3969": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XxIX.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dich mein Siegel nicht erschrecken,", "tokens": ["La\u00df", "dich", "mein", "Sie\u00b7gel", "nicht", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das dir die schwartze Farbe zeigt.", "tokens": ["Das", "dir", "die", "schwart\u00b7ze", "Far\u00b7be", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Inhalt wird dir mehr entdecken,", "tokens": ["Der", "In\u00b7halt", "wird", "dir", "mehr", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein altes Weib ist nun erbleicht.", "tokens": ["Mein", "al\u00b7tes", "Weib", "ist", "nun", "er\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vielleicht wird auch mein banges Trauren", "tokens": ["Viel\u00b7leicht", "wird", "auch", "mein", "ban\u00b7ges", "Trau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht vier und zwantzig Stunden dauren.", "tokens": ["Nicht", "vier", "und", "zwant\u00b7zig", "Stun\u00b7den", "dau\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "CARD", "KON", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}