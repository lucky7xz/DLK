{"textgrid.poem.41097": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pl\u00f6tzlich t\u00f6nen meine Saiten,", "tokens": ["Pl\u00f6tz\u00b7lich", "t\u00f6\u00b7nen", "mei\u00b7ne", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie von Schmerz und Lust bewegt,", "tokens": ["Wie", "von", "Schmerz", "und", "Lust", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmerz, da\u00df ", "tokens": ["Schmerz", ",", "da\u00df"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Schwer sich mir ans Herz gelegt,", "tokens": ["Schwer", "sich", "mir", "ans", "Herz", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lust, da\u00df ", "tokens": ["Lust", ",", "da\u00df"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Einem aus dem S\u00e4ngerbund,", "tokens": ["Ei\u00b7nem", "aus", "dem", "S\u00e4n\u00b7ger\u00b7bund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den gedr\u00fcckt der Erde Schwere,", "tokens": ["Den", "ge\u00b7dr\u00fcckt", "der", "Er\u00b7de", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gabest ", "tokens": ["Ga\u00b7best"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Dies und das hat mich ger\u00fchret,", "tokens": ["Dies", "und", "das", "hat", "mich", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Und ich kann mich halten nicht,", "tokens": ["Und", "ich", "kann", "mich", "hal\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df, wenn sich's auch nicht geb\u00fchret,", "tokens": ["Da\u00df", ",", "wenn", "sich's", "auch", "nicht", "ge\u00b7b\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PIS", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Heut mein Herz zu ", "tokens": ["Heut", "mein", "Herz", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Offen spricht: Vernimm die Wahrheit!", "tokens": ["Of\u00b7fen", "spricht", ":", "Ver\u00b7nimm", "die", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz! ", "tokens": ["Herz", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die in solcher Engelsklarheit", "tokens": ["Die", "in", "sol\u00b7cher", "En\u00b7gels\u00b7klar\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leuchtete dem Lande nie.", "tokens": ["Leuch\u00b7te\u00b7te", "dem", "Lan\u00b7de", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "J\u00fcngst im Traum hab' ich gesehen", "tokens": ["J\u00fcngst", "im", "Traum", "hab'", "ich", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Licht ", "tokens": ["Licht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ein lichtblauer Flor der Feen,", "tokens": ["Ein", "licht\u00b7blau\u00b7er", "Flor", "der", "Feen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Silberschimmernd war ", "tokens": ["Sil\u00b7ber\u00b7schim\u00b7mernd", "war"], "token_info": ["word", "word"], "pos": ["ADJD", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Und zugleich ist mir erschienen", "tokens": ["Und", "zu\u00b7gleich", "ist", "mir", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Land, vom Lenz durchlacht,", "tokens": ["Un\u00b7ser", "Land", ",", "vom", "Lenz", "durch\u00b7lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tal und Berg, die lichten, gr\u00fcnen,", "tokens": ["Tal", "und", "Berg", ",", "die", "lich\u00b7ten", ",", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alter W\u00e4lder dunkle Nacht.", "tokens": ["Al\u00b7ter", "W\u00e4l\u00b7der", "dunk\u00b7le", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "H\u00f6rte seiner Fl\u00fcsse T\u00f6nen,", "tokens": ["H\u00f6r\u00b7te", "sei\u00b7ner", "Fl\u00fcs\u00b7se", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner S\u00e4nger bunten Sang,", "tokens": ["Sei\u00b7ner", "S\u00e4n\u00b7ger", "bun\u00b7ten", "Sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah ", "tokens": ["Sah"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Von des Landes Bl\u00fct' und Klang.", "tokens": ["Von", "des", "Lan\u00b7des", "Bl\u00fct'", "und", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Damals ist es mir gekommen", "tokens": ["Da\u00b7mals", "ist", "es", "mir", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "(", "tokens": ["("], "token_info": ["punct"], "pos": ["$("]}, "line.3": {"text": "Gott zu bitten, da\u00df genommen", "tokens": ["Gott", "zu", "bit\u00b7ten", ",", "da\u00df", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$,", "KOUS", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie ", "tokens": ["Nie"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}, "stanza.9": {"line.1": {"text": "Dieses ist mein Beten, Sehnen,", "tokens": ["Die\u00b7ses", "ist", "mein", "Be\u00b7ten", ",", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ernste Wahrheit, kein Gedicht,", "tokens": ["Erns\u00b7te", "Wahr\u00b7heit", ",", "kein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niederschreib' ich ", "tokens": ["Nie\u00b7der\u00b7schreib'", "ich"], "token_info": ["word", "word"], "pos": ["VVIMP", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Glaub' es, oder glaub' es nicht.", "tokens": ["Glaub'", "es", ",", "o\u00b7der", "glaub'", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Da\u00df ich solches ", "tokens": ["Da\u00df", "ich", "sol\u00b7ches"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wei\u00df kein Mensch auf dieser Welt,", "tokens": ["Wei\u00df", "kein", "Mensch", "auf", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum zernichte nach Belieben", "tokens": ["Drum", "zer\u00b7nich\u00b7te", "nach", "Be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses Blatt, wenn's ", "tokens": ["Die\u00b7ses", "Blatt", ",", "wenn's"], "token_info": ["word", "word", "punct", "word"], "pos": ["PDAT", "NN", "$,", "KOUS"], "meter": "+-+-", "measure": "trochaic.di"}}}}}