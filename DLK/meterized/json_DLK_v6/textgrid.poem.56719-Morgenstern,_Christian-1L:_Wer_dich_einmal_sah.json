{"textgrid.poem.56719": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer dich einmal sah", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer dich einmal sah", "tokens": ["Wer", "dich", "ein\u00b7mal", "sah"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "vom S\u00f6ller des Hochgebirgs,", "tokens": ["vom", "S\u00f6l\u00b7ler", "des", "Hoch\u00b7ge\u00b7birgs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "am Saum der Lande", "tokens": ["am", "Saum", "der", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "emporsteigen,", "tokens": ["em\u00b7por\u00b7stei\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "aus schwarzem Waldscho\u00df", "tokens": ["aus", "schwar\u00b7zem", "Wald\u00b7scho\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "emporgeboren,", "tokens": ["em\u00b7por\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "oder purpurnen Meeren", "tokens": ["o\u00b7der", "pur\u00b7pur\u00b7nen", "Mee\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "dich leicht entwiegend \u2013", "tokens": ["dich", "leicht", "ent\u00b7wie\u00b7gend", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "wer dich einmal sah", "tokens": ["wer", "dich", "ein\u00b7mal", "sah"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "die br\u00e4utliche Erde", "tokens": ["die", "br\u00e4ut\u00b7li\u00b7che", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "aufk\u00fcssen", "tokens": ["auf\u00b7k\u00fcs\u00b7sen"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "aus Morgentr\u00e4umen,", "tokens": ["aus", "Mor\u00b7gen\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "bis sie, von deiner Schw\u00fcre", "tokens": ["bis", "sie", ",", "von", "dei\u00b7ner", "Schw\u00fc\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Flammenodem", "tokens": ["Flam\u00b7me\u00b7no\u00b7dem"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "hei\u00df err\u00f6tend,", "tokens": ["hei\u00df", "er\u00b7r\u00f6\u00b7tend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "dir entgegenbl\u00fchte,", "tokens": ["dir", "ent\u00b7ge\u00b7gen\u00b7bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "in der zitternden Scham,", "tokens": ["in", "der", "zit\u00b7tern\u00b