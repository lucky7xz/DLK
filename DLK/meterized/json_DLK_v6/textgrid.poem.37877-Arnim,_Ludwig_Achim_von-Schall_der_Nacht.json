{"textgrid.poem.37877": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Schall der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm Trost der Nacht, o Nachtigall!", "tokens": ["Komm", "Trost", "der", "Nacht", ",", "o", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df deine Stimm mit Freuden-Schall", "tokens": ["La\u00df", "dei\u00b7ne", "Stimm", "mit", "Freu\u00b7den\u00b7Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aufs lieblichste erklingen,", "tokens": ["Aufs", "lieb\u00b7lichs\u00b7te", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm, komm, und lob den Sch\u00f6pfer dein,", "tokens": ["Komm", ",", "komm", ",", "und", "lob", "den", "Sch\u00f6p\u00b7fer", "dein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "KON", "NN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil andre V\u00f6gel schlafen seyn,", "tokens": ["Weil", "and\u00b7re", "V\u00f6\u00b7gel", "schla\u00b7fen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nicht mehr m\u00f6gen singen;", "tokens": ["Und", "nicht", "mehr", "m\u00f6\u00b7gen", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df dein Stimmlein", "tokens": ["La\u00df", "dein", "Stimm\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Laut erschallen, denn vor allen", "tokens": ["Laut", "er\u00b7schal\u00b7len", ",", "denn", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVINF", "$,", "KON", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kannst du loben", "tokens": ["Kannst", "du", "lo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Gott im Himmel, hoch dort oben.", "tokens": ["Gott", "im", "Him\u00b7mel", ",", "hoch", "dort", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Obschon ist hin der Sonnenschein,", "tokens": ["Ob\u00b7schon", "ist", "hin", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wir im Finstern m\u00fcssen seyn,", "tokens": ["Und", "wir", "im", "Fins\u00b7tern", "m\u00fcs\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So k\u00f6nnen wir doch singen", "tokens": ["So", "k\u00f6n\u00b7nen", "wir", "doch", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Gottes G\u00fct und seiner Macht,", "tokens": ["Von", "Got\u00b7tes", "G\u00fct", "und", "sei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil uns kann hindern keine Nacht,", "tokens": ["Weil", "uns", "kann", "hin\u00b7dern", "kei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Loben zu vollbringen.", "tokens": ["Sein", "Lo\u00b7ben", "zu", "voll\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum dein Stimmlein", "tokens": ["Drum", "dein", "Stimm\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "La\u00df erschallen, denn vor allen", "tokens": ["La\u00df", "er\u00b7schal\u00b7len", ",", "denn", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "KON", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kannst du loben", "tokens": ["Kannst", "du", "lo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Gott im Himmel, hoch dort oben.", "tokens": ["Gott", "im", "Him\u00b7mel", ",", "hoch", "dort", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Echo, der wilde Wiederhall,", "tokens": ["E\u00b7cho", ",", "der", "wil\u00b7de", "Wie\u00b7der\u00b7hall", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Will seyn bei diesem Freudenschall,", "tokens": ["Will", "seyn", "bei", "die\u00b7sem", "Freu\u00b7den\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4\u00dfet sich auch h\u00f6ren;", "tokens": ["Und", "l\u00e4\u00b7\u00dfet", "sich", "auch", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verweist uns alle M\u00fcdigkeit,", "tokens": ["Ver\u00b7weist", "uns", "al\u00b7le", "M\u00fc\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der wir ergeben allezeit,", "tokens": ["Der", "wir", "er\u00b7ge\u00b7ben", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lehrt uns den Schlaf beth\u00f6ren.", "tokens": ["Lehrt", "uns", "den", "Schlaf", "be\u00b7th\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum dein Stimmlein", "tokens": ["Drum", "dein", "Stimm\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "La\u00df erschallen, denn vor allen", "tokens": ["La\u00df", "er\u00b7schal\u00b7len", ",", "denn", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "KON", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kannst du loben", "tokens": ["Kannst", "du", "lo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Gott im Himmel, hoch dort oben.", "tokens": ["Gott", "im", "Him\u00b7mel", ",", "hoch", "dort", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Sterne, so am Himmel stehn,", "tokens": ["Die", "Ster\u00b7ne", ",", "so", "am", "Him\u00b7mel", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich lassen Gott zum Lobe sehn,", "tokens": ["Sich", "las\u00b7sen", "Gott", "zum", "Lo\u00b7be", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Ehre ihm beweisen;", "tokens": ["Und", "Eh\u00b7re", "ihm", "be\u00b7wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Eul' auch, die nicht singen kann,", "tokens": ["Die", "Eul'", "auch", ",", "die", "nicht", "sin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zeigt doch mit ihrem Heulen an,", "tokens": ["Zeigt", "doch", "mit", "ih\u00b7rem", "Heu\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sie auch Gott thu preisen.", "tokens": ["Da\u00df", "sie", "auch", "Gott", "thu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum dein Stimmlein", "tokens": ["Drum", "dein", "Stimm\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "La\u00df erschallen, denn vor allen", "tokens": ["La\u00df", "er\u00b7schal\u00b7len", ",", "denn", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "KON", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kannst du loben", "tokens": ["Kannst", "du", "lo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Gott im Himmel, hoch dort oben.", "tokens": ["Gott", "im", "Him\u00b7mel", ",", "hoch", "dort", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur her, mein liebstes V\u00f6gelein!", "tokens": ["Nur", "her", ",", "mein", "liebs\u00b7tes", "V\u00f6\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wollen nicht die faulsten seyn,", "tokens": ["Wir", "wol\u00b7len", "nicht", "die", "fauls\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schlafen liegen bleiben,", "tokens": ["Und", "schla\u00b7fen", "lie\u00b7gen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vielmehr bis da\u00df die Morgenr\u00f6th", "tokens": ["Viel\u00b7mehr", "bis", "da\u00df", "die", "Mor\u00b7gen\u00b7r\u00f6th"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "KOUS", "ART", "NN"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Erfreuet diese W\u00e4lder-Oed,", "tokens": ["Er\u00b7freu\u00b7et", "die\u00b7se", "W\u00e4l\u00b7der\u00b7Oed", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Gottes Lob vertreiben;", "tokens": ["In", "Got\u00b7tes", "Lob", "ver\u00b7trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df dein Stimmlein", "tokens": ["La\u00df", "dein", "Stimm\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Laut erschallen, denn vor allen", "tokens": ["Laut", "er\u00b7schal\u00b7len", ",", "denn", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVINF", "$,", "KON", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kannst du loben", "tokens": ["Kannst", "du", "lo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Gott im Himmel, hoch dort oben.", "tokens": ["Gott", "im", "Him\u00b7mel", ",", "hoch", "dort", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}