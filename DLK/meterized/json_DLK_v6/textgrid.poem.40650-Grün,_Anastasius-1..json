{"textgrid.poem.40650": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als wettergleich fernher ert\u00f6nt die Kunde,", "tokens": ["Als", "wet\u00b7ter\u00b7gleich", "fern\u00b7her", "er\u00b7t\u00f6nt", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du geschmiedet an den Fels der Leiden,", "tokens": ["Da\u00df", "du", "ge\u00b7schmie\u00b7det", "an", "den", "Fels", "der", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da f\u00fchlt' ich durch das eigne Herz mir schneiden", "tokens": ["Da", "f\u00fchlt'", "ich", "durch", "das", "eig\u00b7ne", "Herz", "mir", "schnei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein gro\u00dfes Ungl\u00fcck, eine tiefe Wunde.", "tokens": ["Ein", "gro\u00b7\u00dfes", "Un\u00b7gl\u00fcck", ",", "ei\u00b7ne", "tie\u00b7fe", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sprie\u00dfe gern f\u00fcr mich allein im Grunde,", "tokens": ["Ich", "sprie\u00b7\u00dfe", "gern", "f\u00fcr", "mich", "al\u00b7lein", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch mocht' an dir zu ranken ich nicht meiden,", "tokens": ["Doch", "mocht'", "an", "dir", "zu", "ran\u00b7ken", "ich", "nicht", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKZU", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Gottesurtheil war mir dein Entscheiden,", "tokens": ["Ein", "Got\u00b7tes\u00b7urt\u00b7heil", "war", "mir", "dein", "Ent\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein liebster Kranz das Lob aus deinem Munde.", "tokens": ["Mein", "liebs\u00b7ter", "Kranz", "das", "Lob", "aus", "dei\u00b7nem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du sprachst mir Muth, als Unmuth mich gebogen,", "tokens": ["Du", "sprachst", "mir", "Muth", ",", "als", "Un\u00b7muth", "mich", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du hieltst mich werth; dein Mund, der nie gelogen,", "tokens": ["Du", "hieltst", "mich", "werth", ";", "dein", "Mund", ",", "der", "nie", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er lehrte mich an eignen Werth noch glauben.", "tokens": ["Er", "lehr\u00b7te", "mich", "an", "eig\u00b7nen", "Werth", "noch", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wollten dich mir die D\u00e4monen rauben,", "tokens": ["Und", "woll\u00b7ten", "dich", "mir", "die", "D\u00e4\u00b7mo\u00b7nen", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerbrochen w\u00e4r' mein Stab, mein Kranz zerrissen,", "tokens": ["Zer\u00b7bro\u00b7chen", "w\u00e4r'", "mein", "Stab", ",", "mein", "Kranz", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und todt in dir mein Hoffen \u2013 mein Gewissen.", "tokens": ["Und", "todt", "in", "dir", "mein", "Hof\u00b7fen", "\u2013", "mein", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}