{"dta.poem.6158": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch bey der \u00fcberall bemerckten Fl\u00fcchtigkeit", "tokens": ["Noch", "bey", "der", "\u00fc\u00b7be\u00b7rall", "be\u00b7merck\u00b7ten", "Fl\u00fcch\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die solche Wunder wirckt, und solche Last bewegt,", "tokens": ["Die", "sol\u00b7che", "Wun\u00b7der", "wirckt", ",", "und", "sol\u00b7che", "Last", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sieht man zu aller Zeit,", "tokens": ["Sieht", "man", "zu", "al\u00b7ler", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In unterschiednen Dingen,", "tokens": ["In", "un\u00b7ter\u00b7schied\u00b7nen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So manche pl\u00f6tzliche Ver\u00e4nderung entspringen,", "tokens": ["So", "man\u00b7che", "pl\u00f6tz\u00b7li\u00b7che", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "ent\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch kleine Federchen erregt,", "tokens": ["Durch", "klei\u00b7ne", "Fe\u00b7der\u00b7chen", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Die nicht zu sehen sind. Man will es stets ergr\u00fcnden,", "tokens": ["Die", "nicht", "zu", "se\u00b7hen", "sind", ".", "Man", "will", "es", "stets", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$.", "PIS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und immer jedem Ding\u2019 ein eigne Ursach finden.", "tokens": ["Und", "im\u00b7mer", "je\u00b7dem", "Ding'", "ein", "eig\u00b7ne", "Ur\u00b7sach", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man gr\u00fcbelt und man sucht so manchen Unterscheid,", "tokens": ["Man", "gr\u00fc\u00b7belt", "und", "man", "sucht", "so", "man\u00b7chen", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "PIS", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "An statt gefundner Deutlichkeit;", "tokens": ["An", "statt", "ge\u00b7fund\u00b7ner", "Deut\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Erf\u00fcllt man alle Ding mit dunckler Eigenschafft.", "tokens": ["Er\u00b7f\u00fcllt", "man", "al\u00b7le", "Ding", "mit", "dunck\u00b7ler", "Ei\u00b7gen\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man h\u00f6ret anders nichts, als eine g\u00e4hrende,", "tokens": ["Man", "h\u00f6\u00b7ret", "an\u00b7ders", "nichts", ",", "als", "ei\u00b7ne", "g\u00e4h\u00b7ren\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIS", "$,", "KOUS", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.13": {"text": "Bald eine Sympathetische,", "tokens": ["Bald", "ei\u00b7ne", "Sym\u00b7pa\u00b7the\u00b7ti\u00b7sche", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Bald eine zu sich ziehnde Krafft.", "tokens": ["Bald", "ei\u00b7ne", "zu", "sich", "ziehn\u00b7de", "Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die einige Bewegung mu\u00df allein", "tokens": ["Die", "ei\u00b7ni\u00b7ge", "Be\u00b7we\u00b7gung", "mu\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VMFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Durch eitle Nahmen nur so offt vervielfacht seyn.", "tokens": ["Durch", "eit\u00b7le", "Nah\u00b7men", "nur", "so", "offt", "ver\u00b7viel\u00b7facht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}