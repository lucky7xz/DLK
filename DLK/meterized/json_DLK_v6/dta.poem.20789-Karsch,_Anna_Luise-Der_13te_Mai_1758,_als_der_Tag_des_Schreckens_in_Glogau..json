{"dta.poem.20789": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der 13te Mai 1758,  \n als der                \n  Tag des Schreckens in Glogau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit einer ihren Gram erz\u00e4hlenden Geb\u00e4rde               ", "tokens": ["Mit", "ei\u00b7ner", "ih\u00b7ren", "Gram", "er\u00b7z\u00e4h\u00b7len\u00b7den", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirft Glogau noch den Blick zur Erde,", "tokens": ["Wirft", "Glo\u00b7gau", "noch", "den", "Blick", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und ist an Schutt und Klagen reich.", "tokens": ["Und", "ist", "an", "Schutt", "und", "Kla\u00b7gen", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fremdling siehet noch den Staub vom Heiligthume,", "tokens": ["Der", "Fremd\u00b7ling", "sie\u00b7het", "noch", "den", "Staub", "vom", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wird, der Menschlichkeit zum Ruhme,", "tokens": ["Und", "wird", ",", "der", "Menschlich\u00b7keit", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bey traurigen Ruinen weich.", "tokens": ["Bey", "trau\u00b7ri\u00b7gen", "Ru\u00b7i\u00b7nen", "weich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und mehr als er vom tiefen Schmerz bezwungen,", "tokens": ["Und", "mehr", "als", "er", "vom", "tie\u00b7fen", "Schmerz", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom st\u00e4rkern Mitleid mehr durchdrungen,", "tokens": ["Vom", "st\u00e4r\u00b7kern", "Mit\u00b7leid", "mehr", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sing ich die halb zerst\u00f6hrte Stadt.", "tokens": ["Sing", "ich", "die", "halb", "zer\u00b7st\u00f6hr\u00b7te", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und noch bet\u00e4ubt vom schwarzen Schreckens-Tage", "tokens": ["Und", "noch", "be\u00b7t\u00e4ubt", "vom", "schwar\u00b7zen", "Schre\u00b7ckens\u00b7Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wird mein Gesang wie ihre Klage,", "tokens": ["Wird", "mein", "Ge\u00b7sang", "wie", "ih\u00b7re", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mein Gedank wie ihrer matt.", "tokens": ["Und", "mein", "Ge\u00b7dank", "wie", "ih\u00b7rer", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KOKOM", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Beim Ueberrest vom ihrem sch\u00f6nsten Kleide,", "tokens": ["Beim", "Ue\u00b7ber\u00b7rest", "vom", "ih\u00b7rem", "sch\u00f6ns\u00b7ten", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beim Aschenhaufen ihrer Freude", "tokens": ["Beim", "A\u00b7schen\u00b7hau\u00b7fen", "ih\u00b7rer", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sitzt sie, und weint, und ist von Seufzen voll.", "tokens": ["Sitzt", "sie", ",", "und", "weint", ",", "und", "ist", "von", "Seuf\u00b7zen", "voll", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "$,", "KON", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Trost komm herab und trockne ihre Wange,", "tokens": ["Trost", "komm", "her\u00b7ab", "und", "trock\u00b7ne", "ih\u00b7re", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und unterst\u00fctze mich, wenn ich ihr im Gesange", "tokens": ["Und", "un\u00b7ter\u00b7st\u00fct\u00b7ze", "mich", ",", "wenn", "ich", "ihr", "im", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Den Tag des Schreckens schildern soll.", "tokens": ["Den", "Tag", "des", "Schre\u00b7ckens", "schil\u00b7dern", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}