{"dta.poem.18435": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft eines lasterhafften  \n Geistlichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kont ein dietrich seyn zu grosser herren hertze", "tokens": ["Ich", "kont", "ein", "diet\u00b7rich", "seyn", "zu", "gros\u00b7ser", "her\u00b7ren", "hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "VAINF", "PTKZU", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich z\u00fcndte l\u00e4nder an/ mein hochmuth war die kertze.", "tokens": ["Ich", "z\u00fcnd\u00b7te", "l\u00e4n\u00b7der", "an", "/", "mein", "hoch\u00b7muth", "war", "die", "kert\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$(", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der mund verehrte GOtt/ den teuffel hertz und sinn/", "tokens": ["Der", "mund", "ver\u00b7ehr\u00b7te", "Gott", "/", "den", "teuf\u00b7fel", "hertz", "und", "sinn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$(", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun/ leser/ dencke doch/ wer ich gewesen bin?", "tokens": ["Nun", "/", "le\u00b7ser", "/", "den\u00b7cke", "doch", "/", "wer", "ich", "ge\u00b7we\u00b7sen", "bin", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "p