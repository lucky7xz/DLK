{"textgrid.poem.47956": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenandacht", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du tauchst empor aus Tau und Tag,", "tokens": ["Du", "tauchst", "em\u00b7por", "aus", "Tau", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du wandelst \u00fcber Hain und Hag,", "tokens": ["du", "wan\u00b7delst", "\u00fc\u00b7ber", "Hain", "und", "Hag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "du liebe, leuchtende Sonne!", "tokens": ["du", "lie\u00b7be", ",", "leuch\u00b7ten\u00b7de", "Son\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du gibst dem Mai den Bl\u00fctenschein", "tokens": ["Du", "gibst", "dem", "Mai", "den", "Bl\u00fc\u00b7ten\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und schenkst dem Herbst den Feuerwein", "tokens": ["und", "schenkst", "dem", "Herbst", "den", "Feu\u00b7er\u00b7wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und allem Leben Wonne.", "tokens": ["und", "al\u00b7lem", "Le\u00b7ben", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du bist das gro\u00dfe reine Licht,", "tokens": ["Du", "bist", "das", "gro\u00b7\u00dfe", "rei\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das sich in schillernden S\u00fcmpfen bricht", "tokens": ["das", "sich", "in", "schil\u00b7lern\u00b7den", "S\u00fcmp\u00b7fen", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und Lilien lockt aus Tiefen . . . .", "tokens": ["und", "Li\u00b7li\u00b7en", "lockt", "aus", "Tie\u00b7fen", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$.", "$.", "$.", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "du bist die Schleierhebende,", "tokens": ["du", "bist", "die", "Schlei\u00b7er\u00b7he\u00b7ben\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "die Zeugende, Belebende,", "tokens": ["die", "Zeu\u00b7gen\u00b7de", ",", "Be\u00b7le\u00b7ben\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "nach der die Keime riefen.", "tokens": ["nach", "der", "die", "Kei\u00b7me", "rie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gro\u00dfe Mutter, in deinem Licht", "tokens": ["Gro\u00b7\u00dfe", "Mut\u00b7ter", ",", "in", "dei\u00b7nem", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "werde ich fromm; mein Trotz zerbricht,", "tokens": ["wer\u00b7de", "ich", "fromm", ";", "mein", "Trotz", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "meine Lippen lernen das Beten.", "tokens": ["mei\u00b7ne", "Lip\u00b7pen", "ler\u00b7nen", "das", "Be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ich bin bereit, aus Nacht und Schuld", "tokens": ["Ich", "bin", "be\u00b7reit", ",", "aus", "Nacht", "und", "Schuld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "an deinen Tag, in deiner Huld", "tokens": ["an", "dei\u00b7nen", "Tag", ",", "in", "dei\u00b7ner", "Huld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "flammenden Kreis zu treten.", "tokens": ["flam\u00b7men\u00b7den", "Kreis", "zu", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Gro\u00dfe Mutter, mein Fehlen und Irr'n", "tokens": ["Gro\u00b7\u00dfe", "Mut\u00b7ter", ",", "mein", "Feh\u00b7len", "und", "Irr'n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "bekenne ich dir: ich neigte die Stirn", "tokens": ["be\u00b7ken\u00b7ne", "ich", "dir", ":", "ich", "neig\u00b7te", "die", "Stirn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "vor den Schatten auf Erden.", "tokens": ["vor", "den", "Schat\u00b7ten", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ich war so schwach, ich war so klein \u2013", "tokens": ["Ich", "war", "so", "schwach", ",", "ich", "war", "so", "klein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "gro\u00dfe Mutter, ich war nicht dein;", "tokens": ["gro\u00b7\u00dfe", "Mut\u00b7ter", ",", "ich", "war", "nicht", "dein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "PPOSAT", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "aber dein will ich werden!", "tokens": ["a\u00b7ber", "dein", "will", "ich", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VMFIN", "PPER", "VAINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "In einen lauteren Goldpokal", "tokens": ["In", "ei\u00b7nen", "lau\u00b7te\u00b7ren", "Gold\u00b7po\u00b7kal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "fang ich jeden leuchtenden Strahl", "tokens": ["fang", "ich", "je\u00b7den", "leuch\u00b7ten\u00b7den", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "der lebendigen Sonne . . . .", "tokens": ["der", "le\u00b7ben\u00b7di\u00b7gen", "Son\u00b7ne", ".", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$.", "$.", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Meine Stirn wird klar, mein Arm wird stark:", "tokens": ["Mei\u00b7ne", "Stirn", "wird", "klar", ",", "mein", "Arm", "wird", "stark", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "ins Leben gie\u00dft du mir Glut und Mark", "tokens": ["ins", "Le\u00b7ben", "gie\u00dft", "du", "mir", "Glut", "und", "Mark"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "und ins Sterben mir Wonne.", "tokens": ["und", "ins", "Ster\u00b7ben", "mir", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Sterben? \u2013 Mutter, mein Lachen klingt!", "tokens": ["Ster\u00b7ben", "?", "\u2013", "Mut\u00b7ter", ",", "mein", "La\u00b7chen", "klingt", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Auf den Fl\u00fcgeln der Lerche schwingt", "tokens": ["Auf", "den", "Fl\u00fc\u00b7geln", "der", "Ler\u00b7che", "schwingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "es sich hoch in ewige Bl\u00e4uen.", "tokens": ["es", "sich", "hoch", "in", "e\u00b7wi\u00b7ge", "Bl\u00e4u\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sterben mag, was da taub und blind . . . .", "tokens": ["Ster\u00b7ben", "mag", ",", "was", "da", "taub", "und", "blind", ".", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "VMFIN", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD", "$.", "$.", "$.", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "sonnenges\u00e4ttigt wird sich dein Kind", "tokens": ["son\u00b7nen\u00b7ge\u00b7s\u00e4t\u00b7tigt", "wird", "sich", "dein", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PRF", "PPOSAT", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "t\u00f6nenden Lebens freuen.", "tokens": ["t\u00f6\u00b7nen\u00b7den", "Le\u00b7bens", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}