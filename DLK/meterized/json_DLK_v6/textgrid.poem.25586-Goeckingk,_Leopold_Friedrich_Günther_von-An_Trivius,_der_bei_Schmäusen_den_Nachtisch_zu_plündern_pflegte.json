{"textgrid.poem.25586": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "An Trivius, der bei Schm\u00e4usen den Nachtisch zu pl\u00fcndern pflegte", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man auf einen Schmaus Confect zu Tische tr\u00e4gt,", "tokens": ["Wenn", "man", "auf", "ei\u00b7nen", "Schmaus", "Con\u00b7fect", "zu", "Ti\u00b7sche", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00fcnschst du dir noch zwei gro\u00dfe Magen.", "tokens": ["W\u00fcnschst", "du", "dir", "noch", "zwei", "gro\u00b7\u00dfe", "Ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hat's die Natur versehn, so kannst du doch nicht klagen;", "tokens": ["Hat's", "die", "Na\u00b7tur", "ver\u00b7sehn", ",", "so", "kannst", "du", "doch", "nicht", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$,", "ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Schneider hat sie dir im Kleide zugelegt.", "tokens": ["Der", "Schnei\u00b7der", "hat", "sie", "dir", "im", "Klei\u00b7de", "zu\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPER", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}