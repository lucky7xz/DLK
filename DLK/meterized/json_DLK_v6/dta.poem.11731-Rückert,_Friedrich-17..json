{"dta.poem.11731": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alswie der Fr\u00fchling, seit er erst der Welt entflohn,", "tokens": ["Als\u00b7wie", "der", "Fr\u00fch\u00b7ling", ",", "seit", "er", "erst", "der", "Welt", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie wiederkehrt, nur oft ein sch\u00f6nes Bild davon;", "tokens": ["Nie", "wie\u00b7der\u00b7kehrt", ",", "nur", "oft", "ein", "sch\u00f6\u00b7nes", "Bild", "da\u00b7von", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADV", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch ein so sch\u00f6nes Bild, das statt der Sache gn\u00fcgt,", "tokens": ["Doch", "ein", "so", "sch\u00f6\u00b7nes", "Bild", ",", "das", "statt", "der", "Sa\u00b7che", "gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sich, solang sie's hat, die Erde gern betr\u00fcgt:", "tokens": ["Da\u00df", "sich", ",", "so\u00b7lang", "sie's", "hat", ",", "die", "Er\u00b7de", "gern", "be\u00b7tr\u00fcgt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "$,", "KOUS", "PPER", "VAFIN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So kam der Jugend Traum mit zartem Fr\u00fchlingstriebe", "tokens": ["So", "kam", "der", "Ju\u00b7gend", "Traum", "mit", "zar\u00b7tem", "Fr\u00fch\u00b7ling\u00b7strie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Traume mir, ein Traum kam mir vom Traum der Liebe.", "tokens": ["Im", "Trau\u00b7me", "mir", ",", "ein", "Traum", "kam", "mir", "vom", "Traum", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die h\u00f6chste Liebe war's, die ich im Traum empfand,", "tokens": ["Die", "h\u00f6chs\u00b7te", "Lie\u00b7be", "wa\u00b7r's", ",", "die", "ich", "im", "Traum", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und die mich liebte, war ein Weib von h\u00f6chstem Stand.", "tokens": ["Und", "die", "mich", "lieb\u00b7te", ",", "war", "ein", "Weib", "von", "h\u00f6chs\u00b7tem", "Stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}