{"textgrid.poem.48840": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "65. An Panomfen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es geht mir gleich wie dir. Wir haben gleiche Freuden", "tokens": ["Es", "geht", "mir", "gleich", "wie", "dir", ".", "Wir", "ha\u00b7ben", "glei\u00b7che", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOKOM", "PPER", "$.", "PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und gleiche Schmerzen auch. Was uns bei Nacht' ergetzt,", "tokens": ["und", "glei\u00b7che", "Schmer\u00b7zen", "auch", ".", "Was", "uns", "bei", "Nacht'", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$.", "PWS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das eben ists, das aus den m\u00fcden Tag verletzt.", "tokens": ["das", "e\u00b7ben", "ists", ",", "das", "aus", "den", "m\u00fc\u00b7den", "Tag", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Je s\u00fc\u00dfer ist die Lust, je herber ist das Leiden.", "tokens": ["Je", "s\u00fc\u00b7\u00dfer", "ist", "die", "Lust", ",", "je", "her\u00b7ber", "ist", "das", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "da\u00df Keines unter uns ins Andre Treue setzt,", "tokens": ["da\u00df", "Kei\u00b7nes", "un\u00b7ter", "uns", "ins", "And\u00b7re", "Treu\u00b7e", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df Keins dem Andern sich an Liebe gleiche sch\u00e4tzt.", "tokens": ["da\u00df", "Keins", "dem", "An\u00b7dern", "sich", "an", "Lie\u00b7be", "glei\u00b7che", "sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "PRF", "APPR", "NN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum wollen wir auch stets beisammen sein und scheiden.", "tokens": ["Drum", "wol\u00b7len", "wir", "auch", "stets", "bei\u00b7sam\u00b7men", "sein", "und", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "VAINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Lieb, wie kan dieses sein? Was schweiget doch dein Mund?", "tokens": ["Lieb", ",", "wie", "kan", "die\u00b7ses", "sein", "?", "Was", "schwei\u00b7get", "doch", "dein", "Mund", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PDAT", "VAINF", "$.", "PWS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Eins deiner Augen nur verr\u00e4t dein ganzes Herze.", "tokens": ["Eins", "dei\u00b7ner", "Au\u00b7gen", "nur", "ver\u00b7r\u00e4t", "dein", "gan\u00b7zes", "Her\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "VVFIN", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum rate dir und mir und unsrer beider Schmerze.", "tokens": ["Drum", "ra\u00b7te", "dir", "und", "mir", "und", "uns\u00b7rer", "bei\u00b7der", "Schmer\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KON", "PPER", "KON", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was dich macht frisch und krank, macht mich krank und gesund.", "tokens": ["Was", "dich", "macht", "frisch", "und", "krank", ",", "macht", "mich", "krank", "und", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wilt du, als wie ich will, so ist gut Rat der Sachen:", "tokens": ["Wilt", "du", ",", "als", "wie", "ich", "will", ",", "so", "ist", "gut", "Rat", "der", "Sa\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PWAV", "PPER", "VMFIN", "$,", "ADV", "VAFIN", "ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df uns nur wachend tun, was wir im Schlafe machen.", "tokens": ["La\u00df", "uns", "nur", "wa\u00b7chend", "tun", ",", "was", "wir", "im", "Schla\u00b7fe", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVINF", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}