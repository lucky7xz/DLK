{"textgrid.poem.31389": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott / der die ganze Welt aus nichts zu nutz erbauet /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott / der die ganze Welt aus nichts zu nutz erbauet /", "tokens": ["Gott", "/", "der", "die", "gan\u00b7ze", "Welt", "aus", "nichts", "zu", "nutz", "er\u00b7bau\u00b7et", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ART", "ADJA", "NN", "APPR", "PIS", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "erwehlt zum Wunder-Zweck ein selbst bekenntes Nichts.", "tokens": ["er\u00b7wehlt", "zum", "Wun\u00b7der\u00b7Zweck", "ein", "selbst", "be\u00b7kenn\u00b7tes", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in Dunckelheit erscheint die Klarheit eines Liechts /", "tokens": ["in", "Dun\u00b7ckel\u00b7heit", "er\u00b7scheint", "die", "Klar\u00b7heit", "ei\u00b7nes", "Liechts", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nachdem die Allmacht hier ihr was zu w\u00fcrken trauet.", "tokens": ["nach\u00b7dem", "die", "All\u00b7macht", "hier", "ihr", "was", "zu", "w\u00fcr\u00b7ken", "trau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr eigen / dieses Werk / man geist-entzuckt anschauet /", "tokens": ["Ihr", "ei\u00b7gen", "/", "die\u00b7ses", "Werk", "/", "man", "geist\u00b7ent\u00b7zuckt", "an\u00b7schau\u00b7et", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$(", "PDAT", "NN", "$(", "PIS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die sich in Schwachheit \u00fcbt / macht in der Ohnmacht Ichts /", "tokens": ["die", "sich", "in", "Schwach\u00b7heit", "\u00fcbt", "/", "macht", "in", "der", "Ohn\u00b7macht", "Ichts", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPR", "NN", "VVFIN", "$(", "VVFIN", "APPR", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "springt ihr auf Heldisch bey mit H\u00fclf des Angesichts /", "tokens": ["springt", "ihr", "auf", "Hel\u00b7disch", "bey", "mit", "H\u00fclf", "des", "An\u00b7ge\u00b7sichts", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "APPR", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und allverm\u00f6gens Safft auf ihre D\u00fcrre thauet.", "tokens": ["und", "all\u00b7ver\u00b7m\u00f6\u00b7gens", "Safft", "auf", "ih\u00b7re", "D\u00fcr\u00b7re", "thau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Gott / wer sich dir l\u00e4sst / dem l\u00e4sst du deine Krafft.", "tokens": ["Mein", "Gott", "/", "wer", "sich", "dir", "l\u00e4sst", "/", "dem", "l\u00e4sst", "du", "dei\u00b7ne", "Krafft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PWS", "PRF", "PPER", "VVFIN", "$(", "ART", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Erde Feuchtigkeit / wenn sie die Stern' auftrinken /", "tokens": ["Der", "Er\u00b7de", "Feuch\u00b7tig\u00b7keit", "/", "wenn", "sie", "die", "Stern'", "auf\u00b7trin\u00b7ken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "wurd' hier auf ihr ein Koht / dort Gl\u00fckk und Lebens-Safft /", "tokens": ["wurd'", "hier", "auf", "ihr", "ein", "Koht", "/", "dort", "Gl\u00fckk", "und", "Le\u00b7bens\u00b7\u00b7S\u00b7afft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "ART", "NN", "$(", "ADV", "NN", "KON", "NN", "$("], "meter": "-----+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "pflegt in die Edlen theil der Edlesten zu sinken.", "tokens": ["pflegt", "in", "die", "Ed\u00b7len", "theil", "der", "Ed\u00b7les\u00b7ten", "zu", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Gar gern will ich gestehn / da\u00df ich nur Staub und Erd:", "tokens": ["Gar", "gern", "will", "ich", "ge\u00b7stehn", "/", "da\u00df", "ich", "nur", "Staub", "und", "Erd", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVPP", "$(", "KOUS", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "auf da\u00df dein' Herrlichkeit in mir erfunden werd.", "tokens": ["auf", "da\u00df", "dein'", "Herr\u00b7lich\u00b7keit", "in", "mir", "er\u00b7fun\u00b7den", "werd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}