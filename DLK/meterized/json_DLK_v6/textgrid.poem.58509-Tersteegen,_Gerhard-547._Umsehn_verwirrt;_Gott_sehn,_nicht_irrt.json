{"textgrid.poem.58509": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "547. Umsehn verwirrt; Gott sehn, nicht irrt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der eine lobt und sch\u00e4tzt mich fromm und \u00fcberfromm,", "tokens": ["Der", "ei\u00b7ne", "lobt", "und", "sch\u00e4tzt", "mich", "fromm", "und", "\u00fc\u00b7ber\u00b7fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein andrer schilt und scheut mich und mein Christentum;", "tokens": ["Ein", "an\u00b7drer", "schilt", "und", "scheut", "mich", "und", "mein", "Chris\u00b7ten\u00b7tum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "KON", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Leitstern ist mein Gott, der f\u00fchrt gerad und fr\u00f6hlich,", "tokens": ["Mein", "Leits\u00b7tern", "ist", "mein", "Gott", ",", "der", "f\u00fchrt", "ge\u00b7rad", "und", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer ihn sieht, sieht nicht sich, wird fromm, verkl\u00e4rt und selig.", "tokens": ["Wer", "ihn", "sieht", ",", "sieht", "nicht", "sich", ",", "wird", "fromm", ",", "ver\u00b7kl\u00e4rt", "und", "se\u00b7lig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PTKNEG", "PRF", "$,", "VAFIN", "ADJD", "$,", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}