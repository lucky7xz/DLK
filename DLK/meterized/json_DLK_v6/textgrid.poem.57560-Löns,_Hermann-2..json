{"textgrid.poem.57560": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldhelle Sonne und taufrische Luft", "tokens": ["Gold\u00b7hel\u00b7le", "Son\u00b7ne", "und", "tauf\u00b7ri\u00b7sche", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Und Lieder und Bl\u00fctenprangen,", "tokens": ["Und", "Lie\u00b7der", "und", "Bl\u00fc\u00b7ten\u00b7pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Des Buchweizens und der Lupine Duft", "tokens": ["Des", "Buch\u00b7wei\u00b7zens", "und", "der", "Lu\u00b7pi\u00b7ne", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Umschmeichelt mir \u00fcppig die Wangen.", "tokens": ["Um\u00b7schmei\u00b7chelt", "mir", "\u00fcp\u00b7pig", "die", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Die Welt so sch\u00f6n und der Arm so leer,", "tokens": ["Die", "Welt", "so", "sch\u00f6n", "und", "der", "Arm", "so", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und das Herz, es hungert nach Liebe \u2013", "tokens": ["Und", "das", "Herz", ",", "es", "hun\u00b7gert", "nach", "Lie\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du qualmiger Wind, jetzt fege daher", "tokens": ["Du", "qual\u00b7mi\u00b7ger", "Wind", ",", "jetzt", "fe\u00b7ge", "da\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "VVFIN", "PAV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit deinem Todesgestiebe!", "tokens": ["Mit", "dei\u00b7nem", "To\u00b7des\u00b7ge\u00b7stie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "O blutrote Sonne und eisiger Rauch", "tokens": ["O", "blut\u00b7ro\u00b7te", "Son\u00b7ne", "und", "ei\u00b7si\u00b7ger", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und keuchend-engbr\u00fcstiges Sausen,", "tokens": ["Und", "keu\u00b7chen\u00b7deng\u00b7br\u00fcs\u00b7ti\u00b7ges", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Jetzt fauche, du d\u00f6rrender Todeshauch,", "tokens": ["Jetzt", "fau\u00b7che", ",", "du", "d\u00f6r\u00b7ren\u00b7der", "To\u00b7des\u00b7hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich lechze nach Schauder und Grausen.", "tokens": ["Ich", "lech\u00b7ze", "nach", "Schau\u00b7der", "und", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Ich lechze nach Sturmwind und Wetterschlag,", "tokens": ["Ich", "lech\u00b7ze", "nach", "Sturm\u00b7wind", "und", "Wet\u00b7ter\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da\u00df Leib und Seele erzittert \u2013", "tokens": ["Da\u00df", "Leib", "und", "See\u00b7le", "er\u00b7zit\u00b7tert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wollte, es w\u00e4re der j\u00fcngste Tag", "tokens": ["Ich", "woll\u00b7te", ",", "es", "w\u00e4\u00b7re", "der", "j\u00fcngs\u00b7te", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und die alte Erde zersplittert ...", "tokens": ["Und", "die", "al\u00b7te", "Er\u00b7de", "zer\u00b7split\u00b7tert", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}