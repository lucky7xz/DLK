{"textgrid.poem.48010": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Der goldene Schl\u00fcssel", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, \u2013", "tokens": ["Dir", ",", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["PPER", "$,", "$("], "meter": "+", "measure": "single.up"}, "line.2": {"text": "dem goldenen Schl\u00fcssel", "tokens": ["dem", "gol\u00b7de\u00b7nen", "Schl\u00fcs\u00b7sel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "zum sonnigen Lande der Freiheit,", "tokens": ["zum", "son\u00b7ni\u00b7gen", "Lan\u00b7de", "der", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "dir sing ich.", "tokens": ["dir", "sing", "ich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Irgendwo, irgendwo in der Welt,", "tokens": ["Ir\u00b7gend\u00b7wo", ",", "ir\u00b7gend\u00b7wo", "in", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u2013 in Orangenw\u00e4ldern vielleicht,", "tokens": ["\u2013", "in", "O\u00b7ran\u00b7gen\u00b7w\u00e4l\u00b7dern", "viel\u00b7leicht", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "wo der Glutwind die Zweige bricht", "tokens": ["wo", "der", "Glut\u00b7wind", "die", "Zwei\u00b7ge", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "und sie reifer, saftstrotzender Fr\u00fcchte voll", "tokens": ["und", "sie", "rei\u00b7fer", ",", "saft\u00b7strot\u00b7zen\u00b7der", "Fr\u00fcch\u00b7te", "voll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "$,", "ADJA", "NN", "ADJD"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "dem Wanderer in den Scho\u00df wirft, \u2013", "tokens": ["dem", "Wan\u00b7de\u00b7rer", "in", "den", "Scho\u00df", "wirft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "oder an Norwegs Felsenkap,", "tokens": ["o\u00b7der", "an", "Nor\u00b7wegs", "Fel\u00b7sen\u00b7kap", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "das die k\u00fchle Stirn", "tokens": ["das", "die", "k\u00fch\u00b7le", "Stirn"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "hoch in schimmernde Wolken hebt", "tokens": ["hoch", "in", "schim\u00b7mern\u00b7de", "Wol\u00b7ken", "hebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "und niederschauend sich spiegelt", "tokens": ["und", "nie\u00b7der\u00b7schau\u00b7end", "sich", "spie\u00b7gelt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "PRF", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "in den tr\u00e4umerisch blauen Augen des Fjords \u2013", "tokens": ["in", "den", "tr\u00e4u\u00b7me\u00b7risch", "blau\u00b7en", "Au\u00b7gen", "des", "Fjords", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "ART", "NN", "$("], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "irgendwo in der Welt", "tokens": ["ir\u00b7gend\u00b7wo", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "weilt die Fee,", "tokens": ["weilt", "die", "Fee", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "die dich mir versprochen", "tokens": ["die", "dich", "mir", "ver\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "ihr Wort mir zu l\u00f6sen.", "tokens": ["ihr", "Wort", "mir", "zu", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "und nun der Zeit nicht gedenkt,", "tokens": ["und", "nun", "der", "Zeit", "nicht", "ge\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "in heiliger Stunde", "tokens": ["in", "hei\u00b7li\u00b7ger", "Stun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Jahre verrauschen,", "tokens": ["Jah\u00b7re", "ver\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "auf meinen Scheitel f\u00e4llt Schnee.", "tokens": ["auf", "mei\u00b7nen", "Schei\u00b7tel", "f\u00e4llt", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In den Tiefen der Seele", "tokens": ["In", "den", "Tie\u00b7fen", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "aber wirkt und schafft", "tokens": ["a\u00b7ber", "wirkt", "und", "schafft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "befruchtete Fr\u00fchlingskraft", "tokens": ["be\u00b7fruch\u00b7te\u00b7te", "Fr\u00fch\u00b7lings\u00b7kraft"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "und keimt und gebiert an das Licht", "tokens": ["und", "keimt", "und", "ge\u00b7biert", "an", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "der Gewi\u00dfheit leuchtende Blume:", "tokens": ["der", "Ge\u00b7wi\u00df\u00b7heit", "leuch\u00b7ten\u00b7de", "Blu\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "ein Tag ", "tokens": ["ein", "Tag"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "und eine Stunde bl\u00fchen", "tokens": ["und", "ei\u00b7ne", "Stun\u00b7de", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "aus dem D\u00e4mmerdunkel des Alltagsdaseins,", "tokens": ["aus", "dem", "D\u00e4m\u00b7mer\u00b7dun\u00b7kel", "des", "All\u00b7tags\u00b7da\u00b7seins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "so wonnig und w\u00e4rmend", "tokens": ["so", "won\u00b7nig", "und", "w\u00e4r\u00b7mend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "von Gebeten begr\u00fc\u00dft,", "tokens": ["von", "Ge\u00b7be\u00b7ten", "be\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "wie die Siegerin Sonne", "tokens": ["wie", "die", "Sie\u00b7ge\u00b7rin", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "der eisigen Oede", "tokens": ["der", "ei\u00b7si\u00b7gen", "O\u00b7e\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "den Schauern der arktischen Nacht enttaucht.", "tokens": ["den", "Schau\u00b7ern", "der", "ark\u00b7ti\u00b7schen", "Nacht", "ent\u00b7taucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Und leise, leise,", "tokens": ["Und", "lei\u00b7se", ",", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "lockend wie Harfenlaut", "tokens": ["lo\u00b7ckend", "wie", "Har\u00b7fen\u00b7laut"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KOKOM", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "klingt es und klirrt es", "tokens": ["klingt", "es", "und", "klirrt", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "vor der T\u00fcr meiner H\u00fctte", "tokens": ["vor", "der", "T\u00fcr", "mei\u00b7ner", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "und pocht und pocht.", "tokens": ["und", "pocht", "und", "pocht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ich erkenne den Laut", "tokens": ["Ich", "er\u00b7ken\u00b7ne", "den", "Laut"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "und erhebe mein Haupt", "tokens": ["und", "er\u00b7he\u00b7be", "mein", "Haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "und l\u00e4chle und lausche . . . . .", "tokens": ["und", "l\u00e4ch\u00b7le", "und", "lau\u00b7sche", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "KON", "ADJA", "$.", "$.", "$.", "$.", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Da knarren und knirschen", "tokens": ["Da", "knar\u00b7ren", "und", "knir\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "die rostigen Riegel:", "tokens": ["die", "ros\u00b7ti\u00b7gen", "Rie\u00b7gel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "die T\u00fcr springt auf.", "tokens": ["die", "T\u00fcr", "springt", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ueber die Schwelle str\u00f6mt", "tokens": ["Ue\u00b7ber", "die", "Schwel\u00b7le", "str\u00f6mt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "eine flimmernde Flut von Sonnensilber \u2013", "tokens": ["ei\u00b7ne", "flim\u00b7mern\u00b7de", "Flut", "von", "Son\u00b7nen\u00b7sil\u00b7ber", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "und mitten drin in dem Sonnenlichtmeer", "tokens": ["und", "mit\u00b7ten", "drin", "in", "dem", "Son\u00b7nen\u00b7licht\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "die Fee,", "tokens": ["die", "Fee", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "die dich mir versprochen,", "tokens": ["die", "dich", "mir", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "den goldenen Schl\u00fcssel zum Lande der Freiheit,", "tokens": ["den", "gol\u00b7de\u00b7nen", "Schl\u00fcs\u00b7sel", "zum", "Lan\u00b7de", "der", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "und die nun gekommen ist,", "tokens": ["und", "die", "nun", "ge\u00b7kom\u00b7men", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "ihr Wort zu l\u00f6sen.", "tokens": ["ihr", "Wort", "zu", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Liebevoll l\u00e4chelnd", "tokens": ["Lie\u00b7be\u00b7voll", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "schreitet die Lichtmar", "tokens": ["schrei\u00b7tet", "die", "Licht\u00b7mar"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "durch das Dunkel der H\u00fctte.", "tokens": ["durch", "das", "Dun\u00b7kel", "der", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Um sie her", "tokens": ["Um", "sie", "her"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PPER", "APZR"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "wallen und weben", "tokens": ["wal\u00b7len", "und", "we\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVINF", "KON", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "glei\u00dfend und glimmernd", "tokens": ["glei\u00b7\u00dfend", "und", "glim\u00b7mernd"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "die goldenen F\u00e4den", "tokens": ["die", "gol\u00b7de\u00b7nen", "F\u00e4\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "und legen ein Lichtband", "tokens": ["und", "le\u00b7gen", "ein", "Licht\u00b7band"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "\u00fcber die lastende Staubschicht am Boden,", "tokens": ["\u00fc\u00b7ber", "die", "las\u00b7ten\u00b7de", "Staub\u00b7schicht", "am", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "\u00fcber die dr\u00fcckenden Ketten am Arm mir,", "tokens": ["\u00fc\u00b7ber", "die", "dr\u00fc\u00b7cken\u00b7den", "Ket\u00b7ten", "am", "Arm", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "PPER", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "\u00fcber den klappernden Webstuhl,", "tokens": ["\u00fc\u00b7ber", "den", "klap\u00b7pern\u00b7den", "Web\u00b7stuhl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["AP