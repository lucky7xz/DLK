{"textgrid.poem.44897": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Diplomat", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein umgekehrter Talleyrand,", "tokens": ["Ein", "um\u00b7ge\u00b7kehr\u00b7ter", "Tal\u00b7ley\u00b7rand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Obwohl sonst gern sein Affe,", "tokens": ["Ob\u00b7wohl", "sonst", "gern", "sein", "Af\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00e4ngt er mit dem Minister an", "tokens": ["F\u00e4ngt", "er", "mit", "dem", "Mi\u00b7nis\u00b7ter", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "APPR"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und endiget als Pfaffe.", "tokens": ["Und", "en\u00b7di\u00b7get", "als", "Pfaf\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}