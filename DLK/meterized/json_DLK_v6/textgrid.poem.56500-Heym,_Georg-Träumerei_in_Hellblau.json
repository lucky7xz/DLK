{"textgrid.poem.56500": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00e4umerei in Hellblau", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle [Landschaften] haben", "tokens": ["Al\u00b7le", "Land\u00b7schaf\u00b7ten", "ha\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["PIAT", "$(", "NN", "$(", "VAFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Sich mit Blau gef\u00fcllt.", "tokens": ["Sich", "mit", "Blau", "ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alle B\u00fcsche und B\u00e4ume des Stromes,", "tokens": ["Al\u00b7le", "B\u00fc\u00b7sche", "und", "B\u00e4u\u00b7me", "des", "Stro\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Der weit in den Norden schwillt.", "tokens": ["Der", "weit", "in", "den", "Nor\u00b7den", "schwillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Blaue L\u00e4nder der Wolken,", "tokens": ["Blau\u00b7e", "L\u00e4n\u00b7der", "der", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wei\u00dfe Segel dicht,", "tokens": ["Wei\u00b7\u00dfe", "Se\u00b7gel", "dicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die Gestade des Himmels in Fernen", "tokens": ["Die", "Ge\u00b7sta\u00b7de", "des", "Him\u00b7mels", "in", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Zergehen in Wind und Licht.", "tokens": ["Zer\u00b7ge\u00b7hen", "in", "Wind", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn die Abende sinken", "tokens": ["Wenn", "die", "A\u00b7ben\u00b7de", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und wir schlafen ein,", "tokens": ["Und", "wir", "schla\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gehen die Tr\u00e4ume, die sch\u00f6nen,", "tokens": ["Ge\u00b7hen", "die", "Tr\u00e4u\u00b7me", ",", "die", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mit leichten F\u00fc\u00dfen herein.", "tokens": ["Mit", "leich\u00b7ten", "F\u00fc\u00b7\u00dfen", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Zymbeln lassen sie klingen", "tokens": ["Zym\u00b7beln", "las\u00b7sen", "sie", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "In den H\u00e4nden licht.", "tokens": ["In", "den", "H\u00e4n\u00b7den", "licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Manche fl\u00fcstern, und halten", "tokens": ["Man\u00b7che", "fl\u00fcs\u00b7tern", ",", "und", "hal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "VVINF", "$,", "KON", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Kerzen vor ihr Gesicht.", "tokens": ["Ker\u00b7zen", "vor", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}}}}