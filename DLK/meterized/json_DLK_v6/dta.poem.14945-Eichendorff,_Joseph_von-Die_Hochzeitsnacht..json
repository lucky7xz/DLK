{"dta.poem.14945": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeitsnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sie sprach: \u201eSchon seh' ich Streifen", "tokens": ["Sie", "sprach", ":", "\u201e", "Schon", "seh'", "ich", "Strei\u00b7fen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So roth im Morgen steh'n,", "tokens": ["So", "roth", "im", "Mor\u00b7gen", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Stimmen h\u00f6r' ich schweifen,", "tokens": ["Und", "Stim\u00b7men", "h\u00f6r'", "ich", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Ufer H\u00e4hne kr\u00e4h'n.", "tokens": ["Vom", "U\u00b7fer", "H\u00e4h\u00b7ne", "kr\u00e4h'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPRART", "NN", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}