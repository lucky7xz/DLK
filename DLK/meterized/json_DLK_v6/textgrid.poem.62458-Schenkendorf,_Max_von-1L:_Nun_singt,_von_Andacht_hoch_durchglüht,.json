info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So soll uns ewig hei\u00dfen roth", "tokens": ["So", "soll", "uns", "e\u00b7wig", "hei\u00b7\u00dfen", "roth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Boden, theures Land,", "tokens": ["Dein", "Bo\u00b7den", ",", "theu\u00b7res", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo man mit Schwert und Henkerstod", "tokens": ["Wo", "man", "mit", "Schwert", "und", "Hen\u00b7ker\u00b7stod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den welschen Feind verbannt.", "tokens": ["Den", "wel\u00b7schen", "Feind", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und wie im alten Testament", "tokens": ["Und", "wie", "im", "al\u00b7ten", "Tes\u00b7ta\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der fromme Pineas", "tokens": ["Der", "from\u00b7me", "Pi\u00b7neas"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Entbrennt in Eifer und in Ha\u00df,", "tokens": ["Ent\u00b7brennt", "in", "Ei\u00b7fer", "und", "in", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR"