{"textgrid.poem.62458": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun singt, von Andacht hoch durchgl\u00fcht,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun singt, von Andacht hoch durchgl\u00fcht,", "tokens": ["Nun", "singt", ",", "von", "An\u00b7dacht", "hoch", "durch\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freiheit Lobgesang!", "tokens": ["Der", "Frei\u00b7heit", "Lob\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Himmel und auf Erden klang", "tokens": ["Im", "Him\u00b7mel", "und", "auf", "Er\u00b7den", "klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch nie ein sch\u00f6nres Lied.", "tokens": ["Noch", "nie", "ein", "sch\u00f6n\u00b7res", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn Freiheit war das Meisterwort,", "tokens": ["Denn", "Frei\u00b7heit", "war", "das", "Meis\u00b7ter\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Gott die Geister schuf;", "tokens": ["Als", "Gott", "die", "Geis\u00b7ter", "schuf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O Freiheit unser Stern und Hort,", "tokens": ["O", "Frei\u00b7heit", "un\u00b7ser", "Stern", "und", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir h\u00f6rten auch den Ruf.", "tokens": ["Wir", "h\u00f6r\u00b7ten", "auch", "den", "Ruf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da brach hervor zu Gotteslust", "tokens": ["Da", "brach", "her\u00b7vor", "zu", "Got\u00b7tes\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was lang im Finstern schlief,", "tokens": ["Was", "lang", "im", "Fins\u00b7tern", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Keim der Freiheit, welcher tief", "tokens": ["Der", "Keim", "der", "Frei\u00b7heit", ",", "wel\u00b7cher", "tief"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entspro\u00df in Menschenbrust.", "tokens": ["Ent\u00b7spro\u00df", "in", "Men\u00b7schen\u00b7brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In tausend Aesten brach es aus,", "tokens": ["In", "tau\u00b7send", "A\u00b7es\u00b7ten", "brach", "es", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das junge zarte Reis,", "tokens": ["Das", "jun\u00b7ge", "zar\u00b7te", "Reis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein reicher voller Bl\u00fctenstrau\u00df", "tokens": ["Ein", "rei\u00b7cher", "vol\u00b7ler", "Bl\u00fc\u00b7ten\u00b7strau\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu Gottes Ehr' und Preis.", "tokens": ["Zu", "Got\u00b7tes", "Ehr'", "und", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sei hochgepriesen, goldne Zeit,", "tokens": ["Sei", "hoch\u00b7ge\u00b7prie\u00b7sen", ",", "gold\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O freie Hirtenwelt!", "tokens": ["O", "frei\u00b7e", "Hir\u00b7ten\u00b7welt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der strengen M\u00e4nner Busen schwellt", "tokens": ["Der", "stren\u00b7gen", "M\u00e4n\u00b7ner", "Bu\u00b7sen", "schwellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dir ein stiller Neid.", "tokens": ["Nach", "dir", "ein", "stil\u00b7ler", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch Sch\u00f6neres wird nie gesehn,", "tokens": ["Doch", "Sch\u00f6\u00b7ne\u00b7res", "wird", "nie", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wenn ein holdes Kind", "tokens": ["Als", "wenn", "ein", "hol\u00b7des", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Emporschaut nach den ew'gen H\u00f6hn,", "tokens": ["Em\u00b7por\u00b7schaut", "nach", "den", "ew'\u00b7gen", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sich der Mensch besinnt.", "tokens": ["Wenn", "sich", "der", "Mensch", "be\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er f\u00fchlt sich Meister jedes Dings", "tokens": ["Er", "f\u00fchlt", "sich", "Meis\u00b7ter", "je\u00b7des", "Dings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kennet sein Geschlecht,", "tokens": ["Und", "ken\u00b7net", "sein", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er bildet sich ein heilig Recht", "tokens": ["Er", "bil\u00b7det", "sich", "ein", "hei\u00b7lig", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und blicket rechts und links.", "tokens": ["Und", "bli\u00b7cket", "rechts", "und", "links", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was ihn als Ahnung fern umschwebt,", "tokens": ["Was", "ihn", "als", "Ah\u00b7nung", "fern", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was schaute die Vernunft,", "tokens": ["Was", "schau\u00b7te", "die", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Sch\u00f6pfertrieb, der in ihm lebt,", "tokens": ["Der", "Sch\u00f6p\u00b7fer\u00b7trieb", ",", "der", "in", "ihm", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Stellt's dar in Haus und Zunft.", "tokens": ["Stellt's", "dar", "in", "Haus", "und", "Zunft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So schaut man im Gesetz verkl\u00e4rt", "tokens": ["So", "schaut", "man", "im", "Ge\u00b7setz", "ver\u00b7kl\u00e4rt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vollkommner Freiheit Bild;", "tokens": ["Voll\u00b7komm\u00b7ner", "Frei\u00b7heit", "Bild", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat auch unser Herz erf\u00fcllt,", "tokens": ["Sie", "hat", "auch", "un\u00b7ser", "Herz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kleinod hoch und werth.", "tokens": ["Ein", "Klei\u00b7nod", "hoch", "und", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum achten wir im blut'gen Bann", "tokens": ["Drum", "ach\u00b7ten", "wir", "im", "blut'\u00b7gen", "Bann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das fluchbeladne Haupt,", "tokens": ["Das", "fluch\u00b7be\u00b7lad\u00b7ne", "Haupt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Feind, den R\u00e4uber, den Tyrann,", "tokens": ["Den", "Feind", ",", "den", "R\u00e4u\u00b7ber", ",", "den", "Ty\u00b7rann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der uns die Freiheit raubt.", "tokens": ["Der", "uns", "die", "Frei\u00b7heit", "raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die alte Kunde schwebt herab", "tokens": ["Die", "al\u00b7te", "Kun\u00b7de", "schwebt", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In wunderbarem Glanz;", "tokens": ["In", "wun\u00b7der\u00b7ba\u00b7rem", "Glanz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir opfern diesen Eichenkranz", "tokens": ["Wir", "op\u00b7fern", "die\u00b7sen", "Ei\u00b7chen\u00b7kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir, o Spartanergrab;", "tokens": ["Dir", ",", "o", "Spar\u00b7ta\u00b7ner\u00b7grab", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wir kennen deinen sch\u00f6nen Brauch,", "tokens": ["Wir", "ken\u00b7nen", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geliebtes Griechenland,", "tokens": ["Ge\u00b7lieb\u00b7tes", "Grie\u00b7chen\u00b7land", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und halten unter Myrthen auch", "tokens": ["Und", "hal\u00b7ten", "un\u00b7ter", "Myr\u00b7then", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Am blanken Stahl die Hand.", "tokens": ["Am", "blan\u00b7ken", "Stahl", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Name Brutus, der geweiht", "tokens": ["Der", "Na\u00b7me", "Bru\u00b7tus", ",", "der", "ge\u00b7weiht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Roma's Schicksal klingt,", "tokens": ["In", "Ro\u00b7ma's", "Schick\u00b7sal", "klingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit dem die Freiheit kommt und sinkt,", "tokens": ["Mit", "dem", "die", "Frei\u00b7heit", "kommt", "und", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist uns ein theurer Eid.", "tokens": ["Ist", "uns", "ein", "theu\u00b7rer", "Eid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Tyrannenm\u00f6rder, seid gegr\u00fc\u00dft,", "tokens": ["Ty\u00b7ran\u00b7nen\u00b7m\u00f6r\u00b7der", ",", "seid", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Priester f\u00fcr das Recht,", "tokens": ["Ihr", "Pries\u00b7ter", "f\u00fcr", "das", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr Helden, eurem Staub entsprie\u00dft", "tokens": ["Ihr", "Hel\u00b7den", ",", "eu\u00b7rem", "Staub", "ent\u00b7sprie\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein herrliches Geschlecht.", "tokens": ["Ein", "herr\u00b7li\u00b7ches", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir kennen auch den wackern Tell", "tokens": ["Wir", "ken\u00b7nen", "auch", "den", "wa\u00b7ckern", "Tell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seines Bogens Kraft,", "tokens": ["Und", "sei\u00b7nes", "Bo\u00b7gens", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir preisen seine Wissenschaft", "tokens": ["Wir", "prei\u00b7sen", "sei\u00b7ne", "Wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor jedem Waidgesell:", "tokens": ["Vor", "je\u00b7dem", "Waid\u00b7ge\u00b7sell", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Freiwillig hat die J\u00e4gerschaar", "tokens": ["Frei\u00b7wil\u00b7lig", "hat", "die", "J\u00e4\u00b7ger\u00b7schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Preu\u00dfen in der Schlacht", "tokens": ["Der", "Preu\u00b7\u00d