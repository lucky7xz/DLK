"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mag der geweihte Becher auch", "tokens": ["Mag", "der", "ge\u00b7weih\u00b7te", "Be\u00b7cher", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch unsre Reihen gehn.", "tokens": ["Durch", "uns\u00b7re", "Rei\u00b7hen", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das ist ein vaterl\u00e4nd'scher Wein,", "tokens": ["Das", "ist", "ein", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7scher", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und Jeder, welcher trinkt,", "tokens": ["Und", "Je\u00b7der", ",", "wel\u00b7cher", "trinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schw\u00f6rt, seiner V\u00e4ter werth zu sein,", "tokens": ["Schw\u00f6rt", ",", "sei\u00b7ner", "V\u00e4\u00b7ter", "werth", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis er voll Wunden sinkt!", "tokens": ["Bis", "er", "voll", "Wun\u00b7den", "sinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}