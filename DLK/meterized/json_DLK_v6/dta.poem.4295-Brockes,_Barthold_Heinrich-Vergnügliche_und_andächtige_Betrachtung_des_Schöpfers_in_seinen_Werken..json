{"dta.poem.4295": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgliche und and\u00e4chtige  \n Betrachtung des Sch\u00f6pfers  \n in seinen Werken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr entlegnen holden B\u00fcsche, leiht mir eure stille Schat-", "tokens": ["Ihr", "ent\u00b7leg\u00b7nen", "hol\u00b7den", "B\u00fc\u00b7sche", ",", "leiht", "mir", "eu\u00b7re", "stil\u00b7le", "Schat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "ten,", "tokens": ["ten", ","], "token_info": ["word", "punct"], "pos": ["FM", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Um in ihnen meine Pflicht unserm Sch\u00f6pfer abzu-", "tokens": ["Um", "in", "ih\u00b7nen", "mei\u00b7ne", "Pflicht", "un\u00b7serm", "Sch\u00f6p\u00b7fer", "ab\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "PPER", "PPOSAT", "NN", "PPOSAT", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "statten.", "tokens": ["stat\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Gar zu lang hat Stadt und Hof, der Affecten Sitz und", "tokens": ["Gar", "zu", "lang", "hat", "Stadt", "und", "Hof", ",", "der", "Af\u00b7fec\u00b7ten", "Sitz", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKA", "ADJD", "VAFIN", "NN", "KON", "NN", "$,", "ART", "NN", "NN", "KON"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Heerd,", "tokens": ["Heerd", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Meiner Sinnen Kraft verwirrt, die Betrachtungen", "tokens": ["Mei\u00b7ner", "Sin\u00b7nen", "Kraft", "ver\u00b7wirrt", ",", "die", "Be\u00b7trach\u00b7tun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "ADJD", "$,", "ART", "NN"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "gest\u00f6rt.", "tokens": ["ge\u00b7st\u00f6rt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Jhr, ihr W\u00e4lder, unterhaltet, zeugt und bringet sie", "tokens": ["Ihr", ",", "ihr", "W\u00e4l\u00b7der", ",", "un\u00b7ter\u00b7hal\u00b7tet", ",", "zeugt", "und", "brin\u00b7get", "sie"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "herf\u00fcr!", "tokens": ["her\u00b7f\u00fcr", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Ehret denn gemeinschaftlich, preiset unsern HErrn mit", "tokens": ["Eh\u00b7ret", "denn", "ge\u00b7mein\u00b7schaft\u00b7lich", ",", "prei\u00b7set", "un\u00b7sern", "Herrn", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "mir.", "tokens": ["mir", "."], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Er bezeugt sich mir in allen: In der Luft erblick ich", "tokens": ["Er", "be\u00b7zeugt", "sich", "mir", "in", "al\u00b7len", ":", "In", "der", "Luft", "er\u00b7blick", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPER", "APPR", "PIAT", "$.", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Jhn;", "tokens": ["Jhn", ";"], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Jhn zeigt uns des weiten Meers und der Erden sch\u00f6nes", "tokens": ["Jhn", "zeigt", "uns", "des", "wei\u00b7ten", "Meers", "und", "der", "Er\u00b7den", "sch\u00f6\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "ART", "NN", "ADJA"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gr\u00fcn.", "tokens": ["Gr\u00fcn", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ja Er gl\u00e4nzt nicht weniger in des Himmels blauen", "tokens": ["Ja", "Er", "gl\u00e4nzt", "nicht", "we\u00b7ni\u00b7ger", "in", "des", "Him\u00b7mels", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Schein.", "tokens": ["Schein", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Dem bewundernden Gesicht zeigt im Glanz, der allge-", "tokens": ["Dem", "be\u00b7wun\u00b7dern\u00b7den", "Ge\u00b7sicht", "zeigt", "im", "Glanz", ",", "der", "all\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,", "ART", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "mein,", "tokens": ["mein", ","], "token_info": ["word", "punct"], "pos": ["PPOSAT", "$,"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Die uns r\u00fchrende Natur ohne Flecken sich und rein.", "tokens": ["Die", "uns", "r\u00fch\u00b7ren\u00b7de", "Na\u00b7tur", "oh\u00b7ne", "Fle\u00b7cken", "sich", "und", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "APPR", "NN", "PRF", "KON", "ADJD", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Die grosse Gottheit triumphirt, mit sel\u2019ger Herrlichkeit", "tokens": ["Die", "gros\u00b7se", "Got\u00b7theit", "tri\u00b7um\u00b7phirt", ",", "mit", "sel'\u00b7ger", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "bekr\u00e4nzt,", "tokens": ["be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "In jedem Ort, in allem Raum, ohn\u2019 End\u2019 und Schranken,", "tokens": ["In", "je\u00b7dem", "Ort", ",", "in", "al\u00b7lem", "Raum", ",", "ohn'", "End'", "und", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIS", "NN", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u00fcberall.", "tokens": ["\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Denn es wird sein unendlichs Wesen, es wird sein unerme\u00df-", "tokens": ["Denn", "es", "wird", "sein", "un\u00b7end\u00b7lichs", "We\u00b7sen", ",", "es", "wird", "sein", "un\u00b7er\u00b7me\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "TRUNC"], "meter": "--+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "lichs All,", "tokens": ["lichs", "All", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Von aller Himmel Himmel H\u00f6h\u2019 nicht eingeschr\u00e4nket, nicht", "tokens": ["Von", "al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel", "H\u00f6h'", "nicht", "ein\u00b7ge\u00b7schr\u00e4n\u00b7ket", ",", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "NN", "PTKNEG", "VVFIN", "$,", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "begrenzt.", "tokens": ["be\u00b7grenzt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Vergib mir denn, o HErr! mein k\u00fchn- und lehrbegierges", "tokens": ["Ver\u00b7gib", "mir", "denn", ",", "o", "Herr", "!", "mein", "k\u00fchn", "und", "lehr\u00b7be\u00b7gier\u00b7ges"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "FM", "NN", "$.", "PPOSAT", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Unterfangen,", "tokens": ["Un\u00b7ter\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "We\u00f1 ich bem\u00fchet bin zu Dir, durch Deine Werke, zu gelangen.", "tokens": ["We\u00f1", "ich", "be\u00b7m\u00fc\u00b7het", "bin", "zu", "Dir", ",", "durch", "Dei\u00b7ne", "Wer\u00b7ke", ",", "zu", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Es sehen mich die B\u00fcsch\u2019 und W\u00e4lder in ihnen, \u00f6fters", "tokens": ["Es", "se\u00b7hen", "mich", "die", "B\u00fcsch'", "und", "W\u00e4l\u00b7der", "in", "ih\u00b7nen", ",", "\u00f6f\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "APPR", "PPER", "$,", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "wechselsweise,", "tokens": ["wech\u00b7sels\u00b7wei\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Von Deiner Liebe mich besprechen, und auch von Deines", "tokens": ["Von", "Dei\u00b7ner", "Lie\u00b7be", "mich", "be\u00b7spre\u00b7chen", ",", "und", "auch", "von", "Dei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,", "KON", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Namens Preise.", "tokens": ["Na\u00b7mens", "Prei\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ich komme darum nicht so oft in diesen Bl\u00e4tter-reichen", "tokens": ["Ich", "kom\u00b7me", "da\u00b7rum", "nicht", "so", "oft", "in", "die\u00b7sen", "Bl\u00e4t\u00b7ter\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "PTKNEG", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "B\u00fcschen,", "tokens": ["B\u00fc\u00b7schen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Der V\u00f6gel s\u00fcssen Schall zu h\u00f6ren, ihr Gurgeln und ihr", "tokens": ["Der", "V\u00f6\u00b7gel", "s\u00fcs\u00b7sen", "Schall", "zu", "h\u00f6\u00b7ren", ",", "ihr", "Gur\u00b7geln", "und", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "KON", "PPOSAT"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "klingend Zischen;", "tokens": ["klin\u00b7gend", "Zi\u00b7schen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ich komme darum hier, o GOtt! damit von mir sey unge-", "tokens": ["Ich", "kom\u00b7me", "da\u00b7rum", "hier", ",", "o", "Gott", "!", "da\u00b7mit", "von", "mir", "sey", "un\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "$,", "FM", "NN", "$.", "PAV", "APPR", "PPER", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "st\u00f6rt,", "tokens": ["st\u00f6rt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "In ihrer stillen Einsamkeit, dein all-erhaltend Wort geh\u00f6rt,", "tokens": ["In", "ih\u00b7rer", "stil\u00b7len", "Ein\u00b7sam\u00b7keit", ",", "dein", "all\u00b7er\u00b7hal\u00b7tend", "Wort", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.12": {"text": "Das die gesch\u00e4ftigte Natur Gesetz und Ordnung halten lehrt.", "tokens": ["Das", "die", "ge\u00b7sch\u00e4f\u00b7tig\u00b7te", "Na\u00b7tur", "Ge\u00b7setz", "und", "Ord\u00b7nung", "hal\u00b7ten", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "NN", "KON", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.13": {"text": "Ich h\u00f6r\u2019 in dick-verwachsnen Gr\u00fcnden, durch mein von Lust", "tokens": ["Ich", "h\u00f6r'", "in", "dick\u00b7ver\u00b7wachs\u00b7nen", "Gr\u00fcn\u00b7den", ",", "durch", "mein", "von", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "APPR", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "gesch\u00e4rftes Lauschen,", "tokens": ["ge\u00b7sch\u00e4rf\u00b7tes", "Lau\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Ein \u00fcber Kiesel rieselnd B\u00e4chlein Dir, seinem HErrn, ein", "tokens": ["Ein", "\u00fc\u00b7ber", "Kie\u00b7sel", "rie\u00b7selnd", "B\u00e4ch\u00b7lein", "Dir", ",", "sei\u00b7nem", "Herrn", ",", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "APPR", "NN", "VVPP", "NN", "PPER", "$,", "PPOSAT", "NN", "$,", "ART"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Lob-Lied rauschen,", "tokens": ["Lob\u00b7Lied", "rau\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Mit welchem ich, von Andacht hei\u00df, gedr\u00e4ngt von innigem", "tokens": ["Mit", "wel\u00b7chem", "ich", ",", "von", "An\u00b7dacht", "hei\u00df", ",", "ge\u00b7dr\u00e4ngt", "von", "in\u00b7ni\u00b7gem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "$,", "APPR", "NN", "ADJD", "$,", "VVPP", "APPR", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Vergn\u00fcgen,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Jm sanften Rauschen meiner Seufzer mein Lob-Lied mich", "tokens": ["Jm", "sanf\u00b7ten", "Rau\u00b7schen", "mei\u00b7ner", "Seuf\u00b7zer", "mein", "Lob\u00b7Lied", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "bem\u00fch' zu f\u00fcgen.", "tokens": ["be\u00b7m\u00fch'", "zu", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ich sp\u00fchre, mit erfreutem Blick, ich sehe, mit ger\u00fchrter", "tokens": ["Ich", "sp\u00fch\u00b7re", ",", "mit", "er\u00b7freu\u00b7tem", "Blick", ",", "ich", "se\u00b7he", ",", "mit", "ge\u00b7r\u00fchr\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "