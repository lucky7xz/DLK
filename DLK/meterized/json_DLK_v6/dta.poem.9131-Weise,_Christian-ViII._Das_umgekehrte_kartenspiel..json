{"dta.poem.9131": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n Das umgekehrte kartenspiel.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wann wir die eitlen Sachen", "tokens": ["Wann", "wir", "die", "eit\u00b7len", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der s\u00fcssen l\u00f6ffeley", "tokens": ["Der", "s\u00fcs\u00b7sen", "l\u00f6f\u00b7fe\u00b7ley"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und etwas kuntbar machen/", "tokens": ["Und", "et\u00b7was", "kunt\u00b7bar", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ist die fantasey/", "tokens": ["So", "ist", "die", "fan\u00b7ta\u00b7sey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo man die warheit sagen will/", "tokens": ["Wo", "man", "die", "war\u00b7heit", "sa\u00b7gen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein umgekehrtes kartenspiel.", "tokens": ["Ein", "um\u00b7ge\u00b7kehr\u00b7tes", "kar\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ein spieler wirfft die bl\u00e4tter", "tokens": ["Ein", "spie\u00b7ler", "wirfft", "die", "bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So eyfrig aus der faust/", "tokens": ["So", "ey\u00b7frig", "aus", "der", "faust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das manchem das geschmetter", "tokens": ["Das", "man\u00b7chem", "das", "ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lang in den ohren sau\u00dft/", "tokens": ["Lang", "in", "den", "oh\u00b7ren", "sau\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein buhler giebt in stiller ruh", "tokens": ["Ein", "buh\u00b7ler", "giebt", "in", "stil\u00b7ler", "ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den h\u00f6chsten trumpf verschwiegen zu.", "tokens": ["Den", "h\u00f6chs\u00b7ten", "trumpf", "ver\u00b7schwie\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ein spieler will gewinnen/", "tokens": ["Ein", "spie\u00b7ler", "will", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wann es doppelt steht/", "tokens": ["Und", "wann", "es", "dop\u00b7pelt", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So richtet sein beginnen", "tokens": ["So", "rich\u00b7tet", "sein", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich eintzig auffs labeth:", "tokens": ["Sich", "eint\u00b7zig", "auffs", "la\u00b7be\u00b7th", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ein buhler nimmt sich schlecht in acht/", "tokens": ["Ein", "buh\u00b7ler", "nimmt", "sich", "schlecht", "in", "acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPR", "CARD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja er verspielet mit bedacht.", "tokens": ["Ja", "er", "ver\u00b7spie\u00b7let", "mit", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ein spieler mischt die karten/", "tokens": ["Ein", "spie\u00b7ler", "mischt", "die", "kar\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bi\u00df er das h\u00f6chste blat", "tokens": ["Bi\u00df", "er", "das", "h\u00f6chs\u00b7te", "blat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach viel versuchten arten", "tokens": ["Nach", "viel", "ver\u00b7such\u00b7ten", "ar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinen h\u00e4nden hat:", "tokens": ["In", "sei\u00b7nen", "h\u00e4n\u00b7den", "hat", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein buhler w\u00fcnscht in seiner pein", "tokens": ["Ein", "buh\u00b7ler", "w\u00fcnscht", "in", "sei\u00b7ner", "pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur das geringste blat zu seyn.", "tokens": ["Nur", "das", "ge\u00b7rings\u00b7te", "blat", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ein spieler wird verdrossen", "tokens": ["Ein", "spie\u00b7ler", "wird", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und meint es sey geschehn/", "tokens": ["Und", "meint", "es", "sey", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann seine spielgenossen", "tokens": ["Wann", "sei\u00b7ne", "spiel\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jhm in die karte sehn:", "tokens": ["Jhm", "in", "die", "kar\u00b7te", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein buhler zeiget seiner zier", "tokens": ["Ein", "buh\u00b7ler", "zei\u00b7get", "sei\u00b7ner", "zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die bl\u00e4tter ungebeten f\u00fcr.", "tokens": ["Die", "bl\u00e4t\u00b7ter", "un\u00b7ge\u00b7be\u00b7ten", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Wiewol bey solchen h\u00e4ndeln", "tokens": ["Wie\u00b7wol", "bey", "sol\u00b7chen", "h\u00e4n\u00b7deln"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht der verlust noch hin/", "tokens": ["Geht", "der", "ver\u00b7lust", "noch", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man mag gleich all\u2019s vertendeln", "tokens": ["Man", "mag", "gleich", "all's", "ver\u00b7ten\u00b7deln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So hat man doch gewinn.", "tokens": ["So", "hat", "man", "doch", "ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn der verspielt bleibt immer reich", "tokens": ["Denn", "der", "ver\u00b7spielt", "bleibt", "im\u00b7mer", "reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und der gewinnt verspielt zugleich.", "tokens": ["Und", "der", "ge\u00b7winnt", "ver\u00b7spielt", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}