{"textgrid.poem.50802": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ist ein Buch, geschw\u00e4rzt und halb verbrannt,", "tokens": ["Da", "ist", "ein", "Buch", ",", "ge\u00b7schw\u00e4rzt", "und", "halb", "ver\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wonach der Mann in Todesangst gesandt;", "tokens": ["Wo\u00b7nach", "der", "Mann", "in", "To\u00b7de\u00b7sangst", "ge\u00b7sandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein J\u00fcngling wagte dran sein junges Blut", "tokens": ["Ein", "J\u00fcng\u00b7ling", "wag\u00b7te", "dran", "sein", "jun\u00b7ges", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und trug's mit keckem Arme aus der Glut.", "tokens": ["Und", "trug's", "mit", "ke\u00b7ckem", "Ar\u00b7me", "aus", "der", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und gierig st\u00fcrzt der Mann sich auf das Buch", "tokens": ["Und", "gie\u00b7rig", "st\u00fcrzt", "der", "Mann", "sich", "auf", "das", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und \u2013 wirft es weg mit einem derben Fluch!", "tokens": ["Und", "\u2013", "wirft", "es", "weg", "mit", "ei\u00b7nem", "der\u00b7ben", "Fluch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein dickes Schuldnerbuch hatt er gemeint!", "tokens": ["Sein", "di\u00b7ckes", "Schuld\u00b7ner\u00b7buch", "hatt", "er", "ge\u00b7meint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.4": {"text": "Nun liegt \u2013 die Bibel vor dem guten Freund.", "tokens": ["Nun", "liegt", "\u2013", "die", "Bi\u00b7bel", "vor", "dem", "gu\u00b7ten", "Freund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie arg und undankbar ist diese Welt!", "tokens": ["Wie", "arg", "und", "un\u00b7dank\u00b7bar", "ist", "die\u00b7se", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie schm\u00e4hlich nun der alte Mann sich stellt!", "tokens": ["Wie", "schm\u00e4h\u00b7lich", "nun", "der", "al\u00b7te", "Mann", "sich", "stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erinnert ihn die Bibel nicht mehr dran,", "tokens": ["E\u00b7rin\u00b7nert", "ihn", "die", "Bi\u00b7bel", "nicht", "mehr", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie g\u00fctlich er sich oft an ihr getan?", "tokens": ["Wie", "g\u00fct\u00b7lich", "er", "sich", "oft", "an", "ihr", "ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Wenn er am Sonntagabend vor ihr sa\u00df", "tokens": ["Wenn", "er", "am", "Sonn\u00b7ta\u00b7ga\u00b7bend", "vor", "ihr", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schmunzelnd dann von dem Kamele las,", "tokens": ["Und", "schmun\u00b7zelnd", "dann", "von", "dem", "Ka\u00b7me\u00b7le", "las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Nadel\u00f6hre und dem Himmelreich,", "tokens": ["Dem", "Na\u00b7de\u00b7l\u00f6h\u00b7re", "und", "dem", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie ward ihm das Gem\u00fct da froh und weich!", "tokens": ["Wie", "ward", "ihm", "das", "Ge\u00b7m\u00fct", "da", "froh", "und", "weich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie manchen Bettler, hungerig und matt,", "tokens": ["Wie", "man\u00b7chen", "Bett\u00b7ler", ",", "hun\u00b7ge\u00b7rig", "und", "matt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Macht' er mit sch\u00f6nen Bibelspr\u00fcchen satt,", "tokens": ["Macht'", "er", "mit", "sch\u00f6\u00b7nen", "Bi\u00b7bel\u00b7spr\u00fc\u00b7chen", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Beteurend hoch und feierlich dabei,", "tokens": ["Be\u00b7teu\u00b7rend", "hoch", "und", "fei\u00b7er\u00b7lich", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df dies sein reichster Trost und Hausschatz sei!", "tokens": ["Da\u00df", "dies", "sein", "reichs\u00b7ter", "Trost", "und", "Haus\u00b7schatz", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "ADJA", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nun liegt das alte Buch zertreten hier,", "tokens": ["Nun", "liegt", "das", "al\u00b7te", "Buch", "zer\u00b7tre\u00b7ten", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Feuer blieb der Ecken Silberzier;", "tokens": ["Im", "Feu\u00b7er", "blieb", "der", "E\u00b7cken", "Sil\u00b7ber\u00b7zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zerri\u00dfnen Angesichtes liegt im Kot", "tokens": ["Zer\u00b7ri\u00df\u00b7nen", "An\u00b7ge\u00b7sich\u00b7tes", "liegt", "im", "Kot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das einst so hoch gepriesne Lebensbrot.", "tokens": ["Das", "einst", "so", "hoch", "ge\u00b7pri\u00b7es\u00b7ne", "Le\u00b7bens\u00b7brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}