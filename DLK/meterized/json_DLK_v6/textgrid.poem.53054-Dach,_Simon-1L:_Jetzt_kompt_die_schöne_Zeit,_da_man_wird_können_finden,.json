{"textgrid.poem.53054": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzt kompt die sch\u00f6ne Zeit, da man wird k\u00f6nnen finden,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt kompt die sch\u00f6ne Zeit, da man wird k\u00f6nnen finden,", "tokens": ["Jetzt", "kompt", "die", "sch\u00f6\u00b7ne", "Zeit", ",", "da", "man", "wird", "k\u00f6n\u00b7nen", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PIS", "VAFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nur Gestreuche steht, im Wald' vnd in den Gr\u00fcnden,", "tokens": ["Wo", "nur", "Ge\u00b7streu\u00b7che", "steht", ",", "im", "Wald'", "vnd", "in", "den", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VVFIN", "$,", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch wo man sonst nur kompt, da\u00df was man jetzt schon hat", "tokens": ["Auch", "wo", "man", "sonst", "nur", "kompt", ",", "da\u00df", "was", "man", "jetzt", "schon", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PIS", "ADV", "ADV", "VVFIN", "$,", "KOUS", "PWS", "PIS", "ADV", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da drau\u00dfen auff dem Land vnd bey vns in der Stadt.", "tokens": ["Da", "drau\u00b7\u00dfen", "auff", "dem", "Land", "vnd", "bey", "vns", "in", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "KON", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist jn etwas Rund, nicht gro\u00df, man mag es gleichen", "tokens": ["Es", "ist", "jn", "et\u00b7was", "Rund", ",", "nicht", "gro\u00df", ",", "man", "mag", "es", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$,", "PTKNEG", "ADJD", "$,", "PIS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Gl\u00fcckes Wanckelmuth, kein Mahler mag erreichen", "tokens": ["Des", "Gl\u00fc\u00b7ckes", "Wan\u00b7ckel\u00b7muth", ",", "kein", "Mah\u00b7ler", "mag", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PIAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Sch\u00f6nheit, so es hat, auch gleicht es einem Fa\u00df',", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "so", "es", "hat", ",", "auch", "gleicht", "es", "ei\u00b7nem", "Fa\u00df'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jedoch geschickt vnd rund, vnd bricht bald wie ein Gla\u00df.", "tokens": ["Je\u00b7doch", "ge\u00b7schickt", "vnd", "rund", ",", "vnd", "bricht", "bald", "wie", "ein", "Gla\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADJD", "$,", "KON", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es sieht al\u00df wer es todt; dadoch, nach dem die W\u00e4rme", "tokens": ["Es", "sieht", "al\u00df", "wer", "es", "todt", ";", "da\u00b7doch", ",", "nach", "dem", "die", "W\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPER", "ADJD", "$.", "PAV", "$,", "APPR", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Recht es vmbschlossen hat, bek\u00f6mpt es sein Ged\u00e4rme,", "tokens": ["Recht", "es", "vmb\u00b7schlos\u00b7sen", "hat", ",", "be\u00b7k\u00f6mpt", "es", "sein", "Ge\u00b7d\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sein Leben allgemach, vnd einen solchen Geist,", "tokens": ["Sein", "Le\u00b7ben", "all\u00b7ge\u00b7mach", ",", "vnd", "ei\u00b7nen", "sol\u00b7chen", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "KON", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der nachmahls singt vnd schreyt, vnd ohn auffh\u00f6ren preist", "tokens": ["Der", "nach\u00b7mahls", "singt", "vnd", "schreyt", ",", "vnd", "ohn", "auff\u00b7h\u00f6\u00b7ren", "preist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVFIN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den der ihn hat gemacht. Man pflegt es zu verg\u00f6nnen", "tokens": ["Den", "der", "ihn", "hat", "ge\u00b7macht", ".", "Man", "pflegt", "es", "zu", "ver\u00b7g\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PRELS", "PPER", "VAFIN", "VVPP", "$.", "PIS", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Am meisten diesen nur, die nicht fast buhlen k\u00f6nnen", "tokens": ["Am", "meis\u00b7ten", "die\u00b7sen", "nur", ",", "die", "nicht", "fast", "buh\u00b7len", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "PIS", "PDAT", "ADV", "$,", "PRELS", "PTKNEG", "ADV", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vnd Vnwerth worden seyn, weil es die Geister regt,", "tokens": ["Vnd", "Vn\u00b7werth", "wor\u00b7den", "seyn", ",", "weil", "es", "die", "Geis\u00b7ter", "regt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAPP", "VAINF", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vnd macht, das mancher gern der Keuschen Liebe pflegt.", "tokens": ["Vnd", "macht", ",", "das", "man\u00b7cher", "gern", "der", "Keu\u00b7schen", "Lie\u00b7be", "pflegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Auch bringt es sonsten vns, was wir verlohren, wieder,", "tokens": ["Auch", "bringt", "es", "sons\u00b7ten", "vns", ",", "was", "wir", "ver\u00b7loh\u00b7ren", ",", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VMFIN", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ertheilt ein sch\u00f6n Gebl\u00fct dem Leib' in alle Glieder,", "tokens": ["Er\u00b7theilt", "ein", "sch\u00f6n", "Ge\u00b7bl\u00fct", "dem", "Leib'", "in", "al\u00b7le", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dadurch empfinden wir wie newe LebensKrafft,", "tokens": ["Da\u00b7durch", "emp\u00b7fin\u00b7den", "wir", "wie", "ne\u00b7we", "Le\u00b7bens", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Vnd was sonst dieses Ding f\u00fcr tausent Nutzen schafft.", "tokens": ["Vnd", "was", "sonst", "die\u00b7ses", "Ding", "f\u00fcr", "tau\u00b7sent", "Nut\u00b7zen", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PDAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nun zweiffelt mir gantz nicht, jhr habt es schon errahten,", "tokens": ["Nun", "zweif\u00b7felt", "mir", "gantz", "nicht", ",", "jhr", "habt", "es", "schon", "er\u00b7rah\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Herr Br\u00e4utgam, doch wo nicht, so kommet euch zu statten", "tokens": ["Herr", "Br\u00e4ut\u00b7gam", ",", "doch", "wo", "nicht", ",", "so", "kom\u00b7met", "euch", "zu", "stat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "KON", "PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ewr Nahm' vnd giebt euch Krafft des ersten lauts bericht:", "tokens": ["Ewr", "Nahm'", "vnd", "giebt", "euch", "Krafft", "des", "ers\u00b7ten", "lauts", "be\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "PPER", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie dieses Ding so sch\u00f6n wie rund, wie bald es bricht.", "tokens": ["Wie", "die\u00b7ses", "Ding", "so", "sch\u00f6n", "wie", "rund", ",", "wie", "bald", "es", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "ADJD", "KOKOM", "ADJD", "$,", "PWAV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dafern' ihr Jungfraw Braut, es auch nicht k\u00f6nt ergr\u00fcnden,", "tokens": ["Da\u00b7fern'", "ihr", "Jung\u00b7fraw", "Braut", ",", "es", "auch", "nicht", "k\u00f6nt", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "NN", "$,", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So sollt ihr gleichfall\u00df die\u00df in ewrem Nahmen finden,", "tokens": ["So", "sollt", "ihr", "gleich\u00b7fall\u00df", "die\u00df", "in", "ew\u00b7rem", "Nah\u00b7men", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PDS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nembt diesen Buchstab ab, der nur von Klagen sagt,", "tokens": ["Nembt", "die\u00b7sen", "Buch\u00b7stab", "ab", ",", "der", "nur", "von", "Kla\u00b7gen", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So habt jhr de\u00df so viel, al\u00df jmmer euch verhagt.", "tokens": ["So", "habt", "jhr", "de\u00df", "so", "viel", ",", "al\u00df", "jm\u00b7mer", "euch", "ver\u00b7hagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ihr geht dem Br\u00e4utgam vor, dieweil er eins nur f\u00fchret,", "tokens": ["Ihr", "geht", "dem", "Br\u00e4ut\u00b7gam", "vor", ",", "die\u00b7weil", "er", "eins", "nur", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "PTKVZ", "$,", "KOUS", "PPER", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ewr Nahm' euch aber des vnzehlig viel geb\u00fchret,", "tokens": ["Ewr", "Nahm'", "euch", "a\u00b7ber", "des", "vn\u00b7zeh\u00b7lig", "viel", "ge\u00b7b\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "ART", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Vieleicht dieweil jhr wolt jhm' allzeit gehen f\u00fcr", "tokens": ["Vie\u00b7leicht", "die\u00b7weil", "jhr", "wolt", "jhm'", "all\u00b7zeit", "ge\u00b7hen", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "APPR"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "An reicher Fruchtbarkeit, des Ehstands besten Zier.", "tokens": ["An", "rei\u00b7cher", "Frucht\u00b7bar\u00b7keit", ",", "des", "Eh\u00b7stands", "bes\u00b7ten", "Zier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Daferne nur die Lieb' an runde dem wird gleichen", "tokens": ["Da\u00b7fer\u00b7ne", "nur", "die", "Lieb'", "an", "run\u00b7de", "dem", "wird", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ADJA", "PDS", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was jhr errahten habt, vnd keins dem andern weichen", "tokens": ["Was", "jhr", "er\u00b7rah\u00b7ten", "habt", ",", "vnd", "keins", "dem", "an\u00b7dern", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "VAFIN", "$,", "KON", "PIAT", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "An h\u00f6chster Freundligkeit, so wird in grosser Zahl", "tokens": ["An", "h\u00f6chs\u00b7ter", "Freund\u00b7lig\u00b7keit", ",", "so", "wird", "in", "gros\u00b7ser", "Zahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Segen vmb euch seyn, erfrewen vberall.", "tokens": ["Der", "Se\u00b7gen", "vmb", "euch", "seyn", ",", "er\u00b7fre\u00b7wen", "vbe\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAINF", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "Mich d\u00fcnckt, ich sehe schon, wie euch auch an Geberden", "tokens": ["Mich", "d\u00fcnckt", ",", "ich", "se\u00b7he", "schon", ",", "wie", "euch", "auch", "an", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Vnd grosser Hoffligkeit, recht werden ehnlich werden", "tokens": ["Vnd", "gros\u00b7ser", "Hoff\u00b7lig\u00b7keit", ",", "recht", "wer\u00b7den", "ehn\u00b7lich", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "VAFIN", "ADJD", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die, so ewr Keusches thun, nach vmbgang seiner Zeit", "tokens": ["Die", ",", "so", "ewr", "Keu\u00b7sches", "thun", ",", "nach", "vmb\u00b7gang", "sei\u00b7ner", "Zeit"], "