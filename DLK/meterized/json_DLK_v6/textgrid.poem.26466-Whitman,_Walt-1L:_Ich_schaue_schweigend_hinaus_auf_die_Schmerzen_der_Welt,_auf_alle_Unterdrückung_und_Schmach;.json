{"textgrid.poem.26466": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich schaue schweigend hinaus auf die Schmerzen der Welt, auf alle Unterdr\u00fcckung und Schmach;", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schaue schweigend hinaus auf die Schmerzen der Welt, auf alle Unterdr\u00fcckung und Schmach;", "tokens": ["Ich", "schau\u00b7e", "schwei\u00b7gend", "hin\u00b7aus", "auf", "die", "Schmer\u00b7zen", "der", "Welt", ",", "auf", "al\u00b7le", "Un\u00b7ter\u00b7dr\u00fc\u00b7ckung", "und", "Schmach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+---+-+--+-+-+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ich h\u00f6re das heimliche Schluchzen junger Leute, die, mit sich selbst zerfallen, Begangenes bereuen;", "tokens": ["Ich", "h\u00f6\u00b7re", "das", "heim\u00b7li\u00b7che", "Schluch\u00b7zen", "jun\u00b7ger", "Leu\u00b7te", ",", "die", ",", "mit", "sich", "selbst", "zer\u00b7fal\u00b7len", ",", "Be\u00b7gan\u00b7ge\u00b7nes", "be\u00b7reu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$,", "PRELS", "$,", "APPR", "PRF", "ADV", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+--+-+-+--+---+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich sehe die abgeh\u00e4rmte, verlassene, im Elend sterbende Mutter;", "tokens": ["Ich", "se\u00b7he", "die", "ab\u00b7ge\u00b7h\u00e4rm\u00b7te", ",", "ver\u00b7las\u00b7se\u00b7ne", ",", "im", "E\u00b7lend", "ster\u00b7ben\u00b7de", "Mut\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "$,", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+---+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ich sehe das vom Manne mi\u00dfhandelte Weib, den verr\u00e4terischen Verf\u00fchrer der Jungfrau;", "tokens": ["Ich", "se\u00b7he", "das", "vom", "Man\u00b7ne", "mi\u00df\u00b7han\u00b7del\u00b7te", "Weib", ",", "den", "ver\u00b7r\u00e4\u00b7te\u00b7ri\u00b7schen", "Ver\u00b7f\u00fch\u00b7rer", "der", "Jung\u00b7frau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPRART", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--+--+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Ich kenne die Eifersucht und die unverstandene Liebe, die man so gern verbergen m\u00f6chte \u2013", "tokens": ["Ich", "ken\u00b7ne", "die", "Ei\u00b7fer\u00b7sucht", "und", "die", "un\u00b7ver\u00b7stan\u00b7de\u00b7ne", "Lie\u00b7be", ",", "die", "man", "so", "gern", "ver\u00b7ber\u00b7gen", "m\u00f6ch\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "ADV", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+--+-+--+-+--+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}