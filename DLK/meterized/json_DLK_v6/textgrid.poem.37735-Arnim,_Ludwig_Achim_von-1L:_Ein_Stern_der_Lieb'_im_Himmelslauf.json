{"textgrid.poem.37735": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Stern der Lieb' im Himmelslauf", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Stern der Lieb' im Himmelslauf", "tokens": ["Ein", "Stern", "der", "Lieb'", "im", "Him\u00b7mels\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die offne Brust mit Glanz mir k\u00fchlt", "tokens": ["Die", "off\u00b7ne", "Brust", "mit", "Glanz", "mir", "k\u00fchlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Fr\u00fchling warm im Herzen spielt,", "tokens": ["Ein", "Fr\u00fch\u00b7ling", "warm", "im", "Her\u00b7zen", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein neues R\u00f6slein bl\u00fcht darauf:", "tokens": ["Ein", "neu\u00b7es", "R\u00f6s\u00b7lein", "bl\u00fcht", "da\u00b7rauf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist der Stern voll Lieb und Lust,", "tokens": ["Du", "bist", "der", "Stern", "voll", "Lieb", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Athem k\u00fchlet meine Brust,", "tokens": ["Dein", "A\u00b7them", "k\u00fch\u00b7let", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du bist der Fr\u00fchling, der mich w\u00e4rmt,", "tokens": ["Du", "bist", "der", "Fr\u00fch\u00b7ling", ",", "der", "mich", "w\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der in des Herzens Blume schw\u00e4rmt,", "tokens": ["Der", "in", "des", "Her\u00b7zens", "Blu\u00b7me", "schw\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du hast die Ros' ans Herz gelegt,", "tokens": ["Du", "hast", "die", "Ros'", "ans", "Herz", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So bl\u00fchst du aussen, bl\u00fchst darin,", "tokens": ["So", "bl\u00fchst", "du", "aus\u00b7sen", ",", "bl\u00fchst", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Fr\u00fchlingskraft sich doppelt regt,", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7kraft", "sich", "dop\u00b7pelt", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das R\u00f6slein w\u00e4chst und f\u00fcllt den Sinn,", "tokens": ["Das", "R\u00f6s\u00b7lein", "w\u00e4chst", "und", "f\u00fcllt", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich schwebe in dem Liebes Duft,", "tokens": ["Ich", "schwe\u00b7be", "in", "dem", "Lie\u00b7bes", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Unendlich scheint das Blau der Luft.", "tokens": ["Un\u00b7end\u00b7lich", "scheint", "das", "Blau", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Sterne w\u00e4hnt in Winternacht,", "tokens": ["Ihr", "Ster\u00b7ne", "w\u00e4hnt", "in", "Win\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich von s\u00fcssem Weine gl\u00fch'.", "tokens": ["Da\u00df", "ich", "von", "s\u00fcs\u00b7sem", "Wei\u00b7ne", "gl\u00fch'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O freut euch, wie ich einsam bl\u00fch,", "tokens": ["O", "freut", "euch", ",", "wie", "ich", "ein\u00b7sam", "bl\u00fch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz bey einer Rose wacht;", "tokens": ["Mein", "Herz", "bey", "ei\u00b7ner", "Ro\u00b7se", "wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie ist der Wein von dem ich gl\u00fch,", "tokens": ["Sie", "ist", "der", "Wein", "von", "dem", "ich", "gl\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ich tiefathmend an mich zieh,", "tokens": ["Die", "ich", "tie\u00b7fath\u00b7mend", "an", "mich", "zieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie ist von dir der Liebesschein,", "tokens": ["Sie", "ist", "von", "dir", "der", "Lie\u00b7bes\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du bist in ihr nun doppelt mein,", "tokens": ["Du", "bist", "in", "ihr", "nun", "dop\u00b7pelt", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gern th\u00e4t sich Lust in Worten kund", "tokens": ["Gern", "th\u00e4t", "sich", "Lust", "in", "Wor\u00b7ten", "kund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NN", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So lebenswarm, wie Tropfen Blut,", "tokens": ["So", "le\u00b7bens\u00b7warm", ",", "wie", "Trop\u00b7fen", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch mir das R\u00f6slein schliest den Mund", "tokens": ["Doch", "mir", "das", "R\u00f6s\u00b7lein", "schliest", "den", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und thut da k\u00fchlend mir so gut;", "tokens": ["Und", "thut", "da", "k\u00fch\u00b7lend", "mir", "so", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Augen f\u00fcllt ein s\u00fcsser Drang:", "tokens": ["Die", "Au\u00b7gen", "f\u00fcllt", "ein", "s\u00fcs\u00b7ser", "Drang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "O Liebesthau, inThr\u00e4nen Dank.", "tokens": ["O", "Lie\u00b7best\u00b7hau", ",", "in", "Thr\u00e4\u00b7nen", "Dank", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}