{"textgrid.poem.38606": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Elegie an den Tod", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo Zipressen traurend niederhangen,", "tokens": ["Wo", "Zip\u00b7res\u00b7sen", "trau\u00b7rend", "nie\u00b7der\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo auf moosbedecktem Leichenstein", "tokens": ["Wo", "auf", "moos\u00b7be\u00b7deck\u00b7tem", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Der Verwesung Schauer mich umfangen,", "tokens": ["Der", "Ver\u00b7we\u00b7sung", "Schau\u00b7er", "mich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hier, o Tod, gedenk' ich ruhig dein.", "tokens": ["Hier", ",", "o", "Tod", ",", "ge\u00b7denk'", "ich", "ru\u00b7hig", "dein", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ADJD", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Freundlich wie im Lenz die Abendsonne,", "tokens": ["Freund\u00b7lich", "wie", "im", "Lenz", "die", "A\u00b7bend\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mild wie Mondlicht in der Bl\u00fctenzeit,", "tokens": ["Mild", "wie", "Mond\u00b7licht", "in", "der", "Bl\u00fc\u00b7ten\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Lacht dein Antliz Paradieseswonne,", "tokens": ["Lacht", "dein", "Ant\u00b7liz", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Genius der Unverg\u00e4nglichkeit.", "tokens": ["Ge\u00b7nius", "der", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer hat mit hohen Engelminen,", "tokens": ["Im\u00b7mer", "hat", "mit", "ho\u00b7hen", "En\u00b7gel\u00b7mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herlich von der Gottheit Licht umstralt,", "tokens": ["Her\u00b7lich", "von", "der", "Got\u00b7theit", "Licht", "um\u00b7stralt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie du einst dem Sokrates erschienen,", "tokens": ["Wie", "du", "einst", "dem", "Sok\u00b7ra\u00b7tes", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NE", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mir die Phantasie dein Bild gemalt.", "tokens": ["Mir", "die", "Phan\u00b7ta\u00b7sie", "dein", "Bild", "ge\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Immer hat im Thr\u00e4nenweidenthale,", "tokens": ["Im\u00b7mer", "hat", "im", "Thr\u00e4\u00b7nen\u00b7wei\u00b7dent\u00b7ha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo der West durch Todtenkr\u00e4nze bebt,", "tokens": ["Wo", "der", "West", "durch", "Tod\u00b7ten\u00b7kr\u00e4n\u00b7ze", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Leuchtend von der Hofnung goldnem Strale,", "tokens": ["Leuch\u00b7tend", "von", "der", "Hof\u00b7nung", "gold\u00b7nem", "Stra\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mich dies engelsch\u00f6ne Bild umschwebt.", "tokens": ["Mich", "dies", "en\u00b7gel\u00b7sch\u00f6\u00b7ne", "Bild", "um\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Holder J\u00fcngling! deines Blickes Milde", "tokens": ["Hol\u00b7der", "J\u00fcng\u00b7ling", "!", "dei\u00b7nes", "Bli\u00b7ckes", "Mil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hebt auf Ahndungsfl\u00fcgeln meinen Geist", "tokens": ["Hebt", "auf", "Ahn\u00b7dungs\u00b7fl\u00fc\u00b7geln", "mei\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In die ewig bl\u00fchenden Gefilde,", "tokens": ["In", "die", "e\u00b7wig", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo Vollendung alles Jammers fleu\u00dft.", "tokens": ["Wo", "Vol\u00b7len\u00b7dung", "al\u00b7les", "Jam\u00b7mers", "fleu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Schweb' ich schon im Lichte der Verkl\u00e4rten,", "tokens": ["Schweb'", "ich", "schon", "im", "Lich\u00b7te", "der", "Ver\u00b7kl\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sanft begr\u00fc\u00dft von Engelharfenton?", "tokens": ["Sanft", "be\u00b7gr\u00fc\u00dft", "von", "En\u00b7gel\u00b7har\u00b7fen\u00b7ton", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Knie' ich schon im Chore der Erh\u00f6rten,", "tokens": ["Knie'", "ich", "schon", "im", "Cho\u00b7re", "der", "Er\u00b7h\u00f6r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Tief anbetend an Jehova's Thron?", "tokens": ["Tief", "an\u00b7be\u00b7tend", "an", "Je\u00b7ho\u00b7va's", "Thron", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NE", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}