{"dta.poem.20788": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "1755.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O h\u00e4tt ich jezt den Geist der Unzerin,                 ", "tokens": ["O", "h\u00e4tt", "ich", "jezt", "den", "Geist", "der", "Un\u00b7ze\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das denkende und das erhabne Wesen,", "tokens": ["Das", "den\u00b7ken\u00b7de", "und", "das", "er\u00b7hab\u00b7ne", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um feuriger und aufgekl\u00e4rt im Sinn", "tokens": ["Um", "feu\u00b7ri\u00b7ger", "und", "auf\u00b7ge\u00b7kl\u00e4rt", "im", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Begriff und Bilder auszulesen,", "tokens": ["Be\u00b7griff", "und", "Bil\u00b7der", "aus\u00b7zu\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Welch ein Gem\u00e4hld\u2019 entw\u00fcrf ich da von dir,", "tokens": ["Welch", "ein", "Ge\u00b7m\u00e4hld'", "ent\u00b7w\u00fcrf", "ich", "da", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O Schicksal! dein verborgnes Winken,", "tokens": ["O", "Schick\u00b7sal", "!", "dein", "ver\u00b7borg\u00b7nes", "Win\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie dein Wink der Dinge Gang regier,", "tokens": ["Und", "wie", "dein", "Wink", "der", "Din\u00b7ge", "Gang", "re\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ART", "NN", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und wie dich der Vern\u00fcnftler D\u00fcnken", "tokens": ["Und", "wie", "dich", "der", "Ver\u00b7n\u00fcnft\u00b7ler", "D\u00fcn\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit schielem Blick un\u00fcberdacht verkennt,", "tokens": ["Mit", "schie\u00b7lem", "Blick", "un\u00b7\u00fc\u00b7berd\u00b7acht", "ver\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und wie dein Ausspruch unserm Leben", "tokens": ["Und", "wie", "dein", "Aus\u00b7spruch", "un\u00b7serm", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Reihen der Begebenheiten nennt;", "tokens": ["Die", "Rei\u00b7hen", "der", "Be\u00b7ge\u00b7ben\u00b7hei\u00b7ten", "nennt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dies alles w\u00fcrd ich dann der Welt vorz\u00fcglich zu", "tokens": ["Dies", "al\u00b7les", "w\u00fcrd", "ich", "dann", "der", "Welt", "vor\u00b7z\u00fcg\u00b7lich", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "PTKZU"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "betrachten geben.", "tokens": ["be\u00b7trach\u00b7ten", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Eh dies Geb\u00e4u, was jezt so pr\u00e4chtig steht,", "tokens": ["Eh", "dies", "Ge\u00b7b\u00e4u", ",", "was", "jezt", "so", "pr\u00e4ch\u00b7tig", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sich aus des Klumpens Unform risse,", "tokens": ["Sich", "aus", "des", "Klum\u00b7pens", "Un\u00b7form", "ris\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch eh der Staub beseelt ward und erh\u00f6ht,", "tokens": ["Noch", "eh", "der", "Staub", "be\u00b7seelt", "ward", "und", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "VAFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und seines Sch\u00f6pfers Abbild hie\u00dfe;", "tokens": ["Und", "sei\u00b7nes", "Sch\u00f6p\u00b7fers", "Ab\u00b7bild", "hie\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da sahst du schon Jahrhunderte entdeckt,", "tokens": ["Da", "sahst", "du", "schon", "Jahr\u00b7hun\u00b7der\u00b7te", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Du sahsts entziffert vor dir liegen,", "tokens": ["Du", "sahsts", "ent\u00b7zif\u00b7fert", "vor", "dir", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wohin der Trieb und seine Folge zweckt,", "tokens": ["Wo\u00b7hin", "der", "Trieb", "und", "sei\u00b7ne", "Fol\u00b7ge", "zweckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hier ordnete dein ewig F\u00fcgen,", "tokens": ["Hier", "ord\u00b7ne\u00b7te", "dein", "e\u00b7wig", "F\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Reiche Fall, hier theiltest du voraus,", "tokens": ["Der", "Rei\u00b7che", "Fall", ",", "hier", "theil\u00b7test", "du", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Eh noch geborne Herrscher waren,", "tokens": ["Eh", "noch", "ge\u00b7bor\u00b7ne", "Herr\u00b7scher", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die theure Last der K\u00f6nig-Kronen aus,", "tokens": ["Die", "theu\u00b7re", "Last", "der", "K\u00f6\u00b7nig\u00b7Kro\u00b7nen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Hier seztest du den Zeitpunkt fest, in welchem wir", "tokens": ["Hier", "sez\u00b7test", "du", "den", "Zeit\u00b7punkt", "fest", ",", "in", "wel\u00b7chem", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "uns offenbaren.", "tokens": ["uns", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dein Finger schreibt in Tafeln hell von Glanz", "tokens": ["Dein", "Fin\u00b7ger", "schreibt", "in", "Ta\u00b7feln", "hell", "von", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Ordnungen, die sich erhalten,", "tokens": ["Die", "Ord\u00b7nun\u00b7gen", ",", "die", "sich", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Die Wesenheit bleibt durch dich immer ganz,", "tokens": ["Die", "We\u00b7sen\u00b7heit", "bleibt", "durch", "dich", "im\u00b7mer", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Welten m\u00fcssen nie veralten,", "tokens": ["Die", "Wel\u00b7ten", "m\u00fcs\u00b7sen", "nie", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du hie\u00dfest sie in ihren Kreisen gehn,", "tokens": ["Du", "hie\u00b7\u00dfest", "sie", "in", "ih\u00b7ren", "Krei\u00b7sen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In denen sie sich jezt noch winden,", "tokens": ["In", "de\u00b7nen", "sie", "sich", "jezt", "noch", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht ungef\u00e4hr kann was geschieht entstehn,", "tokens": ["Nicht", "un\u00b7ge\u00b7f\u00e4hr", "kann", "was", "ge\u00b7schieht", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PIS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es f\u00fcgt dein \u00fcberdacht Verbinden", "tokens": ["Es", "f\u00fcgt", "dein", "\u00fc\u00b7berd\u00b7acht", "Ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In einer Welt, die doch die beste bleibt,", "tokens": ["In", "ei\u00b7ner", "Welt", ",", "die", "doch", "die", "bes\u00b7te", "bleibt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das Einzle in Zusammenh\u00e4nge,", "tokens": ["Das", "Einz\u00b7le", "in", "Zu\u00b7sam\u00b7men\u00b7h\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df jens entspringt, und da\u00df es dies vertreibt,", "tokens": ["Da\u00df", "jens", "ent\u00b7springt", ",", "und", "da\u00df", "es", "dies", "ver\u00b7treibt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "$,", "KON", "KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ist darum, da\u00df sich nichts unachtsam durcheinander", "tokens": ["Ist", "da\u00b7rum", ",", "da\u00df", "sich", "nichts", "u\u00b7nacht\u00b7sam", "durch\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PAV", "$,", "KOUS", "PRF", "PIS", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "menge.", "tokens": ["men\u00b7ge", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "--", "measure": "unknown.measure.zero"}}, "stanza.4": {"line.1": {"text": "Der Bissen Zahl, wodurch der Mensch sich n\u00e4hrt,", "tokens": ["Der", "Bis\u00b7sen", "Zahl", ",", "wo\u00b7durch", "der", "Mensch", "sich", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Berechnest du, eh er sie isset,", "tokens": ["Be\u00b7rech\u00b7nest", "du", ",", "eh", "er", "sie", "is\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Gl\u00fcckliche, was Jeden widerf\u00e4hrt,", "tokens": ["Das", "Gl\u00fcck\u00b7li\u00b7che", ",", "was", "Je\u00b7den", "wi\u00b7der\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist so, wie es dein Vorsehn misset;", "tokens": ["Ist", "so", ",", "wie", "es", "dein", "Vor\u00b7sehn", "mis\u00b7set", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Des Geistes Zier, der innern Gaben Zahl", "tokens": ["Des", "Geis\u00b7tes", "Zier", ",", "der", "in\u00b7nern", "Ga\u00b7ben", "Zahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Theilt die Natur nach deinem Wollen", "tokens": ["Theilt", "die", "Na\u00b7tur", "nach", "dei\u00b7nem", "Wol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf dein Gehei\u00df, nach deiner weisen Wahl", "tokens": ["Auf", "dein", "Ge\u00b7hei\u00df", ",", "nach", "dei\u00b7ner", "wei\u00b7sen", "Wahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mu\u00df die Vernunft uns G\u00fcter zollen;", "tokens": ["Mu\u00df", "die", "Ver\u00b7nunft", "uns", "G\u00fc\u00b7ter", "zol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dem giebt sie viel des innerlichen Lichts,", "tokens": ["Dem", "giebt", "sie", "viel", "des", "in\u00b7ner\u00b7li\u00b7chen", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und Jenem mittelm\u00e4\u00dfge Strahlen,", "tokens": ["Und", "Je\u00b7nem", "mit\u00b7tel\u00b7m\u00e4\u00df\u00b7ge", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der andre sp\u00fchrt vom heitern Witze nichts,", "tokens": ["Der", "and\u00b7re", "sp\u00fchrt", "vom", "hei\u00b7tern", "Wit\u00b7ze", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und dieser kann uns die Natur in ihrer ganzen", "tokens": ["Und", "die\u00b7ser", "kann", "uns", "die", "Na\u00b7tur", "in", "ih\u00b7rer", "gan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VMFIN", "PPER", "ART", "NN", "