{"textgrid.poem.48606": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "17. An Herrn Magnus Schuwarten", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fcnftig will auch ich was melden", "tokens": ["K\u00fcnf\u00b7tig", "will", "auch", "ich", "was", "mel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "ADV", "PPER", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "von den Taten unsrer Zeit", "tokens": ["von", "den", "Ta\u00b7ten", "uns\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und die ritterlichen Helden,", "tokens": ["und", "die", "rit\u00b7ter\u00b7li\u00b7chen", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "so man r\u00fchmet weit und breit,", "tokens": ["so", "man", "r\u00fch\u00b7met", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "in ein solches Buch verleiben,", "tokens": ["in", "ein", "sol\u00b7ches", "Buch", "ver\u00b7lei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df sie sollen ewig bleiben.", "tokens": ["da\u00df", "sie", "sol\u00b7len", "e\u00b7wig", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ph\u00f6bus hat mir Dint' und Feder", "tokens": ["Ph\u00f6\u00b7bus", "hat", "mir", "Dint'", "und", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schon gegeben in die Hand,", "tokens": ["schon", "ge\u00b7ge\u00b7ben", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klio solch Papyr und Leder,", "tokens": ["Klio", "solch", "Pa\u00b7pyr", "und", "Le\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "das nur denen ist bekant,", "tokens": ["das", "nur", "de\u00b7nen", "ist", "be\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "die von solchen Sachen handeln,", "tokens": ["die", "von", "sol\u00b7chen", "Sa\u00b7chen", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die sich nimmermehr verwandeln.", "tokens": ["die", "sich", "nim\u00b7mer\u00b7mehr", "ver\u00b7wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Erstlich werd' ich m\u00fcssen singen", "tokens": ["Erst\u00b7lich", "werd'", "ich", "m\u00fcs\u00b7sen", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "von des stolzen ", "tokens": ["von", "des", "stol\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "welcher ihm mit seinen Dingen", "tokens": ["wel\u00b7cher", "ihm", "mit", "sei\u00b7nen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "selbst den Untergang gesucht,", "tokens": ["selbst", "den", "Un\u00b7ter\u00b7gang", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "welcher sich und uns betr\u00fcbte,", "tokens": ["wel\u00b7cher", "sich", "und", "uns", "be\u00b7tr\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "als er fremde Bulschaft liebte.", "tokens": ["als", "er", "frem\u00b7de", "Bul\u00b7schaft", "lieb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nachmals wird berichtet werden,", "tokens": ["Nach\u00b7mals", "wird", "be\u00b7rich\u00b7tet", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie der gro\u00dfe ", "tokens": ["wie", "der", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "auf ein neues Werk geriet,", "tokens": ["auf", "ein", "neu\u00b7es", "Werk", "ge\u00b7riet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df er, da er Fremde plagte,", "tokens": ["da\u00df", "er", ",", "da", "er", "Frem\u00b7de", "plag\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "seine Liebste selbst verjagte.", "tokens": ["sei\u00b7ne", "Liebs\u00b7te", "selbst", "ver\u00b7jag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "doch des Ungl\u00fccks stetes Ziel,", "tokens": ["doch", "des", "Un\u00b7gl\u00fccks", "ste\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "die so manchen Ritters-S\u00f6hnen", "tokens": ["die", "so", "man\u00b7chen", "Rit\u00b7ter\u00b7sS\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wegen Sch\u00f6nheit wol gefiel,", "tokens": ["we\u00b7gen", "Sch\u00f6n\u00b7heit", "wol", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "werd' ich auch erraten k\u00f6nnen", "tokens": ["werd'", "ich", "auch", "er\u00b7ra\u00b7ten", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "VMINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "aller deiner Buler Sinnen?", "tokens": ["al\u00b7ler", "dei\u00b7ner", "Bu\u00b7ler", "Sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie beherzte deine Sache", "tokens": ["Wie", "be\u00b7herz\u00b7te", "dei\u00b7ne", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "der getreue ", "tokens": ["der", "ge\u00b7treu\u00b7e"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "wie auch mit gerechter Rache", "tokens": ["wie", "auch", "mit", "ge\u00b7rech\u00b7ter", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "schwanger gieng der ", "tokens": ["schwan\u00b7ger", "gieng", "der"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "wie die ungetreue ", "tokens": ["wie", "die", "un\u00b7ge\u00b7treu\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "durch ihr eignes Urteil fiele?", "tokens": ["durch", "ihr", "eig\u00b7nes", "Ur\u00b7teil", "fie\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hier wird man vor Andern h\u00f6ren", "tokens": ["Hier", "wird", "man", "vor", "An\u00b7dern", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "ADJA", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "den ber\u00fchmten Held ", "tokens": ["den", "be\u00b7r\u00fchm\u00b7ten", "Held"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "der von wegen deiner Ehren", "tokens": ["der", "von", "we\u00b7gen", "dei\u00b7ner", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "m\u00e4nlich wagte seine Brust,", "tokens": ["m\u00e4n\u00b7lich", "wag\u00b7te", "sei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "der, da er dir Fried' erwurbe,", "tokens": ["der", ",", "da", "er", "dir", "Fried'", "er\u00b7wur\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "ritterlich zu Felde sturbe.", "tokens": ["rit\u00b7ter\u00b7lich", "zu", "Fel\u00b7de", "stur\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "An der Seiten soll ihm stehen", "tokens": ["An", "der", "Sei\u00b7ten", "soll", "ihm", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "der den ", "tokens": ["der", "den"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "wie er schon Beweis getan,", "tokens": ["wie", "er", "schon", "Be\u00b7weis", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "der demnach des Himmels Willen", "tokens": ["der", "dem\u00b7nach", "des", "Him\u00b7mels", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PAV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "alles Urteil ganz soll stillen.", "tokens": ["al\u00b7les", "Ur\u00b7teil", "ganz", "soll", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Lasse mich n\u00e4chst nach dir gehen,", "tokens": ["Las\u00b7se", "mich", "n\u00e4chst", "nach", "dir", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Meine ", "tokens": ["Mei\u00b7ne"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "neben deiner ", "tokens": ["ne\u00b7ben", "dei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "das so ferne wird gelesen,", "tokens": ["das", "so", "fer\u00b7ne", "wird", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "wird mir Gott das Gl\u00fccke geben", "tokens": ["wird", "mir", "Gott", "das", "Gl\u00fc\u00b7cke", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und mir Einen wecken auf,", "tokens": ["und", "mir", "Ei\u00b7nen", "we\u00b7cken", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "der mich l\u00e4\u00dft in Ruhe leben", "tokens": ["der", "mich", "l\u00e4\u00dft", "in", "Ru\u00b7he", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und der Sachen ihren Lauf", "tokens": ["und", "der", "Sa\u00b7chen", "ih\u00b7ren", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "durch die Westen milder Sinnen", "tokens": ["durch", "die", "Wes\u00b7ten", "mil\u00b7der", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und der Freiheit wird verg\u00fcnnen.", "tokens": ["und", "der", "Frei\u00b7heit", "wird", "ver\u00b7g\u00fcn\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}