{"textgrid.poem.57798": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die freie Pirsch", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der L\u00fcneburger Heide", "tokens": ["Auf", "der", "L\u00fc\u00b7ne\u00b7bur\u00b7ger", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht der Wind die kreuz die quer,", "tokens": ["Geht", "der", "Wind", "die", "kreuz", "die", "quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der L\u00fcneburger Heide", "tokens": ["Auf", "der", "L\u00fc\u00b7ne\u00b7bur\u00b7ger", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jag ich hin und jag ich her.", "tokens": ["Jag", "ich", "hin", "und", "jag", "ich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An die hundert gr\u00fcne J\u00e4ger", "tokens": ["An", "die", "hun\u00b7dert", "gr\u00fc\u00b7ne", "J\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden nicht des Lebens froh,", "tokens": ["Wer\u00b7den", "nicht", "des", "Le\u00b7bens", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn Pa\u00dfupp so hei\u00dft mein Leithund,", "tokens": ["Denn", "Pa\u00b7\u00dfupp", "so", "hei\u00dft", "mein", "Leit\u00b7hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und mein Schwei\u00dfhund hei\u00dft Wahrtoo.", "tokens": ["Und", "mein", "Schwei\u00df\u00b7hund", "hei\u00dft", "Wahr\u00b7too", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "--+-++-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Wenn die lauten Hunde jagen,", "tokens": ["Wenn", "die", "lau\u00b7ten", "Hun\u00b7de", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4hrt der Fuchs zum Baue ein,", "tokens": ["F\u00e4hrt", "der", "Fuchs", "zum", "Bau\u00b7e", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in jedem dritten Dorfe", "tokens": ["Und", "in", "je\u00b7dem", "drit\u00b7ten", "Dor\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein wacker M\u00e4dchen mein.", "tokens": ["Ist", "ein", "wa\u00b7cker", "M\u00e4d\u00b7chen", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Heute die und morgen jene,", "tokens": ["Heu\u00b7te", "die", "und", "mor\u00b7gen", "je\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "KON", "ADV", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heut ein Rehbock, dann ein Hirsch,", "tokens": ["Heut", "ein", "Reh\u00b7bock", ",", "dann", "ein", "Hirsch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosen bl\u00fchn in jedem Garten,", "tokens": ["Ro\u00b7sen", "bl\u00fchn", "in", "je\u00b7dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberall ist frei die Pirsch.", "tokens": ["\u00dc\u00b7be\u00b7rall", "ist", "frei", "die", "Pirsch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}