{"dta.poem.900": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "VeRKL\u00c4RTER HERBST", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Gewaltig endet so das Jahr", "tokens": ["Ge\u00b7wal\u00b7tig", "en\u00b7det", "so", "das", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit goldnem Wein und Frucht der G\u00e4rten.", "tokens": ["Mit", "gold\u00b7nem", "Wein", "und", "Frucht", "der", "G\u00e4r\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Rund schweigen W\u00e4lder wunderbar", "tokens": ["Rund", "schwei\u00b7gen", "W\u00e4l\u00b7der", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und sind des Einsamen Gef\u00e4hrten.", "tokens": ["Und", "sind", "des", "Ein\u00b7sa\u00b7men", "Ge\u00b7f\u00e4hr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sagt der Landmann: Es ist gut.", "tokens": ["Da", "sagt", "der", "Land\u00b7mann", ":", "Es", "ist", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Abendglocken lang und leise", "tokens": ["Ihr", "A\u00b7bend\u00b7glo\u00b7cken", "lang", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gebt noch zum Ende frohen Mut.", "tokens": ["Gebt", "noch", "zum", "En\u00b7de", "fro\u00b7hen", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Vogelzug gr\u00fc\u00dft auf der Reise.", "tokens": ["Ein", "Vo\u00b7gel\u00b7zug", "gr\u00fc\u00dft", "auf", "der", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist der Liebe milde Zeit.", "tokens": ["Es", "ist", "der", "Lie\u00b7be", "mil\u00b7de", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Kahn den blauen Flu\u00df hinunter", "tokens": ["Im", "Kahn", "den", "blau\u00b7en", "Flu\u00df", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "APPO"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sch\u00f6n sich Bild an Bildchen reiht \u2013", "tokens": ["Wie", "sch\u00f6n", "sich", "Bild", "an", "Bild\u00b7chen", "reiht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das geht in Ruh und Schweigen unter.", "tokens": ["Das", "geht", "in", "Ruh", "und", "Schwei\u00b7gen", "un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "APPR", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}