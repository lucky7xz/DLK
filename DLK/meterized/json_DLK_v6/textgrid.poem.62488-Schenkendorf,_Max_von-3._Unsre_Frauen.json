{"textgrid.poem.62488": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "3. Unsre Frauen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vieles hat die Zeit vernichtet", "tokens": ["Vie\u00b7les", "hat", "die", "Zeit", "ver\u00b7nich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem starken Riesengang,", "tokens": ["In", "dem", "star\u00b7ken", "Rie\u00b7sen\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vieles hat sich selbst gerichtet,", "tokens": ["Vie\u00b7les", "hat", "sich", "selbst", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als der Donner Gottes klang;", "tokens": ["Als", "der", "Don\u00b7ner", "Got\u00b7tes", "klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vieles ist in Staub zerstoben,", "tokens": ["Vie\u00b7les", "ist", "in", "Staub", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00fcber N\u00e4chte Wahn entschwand,", "tokens": ["Tr\u00fc\u00b7ber", "N\u00e4ch\u00b7te", "Wahn", "ent\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eines hat sich rein erhoben", "tokens": ["Ei\u00b7nes", "hat", "sich", "rein", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PRF", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus dem allgemeinen Brand.", "tokens": ["Aus", "dem", "all\u00b7ge\u00b7mei\u00b7nen", "Brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Einen Altar auserlesen,", "tokens": ["Ei\u00b7nen", "Al\u00b7tar", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Einen Tempel sel'ger Lust,", "tokens": ["Ei\u00b7nen", "Tem\u00b7pel", "sel'\u00b7ger", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Hatte sich das deutsche Wesen", "tokens": ["Hat\u00b7te", "sich", "das", "deut\u00b7sche", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "L\u00e4ngst in keuscher Frauenbrust.", "tokens": ["L\u00e4ngst", "in", "keu\u00b7scher", "Frau\u00b7en\u00b7brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In des Heidenthumes N\u00e4chten", "tokens": ["In", "des", "Hei\u00b7den\u00b7thu\u00b7mes", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "That sich schon durch Frauenmund", "tokens": ["That", "sich", "schon", "durch", "Frau\u00b7en\u00b7mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fchnes Wort von ew'gen Rechten", "tokens": ["K\u00fch\u00b7nes", "Wort", "von", "ew'\u00b7gen", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von ew'ger Sch\u00f6nheit kund.", "tokens": ["Und", "von", "ew'\u00b7ger", "Sch\u00f6n\u00b7heit", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Frauen frei und ohne Makel", "tokens": ["Frau\u00b7en", "frei", "und", "oh\u00b7ne", "Ma\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlossen stets den Schlachtenreihn,", "tokens": ["Schlos\u00b7sen", "stets", "den", "Schlach\u00b7ten\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Frauen sprachen das Orakel", "tokens": ["Frau\u00b7en", "spra\u00b7chen", "das", "O\u00b7ra\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In dem alten Eichenhain.", "tokens": ["In", "dem", "al\u00b7ten", "Ei\u00b7chen\u00b7hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Aus den Eichen wurden S\u00e4ulen", "tokens": ["Aus", "den", "Ei\u00b7chen", "wur\u00b7den", "S\u00e4u\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "F\u00fcr das deutsche Gotteshaus,", "tokens": ["F\u00fcr", "das", "deut\u00b7sche", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Drin, der V\u00f6lker Weh zu heilen,", "tokens": ["Drin", ",", "der", "V\u00f6l\u00b7ker", "Weh", "zu", "hei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Go\u00df ein Gnadenbrunn sich aus.", "tokens": ["Go\u00df", "ein", "Gna\u00b7den\u00b7brunn", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Reich erf\u00fcllt war nun das Hoffen,", "tokens": ["Reich", "er\u00b7f\u00fcllt", "war", "nun", "das", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnell gestillt ein hei\u00dfes Flehn,", "tokens": ["Schnell", "ge\u00b7stillt", "ein", "hei\u00b7\u00dfes", "Flehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und man sah den Himmel offen,", "tokens": ["Und", "man", "sah", "den", "Him\u00b7mel", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah die Mutter Gottes stehn.", "tokens": ["Sah", "die", "Mut\u00b7ter", "Got\u00b7tes", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Feinde wurden Bundsgenossen,", "tokens": ["Fein\u00b7de", "wur\u00b7den", "Bunds\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ritter dienten fromm und mild,", "tokens": ["Rit\u00b7ter", "dien\u00b7ten", "fromm", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ueber Schwertern und Geschossen", "tokens": ["Ue\u00b7ber", "Schwer\u00b7tern", "und", "Ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Waltete ein Friedensbild.", "tokens": ["Wal\u00b7te\u00b7te", "ein", "Frie\u00b7dens\u00b7bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Friedensbild, dein Zeichen tragen", "tokens": ["Frie\u00b7dens\u00b7bild", ",", "dein", "Zei\u00b7chen", "tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Unsre Frauen heute noch,", "tokens": ["Uns\u00b7re", "Frau\u00b7en", "heu\u00b7te", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie in heil'ger Vorzeit Tagen,", "tokens": ["Wie", "in", "heil'\u00b7ger", "Vor\u00b7zeit", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "L\u00f6sen sie das schwerste Joch.", "tokens": ["L\u00f6\u00b7sen", "sie", "das", "schwers\u00b7te", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Als die schlechte Zeit gekommen", "tokens": ["Als", "die", "schlech\u00b7te", "Zeit", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die welsche Raserei,", "tokens": ["Und", "die", "wel\u00b7sche", "Ra\u00b7se\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blieben unsre Klaren, Frommen", "tokens": ["Blie\u00b7ben", "uns\u00b7re", "Kla\u00b7ren", ",", "From\u00b7men"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem b\u00f6sen Taumel frei.", "tokens": ["Von", "dem", "b\u00f6\u00b7sen", "Tau\u00b7mel", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00e4nnernacken war gebogen,", "tokens": ["M\u00e4n\u00b7ner\u00b7na\u00b7cken", "war", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stolzgewappnet Frauenbrust,", "tokens": ["Stolz\u00b7ge\u00b7wapp\u00b7net", "Frau\u00b7en\u00b7brust", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die deutschen Knaben sogen", "tokens": ["Und", "die", "deut\u00b7schen", "Kna\u00b7ben", "so\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Feindesha\u00df und Freiheitslust.", "tokens": ["Fein\u00b7des\u00b7ha\u00df", "und", "Frei\u00b7heits\u00b7lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Neiget euch den fr\u00f6mmsten Werken,", "tokens": ["Nei\u00b7get", "euch", "den", "fr\u00f6mms\u00b7ten", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Euch dem sch\u00f6nen Liebestod,", "tokens": ["Euch", "dem", "sch\u00f6\u00b7nen", "Lie\u00b7be\u00b7stod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Seht sie wunde Krieger st\u00e4rken", "tokens": ["Seht", "sie", "wun\u00b7de", "Krie\u00b7ger", "st\u00e4r\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mit dem Wort, mit Wein und Brod.", "tokens": ["Mit", "dem", "Wort", ",", "mit", "Wein", "und", "Brod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwei vor Allen, zwei vor Allen", "tokens": ["Zwei", "vor", "Al\u00b7len", ",", "zwei", "vor", "Al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "APPR", "NE", "$,", "CARD", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind es, die der S\u00e4nger nennt,", "tokens": ["Sind", "es", ",", "die", "der", "S\u00e4n\u00b7ger", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, zwei sel'ge Geister, wallen", "tokens": ["Die", ",", "zwei", "sel'\u00b7ge", "Geis\u00b7ter", ",", "wal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "$,", "CARD", "ADJA", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beide jetzt am Firmament.", "tokens": ["Bei\u00b7de", "jetzt", "am", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine hat im Sturmestoben", "tokens": ["Ei\u00b7ne", "hat", "im", "Stur\u00b7me\u00b7sto\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4ngst ihr sch\u00f6nes Haupt gesenkt,", "tokens": ["L\u00e4ngst", "ihr", "sch\u00f6\u00b7nes", "Haupt", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat, ein Gnadenbild, von oben", "tokens": ["Hat", ",", "ein", "Gna\u00b7den\u00b7bild", ",", "von", "o\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "ART", "NN", "$,", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihrer V\u00f6lker Zug gelenkt.", "tokens": ["Ih\u00b7rer", "V\u00f6l\u00b7ker", "Zug", "ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Doch die Reine, Hohe, Zweite", "tokens": ["Doch", "die", "Rei\u00b7ne", ",", "Ho\u00b7he", ",", "Zwei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Stand im hei\u00dfen M\u00e4nnerstreit,", "tokens": ["Stand", "im", "hei\u00b7\u00dfen", "M\u00e4n\u00b7ner\u00b7streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Freiheitskampf und Siegsgel\u00e4ute", "tokens": ["Frei\u00b7heits\u00b7kampf", "und", "Siegs\u00b7ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hat ihr k\u00fchnes Herz erfreut.", "tokens": ["Hat", "ihr", "k\u00fch\u00b7nes", "Herz", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Glorreich ist auch sie erhoben", "tokens": ["Glor\u00b7reich", "ist", "auch", "sie", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "