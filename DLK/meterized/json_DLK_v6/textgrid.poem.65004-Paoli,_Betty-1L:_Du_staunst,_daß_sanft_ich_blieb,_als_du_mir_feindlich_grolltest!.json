{"textgrid.poem.65004": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du staunst, da\u00df sanft ich blieb, als du mir feindlich grolltest!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du staunst, da\u00df sanft ich blieb, als du mir feindlich grolltest!", "tokens": ["Du", "staunst", ",", "da\u00df", "sanft", "ich", "blieb", ",", "als", "du", "mir", "feind\u00b7lich", "groll\u00b7test", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab ich dich denn geliebt, da\u00df du mich lieben solltest?", "tokens": ["Hab", "ich", "dich", "denn", "ge\u00b7liebt", ",", "da\u00df", "du", "mich", "lie\u00b7ben", "soll\u00b7test", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "VVPP", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich habe dich geliebt, weil's so geheischt mein Ich,", "tokens": ["Ich", "ha\u00b7be", "dich", "ge\u00b7liebt", ",", "weil's", "so", "ge\u00b7heischt", "mein", "Ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PRELS", "ADV", "VVFIN", "PPOSAT", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und blieb dir standhaft treu aus Treue gegen mich.", "tokens": ["Und", "blieb", "dir", "stand\u00b7haft", "treu", "aus", "Treu\