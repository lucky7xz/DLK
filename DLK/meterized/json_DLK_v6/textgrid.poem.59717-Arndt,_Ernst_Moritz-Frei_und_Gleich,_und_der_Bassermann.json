n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der edle Hort von Frei und Gleich,", "tokens": ["Der", "ed\u00b7le", "Hort", "von", "Frei", "und", "Gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das Gold, der Glanz vom Deutschen Reich.", "tokens": ["Das", "Gold", ",", "der", "Glanz", "vom", "Deut\u00b7schen", "Reich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Blast, blast, Trompeten! Blast dem Mann,", "tokens": ["Blast", ",", "blast", ",", "Trom\u00b7pe\u00b7ten", "!", "Blast", "dem", "Mann", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$.", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Des Hand zum gro\u00dfen Fund sich reckte,", "tokens": ["Des", "Hand", "zum", "gro\u00b7\u00dfen", "Fund", "sich", "reck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem deutschen Mann, dem Bassermann,", "tokens": ["Dem", "deut\u00b7schen", "Mann", ",", "dem", "Bas\u00b7ser\u00b7mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wie aus b\u00f6sem Schlaf uns weckte.", "tokens": ["Der", "wie", "aus", "b\u00f6\u00b7sem", "Schlaf", "uns", "weck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er spricht den k\u00fchnen Zauberspruch,", "tokens": ["Er", "spricht", "den", "k\u00fch\u00b7nen", "Zau\u00b7ber\u00b7spruch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und flugs entsteigt der Hort den Tiefen,", "tokens": ["Und", "flugs", "ent\u00b7steigt", "der", "Hort", "den", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Geister wie aus einem Buch,", "tokens": ["Und", "Geis\u00b7ter", "wie", "aus", "ei\u00b7nem", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Millionen Geister, welche schliefen,", "tokens": ["Mil\u00b7lion\u00b7en", "Geis\u00b7ter", ",", "wel\u00b7che", "schlie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Unisono von Frei und Gleich,", "tokens": ["U\u00b7ni\u00b7so\u00b7no", "von", "Frei", "und", "Gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Das singen sie und Deutsches Reich.", "tokens": ["Das", "sin\u00b7gen",