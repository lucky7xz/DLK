{"text": "Mit seiner Gro\u00dfmama, der L\u00fcge;", "tokens": ["Mit", "sei\u00b7ner", "Gro\u00df\u00b7ma\u00b7ma", ",", "der", "L\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Schaut, wie der freie deutsche Aar", "tokens": ["Schaut", ",", "wie", "der", "frei\u00b7e", "deut\u00b7sche", "Aar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Froh fliegt die alten Sonnenfl\u00fcge,", "tokens": ["Froh", "fliegt", "die", "al\u00b7ten", "Son\u00b7nen\u00b7fl\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Er klingt und singt sein Frei und Gleich,", "tokens": ["Er", "klingt", "und", "singt", "sein", "Frei", "und", "Gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er klingt und singt vom Deutschen Reich.", "tokens": ["Er", "klingt", "und", "singt", "vom", "Deut\u00b7schen", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Blast, blast, Trompeten! La\u00dft den Klang", "tokens": ["Blast", ",", "blast", ",", "Trom\u00b7pe\u00b7ten", "!", "La\u00dft", "den", "Klang"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", 