{"dta.poem.19464": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Mit edeln Purpurr\u00f6then", "tokens": ["Mit", "e\u00b7deln", "Pur\u00b7pur\u00b7r\u00f6\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hellem Amselschlag,", "tokens": ["Und", "hel\u00b7lem", "Am\u00b7sel\u00b7schlag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Rosen und mit Fl\u00f6ten", "tokens": ["Mit", "Ro\u00b7sen", "und", "mit", "Fl\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stolziert der junge Tag.", "tokens": ["Stol\u00b7ziert", "der", "jun\u00b7ge", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Wanderschritt des Lebens", "tokens": ["Der", "Wan\u00b7der\u00b7schritt", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist noch ein leichter Tanz,", "tokens": ["Ist", "noch", "ein", "leich\u00b7ter", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich gehe wie im Reigen", "tokens": ["Ich", "ge\u00b7he", "wie", "im", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit einem frischen Kranz.", "tokens": ["Mit", "ei\u00b7nem", "fri\u00b7schen", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr thaubenetzten Kr\u00e4nze", "tokens": ["Ihr", "thau\u00b7be\u00b7netz\u00b7ten", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der neuen Morgenkraft,", "tokens": ["Der", "neu\u00b7en", "Mor\u00b7gen\u00b7kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geworfen aus den L\u00fcften", "tokens": ["Ge\u00b7wor\u00b7fen", "aus", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und spielend aufgerafft \u2014", "tokens": ["Und", "spie\u00b7lend", "auf\u00b7ge\u00b7rafft"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl manchen lie\u00df ich welken", "tokens": ["Wohl", "man\u00b7chen", "lie\u00df", "ich", "wel\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch vor der Mittagsglut;", "tokens": ["Noch", "vor", "der", "Mit\u00b7tags\u00b7glut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zerrissen hab' ich manchen", "tokens": ["Zer\u00b7ris\u00b7sen", "hab'", "ich", "man\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus reinem Uebermut!", "tokens": ["Aus", "rei\u00b7nem", "Ue\u00b7ber\u00b7mut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit edeln Purpurr\u00f6then", "tokens": ["Mit", "e\u00b7deln", "Pur\u00b7pur\u00b7r\u00f6\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hellem Amselschlag,", "tokens": ["Und", "hel\u00b7lem", "Am\u00b7sel\u00b7schlag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Rosen und mit Fl\u00f6ten", "tokens": ["Mit", "Ro\u00b7sen", "und", "mit", "Fl\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stolziert der junge Tag \u2014", "tokens": ["Stol\u00b7ziert", "der", "jun\u00b7ge", "Tag"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hinweg, du dunkle Klage,", "tokens": ["Hin\u00b7weg", ",", "du", "dunk\u00b7le", "Kla\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus all dem Licht und Glanz!", "tokens": ["Aus", "all", "dem", "Licht", "und", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Schmerz verlorner Tage", "tokens": ["Den", "Schmerz", "ver\u00b7lor\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bedeckt ein frischer Kranz.", "tokens": ["Be\u00b7deckt", "ein", "fri\u00b7scher", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}