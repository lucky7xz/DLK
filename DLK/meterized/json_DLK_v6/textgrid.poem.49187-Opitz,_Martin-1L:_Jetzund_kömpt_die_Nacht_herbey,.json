{"textgrid.poem.49187": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzund k\u00f6mpt die Nacht herbey,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzund k\u00f6mpt die Nacht herbey,", "tokens": ["Je\u00b7tzund", "k\u00f6mpt", "die", "Nacht", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vieh und Menschen werden frey,", "tokens": ["Vieh", "und", "Men\u00b7schen", "wer\u00b7den", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die gew\u00fcnschte Ruhe geht an;", "tokens": ["Die", "ge\u00b7w\u00fcnschte", "Ru\u00b7he", "geht", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Meine Sorge k\u00f6mpt heran.", "tokens": ["Mei\u00b7ne", "Sor\u00b7ge", "k\u00f6mpt", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6ne gl\u00e4ntzt der Mondenschein", "tokens": ["Sch\u00f6\u00b7ne", "gl\u00e4ntzt", "der", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die g\u00fclden Sternelein;", "tokens": ["Und", "die", "g\u00fcl\u00b7den", "Ster\u00b7ne\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Froh ist alles weit und breit,", "tokens": ["Froh", "ist", "al\u00b7les", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich nur bin in Traurigkeit.", "tokens": ["Ich", "nur", "bin", "in", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zweene mangeln \u00fcberall", "tokens": ["Zwee\u00b7ne", "man\u00b7geln", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An der sch\u00f6nen Sternen Zahl;", "tokens": ["An", "der", "sch\u00f6\u00b7nen", "Ster\u00b7nen", "Zahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Sternen, die ich mein',", "tokens": ["Die\u00b7se", "Ster\u00b7nen", ",", "die", "ich", "mein'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Liebsten Augenschein.", "tokens": ["Ist", "der", "Liebs\u00b7ten", "Au\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach dem Monden frag' ich nicht,", "tokens": ["Nach", "dem", "Mon\u00b7den", "frag'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tunckel ist der Sternen Liecht,", "tokens": ["Tun\u00b7ckel", "ist", "der", "Ster\u00b7nen", "Liecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil sich von mir weggewendt", "tokens": ["Weil", "sich", "von", "mir", "weg\u00b7ge\u00b7wendt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Asteris, mein Firmament.", "tokens": ["As\u00b7te\u00b7ris", ",", "mein", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wann sich aber neigt zu mir", "tokens": ["Wann", "sich", "a\u00b7ber", "neigt", "zu", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADV", "VVFIN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser meiner Sonnen Ziehr,", "tokens": ["Die\u00b7ser", "mei\u00b7ner", "Son\u00b7nen", "Ziehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Acht' ich es das beste seyn,", "tokens": ["Acht'", "ich", "es", "das", "bes\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df kein Stern noch Monde schein.", "tokens": ["Da\u00df", "kein", "Stern", "noch", "Mon\u00b7de", "schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}