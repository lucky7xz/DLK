{"textgrid.poem.33305": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Die Verwandlung", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wundert dich, da\u00df ein so garstig Ding,", "tokens": ["Es", "wun\u00b7dert", "dich", ",", "da\u00df", "ein", "so", "gars\u00b7tig", "Ding", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ART", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als eine Raupe ist, zum sch\u00f6nsten Schmetterling", "tokens": ["Als", "ei\u00b7ne", "Rau\u00b7pe", "ist", ",", "zum", "sch\u00f6ns\u00b7ten", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In wenig Wochen wird: \u2013 mich wundert's nicht;", "tokens": ["In", "we\u00b7nig", "Wo\u00b7chen", "wird", ":", "\u2013", "mich", "wun\u00b7dert's", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "$.", "$(", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn wiss', auch manche Sch\u00f6ne kriecht", "tokens": ["Denn", "wiss'", ",", "auch", "man\u00b7che", "Sch\u00f6\u00b7ne", "kriecht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als Raupe Morgens aus dem Bette,", "tokens": ["Als", "Rau\u00b7pe", "Mor\u00b7gens", "aus", "dem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00f6mmt als Schmetterling von der Toilette.", "tokens": ["Und", "k\u00f6mmt", "als", "Schmet\u00b7ter\u00b7ling", "von", "der", "To\u00b7i\u00b7let\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}