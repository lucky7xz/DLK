{"dta.poem.9931": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine\u0303 ursprung nehm ich erst zwischen zwey behaarten beine\u0303/", "tokens": ["Mein\u1ebd", "ur\u00b7sprung", "nehm", "ich", "erst", "zwi\u00b7schen", "zwey", "be\u00b7haar\u00b7ten", "bein\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "CARD", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Pflege da so sch\u00f6n und wei\u00df/ als der schnee wohl ist/ zu scheinen:", "tokens": ["Pfle\u00b7ge", "da", "so", "sch\u00f6n", "und", "wei\u00df", "/", "als", "der", "schnee", "wohl", "ist", "/", "zu", "schei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJD", "KON", "VVFIN", "$(", "KOUS", "ART", "NN", "ADV", "VAFIN", "$(", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Drauf so sperrt man mich in l\u00f6cher/ wo man mich offt st\u00f6\u00dft und", "tokens": ["Drauf", "so", "sperrt", "man", "mich", "in", "l\u00f6\u00b7cher", "/", "wo", "man", "mich", "offt", "st\u00f6\u00dft", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "PIS", "PRF", "APPR", "NE", "$(", "PWAV", "PIS", "PRF", "ADV", "VVFIN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "schl\u00e4gt/", "tokens": ["schl\u00e4gt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Bi\u00df mein leib f\u00fcr seiner weisse/ gelbe farben an sich tr\u00e4gt.", "tokens": ["Bi\u00df", "mein", "leib", "f\u00fcr", "sei\u00b7ner", "weis\u00b7se", "/", "gel\u00b7be", "far\u00b7ben", "an", "sich", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "$(", "ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "(schlagen/", "tokens": ["(", "schla\u00b7gen", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}}}}}