{"dta.poem.2510": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er ihr das im Schertze abgenom-  \n mene Schnuptuch wieder \u00fcber-  \n sendete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Geh hin du sch\u00f6nes Tuch zu meiner K\u00f6niginnen/", "tokens": ["Geh", "hin", "du", "sch\u00f6\u00b7nes", "Tuch", "zu", "mei\u00b7ner", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dich mit Recht befitzt/ geh hin und meid ihr an", "tokens": ["Die", "dich", "mit", "Recht", "be\u00b7fitzt", "/", "geh", "hin", "und", "meid", "ihr", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "NN", "VVPP", "$(", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich dir solch\u2019 ein\u2019 Ehr vor Sie hab angethan/", "tokens": ["Da\u00df", "ich", "dir", "solch'", "ein'", "Ehr", "vor", "Sie", "hab", "an\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "ART", "NN", "APPR", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sag da\u00df ich dich gek\u00fcsst mit treuverliebten Sinnen/", "tokens": ["Sag", "da\u00df", "ich", "dich", "ge\u00b7k\u00fcsst", "mit", "treu\u00b7ver\u00b7lieb\u00b7ten", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PRF", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu hun dert tausendmal. Wolan/ so lauff dann hin/", "tokens": ["Zu", "hun", "dert", "tau\u00b7send\u00b7mal", ".", "Wo\u00b7lan", "/", "so", "lauff", "dann", "hin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "ADV", "$.", "ADV", "$(", "ADV", "ADJD", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sprich da\u00df ich bi\u00df zum Tod ihr treuer Diener bin.", "tokens": ["Sprich", "da\u00df", "ich", "bi\u00df", "zum", "Tod", "ihr", "treu\u00b7er", "Die\u00b7ner", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}