{"textgrid.poem.36969": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Kater sitzt vor'm dicken Buch", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kater sitzt vor'm dicken Buch", "tokens": ["Ein", "Ka\u00b7ter", "sitzt", "vor'm", "di\u00b7cken", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Brille auf der Nase;", "tokens": ["Die", "Bril\u00b7le", "auf", "der", "Na\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man sieht's, er denkt gewaltig klug", "tokens": ["Man", "sieht's", ",", "er", "denkt", "ge\u00b7wal\u00b7tig", "klug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob einer dunklen Phrase.", "tokens": ["Ob", "ei\u00b7ner", "dunk\u00b7len", "Phra\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er zieht die Stirne kraus und krumm,", "tokens": ["Er", "zieht", "die", "Stir\u00b7ne", "kraus", "und", "krumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Legt sie in hundert Falten;", "tokens": ["Legt", "sie", "in", "hun\u00b7dert", "Fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es geht ihm Viel im Kopf herum,", "tokens": ["Es", "geht", "ihm", "Viel", "im", "Kopf", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn er studirt die Alten.", "tokens": ["Denn", "er", "stu\u00b7dirt", "die", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Die sch\u00f6nsten Frauchen schleichen dort", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Frau\u00b7chen", "schlei\u00b7chen", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am stillen Schornsteinplatze;", "tokens": ["Am", "stil\u00b7len", "Schorn\u00b7stein\u00b7plat\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie sie miauen fort und fort:", "tokens": ["Wie", "sie", "mi\u00b7au\u00b7en", "fort", "und", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er liebt nicht eine Katze!", "tokens": ["Er", "liebt", "nicht", "ei\u00b7ne", "Kat\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lieb', Freundschaft, Sch\u00f6nheit prallen ab", "tokens": ["Lieb'", ",", "Freund\u00b7schaft", ",", "Sch\u00f6n\u00b7heit", "pral\u00b7len", "ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von seiner Brust, der kalten;", "tokens": ["Von", "sei\u00b7ner", "Brust", ",", "der", "kal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein Inn'res ist ein weites Grab,", "tokens": ["Sein", "Inn'\u00b7res", "ist", "ein", "wei\u00b7tes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drinn spucken nur die Alten.", "tokens": ["Drinn", "spu\u00b7cken", "nur", "die", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er w\u00e4scht sich nicht, er k\u00e4mmt sich nicht;", "tokens": ["Er", "w\u00e4scht", "sich", "nicht", ",", "er", "k\u00e4mmt", "sich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$,", "PPER", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er bleibt in seinem Schmutze:", "tokens": ["Er", "bleibt", "in", "sei\u00b7nem", "Schmut\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was scheert mich mein gemein Gesicht,", "tokens": ["Was", "scheert", "mich", "mein", "ge\u00b7mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich die Seele putze!", "tokens": ["Wenn", "ich", "die", "See\u00b7le", "put\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So schnurrt den Muhmen er Bescheid,", "tokens": ["So", "schnurrt", "den", "Muh\u00b7men", "er", "Be\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ihren Vetter schalten:", "tokens": ["Die", "ih\u00b7ren", "Vet\u00b7ter", "schal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was brauch' ich eure Sauberkeit", "tokens": ["Was", "brauch'", "ich", "eu\u00b7re", "Sau\u00b7ber\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Schattenreich der Alten!", "tokens": ["Im", "Schat\u00b7ten\u00b7reich", "der", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Komm' mit! sagt ihm sein Kamerad,", "tokens": ["Komm'", "mit", "!", "sagt", "ihm", "sein", "Ka\u00b7me\u00b7rad", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hier nebenan im H\u00e4uschen,", "tokens": ["Hier", "ne\u00b7be\u00b7nan", "im", "H\u00e4usc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da schmausen wir ganz delicat,", "tokens": ["Da", "schmau\u00b7sen", "wir", "ganz", "de\u00b7li\u00b7cat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Da gibt's die fett'sten M\u00e4uschen!", "tokens": ["Da", "gibt's", "die", "fett'\u00b7sten", "M\u00e4u\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Kater wirft zwar einen Blick", "tokens": ["Der", "Ka\u00b7ter", "wirft", "zwar", "ei\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch seines Bodens Spalten,", "tokens": ["Durch", "sei\u00b7nes", "Bo\u00b7dens", "Spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch zieht er sich sogleich zur\u00fcck", "tokens": ["Doch", "zieht", "er", "sich", "sog\u00b7leich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hungert bei den Alten.", "tokens": ["Und", "hun\u00b7gert", "bei", "den", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der K\u00f6nig seines Vaterlands,", "tokens": ["Der", "K\u00f6\u00b7nig", "sei\u00b7nes", "Va\u00b7ter\u00b7lands", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist ein arger S\u00fcnder;", "tokens": ["Das", "ist", "ein", "ar\u00b7ger", "S\u00fcn\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00fcrger all' des Katerlands", "tokens": ["Die", "B\u00fcr\u00b7ger", "all'", "des", "Ka\u00b7ter\u00b7lands"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schreien wie die Kinder.", "tokens": ["Sie", "schrei\u00b7en", "wie", "die", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das ganze Reich ist voll Miaus", "tokens": ["Das", "gan\u00b7ze", "Reich", "ist", "voll", "Mi\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ob des Tyrannen Walten:", "tokens": ["Ob", "des", "Ty\u00b7ran\u00b7nen", "Wal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Kater macht sich gar Nichts draus,", "tokens": ["Der", "Ka\u00b7ter", "macht", "sich", "gar", "Nichts", "draus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "PIS", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn er ist bei den Alten.", "tokens": ["Denn", "er", "ist", "bei", "den", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Die Feinde dringen in das Land,", "tokens": ["Die", "Fein\u00b7de", "drin\u00b7gen", "in", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gro\u00dfen Metzgerhunde;", "tokens": ["Die", "gro\u00b7\u00dfen", "Metz\u00b7ger\u00b7hun\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von jeder Mauer, jeder Wand", "tokens": ["Von", "je\u00b7der", "Mau\u00b7er", ",", "je\u00b7der", "Wand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt man die Schreckenskunde;", "tokens": ["H\u00f6rt", "man", "die", "Schre\u00b7cken\u00b7skun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Man zieht die Krallen vor, um", "tokens": ["Man", "zieht", "die", "Kral\u00b7len", "vor", ",", "um"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUI"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "Wauwauer abzuhalten!", "tokens": ["Wau\u00b7wau\u00b7er", "ab\u00b7zu\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nur Einer, das gelehrte Vieh,", "tokens": ["Nur", "Ei\u00b7ner", ",", "das", "ge\u00b7lehr\u00b7te", "Vieh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bleibt ruhig bei den Alten.", "tokens": ["Bleibt", "ru\u00b7hig", "bei", "den", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Im ganzen Reiche rundherum", "tokens": ["Im", "gan\u00b7zen", "Rei\u00b7che", "rund\u00b7he\u00b7rum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Murrt man von ihm am Schlimmsten;", "tokens": ["Murrt", "man", "von", "ihm", "am", "Schlimms\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So manchen Kater nennt man dumm,", "tokens": ["So", "man\u00b7chen", "Ka\u00b7ter", "nennt", "man", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ihn den Allerd\u00fcmmsten:", "tokens": ["Doch", "ihn", "den", "Al\u00b7ler\u00b7d\u00fcmms\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er lachte, sang und liebte nie,", "tokens": ["Er", "lach\u00b7te", ",", "sang", "und", "lieb\u00b7te", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn wir die Lust umkrallten;", "tokens": ["Wenn", "wir", "die", "Lust", "um\u00b7krall\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So la\u00dft denn das gelehrte Vieh", "tokens": ["So", "la\u00dft", "denn", "das", "ge\u00b7lehr\u00b7te", "Vieh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verfaulen bei den Alten!", "tokens": ["Ver\u00b7fau\u00b7len", "bei", "den", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er starb. Kein Kater, keine Katz'", "tokens": ["Er", "starb", ".", "Kein", "Ka\u00b7ter", ",", "kei\u00b7ne", "Katz'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat kl\u00e4glich drob miauet;", "tokens": ["Hat", "kl\u00e4g\u00b7lich", "drob", "mi\u00b7au\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Gegentheil: sein Studienplatz", "tokens": ["Im", "Ge\u00b7gen\u00b7theil", ":", "sein", "Stu\u00b7di\u00b7en\u00b7platz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ward ekelhaft besauet.", "tokens": ["Ward", "e\u00b7kel\u00b7haft", "be\u00b7sau\u00b7et", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Wissen, das mit ihm verscharrt,", "tokens": ["Sein", "Wis\u00b7sen", ",", "das", "mit", "ihm", "ver\u00b7scharrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schrien sie, er mag's behalten!", "tokens": ["Schri\u00b7en", "sie", ",", "er", "mag's", "be\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wir leben in der Gegenwart", "tokens": ["Wir", "le\u00b7ben", "in", "der", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und schnurren auf die Alten!", "tokens": ["Und", "schnur\u00b7ren", "auf", "die", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}