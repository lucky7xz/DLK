{"textgrid.poem.53951": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Jakubowski", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Hand gekrampft um die Barriere,", "tokens": ["Die", "Hand", "ge\u00b7krampft", "um", "die", "Bar\u00b7rie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "so steht der graue Mann, geduckt.", "tokens": ["so", "steht", "der", "grau\u00b7e", "Mann", ",", "ge\u00b7duckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In seinen Augen fragt die Leere \u2013", "tokens": ["In", "sei\u00b7nen", "Au\u00b7gen", "fragt", "die", "Lee\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "man sieht, wie er den Speichel schluckt.", "tokens": ["man", "sieht", ",", "wie", "er", "den", "Spei\u00b7chel", "schluckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm gegen\u00fcber, hoch erhoben,", "tokens": ["Ihm", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ",", "hoch", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ein kalter Stahl, der Staatsanwalt.", "tokens": ["ein", "kal\u00b7ter", "Stahl", ",", "der", "Staats\u00b7an\u00b7walt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es d\u00e4mmern dumpf die andern Roben,", "tokens": ["Es", "d\u00e4m\u00b7mern", "dumpf", "die", "an\u00b7dern", "Ro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geschworne lauschen, Rede knallt:", "tokens": ["Ge\u00b7schwor\u00b7ne", "lau\u00b7schen", ",", "Re\u00b7de", "knallt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbso haben wir den Mord bewiesen \u2013", "tokens": ["\u00bb", "so", "ha\u00b7ben", "wir", "den", "Mord", "be\u00b7wie\u00b7sen", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und jedes Mitleid w\u00e4re schlapp!", "tokens": ["und", "je\u00b7des", "Mit\u00b7leid", "w\u00e4\u00b7re", "schlapp", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich fordere von Ihnen diesen!", "tokens": ["Ich", "for\u00b7de\u00b7re", "von", "Ih\u00b7nen", "die\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PDAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Haun wir ihm den Kopf ab!", "tokens": ["Haun", "wir", "ihm", "den", "Kopf", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Haun wir ihm den Kopf ab!", "tokens": ["Haun", "wir", "ihm", "den", "Kopf", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Haun wir ihm den Kopf ab \u2013!\u00ab", "tokens": ["Haun", "wir", "ihm", "den", "Kopf", "ab", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$(", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Grau-regnerisch w\u00f6lbt sich der Himmel", "tokens": ["Grau\u00b7reg\u00b7ne\u00b7risch", "w\u00f6lbt", "sich", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN"], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "fahl \u00fcber rotem Backsteinbau;", "tokens": ["fahl", "\u00fc\u00b7ber", "ro\u00b7tem", "Back\u00b7stein\u00b7bau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "vom nahen T\u00fcrmchen ein Gebimmel \u2013", "tokens": ["vom", "na\u00b7hen", "T\u00fcrm\u00b7chen", "ein", "Ge\u00b7bim\u00b7mel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die schmutzige Turmuhr zeigt genau", "tokens": ["die", "schmut\u00b7zi\u00b7ge", "Tur\u00b7muhr", "zeigt", "ge\u00b7nau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "halb f\u00fcnf. Es \u00f6ffnet sich die Pforte,", "tokens": ["halb", "f\u00fcnf", ".", "Es", "\u00f6ff\u00b7net", "sich", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "CARD", "$.", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "heraus schwankt ein halbtotes Tier.", "tokens": ["he\u00b7raus", "schwankt", "ein", "halb\u00b7to\u00b7tes", "Tier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Der Mund sagt unh\u00f6rbare Worte . . .", "tokens": ["Der", "Mund", "sagt", "un\u00b7h\u00f6r\u00b7ba\u00b7re", "Wor\u00b7te", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": " \u2013\u00bbNa, wird das! Sind wir alle hier?\u00ab", "tokens": ["\u2013", "\u00bb", "Na", ",", "wird", "das", "!", "Sind", "wir", "al\u00b7le", "hier", "?", "\u00ab"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "ITJ", "$,", "VAFIN", "PDS", "$.", "VAFIN", "PPER", "PIS", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Staatsanwalt h\u00e4lt einen Bogen", "tokens": ["Der", "Staats\u00b7an\u00b7walt", "h\u00e4lt", "ei\u00b7nen", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "und liest gef\u00fchllos, schnell und knapp.", "tokens": ["und", "liest", "ge\u00b7f\u00fchl\u00b7los", ",", "schnell", "und", "knapp", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbund somit \u00fcbergebe ich Sie dem Nachrichter!\u00ab", "tokens": ["\u00bb", "und", "so\u00b7mit", "\u00fc\u00b7ber\u00b7ge\u00b7be", "ich", "Sie", "dem", "Nach\u00b7rich\u00b7ter", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Hier geschieht ein Mord!", "tokens": ["Hier", "ge\u00b7schieht", "ein", "Mord", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Sie hauen ihm den Kopf ab!", "tokens": ["Sie", "hau\u00b7en", "ihm", "den", "Kopf", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Sie hauen ihm den Kopf ab!", "tokens": ["Sie", "hau\u00b7en", "ihm", "den", "Kopf", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Sie hauen ihm den Kopf ab \u2013!", "tokens": ["Sie", "hau\u00b7en", "ihm", "den", "Kopf", "ab", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$(", "$."], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Der K\u00f6rper liegt in fremder Erde.", "tokens": ["Der", "K\u00f6r\u00b7per", "liegt", "in", "frem\u00b7der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Kreuz \u2013 wie einen Hund verscharrt.", "tokens": ["Kein", "Kreuz", "\u2013", "wie", "ei\u00b7nen", "Hund", "ver\u00b7scharrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die schlafen gut. Ein Schrei: \u00bbEs werde", "tokens": ["Die", "schla\u00b7fen", "gut", ".", "Ein", "Schrei", ":", "\u00bb", "Es", "wer\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "$.", "ART", "NN", "$.", "$(", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gerechtigkeit!\u00ab", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "!", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "La\u00dft ihr euch die Justiz gefallen?", "tokens": ["La\u00dft", "ihr", "euch", "die", "Jus\u00b7tiz", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "H\u00e4lt euch ein Korpsstudent zur\u00fcck?", "tokens": ["H\u00e4lt", "euch", "ein", "Korps\u00b7stu\u00b7dent", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Steht denn Justitia \u00fcber allen?", "tokens": ["Steht", "denn", "Jus\u00b7ti\u00b7tia", "\u00fc\u00b7ber", "al\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NE", "APPR", "PIS", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wagt euch an dieses Hurenst\u00fcck!", "tokens": ["Wagt", "euch", "an", "die\u00b7ses", "Hu\u00b7ren\u00b7st\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Soll dieser Jammer niemals enden?", "tokens": ["Soll", "die\u00b7ser", "Jam\u00b7mer", "nie\u00b7mals", "en\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Werft die Figur vom Postament herab!", "tokens": ["Werft", "die", "Fi\u00b7gur", "vom", "Pos\u00b7ta\u00b7ment", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Einen Tritt ins Ges\u00e4\u00df!", "tokens": ["Ei\u00b7nen", "Tritt", "ins", "Ge\u00b7s\u00e4\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Hauen wir ihr den Kopf ab!", "tokens": ["Hau\u00b7en", "wir", "ihr", "den", "Kopf", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Hauen wir ihr den Kopf ab!", "tokens": ["Hau\u00b7en", "wir", "ihr", "den", "Kopf", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Hauen wir ihr den Kopf ab \u2013!", "tokens": ["Hau\u00b7en", "wir", "ihr", "den", "Kopf", "ab", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$(", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}