{"dta.poem.1121": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "7. Die Sch\u00f6ne Gelbs\u00fcchtige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer wirfft dir/ Nymphe/ vor das Sonnenfarbne Kleid/ ", "tokens": ["Wer", "wirfft", "dir", "/", "Nym\u00b7phe", "/", "vor", "das", "Son\u00b7nen\u00b7farb\u00b7ne", "Kleid", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "NN", "$(", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das dein geschickter Hal\u00df und dein Gesichte f\u00fchret/", "tokens": ["das", "dein", "ge\u00b7schick\u00b7ter", "Hal\u00df", "und", "dein", "Ge\u00b7sich\u00b7te", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn sich manch weisser Stern f\u00fcr jener Glantz verlieret?", "tokens": ["Wenn", "sich", "manch", "weis\u00b7ser", "Stern", "f\u00fcr", "je\u00b7ner", "Glantz", "ver\u00b7lie\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist nicht das gelbe Gold der Meister unsrer Zeit?", "tokens": ["Ist", "nicht", "das", "gel\u00b7be", "Gold", "der", "Meis\u00b7ter", "uns\u00b7rer", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bley/ Kupffer/ Eisen bleibt f\u00fcr ihm in Tunckelheit.", "tokens": ["Bley", "/", "Kupf\u00b7fer", "/", "Ei\u00b7sen", "bleibt", "f\u00fcr", "ihm", "in", "Tun\u00b7ckel\u00b7heit", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "VVFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es wird vom gelben Stroh am ehsten Glutt gesp\u00fcret.", "tokens": ["Es", "wird", "vom", "gel\u00b7ben", "Stroh", "am", "ehs\u00b7ten", "Glutt", "ge\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch gelber Rose Pracht der theure Stock gezieret.", "tokens": ["Durch", "gel\u00b7ber", "Ro\u00b7se", "Pracht", "der", "theu\u00b7re", "Stock", "ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "F\u00fchlt wohl der Hagedorn um roth und wei\u00df viel Neid?", "tokens": ["F\u00fchlt", "wohl", "der", "Ha\u00b7ge\u00b7dorn", "um", "roth", "und", "wei\u00df", "viel", "Neid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ADJD", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil schwer-", "tokens": ["Weil", "schwer"], "token_info": ["word", "word"], "pos": ["KOUS", "TRUNC"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Weil man das n\u00fctze Stroh und seltne Rosen liebet/", "tokens": ["Weil", "man", "das", "n\u00fct\u00b7ze", "Stroh", "und", "selt\u00b7ne", "Ro\u00b7sen", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So seh\u2019 ich nicht/ was man dir auszustellen wei\u00df:", "tokens": ["So", "seh'", "ich", "nicht", "/", "was", "man", "dir", "aus\u00b7zu\u00b7stel\u00b7len", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$(", "PWS", "PIS", "PPER", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zumahl noch deinem Ruhm den gr\u00f6sten Ausschlag giebet:", "tokens": ["Zu\u00b7mahl", "noch", "dei\u00b7nem", "Ruhm", "den", "gr\u00f6s\u00b7ten", "Aus\u00b7schlag", "gie\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df Stahl und Gold dein Tranck/ und du/ was andre scheuen/", "tokens": ["Da\u00df", "Stahl", "und", "Gold", "dein", "Tranck", "/", "und", "du", "/", "was", "and\u00b7re", "scheu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPOSAT", "NN", "$(", "KON", "PPER", "$(", "PWS", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gradivens Eisen selbst mit Nutzen kanst verd\u00e4uen.", "tokens": ["Gra\u00b7di\u00b7vens", "Ei\u00b7sen", "selbst", "mit", "Nut\u00b7zen", "kanst", "ver\u00b7d\u00e4u\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}