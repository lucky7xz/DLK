{"textgrid.poem.51579": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht r\u00e4che dich an ihm, wann dich dein Feind wil plagen,", "tokens": ["Nicht", "r\u00e4\u00b7che", "dich", "an", "ihm", ",", "wann", "dich", "dein", "Feind", "wil", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "APPR", "PPER", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du dich \u00fcberwindst, alsbald ist er geschlagen.", "tokens": ["Wenn", "du", "dich", "\u00fc\u00b7berw\u00b7indst", ",", "als\u00b7bald", "ist", "er", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}