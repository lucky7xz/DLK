{"textgrid.poem.25487": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Darf der noch vor dein Antlitz kommen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Darf der noch vor dein Antlitz kommen,", "tokens": ["Darf", "der", "noch", "vor", "dein", "Ant\u00b7litz", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der deine Liederchen, Mimi,", "tokens": ["Der", "dei\u00b7ne", "Lie\u00b7der\u00b7chen", ",", "Mi\u00b7mi", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lang behielt, als h\u00e4tt' er sie", "tokens": ["So", "lang", "be\u00b7hielt", ",", "als", "h\u00e4tt'", "er", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit sich hinab ins Grab genommen?", "tokens": ["Mit", "sich", "hin\u00b7ab", "ins", "Grab", "ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum wenigsten, als h\u00e4tt' er schon,", "tokens": ["Zum", "we\u00b7nigs\u00b7ten", ",", "als", "h\u00e4tt'", "er", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Zu seinem wohlverdienten Lohn',", "tokens": ["Zu", "sei\u00b7nem", "wohl\u00b7ver\u00b7dien\u00b7ten", "Lohn'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In einem Zitadellgew\u00f6lbe,", "tokens": ["In", "ei\u00b7nem", "Zi\u00b7ta\u00b7dell\u00b7ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Und f\u00fcr die Freundin an der Elbe", "tokens": ["Und", "f\u00fcr", "die", "Freun\u00b7din", "an", "der", "El\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht Feder und Papier gehabt?", "tokens": ["Nicht", "Fe\u00b7der", "und", "Pa\u00b7pier", "ge\u00b7habt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "VAPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich lebe, war auch nicht gefangen,", "tokens": ["Ich", "le\u00b7be", ",", "war", "auch", "nicht", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Gesund, trotz einem Tagedieb'!", "tokens": ["Ge\u00b7sund", ",", "trotz", "ei\u00b7nem", "Ta\u00b7ge\u00b7dieb'", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Noch mehr: Kein Tag ist mir vergangen,", "tokens": ["Noch", "mehr", ":", "Kein", "Tag", "ist", "mir", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "An dem ich nicht f\u00fcnf Stunden schrieb.", "tokens": ["An", "dem", "ich", "nicht", "f\u00fcnf", "Stun\u00b7den", "schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PTKNEG", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Um desto minder, wirst du sagen,", "tokens": ["Um", "des\u00b7to", "min\u00b7der", ",", "wirst", "du", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "$,", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ist solch ein Aufschub zu verzeihn!", "tokens": ["Ist", "solch", "ein", "Auf\u00b7schub", "zu", "ver\u00b7zeihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So scheint es freilich wohl; allein", "tokens": ["So", "scheint", "es", "frei\u00b7lich", "wohl", ";", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sey g\u00fctig, la\u00df mein Leid dir klagen.", "tokens": ["Sey", "g\u00fc\u00b7tig", ",", "la\u00df", "mein", "Leid", "dir", "kla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VVIMP", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die Lust und F\u00e4higkeit, zu reimen,", "tokens": ["Die", "Lust", "und", "F\u00e4\u00b7hig\u00b7keit", ",", "zu", "rei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Steht mir nicht zu Gebot wie ", "tokens": ["Steht", "mir", "nicht", "zu", "Ge\u00b7bot", "wie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NN", "KOKOM"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.21": {"text": "Der Verse, wie die Tauben, lockt.", "tokens": ["Der", "Ver\u00b7se", ",", "wie", "die", "Tau\u00b7ben", ",", "lockt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.22": {"text": "Mag ich nach ihr mich zierend sehnen,", "tokens": ["Mag", "ich", "nach", "ihr", "mich", "zie\u00b7rend", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die Muse, gegen mich verstockt,", "tokens": ["Die", "Mu\u00b7se", ",", "ge\u00b7gen", "mich", "ver\u00b7stockt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Scheint mich noch obenein zu h\u00f6hnen.", "tokens": ["Scheint", "mich", "noch", "o\u00b7be\u00b7nein", "zu", "h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Sie \u00f6ffnet meine Stubenth\u00fcr,", "tokens": ["Sie", "\u00f6ff\u00b7net", "mei\u00b7ne", "Stu\u00b7bent\u00b7h\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Blickt sp\u00f6ttisch auf die Aktenf\u00e4cher,", "tokens": ["Blickt", "sp\u00f6t\u00b7tisch", "auf", "die", "Ak\u00b7ten\u00b7f\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Spricht achselzuckend: \u00bbArmer Sch\u00e4cher!", "tokens": ["Spricht", "ach\u00b7sel\u00b7zu\u00b7ckend", ":", "\u00bb", "Ar\u00b7mer", "Sch\u00e4\u00b7cher", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Leb' wohl! da hast du viel Papier!", "tokens": ["Leb'", "wohl", "!", "da", "hast", "du", "viel", "Pa\u00b7pier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Was brauchst du mich und Amors K\u00f6cher? \u2013\u00ab", "tokens": ["Was", "brauchst", "du", "mich", "und", "A\u00b7mors", "K\u00f6\u00b7cher", "?", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "KON", "NE", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und husch! verschwindet sie vor mir.", "tokens": ["Und", "husch", "!", "ver\u00b7schwin\u00b7det", "sie", "vor", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Ich, der der Freundschaft und der Minne,", "tokens": ["Ich", ",", "der", "der", "Freund\u00b7schaft", "und", "der", "Min\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Nicht F\u00fcrsten, ihre Lieder sang,", "tokens": ["Nicht", "F\u00fcrs\u00b7ten", ",", "ih\u00b7re", "Lie\u00b7der", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Noch lieber jetzt ein Herz gewinne,", "tokens": ["Noch", "lie\u00b7ber", "jetzt", "ein", "Herz", "ge\u00b7win\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Als einer Hoheit reichsten Dank;", "tokens": ["Als", "ei\u00b7ner", "Ho\u00b7heit", "reichs\u00b7ten", "Dank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Womit verdient' ich wohl die Strafe,", "tokens": ["Wo\u00b7mit", "ver\u00b7dient'", "ich", "wohl", "die", "Stra\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Da\u00df sie mir kalt den R\u00fccken dreht,", "tokens": ["Da\u00df", "sie", "mir", "kalt", "den", "R\u00fc\u00b7cken", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Ja! nicht einmal, wie sonst, im Schlafe", "tokens": ["Ja", "!", "nicht", "ein\u00b7mal", ",", "wie", "sonst", ",", "im", "Schla\u00b7fe"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Mir heitre Tr\u00e4um' aufs Lager weht?", "tokens": ["Mir", "heit\u00b7re", "Tr\u00e4um'", "aufs", "La\u00b7ger", "weht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.39": {"text": "Dem Winzer gibt sie unter Reben,", "tokens": ["Dem", "Win\u00b7zer", "gibt", "sie", "un\u00b7ter", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Der Harkerin, auf ihrem Grummt", "tokens": ["Der", "Har\u00b7ke\u00b7rin", ",", "auf", "ih\u00b7rem", "Grummt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Am Abend singend, neues Leben,", "tokens": ["Am", "A\u00b7bend", "sin\u00b7gend", ",", "neu\u00b7es", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Ich, ich allein bin nur verstummt.", "tokens": ["Ich", ",", "ich", "al\u00b7lein", "bin", "nur", "ver\u00b7stummt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADV", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Vielleicht inde\u00df zu meinem Gl\u00fccke!", "tokens": ["Viel\u00b7leicht", "in\u00b7de\u00df", "zu", "mei\u00b7nem", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Sie mochte wohl zum voraus schaun,", "tokens": ["Sie", "moch\u00b7te", "wohl", "zum", "vo\u00b7raus", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Mir sey, bei meines D\u00e4mons T\u00fccke,", "tokens": ["Mir", "sey", ",", "bei", "mei\u00b7nes", "D\u00e4\u00b7mons", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Kein Saitenspiel mehr zu vertraun.", "tokens": ["Kein", "Sai\u00b7ten\u00b7spiel", "mehr", "zu", "ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und wahrlich! Wer gleich Juvenalen,", "tokens": ["Und", "wahr\u00b7lich", "!", "Wer", "gleich", "Ju\u00b7ve\u00b7na\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PWS", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Die hohen Thoren seiner Zeit", "tokens": ["Die", "ho\u00b7hen", "Tho\u00b7ren", "sei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "In ihrer H\u00e4\u00dflichkeit will mahlen,", "tokens": ["In", "ih\u00b7rer", "H\u00e4\u00df\u00b7lich\u00b7keit", "will", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Der gehe ja, eh' sie bezahlen,", "tokens": ["Der", "ge\u00b7he", "ja", ",", "eh'", "sie", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Dahin, wo kein Zensuredikt", "tokens": ["Da\u00b7hin", ",", "wo", "kein", "Zen\u00b7su\u00b7re\u00b7dikt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Die Wahrheit in der Wieg' erstickt,", "tokens": ["Die", "Wahr\u00b7heit", "in", "der", "Wieg'", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Sonst wart' er, bis er dahin r\u00fcckt,", "tokens": ["Sonst", "wart'", "er", ",", "bis", "er", "da\u00b7hin", "r\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Wo Lucian mit Juvenalen", "tokens": ["Wo", "Lu\u00b7ci\u00b7an", "mit", "Ju\u00b7ve\u00b7na\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Sejanen dreist ins Auge blickt.", "tokens": ["Se\u00b7ja\u00b7nen", "dreist", "ins", "Au\u00b7ge", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Dank denn, o gute Muse! dir,", "tokens": ["Dank", "denn", ",", "o", "gu\u00b7te", "Mu\u00b7se", "!", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "$,", "FM", "ADJA", "NN", "$.", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Da\u00df du seit Jahren hast geschwiegen,", "tokens": ["Da\u00df", "du", "seit", "Jah\u00b7ren", "hast", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Noch n\u00f6thiger ist Ruhe mir,", "tokens": ["Noch", "n\u00f6\u00b7thi\u00b7ger", "ist", "Ru\u00b7he", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Als Geist zu Liedern und Vergn\u00fcgen.", "tokens": ["Als", "Geist", "zu", "Lie\u00b7dern", "und", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Du, die Gelehrte streiten l\u00e4\u00dft,", "tokens": ["Du", ",", "die", "Ge\u00b7lehr\u00b7te", "strei\u00b7ten", "l\u00e4\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Und unverblendet von dem Schimmer", "tokens": ["Und", "un\u00b7ver\u00b7blen\u00b7det", "von", "dem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.62": {"text": "Des Ruhmes, sich nicht Lob erpre\u00dft,", "tokens": ["Des", "Ruh\u00b7mes", ",", "sich", "nicht", "Lob", "er\u00b7pre\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.63": {"text": "Wohl aber oft ein frohes Fest", "tokens": ["Wohl", "a\u00b7ber", "oft", "ein", "fro\u00b7hes", "Fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "Den Musen gibt in ihrem Zimmer,", "tokens": ["Den", "Mu\u00b7sen", "gibt", "in", "ih\u00b7rem", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.65": {"text": "Inde\u00df der Mann das Wespennest", "tokens": ["In\u00b7de\u00df", "der", "Mann", "das", "Wes\u00b7pen\u00b7nest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.66": {"text": "Vom Aber- und vom Wunderglauben,", "tokens": ["Vom", "A\u00b7ber", "und", "vom", "Wun\u00b7der\u00b7glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.67": {"text": "Und von geheimen Orden st\u00f6rt:", "tokens": ["Und", "von", "ge\u00b7hei\u00b7men", "Or\u00b7den", "st\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.68": {"text": "Sing' in versteckten Rebenlauben,", "tokens": ["Sing'", "in", "ver\u00b7steck\u00b7ten", "Re\u00b7ben\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.69": {"text": "Von deinen Freunden nur geh\u00f6rt,", "tokens": ["Von", "dei\u00b7nen", "Freun\u00b7den", "nur", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.70": {"text": "Das seltne Gl\u00fcck zufriedner Ehe,", "tokens": ["Das", "selt\u00b7ne", "Gl\u00fcck", "zu\u00b7fried\u00b7ner", "E\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.71": {"text": "(ein Sohn, wie Amor, sey dein Lohn!)", "tokens": ["(", "ein", "Sohn", ",", "wie", "A\u00b7mor", ",", "sey", "dein", "Lohn", "!", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "PWAV", "NE", "$,", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.72": {"text": "Und weh' dem Kritikaster, wehe!", "tokens": ["Und", "weh'", "dem", "Kri\u00b7ti\u00b7kas\u00b7ter", ",", "we\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.73": {"text": "Der einen falschen Semiton", "tokens": ["Der", "ei\u00b7nen", "fal\u00b7schen", "Se\u00b7mi\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.74": {"text": "Dir aufzumutzen wagen k\u00f6nnte,", "tokens": ["Dir", "auf\u00b7zu\u00b7mut\u00b7zen", "wa\u00b7gen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.75": {"text": "Wenn alle Freund' ihn gern verzeihn.", "tokens": ["Wenn", "al\u00b7le", "Freund'", "ihn", "gern", "ver\u00b7zeihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.76": {"text": "Das allersch\u00f6nste der Talente", "tokens": ["Das", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "der", "Ta\u00b7len\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.77": {"text": "Ist doch: der Freunde Herz erfreun!", "tokens": ["Ist", "doch", ":", "der", "Freun\u00b7de", "Herz", "er\u00b7freun", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$.", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}