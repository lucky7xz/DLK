{"textgrid.poem.48703": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4. Auf seiner Stiefmutter Absterben (1633 December), welches er in fremden Landen erfuhre", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe noch die Angst des f\u00fcrchtenden Gesichtes,", "tokens": ["Ich", "se\u00b7he", "noch", "die", "Angst", "des", "f\u00fcrch\u00b7ten\u00b7den", "Ge\u00b7sich\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als, Mutter, ich vor euch mit halber Freude trat,", "tokens": ["als", ",", "Mut\u00b7ter", ",", "ich", "vor", "euch", "mit", "hal\u00b7ber", "Freu\u00b7de", "trat", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "NN", "$,", "PPER", "APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und um zu reisen aus gewollten Urlaub bat,", "tokens": ["und", "um", "zu", "rei\u00b7sen", "aus", "ge\u00b7woll\u00b7ten", "Ur\u00b7laub", "bat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den ich euch fast zwung' ab. Das Urtheil des Gerichtes", "tokens": ["den", "ich", "euch", "fast", "zwung'", "ab", ".", "Das", "Ur\u00b7theil", "des", "Ge\u00b7rich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADV", "VVFIN", "PTKVZ", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "und der gebrochne Stab des strengen B\u00f6sewichtes", "tokens": ["und", "der", "ge\u00b7broch\u00b7ne", "Stab", "des", "stren\u00b7gen", "B\u00f6\u00b7se\u00b7wich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "geht allzufr\u00fch' an euch. Ach, da\u00df des Himmels Rat", "tokens": ["geht", "all\u00b7zu\u00b7fr\u00fch'", "an", "euch", ".", "Ach", ",", "da\u00df", "des", "Him\u00b7mels", "Rat"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$.", "ITJ", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "mich damals euch nicht lie\u00df, da er gewu\u00dft schon hat,", "tokens": ["mich", "da\u00b7mals", "euch", "nicht", "lie\u00df", ",", "da", "er", "ge\u00b7wu\u00dft", "schon", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ich w\u00fcrde mehr nicht sehn den Schatten eures Lichtes.", "tokens": ["ich", "w\u00fcr\u00b7de", "mehr", "nicht", "sehn", "den", "Schat\u00b7ten", "eu\u00b7res", "Lich\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVINF", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verzeiht mirs, Selige, hab' ich euch da betr\u00fcbt", "tokens": ["Ver\u00b7zeiht", "mirs", ",", "Se\u00b7li\u00b7ge", ",", "hab'", "ich", "euch", "da", "be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "$,", "NN", "$,", "VAFIN", "PPER", "PPER", "ADV", "VVFIN"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "und etwas Fremdes mehr als euren Wundsch geliebt!", "tokens": ["und", "et\u00b7was", "Frem\u00b7des", "mehr", "als", "eu\u00b7ren", "Wund\u00b7sch", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PIAT", "KOKOM", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was Gott beschlossen hat, ist mir und euch geschehen,", "tokens": ["Was", "Gott", "be\u00b7schlos\u00b7sen", "hat", ",", "ist", "mir", "und", "euch", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "VAFIN", "$,", "VAFIN", "PPER", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "was nun ist hinter euch, das hab' ich noch vor mir.", "tokens": ["was", "nun", "ist", "hin\u00b7ter", "euch", ",", "das", "hab'", "ich", "noch", "vor", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "APPR", "PPER", "$,", "PDS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will er, so will auch ich noch heute mit Begier", "tokens": ["Will", "er", ",", "so", "will", "auch", "ich", "noch", "heu\u00b7te", "mit", "Be\u00b7gier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "ADV", "VMFIN", "ADV", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "euch in der Ewigkeit mit diesen Augen sehen.", "tokens": ["euch", "in", "der", "E\u00b7wig\u00b7keit", "mit", "die\u00b7sen", "Au\u00b7gen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}