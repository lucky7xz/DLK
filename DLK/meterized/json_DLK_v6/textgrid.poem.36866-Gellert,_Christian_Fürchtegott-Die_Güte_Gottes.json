{"textgrid.poem.36866": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Die G\u00fcte Gottes", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gro\u00df ist des Allm\u00e4chtgen G\u00fcte!", "tokens": ["Wie", "gro\u00df", "ist", "des", "All\u00b7m\u00e4cht\u00b7gen", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ist der ein Mensch, den sie nicht r\u00fchrt?", "tokens": ["Ist", "der", "ein", "Mensch", ",", "den", "sie", "nicht", "r\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mit verh\u00e4rtetem Gem\u00fcte", "tokens": ["Der", "mit", "ver\u00b7h\u00e4r\u00b7te\u00b7tem", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Dank erstickt, der ihm geb\u00fchrt?", "tokens": ["Den", "Dank", "er\u00b7stickt", ",", "der", "ihm", "ge\u00b7b\u00fchrt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nein, seine Liebe zu ermessen,", "tokens": ["Nein", ",", "sei\u00b7ne", "Lie\u00b7be", "zu", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sei ewig meine gr\u00f6\u00dfte Pflicht!", "tokens": ["Sei", "e\u00b7wig", "mei\u00b7ne", "gr\u00f6\u00df\u00b7te", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Herr hat mein noch nie vergessen;", "tokens": ["Der", "Herr", "hat", "mein", "noch", "nie", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vergi\u00df, mein Herz, auch seiner nicht.", "tokens": ["Ver\u00b7gi\u00df", ",", "mein", "Herz", ",", "auch", "sei\u00b7ner", "nicht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer hat mich wunderbar bereitet?", "tokens": ["Wer", "hat", "mich", "wun\u00b7der\u00b7bar", "be\u00b7rei\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gott, der meiner nicht bedarf.", "tokens": ["Der", "Gott", ",", "der", "mei\u00b7ner", "nicht", "be\u00b7darf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hat mit Langmut mich geleitet?", "tokens": ["Wer", "hat", "mit", "Lang\u00b7mut", "mich", "ge\u00b7lei\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er, dessen Rat ich oft verwarf.", "tokens": ["Er", ",", "des\u00b7sen", "Rat", "ich", "oft", "ver\u00b7warf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer st\u00e4rkt den Frieden im Gewissen?", "tokens": ["Wer", "st\u00e4rkt", "den", "Frie\u00b7den", "im", "Ge\u00b7wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer gibt dem Geiste neue Kraft?", "tokens": ["Wer", "gibt", "dem", "Geis\u00b7te", "neu\u00b7e", "Kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer l\u00e4\u00dft mich so viel Gl\u00fcck genie\u00dfen?", "tokens": ["Wer", "l\u00e4\u00dft", "mich", "so", "viel", "Gl\u00fcck", "ge\u00b7nie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist's nicht sein Arm, der alles schafft?", "tokens": ["Ist's", "nicht", "sein", "Arm", ",", "der", "al\u00b7les", "schafft", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schau, o mein Geist! in jenes Leben,", "tokens": ["Schau", ",", "o", "mein", "Geist", "!", "in", "je\u00b7nes", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "PPOSAT", "NN", "$.", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu welchem du erschaffen bist;", "tokens": ["Zu", "wel\u00b7chem", "du", "er\u00b7schaf\u00b7fen", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo du, mit Herrlichkeit umgeben,", "tokens": ["Wo", "du", ",", "mit", "Herr\u00b7lich\u00b7keit", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott ewig sehn wirst, wie er ist.", "tokens": ["Gott", "e\u00b7wig", "sehn", "wirst", ",", "wie", "er", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "VAFIN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast ein Recht zu diesen Freuden;", "tokens": ["Du", "hast", "ein", "Recht", "zu", "die\u00b7sen", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Gottes G\u00fcte sind sie dein.", "tokens": ["Durch", "Got\u00b7tes", "G\u00fc\u00b7te", "sind", "sie", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sieh, darum mu\u00dfte Christus leiden,", "tokens": ["Sieh", ",", "da\u00b7rum", "mu\u00df\u00b7te", "Chris\u00b7tus", "lei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PAV", "VMFIN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Damit du k\u00f6nntest selig sein.", "tokens": ["Da\u00b7mit", "du", "k\u00f6nn\u00b7test", "se\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und diesen Gott sollt ich nicht ehren?", "tokens": ["Und", "die\u00b7sen", "Gott", "sollt", "ich", "nicht", "eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seine G\u00fcte nicht verstehn?", "tokens": ["Und", "sei\u00b7ne", "G\u00fc\u00b7te", "nicht", "ver\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sollte rufen; ich nicht h\u00f6ren?", "tokens": ["Er", "soll\u00b7te", "ru\u00b7fen", ";", "ich", "nicht", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Weg, den er mir zeigt, nicht gehn?", "tokens": ["Den", "Weg", ",", "den", "er", "mir", "zeigt", ",", "nicht", "gehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Will ist mir ins Herz geschrieben;", "tokens": ["Sein", "Will", "ist", "mir", "ins", "Herz", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Wort best\u00e4rkt ihn ewiglich.", "tokens": ["Sein", "Wort", "be\u00b7st\u00e4rkt", "ihn", "e\u00b7wig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gott soll ich \u00fcber alles lieben,", "tokens": ["Gott", "soll", "ich", "\u00fc\u00b7ber", "al\u00b7les", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und meinen N\u00e4chsten gleich als mich.", "tokens": ["Und", "mei\u00b7nen", "N\u00e4chs\u00b7ten", "gleich", "als", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dies ist mein Dank, dies ist sein Wille.", "tokens": ["Dies", "ist", "mein", "Dank", ",", "dies", "ist", "sein", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich soll vollkommen sein, wie er.", "tokens": ["Ich", "soll", "voll\u00b7kom\u00b7men", "sein", ",", "wie", "er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lang ich dies Gebot erf\u00fclle,", "tokens": ["So", "lang", "ich", "dies", "Ge\u00b7bot", "er\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stell ich sein Bildnis in mir her.", "tokens": ["Stell", "ich", "sein", "Bild\u00b7nis", "in", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lebt seine Lieb in meiner Seele:", "tokens": ["Lebt", "sei\u00b7ne", "Lieb", "in", "mei\u00b7ner", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So treibt sie mich zu jeder Pflicht.", "tokens": ["So", "treibt", "sie", "mich", "zu", "je\u00b7der", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ob ich schon aus Schwachheit fehle,", "tokens": ["Und", "ob", "ich", "schon", "aus", "Schwach\u00b7heit", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Herrscht doch in mir die S\u00fcnde nicht.", "tokens": ["Herrscht", "doch", "in", "mir", "die", "S\u00fcn\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O Gott! la\u00df deine G\u00fct und Liebe", "tokens": ["O", "Gott", "!", "la\u00df", "dei\u00b7ne", "G\u00fct", "und", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "VVIMP", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir immerdar vor Augen sein!", "tokens": ["Mir", "im\u00b7mer\u00b7dar", "vor", "Au\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie st\u00e4rk in mir die guten Triebe,", "tokens": ["Sie", "st\u00e4rk", "in", "mir", "die", "gu\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein ganzes Leben dir zu weihn.", "tokens": ["Mein", "gan\u00b7zes", "Le\u00b7ben", "dir", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie tr\u00f6ste mich zur Zeit der Schmerzen;", "tokens": ["Sie", "tr\u00f6s\u00b7te", "mich", "zur", "Zeit", "der", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie leite mich zur Zeit des Gl\u00fccks;", "tokens": ["Sie", "lei\u00b7te", "mich", "zur", "Zeit", "des", "Gl\u00fccks", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sie besieg in meinem Herzen", "tokens": ["Und", "sie", "be\u00b7sieg", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Furcht des letzten Augenblicks.", "tokens": ["Die", "Furcht", "des", "letz\u00b7ten", "Au\u00b7gen\u00b7blicks", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}