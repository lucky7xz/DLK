{"dta.poem.18374": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wie sie die schwanen auf dem  \n wasser an sich lockte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Venus neulich war zum himmel aufgeflogen/", "tokens": ["Als", "Ve\u00b7nus", "neu\u00b7lich", "war", "zum", "him\u00b7mel", "auf\u00b7ge\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VAFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil frost und strenge lufft sich hier auf erden faud/", "tokens": ["Weil", "frost", "und", "stren\u00b7ge", "lufft", "sich", "hier", "auf", "er\u00b7den", "faud", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJA", "NN", "PRF", "ADV", "APPR", "NN", "NE", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und sich in schneller eil der kalten welt entzogen/", "tokens": ["Und", "sich", "in", "schnel\u00b7ler", "eil", "der", "kal\u00b7ten", "welt", "ent\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch ehstens hier zu seyn in frischer hoffnung stand;", "tokens": ["Doch", "ehs\u00b7tens", "hier", "zu", "seyn", "in", "fri\u00b7scher", "hoff\u00b7nung", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PTKZU", "VAINF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So lie\u00df sie noch zuvor die schwanen f\u00fcr sich kommen/", "tokens": ["So", "lie\u00df", "sie", "noch", "zu\u00b7vor", "die", "schwa\u00b7nen", "f\u00fcr", "sich", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die sonst der Venus volck und liebes-boten seyn/", "tokens": ["Die", "sonst", "der", "Ve\u00b7nus", "volck", "und", "lie\u00b7bes\u00b7bo\u00b7ten", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "KON", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und die sie anfangs bald zu diensten angenommen/", "tokens": ["Und", "die", "sie", "an\u00b7fangs", "bald", "zu", "diens\u00b7ten", "an\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als diese g\u00f6ttin erst sich fand auf erden ein.", "tokens": ["Als", "die\u00b7se", "g\u00f6t\u00b7tin", "erst", "sich", "fand", "auf", "er\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "PRF", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie sprach: Mein werthes volck/ ihr meine hof-trabanten/", "tokens": ["Sie", "sprach", ":", "Mein", "wert\u00b7hes", "volck", "/", "ihr", "mei\u00b7ne", "hof\u00b7tra\u00b7ban\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "$(", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die ihr mir jederzeit bisher getreu gewest/", "tokens": ["Die", "ihr", "mir", "je\u00b7der\u00b7zeit", "bis\u00b7her", "ge\u00b7treu", "ge\u00b7west", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und als der buhlerey geheimste abgesandten/", "tokens": ["Und", "als", "der", "buh\u00b7le\u00b7rey", "ge\u00b7heims\u00b7te", "ab\u00b7ge\u00b7sand\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Euch flei\u00dfig eingestellt bey meinem opfer-fest/", "tokens": ["Euch", "flei\u00b7\u00dfig", "ein\u00b7ge\u00b7stellt", "bey", "mei\u00b7nem", "op\u00b7fer\u00b7fest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jhr zeugen meiner lust/ ich mu\u00df itzt von euch scheiden/", "tokens": ["Ihr", "zeu\u00b7gen", "mei\u00b7ner", "lust", "/", "ich", "mu\u00df", "itzt", "von", "euch", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die rauhe winters-zeit verjagt mich aus der welt/", "tokens": ["Die", "rau\u00b7he", "win\u00b7ter\u00b7szeit", "ver\u00b7jagt", "mich", "aus", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich mu\u00df/ ob wolt ich nicht/ euch eine zeitlang meiden/", "tokens": ["Ich", "mu\u00df", "/", "ob", "wolt", "ich", "nicht", "/", "euch", "ei\u00b7ne", "zeit\u00b7lang", "mei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "KOUS", "VMFIN", "PPER", "PTKNEG", "$(", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und dort im himmel seyn den g\u00f6ttern beygesellt.", "tokens": ["Und", "dort", "im", "him\u00b7mel", "seyn", "den", "g\u00f6t\u00b7tern", "bey\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PPOSAT", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch hof ich auch zugleich in kurtzem euch zu schauen/", "tokens": ["Doch", "hof", "ich", "auch", "zu\u00b7gleich", "in", "kurt\u00b7zem", "euch", "zu", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zieht ihr indessen hin/ gebrauchet euch der zeit/", "tokens": ["Zieht", "ihr", "in\u00b7des\u00b7sen", "hin", "/", "ge\u00b7brau\u00b7chet", "euch", "der", "zeit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$(", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Lebt wohl in s\u00fcsser lust und lieblichem vertrauen/", "tokens": ["Lebt", "wohl", "in", "s\u00fcs\u00b7ser", "lust", "und", "lieb\u00b7li\u00b7chem", "ver\u00b7trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "KON", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bi\u00df meine gegenwart euch wiederum erfreut.", "tokens": ["Bi\u00df", "mei\u00b7ne", "ge\u00b7gen\u00b7wart", "euch", "wie\u00b7de\u00b7rum", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hiemit so hat sie sich bald in die lufft geschwungen/", "tokens": ["Hie\u00b7mit", "so", "hat", "sie", "sich", "bald", "in", "die", "lufft", "ge\u00b7schwun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und lie\u00df dies wei\u00dfe volck best\u00fcrtzt und traurig stehn/", "tokens": ["Und", "lie\u00df", "dies", "wei\u00b7\u00dfe", "volck", "be\u00b7st\u00fcrtzt", "und", "trau\u00b7rig", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "ADJA", "NN", "VVPP", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der schmertz kam hauffen-wei\u00df auf sie herzugedrungen/", "tokens": ["Der", "schmertz", "kam", "hauf\u00b7fen\u00b7wei\u00df", "auf", "sie", "her\u00b7zu\u00b7ge\u00b7drun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADJD", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Es schien/ sie solten fast in noth und angst vergehn.", "tokens": ["Es", "schien", "/", "sie", "sol\u00b7ten", "fast", "in", "noth", "und", "angst", "ver\u00b7gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VMFIN", "ADV", "APPR", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sie zitterten vor furcht und bebeten vor schrecken/", "tokens": ["Sie", "zit\u00b7ter\u00b7ten", "vor", "furcht", "und", "be\u00b7be\u00b7ten", "vor", "schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "APPR", "VVINF", "$("], "meter": "-+--++--+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Sie schwommen in der irr gantz einsam und betr\u00fcbt.", "tokens": ["Sie", "schwom\u00b7men", "in", "der", "irr", "gantz", "ein\u00b7sam", "und", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und hat die bange zeit/ so alles wil beflecken/", "tokens": ["Und", "hat", "die", "ban\u00b7ge", "zeit", "/", "so", "al\u00b7les", "wil", "be\u00b7fle\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$(", "ADV", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die gr\u00f6ste grausamkeit an ihnen ausge\u00fcbt.", "tokens": ["Die", "gr\u00f6s\u00b7te", "grau\u00b7sam\u00b7keit", "an", "ih\u00b7nen", "aus\u00b7ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch gestern wei\u00df ich nicht/ wies das verh\u00e4ngn\u00fcs schickte/", "tokens": ["Doch", "ge\u00b7stern", "wei\u00df", "ich", "nicht", "/", "wies", "das", "ver\u00b7h\u00e4ng\u00b7n\u00fcs", "schick\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$(", "VVFIN", "ART", "ADJA", "VVFIN", "$("], "meter": "---+--+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Di\u00df aber weis ich wohl/ es kam von ohngefehr/", "tokens": ["Di\u00df", "a\u00b7ber", "weis", "ich", "wohl", "/", "es", "kam", "von", "ohn\u00b7ge\u00b7fehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKVZ", "PPER", "ADV", "$(", "PPER", "VVFIN", "APPR", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df dieses schwanen-volck/ Melinde/ dich erblickte/", "tokens": ["Da\u00df", "die\u00b7ses", "schwa\u00b7nen\u00b7volck", "/", "Me\u00b7lin\u00b7de", "/", "dich", "er\u00b7blick\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$(", "NE", "$(", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als es noch voller schmertz so irrte hin und her.", "tokens": ["Als", "es", "noch", "vol\u00b7ler", "schmertz", "so", "irr\u00b7te", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "ADV", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie h\u00e4ufig kamen sie nicht auf dich zugedrungen/", "tokens": ["Wie", "h\u00e4u\u00b7fig", "ka\u00b7men", "sie", "nicht", "auf", "dich", "zu\u00b7ge\u00b7drun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der meinung: Venus sey nun wieder auf der welt/", "tokens": ["Der", "mei\u00b7nung", ":", "Ve\u00b7nus", "sey", "nun", "wie\u00b7der", "auf", "der", "welt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sie habe wiederum sich himmelab geschwungen/", "tokens": ["Sie", "ha\u00b7be", "wie\u00b7de\u00b7rum", "sich", "him\u00b7mel\u00b7ab", "ge\u00b7schwun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und sich in deine zierd und sch\u00f6nste pracht verstellt.", "tokens": ["Und", "sich", "in", "dei\u00b7ne", "zierd", "und", "sch\u00f6ns\u00b7te", "pracht", "ver\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sie sahen auch das feur in deinen augen blitzen/", "tokens": ["Sie", "sa\u00b7hen", "auch", "das", "feur", "in", "dei\u00b7nen", "au\u00b7gen", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das Venus nur allein in ihren sonnen tr\u00e4gt/", "tokens": ["Das", "Ve\u00b7nus", "nur", "al\u00b7lein", "in", "ih\u00b7ren", "son\u00b7nen", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und das durch einen blick kan seel und geist erhitzen/", "tokens": ["Und", "das", "durch", "ei\u00b7nen", "blick", "kan", "seel", "und", "geist", "er\u00b7hit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "VMFIN", "ADJD", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wann dieser wunderzeug die heissen kr\u00e4ffte regt.", "tokens": ["Wann", "die\u00b7ser", "wun\u00b7der\u00b7zeug", "die", "heis\u00b7sen", "kr\u00e4ff\u00b7te", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PDAT", "NN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Den purpur/ den sie sonst auff Venus mund gefunden/", "tokens": ["Den", "pur\u00b7pur", "/", "den", "sie", "sonst", "auff", "Ve\u00b7nus", "mund", "ge\u00b7fun\u00