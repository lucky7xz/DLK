{"textgrid.poem.44268": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[last sehn, wer unter euch am ersten fertig sey!]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Last sehn, wer unter euch am ersten fertig sey!", "tokens": ["Last", "sehn", ",", "wer", "un\u00b7ter", "euch", "am", "ers\u00b7ten", "fer\u00b7tig", "sey", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWS", "APPR", "PPER", "APPRART", "ADJA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht fort, ihr M\u00e4gdgen, eilt und schaft den Kranz herbey!", "tokens": ["Macht", "fort", ",", "ihr", "M\u00e4gd\u00b7gen", ",", "eilt", "und", "schaft", "den", "Kranz", "her\u00b7bey", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "KON", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die erste, so ihn bringt, (verspizt euch auf die Gaben!)", "tokens": ["Die", "ers\u00b7te", ",", "so", "ihn", "bringt", ",", "(", "ver\u00b7spizt", "euch", "auf", "die", "Ga\u00b7ben", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "PPER", "VVFIN", "$,", "$(", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Soll einen Mann davor noch diese Fastnacht haben.", "tokens": ["Soll", "ei\u00b7nen", "Mann", "da\u00b7vor", "noch", "die\u00b7se", "Fast\u00b7nacht", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PAV", "ADV", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O ho, wie laufen sie, o welcher Schwarm ist da!", "tokens": ["O", "ho", ",", "wie", "lau\u00b7fen", "sie", ",", "o", "wel\u00b7cher", "Schwarm", "ist", "da", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "PWAV", "VVFIN", "PPER", "$,", "FM", "PWAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Herr Bruder, folge mir und tritt anjezt nicht nah,", "tokens": ["Herr", "Bru\u00b7der", ",", "fol\u00b7ge", "mir", "und", "tritt", "an\u00b7jezt", "nicht", "nah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sonst kommt dein junges Haupt in dieses Lustgedr\u00e4nge", "tokens": ["Sonst", "kommt", "dein", "jun\u00b7ges", "Haupt", "in", "die\u00b7ses", "Lust\u00b7ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wird vielleicht wohl gar von solcher Blumenmenge", "tokens": ["Und", "wird", "viel\u00b7leicht", "wohl", "gar", "von", "sol\u00b7cher", "Blu\u00b7men\u00b7men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie jene R\u00f6merin von g\u00fcldner Beut erstickt.", "tokens": ["Wie", "je\u00b7ne", "R\u00f6\u00b7me\u00b7rin", "von", "g\u00fcld\u00b7ner", "Beut", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jedoch, o sanfter Tod, wen so ein Schaz erdr\u00fcckt,", "tokens": ["Je\u00b7doch", ",", "o", "sanf\u00b7ter", "Tod", ",", "wen", "so", "ein", "Schaz", "er\u00b7dr\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "ADJA", "NN", "$,", "PWS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dem wird der Parzen Schaar zu holden Charitinnen,", "tokens": ["Dem", "wird", "der", "Par\u00b7zen", "Schaar", "zu", "hol\u00b7den", "Cha\u00b7ri\u00b7tin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er aber f\u00e4hrt mit Lust wie Curtius von hinnen.", "tokens": ["Er", "a\u00b7ber", "f\u00e4hrt", "mit", "Lust", "wie", "Cur\u00b7tius", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "KOKOM", "NE", "APPR", "ADV", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Gescherzt ist nicht geschimpft. Allein den Scherz bey Seit:", "tokens": ["Ge\u00b7scherzt", "ist", "nicht", "ge\u00b7schimpft", ".", "Al\u00b7lein", "den", "Scherz", "bey", "Seit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PTKNEG", "VVPP", "$.", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Herr Bruder, sage mir, wie lebstu in der Zeit?", "tokens": ["Herr", "Bru\u00b7der", ",", "sa\u00b7ge", "mir", ",", "wie", "lebs\u00b7tu", "in", "der", "Zeit", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was plagt, was ficht dich an, die Handvoll Ruhmviolen", "tokens": ["Was", "plagt", ",", "was", "ficht", "dich", "an", ",", "die", "Hand\u00b7voll", "Ruhm\u00b7vi\u00b7o\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So langsam und so sp\u00e4t am Helicon zu holen?", "tokens": ["So", "lang\u00b7sam", "und", "so", "sp\u00e4t", "am", "He\u00b7li\u00b7con", "zu", "ho\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du weist ja, wer du bist: Du bist Astr\u00e4ens Mann,", "tokens": ["Du", "weist", "ja", ",", "wer", "du", "bist", ":", "Du", "bist", "A\u00b7str\u00e4\u00b7ens", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VAFIN", "$.", "PPER", "VAFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die in der Christenheit kein Kebsweib leiden kan,", "tokens": ["Die", "in", "der", "Chris\u00b7ten\u00b7heit", "kein", "Kebs\u00b7weib", "lei\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und sch\u00e4mst dich gleichwohl nicht, auch wieder dein Gewi\u00dfen", "tokens": ["Und", "sch\u00e4mst", "dich", "gleich\u00b7wohl", "nicht", ",", "auch", "wie\u00b7der", "dein", "Ge\u00b7wi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dies Fieckchen herzuziehn und nebenbey zu k\u00fc\u00dfen.", "tokens": ["Dies", "Fieck\u00b7chen", "her\u00b7zu\u00b7ziehn", "und", "ne\u00b7ben\u00b7bey", "zu", "k\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVINF", "KON", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gut, da\u00df die erste Frau verbundne Blicke tr\u00e4gt;", "tokens": ["Gut", ",", "da\u00df", "die", "ers\u00b7te", "Frau", "ver\u00b7bund\u00b7ne", "Bli\u00b7cke", "tr\u00e4gt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Sie h\u00e4tte, s\u00e4h sie dies, den Kram gewis gelegt", "tokens": ["Sie", "h\u00e4t\u00b7te", ",", "s\u00e4h", "sie", "dies", ",", "den", "Kram", "ge\u00b7wis", "ge\u00b7legt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "PDS", "$,", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und w\u00fcrde, wenn du so mit andern zugehalten,", "tokens": ["Und", "w\u00fcr\u00b7de", ",", "wenn", "du", "so", "mit", "an\u00b7dern", "zu\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Durch ihr gerechtes Schwerd das neue B\u00fcndn\u00fc\u00df spalten.", "tokens": ["Durch", "ihr", "ge\u00b7rech\u00b7tes", "Schwerd", "das", "neu\u00b7e", "B\u00fcnd\u00b7n\u00fc\u00df", "spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jedoch so schlecht sie sieht, so sieht sie mehr als du;", "tokens": ["Je\u00b7doch", "so", "schlecht", "sie", "sieht", ",", "so", "sieht", "sie", "mehr", "als", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn jezo eilt dein Herz nach einer Dirne zu,", "tokens": ["Denn", "je\u00b7zo", "eilt", "dein", "Herz", "nach", "ei\u00b7ner", "Dir\u00b7ne", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der jeder Knoblochskopf bis an den Nabel gucket", "tokens": ["Der", "je\u00b7der", "Kno\u00b7blochs\u00b7kopf", "bis", "an", "den", "Na\u00b7bel", "gu\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und jeder ", "tokens": ["Und", "je\u00b7der"], "token_info": ["word", "word"], "pos": ["KON", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Wenn ich was lieben soll, so bild ich mir stets ein,", "tokens": ["Wenn", "ich", "was", "lie\u00b7ben", "soll", ",", "so", "bild", "ich", "mir", "stets", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,", "ADV", "ADJD", "PPER", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es m\u00fc\u00dfe sch\u00f6n und zart und jung und niedlich seyn.", "tokens": ["Es", "m\u00fc\u00b7\u00dfe", "sch\u00f6n", "und", "zart", "und", "jung", "und", "nied\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Allein, ich bitte dich, mit was kan Fieckchen reizen?", "tokens": ["Al\u00b7lein", ",", "ich", "bit\u00b7te", "dich", ",", "mit", "was", "kan", "Fieck\u00b7chen", "rei\u00b7zen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "$,", "APPR", "PWS", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sie stinckt nach Schwei\u00df und Oel wie angebrandter Weizen", "tokens": ["Sie", "stinckt", "nach", "Schwei\u00df", "und", "O\u00b7el", "wie", "an\u00b7ge\u00b7brand\u00b7ter", "Wei\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "KOKOM", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Und wie der Schlafrock roch, in dem sie manche Nacht", "tokens": ["Und", "wie", "der", "Schla\u00b7frock", "roch", ",", "in", "dem", "sie", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "APPR", "PRELS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dem Aristoteles den Schnabel warm gemacht.", "tokens": ["Dem", "A\u00b7ris\u00b7to\u00b7te\u00b7les", "den", "Schna\u00b7bel", "warm", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "O las das wilde Ding; du siehst, es buhlt mit Leuten,", "tokens": ["O", "las", "das", "wil\u00b7de", "Ding", ";", "du", "siehst", ",", "es", "buhlt", "mit", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die kaum noch trocken sind, und hat vor alten Zeiten", "tokens": ["Die", "kaum", "noch", "tro\u00b7cken", "sind", ",", "und", "hat", "vor", "al\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "$,", "KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Den h\u00fcndisch-groben Kerl im Fa\u00dfe so geleckt,", "tokens": ["Den", "h\u00fcn\u00b7dischgro\u00b7ben", "Kerl", "im", "Fa\u00b7\u00dfe", "so", "ge\u00b7leckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.38": {"text": "Da\u00df jezo noch ihr Mund nach Tonnenk\u00e4se schmeckt,", "tokens": ["Da\u00df", "je\u00b7zo", "noch", "ihr", "Mund", "nach", "Ton\u00b7nen\u00b7k\u00e4\u00b7se", "schmeckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Den dort ihr Courtisan durchs Spundloch angenommen,", "tokens": ["Den", "dort", "ihr", "Cour\u00b7ti\u00b7san", "durchs", "Spund\u00b7loch", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn eine Heringsfrau zum weisen Meister kommen.", "tokens": ["Wenn", "ei\u00b7ne", "He\u00b7rings\u00b7frau", "zum", "wei\u00b7sen", "Meis\u00b7ter", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sie leugnet selber nicht und nimmt den Titul an:", "tokens": ["Sie", "leug\u00b7net", "sel\u00b7ber", "nicht", "und", "nimmt", "den", "Ti\u00b7tul", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das, was uns noch an ihr mit Recht vergn\u00fcgen kan,", "tokens": ["Das", ",", "was", "uns", "noch", "an", "ihr", "mit", "Recht", "ver\u00b7gn\u00fc\u00b7gen", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADV", "APPR", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.43": {"text": "Sey ausgelecktes", "tokens": ["Sey", "aus\u00b7ge\u00b7leck\u00b7tes"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.44": {"text": "Der ihre Schand entdeckt und allen feil getragen.", "tokens": ["Der", "ih\u00b7re", "Schand", "ent\u00b7deckt", "und", "al\u00b7len", "feil", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Bekehrt dich dies noch nicht und hastu gleichwohl Lust,", "tokens": ["Be\u00b7kehrt", "dich", "dies", "noch", "nicht", "und", "has\u00b7tu", "gleich\u00b7wohl", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ADV", "PTKNEG", "KON", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "So glaub ich, da\u00df du wohl viel Mitgift suchen must.", "tokens": ["So", "glaub", "ich", ",", "da\u00df", "du", "wohl", "viel", "Mit\u00b7gift", "su\u00b7chen", "must", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Allein was ist ihr Schaz? Eintausend auf Papiere,", "tokens": ["Al\u00b7lein", "was", "ist", "ihr", "Schaz", "?", "Ein\u00b7tau\u00b7send", "auf", "Pa\u00b7pie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "PPOSAT", "NN", "$.", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ein kalt- und saurer Rausch von ihrem Hochzeitbiere,", "tokens": ["Ein", "kal\u00b7t", "und", "sau\u00b7rer", "Rausch", "von", "ih\u00b7rem", "Hoch\u00b7zeit\u00b7bie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.49": {"text": "Ein halb Collegium, ein klein- und blauer Hut,", "tokens": ["Ein", "halb", "Col\u00b7le\u00b7gi\u00b7um", ",", "ein", "klein", "und", "blau\u00b7er", "Hut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.50": {"text": "Der wieder Frost und Schwei\u00df gar schlechte Dienste thut.", "tokens": ["Der", "wie\u00b7der", "Frost", "und", "Schwei\u00df", "gar", "schlech\u00b7te", "Diens\u00b7te", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "KON", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ey, nimm doch Warnung an und las sie ungeschoren!", "tokens": ["Ey", ",", "nimm", "doch", "War\u00b7nung", "an", "und", "las", "sie", "un\u00b7ge\u00b7scho\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ADV", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Die Themis, so dich l\u00e4ngst vor ihren Leib erkohren,", "tokens": ["Die", "The\u00b7mis", ",", "so", "dich", "l\u00e4ngst", "vor", "ih\u00b7ren", "Leib", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ADV", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Sieht sch\u00f6n- und be\u00dfer aus und schenckt dir h\u00f6hern Rang", "tokens": ["Sieht", "sch\u00f6n", "und", "be\u00b7\u00dfer", "aus", "und", "schenckt", "dir", "h\u00f6\u00b7hern", "Rang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "TRUNC", "KON", "ADJD", "PTKVZ", "KON", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Als jener Zwietrachtssiz der Opponentenbanck,", "tokens": ["Als", "je\u00b7ner", "Zwiet\u00b7rachts\u00b7siz", "der", "Op\u00b7po\u00b7nen\u00b7ten\u00b7banck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Wo Warheit l\u00fcgen mu\u00df und Menschen dieser Erden", "