{"dta.poem.7423": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Freiheit eine Gasse!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vorm Feinde stand in Reih' und Glied", "tokens": ["Vorm", "Fein\u00b7de", "stand", "in", "Reih'", "und", "Glied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Volk um seine Fahnen,", "tokens": ["Das", "Volk", "um", "sei\u00b7ne", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da rief Herr Struthahn Winkelried:", "tokens": ["Da", "rief", "Herr", "Strut\u00b7hahn", "Win\u00b7kel\u00b7ried", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eich will den Weg Euch bahnen!", "tokens": ["\u201e", "ich", "will", "den", "Weg", "Euch", "bah\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201edir, Gott, befehl' ich Weib und Kind,", "tokens": ["\u201e", "dir", ",", "Gott", ",", "be\u00b7fehl'", "ich", "Weib", "und", "Kind", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ich auf Erden lasse \u2014 \u201c", "tokens": ["Die", "ich", "auf", "Er\u00b7den", "las\u00b7se", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und also sprengt' er pfeilgeschwind", "tokens": ["Und", "al\u00b7so", "sprengt'", "er", "pfeil\u00b7ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Freiheit eine Gasse.", "tokens": ["Der", "Frei\u00b7heit", "ei\u00b7ne", "Gas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das war ein Ritter noch mit Fug,", "tokens": ["Das", "war", "ein", "Rit\u00b7ter", "noch", "mit", "Fug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wie ein hei\u00df Gewitter", "tokens": ["Der", "wie", "ein", "hei\u00df", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Knechte vor sich niederschlug \u2014", "tokens": ["Die", "Knech\u00b7te", "vor", "sich", "nie\u00b7der\u00b7schlug"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O w\u00e4r' ich solch ein Ritter,", "tokens": ["O", "w\u00e4r'", "ich", "solch", "ein", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf stolzem Ro\u00df von schnellem Huf,", "tokens": ["Auf", "stol\u00b7zem", "Ro\u00df", "von", "schnel\u00b7lem", "Huf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In schimmerndem K\u00fcrasse,", "tokens": ["In", "schim\u00b7mern\u00b7dem", "K\u00fc\u00b7ras\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu sterben mit dem Donnerruf:", "tokens": ["Zu", "ster\u00b7ben", "mit", "dem", "Don\u00b7ner\u00b7ruf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Freiheit eine Gasse!", "tokens": ["Der", "Frei\u00b7heit", "ei\u00b7ne", "Gas\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch zittert nicht! ich bin allein,", "tokens": ["Doch", "zit\u00b7tert", "nicht", "!", "ich", "bin", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allein mit meinem Grimme;", "tokens": ["Al\u00b7lein", "mit", "mei\u00b7nem", "Grim\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie k\u00f6nnt' ich Euch gef\u00e4hrlich sein", "tokens": ["Wie", "k\u00f6nnt'", "ich", "Euch", "ge\u00b7f\u00e4hr\u00b7lich", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meiner schwachen Stimme?", "tokens": ["Mit", "mei\u00b7ner", "schwa\u00b7chen", "Stim\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Herrscher bildet sein Spalier,", "tokens": ["Dem", "Herr\u00b7scher", "bil\u00b7det", "sein", "Spa\u00b7lier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sonst, des Volkes Masse,", "tokens": ["Wie", "sonst", ",", "des", "Vol\u00b7kes", "Mas\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Niemand, Niemand ruft mit mir:", "tokens": ["Und", "Nie\u00b7mand", ",", "Nie\u00b7mand", "ruft", "mit", "mir", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Freiheit eine Gasse!", "tokens": ["Der", "Frei\u00b7heit", "ei\u00b7ne", "Gas\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr Deutschen ebnet Berg und Thal", "tokens": ["Ihr", "Deut\u00b7schen", "eb\u00b7net", "Berg", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Eure Feuerwagen,", "tokens": ["F\u00fcr", "Eu\u00b7re", "Feu\u00b7er\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man sieht auf Strassen ohne Zahl", "tokens": ["Man", "sieht", "auf", "Stras\u00b7sen", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch durch die L\u00e4nder jagen;", "tokens": ["Euch", "durch", "die", "L\u00e4n\u00b7der", "ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auch dieser Dampf ist Opferdampf \u2014", "tokens": ["Auch", "die\u00b7ser", "Dampf", "ist", "Op\u00b7fer\u00b7dampf"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Glaubt nicht, da\u00df ich ihn hasse \u2014", "tokens": ["Glaubt", "nicht", ",", "da\u00df", "ich", "ihn", "has\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch bahnet erst in Streit und Kampf", "tokens": ["Doch", "bah\u00b7net", "erst", "in", "Streit", "und", "Kampf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Freiheit eine Gasse!", "tokens": ["Der", "Frei\u00b7heit", "ei\u00b7ne", "Gas\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn alle Welt den Mut verlor,", "tokens": ["Wenn", "al\u00b7le", "Welt", "den", "Mut", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fehde zu beginnen,", "tokens": ["Die", "Feh\u00b7de", "zu", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Tritt Du, mein Volk, den V\u00f6lkern vor,", "tokens": ["Tritt", "Du", ",", "mein", "Volk", ",", "den", "V\u00f6l\u00b7kern", "vor", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df Du Dein Herzblut rinnen!", "tokens": ["La\u00df", "Du", "Dein", "Herz\u00b7blut", "rin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gib uns den Mann, der das Panier", "tokens": ["Gib", "uns", "den", "Mann", ",", "der", "das", "Pa\u00b7nier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der neuen Zeit erfasse,", "tokens": ["Der", "neu\u00b7en", "Zeit", "er\u00b7fas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und durch Europa brechen wir", "tokens": ["Und", "durch", "Eu\u00b7ro\u00b7pa", "bre\u00b7chen", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Freiheit eine Gasse!", "tokens": ["Der", "Frei\u00b7heit", "ei\u00b7ne", "Gas\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}