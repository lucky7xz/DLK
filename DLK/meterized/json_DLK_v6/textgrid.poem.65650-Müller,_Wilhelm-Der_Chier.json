{"textgrid.poem.65650": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Chier", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatt' ein sch\u00f6nes Schlo\u00df mit hohen, blanken Zinnen,", "tokens": ["Ich", "hatt'", "ein", "sch\u00f6\u00b7nes", "Schlo\u00df", "mit", "ho\u00b7hen", ",", "blan\u00b7ken", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mancherlei Geschirr von Gold und Silber drinnen;", "tokens": ["Und", "man\u00b7cher\u00b7lei", "Ge\u00b7schirr", "von", "Gold", "und", "Sil\u00b7ber", "drin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wenn ich von dem Dach hinab mein Auge schickte,", "tokens": ["Und", "wenn", "ich", "von", "dem", "Dach", "hin\u00b7ab", "mein", "Au\u00b7ge", "schick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "War alles meine Flur, was es rundum erblickte.", "tokens": ["War", "al\u00b7les", "mei\u00b7ne", "Flur", ",", "was", "es", "run\u00b7dum", "er\u00b7blick\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ich hatt' ein edles Weib, die Flamme meiner Jugend,", "tokens": ["Ich", "hatt'", "ein", "ed\u00b7les", "Weib", ",", "die", "Flam\u00b7me", "mei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Herrin jeder Huld, das Abbild aller Tugend.", "tokens": ["Die", "Her\u00b7rin", "je\u00b7der", "Huld", ",", "das", "Ab\u00b7bild", "al\u00b7ler", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drei S\u00f6hne hatt' ich auch in rother Knabenbl\u00fcthe,", "tokens": ["Drei", "S\u00f6h\u00b7ne", "hatt'", "ich", "auch", "in", "ro\u00b7ther", "Kna\u00b7ben\u00b7bl\u00fct\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In deren klarem Blick ein Hoffnungsmorgen gl\u00fchte,", "tokens": ["In", "de\u00b7ren", "kla\u00b7rem", "Blick", "ein", "Hoff\u00b7nungs\u00b7mor\u00b7gen", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der einen Tag verhie\u00df von reiner, steter Sonne.", "tokens": ["Der", "ei\u00b7nen", "Tag", "ver\u00b7hie\u00df", "von", "rei\u00b7ner", ",", "ste\u00b7ter", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich hatt' ein T\u00f6chterlein, der Mutter bange Wonne,", "tokens": ["Ich", "hatt'", "ein", "T\u00f6ch\u00b7ter\u00b7lein", ",", "der", "Mut\u00b7ter", "ban\u00b7ge", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Halb Jungfrau und halb Kind, ein R\u00f6slein, das die Schale", "tokens": ["Halb", "Jung\u00b7frau", "und", "halb", "Kind", ",", "ein", "R\u00f6s\u00b7lein", ",", "das", "die", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Knospe scheu und froh durchblickt zum ersten Male. \u2013", "tokens": ["Der", "Knos\u00b7pe", "scheu", "und", "froh", "durch\u00b7blickt", "zum", "ers\u00b7ten", "Ma\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "VVPP", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nun hab' ich nichts, als mich und eine scharfe Klinge,", "tokens": ["Nun", "hab'", "ich", "nichts", ",", "als", "mich", "und", "ei\u00b7ne", "schar\u00b7fe", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$,", "KOUS", "PPER", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und wenn ich meinen Stahl auf die Barbaren schwinge,", "tokens": ["Und", "wenn", "ich", "mei\u00b7nen", "Stahl", "auf", "die", "Bar\u00b7ba\u00b7ren", "schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "F\u00fchl' ich mich wunderreich. Bald hab' ich alles wieder,", "tokens": ["F\u00fchl'", "ich", "mich", "wun\u00b7der\u00b7reich", ".", "Bald", "hab'", "ich", "al\u00b7les", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "$.", "ADV", "VAFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wann um mich weit und breit zerst\u00fcckte T\u00fcrkenglieder,", "tokens": ["Wann", "um", "mich", "weit", "und", "breit", "zer\u00b7st\u00fcck\u00b7te", "T\u00fcr\u00b7ken\u00b7glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOUI", "PRF", "ADJD", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zu Bergen aufgeh\u00e4uft, als Rachemahle prangen.", "tokens": ["Zu", "Ber\u00b7gen", "auf\u00b7ge\u00b7h\u00e4uft", ",", "als", "Ra\u00b7che\u00b7mah\u00b7le", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dann ist es satt getr\u00e4nkt, das br\u00fcnstige Verlangen", "tokens": ["Dann", "ist", "es", "satt", "ge\u00b7tr\u00e4nkt", ",", "das", "br\u00fcns\u00b7ti\u00b7ge", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nach meinem edlen Gut, und \u00fcber meinen Sch\u00e4tzen", "tokens": ["Nach", "mei\u00b7nem", "ed\u00b7len", "Gut", ",", "und", "\u00fc\u00b7ber", "mei\u00b7nen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Lieg' ich dahingestreckt, nicht todt daran zu letzen.", "tokens": ["Lieg'", "ich", "da\u00b7hin\u00b7ge\u00b7streckt", ",", "nicht", "todt", "da\u00b7ran", "zu", "let\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "PTKNEG", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}