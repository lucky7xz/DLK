{"textgrid.poem.60247": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "Trostgesang f\u00fcr Neu-Ruppin", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blick auf! blick auf von deinem Aschenh\u00fcgel,", "tokens": ["Blick", "auf", "!", "blick", "auf", "von", "dei\u00b7nem", "A\u00b7schen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinauf zum Herrn, den keiner fragen darf,", "tokens": ["Hin\u00b7auf", "zum", "Herrn", ",", "den", "kei\u00b7ner", "fra\u00b7gen", "darf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Warum er schnell durch seines Sturmwinds Fl\u00fcgel", "tokens": ["Wa\u00b7rum", "er", "schnell", "durch", "sei\u00b7nes", "Sturm\u00b7winds", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In deinen Kranz den Feuerwirbel warf?", "tokens": ["In", "dei\u00b7nen", "Kranz", "den", "Feu\u00b7er\u00b7wir\u00b7bel", "warf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im vollen Schmuck sah dich der Mittag schimmern,", "tokens": ["Im", "vol\u00b7len", "Schmuck", "sah", "dich", "der", "Mit\u00b7tag", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und traurig sah die Abendsonne sich", "tokens": ["Und", "trau\u00b7rig", "sah", "die", "A\u00b7bend\u00b7son\u00b7ne", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch einmal um, du lagst bei deinen Tr\u00fcmmern", "tokens": ["Noch", "ein\u00b7mal", "um", ",", "du", "lagst", "bei", "dei\u00b7nen", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verh\u00fcllt in Dampf, und weintest bitterlich.", "tokens": ["Ver\u00b7h\u00fcllt", "in", "Dampf", ",", "und", "wein\u00b7test", "bit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gott h\u00f6rt die Brut verla\u00dfner Waldesnester,", "tokens": ["Gott", "h\u00f6rt", "die", "Brut", "ver\u00b7la\u00df\u00b7ner", "Wal\u00b7des\u00b7nes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er h\u00f6rt nach Brod auch deine Kinder schreyn;", "tokens": ["Er", "h\u00f6rt", "nach", "Brod", "auch", "dei\u00b7ne", "Kin\u00b7der", "schreyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er haucht in deine k\u00f6nigliche Schwester,", "tokens": ["Er", "haucht", "in", "dei\u00b7ne", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In sein Berlin, den Geist des Mitleids ein.", "tokens": ["In", "sein", "Ber\u00b7lin", ",", "den", "Geist", "des", "Mit\u00b7leids", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "$,", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Blick auf! und schau dahin nach jener Seite,", "tokens": ["Blick", "auf", "!", "und", "schau", "da\u00b7hin", "nach", "je\u00b7ner", "Sei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "KON", "ADJD", "PAV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da kam der Sturm, gewaltig wie das Meer,", "tokens": ["Da", "kam", "der", "Sturm", ",", "ge\u00b7wal\u00b7tig", "wie", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und st\u00fcrzte dich zum Staub herab, und heute", "tokens": ["Und", "st\u00fcrz\u00b7te", "dich", "zum", "Staub", "her\u00b7ab", ",", "und", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "K\u00f6mmt wie vom Himmel Trost f\u00fcr dich daher.", "tokens": ["K\u00f6mmt", "wie", "vom", "Him\u00b7mel", "Trost", "f\u00fcr", "dich", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPRART", "NN", "NN", "APPR", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da kommen Wagen dir so vollgeh\u00e4ufet", "tokens": ["Da", "kom\u00b7men", "Wa\u00b7gen", "dir", "so", "voll\u00b7ge\u00b7h\u00e4u\u00b7fet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Wagen, die das Erndtevolk regiert,", "tokens": ["Wie", "Wa\u00b7gen", ",", "die", "das", "Ernd\u00b7te\u00b7volk", "re\u00b7giert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenns Weizen, den die Sonnenglut gereifet,", "tokens": ["Wenns", "Wei\u00b7zen", ",", "den", "die", "Son\u00b7nen\u00b7glut", "ge\u00b7rei\u00b7fet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Lobgesang ins frohe D\u00f6rfchen f\u00fchrt.", "tokens": ["Mit", "Lob\u00b7ge\u00b7sang", "ins", "fro\u00b7he", "D\u00f6rf\u00b7chen", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die M\u00e4nner und die Frauen frommer Sitte", "tokens": ["Die", "M\u00e4n\u00b7ner", "und", "die", "Frau\u00b7en", "from\u00b7mer", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die theilten ihren Kleiderschrank mit dir,", "tokens": ["Die", "theil\u00b7ten", "ih\u00b7ren", "Klei\u00b7der\u00b7schrank", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Pallast an bis zu der kleinsten H\u00fctte", "tokens": ["Vom", "Pal\u00b7last", "an", "bis", "zu", "der", "kleins\u00b7ten", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herrscht Th\u00e4tigkeit f\u00fcr deine H\u00fclfbegier.", "tokens": ["Herrscht", "Th\u00e4\u00b7tig\u00b7keit", "f\u00fcr", "dei\u00b7ne", "H\u00fclf\u00b7be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Kaum kann der Mai mehr auszusch\u00fctteln haben,", "tokens": ["Kaum", "kann", "der", "Mai", "mehr", "aus\u00b7zu\u00b7sch\u00fct\u00b7teln", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ihn die Zeit sein F\u00fcllhorn schwingen l\u00e4\u00dft;", "tokens": ["Wenn", "ihn", "die", "Zeit", "sein", "F\u00fcll\u00b7horn", "schwin\u00b7gen", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kaum giebt der Herbst uns mehr Erquickungsgaben,", "tokens": ["Kaum", "giebt", "der", "Herbst", "uns", "mehr", "Er\u00b7quic\u00b7kungs\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als dir Berlin zum s\u00fc\u00dfen Labefest.", "tokens": ["Als", "dir", "Ber\u00b7lin", "zum", "s\u00fc\u00b7\u00dfen", "La\u00b7be\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Im Umfang ihrer Mauern wohnet keiner,", "tokens": ["Im", "Um\u00b7fang", "ih\u00b7rer", "Mau\u00b7ern", "woh\u00b7net", "kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der nicht f\u00fcr dich zum Wohlthun ward ger\u00fchet;", "tokens": ["Der", "nicht", "f\u00fcr", "dich", "zum", "Wohl\u00b7thun", "ward", "ge\u00b7r\u00fc\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Nation gedenkt auch th\u00e4tig deiner,", "tokens": ["Die", "Na\u00b7tion", "ge\u00b7denkt", "auch", "th\u00e4\u00b7tig", "dei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "PPOSAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die m\u00e4chtig aus Egypten ward gef\u00fchrt", "tokens": ["Die", "m\u00e4ch\u00b7tig", "aus", "E\u00b7gyp\u00b7ten", "ward", "ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Nimm was da k\u00f6mmt, und eile Dank zu sagen", "tokens": ["Nimm", "was", "da", "k\u00f6mmt", ",", "und", "ei\u00b7le", "Dank", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PWS", "ADV", "VVFIN", "$,", "KON", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(im Tempel, den die Flamme nicht ber\u00fchrt)", "tokens": ["(", "im", "Tem\u00b7pel", ",", "den", "die", "Flam\u00b7me", "nicht", "be\u00b7r\u00fchrt", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Vaterhand, die dich so hart geschlagen,", "tokens": ["Der", "Va\u00b7ter\u00b7hand", ",", "die", "dich", "so", "hart", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dir zum Heil die Herzen jezt regiert.", "tokens": ["Und", "dir", "zum", "Heil", "die", "Her\u00b7zen", "jezt", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Sie hats der Flamme, hats dem Sturm geboten;", "tokens": ["Sie", "hats", "der", "Flam\u00b7me", ",", "hats", "dem", "Sturm", "ge\u00b7bo\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis hieher und nicht weiter sollt ihr gehn,", "tokens": ["Bis", "hie\u00b7her", "und", "nicht", "wei\u00b7ter", "sollt", "ihr", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "KON", "PTKNEG", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie hei\u00dft im Glanz, wie auferweckte Todten,", "tokens": ["Sie", "hei\u00dft", "im", "Glanz", ",", "wie", "au\u00b7fer\u00b7weck\u00b7te", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die H\u00e4user und die Tempel neu entstehn.", "tokens": ["Die", "H\u00e4u\u00b7ser", "und", "die", "Tem\u00b7pel", "neu", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Du wirst es sehn, wirst nicht die Hand verkennen,", "tokens": ["Du", "wirst", "es", "sehn", ",", "wirst", "nicht", "die", "Hand", "ver\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "VAFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn h\u00f6her dich dein K\u00f6nig hebt empor;", "tokens": ["Wenn", "h\u00f6\u00b7her", "dich", "dein", "K\u00f6\u00b7nig", "hebt", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann werden dich die Schwestern sch\u00f6ner nennen,", "tokens": ["Dann", "wer\u00b7den", "dich", "die", "Schwes\u00b7tern", "sch\u00f6\u00b7ner", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seliger dich preisen wie zuvor.", "tokens": ["Und", "se\u00b7li\u00b7ger", "dich", "prei\u00b7sen", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Sie seufzen alle mit in deine Klagen,", "tokens": ["Sie", "seuf\u00b7zen", "al\u00b7le", "mit", "in", "dei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stellen einen edlen Wettlauf an,", "tokens": ["Und", "stel\u00b7len", "ei\u00b7nen", "ed\u00b7len", "Wett\u00b7lauf", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir wie auf Windesfl\u00fcgeln zuzutragen", "tokens": ["Dir", "wie", "auf", "Win\u00b7des\u00b7fl\u00fc\u00b7geln", "zu\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KOKOM", "APPR", "NN", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Trost, der dich wieder freudig machen kann.", "tokens": ["Trost", ",", "der", "dich", "wie\u00b7der", "freu\u00b7dig", "ma\u00b7chen", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}