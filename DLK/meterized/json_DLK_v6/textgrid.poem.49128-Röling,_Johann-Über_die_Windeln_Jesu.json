{"textgrid.poem.49128": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcber die Windeln Jesu", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie windelt man dich, Jesu, ein,", "tokens": ["Wie", "win\u00b7delt", "man", "dich", ",", "Je\u00b7su", ",", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PPER", "$,", "NE", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn alle Himmel sind zu klein,", "tokens": ["Denn", "al\u00b7le", "Him\u00b7mel", "sind", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und der die gantze Welt bewegt,", "tokens": ["Und", "der", "die", "gant\u00b7ze", "Welt", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird unbeweglich hingelegt.", "tokens": ["Wird", "un\u00b7be\u00b7weg\u00b7lich", "hin\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr, dies macht deine Lieb' und Huld;", "tokens": ["Herr", ",", "dies", "macht", "dei\u00b7ne", "Lieb'", "und", "Huld", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wir, die die Fesseln vieler Schuld", "tokens": ["Wir", ",", "die", "die", "Fes\u00b7seln", "vie\u00b7ler", "Schuld"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Tode l\u00e4ngst gegeben ein,", "tokens": ["Dem", "To\u00b7de", "l\u00e4ngst", "ge\u00b7ge\u00b7ben", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sollen ewig Schlaven seyn.", "tokens": ["Wir", "sol\u00b7len", "e\u00b7wig", "Schla\u00b7ven", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, mehr als wohlverdientes Joch!", "tokens": ["Ach", ",", "mehr", "als", "wohl\u00b7ver\u00b7dien\u00b7tes", "Joch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und du g\u00f6nnst uns die Freiheit doch.", "tokens": ["Und", "du", "g\u00f6nnst", "uns", "die", "Frei\u00b7heit", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nichts halff uns au\u00df, nichts stand uns bey,", "tokens": ["Nichts", "halff", "uns", "au\u00df", ",", "nichts", "stand", "uns", "bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So k\u00f6mmstu selbst und machst uns frey.", "tokens": ["So", "k\u00f6mms\u00b7tu", "selbst", "und", "machst", "uns", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du legst die Banden wieder an,", "tokens": ["Du", "legst", "die", "Ban\u00b7den", "wie\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem du uns sie abgethan", "tokens": ["Nach\u00b7dem", "du", "uns", "sie", "ab\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gehst so schwere Knechtschafft ein,", "tokens": ["Und", "gehst", "so", "schwe\u00b7re", "Knecht\u00b7schafft", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auff da\u00df wir Herren m\u00f6chten seyn.", "tokens": ["Auff", "da\u00df", "wir", "Her\u00b7ren", "m\u00f6ch\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O, eine Freundschafft, der die Welt", "tokens": ["O", ",", "ei\u00b7ne", "Freund\u00b7schafft", ",", "der", "die", "Welt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Beyspiel halten wird noch h\u00e4lt,", "tokens": ["Kein", "Bey\u00b7spiel", "hal\u00b7ten", "wird", "noch", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja wol, die Welt, da Jedermann", "tokens": ["Ja", "wol", ",", "die", "Welt", ",", "da", "Je\u00b7der\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "ADV", "$,", "ART", "NN", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Andern st\u00fcrtzt, so gut er kan.", "tokens": ["Den", "An\u00b7dern", "st\u00fcrtzt", ",", "so", "gut", "er", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gott seyn und nehmen Menschheit an,", "tokens": ["Gott", "seyn", "und", "neh\u00b7men", "Menschheit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dem dienen, der uns unterthan,", "tokens": ["Dem", "die\u00b7nen", ",", "der", "uns", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem helffen, der sein \u00e4rgster Feind,", "tokens": ["Dem", "helf\u00b7fen", ",", "der", "sein", "\u00e4rgs\u00b7ter", "Feind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies thut allein ein Himmels-Freund.", "tokens": ["Dies", "thut", "al\u00b7lein", "ein", "Him\u00b7mels\u00b7Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Entbind mich, Jesu, meiner S\u00fcnd", "tokens": ["Ent\u00b7bind", "mich", ",", "Je\u00b7su", ",", "mei\u00b7ner", "S\u00fcnd"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00f6se mich, da ich dich bind,", "tokens": ["Und", "l\u00f6\u00b7se", "mich", ",", "da", "ich", "dich", "bind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So windel' ich mich zu dir ein", "tokens": ["So", "win\u00b7del'", "ich", "mich", "zu", "dir", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wil dir stets verbunden seyn.", "tokens": ["Und", "wil", "dir", "stets", "ver\u00b7bun\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}