{"textgrid.poem.67039": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An meinen B.", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund! wo \u00fcber das Tal schauerlich Wald und Fels", "tokens": ["Freund", "!", "wo", "\u00fc\u00b7ber", "das", "Tal", "schau\u00b7er\u00b7lich", "Wald", "und", "Fels"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "APPR", "ART", "NN", "ADJD", "NN", "KON", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Herh\u00e4ngt, wo das Gefild leise die Erms durchschleicht,", "tokens": ["Her\u00b7h\u00e4ngt", ",", "wo", "das", "Ge\u00b7fild", "lei\u00b7se", "die", "Erms", "durch\u00b7schleicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und das Reh des Geb\u00fcrges", "tokens": ["Und", "das", "Reh", "des", "Ge\u00b7b\u00fcr\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Stolz an ihrem Gestade geht \u2013", "tokens": ["Stolz", "an", "ih\u00b7rem", "Ge\u00b7sta\u00b7de", "geht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wo im Knabengelock heiter und unschuldsvoll", "tokens": ["Wo", "im", "Kna\u00b7ben\u00b7ge\u00b7lock", "hei\u00b7ter", "und", "un\u00b7schulds\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJD", "KON", "ADJD"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wenge Stunden mir einst l\u00e4chelnd vor\u00fcberflohn \u2013", "tokens": ["Wen\u00b7ge", "Stun\u00b7den", "mir", "einst", "l\u00e4\u00b7chelnd", "vor\u00b7\u00fc\u00b7berf\u00b7lohn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "ADJD", "VVIZU", "$("], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dort sind H\u00fctten des Segens,", "tokens": ["Dort", "sind", "H\u00fct\u00b7ten", "des", "Se\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Freund! \u2013 du kennest die H\u00fctten auch;", "tokens": ["Freund", "!", "\u2013", "du", "ken\u00b7nest", "die", "H\u00fct\u00b7ten", "auch", ";"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Dort am schattichten Hain wandelt Amalia.", "tokens": ["Dort", "am", "schat\u00b7tich\u00b7ten", "Hain", "wan\u00b7delt", "A\u00b7ma\u00b7lia", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "NE", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Segne, segne mein Lied, kr\u00e4nze die Harfe mir,", "tokens": ["Seg\u00b7ne", ",", "seg\u00b7ne", "mein", "Lied", ",", "kr\u00e4n\u00b7ze", "die", "Har\u00b7fe", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Denn sie nannte den Namen,", "tokens": ["Denn", "sie", "nann\u00b7te", "den", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Den, du wei\u00dfts, des Get\u00fcmmels Ohr", "tokens": ["Den", ",", "du", "wei\u00dfts", ",", "des", "Ge\u00b7t\u00fcm\u00b7mels", "Ohr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Nicht zu kennen verdient. Stille, der Tugend nur", "tokens": ["Nicht", "zu", "ken\u00b7nen", "ver\u00b7dient", ".", "Stil\u00b7le", ",", "der", "Tu\u00b7gend", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PTKZU", "VVINF", "VVPP", "$.", "NN", "$,", "ART", "NN", "ADV"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und der Freundschaft bekannt, wandelt die Gute dort.", "tokens": ["Und", "der", "Freund\u00b7schaft", "be\u00b7kannt", ",", "wan\u00b7delt", "die", "Gu\u00b7te", "dort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Liebes M\u00e4dchen, es tr\u00fcbe", "tokens": ["Lie\u00b7bes", "M\u00e4d\u00b7chen", ",", "es", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nie dein himmlisches Auge sich.", "tokens": ["Nie", "dein", "himm\u00b7li\u00b7sches", "Au\u00b7ge", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}