{"textgrid.poem.65054": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stumm hing die Harfe an der Wand,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stumm hing die Harfe an der Wand,", "tokens": ["Stumm", "hing", "die", "Har\u00b7fe", "an", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam der West gegangen,", "tokens": ["Da", "kam", "der", "West", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df, wie ber\u00fchrt von Geisterhand,", "tokens": ["Da\u00df", ",", "wie", "be\u00b7r\u00fchrt", "von", "Geis\u00b7ter\u00b7hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Saiten hell erklangen.", "tokens": ["Die", "Sai\u00b7ten", "hell", "er\u00b7klan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Stumm war mein Herz, da wehte mild", "tokens": ["Stumm", "war", "mein", "Herz", ",", "da", "weh\u00b7te", "mild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Liebeshauch dar\u00fcber,", "tokens": ["Ein", "Lie\u00b7bes\u00b7hauch", "da\u00b7r\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus den durchseelten Saiten quillt", "tokens": ["Aus", "den", "durch\u00b7seel\u00b7ten", "Sai\u00b7ten", "quillt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Sang, ein selig tr\u00fcber.", "tokens": ["Ein", "Sang", ",", "ein", "se\u00b7lig", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}