{"dta.poem.12889": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Schertz-gedichte an Daphnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, Daphne! hilff mir doch! es ist mit mir geschehn:", "tokens": ["Ach", ",", "Daph\u00b7ne", "!", "hilff", "mir", "doch", "!", "es", "ist", "mit", "mir", "ge\u00b7schehn", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "NN", "PPER", "ADV", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mi\u00dfvergn\u00fcgte mund z\u00f6rnt und bekriegt die augen;", "tokens": ["Der", "mi\u00df\u00b7ver\u00b7gn\u00fcg\u00b7te", "mund", "z\u00f6rnt", "und", "be\u00b7kriegt", "die", "au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dieweil sie ihre lust fast alle tage sehn,", "tokens": ["Die\u00b7weil", "sie", "ih\u00b7re", "lust", "fast", "al\u00b7le", "ta\u00b7ge", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seine lippen nur am hunger-tuche saugen.", "tokens": ["Und", "sei\u00b7ne", "lip\u00b7pen", "nur", "am", "hun\u00b7ger\u00b7tu\u00b7che", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum komm und lege dich in diese h\u00e4ndel ein,", "tokens": ["Drum", "komm", "und", "le\u00b7ge", "dich", "in", "die\u00b7se", "h\u00e4n\u00b7del", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und la\u00df Leandern nicht so lang in sorgen stehen;", "tokens": ["Und", "la\u00df", "Le\u00b7an\u00b7dern", "nicht", "so", "lang", "in", "sor\u00b7gen", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "NN", "PTKNEG", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch soll der friede bald und wohl von statten gehen,", "tokens": ["Doch", "soll", "der", "frie\u00b7de", "bald", "und", "wohl", "von", "stat\u00b7ten", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "KON", "ADV", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So mu\u00df der handlungs-ort auf deinem munde seyn.", "tokens": ["So", "mu\u00df", "der", "hand\u00b7lungs\u00b7ort", "auf", "dei\u00b7nem", "mun\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}