{"dta.poem.8739": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Du darfst es nicht ergr\u00fcnden,", "tokens": ["Du", "darfst", "es", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es sagte dir's die Sympathie,", "tokens": ["Es", "sag\u00b7te", "dir's", "die", "Sym\u00b7pa\u00b7thie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn wir uns ganz verst\u00fcnden.", "tokens": ["Wenn", "wir", "uns", "ganz", "ver\u00b7st\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht jeder ird'sche Geist erkennt", "tokens": ["Nicht", "je\u00b7der", "ird'\u00b7sche", "Geist", "er\u00b7kennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein eignes Loos hienieden:", "tokens": ["Sein", "eig\u00b7nes", "Loos", "hien\u00b7ie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht weiter frage, was uns trennt,", "tokens": ["Nicht", "wei\u00b7ter", "fra\u00b7ge", ",", "was", "uns", "trennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Genug, wir sind geschieden!", "tokens": ["Ge\u00b7nug", ",", "wir", "sind", "ge\u00b7schie\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es spornt mich ja nicht eitle Kraft,", "tokens": ["Es", "spornt", "mich", "ja", "nicht", "eit\u00b7le", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich am Geschick zu proben:", "tokens": ["Mich", "am", "Ge\u00b7schick", "zu", "pro\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir alle geben Rechenschaft", "tokens": ["Wir", "al\u00b7le", "ge\u00b7ben", "Re\u00b7chen\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PIS", "VVINF", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr unsern Ruf von oben.", "tokens": ["F\u00fcr", "un\u00b7sern", "Ruf", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was um mich ist, err\u00e4th mich nicht,", "tokens": ["Was", "um", "mich", "ist", ",", "er\u00b7r\u00e4\u00b7th", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und dr\u00e4ngt und dr\u00fcckt mich nieder;", "tokens": ["Und", "dr\u00e4ngt", "und", "dr\u00fcckt", "mich", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch, such' ich Trost mir im Gedicht,", "tokens": ["Doch", ",", "such'", "ich", "Trost", "mir", "im", "Ge\u00b7dicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann find' ich ganz mich wieder!", "tokens": ["Dann", "find'", "ich", "ganz", "mich", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}