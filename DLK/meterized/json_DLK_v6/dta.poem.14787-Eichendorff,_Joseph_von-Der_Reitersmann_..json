{"dta.poem.14787": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Reitersmann .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eund hoch auf des Sees Weite,", "tokens": ["\u201e", "und", "hoch", "auf", "des", "Sees", "Wei\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Wenn alles finster und still,", "tokens": ["Wenn", "al\u00b7les", "fins\u00b7ter", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da rudern zwei stille Leute, \u2014", "tokens": ["Da", "ru\u00b7dern", "zwei", "stil\u00b7le", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Eine Dich haben will.\u201c", "tokens": ["Der", "Ei\u00b7ne", "Dich", "ha\u00b7ben", "will", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "PPER", "VAINF", "VMFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}