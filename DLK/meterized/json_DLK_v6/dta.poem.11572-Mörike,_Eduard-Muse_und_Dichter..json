{"dta.poem.11572": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Muse und Dichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egraben kann ich nicht: nun vollends krank! und zu", "tokens": ["\u201e", "gra\u00b7ben", "kann", "ich", "nicht", ":", "nun", "vol\u00b7lends", "krank", "!", "und", "zu"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVINF", "VMFIN", "PPER", "PTKNEG", "$.", "ADV", "ADV", "ADJD", "$.", "KON", "APPR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sch\u00e4me ich mich. Du schweigst, Muse? O rathe mir!", "tokens": ["Sch\u00e4\u00b7me", "ich", "mich", ".", "Du", "schweigst", ",", "Mu\u00b7se", "?", "O", "ra\u00b7the", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "PPER", "VVFIN", "$,", "NN", "$.", "NE", "VVFIN", "PPER", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Gib die Leyer!\u201c \u2014 Nicht doch! Dir ist die Ruhe", "tokens": ["Gib", "die", "Le\u00b7yer", "!", "\u201c", "Nicht", "doch", "!", "Dir", "ist", "die", "Ru\u00b7he"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "$.", "$(", "$(", "PTKNEG", "ADV", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "+-++-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Schlafe, tr\u00e4ume nur! still ruf' ich dir H\u00fclfe herab.", "tokens": ["Schla\u00b7fe", ",", "tr\u00e4u\u00b7me", "nur", "!", "still", "ruf'", "ich", "dir", "H\u00fcl\u00b7fe", "her\u00b7ab", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$.", "ADJD", "VVFIN", "PPER", "PPER", "NN", "ADV", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Deinem Haupte noch bl\u00fchet ein Kranz; und sey es zum", "tokens": ["Dei\u00b7nem", "Haup\u00b7te", "noch", "bl\u00fc\u00b7het", "ein", "Kranz", ";", "und", "sey", "es", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ART", "NN", "$.", "KON", "VAFIN", "PPER", "APPRART"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Sey's zum Tode; getrost! meine Hand windet ihn dir.", "tokens": ["Sey's", "zum", "To\u00b7de", ";", "ge\u00b7trost", "!", "mei\u00b7ne", "Hand", "win\u00b7det", "ihn", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$.", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+--+--++--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "\u201ekeinen Lorbeer will ich, die kalte Stirne zu schm\u00fccken:", "tokens": ["\u201e", "kei\u00b7nen", "Lor\u00b7beer", "will", "ich", ",", "die", "kal\u00b7te", "Stir\u00b7ne", "zu", "schm\u00fc\u00b7cken", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VMFIN", "PPER", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "La\u00df mich leben! und gib fr\u00f6hliche Blumen zum Strau\u00df.\u201c", "tokens": ["La\u00df", "mich", "le\u00b7ben", "!", "und", "gib", "fr\u00f6h\u00b7li\u00b7che", "Blu\u00b7men", "zum", "Strau\u00df", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "KON", "VVIMP", "ADJA", "NN", "APPRART", "NE", "$.", "$("], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}