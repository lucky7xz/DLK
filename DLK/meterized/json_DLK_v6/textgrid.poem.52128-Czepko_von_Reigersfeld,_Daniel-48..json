{"textgrid.poem.52128": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Todes Alkahest, das l\u00f6st mich auff allhier:", "tokens": ["Das", "To\u00b7des", "Al\u00b7ka\u00b7hest", ",", "das", "l\u00f6st", "mich", "auff", "all\u00b7hier", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PDS", "VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Freund, ich werd' auffstehn in Engels reiner Zier:", "tokens": ["Mein", "Freund", ",", "ich", "werd'", "auffs\u00b7tehn", "in", "En\u00b7gels", "rei\u00b7ner", "Zier", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "VVPP", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein schwartzes Ertzt wusch ich, und es ward wei\u00df und roth,", "tokens": ["Ein", "schwart\u00b7zes", "Ertzt", "wusch", "ich", ",", "und", "es", "ward", "wei\u00df", "und", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "KON", "PPER", "VAFIN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Giebt dieses uns die Kunst, was zweiffelst du an Gott?", "tokens": ["Giebt", "die\u00b7ses", "uns", "die", "Kunst", ",", "was", "zweif\u00b7felst", "du", "an", "Gott", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "po