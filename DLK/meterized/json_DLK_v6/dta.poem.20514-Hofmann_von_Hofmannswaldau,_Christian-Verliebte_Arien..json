{"dta.poem.20514": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Armseliger/ was hilfft dich doch dein lieben?", "tokens": ["Arm\u00b7se\u00b7li\u00b7ger", "/", "was", "hilfft", "dich", "doch", "dein", "lie\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Du liebest/ was nicht lieben kan/", "tokens": ["Du", "lie\u00b7best", "/", "was", "nicht", "lie\u00b7ben", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des himmels schlu\u00df hat dich itzt zwar getrieben/", "tokens": ["Des", "him\u00b7mels", "schlu\u00df", "hat", "dich", "itzt", "zwar", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch r\u00fchrst du was verbotnes an.", "tokens": ["Doch", "r\u00fchrst", "du", "was", "ver\u00b7bot\u00b7nes", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die sch\u00f6nheit/ die dein hertze sucht/", "tokens": ["Die", "sch\u00f6n\u00b7heit", "/", "die", "dein", "hert\u00b7ze", "sucht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist des verbotnen baumes frucht.", "tokens": ["Ist", "des", "ver\u00b7bot\u00b7nen", "bau\u00b7mes", "frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die hoffnung lud mich einsten zwar zu gaste/", "tokens": ["Die", "hoff\u00b7nung", "lud", "mich", "eins\u00b7ten", "zwar", "zu", "gas\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der neid l\u00e4st aber mich nicht ein/", "tokens": ["Der", "neid", "l\u00e4st", "a\u00b7ber", "mich", "nicht", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mi\u00dfgunst macht mir eine stete faste/", "tokens": ["Die", "mi\u00df\u00b7gunst", "macht", "mir", "ei\u00b7ne", "ste\u00b7te", "fas\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In der viel marter-wochen seyn.", "tokens": ["In", "der", "viel", "mar\u00b7ter\u00b7wo\u00b7chen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein paradie\u00df ist zugemacht/", "tokens": ["Mein", "pa\u00b7ra\u00b7die\u00df", "ist", "zu\u00b7ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wird von eyffersucht bewacht.", "tokens": ["Und", "wird", "von", "eyf\u00b7fer\u00b7sucht", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer lescht den durst mir nun in dieser w\u00fcste?", "tokens": ["Wer", "lescht", "den", "durst", "mir", "nun", "in", "die\u00b7ser", "w\u00fcs\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich leide noth bey \u00fcberflu\u00df.", "tokens": ["Ich", "lei\u00b7de", "noth", "bey", "\u00fc\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was n\u00fctzt der auszug engels gleicher l\u00fcste/", "tokens": ["Was", "n\u00fctzt", "der", "aus\u00b7zug", "en\u00b7gels", "glei\u00b7cher", "l\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NE", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil sie kein mund geniessen mu\u00df?", "tokens": ["Weil", "sie", "kein", "mund", "ge\u00b7nies\u00b7sen", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das auge/ das vergn\u00fcgung hat/", "tokens": ["Das", "au\u00b7ge", "/", "das", "ver\u00b7gn\u00fc\u00b7gung", "hat", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht durchs gesicht kein hertze satt.", "tokens": ["Macht", "durchs", "ge\u00b7sicht", "kein", "hert\u00b7ze", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PIAT", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ich k\u00fcsse gnug/ und spiele in gedancken/", "tokens": ["Ich", "k\u00fcs\u00b7se", "gnug", "/", "und", "spie\u00b7le", "in", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedancken aber speisen nicht.", "tokens": ["Ge\u00b7dan\u00b7cken", "a\u00b7ber", "spei\u00b7sen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Manch s\u00fcsser traum setzt mich in liebes-schrancken;", "tokens": ["Manch", "s\u00fcs\u00b7ser", "traum", "setzt", "mich", "in", "lie\u00b7bes\u00b7schran\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ist ein traum beym tage-licht?", "tokens": ["Was", "ist", "ein", "traum", "beym", "ta\u00b7ge\u00b7licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Di\u00df blendwerck schwindet wie ein dunst/", "tokens": ["Di\u00df", "blend\u00b7werck", "schwin\u00b7det", "wie", "ein", "dunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und das ergetzen ist umsunst.", "tokens": ["Und", "das", "er\u00b7get\u00b7zen", "ist", "um\u00b7sunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVINF", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So m\u00fch ich mich mit irrwisch und auch schatten/", "tokens": ["So", "m\u00fch", "ich", "mich", "mit", "irr\u00b7wisch", "und", "auch", "schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "APPR", "ADJD", "KON", "ADV", "VVFIN", "$("], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und der ichs klage/ lacht darzu;", "tokens": ["Und", "der", "ichs", "kla\u00b7ge", "/", "lacht", "dar\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VVFIN", "$(", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schw\u00e4rmen kaum ums licht so viel der matten/", "tokens": ["Es", "schw\u00e4r\u00b7men", "kaum", "ums", "licht", "so", "viel", "der", "mat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ADV", "ADV", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als seuffzer st\u00f6hren meine ruh.", "tokens": ["Als", "seuff\u00b7zer", "st\u00f6h\u00b7ren", "mei\u00b7ne", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein stetes w\u00fcnschen fri\u00dft mich ab/", "tokens": ["Ein", "ste\u00b7tes", "w\u00fcn\u00b7schen", "fri\u00dft", "mich", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nichts erhalten bringt das grab.", "tokens": ["Und", "nichts", "er\u00b7hal\u00b7ten", "bringt", "das", "grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Stirb/ Lysis/ stirb/ viel besser ists gestorben/", "tokens": ["Stirb", "/", "Ly\u00b7sis", "/", "stirb", "/", "viel", "bes\u00b7ser", "ists", "ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "NE", "$(", "VVIMP", "$(", "ADV", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ohne Fillis gunst gelebt;", "tokens": ["Als", "oh\u00b7ne", "Fil\u00b7lis", "gunst", "ge\u00b7lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer so verdirbt/ der ist/ traun/ wohl verdorben/", "tokens": ["Wer", "so", "ver\u00b7dirbt", "/", "der", "ist", "/", "traun", "/", "wohl", "ver\u00b7dor\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$(", "ART", "VAFIN", "$(", "VVINF", "$(", "ADV", "VVPP", "$("], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn ihn das gl\u00fccke so erhebt;", "tokens": ["Wenn", "ihn", "das", "gl\u00fc\u00b7cke", "so", "er\u00b7hebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vielleicht r\u00fchrt Fillis tod mich an/", "tokens": ["Viel\u00b7leicht", "r\u00fchrt", "Fil\u00b7lis", "tod", "mich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als sie im leben nicht gethan.", "tokens": ["Als", "sie", "im", "le\u00b7ben", "nicht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "VVFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}