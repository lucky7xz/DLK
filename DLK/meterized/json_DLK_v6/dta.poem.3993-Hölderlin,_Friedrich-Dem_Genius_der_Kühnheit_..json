{"dta.poem.3993": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Dem Genius der K\u00fchnheit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du? wie zur Beute, breitet", "tokens": ["Wer", "bist", "du", "?", "wie", "zur", "Beu\u00b7te", ",", "brei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PWAV", "APPRART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Unerme\u00dfliche vor dir sich aus,", "tokens": ["Das", "Un\u00b7er\u00b7me\u00df\u00b7li\u00b7che", "vor", "dir", "sich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du Herrlicher! mein Saitenspiel geleitet", "tokens": ["Du", "Herr\u00b7li\u00b7cher", "!", "mein", "Sai\u00b7ten\u00b7spiel", "ge\u00b7lei\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$.", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich auch hinab in Plutons dunkles Haus;", "tokens": ["Dich", "auch", "hin\u00b7ab", "in", "Plu\u00b7tons", "dunk\u00b7les", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So flogen auf Ortygias Gestaden,", "tokens": ["So", "flo\u00b7gen", "auf", "Or\u00b7ty\u00b7gi\u00b7as", "Ge\u00b7sta\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Inde\u00df der Lieder Sturm die Wolken brach,", "tokens": ["In\u00b7de\u00df", "der", "Lie\u00b7der", "Sturm", "die", "Wol\u00b7ken", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem Rebengott die taumelnden M\u00e4naden", "tokens": ["Dem", "Re\u00b7ben\u00b7gott", "die", "tau\u00b7meln\u00b7den", "M\u00e4\u00b7na\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In wilder Lust durch Hain und Kl\u00fcfte nach.", "tokens": ["In", "wil\u00b7der", "Lust", "durch", "Hain", "und", "Kl\u00fcf\u00b7te", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einst war, wie mir, der stille Funken", "tokens": ["Einst", "war", ",", "wie", "mir", ",", "der", "stil\u00b7le", "Fun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu freier heitrer Flamme dir erwacht,", "tokens": ["Zu", "frei\u00b7er", "hei\u00b7trer", "Flam\u00b7me", "dir", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du braustest so, von junger Freude trunken,", "tokens": ["Du", "braus\u00b7test", "so", ",", "von", "jun\u00b7ger", "Freu\u00b7de", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voll Uebermuths durch deiner W\u00e4lder Nacht,", "tokens": ["Voll", "Ue\u00b7ber\u00b7muths", "durch", "dei\u00b7ner", "W\u00e4l\u00b7der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als von der Meisterin, der Noth, geleitet,", "tokens": ["Als", "von", "der", "Meis\u00b7te\u00b7rin", ",", "der", "Noth", ",", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein ungewohnter Arm die Keule schwang,", "tokens": ["Dein", "un\u00b7ge\u00b7wohn\u00b7ter", "Arm", "die", "Keu\u00b7le", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und drohend sich, vom ersten Feind erbeutet,", "tokens": ["Und", "dro\u00b7hend", "sich", ",", "vom", "ers\u00b7ten", "Feind", "er\u00b7beu\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die L\u00f6wenhaut um deine Schulter schlang.", "tokens": ["Die", "L\u00f6\u00b7wen\u00b7haut", "um", "dei\u00b7ne", "Schul\u00b7ter", "schlang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie nun im jugendlichen Kriege", "tokens": ["Wie", "nun", "im", "ju\u00b7gend\u00b7li\u00b7chen", "Krie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heroenkraft mit der Natur sich ma\u00df!", "tokens": ["He\u00b7ro\u00b7en\u00b7kraft", "mit", "der", "Na\u00b7tur", "sich", "ma\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach! wie der Geist, vom wunderbaren Siege", "tokens": ["Ach", "!", "wie", "der", "Geist", ",", "vom", "wun\u00b7der\u00b7ba\u00b7ren", "Sie\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Berauscht, der armen Sterblichkeit verga\u00df;", "tokens": ["Be\u00b7rauscht", ",", "der", "ar\u00b7men", "Sterb\u00b7lich\u00b7keit", "ver\u00b7ga\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die stolzen J\u00fcnglinge! die k\u00fchnen!", "tokens": ["Die", "stol\u00b7zen", "J\u00fcng\u00b7lin\u00b7ge", "!", "die", "k\u00fch\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie legten froh dem Tieger Fesseln an,", "tokens": ["Sie", "leg\u00b7ten", "froh", "dem", "Tie\u00b7ger", "Fes\u00b7seln", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie b\u00e4ndigten, von staunenden Delphinen", "tokens": ["Sie", "b\u00e4n\u00b7dig\u00b7ten", ",", "von", "stau\u00b7nen\u00b7den", "Del\u00b7phi\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Umtanzt, den k\u00f6niglichen Ozean.", "tokens": ["Um\u00b7tanzt", ",", "den", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "O\u00b7ze\u00b7an", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Oft h\u00f6r' ich deine Wehre rauschen,", "tokens": ["Oft", "h\u00f6r'", "ich", "dei\u00b7ne", "Weh\u00b7re", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Genius der K\u00fchnen! und die Lust,", "tokens": ["Du", "Ge\u00b7nius", "der", "K\u00fch\u00b7nen", "!", "und", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$.", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Den Wundern deines Heldenvolks zu lauschen,", "tokens": ["Den", "Wun\u00b7dern", "dei\u00b7nes", "Hel\u00b7den\u00b7volks", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie st\u00e4rkt mir oft die lebensm\u00fcde Brust;", "tokens": ["Sie", "st\u00e4rkt", "mir", "oft", "die", "le\u00b7bens\u00b7m\u00fc\u00b7de", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch weilst du freundlicher um stille Laren,", "tokens": ["Doch", "weilst", "du", "freund\u00b7li\u00b7cher", "um", "stil\u00b7le", "La\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo eine Welt der K\u00fcnstler k\u00fchn belebt,", "tokens": ["Wo", "ei\u00b7ne", "Welt", "der", "K\u00fcnst\u00b7ler", "k\u00fchn", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo um die Majest\u00e4t des Unsichtbaren", "tokens": ["Wo", "um", "die", "Ma\u00b7jes\u00b7t\u00e4t", "des", "Un\u00b7sicht\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein edler Geist der Dichtung Schleier webt.", "tokens": ["Ein", "ed\u00b7ler", "Geist", "der", "Dich\u00b7tung", "Schlei\u00b7er", "webt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Den Geist des Alls und seine F\u00fclle", "tokens": ["Den", "Geist", "des", "Alls", "und", "sei\u00b7ne", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begr\u00fc\u00dfte M\u00e4ons Sohn auf heil'ger Spur,", "tokens": ["Be\u00b7gr\u00fc\u00df\u00b7te", "M\u00e4ons", "Sohn", "auf", "heil'\u00b7ger", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie stand vor ihm, mit abgelegter H\u00fclle,", "tokens": ["Sie", "stand", "vor", "ihm", ",", "mit", "ab\u00b7ge\u00b7leg\u00b7ter", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voll Ernstes da, die ewige Natur;", "tokens": ["Voll", "Erns\u00b7tes", "da", ",", "die", "e\u00b7wi\u00b7ge", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er rief sie k\u00fchn vom dunklen Geisterlande,", "tokens": ["Er", "rief", "sie", "k\u00fchn", "vom", "dunk\u00b7len", "Geis\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und l\u00e4chelnd trat, in aller Freuden Chor,", "tokens": ["Und", "l\u00e4\u00b7chelnd", "trat", ",", "in", "al\u00b7ler", "Freu\u00b7den", "Chor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Entz\u00fcckender im menschlichen Gewande", "tokens": ["Ent\u00b7z\u00fc\u00b7cken\u00b7der", "im", "menschli\u00b7chen", "Ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die namenlose K\u00f6nigin hervor.", "tokens": ["Die", "na\u00b7men\u00b7lo\u00b7se", "K\u00f6\u00b7ni\u00b7gin", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Er sah die d\u00e4mmernden Gebiete,", "tokens": ["Er", "sah", "die", "d\u00e4m\u00b7mern\u00b7den", "Ge\u00b7bie\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohin das Herz in banger Lust begehrt,", "tokens": ["Wo\u00b7hin", "das", "Herz", "in", "ban\u00b7ger", "Lust", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er streuete der Hoffnung s\u00fc\u00dfe Bl\u00fcthe", "tokens": ["Er", "streu\u00b7e\u00b7te", "der", "Hoff\u00b7nung", "s\u00fc\u00b7\u00dfe", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ins Labyrinth, wo Keiner wiederkehrt,", "tokens": ["Ins", "La\u00b7by\u00b7rinth", ",", "wo", "Kei\u00b7ner", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dort gl\u00e4nzte nun in mildem Rosenlichte", "tokens": ["Dort", "gl\u00e4nz\u00b7te", "nun", "in", "mil\u00b7dem", "Ro\u00b7sen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Lieb' und Ruh' ein l\u00e4chelnd Heiligthum,", "tokens": ["Der", "Lieb'", "und", "Ruh'", "ein", "l\u00e4\u00b7chelnd", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er pflanzte dort der Hesperiden Fr\u00fcchte,", "tokens": ["Er", "pflanz\u00b7te", "dort", "der", "Hes\u00b7pe\u00b7ri\u00b7den", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dort stillt die Sorgen nun Elysium.", "tokens": ["Dort", "stillt", "die", "Sor\u00b7gen", "nun", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch schrecklich war, du Gott der K\u00fchnen!", "tokens": ["Doch", "schreck\u00b7lich", "war", ",", "du", "Gott", "der", "K\u00fch\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein heilig Wort, wenn unter Nacht und Schlaf", "tokens": ["Dein", "hei\u00b7lig", "Wort