{"textgrid.poem.45955": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ah bassa manelki teremtete]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.71", "no:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ah bassa manelki teremtete,", "tokens": ["Ah", "bas\u00b7sa", "ma\u00b7nel\u00b7ki", "te\u00b7rem\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So bl\u00e4st der Trompeter, so w\u00fcnschet ein jeder,", "tokens": ["So", "bl\u00e4st", "der", "Trom\u00b7pe\u00b7ter", ",", "so", "w\u00fcn\u00b7schet", "ein", "je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "PIS", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf da\u00df es nun endlich recht drauf und dran geh'!", "tokens": ["Auf", "da\u00df", "es", "nun", "end\u00b7lich", "recht", "drauf", "und", "dran", "geh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "ADV", "ADJD", "PTKVZ", "KON", "PAV", "VVFIN", "$."], "meter": "--+-+-++-+-", "measure": "anapaest.init"}, "line.4": {"text": "Man streicht sich den Schnurrbart, und giebt ihm den Zwick", "tokens": ["Man", "streicht", "sich", "den", "Schnurr\u00b7bart", ",", "und", "giebt", "ihm", "den", "Zwick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und wiegt in dem S\u00e4bel des Feindes Geschick.", "tokens": ["Und", "wiegt", "in", "dem", "S\u00e4\u00b7bel", "des", "Fein\u00b7des", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Chor:", "tokens": ["Chor", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Schlechte Reiter Sind nichts weiter", "tokens": ["Schlech\u00b7te", "Rei\u00b7ter", "Sind", "nichts", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als sechsbeinige B\u00e4renh\u00e4uter.", "tokens": ["Als", "sechs\u00b7bei\u00b7ni\u00b7ge", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ah bassa manelki teremtete!", "tokens": ["Ah", "bas\u00b7sa", "ma\u00b7nel\u00b7ki", "te\u00b7rem\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir fahren auf Rossen zusammengegossen", "tokens": ["Wir", "fah\u00b7ren", "auf", "Ros\u00b7sen", "zu\u00b7sam\u00b7men\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie die Wetterwolken in himmlischer H\u00f6h',", "tokens": ["Wie", "die", "Wet\u00b7ter\u00b7wol\u00b7ken", "in", "himm\u00b7li\u00b7scher", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es schmettern wie Blitze die S\u00e4bel hervor,", "tokens": ["Es", "schmet\u00b7tern", "wie", "Blit\u00b7ze", "die", "S\u00e4\u00b7bel", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wer fest nicht im Sitze, der kriegt eins ans Ohr.", "tokens": ["Wer", "fest", "nicht", "im", "Sit\u00b7ze", ",", "der", "kriegt", "eins", "ans", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "APPRART", "NN", "$,", "PRELS", "VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Chor:", "tokens": ["Chor", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Schlechte Reiter Sind nichts weiter", "tokens": ["Schlech\u00b7te", "Rei\u00b7ter", "Sind", "nichts", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als sechsbeinige B\u00e4renh\u00e4uter.", "tokens": ["Als", "sechs\u00b7bei\u00b7ni\u00b7ge", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ah bassa manelki teremtete!", "tokens": ["Ah", "bas\u00b7sa", "ma\u00b7nel\u00b7ki", "te\u00b7rem\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir ungrischen Husaren, wir haben erfahren,", "tokens": ["Wir", "un\u00b7gri\u00b7schen", "Hu\u00b7sa\u00b7ren", ",", "wir", "ha\u00b7ben", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df der Feind nicht gern in die Augen uns seh',", "tokens": ["Da\u00df", "der", "Feind", "nicht", "gern", "in", "die", "Au\u00b7gen", "uns", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein schlechtes Gewissen vertr\u00e4gt kein Gericht,", "tokens": ["Sein", "schlech\u00b7tes", "Ge\u00b7wis\u00b7sen", "ver\u00b7tr\u00e4gt", "kein", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ins Gras oft gebissen hat vor uns der Wicht.", "tokens": ["Ins", "Gras", "oft", "ge\u00b7bis\u00b7sen", "hat", "vor", "uns", "der", "Wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Chor:", "tokens": ["Chor", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Schlechte Reiter Sind nichts weiter", "tokens": ["Schlech\u00b7te", "Rei\u00b7ter", "Sind", "nichts", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als sechsbeinige B\u00e4renh\u00e4uter.", "tokens": ["Als", "sechs\u00b7bei\u00b7ni\u00b7ge", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ah bassa manelki teremtete!", "tokens": ["Ah", "bas\u00b7sa", "ma\u00b7nel\u00b7ki", "te\u00b7rem\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein S\u00e4bel, der treue, den Kampf nun erneue,", "tokens": ["Mein", "S\u00e4\u00b7bel", ",", "der", "treu\u00b7e", ",", "den", "Kampf", "nun", "er\u00b7neu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da\u00df noster Franciscus Justitiam seh'.", "tokens": ["Da\u00df", "nos\u00b7ter", "Fran\u00b7cis\u00b7cus", "Jus\u00b7ti\u00b7ti\u00b7am", "seh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NE", "NE", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wir Ungern wir schlagen mit dem S\u00e4bel auf'n Tisch,", "tokens": ["Wir", "Un\u00b7gern", "wir", "schla\u00b7gen", "mit", "dem", "S\u00e4\u00b7bel", "auf'n", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Protestor wir sagen, dann geht es von frisch.", "tokens": ["Pro\u00b7tes\u00b7tor", "wir", "sa\u00b7gen", ",", "dann", "geht", "es", "von", "frisch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$,", "ADV", "VVFIN", "PPER", "APPR", "ADJD", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Chor:", "tokens": ["Chor", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Schlechte Reiter Sind nichts weiter", "tokens": ["Schlech\u00b7te", "Rei\u00b7ter", "Sind", "nichts", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als sechsbeinige B\u00e4renh\u00e4uter.", "tokens": ["Als", "sechs\u00b7bei\u00b7ni\u00b7ge", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ah bassa manelki teremtete!", "tokens": ["Ah", "bas\u00b7sa", "ma\u00b7nel\u00b7ki", "te\u00b7rem\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Heraus ihr Neunt\u00f6ter, Pariser Dekreter,", "tokens": ["He\u00b7raus", "ihr", "Neun\u00b7t\u00f6\u00b7ter", ",", "Pa\u00b7ri\u00b7ser", "De\u00b7kre\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Auf da\u00df man euch a bi\u00dfl die Kundschaft nachseh'", "tokens": ["Auf", "da\u00df", "man", "euch", "a", "bi\u00dfl", "die", "Kund\u00b7schaft", "nach\u00b7seh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PIS", "PPER", "NE", "NE", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und wer nicht kapabel mit F\u00fc\u00df' und mit H\u00e4nd',", "tokens": ["Und", "wer", "nicht", "ka\u00b7pa\u00b7bel", "mit", "F\u00fc\u00df'", "und", "mit", "H\u00e4nd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dem schreibet mein Sabel mit Blut aufs Patent:", "tokens": ["Dem", "schrei\u00b7bet", "mein", "Sa\u00b7bel", "mit", "Blut", "aufs", "Pa\u00b7tent", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Chor:", "tokens": ["Chor", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Schlechte Reiter Sind nichts weiter", "tokens": ["Schlech\u00b7te", "Rei\u00b7ter", "Sind", "nichts", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als sechsbeinige B\u00e4renh\u00e4uter.", "tokens": ["Als", "sechs\u00b7bei\u00b7ni\u00b7ge", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ah bassa manelki teremtete!", "tokens": ["Ah", "bas\u00b7sa", "ma\u00b7nel\u00b7ki", "te\u00b7rem\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie riecht ihr neubacken, die russ'schen Kosaken,", "tokens": ["Wie", "riecht", "ihr", "neu\u00b7ba\u00b7cken", ",", "die", "russ'\u00b7schen", "Ko\u00b7sa\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVINF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die suchten euch wahrlich recht gr\u00fcndlich die Fl\u00f6h',", "tokens": ["Die", "such\u00b7ten", "euch", "wahr\u00b7lich", "recht", "gr\u00fcnd\u00b7lich", "die", "Fl\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gespickt mit der Nadel, gespie\u00dft und rotiert", "tokens": ["Ge\u00b7spickt", "mit", "der", "Na\u00b7del", ",", "ge\u00b7spie\u00dft", "und", "ro\u00b7tiert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "VVPP", "KON", "NE"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Heraus mit dem Bratel, nun wird es transchiert.", "tokens": ["He\u00b7raus", "mit", "dem", "Bra\u00b7tel", ",", "nun", "wird", "es", "tran\u00b7schiert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Chor:", "tokens": ["Chor", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Schlechte Reiter Sind nichts weiter", "tokens": ["Schlech\u00b7te", "Rei\u00b7ter", "Sind", "nichts", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als sechsbeinige B\u00e4renh\u00e4uter.", "tokens": ["Als", "sechs\u00b7bei\u00b7ni\u00b7ge", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ah bassa manelki teremtete!", "tokens": ["Ah", "bas\u00b7sa", "ma\u00b7nel\u00b7ki", "te\u00b7rem\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Pariser Husaren, die \u00f6fters schon waren,", "tokens": ["Pa\u00b7ri\u00b7ser", "Hu\u00b7sa\u00b7ren", ",", "die", "\u00f6f\u00b7ters", "schon", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wo seid ihr? Da grunzet ein Schwein in die H\u00f6h':", "tokens": ["Wo", "seid", "ihr", "?", "Da", "grun\u00b7zet", "ein", "Schwein", "in", "die", "H\u00f6h'", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Vor Magdeburg hieben die Preu\u00dfen sie klein,", "tokens": ["Vor", "Mag\u00b7de\u00b7burg", "hie\u00b7ben", "die", "Preu\u00b7\u00dfen", "sie", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Was \u00fcbrig geblieben, das fra\u00df ich allein.", "tokens": ["Was", "\u00fcb\u00b7r