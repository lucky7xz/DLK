{"dta.poem.19614": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "C\u00e4sar Borja's Ohnmacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wer bin ich? Einer welcher unterging,", "tokens": ["Wer", "bin", "ich", "?", "Ei\u00b7ner", "wel\u00b7cher", "un\u00b7ter\u00b7ging", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PIS", "PWAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Kranz im Haar, den Becher in der Faust,", "tokens": ["Den", "Kranz", "im", "Haar", ",", "den", "Be\u00b7cher", "in", "der", "Faust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit einem herculanischen Gelag", "tokens": ["Mit", "ei\u00b7nem", "her\u00b7cu\u00b7la\u00b7ni\u00b7schen", "Ge\u00b7lag"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von einem ungeheuren Sturz bedeckt?", "tokens": ["Von", "ei\u00b7nem", "un\u00b7ge\u00b7heu\u00b7ren", "Sturz", "be\u00b7deckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wei\u00df den Becher nur und meinen Sturz ...", "tokens": ["Ich", "wei\u00df", "den", "Be\u00b7cher", "nur", "und", "mei\u00b7nen", "Sturz", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im Belvedere ... Gestern ... Am Bankett .. .", "tokens": ["Im", "Bel\u00b7ve\u00b7de\u00b7re", "...", "Ge\u00b7stern", "...", "Am", "Ban\u00b7kett", "..", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$(", "NN", "$(", "APPRART", "NN", "$.", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Den Becher, ihn kredenzte schl\u00fcrfend mir", "tokens": ["Den", "Be\u00b7cher", ",", "ihn", "kre\u00b7denz\u00b7te", "schl\u00fcr\u00b7fend", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Papst, der ewig heiter l\u00e4chelnde,", "tokens": ["Der", "Papst", ",", "der", "e\u00b7wig", "hei\u00b7ter", "l\u00e4\u00b7cheln\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Denn C\u00e4sar Borja bin ich, Sohn des Papsts!", "tokens": ["Denn", "C\u00e4\u00b7sar", "Bor\u00b7ja", "bin", "ich", ",", "Sohn", "des", "Papsts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Ampel \u00fcber meinem Lager k\u00e4mpft", "tokens": ["Die", "Am\u00b7pel", "\u00fc\u00b7ber", "mei\u00b7nem", "La\u00b7ger", "k\u00e4mpft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit eines neuen Tages fahlem Schein ...", "tokens": ["Mit", "ei\u00b7nes", "neu\u00b7en", "Ta\u00b7ges", "fah\u00b7lem", "Schein", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob's gestern oder ehegestern war,", "tokens": ["Ob's", "ge\u00b7stern", "o\u00b7der", "e\u00b7he\u00b7ge\u00b7stern", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "KON", "ADV", "VAFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ich wei\u00df es nicht, doch Eines wei\u00df ich wohl:", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "doch", "Ei\u00b7nes", "wei\u00df", "ich", "wohl", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In jenem Becher gohr der Borja Gift.", "tokens": ["In", "je\u00b7nem", "Be\u00b7cher", "gohr", "der", "Bor\u00b7ja", "Gift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er galt dem Gast, dem Bischof. Selbst gew\u00fcrzt", "tokens": ["Er", "galt", "dem", "Gast", ",", "dem", "Bi\u00b7schof", ".", "Selbst", "ge\u00b7w\u00fcrzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$.", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hat sich der Vater ew'gen Schlummers Trunk!", "tokens": ["Hat", "sich", "der", "Va\u00b7ter", "ew'\u00b7gen", "Schlum\u00b7mers", "Trunk", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Becher ward verwechselt. Warum nicht?", "tokens": ["Ein", "Be\u00b7cher", "ward", "ver\u00b7wech\u00b7selt", ".", "Wa\u00b7rum", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PWAV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Verrath des Schenken? Zufall? ... Es geschah.", "tokens": ["Ver\u00b7rath", "des", "Schen\u00b7ken", "?", "Zu\u00b7fall", "?", "...", "Es", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "$.", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich lebe. Meine Drachenkraft bezwang", "tokens": ["Ich", "le\u00b7be", ".", "Mei\u00b7ne", "Dra\u00b7chen\u00b7kraft", "be\u00b7zwang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das Drachengift. Die Stunde ruft. Zur That!", "tokens": ["Das", "Dra\u00b7chen\u00b7gift", ".", "Die", "Stun\u00b7de", "ruft", ".", "Zur", "That", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVFIN", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Leer steht ein Thron und eine Krone rollt.", "tokens": ["Leer", "steht", "ein", "Thron", "und", "ei\u00b7ne", "Kro\u00b7ne", "rollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verbraucht ist das Apostelm\u00e4rchen. Weg", "tokens": ["Ver\u00b7braucht", "ist", "das", "A\u00b7pos\u00b7tel\u00b7m\u00e4r\u00b7chen", ".", "Weg"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Damit! Der Vater war der letzte Papst!", "tokens": ["Da\u00b7mit", "!", "Der", "Va\u00b7ter", "war", "der", "letz\u00b7te", "Papst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein K\u00f6nig folgt ihm nach und der bin ich.", "tokens": ["Ein", "K\u00f6\u00b7nig", "folgt", "ihm", "nach", "und", "der", "bin", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "KON", "ART", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Entscheidungsstunde, nicht erschreckst du mich,", "tokens": ["Ent\u00b7schei\u00b7dungs\u00b7stun\u00b7de", ",", "nicht", "er\u00b7schreckst", "du", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich habe lange dich voraus bedacht:", "tokens": ["Ich", "ha\u00b7be", "lan\u00b7ge", "dich", "vo\u00b7raus", "be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Entlarve mir dein k\u00fchnes Angesicht!", "tokens": ["Ent\u00b7lar\u00b7ve", "mir", "dein", "k\u00fch\u00b7nes", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du hei\u00dfest Heute! K\u00e4mmrer, gieb das Schwert!", "tokens": ["Du", "hei\u00b7\u00dfest", "Heu\u00b7te", "!", "K\u00e4mm\u00b7rer", ",", "gieb", "das", "Schwert", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Reif stehn die Ernten und die Sichel blitzt.", "tokens": ["Reif", "stehn", "die", "Ern\u00b7ten", "und", "die", "Si\u00b7chel", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Marsch, meine Banden! Richtet das Gesch\u00fctz", "tokens": ["Marsch", ",", "mei\u00b7ne", "Ban\u00b7den", "!", "Rich\u00b7tet", "das", "Ge\u00b7sch\u00fctz"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Auf des Conclave Kammern! Suchst du mich,", "tokens": ["Auf", "des", "Con\u00b7cla\u00b7ve", "Kam\u00b7mern", "!", "Suchst", "du", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Hauptmann? Im Borgo, sagst du, wird gek\u00e4mpft?", "tokens": ["Haupt\u00b7mann", "?", "Im", "Bor\u00b7go", ",", "sagst", "du", ",", "wird", "ge\u00b7k\u00e4mpft", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "APPRART", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Ich komme! Ich vertausendfache mich!", "tokens": ["Ich", "kom\u00b7me", "!", "Ich", "ver\u00b7tau\u00b7send\u00b7fa\u00b7che", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ich steige mordend auf das Capitol", "tokens": ["Ich", "stei\u00b7ge", "mor\u00b7dend", "auf", "das", "Ca\u00b7pi\u00b7tol"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und mit Italiens Krone kr\u00f6n' ich mir", "tokens": ["Und", "mit", "I\u00b7ta\u00b7li\u00b7ens", "Kro\u00b7ne", "kr\u00f6n'", "ich", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN", "VVFIN", "PPER", "PPER"], "meter": "+-++--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Dies Haupt das seine Frevel \u00fcberragt!", "tokens": ["Dies", "Haupt", "das", "sei\u00b7ne", "Fre\u00b7vel", "\u00fc\u00b7berr\u00b7agt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich tr\u00e4ume nur und komme nicht vom Platz.", "tokens": ["Ich", "tr\u00e4u\u00b7me", "nur", "und", "kom\u00b7me", "nicht", "vom", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sturmlaufend bleib' ich eingewurzelt stehn.", "tokens": ["Sturm\u00b7lau\u00b7fend", "bleib'", "ich", "ein\u00b7ge\u00b7wur\u00b7zelt", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VVPP", "VVINF", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Gel\u00e4hmte Sehnen! Meuchlerisches Gift!", "tokens": ["Ge\u00b7l\u00e4hm\u00b7te", "Seh\u00b7nen", "!", "Meuch\u00b7le\u00b7ri\u00b7sches", "Gift", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf einem Krankenlager kr\u00fcmm' ich mich.", "tokens": ["Auf", "ei\u00b7nem", "Kran\u00b7ken\u00b7la\u00b7ger", "kr\u00fcmm'", "ich", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kein Diener hier! Kein Arzt an meinem Pf\u00fchl!", "tokens": ["Kein", "Die\u00b7ner", "hier", "!", "Kein", "Arzt", "an", "mei\u00b7nem", "Pf\u00fchl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$.", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Miethlinge! Meine Stunde schwebt vorbei,", "tokens": ["Mieth\u00b7lin\u00b7ge", "!", "Mei\u00b7ne", "Stun\u00b7de", "schwebt", "vor\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit flieh'ndem Fu\u00df ber\u00fchrt sie spottend mir", "tokens": ["Mit", "flieh'n\u00b7dem", "Fu\u00df", "be\u00b7r\u00fchrt", "sie", "spot\u00b7tend", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Faust, die ein erdichtet Schwert umkrampft.", "tokens": ["Die", "Faust", ",", "die", "ein", "er\u00b7dich\u00b7tet", "Schwert", "um\u00b7krampft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Verweile, Schicksalsstunde! ... Doch sie schwebt.", "tokens": ["Ver\u00b7wei\u00b7le", ",", "Schick\u00b7sals\u00b7stun\u00b7de", "!", "...", "Doch", "sie", "schwebt", "."], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "$(", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich f\u00fchle meiner Feinde heimlich Werk:", "tokens": ["Ich", "f\u00fch\u00b7le", "mei\u00b7ner", "Fein\u00b7de", "heim\u00b7lich", "Werk", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sie schaufeln, sie miniren, w\u00e4hrend ich", "tokens": ["Sie", "schau\u00b7feln", ",", "sie", "mi\u00b7ni\u00b7ren", ",", "w\u00e4h\u00b7rend", "ich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Geschleudert aus der Schranke liege ... Dort!", "tokens": ["Ge\u00b7schleu\u00b7dert", "aus", "der", "Schran\u00b7ke", "lie\u00b7ge", "...", "Dort", "!"], "token_info": ["word", "word", "