{"textgrid.poem.28531": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cupido zielte nechst und meint, es w\u00fcrde gl\u00fccken,", "tokens": ["Cu\u00b7pi\u00b7do", "ziel\u00b7te", "nechst", "und", "meint", ",", "es", "w\u00fcr\u00b7de", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "KON", "VVFIN", "$,", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff Polla Hertze zu; sie wandte sich; im R\u00fccken", "tokens": ["Auff", "Pol\u00b7la", "Hert\u00b7ze", "zu", ";", "sie", "wand\u00b7te", "sich", ";", "im", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PRF", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bestund der heisse Pfeil; das macht, da\u00df sichs begibt,", "tokens": ["Be\u00b7stund", "der", "heis\u00b7se", "Pfeil", ";", "das", "macht", ",", "da\u00df", "sichs", "be\u00b7gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "PDS", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df nimmer nichts wird drau\u00df, sie liebe, wen sie liebt.", "tokens": ["Da\u00df", "nim\u00b7mer", "nichts", "wird", "drau\u00df", ",", "sie", "lie\u00b7be", ",", "wen", "sie", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VAFIN", "PAV", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}