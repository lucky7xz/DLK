{"dta.poem.2473": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Comes virtutis invidia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nur vom Fett' und Blut uns Leib und Adern", "tokens": ["Wenn", "nur", "vom", "Fett'", "und", "Blut", "uns", "Leib", "und", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN", "KON", "NN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schwellen", "tokens": ["schwel\u00b7len"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So hat man unn\u00fctz Vieh zu treuen Mitgesellen", "tokens": ["So", "hat", "man", "un\u00b7n\u00fctz", "Vieh", "zu", "treu\u00b7en", "Mit\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00e4llt aber nur der Mensch in eine Todensucht/", "tokens": ["F\u00e4llt", "a\u00b7ber", "nur", "der", "Mensch", "in", "ei\u00b7ne", "To\u00b7den\u00b7sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann weichen sie von ihm/ und geben bald die Flucht;", "tokens": ["Dann", "wei\u00b7chen", "sie", "von", "ihm", "/", "und", "ge\u00b7ben", "bald", "die", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$(", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So sieht man eben auch/ wenn Gl\u00fckk und Wollfahrt bl\u00fc-", "tokens": ["So", "sieht", "man", "e\u00b7ben", "auch", "/", "wenn", "Gl\u00fckk", "und", "Woll\u00b7fahrt", "bl\u00fc"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "$(", "KOUS", "NN", "KON", "NE", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "hen/", "tokens": ["hen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Da\u00df Freunde hier und dar zu dienen sich bem\u00fchen", "tokens": ["Da\u00df", "Freun\u00b7de", "hier", "und", "dar", "zu", "die\u00b7nen", "sich", "be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "KON", "PTKVZ", "PTKZU", "VVINF", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn aber das Gel\u00fckk\u2019 uns seinen R\u00fckken weist/", "tokens": ["Wenn", "a\u00b7ber", "das", "Ge\u00b7l\u00fc\u00b7kk'", "uns", "sei\u00b7nen", "R\u00fck\u00b7ken", "weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "So h\u00f6hrt die Freundschafft auff/ die Diener sind ver-", "tokens": ["So", "h\u00f6hrt", "die", "Freund\u00b7schafft", "auff", "/", "die", "Die\u00b7ner", "sind", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "$(", "ART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "rei\u00df.", "tokens": ["rei\u00df", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}