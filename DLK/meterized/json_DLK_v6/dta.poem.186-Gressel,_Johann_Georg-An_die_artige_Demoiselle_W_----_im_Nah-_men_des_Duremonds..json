{"dta.poem.186": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die artige  Demoiselle  W ---- im Nah-  \n men des  Duremonds.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Blatt voll Schaam und Furcht wirfft sich zu euren", "tokens": ["Ein", "Blatt", "voll", "Schaam", "und", "Furcht", "wirfft", "sich", "zu", "eu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "VVFIN", "PRF", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcssen/", "tokens": ["F\u00fcs\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Es scheut sich/ da\u00df es soll die H\u00e4nde r\u00fchren an/", "tokens": ["Es", "scheut", "sich", "/", "da\u00df", "es", "soll", "die", "H\u00e4n\u00b7de", "r\u00fch\u00b7ren", "an", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$(", "KOUS", "PPER", "VMFIN", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die als der Unschuld Bild von keiner Falschheit wissen/", "tokens": ["Die", "als", "der", "Un\u00b7schuld", "Bild", "von", "kei\u00b7ner", "Falschheit", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "NN", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ach! da\u00df ich mich des Ruhms nicht w\u00fcrdig sch\u00e4tzen kan.", "tokens": ["Ach", "!", "da\u00df", "ich", "mich", "des", "Ruhms", "nicht", "w\u00fcr\u00b7dig", "sch\u00e4t\u00b7zen", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PRF", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Z\u00fcrnt nicht ", "tokens": ["Z\u00fcrnt", "nicht"], "token_info": ["word", "word"], "pos": ["NE", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "So euch mein falscher Mund mit Lieblichkeit versprach/", "tokens": ["So", "euch", "mein", "fal\u00b7scher", "Mund", "mit", "Lieb\u00b7lich\u00b7keit", "ver\u00b7sprach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jhr seyd schon/ meine Lust/ genug an mich gerochen/", "tokens": ["Ihr", "seyd", "schon", "/", "mei\u00b7ne", "Lust", "/", "ge\u00b7nug", "an", "mich", "ge\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "PPOSAT", "NN", "$(", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es brauchts nicht/ da\u00df ihr mehr besinnet euch auf Rach\u2019.", "tokens": ["Es", "brauchts", "nicht", "/", "da\u00df", "ihr", "mehr", "be\u00b7sin\u00b7net", "euch", "auf", "Rach'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mein g\u00fctiges Geschick/ mein bl\u00fchendes Gel\u00fccke", "tokens": ["Mein", "g\u00fc\u00b7ti\u00b7ges", "Ge\u00b7schick", "/", "mein", "bl\u00fc\u00b7hen\u00b7des", "Ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verehrte eure Gunst mir als ein Eigenthum/", "tokens": ["Ver\u00b7ehr\u00b7te", "eu\u00b7re", "Gunst", "mir", "als", "ein", "Ei\u00b7gen\u00b7thum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So bald ich euch ersah\u2019 und eure holde Blicke/", "tokens": ["So", "bald", "ich", "euch", "er\u00b7sah'", "und", "eu\u00b7re", "hol\u00b7de", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da war mein freyer Sinn um seine Freyheit um.", "tokens": ["Da", "war", "mein", "frey\u00b7er", "Sinn", "um", "sei\u00b7ne", "Frey\u00b7heit", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich starrete vor Lust/ ich fing euch an zu lieben/", "tokens": ["Ich", "star\u00b7re\u00b7te", "vor", "Lust", "/", "ich", "fing", "euch", "an", "zu", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$(", "PPER", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und ihr erregetet ein Feur in meiner Brust/", "tokens": ["Und", "ihr", "er\u00b7re\u00b7ge\u00b7tet", "ein", "Feur", "in", "mei\u00b7ner", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Allein die Linderung war nicht zu weit geblieben/", "tokens": ["Al\u00b7lein", "die", "Lin\u00b7de\u00b7rung", "war", "nicht", "zu", "weit", "ge\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PTKNEG", "PTKA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein heisser ", "tokens": ["Ein", "heis\u00b7ser"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Ich kunte euch/ mein Schatz/ da sehen und auch sprechen/", "tokens": ["Ich", "kun\u00b7te", "euch", "/", "mein", "Schatz", "/", "da", "se\u00b7hen", "und", "auch", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "ADV", "VVINF", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und unser Liebe-Grund ward bey dem ", "tokens": ["Und", "un\u00b7ser", "Lie\u00b7be\u00b7Grund", "ward", "bey", "dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Eur vordem hartes Hertz/ das fing da an zu brechen/", "tokens": ["Eur", "vor\u00b7dem", "har\u00b7tes", "Hertz", "/", "das", "fing", "da", "an", "zu", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$(", "PDS", "VVFIN", "ADV", "APZR", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jhr wurdet so wie ich durch gleichen Trieb bewegt.", "tokens": ["Ihr", "wur\u00b7det", "so", "wie", "ich", "durch", "glei\u00b7chen", "Trieb", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Noch ferner bauten wir das Wohn-Haus unser Liebe/", "tokens": ["Noch", "fer\u00b7ner", "bau\u00b7ten", "wir", "das", "Wohn\u00b7Haus", "un\u00b7ser", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als man durch Spei\u00df und Tranck den matten Leib erhielt/", "tokens": ["Als", "man", "durch", "Spei\u00df", "und", "Tranck", "den", "mat\u00b7ten", "Leib", "er\u00b7hielt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bald war eur Antlitz klahr/ bald wurd es wieder tr\u00fcbe/", "tokens": ["Bald", "war", "eur", "Ant\u00b7litz", "klahr", "/", "bald", "wurd", "es", "wie\u00b7der", "tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Von euren Augen wurd ein steter Blitz gespielt.", "tokens": ["Von", "eu\u00b7ren", "Au\u00b7gen", "wurd", "ein", "ste\u00b7ter", "Blitz", "ge\u00b7spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da fing nun jedes Hertz an gegen euch zu brennen/", "tokens": ["Da", "fing", "nun", "je\u00b7des", "Hertz", "an", "ge\u00b7gen", "euch", "zu", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "APPR", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die gantze ", "tokens": ["Die", "gant\u00b7ze"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "Einjeder m\u00fchte sich euch zu gefallen k\u00f6nnen/", "tokens": ["Ein\u00b7je\u00b7der", "m\u00fch\u00b7te", "sich", "euch", "zu", "ge\u00b7fal\u00b7len", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PPER", "PTKZU", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und nach dem Essen ward so manche Lust erdacht.", "tokens": ["Und", "nach", "dem", "Es\u00b7sen", "ward", "so", "man\u00b7che", "Lust", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Man sang/ man spielete vermischt mit einem Tantzen/", "tokens": ["Man", "sang", "/", "man", "spie\u00b7le\u00b7te", "ver\u00b7mischt", "mit", "ei\u00b7nem", "Tant\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "PIS", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da wurde ich von euch zu euren Mann erkl\u00e4hrt/", "tokens": ["Da", "wur\u00b7de", "ich", "von", "euch", "zu", "eu\u00b7ren", "Mann", "er\u00b7kl\u00e4hrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und drauf begunten wir den Lebens-Baum zu pflantzen/", "tokens": ["Und", "drauf", "be\u00b7gun\u00b7ten", "wir", "den", "Le\u00b7bens\u00b7Baum", "zu", "pflant\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Eur Hertze wurde mir und meines euch verehrt.", "tokens": ["Eur", "Hert\u00b7ze", "wur\u00b7de", "mir", "und", "mei\u00b7nes", "euch", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "PPER", "KON", "PPOSAT", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Darnach so machten wir vers\u00fc\u00dfte Kalte-Schaalen/", "tokens": ["Dar\u00b7nach", "so", "mach\u00b7ten", "wir", "ver\u00b7s\u00fc\u00df\u00b7te", "Kal\u00b7te\u00b7Schaa\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Erdbeeren/ Zucker/ Wein genossen wir darinn/", "tokens": ["Erd\u00b7bee\u00b7ren", "/", "Zu\u00b7cker", "/", "Wein", "ge\u00b7nos\u00b7sen", "wir", "da\u00b7rinn", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "VVFIN", "PPER", "PAV", "$("], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.36": {"text": "Jhr waret gantz vergn\u00fcgt mit solchen Liebes-Mahlen/", "tokens": ["Ihr", "wa\u00b7ret", "gantz", "ver\u00b7gn\u00fcgt", "mit", "sol\u00b7chen", "Lie\u00b7bes\u00b7Mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es zeigte Lustbarkeiteur angenehmer Sinn/", "tokens": ["Es", "zeig\u00b7te", "Lust\u00b7bar\u00b7kei\u00b7teur", "an\u00b7ge\u00b7neh\u00b7mer", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wie diese war verzehrt/ da fing man an zu trincken", "tokens": ["Wie", "die\u00b7se", "war", "ver\u00b7zehrt", "/", "da", "fing", "man", "an", "zu", "trin\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "VAFIN", "VVPP", "$(", "ADV", "VVFIN", "PIS", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Den heissen ", "tokens": ["Den", "heis\u00b7sen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "Und als die Sonne schon am Himmel fing zu blincken/", "tokens": ["Und", "als", "die", "Son\u00b7ne", "schon", "am", "Him\u00b7mel", "fing", "zu", "blin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Da ward der matte Leib zur Ruhe erst gebracht.", "tokens": ["Da", "ward", "der", "mat\u00b7te", "Leib", "zur", "Ru\u00b7he", "erst", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Als nun der s\u00fcsse Schlaff die Augen-Burg verlassen/", "tokens": ["Als", "nun", "der", "s\u00fcs\u00b7se", "Schlaff", "die", "Au\u00b7gen\u00b7Burg", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da ging die Lustbarkeit von neuen wieder an/", "tokens": ["Da", "ging", "die", "Lust\u00b7bar\u00b7keit", "von", "neu\u00b7en", "wie\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Man schenckte siedentheis den ", "tokens": ["Man", "schenck\u00b7te", "sie\u00b7den\u00b7theis", "den"], "token