{"textgrid.poem.36946": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn die Hoffnung nicht w\u00e4r',", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Hoffnung nicht w\u00e4r',", "tokens": ["Wenn", "die", "Hoff\u00b7nung", "nicht", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wir lebten nicht mehr!", "tokens": ["Wir", "leb\u00b7ten", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie allein kann uns tr\u00f6sten,", "tokens": ["Sie", "al\u00b7lein", "kann", "uns", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kann lindern die Pein.", "tokens": ["Kann", "lin\u00b7dern", "die", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie gieng es denn hin, wie gieng es denn her,", "tokens": ["Wie", "gieng", "es", "denn", "hin", ",", "wie", "gieng", "es", "denn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn die Hoffnung nicht w\u00e4r'!", "tokens": ["Wenn", "die", "Hoff\u00b7nung", "nicht", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VAFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Die Armuth, sie weint,", "tokens": ["Die", "Ar\u00b7muth", ",", "sie", "weint", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ihr Gold wird verpra\u00dft;", "tokens": ["Ihr", "Gold", "wird", "ver\u00b7pra\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die g\u00f6ttliche Wahrheit,", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie ist verha\u00dft!", "tokens": ["Sie", "ist", "ver\u00b7ha\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wie gieng es denn hin, wie gieng es denn her,", "tokens": ["Wie", "gieng", "es", "denn", "hin", ",", "wie", "gieng", "es", "denn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn die Hoffnung nicht w\u00e4r'!", "tokens": ["Wenn", "die", "Hoff\u00b7nung", "nicht", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VAFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Vor Gott sind wir gleich,", "tokens": ["Vor", "Gott", "sind", "wir", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Hier aber liegt, ach!", "tokens": ["Hier", "a\u00b7ber", "liegt", ",", "ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "ITJ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In der Wiege die Gr\u00f6\u00dfe,", "tokens": ["In", "der", "Wie\u00b7ge", "die", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "In der Wiege die Schmach!", "tokens": ["In", "der", "Wie\u00b7ge", "die", "Schmach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie gieng es denn hin, wie gieng es denn her,", "tokens": ["Wie", "gieng", "es", "denn", "hin", ",", "wie", "gieng", "es", "denn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn die Hoffnung nicht w\u00e4r'!", "tokens": ["Wenn", "die", "Hoff\u00b7nung", "nicht", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VAFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Nur der Adel regiert,", "tokens": ["Nur", "der", "A\u00b7del", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der B\u00fcrger ist Sclav,", "tokens": ["Der", "B\u00fcr\u00b7ger", "ist", "Sclav", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und ist doch voll Weisheit,", "tokens": ["Und", "ist", "doch", "voll", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ist kr\u00e4ftig und brav!", "tokens": ["Ist", "kr\u00e4f\u00b7tig", "und", "brav", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie gieng es denn hin, wie gieng es denn her,", "tokens": ["Wie", "gieng", "es", "denn", "hin", ",", "wie", "gieng", "es", "denn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn die Hoffnung nicht w\u00e4r'!", "tokens": ["Wenn", "die", "Hoff\u00b7nung", "nicht", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VAFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Unser Recht vom Himmel,", "tokens": ["Un\u00b7ser", "Recht", "vom", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sie schlugen's entzwei;", "tokens": ["Sie", "schlu\u00b7gen's", "ent\u00b7zwei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie traten's mit F\u00fc\u00dfen,", "tokens": ["Sie", "tra\u00b7ten's", "mit", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und wir dachten dabei:", "tokens": ["Und", "wir", "dach\u00b7ten", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie gieng es denn hin, wie gieng es denn her,", "tokens": ["Wie", "gieng", "es", "denn", "hin", ",", "wie", "gieng", "es", "denn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn die Hoffnung nicht w\u00e4r'!", "tokens": ["Wenn", "die", "Hoff\u00b7nung", "nicht", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VAFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "So ha'n wir gehoffet,", "tokens": ["So", "ha'n", "wir", "ge\u00b7hof\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und hoffen noch jetzt;", "tokens": ["Und", "hof\u00b7fen", "noch", "jetzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Aber Hoffen und Harren", "tokens": ["A\u00b7ber", "Hof\u00b7fen", "und", "Har\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Macht Narren zuletzt.", "tokens": ["Macht", "Nar\u00b7ren", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Es ging besser hin, es ging besser her,", "tokens": ["Es", "ging", "bes\u00b7ser", "hin", ",", "es", "ging", "bes\u00b7ser", "her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn die Hoffnung nicht w\u00e4r'!", "tokens": ["Wenn", "die", "Hoff\u00b7nung", "nicht", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VAFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Unsre Hoffnung ist der Geist,", "tokens": ["Uns\u00b7re", "Hoff\u00b7nung", "ist", "der", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die Ketten zerrei\u00dft;", "tokens": ["Der", "die", "Ket\u00b7ten", "zer\u00b7rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Unsre Hoffnung ist das Schwert,", "tokens": ["Uns\u00b7re", "Hoff\u00b7nung", "ist", "das", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gen Tyrannen gekehrt!", "tokens": ["Gen", "Ty\u00b7ran\u00b7nen", "ge\u00b7kehrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wie ging es denn hin, wie ging es denn her,", "tokens": ["Wie", "ging", "es", "denn", "hin", ",", "wie", "ging", "es", "denn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}}}}