{"textgrid.poem.43730": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Kuckuck", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Sommer kehret wieder,", "tokens": ["Wenn", "der", "Som\u00b7mer", "keh\u00b7ret", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehr' auch ich ins Land zur\u00fcck.", "tokens": ["Kehr'", "auch", "ich", "ins", "Land", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singen dann die V\u00f6gel Lieder,", "tokens": ["Sin\u00b7gen", "dann", "die", "V\u00f6\u00b7gel", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sing' auch ich mein Meisterst\u00fcck.", "tokens": ["Sing'", "auch", "ich", "mein", "Meis\u00b7ter\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich mu\u00df dann immer wandern", "tokens": ["Und", "ich", "mu\u00df", "dann", "im\u00b7mer", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Nest und heimatlos.", "tokens": ["Oh\u00b7ne", "Nest", "und", "hei\u00b7mat\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch es ziehn mir gern die andern", "tokens": ["Doch", "es", "ziehn", "mir", "gern", "die", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine eignen Kinder gro\u00df.", "tokens": ["Mei\u00b7ne", "eig\u00b7nen", "Kin\u00b7der", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "K\u00f6nnt' ich lesen nun und schreiben,", "tokens": ["K\u00f6nnt'", "ich", "le\u00b7sen", "nun", "und", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schrieb' ich meinen Namen noch.", "tokens": ["Schrieb'", "ich", "mei\u00b7nen", "Na\u00b7men", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nein, ich la\u00df es bleiben:", "tokens": ["A\u00b7ber", "nein", ",", "ich", "la\u00df", "es", "blei\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Gesang verr\u00e4th mich doch.", "tokens": ["Mein", "Ge\u00b7sang", "ver\u00b7r\u00e4\u00b7th", "mich", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}