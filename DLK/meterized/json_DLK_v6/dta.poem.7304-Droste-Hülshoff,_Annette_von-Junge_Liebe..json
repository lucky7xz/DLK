{"dta.poem.7304": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Junge Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber dem Br\u00fcnnlein nicket der Zweig,", "tokens": ["Ue\u00b7ber", "dem", "Br\u00fcnn\u00b7lein", "ni\u00b7cket", "der", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Waldv\u00f6gel zwitschern und fl\u00f6ten,", "tokens": ["Wald\u00b7v\u00f6\u00b7gel", "zwit\u00b7schern", "und", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wild Anemon' und Schlehdorn bleich", "tokens": ["Wild", "A\u00b7ne\u00b7mon'", "und", "Schleh\u00b7dorn", "bleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Abendstrale sich r\u00f6then,", "tokens": ["Im", "A\u00b7bend\u00b7stra\u00b7le", "sich", "r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und ein M\u00e4dchen mit blondem Haar", "tokens": ["Und", "ein", "M\u00e4d\u00b7chen", "mit", "blon\u00b7dem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Beugt \u00fcber der glitzernden Welle,", "tokens": ["Beugt", "\u00fc\u00b7ber", "der", "glit\u00b7zern\u00b7den", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Schlankes M\u00e4dchen, kaum f\u00fcnfzehn Jahr,", "tokens": ["Schlan\u00b7kes", "M\u00e4d\u00b7chen", ",", "kaum", "f\u00fcnf\u00b7zehn", "Jahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "CARD", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Mit dem Auge der scheuen Gazelle.", "tokens": ["Mit", "dem", "Au\u00b7ge", "der", "scheu\u00b7en", "Ga\u00b7zel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ringelblumen bl\u00e4ttert sie ab:", "tokens": ["Rin\u00b7gel\u00b7blu\u00b7men", "bl\u00e4t\u00b7tert", "sie", "ab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u201eliebt er, liebt er mich nimmer?\u201c", "tokens": ["\u201e", "liebt", "er", ",", "liebt", "er", "mich", "nim\u00b7mer", "?", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PRF", "ADV", "$.", "$("], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und wenn \u201eliebt\u201c das Orakel gab,", "tokens": ["Und", "wenn", "\u201e", "liebt", "\u201c", "das", "O\u00b7ra\u00b7kel", "gab", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$(", "VVFIN", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "--+----+", "measure": "anapaest.init"}, "line.4": {"text": "Um ihr Antlitz gleitet ein Schimmer:", "tokens": ["Um", "ihr", "Ant\u00b7litz", "glei\u00b7tet", "ein", "Schim\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "\u201eliebt er nicht\u201c \u2014 o Grimm und Graus!", "tokens": ["\u201e", "liebt", "er", "nicht", "\u201c", "o", "Grimm", "und", "Graus", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "$(", "$(", "FM", "NE", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Da\u00df der Himmel den Bl\u00fcten gnade!", "tokens": ["Da\u00df", "der", "Him\u00b7mel", "den", "Bl\u00fc\u00b7ten", "gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Gras und Blumen, den ganzen Strau\u00df,", "tokens": ["Gras", "und", "Blu\u00b7men", ",", "den", "gan\u00b7zen", "Strau\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ART", "ADJA", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Wirft sie z\u00fcrnend in die Cascade.", "tokens": ["Wirft", "sie", "z\u00fcr\u00b7nend", "in", "die", "Ca\u00b7sca\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Gleitet dann in die Kr\u00e4uter lind,", "tokens": ["Glei\u00b7tet", "dann", "in", "die", "Kr\u00e4u\u00b7ter", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ihr Auge wird ernst und sinnend;", "tokens": ["Ihr", "Au\u00b7ge", "wird", "ernst", "und", "sin\u00b7nend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Frommer Eltern heftiges Kind,", "tokens": ["From\u00b7mer", "El\u00b7tern", "hef\u00b7ti\u00b7ges", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nur Minne nehmend und minnend,", "tokens": ["Nur", "Min\u00b7ne", "neh\u00b7mend", "und", "min\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Kannte sie nie ein anderes Band", "tokens": ["Kann\u00b7te", "sie", "nie", "ein", "an\u00b7de\u00b7res", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Als des Blutes, die sch\u00fcchterne Hinde;", "tokens": ["Als", "des", "Blu\u00b7tes", ",", "die", "sch\u00fcch\u00b7ter\u00b7ne", "Hin\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Und nun Einer, der nicht verwandt \u2014", "tokens": ["Und", "nun", "Ei\u00b7ner", ",", "der", "nicht", "ver\u00b7wandt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "$,", "PRELS", "PTKNEG", "VVPP", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ist das nicht eine schwere S\u00fcnde?", "tokens": ["Ist", "das", "nicht", "ei\u00b7ne", "schwe\u00b7re", "S\u00fcn\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Muthlos seufzet sie niederw\u00e4rts,", "tokens": ["Muth\u00b7los", "seuf\u00b7zet", "sie", "nie\u00b7der\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "In argem Sch\u00e4men und Gr\u00e4men,", "tokens": ["In", "ar\u00b7gem", "Sch\u00e4\u00b7men", "und", "Gr\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Will zuletzt ihr verstocktes Herz", "tokens": ["Will", "zu\u00b7letzt", "ihr", "ver\u00b7stock\u00b7tes", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Recht ernstlich in Frage nehmen.", "tokens": ["Recht", "ernst\u00b7lich", "in", "Fra\u00b7ge", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Abentheuer sinnet sie aus:", "tokens": ["A\u00b7bent\u00b7heu\u00b7er", "sin\u00b7net", "sie", "aus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wenn das Haus nun st\u00e4nde in Flammen,", "tokens": ["Wenn", "das", "Haus", "nun", "st\u00e4n\u00b7de", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und um H\u00fclfe riefen heraus", "tokens": ["Und", "um", "H\u00fcl\u00b7fe", "rie\u00b7fen", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PTKVZ"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Der Carl und die Mutter zusammen?", "tokens": ["Der", "Carl", "und", "die", "Mut\u00b7ter", "zu\u00b7sam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Pl\u00f6tzlich ein Perlenregen dicht", "tokens": ["Pl\u00f6tz\u00b7lich", "ein", "Per\u00b7len\u00b7re\u00b7gen", "dicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "St\u00fcrzt ihr gl\u00e4nzend aus beiden Augen,", "tokens": ["St\u00fcrzt", "ihr", "gl\u00e4n\u00b7zend", "aus", "bei\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "In die Kr\u00e4uter gedr\u00fcckt ihr Gesicht,", "tokens": ["In", "die", "Kr\u00e4u\u00b7ter", "ge\u00b7dr\u00fcckt", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wie das Blut der Erde zu saugen,", "tokens": ["Wie", "das", "Blut", "der", "Er\u00b7de", "zu", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ruft sie schluchzend: \u201eja, ja, ja!\u201c", "tokens": ["Ruft", "sie", "schluch\u00b7zend", ":", "\u201e", "ja", ",", "ja", ",", "ja", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "$(", "PTKANT", "$,", "PTKANT", "$,", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihre kleinen H\u00e4nde sich ringen,", "tokens": ["Ih\u00b7re", "klei\u00b7nen", "H\u00e4n\u00b7de", "sich", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "\u201eretten, retten w\u00fcrd' ich Mama,", "tokens": ["\u201e", "ret\u00b7ten", ",", "ret\u00b7ten", "w\u00fcrd'", "ich", "Ma\u00b7ma", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "VVINF", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und zum Carl in die Flamme springen!\u201c", "tokens": ["Und", "zum", "Carl", "in", "die", "Flam\u00b7me", "sprin\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPRART", "NE", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}