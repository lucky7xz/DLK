{"textgrid.poem.51034": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da liegt vor mir dein ungl\u00fccksel'ger Brief,", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da liegt vor mir dein ungl\u00fccksel'ger Brief,", "tokens": ["Da", "liegt", "vor", "mir", "dein", "un\u00b7gl\u00fcck\u00b7sel'\u00b7ger", "Brief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und weder Rat noch Hilfe seh ich winken;", "tokens": ["Und", "we\u00b7der", "Rat", "noch", "Hil\u00b7fe", "seh", "ich", "win\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "NN", "ADV", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schwer ist das Aufstehn wohl nach solchem Sinken,", "tokens": ["Schwer", "ist", "das", "Auf\u00b7stehn", "wohl", "nach", "sol\u00b7chem", "Sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du aber, Freund, du sankest fast zu tief!", "tokens": ["Du", "a\u00b7ber", ",", "Freund", ",", "du", "san\u00b7kest", "fast", "zu", "tief", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "PPER", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Lenz, der dich von Blum' zu Blume rief,", "tokens": ["Der", "Lenz", ",", "der", "dich", "von", "Blum'", "zu", "Blu\u00b7me", "rief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erloschen ist jetzt seiner Sonne Blinken;", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "jetzt", "sei\u00b7ner", "Son\u00b7ne", "Blin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den du so sinnlos hastig mu\u00dftest trinken,", "tokens": ["Den", "du", "so", "sinn\u00b7los", "has\u00b7tig", "mu\u00df\u00b7test", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Siehst du, was auf des Bechers Grunde schlief?", "tokens": ["Siehst", "du", ",", "was", "auf", "des", "Be\u00b7chers", "Grun\u00b7de", "schlief", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich aber steh in Ohnmacht, in der Ferne,", "tokens": ["Ich", "a\u00b7ber", "steh", "in", "Ohn\u00b7macht", ",", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fluch der Kraft, die dich von mir getrieben,", "tokens": ["Und", "fluch", "der", "Kraft", ",", "die", "dich", "von", "mir", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die nur zu wirren wei\u00df und nie zu l\u00f6sen.", "tokens": ["Die", "nur", "zu", "wir\u00b7ren", "wei\u00df", "und", "nie", "zu", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKZU", "VVINF", "VVFIN", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Am Ende preis ich meine d\u00fcrft'gen Sterne:", "tokens": ["Am", "En\u00b7de", "preis", "ich", "mei\u00b7ne", "d\u00fcrft'\u00b7gen", "Ster\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Guten tr\u00e4ge und zu bl\u00f6d im B\u00f6sen,", "tokens": ["Im", "Gu\u00b7ten", "tr\u00e4\u00b7ge", "und", "zu", "bl\u00f6d", "im", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "PTKA", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bin ich ein stilles Kind im Land geblieben!", "tokens": ["Bin", "ich", "ein", "stil\u00b7les", "Kind", "im", "Land", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}