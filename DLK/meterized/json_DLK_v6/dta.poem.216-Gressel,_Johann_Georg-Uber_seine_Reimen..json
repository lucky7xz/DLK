{"dta.poem.216": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber seine Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Verf\u00fcg dich schlechtes Buch zu Mirabellen hin/", "tokens": ["Ver\u00b7f\u00fcg", "dich", "schlech\u00b7tes", "Buch", "zu", "Mi\u00b7ra\u00b7bel\u00b7len", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wegen der Gestalt den Scepter solte f\u00fchren/", "tokens": ["Die", "we\u00b7gen", "der", "Ge\u00b7stalt", "den", "Scep\u00b7ter", "sol\u00b7te", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und zeuge ihr/ da\u00df ich ihr treuer Diener bin/", "tokens": ["Und", "zeu\u00b7ge", "ihr", "/", "da\u00df", "ich", "ihr", "treu\u00b7er", "Die\u00b7ner", "bin", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch scheu der Augen-Brand/ sonst werd ich dich verliehren.", "tokens": ["Doch", "scheu", "der", "Au\u00b7gen\u00b7Brand", "/", "sonst", "werd", "ich", "dich", "ver\u00b7lieh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$(", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar m\u00fc\u00dfte ich jetzo wol dein Gef\u00e4hrte seyn/", "tokens": ["Zwar", "m\u00fc\u00df\u00b7te", "ich", "jet\u00b7zo", "wol", "dein", "Ge\u00b7f\u00e4hr\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn nicht mein Hertze schon in ihren Br\u00fcsten lege/", "tokens": ["Wenn", "nicht", "mein", "Hert\u00b7ze", "schon", "in", "ih\u00b7ren", "Br\u00fcs\u00b7ten", "le\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.7": {"text": "L\u00e4\u00dft ihre H\u00f6fflichkeit auch nun die Bl\u00e4tter ein/", "tokens": ["L\u00e4\u00dft", "ih\u00b7re", "H\u00f6ff\u00b7lich\u00b7keit", "auch", "nun", "die", "Bl\u00e4t\u00b7ter", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So hat mein Hertz und Reim bey ihr fast gleiche Pflege.", "tokens": ["So", "hat", "mein", "Hertz", "und", "Reim", "bey", "ihr", "fast", "glei\u00b7che", "Pfle\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN", "APPR", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}