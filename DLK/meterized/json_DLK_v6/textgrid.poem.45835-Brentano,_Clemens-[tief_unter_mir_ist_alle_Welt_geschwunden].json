{"textgrid.poem.45835": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[tief unter mir ist alle Welt geschwunden]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief unter mir ist alle Welt geschwunden,", "tokens": ["Tief", "un\u00b7ter", "mir", "ist", "al\u00b7le", "Welt", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit ich an eines sch\u00f6nen Geistes Hand,", "tokens": ["Seit", "ich", "an", "ei\u00b7nes", "sch\u00f6\u00b7nen", "Geis\u00b7tes", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Binde von den Augen losgebunden,", "tokens": ["Die", "Bin\u00b7de", "von", "den", "Au\u00b7gen", "los\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf meines Daseins h\u00f6chster Zinne stand,", "tokens": ["Auf", "mei\u00b7nes", "Da\u00b7seins", "h\u00f6chs\u00b7ter", "Zin\u00b7ne", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist alle Lust oft rund um mich gewunden,", "tokens": ["Ist", "al\u00b7le", "Lust", "oft", "rund", "um", "mich", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weil sich die Liebe schaffend um mich wand;", "tokens": ["Weil", "sich", "die", "Lie\u00b7be", "schaf\u00b7fend", "um", "mich", "wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auch wird wohl einst mein krankes Herz gesunden,", "tokens": ["Auch", "wird", "wohl", "einst", "mein", "kran\u00b7kes", "Herz", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hab' ich die Aussicht wieder nur gefunden.", "tokens": ["Hab'", "ich", "die", "Aus\u00b7sicht", "wie\u00b7der", "nur", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}