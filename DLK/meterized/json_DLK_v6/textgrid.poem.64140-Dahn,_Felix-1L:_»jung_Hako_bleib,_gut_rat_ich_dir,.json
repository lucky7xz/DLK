{"textgrid.poem.64140": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbjung Hako bleib, gut rat ich dir,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbjung Hako bleib, gut rat ich dir,", "tokens": ["\u00bb", "jung", "Ha\u00b7ko", "bleib", ",", "gut", "rat", "ich", "dir", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NE", "VVFIN", "$,", "ADJD", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wankt mein Schritt zu Grab: \u2013", "tokens": ["Es", "wankt", "mein", "Schritt", "zu", "Grab", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein sei dies stille M\u00e4dchen hier", "tokens": ["Dein", "sei", "dies", "stil\u00b7le", "M\u00e4d\u00b7chen", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "PDS", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dein mein K\u00f6nigsstab.", "tokens": ["Und", "dein", "mein", "K\u00f6\u00b7nigs\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Arm ist der Nord, doch ist er treu,", "tokens": ["Arm", "ist", "der", "Nord", ",", "doch", "ist", "er", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und ist dein Heimatland:", "tokens": ["Und", "ist", "dein", "Hei\u00b7mat\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Fremde Gl\u00fcck birgt bittre Reu:\u00ab \u2013", "tokens": ["Der", "Frem\u00b7de", "Gl\u00fcck", "birgt", "bitt\u00b7re", "Reu", ":", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJA", "NE", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Hako hob die Hand:", "tokens": ["Doch", "Ha\u00b7ko", "hob", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbnein, K\u00f6nig Frode, dreimal nein!", "tokens": ["\u00bb", "nein", ",", "K\u00f6\u00b7nig", "Fro\u00b7de", ",", "drei\u00b7mal", "nein", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "NE", "$,", "ADV", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gib S\u00e4ldas stilles Herz,", "tokens": ["Gib", "S\u00e4l\u00b7das", "stil\u00b7les", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gib weiserm Mann die Krone dein: \u2013", "tokens": ["Gib", "wei\u00b7serm", "Mann", "die", "Kro\u00b7ne", "dein", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "ADJA", "NN", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich treibt es mittagw\u00e4rts.", "tokens": ["Mich", "treibt", "es", "mit\u00b7tag\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Hold ist ihr Antlitz, zart ihr Sinn,", "tokens": ["Hold", "ist", "ihr", "Ant\u00b7litz", ",", "zart", "ihr", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Herz ist tief und rein:", "tokens": ["Ihr", "Herz", "ist", "tief", "und", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch Hakos Hei\u00dfherz K\u00f6nigin", "tokens": ["Doch", "Ha\u00b7kos", "Hei\u00df\u00b7herz", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df hei\u00dfern Herzens sein!", "tokens": ["Mu\u00df", "hei\u00b7\u00dfern", "Her\u00b7zens", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hier K\u00f6nig \u00fcber Norges Eis", "tokens": ["Hier", "K\u00f6\u00b7nig", "\u00fc\u00b7ber", "Nor\u00b7ges", "Eis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Ficht' und F\u00f6hre sein,", "tokens": ["Und", "Ficht'", "und", "F\u00f6h\u00b7re", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Recht und Frieden sprechen weis'? \u2013", "tokens": ["Und", "Recht", "und", "Frie\u00b7den", "spre\u00b7chen", "weis'", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein, K\u00f6nig Frode, nein! \u2013", "tokens": ["Nein", ",", "K\u00f6\u00b7nig", "Fro\u00b7de", ",", "nein", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "NN", "NE", "$,", "PTKANT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und ruhn zuletzt im H\u00fcgelgrab,", "tokens": ["Und", "ruhn", "zu\u00b7letzt", "im", "H\u00fc\u00b7gel\u00b7grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Schlaf gewiegt vom Meer? \u2013 \u2013", "tokens": ["In", "Schlaf", "ge\u00b7wiegt", "vom", "Meer", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "VVPP", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Behalte deinen K\u00f6nigstab:", "tokens": ["Be\u00b7hal\u00b7te", "dei\u00b7nen", "K\u00f6\u00b7nigs\u00b7tab", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fort, fort dr\u00e4ngt mein Begehr!", "tokens": ["Fort", ",", "fort", "dr\u00e4ngt", "mein", "Be\u00b7gehr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Empor auf stolzen S\u00e4ulen steigt", "tokens": ["Em\u00b7por", "auf", "stol\u00b7zen", "S\u00e4u\u00b7len", "steigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch' Haus in Marmorglanz,", "tokens": ["Man\u00b7ch'", "Haus", "in", "Mar\u00b7mor\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von Myrt' und Lorbeer \u00fcberzweigt,", "tokens": ["Von", "Myrt'", "und", "Lor\u00b7beer", "\u00fc\u00b7berz\u00b7weigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Meere von Byzanz.", "tokens": ["Im", "Mee\u00b7re", "von", "By\u00b7zanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Manch Steinbild, alabasterwei\u00df,", "tokens": ["Manch", "Stein\u00b7bild", ",", "a\u00b7la\u00b7bas\u00b7ter\u00b7wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lauscht dort aus stillem Gr\u00fcn,", "tokens": ["Lauscht", "dort", "aus", "stil\u00b7lem", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00f6ner noch und lebenshei\u00df", "tokens": ["Und", "sch\u00f6\u00b7ner", "noch", "und", "le\u00b7bens\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nachtlock'ge Frauen gl\u00fchn.", "tokens": ["Nacht\u00b7lock'\u00b7ge", "Frau\u00b7en", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Hei! Gold und Wein und Rausch und Macht,", "tokens": ["Hei", "!", "Gold", "und", "Wein", "und", "Rausch", "und", "Macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dazwischen Kampf und Blut:", "tokens": ["Da\u00b7zwi\u00b7schen", "Kampf", "und", "Blut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Segelbr\u00fcder, taucht vor Nacht", "tokens": ["Ihr", "Se\u00b7gel\u00b7br\u00fc\u00b7der", ",", "taucht", "vor", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Seewolf in die Flut.", "tokens": ["Den", "See\u00b7wolf", "in", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Eudoxia, du Kaiserkind,", "tokens": ["Eu\u00b7do\u00b7xia", ",", "du", "Kai\u00b7ser\u00b7kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Halt' Kron' und G\u00fcrtel fest:", "tokens": ["Halt'", "Kron'", "und", "G\u00fcr\u00b7tel", "fest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn Hako Hei\u00dfherz freit geschwind!", "tokens": ["Denn", "Ha\u00b7ko", "Hei\u00df\u00b7herz", "freit", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf, Seewolf, gen S\u00fcdwest!\u00ab \u2013", "tokens": ["Auf", ",", "See\u00b7wolf", ",", "gen", "S\u00fcd\u00b7west", "!", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "$,", "NE", "$,", "APPR", "NN", "$.", "$(", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Zehn Winter flohn. \u2013 Still Abendrot", "tokens": ["Zehn", "Win\u00b7ter", "flohn", ".", "\u2013", "Still", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "$.", "$(", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag \u00fcber Meer und Strand \u2013", "tokens": ["Lag", "\u00fc\u00b7ber", "Meer", "und", "Strand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da stieg aus morschem Fischerboot", "tokens": ["Da", "stieg", "aus", "mor\u00b7schem", "Fi\u00b7scher\u00b7boot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein m\u00fcder Mann zu Land.", "tokens": ["Ein", "m\u00fc\u00b7der", "Mann", "zu", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Im Kronenschmuck ging S\u00e4lda hin,", "tokens": ["Im", "Kro\u00b7nen\u00b7schmuck", "ging", "S\u00e4l\u00b7da", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Ufer mit den Fraun, \u2013", "tokens": ["Am", "U\u00b7fer", "mit", "den", "Fraun", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er rief sie an: \u00bbHeil K\u00f6nigin!", "tokens": ["Er", "rief", "sie", "an", ":", "\u00bb", "Heil", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich einmal noch zu schaun!", "tokens": ["Dich", "ein\u00b7mal", "noch", "zu", "schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Nun scheid' ich gern! o Heimatland!", "tokens": ["Nun", "scheid'", "ich", "gern", "!", "o", "Hei\u00b7mat\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Norges Tannengr\u00fcn!", "tokens": ["O", "Nor\u00b7ges", "Tan\u00b7nen\u00b7gr\u00fcn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O M\u00f6wenschrei auf D\u00fcnensand,", "tokens": ["O", "M\u0