{"textgrid.poem.53031": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "Auf seine vorgehabte Uebersetzung des Virgils", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsterblicher Virgil, du Ehre deines Roms,", "tokens": ["U\u00b7nsterb\u00b7li\u00b7cher", "Vir\u00b7gil", ",", "du", "Eh\u00b7re", "dei\u00b7nes", "Roms", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Du w\u00fcrdiger bekr\u00e4ntzter Folger", "tokens": ["Du", "w\u00fcr\u00b7di\u00b7ger", "be\u00b7kr\u00b7\u00e4ntz\u00b7ter", "Fol\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Nebenbuhler des Homers,", "tokens": ["Und", "Ne\u00b7ben\u00b7buh\u00b7ler", "des", "Ho\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der du den frommen Held auf seinen starcken Schwingen", "tokens": ["Der", "du", "den", "from\u00b7men", "Held", "auf", "sei\u00b7nen", "star\u00b7cken", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem Schatten der Vergessenheit", "tokens": ["Dem", "Schat\u00b7ten", "der", "Ver\u00b7ges\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch deinen weisen Flug entrissen,", "tokens": ["Durch", "dei\u00b7nen", "wei\u00b7sen", "Flug", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ihn der sp\u00e4ten Welt dort in dem hellen Tempel", "tokens": ["Und", "ihn", "der", "sp\u00e4\u00b7ten", "Welt", "dort", "in", "dem", "hel\u00b7len", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Ewigkeit noch zur Bewundrung zeigst.", "tokens": ["Der", "E\u00b7wig\u00b7keit", "noch", "zur", "Be\u00b7wund\u00b7rung", "zeigst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich irr, und singe hier von Waffen, Mann und Streit,", "tokens": ["Ich", "irr", ",", "und", "sin\u00b7ge", "hier", "von", "Waf\u00b7fen", ",", "Mann", "und", "Streit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "KON", "VVFIN", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach dir, in meine deutschen Sayten,", "tokens": ["Nach", "dir", ",", "in", "mei\u00b7ne", "deut\u00b7schen", "Say\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wiederhole ungeh\u00f6rt", "tokens": ["Ich", "wie\u00b7der\u00b7ho\u00b7le", "un\u00b7ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein feurig edles Lied in Th\u00e4lern und in W\u00e4ldern:", "tokens": ["Dein", "feu\u00b7rig", "ed\u00b7les", "Lied", "in", "Th\u00e4\u00b7lern", "und", "in", "W\u00e4l\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich baue dir ein Ehrenmal", "tokens": ["Ich", "bau\u00b7e", "dir", "ein", "Eh\u00b7ren\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kr\u00f6ne dir mit frischen Lorbern", "tokens": ["Ich", "kr\u00f6\u00b7ne", "dir", "mit", "fri\u00b7schen", "Lor\u00b7bern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Grab und den Altar, und opfre dir die Reime", "tokens": ["Dein", "Grab", "und", "den", "Al\u00b7tar", ",", "und", "opf\u00b7re", "dir", "die", "Rei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So bis hieher der Dichtkunst Feld zerst\u00f6rt.", "tokens": ["So", "bis", "hie\u00b7her", "der", "Dicht\u00b7kunst", "Feld", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PAV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dein ewiges Gedicht entri\u00df dort ein August", "tokens": ["Dein", "e\u00b7wi\u00b7ges", "Ge\u00b7dicht", "ent\u00b7ri\u00df", "dort", "ein", "Au\u00b7gust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus unverdient gedrohten Flammen,", "tokens": ["Aus", "un\u00b7ver\u00b7di\u00b7ent", "ge\u00b7droh\u00b7ten", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wer aber wird in deutsche Luft", "tokens": ["Wer", "a\u00b7ber", "wird", "in", "deut\u00b7sche", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wieder an das Licht des hellen Tages ruffen,", "tokens": ["Es", "wie\u00b7der", "an", "das", "Licht", "des", "hel\u00b7len", "Ta\u00b7ges", "ruf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer ist der Held, der Musen Lust?", "tokens": ["Wer", "ist", "der", "Held", ",", "der", "Mu\u00b7sen", "Lust", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Hofnung, suchst du mich zu t\u00e4uschen?", "tokens": ["O", "Hof\u00b7nung", ",", "suchst", "du", "mich", "zu", "t\u00e4u\u00b7schen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erwach, und schau den Held. Er nur allein ist w\u00fcrdig", "tokens": ["Er\u00b7wach", ",", "und", "schau", "den", "Held", ".", "Er", "nur", "al\u00b7lein", "ist", "w\u00fcr\u00b7dig"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADJD", "ART", "NN", "$.", "PPER", "ADV", "ADV", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Ruhm mit dem August zu theilen.", "tokens": ["Den", "Ruhm", "mit", "dem", "Au\u00b7gust", "zu", "thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}