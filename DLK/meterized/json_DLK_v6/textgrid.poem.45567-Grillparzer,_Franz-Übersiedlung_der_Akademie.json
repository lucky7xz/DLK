{"textgrid.poem.45567": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcbersiedlung der Akademie", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sucht' euch eine Wohnung aus.", "tokens": ["Man", "sucht'", "euch", "ei\u00b7ne", "Woh\u00b7nung", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer doch eine gleiche h\u00e4tte!", "tokens": ["Wer", "doch", "ei\u00b7ne", "glei\u00b7che", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Wissenschaft hat nun ein Haus;", "tokens": ["Die", "Wis\u00b7sen\u00b7schaft", "hat", "nun", "ein", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch hat sie drum eine St\u00e4tte?", "tokens": ["Doch", "hat", "sie", "drum", "ei\u00b7ne", "St\u00e4t\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PAV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}