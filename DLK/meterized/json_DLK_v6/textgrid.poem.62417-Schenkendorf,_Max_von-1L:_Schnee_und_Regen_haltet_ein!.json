{"textgrid.poem.62417": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schnee und Regen haltet ein!", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schnee und Regen haltet ein!", "tokens": ["Schnee", "und", "Re\u00b7gen", "hal\u00b7tet", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmer zwingt ihr mein Gebein;", "tokens": ["Nim\u00b7mer", "zwingt", "ihr", "mein", "Ge\u00b7bein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nicht mit k\u00fchler Flut,", "tokens": ["A\u00b7ber", "nicht", "mit", "k\u00fch\u00b7ler", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein mit Feuer und mit Glut,", "tokens": ["Nein", "mit", "Feu\u00b7er", "und", "mit", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll man hier die Ritter taufen.", "tokens": ["Soll", "man", "hier", "die", "Rit\u00b7ter", "tau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt, ihr Blitze, brecht hervor,", "tokens": ["Kommt", ",", "ihr", "Blit\u00b7ze", ",", "brecht", "her\u00b7vor", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich finden mag das Thor", "tokens": ["Da\u00df", "ich", "fin\u00b7den", "mag", "das", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu der Burg der Hohenstaufen.", "tokens": ["Zu", "der", "Burg", "der", "Ho\u00b7hen\u00b7stau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einsam steig' ich auf die H\u00f6h'n,", "tokens": ["Ein\u00b7sam", "steig'", "ich", "auf", "die", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die letzten Tr\u00fcmmern stehn,", "tokens": ["Wo", "die", "letz\u00b7ten", "Tr\u00fcm\u00b7mern", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will dort wecken meinen Zorn,", "tokens": ["Will", "dort", "we\u00b7cken", "mei\u00b7nen", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will mir sch\u00e4rfen Schwert und Sporn", "tokens": ["Will", "mir", "sch\u00e4r\u00b7fen", "Schwert", "und", "Sporn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An den alten heil'gen Steinen.", "tokens": ["An", "den", "al\u00b7ten", "heil'\u00b7gen", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Denn mir kam ein Heergebot,", "tokens": ["Denn", "mir", "kam", "ein", "Heer\u00b7ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und im Osten sah ich roth", "tokens": ["Und", "im", "Os\u00b7ten", "sah", "ich", "roth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schon die Flammenlosung scheinen.", "tokens": ["Schon", "die", "Flam\u00b7men\u00b7lo\u00b7sung", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alte, gute, deutsche Zeit,", "tokens": ["Al\u00b7te", ",", "gu\u00b7te", ",", "deut\u00b7sche", "Zeit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weckest nimmer Gram und Neid,", "tokens": ["We\u00b7ckest", "nim\u00b7mer", "Gram", "und", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun aus deiner tiefen Gruft", "tokens": ["Nun", "aus", "dei\u00b7ner", "tie\u00b7fen", "Gruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich des Volkes Stimme ruft.", "tokens": ["Dich", "des", "Vol\u00b7kes", "Stim\u00b7me", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wieder sollen Lieder schallen,", "tokens": ["Wie\u00b7der", "sol\u00b7len", "Lie\u00b7der", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wieder h\u00f6rt man frohe M\u00e4r,", "tokens": ["Wie\u00b7der", "h\u00f6rt", "man", "fro\u00b7he", "M\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von der Deutschen Sieg und Ehr,", "tokens": ["Von", "der", "Deut\u00b7schen", "Sieg", "und", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie in Kaiser Friedrichs Hallen.", "tokens": ["Wie", "in", "Kai\u00b7ser", "Fried\u00b7richs", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zeuch in Gottes Krieg hinaus,", "tokens": ["Zeuch", "in", "Got\u00b7tes", "Krieg", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Altes Hohenstaufen-Haus!", "tokens": ["Al\u00b7tes", "Ho\u00b7hen\u00b7stau\u00b7fen\u00b7Haus", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo man Teufels K\u00fcnste d\u00e4mpft,", "tokens": ["Wo", "man", "Teu\u00b7fels", "K\u00fcns\u00b7te", "d\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird um Gottes Reich gek\u00e4mpft.", "tokens": ["Wird", "um", "Got\u00b7tes", "Reich", "ge\u00b7k\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier auch gibt es Sarazenen,", "tokens": ["Hier", "auch", "gibt", "es", "Sa\u00b7ra\u00b7ze\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier auch ist ein Orient,", "tokens": ["Hier", "auch", "ist", "ein", "O\u00b7rient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo die deutsche Liebe brennt,", "tokens": ["Wo", "die", "deut\u00b7sche", "Lie\u00b7be", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hier auch ist ein Platz der Thr\u00e4nen.", "tokens": ["Hier", "auch", "ist", "ein", "Platz", "der", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Wo man unsre Mutter schlug,", "tokens": ["Wo", "man", "uns\u00b7re", "Mut\u00b7ter", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die uns All' am Herzen trug.", "tokens": ["Die", "uns", "All'", "am", "Her\u00b7zen", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier auch ist ein heil'ges Grab,", "tokens": ["Hier", "auch", "ist", "ein", "heil'\u00b7ges", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wo die Herrin sich hinab", "tokens": ["Wo", "die", "Her\u00b7rin", "sich", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Barg mit vielen tiefen Wunden,", "tokens": ["Barg", "mit", "vie\u00b7len", "tie\u00b7fen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo sie einsam harrt und lauscht,", "tokens": ["Wo", "sie", "ein\u00b7sam", "harrt", "und", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob der Sieger Flug nicht rauscht,", "tokens": ["Ob", "der", "Sie\u00b7ger", "Flug", "nicht", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, schon viele Tag' und Stunden.", "tokens": ["Ach", ",", "schon", "vie\u00b7le", "Tag'", "und", "Stun\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zieh' dem deutschen Heer voraus,", "tokens": ["Zieh'", "dem", "deut\u00b7schen", "Heer", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Altes Hohenstaufen-Haus,", "tokens": ["Al\u00b7tes", "Ho\u00b7hen\u00b7stau\u00b7fen\u00b7Haus", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder wer berufen ist,", "tokens": ["O\u00b7der", "wer", "be\u00b7ru\u00b7fen", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer ein Deutscher ist, ein Christ \u2013", "tokens": ["Wer", "ein", "Deut\u00b7scher", "ist", ",", "ein", "Christ", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Freier wohlgeboren,", "tokens": ["Und", "ein", "Frei\u00b7er", "wohl\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ritter, Priester, Bauersmann,", "tokens": ["Rit\u00b7ter", ",", "Pries\u00b7ter", ",", "Bau\u00b7ers\u00b7mann", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zieh' voran dem heil'gen Bann,", "tokens": ["Zieh'", "vo\u00b7ran", "dem", "heil'\u00b7gen", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle haben ihn erkoren.", "tokens": ["Al\u00b7le", "ha\u00b7ben", "ihn", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Flammen lodern, Fahnen wehn,", "tokens": ["Flam\u00b7men", "lo\u00b7dern", ",", "Fah\u00b7nen", "wehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es wird mit Gott geschehn,", "tokens": ["Und", "es", "wird", "mit", "Gott", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was der Weisen Muth erkor,", "tokens": ["Was", "der", "Wei\u00b7sen", "Muth", "er\u00b7kor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der Treuen Herz beschwor.", "tokens": ["Was", "der", "Treu\u00b7en", "Herz", "be\u00b7schwor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebet wohl, ihr heil'ge Mauern,", "tokens": ["Le\u00b7bet", "wohl", ",", "ihr", "heil'\u00b7ge", "Mau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Siegeslust wird bald euch kund,", "tokens": ["Sie\u00b7ges\u00b7lust", "wird", "bald", "euch", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und der neue deutsche Bund,", "tokens": ["Und", "der", "neu\u00b7e", "deut\u00b7sche", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll euch Steine \u00fcberdauern.", "tokens": ["Soll", "euch", "Stei\u00b7ne", "\u00fc\u00b7berd\u00b7au\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}