{"textgrid.poem.25629": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen schlechten Schauspieler, der die Hauptrolle im L\u00fcgner gespielt hatte", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Beifall' des Parterr' erhitzt,", "tokens": ["Vom", "Bei\u00b7fall'", "des", "Par\u00b7terr'", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hab' er den L\u00fcgner gut gespielt;", "tokens": ["Hab'", "er", "den", "L\u00fcg\u00b7ner", "gut", "ge\u00b7spielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sagt er zwar, doch jeder f\u00fchlt,", "tokens": ["So", "sagt", "er", "zwar", ",", "doch", "je\u00b7der", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wahr spiel' er ihn erst itzt.", "tokens": ["Wahr", "spiel'", "er", "ihn", "erst", "itzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}