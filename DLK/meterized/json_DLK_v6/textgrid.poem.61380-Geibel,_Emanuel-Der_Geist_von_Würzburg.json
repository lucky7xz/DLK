{"textgrid.poem.61380": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Geist von W\u00fcrzburg", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu W\u00fcrzburg in der g\u00fcldnen Blum',", "tokens": ["Zu", "W\u00fcrz\u00b7burg", "in", "der", "g\u00fcld\u00b7nen", "Blum'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da, sagt man, geht ein Geist herum,", "tokens": ["Da", ",", "sagt", "man", ",", "geht", "ein", "Geist", "he\u00b7rum", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PIS", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der hat dem Wirt von Mitternacht", "tokens": ["Der", "hat", "dem", "Wirt", "von", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis Eins schon manchen Schreck gemacht.", "tokens": ["Bis", "Eins", "schon", "man\u00b7chen", "Schreck", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kamen einmal drei Studiosen", "tokens": ["Ka\u00b7men", "ein\u00b7mal", "drei", "Stu\u00b7di\u00b7o\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "CARD", "NN"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Mit knappem Reitwams, Lederhosen", "tokens": ["Mit", "knap\u00b7pem", "Reit\u00b7wams", ",", "Le\u00b7der\u00b7ho\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und hellem Sporenklang daher,", "tokens": ["Und", "hel\u00b7lem", "Spo\u00b7ren\u00b7klang", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denen erz\u00e4hlt der Wirt die M\u00e4r.", "tokens": ["De\u00b7nen", "er\u00b7z\u00e4hlt", "der", "Wirt", "die", "M\u00e4r."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Machen die Herren ein klug Gesicht,", "tokens": ["Ma\u00b7chen", "die", "Her\u00b7ren", "ein", "klug", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Sagen, sie glaubten kein Wort ihm nicht,", "tokens": ["Sa\u00b7gen", ",", "sie", "glaub\u00b7ten", "kein", "Wort", "ihm", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "PPER", "PTKNEG", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Sei'n gar gewitzt und viel gereist", "tokens": ["Sei'n", "gar", "ge\u00b7witzt", "und", "viel", "ge\u00b7reist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "VVPP", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und forcht'ten sich vor keinem Geist!", "tokens": ["Und", "forcht'\u00b7ten", "sich", "vor", "kei\u00b7nem", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wollten noch heut die Probe machen,", "tokens": ["Woll\u00b7ten", "noch", "heut", "die", "Pro\u00b7be", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Den Geist zu bannen und auszulachen.", "tokens": ["Den", "Geist", "zu", "ban\u00b7nen", "und", "aus\u00b7zu\u00b7la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "KON", "VVIZU", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So satzten sie vergn\u00fcgt im Sinn", "tokens": ["So", "satz\u00b7ten", "sie", "ver\u00b7gn\u00fcgt", "im", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In die verrufene Kammer sich hin,", "tokens": ["In", "die", "ver\u00b7ru\u00b7fe\u00b7ne", "Kam\u00b7mer", "sich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Stellten drei Lichter auf den Tisch,", "tokens": ["Stell\u00b7ten", "drei", "Lich\u00b7ter", "auf", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der Wirt bracht' ihnen vom Wei\u00dfen frisch;", "tokens": ["Der", "Wirt", "bracht'", "ih\u00b7nen", "vom", "Wei\u00b7\u00dfen", "frisch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie diskurrierten hin und her,", "tokens": ["Sie", "dis\u00b7kur\u00b7rier\u00b7ten", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trank jeder ein Ma\u00df und wohl noch mehr.", "tokens": ["Trank", "je\u00b7der", "ein", "Ma\u00df", "und", "wohl", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ART", "NN", "KON", "ADV", "ADV", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und als es schlug die zehnte Stunden,", "tokens": ["Und", "als", "es", "schlug", "die", "zehn\u00b7te", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Wei\u00dfe wollt' ihn'n nicht mehr munden,", "tokens": ["Der", "Wei\u00b7\u00dfe", "wollt'", "ihn'n", "nicht", "mehr", "mun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Lie\u00dfen sich drum vom Roten bringen;", "tokens": ["Lie\u00b7\u00dfen", "sich", "drum", "vom", "Ro\u00b7ten", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Der machte sie alsbalde singen,", "tokens": ["Der", "mach\u00b7te", "sie", "als\u00b7bal\u00b7de", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und jeder zu besundrer Lust", "tokens": ["Und", "je\u00b7der", "zu", "be\u00b7sund\u00b7rer", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Viel neuer Schw\u00e4nk' und Liedel wu\u00dft'.", "tokens": ["Viel", "neu\u00b7er", "Schw\u00e4nk'", "und", "Lie\u00b7del", "wu\u00dft'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Doch als die Turmuhr Eilfe schlug,", "tokens": ["Doch", "als", "die", "Tur\u00b7muhr", "Eil\u00b7fe", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sie hatten des Roten auch genug;", "tokens": ["Sie", "hat\u00b7ten", "des", "Ro\u00b7ten", "auch", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Forderten mit geschliffnen Kelchen", "tokens": ["For\u00b7der\u00b7ten", "mit", "ge\u00b7schliff\u00b7nen", "Kel\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Noch ", "tokens": ["Noch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Der hell im Glase rauscht und s\u00e4uselt", "tokens": ["Der", "hell", "im", "Gla\u00b7se", "rauscht", "und", "s\u00e4u\u00b7selt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und lichten Schaum und Perlen kr\u00e4uselt.", "tokens": ["Und", "lich\u00b7ten", "Schaum", "und", "Per\u00b7len", "kr\u00e4u\u00b7selt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Des tranken sie nun auch ihr Teil,", "tokens": ["Des", "tran\u00b7ken", "sie", "nun", "auch", "ihr", "Teil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Hatten dabei nicht lange Weil,", "tokens": ["Hat\u00b7ten", "da\u00b7bei", "nicht", "lan\u00b7ge", "Weil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PTKNEG", "ADV", "KOUS", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Bis endlich mit gelindem Schwanken", "tokens": ["Bis", "end\u00b7lich", "mit", "ge\u00b7lin\u00b7dem", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Umgingen ihnen die Gedanken,", "tokens": ["Um\u00b7gin\u00b7gen", "ih\u00b7nen", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ein leiser Frost sie \u00fcberkam,", "tokens": ["Ein", "lei\u00b7ser", "Frost", "sie", "\u00fc\u00b7ber\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der Kopf ward schwer, die Zunge lahm.", "tokens": ["Der", "Kopf", "ward", "schwer", ",", "die", "Zun\u00b7ge", "lahm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Da schlug es Mitternacht vom Turm;", "tokens": ["Da", "schlug", "es", "Mit\u00b7ter\u00b7nacht", "vom", "Turm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Auffuhr die T\u00fcr als wie im Sturm,", "tokens": ["Auf\u00b7fuhr", "die", "T\u00fcr", "als", "wie", "im", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOUS", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und trat herein zu ihrem Graun", "tokens": ["Und", "trat", "her\u00b7ein", "zu", "ih\u00b7rem", "Graun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der Geist, entsetzlich anzuschaun,", "tokens": ["Der", "Geist", ",", "ent\u00b7setz\u00b7lich", "an\u00b7zu\u00b7schaun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Aschfarb von Antlitz, Kleid und Schopf,", "tokens": ["Aschfarb", "von", "Ant\u00b7litz", ",", "Kleid", "und", "Schopf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Hinten mit einem langen Zopf,", "tokens": ["Hin\u00b7ten", "mit", "ei\u00b7nem", "lan\u00b7gen", "Zopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.31": {"text": "Die Nas' allein in rotem Schein", "tokens": ["Die", "Nas'", "al\u00b7lein", "in", "ro\u00b7tem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ergl\u00fchend wie Karfunkelstein.", "tokens": ["Er\u00b7gl\u00fc\u00b7hend", "wie", "Kar\u00b7fun\u00b7kel\u00b7stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Hertrat zum Tisch das Unget\u00fcm,", "tokens": ["Her\u00b7trat", "zum", "Tisch", "das", "Un\u00b7ge\u00b7t\u00fcm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Fuhr an die Herrn mit heisrer Stimm':", "tokens": ["Fuhr", "an", "die", "Herrn", "mit", "heis\u00b7rer", "Stimm'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "\u00bbwas treff' ich euch, ihr lockern Buben,", "tokens": ["\u00bb", "was", "treff'", "ich", "euch", ",", "ihr", "lo\u00b7ckern", "Bu\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Zu solcher Zeit in dieser Stuben?", "tokens": ["Zu", "sol\u00b7cher", "Zeit", "in", "die\u00b7ser", "Stu\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "K\u00f6nnt ihr nicht ruhig schlafen aus", "tokens": ["K\u00f6nnt", "ihr", "nicht", "ru\u00b7hig", "schla\u00b7fen", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Oder mit rechtem Flei\u00df zu Haus", "tokens": ["O\u00b7der", "mit", "rech\u00b7tem", "Flei\u00df", "zu", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Aristotelem exponieren,", "tokens": ["A\u00b7ris\u00b7to\u00b7te\u00b7lem", "ex\u00b7po\u00b7nie\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.40": {"text": "Euch aufs Examen pr\u00e4parieren?", "tokens": ["Euch", "aufs", "E\u00b7xa\u00b7men", "pr\u00e4\u00b7pa\u00b7rie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.41": {"text": "Statt dessen weicht ihr hier im Wein", "tokens": ["Statt", "des\u00b7sen", "weicht", "ihr", "hier", "im", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Eure steinharten K\u00f6pfe ein,", "toke