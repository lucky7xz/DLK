{"textgrid.poem.40269": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "der seine hellsten Gluten wagt?", "tokens": ["der", "sei\u00b7ne", "hells\u00b7ten", "Glu\u00b7ten", "wagt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er gl\u00fcht. Und ob er feuertrunken", "tokens": ["Er", "gl\u00fcht", ".", "Und", "ob", "er", "feu\u00b7er\u00b7trun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "vergl\u00fcht zu Asche \u00fcber Nacht:", "tokens": ["ver\u00b7gl\u00fcht", "zu", "A\u00b7sche", "\u00fc\u00b7ber", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "er gl\u00fcht! sein ", "tokens": ["er", "gl\u00fcht", "!", "sein"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "\u00bblieber kein Gl\u00fcck, nur lauter sein\u00ab,", "tokens": ["\u00bb", "lie\u00b7ber", "kein", "Gl\u00fcck", ",", "nur", "lau\u00b7ter", "sein", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PIAT", "NN", "$,", "ADV", "ADJD", "VAINF", "$(", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "nur lauter!", "tokens": ["nur", "lau\u00b7ter", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Ich hab ein ", "tokens": ["Ich", "hab", "ein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Woge, die zur Brandung sch\u00e4umt?", "tokens": ["Wo\u00b7ge", ",", "die", "zur", "Bran\u00b7dung", "sch\u00e4umt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kennst du den Sturmgeist, der die tolle", "tokens": ["Kennst", "du", "den", "Sturm\u00b7geist", ",", "der", "die", "tol\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "springende Woge noch toller b\u00e4umt?", "tokens": ["sprin\u00b7gen\u00b7de", "Wo\u00b7ge", "noch", "tol\u00b7ler", "b\u00e4umt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Steil starrt die Klippe: brecht, Wogen, brecht \u2013", "tokens": ["Steil", "starrt", "die", "Klip\u00b7pe", ":", "brecht", ",", "Wo\u00b7gen", ",", "brecht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "VVFIN", "$,", "NN", "$,", "VVFIN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "\u00bbnur keinen Schritt abseits vom Recht\u00ab,", "tokens": ["\u00bb", "nur", "kei\u00b7nen", "Schritt", "ab\u00b7seits", "vom", "Recht", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PIAT", "NN", "ADV", "APPRART", "NN", "$(", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "keinen Schritt!", "tokens": ["kei\u00b7nen", "Schritt", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "In meine tiefste Seelenstille", "tokens": ["In", "mei\u00b7ne", "tiefs\u00b7te", "See\u00b7len\u00b7stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "horcht mein erstauntes Ohr hinab;", "tokens": ["horcht", "mein", "er\u00b7staun\u00b7tes", "Ohr", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da ringt ein Trieb, da w\u00e4chst ein Wille,", "tokens": ["da", "ringt", "ein", "Trieb", ",", "da", "w\u00e4chst", "ein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "den eine tiefere Macht mir gab!", "tokens": ["den", "ei\u00b7ne", "tie\u00b7fe\u00b7re", "Macht", "mir", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich bin kein Frevler am Geschick \u2013", "tokens": ["Ich", "bin", "kein", "Frev\u00b7ler", "am", "Ge\u00b7schick", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbnur keine Schuld, lieber kein Gl\u00fcck\u00ab,", "tokens": ["\u00bb", "nur", "kei\u00b7ne", "Schuld", ",", "lie\u00b7ber", "kein", "Gl\u00fcck", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$(", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "nein: keine Schuld.", "tokens": ["nein", ":", "kei\u00b7ne", "Schuld", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Seit meiner Jugend droht im R\u00fccken", "tokens": ["Seit", "mei\u00b7ner", "Ju\u00b7gend", "droht", "im", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mir eine flach erhobne Hand;", "tokens": ["mir", "ei\u00b7ne", "flach", "er\u00b7hob\u00b7ne", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich mu\u00df mich mit mir selbst begl\u00fccken,", "tokens": ["ich", "mu\u00df", "mich", "mit", "mir", "selbst", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "seit ich die Welt so feige fand!", "tokens": ["seit", "ich", "die", "Welt", "so", "fei\u00b7ge", "fand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du meine Inbrunst, du mein Recht \u2013", "tokens": ["Du", "mei\u00b7ne", "In\u00b7brunst", ",", "du", "mein", "Recht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "\u00bbo Gott, ich st\u00fcrbe, w\u00fcrd'ich schlecht\u00ab,", "tokens": ["\u00bb", "o", "Gott", ",", "ich", "st\u00fcr\u00b7be", ",", "w\u00fcrd'\u00b7ich", "schlecht", "\u00ab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$,", "PPER", "VVFIN", "$,", "ADJD", "ADJD", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}