{"textgrid.poem.37536": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00dfchor", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mit dieser Welt gemeint,", "tokens": ["Was", "mit", "die\u00b7ser", "Welt", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheint mir keine Frage.", "tokens": ["Scheint", "mir", "kei\u00b7ne", "Fra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alle sind wir hier vereint", "tokens": ["Al\u00b7le", "sind", "wir", "hier", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh beim Festgelage.", "tokens": ["Froh", "beim", "Fest\u00b7ge\u00b7la\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Setzt Euch her und schaut Euch um,", "tokens": ["Setzt", "Euch", "her", "und", "schaut", "Euch", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Voll sind alle Tische;", "tokens": ["Voll", "sind", "al\u00b7le", "Ti\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Keiner ist von uns so dumm,", "tokens": ["Kei\u00b7ner", "ist", "von", "uns", "so", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er nichts erwische.", "tokens": ["Da\u00df", "er", "nichts", "er\u00b7wi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Jeder schau der Nachbarin", "tokens": ["Je\u00b7der", "schau", "der", "Nach\u00b7ba\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Augensterne,", "tokens": ["In", "die", "Au\u00b7gens\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df er den geheimen Sinn", "tokens": ["Da\u00df", "er", "den", "ge\u00b7hei\u00b7men", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses Lebens lerne.", "tokens": ["Die\u00b7ses", "Le\u00b7bens", "ler\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sto\u00dfet an! Die Wonnekraft", "tokens": ["Sto\u00b7\u00dfet", "an", "!", "Die", "Won\u00b7ne\u00b7kraft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6ge selig walten,", "tokens": ["M\u00f6\u00b7ge", "se\u00b7lig", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bis die Zeit uns fortgerafft", "tokens": ["Bis", "die", "Zeit", "uns", "fort\u00b7ge\u00b7rafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem Chor der Alten;", "tokens": ["Zu", "dem", "Chor", "der", "Al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Bis in s\u00fc\u00dfem Unverstand", "tokens": ["Bis", "in", "s\u00fc\u00b7\u00dfem", "Un\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Lippen lallen,", "tokens": ["Uns\u00b7re", "Lip\u00b7pen", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bis das Auge und die Hand,", "tokens": ["Bis", "das", "Au\u00b7ge", "und", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis wir selber fallen. \u2013", "tokens": ["Bis", "wir", "sel\u00b7ber", "fal\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Dann so tragt mich nur beiseit", "tokens": ["Dann", "so", "tragt", "mich", "nur", "bei\u00b7seit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In die dunkle Kammer,", "tokens": ["In", "die", "dunk\u00b7le", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auszuruhn in Ewigkeit", "tokens": ["Aus\u00b7zu\u00b7ruhn", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Katzenjammer.", "tokens": ["Oh\u00b7ne", "Kat\u00b7zen\u00b7jam\u00b7mer", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}