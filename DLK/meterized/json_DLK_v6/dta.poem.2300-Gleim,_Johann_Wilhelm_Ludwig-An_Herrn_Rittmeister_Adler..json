{"dta.poem.2300": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Rittmeister Adler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Wein vertreibt die Grillen,", "tokens": ["Mein", "Wein", "ver\u00b7treibt", "die", "Gril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Schwerdt die bl\u00f6den Helden,", "tokens": ["Mein", "Schwerdt", "die", "bl\u00f6\u00b7den", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Lob die lauten Schmeichler,", "tokens": ["Mein", "Lob", "die", "lau\u00b7ten", "Schmeich\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Tanz die Wintern\u00e4chte,", "tokens": ["Mein", "Tanz", "die", "Win\u00b7ter\u00b7n\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Spott den Schwarm der Narren,", "tokens": ["Mein", "Spott", "den", "Schwarm", "der", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein taubes Ohr die Praler,", "tokens": ["Mein", "tau\u00b7bes", "Ohr", "die", "Pra\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mein Schimpf die falschen Freunde,", "tokens": ["Mein", "Schimpf", "die", "fal\u00b7schen", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Glaub und meine Lieder", "tokens": ["Mein", "Glaub", "und", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Vertreiben tausend Teufel.", "tokens": ["Ver\u00b7trei\u00b7ben", "tau\u00b7send", "Teu\u00b7fel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nur den verschmitzten Amor,", "tokens": ["Nur", "den", "ver\u00b7schmitz\u00b7ten", "A\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "Den Schmeichler, den Tirannen,", "tokens": ["Den", "Schmeich\u00b7ler", ",", "den", "Ti\u00b7ran\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Kann kein Gebet, kein Degen,", "tokens": ["Kann", "kein", "Ge\u00b7bet", ",", "kein", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Kein Spott, kein Schimpf, kein Lachen,", "tokens": ["Kein", "Spott", ",", "kein", "Schimpf", ",", "kein", "La\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Und auch kein Wein veriagen.", "tokens": ["Und", "auch", "kein", "Wein", "ve\u00b7ria\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Freund! mit dem krummen Schwerdte,", "tokens": ["Freund", "!", "mit", "dem", "krum\u00b7men", "Schwerd\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.16": {"text": "Wei\u00dft du ihn zu vertreiben?", "tokens": ["Wei\u00dft", "du", "ihn", "zu", "ver\u00b7trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Kannst du es mit Husaren?", "tokens": ["Kannst", "du", "es", "mit", "Hu\u00b7sa\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}