{"textgrid.poem.40841": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Apalachen Wellenberge loh'n", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Apalachen Wellenberge loh'n", "tokens": ["Der", "A\u00b7pa\u00b7la\u00b7chen", "Wel\u00b7len\u00b7ber\u00b7ge", "loh'n"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Abendrothe, w\u00e4hrend Glockenton", "tokens": ["Im", "A\u00b7ben\u00b7dro\u00b7the", ",", "w\u00e4h\u00b7rend", "Glo\u00b7cken\u00b7ton"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Feierabend durch die Pflanzung hallt,", "tokens": ["Zum", "Fei\u00b7er\u00b7a\u00b7bend", "durch", "die", "Pflan\u00b7zung", "hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und m\u00e4hlich still es wird im dunklen Wald.", "tokens": ["Und", "m\u00e4h\u00b7lich", "still", "es", "wird", "im", "dunk\u00b7len", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Specht, Urwalds Kapellenmeister, pickt", "tokens": ["Der", "Specht", ",", "Ur\u00b7walds", "Ka\u00b7pel\u00b7len\u00b7meis\u00b7ter", ",", "pickt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht mehr den Takt; er wei\u00df, da\u00df ihm's nicht gl\u00fcckt", "tokens": ["Nicht", "mehr", "den", "Takt", ";", "er", "wei\u00df", ",", "da\u00df", "ihm's", "nicht", "gl\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu stimmen in des Einklangs Melodei", "tokens": ["Zu", "stim\u00b7men", "in", "des", "Ein\u00b7klangs", "Me\u00b7lo\u00b7dei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des K\u00e4uzchens Pfiff, des Papageien Schrei.", "tokens": ["Des", "K\u00e4uz\u00b7chens", "Pfiff", ",", "des", "Pa\u00b7pa\u00b7gei\u00b7en", "Schrei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Schatten einer Sycomore sitzt", "tokens": ["Im", "Schat\u00b7ten", "ei\u00b7ner", "Sy\u00b7co\u00b7mo\u00b7re", "sitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am r\u00e4um'gen Tisch, aus Acajou geschnitzt,", "tokens": ["Am", "r\u00e4um'\u00b7gen", "Tisch", ",", "aus", "A\u00b7caj\u00b7ou", "ge\u00b7schnitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Pflanzer, dem aus Kannen silberblank", "tokens": ["Der", "Pflan\u00b7zer", ",", "dem", "aus", "Kan\u00b7nen", "sil\u00b7ber\u00b7blank"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entgegenqualmt des Theebaums duft'ger Trank.", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7qualmt", "des", "Thee\u00b7baums", "duft'\u00b7ger", "Trank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Geschmiegt an ihn der ros'gen Kinder Schaar", "tokens": ["Ge\u00b7schmiegt", "an", "ihn", "der", "ros'\u00b7gen", "Kin\u00b7der", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ihm die schlanke Lieblings-Skwa gebar,", "tokens": ["Die", "ihm", "die", "schlan\u00b7ke", "Lieb\u00b7lings\u00b7Skwa", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umbl\u00fcht versch\u00f6nend seine rauhe Kraft,", "tokens": ["Um\u00b7bl\u00fcht", "ver\u00b7sch\u00f6\u00b7nend", "sei\u00b7ne", "rau\u00b7he", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Nikisranken bl\u00fchn am Cedernschaft.", "tokens": ["Wie", "Ni\u00b7kis\u00b7ran\u00b7ken", "bl\u00fchn", "am", "Ce\u00b7dern\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Welch Segensfeld liegt vor mir aufgethan!", "tokens": ["Welch", "Se\u00b7gens\u00b7feld", "liegt", "vor", "mir", "auf\u00b7ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein wei\u00dfes Wohnhaus blinkt im Wiesenplan,", "tokens": ["Sein", "wei\u00b7\u00dfes", "Wohn\u00b7haus", "blinkt", "im", "Wie\u00b7sen\u00b7plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Maisfeld rauscht, die Baumwollstaude weht,", "tokens": ["Das", "Mais\u00b7feld", "rauscht", ",", "die", "Baum\u00b7woll\u00b7stau\u00b7de", "weht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Zuckerrohr in hellen Bl\u00fcthen steht.", "tokens": ["Das", "Zu\u00b7cker\u00b7rohr", "in", "hel\u00b7len", "Bl\u00fc\u00b7then", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wie eine Opferschale, feierlich,", "tokens": ["Wie", "ei\u00b7ne", "Op\u00b7fer\u00b7scha\u00b7le", ",", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4lt er die volle Tasse jetzt vor sich,", "tokens": ["H\u00e4lt", "er", "die", "vol\u00b7le", "Tas\u00b7se", "jetzt", "vor", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und der Begeist'rung stiller Glanz umflicht", "tokens": ["Und", "der", "Be\u00b7geist'\u00b7rung", "stil\u00b7ler", "Glanz", "um\u00b7flicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Fast priesterlich sein strenges Angesicht:", "tokens": ["Fast", "pries\u00b7ter\u00b7lich", "sein", "stren\u00b7ges", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "\u00bbheil China dir! Durch ferne Meere weit", "tokens": ["\u00bb", "heil", "Chi\u00b7na", "dir", "!", "Durch", "fer\u00b7ne", "Mee\u00b7re", "weit"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "NE", "PPER", "$.", "APPR", "ADJA", "NN", "ADJD"], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eilt jetzt mein Dank zur\u00fcck in ferne Zeit", "tokens": ["Eilt", "jetzt", "mein", "Dank", "zu\u00b7r\u00fcck", "in", "fer\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sucht den Mann, der dieses heil'ge Kraut,", "tokens": ["Und", "sucht", "den", "Mann", ",", "der", "die\u00b7ses", "heil'\u00b7ge", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Nektar unsrer Freiheit, einst gebaut!", "tokens": ["Den", "Nek\u00b7tar", "uns\u00b7rer", "Frei\u00b7heit", ",", "einst", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Als er noch schritt an des Hoangho Strand,", "tokens": ["Als", "er", "noch", "schritt", "an", "des", "Ho\u00b7an\u00b7gho", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Und still die Saat entsunken seiner Hand,", "tokens": ["Und", "still", "die", "Saat", "ent\u00b7sun\u00b7ken", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohl hat kein Ahnen dessen ihn umweht,", "tokens": ["Wohl", "hat", "kein", "Ah\u00b7nen", "des\u00b7sen", "ihn", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PDS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df eines Welttheils Freiheit er ges\u00e4't!", "tokens": ["Da\u00df", "ei\u00b7nes", "Welt\u00b7theils", "Frei\u00b7heit", "er", "ge\u00b7s\u00e4't", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Hoch vom Pagodenthurm der Mandarin", "tokens": ["Hoch", "vom", "Pa\u00b7go\u00b7den\u00b7thurm", "der", "Man\u00b7da\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schaut \u00fcbers Land und streicht sich froh das Kinn!", "tokens": ["Schaut", "\u00fc\u00b7bers", "Land", "und", "streicht", "sich", "froh", "das", "Kinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Theebaum s\u00e4uselt so geheimni\u00dfvoll,", "tokens": ["Der", "Thee\u00b7baum", "s\u00e4u\u00b7selt", "so", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als ob er mehr als Bl\u00fcthen tragen soll.", "tokens": ["Als", "ob", "er", "mehr", "als", "Bl\u00fc\u00b7then", "tra\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "KOUS", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ob sein Vasall es leise nur errieth,", "tokens": ["Ob", "sein", "Va\u00b7sall", "es", "lei\u00b7se", "nur", "er\u00b7rieth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als er die\u00df Kraut auf gl\u00fchem Roste briet,", "tokens": ["Als", "er", "die\u00df", "Kraut", "auf", "gl\u00fc\u00b7hem", "Ros\u00b7te", "briet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Sankt Laurenzens Rost er sch\u00fcrt und facht,", "tokens": ["Da\u00df", "Sankt", "Lau\u00b7ren\u00b7zens", "Rost", "er", "sch\u00fcrt", "und", "facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der einst als Blutzeug' unsres Worts erwacht?", "tokens": ["Der", "einst", "als", "Blut\u00b7zeug'", "uns\u00b7res", "Worts", "er\u00b7wacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOUS", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Der Arzt, de\u00df Forschergeist aus diesem Kraut", "tokens": ["Der", "Arzt", ",", "de\u00df", "For\u00b7scher\u00b7geist", "aus", "die\u00b7sem", "Kraut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Siechen wunderkr\u00e4ft'gen Trank gebraut,", "tokens": ["Dem", "Sie\u00b7chen", "wun\u00b7der\u00b7kr\u00e4ft'\u00b7gen", "Trank", "ge\u00b7braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er wu\u00dft' es doch nicht, der gelahrte Mann,", "tokens": ["Er", "wu\u00dft'", "es", "doch", "nicht", ",", "der", "ge\u00b7lahr\u00b7te", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie da\u00df sein Kraut auch Ketten sprengen kann!", "tokens": ["Wie", "da\u00df", "sein", "Kraut", "auch", "Ket\u00b7ten", "spren\u00b7gen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPOSAT", "NN", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Der Britte, der einst mit dem dunklen Kraut", "tokens": ["Der", "Brit\u00b7te", ",", "der", "einst", "mit", "dem", "dunk\u00b7len", "Kraut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll seines Segelschiffes Bauch gestaut,", "tokens": ["Voll", "sei\u00b7nes", "Se\u00b7gel\u00b7schif\u00b7fes", "Bauch", "ge\u00b7staut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht wu\u00dft' er's, da\u00df die Rach' er f\u00fchrt' als Gast,", "tokens": ["Nicht", "wu\u00dft'", "er's", ",", "da\u00df", "die", "Rach'", "er", "f\u00fchrt'", "als", "Gast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PIS", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und da\u00df die Freiheit schwebt' ob seinem Mast!", "tokens": ["Und", "da\u00df", "die", "Frei\u00b7heit", "schwebt'", "ob", "sei\u00b7nem", "Mast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Hat jema