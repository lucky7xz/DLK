{"textgrid.poem.34613": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Uber ihren vermeynten todt", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der weit-erschollne tod der sch\u00f6nen Sylvia/", "tokens": ["Der", "weit\u00b7er\u00b7scholl\u00b7ne", "tod", "der", "sch\u00f6\u00b7nen", "Syl\u00b7via", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NE", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der nur/ der meynung nach/ nicht in der that geschah/", "tokens": ["Der", "nur", "/", "der", "mey\u00b7nung", "nach", "/", "nicht", "in", "der", "that", "ge\u00b7schah", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$(", "ART", "NN", "APPR", "$(", "PTKNEG", "APPR", "ART", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gieng ihrem Celadon so ungemein zu hertzen/", "tokens": ["Gieng", "ih\u00b7rem", "Ce\u00b7la\u00b7don", "so", "un\u00b7ge\u00b7mein", "zu", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er das feld verlie\u00df/ und voll entbrannter schmertzen", "tokens": ["Da\u00df", "er", "das", "feld", "ver\u00b7lie\u00df", "/", "und", "voll", "ent\u00b7brann\u00b7ter", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "KON", "ADJD", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In eine w\u00fcste lieff/ allwo er lange zeit", "tokens": ["In", "ei\u00b7ne", "w\u00fcs\u00b7te", "lieff", "/", "all\u00b7wo", "er", "lan\u00b7ge", "zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$(", "PWAV", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00fcr vielen seuffzern schwieg: bald aber weit und breit", "tokens": ["F\u00fcr", "vie\u00b7len", "seuff\u00b7zern", "schwieg", ":", "bald", "a\u00b7ber", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$.", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "(nachdem ein thr\u00e4nen-gu\u00df die erste regung stillte)", "tokens": ["(", "nach\u00b7dem", "ein", "thr\u00e4\u00b7nen\u00b7gu\u00df", "die", "ers\u00b7te", "re\u00b7gung", "still\u00b7te", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die ausgespannte lufft mit diesen klagen f\u00fcllte:", "tokens": ["Die", "aus\u00b7ge\u00b7spann\u00b7te", "lufft", "mit", "die\u00b7sen", "kla\u00b7gen", "f\u00fcll\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Betr\u00fcbter Celadon/ was hast du doch erlebt?", "tokens": ["Be\u00b7tr\u00fcb\u00b7ter", "Ce\u00b7la\u00b7don", "/", "was", "hast", "du", "doch", "er\u00b7lebt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein liebes-faden ward mit weh und angst gewebt/", "tokens": ["Ein", "lie\u00b7bes\u00b7fa\u00b7den", "ward", "mit", "weh", "und", "angst", "ge\u00b7webt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "APPR", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit schmertzen wird er nun auch wieder abgeschnitten", "tokens": ["Mit", "schmert\u00b7zen", "wird", "er", "nun", "auch", "wie\u00b7der", "ab\u00b7ge\u00b7schnit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVINF", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du hast sehr viel gehofft/ noch aber mehr erlitten.", "tokens": ["Du", "hast", "sehr", "viel", "ge\u00b7hofft", "/", "noch", "a\u00b7ber", "mehr", "er\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$(", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein tag schlo\u00df deinen geist in schwere ketten ein:", "tokens": ["Ein", "tag", "schlo\u00df", "dei\u00b7nen", "geist", "in", "schwe\u00b7re", "ket\u00b7ten", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Itzt heist ein andrer dich frey/ aber elend seyn.", "tokens": ["Itzt", "heist", "ein", "an\u00b7drer", "dich", "frey", "/", "a\u00b7ber", "e\u00b7lend", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "PPER", "ADJD", "$(", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "O himmel/ erd' und lufft/ erh\u00f6ret meine lieder!", "tokens": ["O", "him\u00b7mel", "/", "erd'", "und", "lufft", "/", "er\u00b7h\u00f6\u00b7ret", "mei\u00b7ne", "lie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ADJD", "KON", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Schafft meine Sylvia/ schafft meine liebste wieder.", "tokens": ["Schafft", "mei\u00b7ne", "Syl\u00b7via", "/", "schafft", "mei\u00b7ne", "liebs\u00b7te", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NE", "$(", "VVFIN", "PPOSAT", "ADJA", "ADV", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Mich d\u00fcnckt/ ich kan den ort annoch im traume sehn/", "tokens": ["Mich", "d\u00fcnckt", "/", "ich", "kan", "den", "ort", "an\u00b7noch", "im", "trau\u00b7me", "sehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VMFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wo unser erster blick/ wo unser ku\u00df geschehn.", "tokens": ["Wo", "un\u00b7ser", "ers\u00b7ter", "blick", "/", "wo", "un\u00b7ser", "ku\u00df", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$(", "PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Hier hat das liebe kind mir blumen abgepfl\u00fccket;", "tokens": ["Hier", "hat", "das", "lie\u00b7be", "kind", "mir", "blu\u00b7men", "ab\u00b7ge\u00b7pfl\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dort hab ich ihren mund mit s\u00fcsser milch erqvicket.", "tokens": ["Dort", "hab", "ich", "ih\u00b7ren", "mund", "mit", "s\u00fcs\u00b7ser", "milch", "er\u00b7qvic\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier sang/ hier spielte sie/ dort weinte sie f\u00fcr leid/", "tokens": ["Hier", "sang", "/", "hier", "spiel\u00b7te", "sie", "/", "dort", "wein\u00b7te", "sie", "f\u00fcr", "leid", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "$(", "ADV", "VVFIN", "PPER", "APPR", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und k\u00fc\u00dfte/ da sie schied/ mich voller traurigkeit.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "/", "da", "sie", "schied", "/", "mich", "vol\u00b7ler", "trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "O himmel/ erd' und lufft/ erh\u00f6ret meine lieder!", "tokens": ["O", "him\u00b7mel", "/", "erd'", "und", "lufft", "/", "er\u00b7h\u00f6\u00b7ret", "mei\u00b7ne", "lie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ADJD", "KON", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Schafft meine Sylvia/ schafft meine liebste wieder.", "tokens": ["Schafft", "mei\u00b7ne", "Syl\u00b7via", "/", "schafft", "mei\u00b7ne", "liebs\u00b7te", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NE", "$(", "VVFIN", "PPOSAT", "ADJA", "ADV", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Die sterne strahlen sehr/ noch sch\u00e4rffer Cynthia;", "tokens": ["Die", "ster\u00b7ne", "strah\u00b7len", "sehr", "/", "noch", "sch\u00e4rf\u00b7fer", "Cyn\u00b7thia", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "$(", "ADV", "ADJD", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.26": {"text": "Doch lange nicht so sch\u00f6n/ als meine Sylvia.", "tokens": ["Doch", "lan\u00b7ge", "nicht", "so", "sch\u00f6n", "/", "als", "mei\u00b7ne", "Syl\u00b7via", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "ADJD", "$(", "KOUS", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "F\u00fcr ihrem munde must' Aurora selbst erbleichen;", "tokens": ["F\u00fcr", "ih\u00b7rem", "mun\u00b7de", "must'", "Au\u00b7ro\u00b7ra", "selbst", "er\u00b7blei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Narcissus durffte sich nicht ihren wangen gleichen/", "tokens": ["Nar\u00b7cis\u00b7sus", "durff\u00b7te", "sich", "nicht", "ih\u00b7ren", "wan\u00b7gen", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ihr hals und ihre brust war schnee und elffenbein/", "tokens": ["Ihr", "hals", "und", "ih\u00b7re", "brust", "war", "schnee", "und", "elf\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ihr s\u00fcsses augen-licht ein steter sonnenschein.", "tokens": ["Ihr", "s\u00fcs\u00b7ses", "au\u00b7gen\u00b7licht", "ein", "ste\u00b7ter", "son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "O himmel/ erd' und lufft/ erh\u00f6ret meine lieder!", "tokens": ["O", "him\u00b7mel", "/", "erd'", "und", "lufft", "/", "er\u00b7h\u00f6\u00b7ret", "mei\u00b7ne", "lie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ADJD", "KON", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Schafft meine Sylvia/ schafft meine liebste wieder.", "tokens": ["Schafft", "mei\u00b7ne", "Syl\u00b7via", "/", "schafft", "mei\u00b7ne", "liebs\u00b7te", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NE", "$(", "VVFIN", "PPOSAT", "ADJA", "ADV", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Wenn ich mein morgen-brod mit saltz und thr\u00e4nen a\u00df/", "tokens": ["Wenn", "ich", "mein", "mor\u00b7gen\u00b7brod", "mit", "saltz", "und", "thr\u00e4\u00b7nen", "a\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ADJD", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So fiel sie neben mich in das bethaute gras/", "tokens": ["So", "fiel", "sie", "ne\u00b7ben", "mich", "in", "das", "be\u00b7thau\u00b7te", "gras", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und sang/ ob wolte sie die gantze welt bewegen.", "tokens": ["Und", "sang", "/", "ob", "wol\u00b7te", "sie", "die", "gant\u00b7ze", "welt", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die winde musten sich auff ihre seuffzer legen:", "tokens": ["Die", "win\u00b7de", "mus\u00b7ten", "sich", "auff", "ih\u00b7re", "seuff\u00b7zer", "le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die blitze stunden still/ und Ph\u00f6bus trat die bahn/", "tokens": ["Die", "blit\u00b7ze", "stun\u00b7den", "still", "/", "und", "Ph\u00f6\u00b7bus", "trat", "die", "bahn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$(", "KON", "NE", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So offt er sie ersah/ mit vollen freuden an.", "tokens": ["So", "offt", "er", "sie", "er\u00b7sah", "/", "mit", "vol\u00b7len", "freu\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "$(", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "O himmel/ erd' und lufft/ erh\u00f6ret meine lieder!", "tokens": ["O", "him\u00b7mel", "/", "erd'", "und", "lufft", "/", "er\u00b7h\u00f6\u00b7ret", "mei\u00b7ne", "lie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ADJD", "KON", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Schafft meine Sylvia/ schafft meine liebste wieder.", "tokens": ["Schafft", "mei\u00b7ne", "Syl\u00b7via", "/", "schafft", "mei\u00b7ne", "liebs\u00b7te", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NE", "$(", "VVFIN", "PPOSAT", "ADJA", "ADV", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Ihr qvellen/ die ihr mich mit wasser offt getr\u00e4nckt/", "tokens": ["Ihr", "qvel\u00b7len", "/", "die", "ihr", "mich", "mit", "was\