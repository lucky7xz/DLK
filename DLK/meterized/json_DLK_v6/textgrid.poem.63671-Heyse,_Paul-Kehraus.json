{"textgrid.poem.63671": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Kehraus", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gib dich drein, Herz, gib dich drein!", "tokens": ["Gib", "dich", "drein", ",", "Herz", ",", "gib", "dich", "drein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Tanz mu\u00df mal zu Ende sein.", "tokens": ["Der", "Tanz", "mu\u00df", "mal", "zu", "En\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So helle die Geigen,", "tokens": ["So", "hel\u00b7le", "die", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die T\u00e4nzer so jung,", "tokens": ["Die", "T\u00e4n\u00b7zer", "so", "jung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie schwangen im Reigen", "tokens": ["Sie", "schwan\u00b7gen", "im", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sich nimmer genung.", "tokens": ["Sich", "nim\u00b7mer", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Nun \u00e4chzet die Fiedel,", "tokens": ["Nun", "\u00e4ch\u00b7zet", "die", "Fie\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Nun schnarrt das Fagott", "tokens": ["Nun", "schnarrt", "das", "Fa\u00b7gott"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ein schl\u00e4friges Liedel \u2013", "tokens": ["Ein", "schl\u00e4f\u00b7ri\u00b7ges", "Lie\u00b7del", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "So helfe dir Gott!", "tokens": ["So", "hel\u00b7fe", "dir", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Gib dich drein!", "tokens": ["Gib", "dich", "drein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Gib dich drein, Herz, gib dich drein!", "tokens": ["Gib", "dich", "drein", ",", "Herz", ",", "gib", "dich", "drein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es bricht einmal die Nacht herein.", "tokens": ["Es", "bricht", "ein\u00b7mal", "die", "Nacht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nebligen Sterne", "tokens": ["Die", "neb\u00b7li\u00b7gen", "Ster\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit flackerndem Licht,", "tokens": ["Mit", "fla\u00b7ckern\u00b7dem", "Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie blinken so ferne", "tokens": ["Sie", "blin\u00b7ken", "so", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und w\u00e4rmen dich nicht.", "tokens": ["Und", "w\u00e4r\u00b7men", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Noch d\u00e4mmert ein Funken,", "tokens": ["Noch", "d\u00e4m\u00b7mert", "ein", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Im Busen entfacht;", "tokens": ["Im", "Bu\u00b7sen", "ent\u00b7facht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Auch der nun versunken,", "tokens": ["Auch", "der", "nun", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Rings g\u00e4hnet die Nacht.", "tokens": ["Rings", "g\u00e4h\u00b7net", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Gib dich drein!", "tokens": ["Gib", "dich", "drein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Gib dich drein, Herz, gib dich drein!", "tokens": ["Gib", "dich", "drein", ",", "Herz", ",", "gib", "dich", "drein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer wei\u00df, dich weckt ein Morgenschein.", "tokens": ["Wer", "wei\u00df", ",", "dich", "weckt", "ein", "Mor\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du l\u00e4gest wohl lieber,", "tokens": ["Du", "l\u00e4\u00b7gest", "wohl", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Verschliefst immerzu", "tokens": ["Ver\u00b7schliefst", "im\u00b7mer\u00b7zu"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Das hastige Fieber", "tokens": ["Das", "has\u00b7ti\u00b7ge", "Fie\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Des Lebens in Ruh.", "tokens": ["Des", "Le\u00b7bens", "in", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Doch hebt dann aufs neue", "tokens": ["Doch", "hebt", "dann", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der Tanz sich an,", "tokens": ["Der", "Tanz", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Tritt keck in die Reihe", "tokens": ["Tritt", "keck", "in", "die", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Und steh deinen Mann!", "tokens": ["Und", "steh", "dei\u00b7nen", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Gib dich drein!", "tokens": ["Gib", "dich", "drein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}