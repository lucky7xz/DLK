{"dta.poem.8545": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Unerh\u00f6rt .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ossa sprach zum Pelion:", "tokens": ["Der", "Os\u00b7sa", "sprach", "zum", "Pe\u00b7li\u00b7on", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewas ist f\u00fcr ein Klang in den L\u00fcften?", "tokens": ["\u201e", "was", "ist", "f\u00fcr", "ein", "Klang", "in", "den", "L\u00fcf\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Singt wohl die sterbende Nachtigall?", "tokens": ["Singt", "wohl", "die", "ster\u00b7ben\u00b7de", "Nach\u00b7ti\u00b7gall", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Oder eine versto\u00dfene Houri?", "tokens": ["O\u00b7der", "ei\u00b7ne", "ver\u00b7sto\u00b7\u00dfe\u00b7ne", "Hou\u00b7ri", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NE", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Zehnmal fielen meine Cedern hin,", "tokens": ["Zehn\u00b7mal", "fie\u00b7len", "mei\u00b7ne", "Ce\u00b7dern", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und meine Felsen zerbr\u00f6ckeln;", "tokens": ["Und", "mei\u00b7ne", "Fel\u00b7sen", "zer\u00b7br\u00f6\u00b7ckeln", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sechstausend Jahre machten mich grau", "tokens": ["Sech\u00b7stau\u00b7send", "Jah\u00b7re", "mach\u00b7ten", "mich", "grau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und sechszigtausend Stunden;", "tokens": ["Und", "sechs\u00b7zig\u00b7tau\u00b7send", "Stun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Doch nie drang solch ein Laut zu mir", "tokens": ["Doch", "nie", "drang", "solch", "ein", "Laut", "zu", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "ART", "APPR", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vom Thal oder aus der H\u00f6he.\u201c", "tokens": ["Vom", "Thal", "o\u00b7der", "aus", "der", "H\u00f6\u00b7he", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Eine Mutter am Hange steht,", "tokens": ["Ei\u00b7ne", "Mut\u00b7ter", "am", "Han\u00b7ge", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Die weint ihr einzig S\u00f6hnlein.", "tokens": ["Die", "weint", "ihr", "ein\u00b7zig", "S\u00f6hn\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}