{"dta.poem.21106": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Namen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Deines Namens fr\u00f6hlichen Wuchs, den wir s&#228;'ten               ", "tokens": ["Dei\u00b7nes", "Na\u00b7mens", "fr\u00f6h\u00b7li\u00b7chen", "Wuchs", ",", "den", "wir", "s", "&#228;", "'ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "XML_entity", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "PRELS", "PPER", "NE", "$(", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "in Kresse,", "tokens": ["in", "Kres\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Staunst du mit inniger Lust, liebliches", "tokens": ["Staunst", "du", "mit", "in\u00b7ni\u00b7ger", "Lust", ",", "lieb\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "ADJA"], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "T\u00f6chterchen, an?", "tokens": ["T\u00f6ch\u00b7ter\u00b7chen", ",", "an", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "L\u00e4sest du erst den Namen, ges\u00e4't vom Finger der", "tokens": ["L\u00e4\u00b7sest", "du", "erst", "den", "Na\u00b7men", ",", "ge\u00b7s\u00e4't", "vom", "Fin\u00b7ger", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "VVPP", "APPRART", "NN", "ART"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Allmacht,", "tokens": ["All\u00b7macht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "In den Blumen der Flur, in den Gestirnen", "tokens": ["In", "den", "Blu\u00b7men", "der", "Flur", ",", "in", "den", "Ge\u00b7stir\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "der Nacht!", "tokens": ["der", "Nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}