{"textgrid.poem.36860": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Gelassenheit", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist's, da\u00df ich mich qu\u00e4le?", "tokens": ["Was", "ist's", ",", "da\u00df", "ich", "mich", "qu\u00e4\u00b7le", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Harr Seiner, meine Seele,", "tokens": ["Harr", "Sei\u00b7ner", ",", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Harr und sei unverzagt!", "tokens": ["Harr", "und", "sei", "un\u00b7ver\u00b7zagt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VAFIN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Du wei\u00dft nicht, was dir n\u00fctzet:", "tokens": ["Du", "wei\u00dft", "nicht", ",", "was", "dir", "n\u00fct\u00b7zet", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gott wei\u00df es, und Gott sch\u00fctzet;", "tokens": ["Gott", "wei\u00df", "es", ",", "und", "Gott", "sch\u00fct\u00b7zet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er sch\u00fctzet den, der nach ihm fragt.", "tokens": ["Er", "sch\u00fct\u00b7zet", "den", ",", "der", "nach", "ihm", "fragt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er z\u00e4hlte meine Tage,", "tokens": ["Er", "z\u00e4hl\u00b7te", "mei\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Gl\u00fcck und meine Plage,", "tokens": ["Mein", "Gl\u00fcck", "und", "mei\u00b7ne", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Eh ich die Welt noch sah.", "tokens": ["Eh", "ich", "die", "Welt", "noch", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Eh ich mich selbst noch kannte,", "tokens": ["Eh", "ich", "mich", "selbst", "noch", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Eh ich ihn Vater nannte,", "tokens": ["Eh", "ich", "ihn", "Va\u00b7ter", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War er mir schon mit H\u00fclfe nah.", "tokens": ["War", "er", "mir", "schon", "mit", "H\u00fcl\u00b7fe", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die kleinste meiner Sorgen", "tokens": ["Die", "kleins\u00b7te", "mei\u00b7ner", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist dem Gott nicht verborgen,", "tokens": ["Ist", "dem", "Gott", "nicht", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Der alles sieht und h\u00e4lt;", "tokens": ["Der", "al\u00b7les", "sieht", "und", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und was er mir beschieden,", "tokens": ["Und", "was", "er", "mir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das dient zu meinem Frieden,", "tokens": ["Das", "dient", "zu", "mei\u00b7nem", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "W\u00e4r's auch die gr\u00f6\u00dfte Last der Welt.", "tokens": ["W\u00e4r's", "auch", "die", "gr\u00f6\u00df\u00b7te", "Last", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich lebe nicht auf Erden,", "tokens": ["Ich", "le\u00b7be", "nicht", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um gl\u00fccklich hier zu werden;", "tokens": ["Um", "gl\u00fcck\u00b7lich", "hier", "zu", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Lust der Welt vergeht.", "tokens": ["Die", "Lust", "der", "Welt", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich lebe hier, im Segen", "tokens": ["Ich", "le\u00b7be", "hier", ",", "im", "Se\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Grund zum Gl\u00fcck zu legen,", "tokens": ["Den", "Grund", "zum", "Gl\u00fcck", "zu", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das ewig, wie mein Geist, besteht.", "tokens": ["Das", "e\u00b7wig", ",", "wie", "mein", "Geist", ",", "be\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADJD", "$,", "KOKOM", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was dieses Gl\u00fcck vermehret,", "tokens": ["Was", "die\u00b7ses", "Gl\u00fcck", "ver\u00b7meh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sei mir von dir gew\u00e4hret!", "tokens": ["Sei", "mir", "von", "dir", "ge\u00b7w\u00e4h\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott, du gew\u00e4hrst es gern.", "tokens": ["Gott", ",", "du", "ge\u00b7w\u00e4hrst", "es", "gern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Was dieses Gl\u00fcck verletzet,", "tokens": ["Was", "die\u00b7ses", "Gl\u00fcck", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn's alle Welt auch sch\u00e4tzet,", "tokens": ["Wenn's", "al\u00b7le", "Welt", "auch", "sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sei, Herr, mein Gott, mir ewig fern!", "tokens": ["Sei", ",", "Herr", ",", "mein", "Gott", ",", "mir", "e\u00b7wig", "fern", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sind auch der Krankheit Plagen,", "tokens": ["Sind", "auch", "der", "Krank\u00b7heit", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Mangel schwer zu tragen,", "tokens": ["Der", "Man\u00b7gel", "schwer", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch schwerer Ha\u00df und Spott:", "tokens": ["Noch", "schwe\u00b7rer", "Ha\u00df", "und", "Spott", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So harr ich, und bin stille", "tokens": ["So", "harr", "ich", ",", "und", "bin", "stil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "$,", "KON", "VAFIN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu Gott; denn nicht mein Wille,", "tokens": ["Zu", "Gott", ";", "denn", "nicht", "mein", "Wil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Wille nur, gescheh, o Gott!", "tokens": ["Dein", "Wil\u00b7le", "nur", ",", "ge\u00b7scheh", ",", "o", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "VVFIN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du bist der M\u00fcden St\u00e4rke,", "tokens": ["Du", "bist", "der", "M\u00fc\u00b7den", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und aller deiner Werke", "tokens": ["Und", "al\u00b7ler", "dei\u00b7ner", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erbarmst du ewig dich.", "tokens": ["Er\u00b7barmst", "du", "e\u00b7wig", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was kann mir widerfahren,", "tokens": ["Was", "kann", "mir", "wi\u00b7der\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn Gott mich will bewahren?", "tokens": ["Wenn", "Gott", "mich", "will", "be\u00b7wah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und er, mein Gott, bewahret mich.", "tokens": ["Und", "er", ",", "mein", "Gott", ",", "be\u00b7wah\u00b7ret", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}