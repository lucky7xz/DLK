{"textgrid.poem.49463": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Toten, die im Grab wir bluten lassen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Toten, die im Grab wir bluten lassen,", "tokens": ["Die", "To\u00b7ten", ",", "die", "im", "Grab", "wir", "blu\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die r\u00e4chen sich.", "tokens": ["Die", "r\u00e4\u00b7chen", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wen sie mit schattenhaften H\u00e4nden fassen,", "tokens": ["Wen", "sie", "mit", "schat\u00b7ten\u00b7haf\u00b7ten", "H\u00e4n\u00b7den", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der jammert mich.", "tokens": ["Der", "jam\u00b7mert", "mich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O besser ist's, das Leben nie durchwandern,", "tokens": ["O", "bes\u00b7ser", "ist's", ",", "das", "Le\u00b7ben", "nie", "durch\u00b7wan\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ja selbst ein qualvoll Sterben nach dem andern,", "tokens": ["Ja", "selbst", "ein", "qual\u00b7voll", "Ster\u00b7ben", "nach", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJD", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So lange ist die Zeit, ihr Schlag so f\u00fcrchterlich.", "tokens": ["So", "lan\u00b7ge", "ist", "die", "Zeit", ",", "ihr", "Schlag", "so", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Menschen, die wir weinen machen, r\u00e4chen", "tokens": ["Die", "Men\u00b7schen", ",", "die", "wir", "wei\u00b7nen", "ma\u00b7chen", ",", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich manches Mal,", "tokens": ["Sich", "man\u00b7ches", "Mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und weh dem Schuld'gen, dessen Herz sie brechen", "tokens": ["Und", "weh", "dem", "Schuld'\u00b7gen", ",", "des\u00b7sen", "Herz", "sie", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Todesqual.", "tokens": ["In", "To\u00b7des\u00b7qual", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O besser mit dem grimmen B\u00e4ren ringen,", "tokens": ["O", "bes\u00b7ser", "mit", "dem", "grim\u00b7men", "B\u00e4\u00b7ren", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O besser hundertmal die hanfnen Schlingen,", "tokens": ["O", "bes\u00b7ser", "hun\u00b7dert\u00b7mal", "die", "hanf\u00b7nen", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das Federbett Othellos hundertmal.", "tokens": ["Das", "Fe\u00b7der\u00b7bett", "O\u00b7thel\u00b7los", "hun\u00b7dert\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verfolger, f\u00fcrchte den Vampir im Herzen,", "tokens": ["Ver\u00b7fol\u00b7ger", ",", "f\u00fcrch\u00b7te", "den", "Vam\u00b7pir", "im", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der dich verdammt.", "tokens": ["Der", "dich", "ver\u00b7dammt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "++-+", "measure": "iambic.di"}, "line.3": {"text": "Am Tag des Zorns kr\u00f6nt alle deine Schmerzen", "tokens": ["Am", "Tag", "des", "Zorns", "kr\u00f6nt", "al\u00b7le", "dei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein R\u00e4cheramt.", "tokens": ["Sein", "R\u00e4\u00b7cher\u00b7amt", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Halt auf den grossen Tag den Blick gerichtet,", "tokens": ["Halt", "auf", "den", "gros\u00b7sen", "Tag", "den", "Blick", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der wie ein Mord den M\u00f6rder einst vernichtet,", "tokens": ["Der", "wie", "ein", "Mord", "den", "M\u00f6r\u00b7der", "einst", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dem Dieb gleich, auf den Diebstahl niederflammt!", "tokens": ["Dem", "Dieb", "gleich", ",", "auf", "den", "Dieb\u00b7stahl", "nie\u00b7der\u00b7flammt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}