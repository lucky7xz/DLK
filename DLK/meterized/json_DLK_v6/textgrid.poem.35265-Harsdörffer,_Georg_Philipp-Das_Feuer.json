{"textgrid.poem.35265": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "Das Feuer", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Mensch auf dieser Welt kann meiner lang' entbehren,", "tokens": ["Kein", "Mensch", "auf", "die\u00b7ser", "Welt", "kann", "mei\u00b7ner", "lang'", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PDAT", "NN", "VMFIN", "PPOSAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch kann ich Jedermann verderben und gef\u00e4hren.", "tokens": ["Doch", "kann", "ich", "Je\u00b7der\u00b7mann", "ver\u00b7der\u00b7ben", "und", "ge\u00b7f\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich esse, was man mir giebt, ohne gro\u00dfen Dank,", "tokens": ["Ich", "es\u00b7se", ",", "was", "man", "mir", "giebt", ",", "oh\u00b7ne", "gro\u00b7\u00dfen", "Dank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$,", "KOUI", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und sterbe, wenn man mich will zwingen zum Getrank.", "tokens": ["Und", "ster\u00b7be", ",", "wenn", "man", "mich", "will", "zwin\u00b7gen", "zum", "Ge\u00b7trank", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VMFIN", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}