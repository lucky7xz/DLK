{"dta.poem.13319": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eso bleibt uns nur das ", "tokens": ["\u201e", "so", "bleibt", "uns", "nur", "das"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "\u201ein Eins vereiniget uns vorgestellet war.", "tokens": ["\u201e", "in", "Eins", "ver\u00b7ei\u00b7ni\u00b7get", "uns", "vor\u00b7ge\u00b7stel\u00b7let", "war", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VVFIN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewohlan! die\u00df ist der Prei\u00df, um den wir alle stritten;", "tokens": ["\u201e", "wo\u00b7hlan", "!", "die\u00df", "ist", "der", "Prei\u00df", ",", "um", "den", "wir", "al\u00b7le", "strit\u00b7ten", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "PDS", "VAFIN", "ART", "NN", "$,", "KOUI", "ART", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "440\u201dDie\u00df ists, um dessen Wohl und Heil wir uns bem\u00fchten.", "tokens": ["\"", "Die\u00df", "ists", ",", "um", "des\u00b7sen", "Wohl", "und", "Heil", "wir", "uns", "be\u00b7m\u00fch\u00b7ten", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "$,", "KOUI", "PDS", "ADV", "KON", "NN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ejhr wi\u00dft, was in dem Bau gedoppelt einfach sey;", "tokens": ["\u201e", "jhr", "wi\u00dft", ",", "was", "in", "dem", "Bau", "ge\u00b7dop\u00b7pelt", "ein\u00b7fach", "sey", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edem waren wir bisher gehorsam, lieb und treu.", "tokens": ["\u201e", "dem", "wa\u00b7ren", "wir", "bis\u00b7her", "ge\u00b7hor\u00b7sam", ",", "lieb", "und", "treu", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ehat uns der Himmel nicht zu dessen Wach\u2019 erkohren?", "tokens": ["\u201e", "hat", "uns", "der", "Him\u00b7mel", "nicht", "zu", "des\u00b7sen", "Wach'", "er\u00b7koh\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "PRELAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eund haben wir nicht selbst zu folchem Amt geschworen?", "tokens": ["\u201e", "und", "ha\u00b7ben", "wir", "nicht", "selbst", "zu", "fol\u00b7chem", "Amt", "ge\u00b7schwo\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}