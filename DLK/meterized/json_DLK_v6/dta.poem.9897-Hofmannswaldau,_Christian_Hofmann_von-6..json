{"dta.poem.9897": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich la\u00df/ wenn es mir gef\u00e4llet/ sehn mein haupt ohn schmuck un\u0303", "tokens": ["Ich", "la\u00df", "/", "wenn", "es", "mir", "ge\u00b7f\u00e4l\u00b7let", "/", "sehn", "mein", "haupt", "ohn", "schmuck", "u\u00f1"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PPER", "VVPP", "$(", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "NE"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "haar/", "tokens": ["haar", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Welcher mein gestchte schauet/ siehet mich auch gantz und gar/", "tokens": ["Wel\u00b7cher", "mein", "ge\u00b7stch\u00b7te", "schau\u00b7et", "/", "sie\u00b7het", "mich", "auch", "gantz", "und", "gar", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "ADJA", "VVFIN", "$(", "VVFIN", "PPER", "ADV", "ADV", "KON", "ADV", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Denn und wenn so zeig ich nur von demselben eine selte/", "tokens": ["Denn", "und", "wenn", "so", "zeig", "ich", "nur", "von", "dem\u00b7sel\u00b7ben", "ei\u00b7ne", "sel\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "KOUS", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "ART", "ADJA", "$("], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Halte meinen steten gang/ ausser den ich niemals schreite.", "tokens": ["Hal\u00b7te", "mei\u00b7nen", "ste\u00b7ten", "gang", "/", "aus\u00b7ser", "den", "ich", "nie\u00b7mals", "schrei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$(", "APPR", "ART", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}