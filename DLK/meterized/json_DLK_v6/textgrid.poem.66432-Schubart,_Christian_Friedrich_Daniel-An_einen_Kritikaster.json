{"textgrid.poem.66432": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An einen Kritikaster", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nichts als deinen Geifer kennt,", "tokens": ["Wer", "nichts", "als", "dei\u00b7nen", "Gei\u00b7fer", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "KOKOM", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der \u00e4cht Verdienst zu st\u00fcrzen brennt,", "tokens": ["Der", "\u00e4cht", "Ver\u00b7dienst", "zu", "st\u00fcr\u00b7zen", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ruft: der verfluchte Recensent!", "tokens": ["Ruft", ":", "der", "ver\u00b7fluch\u00b7te", "Re\u00b7cen\u00b7sent", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Schlagt todt den Hund, schlagt todt!", "tokens": ["Schlagt", "todt", "den", "Hund", ",", "schlagt", "todt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer aber deine Ohnmacht kennt,", "tokens": ["Wer", "a\u00b7ber", "dei\u00b7ne", "Ohn\u00b7macht", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer wei\u00df, du schimpfst ums liebe Brot,", "tokens": ["Wer", "wei\u00df", ",", "du", "schimpfst", "ums", "lie\u00b7be", "Brot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sagt: mit dem Schlingel hat's nicht Noth!", "tokens": ["Sagt", ":", "mit", "dem", "Schlin\u00b7gel", "hat's", "nicht", "Noth", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "APPR", "ART", "NN", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00dft ihn nur leben, er ist todt!", "tokens": ["La\u00dft", "ihn", "nur", "le\u00b7ben", ",", "er", "ist", "todt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}