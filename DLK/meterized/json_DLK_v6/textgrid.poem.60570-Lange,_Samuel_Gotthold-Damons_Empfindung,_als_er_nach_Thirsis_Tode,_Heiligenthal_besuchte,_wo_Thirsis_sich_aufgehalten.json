 "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja schweigt und trauret, Berg und Th\u00e4ler!", "tokens": ["Ja", "schweigt", "und", "trau\u00b7ret", ",", "Berg", "und", "Th\u00e4\u00b7ler", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "KON", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seyd stumm, ihr sonst so lauten H\u00fcgel,", "tokens": ["Seyd", "stumm", ",", "ihr", "sonst", "so", "lau\u00b7ten", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und schweig auch, ekler Wiederschal,", "tokens": ["Und", "schweig", "auch", ",", "ek\u00b7ler", "Wie\u00b7der\u00b7schal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn Thirsis reitzet dich nicht mehr.", "tokens": ["Denn", "Thir\u00b7sis", "reit\u00b7zet", "dich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch, Echo, weine, wenn ich weine,", "tokens": ["Doch", ",", "E\u00b7cho", ",", "wei\u00b7ne", ",", "wenn", "ich", "wei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir z\u00e4rtlich und gebrochen nach,", "tokens": ["Mir", "z\u00e4rt\u00b7lich", "und", "ge\u00b7bro\u00b7chen", "nach"