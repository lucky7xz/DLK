{"textgrid.poem.60570": {"metadata": {"author": {"name": "Lange, Samuel Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Damons Empfindung, als er nach Thirsis Tode, Heiligenthal besuchte, wo Thirsis sich aufgehalten", "genre": "verse", "period": "N.A.", "pub_year": 1746, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh, ich seh euch selge H\u00fctten,", "tokens": ["Ich", "seh", ",", "ich", "seh", "euch", "sel\u00b7ge", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich angenehmes Heilgenthal,", "tokens": ["Dich", "an\u00b7ge\u00b7neh\u00b7mes", "Heil\u00b7gen\u00b7thal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich gr\u00fc\u00df euch Gr\u00fcnde und ihr Berge,", "tokens": ["Ich", "gr\u00fc\u00df", "euch", "Gr\u00fcn\u00b7de", "und", "ihr", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich gr\u00fcsse dich beliebter Ort,", "tokens": ["Ich", "gr\u00fcs\u00b7se", "dich", "be\u00b7lieb\u00b7ter", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den ich zur Wallfart mir geweihet,", "tokens": ["Den", "ich", "zur", "Wall\u00b7fart", "mir", "ge\u00b7wei\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich gr\u00fc\u00df ich, doch mit heissen Thr\u00e4nen,", "tokens": ["Dich", "gr\u00fc\u00df", "ich", ",", "doch", "mit", "heis\u00b7sen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die mir die Liebe ausgepre\u00dft,", "tokens": ["Die", "mir", "die", "Lie\u00b7be", "aus\u00b7ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und die kein Zeitlauf hemmen kan.", "tokens": ["Und", "die", "kein", "Zeit\u00b7lauf", "hem\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier find ich dich, du steiler H\u00fcgel,", "tokens": ["Hier", "find", "ich", "dich", ",", "du", "stei\u00b7ler", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der meinem Freund ein Lustgang war", "tokens": ["Der", "mei\u00b7nem", "Freund", "ein", "Lust\u00b7gang", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo seine nimmer m\u00fcden Schritte", "tokens": ["Wo", "sei\u00b7ne", "nim\u00b7mer", "m\u00fc\u00b7den", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey Sturm und rauhem Kie\u00df und Sand,", "tokens": ["Bey", "Sturm", "und", "rau\u00b7hem", "Kie\u00df", "und", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In dem er mir entgegen sahe", "tokens": ["In", "dem", "er", "mir", "ent\u00b7ge\u00b7gen", "sa\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PPER", "PTKVZ", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den neuen Fu\u00dfsteig sich gebahnet,", "tokens": ["Den", "neu\u00b7en", "Fu\u00df\u00b7steig", "sich", "ge\u00b7bah\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo er, voll sehnlicher Begier,", "tokens": ["Wo", "er", ",", "voll", "sehn\u00b7li\u00b7cher", "Be\u00b7gier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Oft Stunden lang nach mir gesehn.", "tokens": ["Oft", "Stun\u00b7den", "lang", "nach", "mir", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dort unten rauscht in gr\u00fcner D\u00e4mmrung", "tokens": ["Dort", "un\u00b7ten", "rauscht", "in", "gr\u00fc\u00b7ner", "D\u00e4mm\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der kleine lautre Schmerlenbach,", "tokens": ["Der", "klei\u00b7ne", "laut\u00b7re", "Schmer\u00b7len\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dis ist das Thal, das er verschm\u00e4het,", "tokens": ["Dis", "ist", "das", "Thal", ",", "das", "er", "ver\u00b7schm\u00e4\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So reitzend es ihn auch gelockt,", "tokens": ["So", "reit\u00b7zend", "es", "ihn", "auch", "ge\u00b7lockt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wenn er, um mich in weiter Ferne", "tokens": ["Wenn", "er", ",", "um", "mich", "in", "wei\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUI", "PRF", "APPR", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von diesen Bergen zu entdecken,", "tokens": ["Von", "die\u00b7sen", "Ber\u00b7gen", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht Wind und rauhe Luft gescheut", "tokens": ["Nicht", "Wind", "und", "rau\u00b7he", "Luft", "ge\u00b7scheut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "KON", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und durch der St\u00fcrme Strudel brach.", "tokens": ["Und", "durch", "der", "St\u00fcr\u00b7me", "Stru\u00b7del", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun gr\u00fc\u00df ich dich begl\u00fcckte Wohnung,", "tokens": ["Nun", "gr\u00fc\u00df", "ich", "dich", "be\u00b7gl\u00fcck\u00b7te", "Woh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die meinen Thirsis in sich schlo\u00df:", "tokens": ["Die", "mei\u00b7nen", "Thir\u00b7sis", "in", "sich", "schlo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier war es, da er mich umarmte,", "tokens": ["Hier", "war", "es", ",", "da", "er", "mich", "um\u00b7arm\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier dr\u00fcckt er mich an seine Brust", "tokens": ["Hier", "dr\u00fcckt", "er", "mich", "an", "sei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie feurig k\u00fc\u00dft er im Umarmen?", "tokens": ["Wie", "feu\u00b7rig", "k\u00fc\u00dft", "er", "im", "Um\u00b7ar\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O, wie erg\u00f6tzt ihn nicht die Ankunft", "tokens": ["O", ",", "wie", "er\u00b7g\u00f6tzt", "ihn", "nicht", "die", "An\u00b7kunft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVPP", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Freundes, der in seiner Huld", "tokens": ["Des", "Freun\u00b7des", ",", "der", "in", "sei\u00b7ner", "Huld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein wahres Gl\u00fcck gesucht, und fand.", "tokens": ["Ein", "wah\u00b7res", "Gl\u00fcck", "ge\u00b7sucht", ",", "und", "fand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Itzt segn' ich dich du Stub und Kammer", "tokens": ["Itzt", "segn'", "ich", "dich", "du", "Stub", "und", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NE", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Thirsis sa\u00df und wo er schlief,", "tokens": ["Wo", "Thir\u00b7sis", "sa\u00df", "und", "wo", "er", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr, vormals Sitze keuscher Musen", "tokens": ["Ihr", ",", "vor\u00b7mals", "Sit\u00b7ze", "keu\u00b7scher", "Mu\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ADV", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr, nunmehr nichts als W\u00fcsteney!", "tokens": ["Ihr", ",", "nun\u00b7mehr", "nichts", "als", "W\u00fcs\u00b7te\u00b7ney", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier ist der Ort, wo er gedichtet;", "tokens": ["Hier", "ist", "der", "Ort", ",", "wo", "er", "ge\u00b7dich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier sang er mich und meine Doris;", "tokens": ["Hier", "sang", "er", "mich", "und", "mei\u00b7ne", "Do\u00b7ris", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "PPOSAT", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Hier weint er, als ich wieder gieng", "tokens": ["Hier", "weint", "er", ",", "als", "ich", "wie\u00b7der", "gieng"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und er um meine Brust sich hing.", "tokens": ["Und", "er", "um", "mei\u00b7ne", "Brust", "sich", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie lieb ich euch ihr stillen W\u00e4nde,", "tokens": ["Wie", "lieb", "ich", "euch", "ihr", "stil\u00b7len", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr sein Lied sehr oft geh\u00f6rt!", "tokens": ["Die", "ihr", "sein", "Lied", "sehr", "oft", "ge\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich lieb ich auch du edle Jugend,", "tokens": ["Dich", "lieb", "ich", "auch", "du", "ed\u00b7le", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Thirsis tugendhaft gef\u00fchrt:", "tokens": ["Die", "Thir\u00b7sis", "tu\u00b7gend\u00b7haft", "ge\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich r\u00fchren deine zarten Triebe,", "tokens": ["Mich", "r\u00fch\u00b7ren", "dei\u00b7ne", "zar\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und deine Thr\u00e4nen, die du weintest,", "tokens": ["Und", "dei\u00b7ne", "Thr\u00e4\u00b7nen", ",", "die", "du", "wein\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als Thirsis, der dich treu geliebt", "tokens": ["Als", "Thir\u00b7sis", ",", "der", "dich", "treu", "ge\u00b7liebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "$,", "PRELS", "PRF", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dich, doch gezwungen nur verlie\u00df.", "tokens": ["Dich", ",", "doch", "ge\u00b7zwun\u00b7gen", "nur", "ver\u00b7lie\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "VVPP", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Nun rei\u00df ich mich aus diesen Mauren,", "tokens": ["Nun", "rei\u00df", "ich", "mich", "aus", "die\u00b7sen", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nein, hier setz ich mich erst hin.", "tokens": ["Doch", "nein", ",", "hier", "setz", "ich", "mich", "erst", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier hat mein Thirsis stets gesessen,", "tokens": ["Hier", "hat", "mein", "Thir\u00b7sis", "stets", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und diesen Sitz so hoch geweiht.", "tokens": ["Und", "die\u00b7sen", "Sitz", "so", "hoch", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch, hier ist mir alles \u00f6de;", "tokens": ["Je\u00b7doch", ",", "hier", "ist", "mir", "al\u00b7les", "\u00f6\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn Thirsis fehlt, drum will ich gehen,", "tokens": ["Denn", "Thir\u00b7sis", "fehlt", ",", "drum", "will", "ich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was hast du, Dorf, das wohlgef\u00e4llt?", "tokens": ["Was", "hast", "du", ",", "Dorf", ",", "das", "wohl\u00b7ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nichts, als da\u00df Thirsis hier gewohnt.", "tokens": ["Nichts", ",", "als", "da\u00df", "Thir\u00b7sis", "hier", "ge\u00b7wohnt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOKOM", "KOUS", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und dadurch hat er dich geheiligt.", "tokens": ["Und", "da\u00b7durch", "hat", "er", "dich", "ge\u00b7hei\u00b7ligt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drum mach ich dich der Welt bekannt,", "tokens": ["Drum", "mach", "ich", "dich", "der", "Welt", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wanderer soll