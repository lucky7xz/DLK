{"textgrid.poem.48301": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Dessauer", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will ein Lied euch singen!", "tokens": ["Ich", "will", "ein", "Lied", "euch", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Held ist eigner Art:", "tokens": ["Mein", "Held", "ist", "eig\u00b7ner", "Art", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Zopf vor allen Dingen,", "tokens": ["Ein", "Zopf", "vor", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dreimaster, Knebelbart,", "tokens": ["Drei\u00b7mas\u00b7ter", ",", "Kne\u00b7bel\u00b7bart", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Blitzblank der Rock vom B\u00fcrsten", "tokens": ["Blitz\u00b7blank", "der", "Rock", "vom", "B\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und jeder Knopf wie Gold \u2013", "tokens": ["Und", "je\u00b7der", "Knopf", "wie", "Gold", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KOKOM", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr merkt, es gilt dem F\u00fcrsten,", "tokens": ["Ihr", "merkt", ",", "es", "gilt", "dem", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem alten ", "tokens": ["Dem", "al\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "All' Wissenschaft und Dichtung", "tokens": ["All'", "Wis\u00b7sen\u00b7schaft", "und", "Dich\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Lebtag er vermied,", "tokens": ["Sein", "Leb\u00b7tag", "er", "ver\u00b7mied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sprach er je von \u00bb", "tokens": ["Und", "sprach", "er", "je", "von", "\u00bb"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Meint' er in Reih und Glied;", "tokens": ["Meint'", "er", "in", "Reih", "und", "Glied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Statt Opern aller Arten", "tokens": ["Statt", "O\u00b7pern", "al\u00b7ler", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hatt' er nur einen Marsch,", "tokens": ["Hatt'", "er", "nur", "ei\u00b7nen", "Marsch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Und selbst mit Schriftgelahrten", "tokens": ["Und", "selbst", "mit", "Schrift\u00b7ge\u00b7lahr\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verfuhr er etwas barsch.", "tokens": ["Ver\u00b7fuhr", "er", "et\u00b7was", "barsch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht mocht' er Phrasen t\u00fcrmen", "tokens": ["Nicht", "mocht'", "er", "Phra\u00b7sen", "t\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Fortschritt, glatt und sch\u00f6n,", "tokens": ["Von", "Fort\u00b7schritt", ",", "glatt", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wu\u00dfte nur zu st\u00fcrmen", "tokens": ["Er", "wu\u00df\u00b7te", "nur", "zu", "st\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Kesselsdorfer H\u00f6hn;", "tokens": ["Die", "Kes\u00b7sels\u00b7dor\u00b7fer", "H\u00f6hn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er hielt nicht viel vom Zweifel", "tokens": ["Er", "hielt", "nicht", "viel", "vom", "Zwei\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wen'ger noch vom Spott,", "tokens": ["Und", "wen'\u00b7ger", "noch", "vom", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er war ein dummer Teufel", "tokens": ["Er", "war", "ein", "dum\u00b7mer", "Teu\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und glaubte noch an Gott.", "tokens": ["Und", "glaub\u00b7te", "noch", "an", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja, ja, er war im Leben", "tokens": ["Ja", ",", "ja", ",", "er", "war", "im", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beschr\u00e4nkt, wie man's so hei\u00dft,", "tokens": ["Be\u00b7schr\u00e4nkt", ",", "wie", "man's", "so", "hei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und soll ich Antwort geben,", "tokens": ["Und", "soll", "ich", "Ant\u00b7wort", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warum mein Lied ihn preist?", "tokens": ["Wa\u00b7rum", "mein", "Lied", "ihn", "preist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun denn, weil nie mit Worten", "tokens": ["Nun", "denn", ",", "weil", "nie", "mit", "Wor\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er seine Feinde fra\u00df,", "tokens": ["Er", "sei\u00b7ne", "Fein\u00b7de", "fra\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und weil ihm rechter Orten", "tokens": ["Und", "weil", "ihm", "rech\u00b7ter", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So Herz wie Galle sa\u00df.", "tokens": ["So", "Herz", "wie", "Gal\u00b7le", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir haben viel von N\u00f6ten,", "tokens": ["Wir", "ha\u00b7ben", "viel", "von", "N\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trotz allem guten Rat,", "tokens": ["Trotz", "al\u00b7lem", "gu\u00b7ten", "Rat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sollten schier err\u00f6ten", "tokens": ["Und", "soll\u00b7ten", "schier", "er\u00b7r\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor solchem Mann der Tat;", "tokens": ["Vor", "sol\u00b7chem", "Mann", "der", "Tat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verschnittnes Haar im Schopfe", "tokens": ["Ver\u00b7schnitt\u00b7nes", "Haar", "im", "Schop\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Macht nicht allein den Mann \u2013", "tokens": ["Macht", "nicht", "al\u00b7lein", "den", "Mann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ART", "NN", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}}}}}