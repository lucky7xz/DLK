{"textgrid.poem.39630": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der lahme Pamphletist", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn politisch er saalbadert,", "tokens": ["Wenn", "po\u00b7li\u00b7tisch", "er", "saal\u00b7ba\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit euch, ihr F\u00fcrsten, hadert,", "tokens": ["Und", "mit", "euch", ",", "ihr", "F\u00fcrs\u00b7ten", ",", "ha\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein k\u00fchner Patriot:", "tokens": ["Als", "ein", "k\u00fch\u00b7ner", "Pat\u00b7ri\u00b7ot", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denkt nicht, da\u00df euch Unheil droht.", "tokens": ["Denkt", "nicht", ",", "da\u00df", "euch", "Un\u00b7heil", "droht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, es d\u00e4mpft, gleich Morpheus Mohnen,", "tokens": ["Nein", ",", "es", "d\u00e4mpft", ",", "gleich", "Mor\u00b7pheus", "Moh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "ADV", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sein Pamphlet Rebellionen.", "tokens": ["Sein", "Pam\u00b7ph\u00b7let", "Re\u00b7bel\u00b7li\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Alles bricht in G\u00e4hnen aus,", "tokens": ["Al\u00b7les", "bricht", "in", "G\u00e4h\u00b7nen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und das Volk schleicht still nach Haus.", "tokens": ["Und", "das", "Volk", "schleicht", "still", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}