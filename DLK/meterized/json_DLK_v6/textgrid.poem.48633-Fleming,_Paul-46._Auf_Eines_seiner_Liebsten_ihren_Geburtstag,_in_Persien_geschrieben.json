{"textgrid.poem.48633": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "46. Auf Eines seiner Liebsten ihren Geburtstag, in Persien geschrieben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, der du es herzlich bist,", "tokens": ["Freund", ",", "der", "du", "es", "herz\u00b7lich", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "la\u00df dir etwas Liebes sagen", "tokens": ["la\u00df", "dir", "et\u00b7was", "Lie\u00b7bes", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und vernim es mit Behagen,", "tokens": ["und", "ver\u00b7nim", "es", "mit", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df, um was du wirst begr\u00fc\u00dft,", "tokens": ["da\u00df", ",", "um", "was", "du", "wirst", "be\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "von der Allerliebsten wegen", "tokens": ["von", "der", "Al\u00b7ler\u00b7liebs\u00b7ten", "we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "k\u00f6mt dir dieser Gru\u00df entgegen!", "tokens": ["k\u00f6mt", "dir", "die\u00b7ser", "Gru\u00df", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie zwar wird di\u00df sch\u00f6ne Licht", "tokens": ["Sie", "zwar", "wird", "di\u00df", "sch\u00f6\u00b7ne", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PDS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "mehr beseufzen als belachen,", "tokens": ["mehr", "be\u00b7seuf\u00b7zen", "als", "be\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KOKOM", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "weil du, Sch\u00f6nstes ihrer Sachen,", "tokens": ["weil", "du", ",", "Sch\u00f6ns\u00b7tes", "ih\u00b7rer", "Sa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dich hei ihr wilst finden nicht,", "tokens": ["dich", "hei", "ihr", "wilst", "fin\u00b7den", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VMFIN", "VVINF", "PTKNEG", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "nun sie in drei halben Jahren", "tokens": ["nun", "sie", "in", "drei", "hal\u00b7ben", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "CARD", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "nichts nicht hat von dir erfahren.", "tokens": ["nichts", "nicht", "hat", "von", "dir", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie mag hoffen, was sie kan!", "tokens": ["Sie", "mag", "hof\u00b7fen", ",", "was", "sie", "kan", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PRELS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil denn uns auch ist benommen", "tokens": ["Weil", "denn", "uns", "auch", "ist", "be\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "das, worzu da nicht kanst kommen,", "tokens": ["das", ",", "wor\u00b7zu", "da", "nicht", "kanst", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "wol, so binden wir dich an,", "tokens": ["wol", ",", "so", "bin\u00b7den", "wir", "dich", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "dich, in dem ihr Name lebet", "tokens": ["dich", ",", "in", "dem", "ihr", "Na\u00b7me", "le\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und ihr ganzes Herze schwebet.", "tokens": ["und", "ihr", "gan\u00b7zes", "Her\u00b7ze", "schwe\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "L\u00f6se dich f\u00fcr dich und sie", "tokens": ["L\u00f6\u00b7se", "dich", "f\u00fcr", "dich", "und", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "KON", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und vergi\u00df der blassen Sorgen!", "tokens": ["und", "ver\u00b7gi\u00df", "der", "blas\u00b7sen", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaff uns Lust bis an den Morgen!", "tokens": ["Schaff", "uns", "Lust", "bis", "an", "den", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du wirst wissen, wo und wie.", "tokens": ["Du", "wirst", "wis\u00b7sen", ",", "wo", "und", "wie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PWAV", "KON", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprich nur stracks, da\u00df man auf heute", "tokens": ["Sprich", "nur", "stracks", ",", "da\u00df", "man", "auf", "heu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "$,", "KOUS", "PIS", "APPR", "ADV"], "meter": "---+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "zu der gro\u00dfen Messe leute!", "tokens": ["zu", "der", "gro\u00b7\u00dfen", "Mes\u00b7se", "leu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}