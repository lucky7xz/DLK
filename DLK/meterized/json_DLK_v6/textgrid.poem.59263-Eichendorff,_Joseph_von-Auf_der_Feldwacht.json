{"textgrid.poem.59263": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Feldwacht", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gewehr im Arme steh ich", "tokens": ["Mein", "Ge\u00b7wehr", "im", "Ar\u00b7me", "steh", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier verloren auf der Wacht,", "tokens": ["Hier", "ver\u00b7lo\u00b7ren", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Still nach jener Gegend seh ich,", "tokens": ["Still", "nach", "je\u00b7ner", "Ge\u00b7gend", "seh", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab so oft dahin gedacht!", "tokens": ["Hab", "so", "oft", "da\u00b7hin", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PAV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fernher Abendglocken klingen", "tokens": ["Fern\u00b7her", "A\u00b7bend\u00b7glo\u00b7cken", "klin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die sch\u00f6ne Einsamkeit;", "tokens": ["Durch", "die", "sch\u00f6\u00b7ne", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So, wenn wir zusammen gingen,", "tokens": ["So", ",", "wenn", "wir", "zu\u00b7sam\u00b7men", "gin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "H\u00f6rt ich's oft in alter Zeit.", "tokens": ["H\u00f6rt", "ich's", "oft", "in", "al\u00b7ter", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wolken da wie T\u00fcrme prangen,", "tokens": ["Wol\u00b7ken", "da", "wie", "T\u00fcr\u00b7me", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als s\u00e4h ich im Dust mein Wien,", "tokens": ["Als", "s\u00e4h", "ich", "im", "Dust", "mein", "Wi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die Donau hell ergangen", "tokens": ["Und", "die", "Do\u00b7nau", "hell", "er\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen Burgen durch das Gr\u00fcn.", "tokens": ["Zwi\u00b7schen", "Bur\u00b7gen", "durch", "das", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wie fern sind Strom und T\u00fcrme!", "tokens": ["Doch", "wie", "fern", "sind", "Strom", "und", "T\u00fcr\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wer da wohnt, denkt mein noch kaum,", "tokens": ["Wer", "da", "wohnt", ",", "denkt", "mein", "noch", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VVFIN", "PPOSAT", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herbstlich rauschen schon die St\u00fcrme,", "tokens": ["Herbst\u00b7lich", "rau\u00b7schen", "schon", "die", "St\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich stehe wie im Traum.", "tokens": ["Und", "ich", "ste\u00b7he", "wie", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}