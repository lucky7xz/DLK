{"textgrid.poem.53811": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "L\u00fctzows wilde Jagd", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was gl\u00e4nzt dort vom Walde im Sonnenschein?", "tokens": ["Was", "gl\u00e4nzt", "dort", "vom", "Wal\u00b7de", "im", "Son\u00b7nen\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "H\u00f6rs n\u00e4her und n\u00e4her brausen.", "tokens": ["H\u00f6rs", "n\u00e4\u00b7her", "und", "n\u00e4\u00b7her", "brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es zieht sich herunter in d\u00fcstern Reihn,", "tokens": ["Es", "zieht", "sich", "her\u00b7un\u00b7ter", "in", "d\u00fcs\u00b7tern", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "APPR", "VVFIN", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "und gellende H\u00f6rner, sie schmettern drein", "tokens": ["und", "gel\u00b7len\u00b7de", "H\u00f6r\u00b7ner", ",", "sie", "schmet\u00b7tern", "drein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "und erf\u00fcllen die Seele mit Grausen.", "tokens": ["und", "er\u00b7f\u00fcl\u00b7len", "die", "See\u00b7le", "mit", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Und wenn ihr die schwarzen Gesellen fragt:", "tokens": ["Und", "wenn", "ihr", "die", "schwar\u00b7zen", "Ge\u00b7sel\u00b7len", "fragt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Es ist", "tokens": ["Es", "ist"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Was streift dort rasch durch den finstern Wald", "tokens": ["Was", "streift", "dort", "rasch", "durch", "den", "fins\u00b7tern", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und jaget von Bergen zu Bergen?", "tokens": ["und", "ja\u00b7get", "von", "Ber\u00b7gen", "zu", "Ber\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es legt sich in n\u00e4chtlichen Hinterhalt;", "tokens": ["Es", "legt", "sich", "in", "n\u00e4cht\u00b7li\u00b7chen", "Hin\u00b7ter\u00b7halt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "das Hurra jauchzet, die B\u00fcchse knallt,", "tokens": ["das", "Hur\u00b7ra", "jauch\u00b7zet", ",", "die", "B\u00fcch\u00b7se", "knallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "es st\u00fcrzen die j\u00fcdischen Schergen.", "tokens": ["es", "st\u00fcr\u00b7zen", "die", "j\u00fc\u00b7di\u00b7schen", "Scher\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und wenn ihr die schwarzen J\u00e4ger fragt:", "tokens": ["Und", "wenn", "ihr", "die", "schwar\u00b7zen", "J\u00e4\u00b7ger", "fragt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Es ist", "tokens": ["Es", "ist"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Die wilde Jagd und die deutsche Jagd", "tokens": ["Die", "wil\u00b7de", "Jagd", "und", "die", "deut\u00b7sche", "Jagd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "auf Henkersblut und Tyrannen!", "tokens": ["auf", "Hen\u00b7kers\u00b7blut", "und", "Ty\u00b7ran\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Drum, die ihr uns liebt, nicht geweint und geklagt!", "tokens": ["Drum", ",", "die", "ihr", "uns", "liebt", ",", "nicht", "ge\u00b7weint", "und", "ge\u00b7klagt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "PTKNEG", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das Land ist ja frei, und des Reimes wegen der Morgen tagt,", "tokens": ["Das", "Land", "ist", "ja", "frei", ",", "und", "des", "Rei\u00b7mes", "we\u00b7gen", "der", "Mor\u00b7gen", "tagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "wenn wirs auch erst sterbend gewannen!", "tokens": ["wenn", "wirs", "auch", "erst", "ster\u00b7bend", "ge\u00b7wan\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und von Enkeln zu Enkeln seis nachgesagt:", "tokens": ["Und", "von", "En\u00b7keln", "zu", "En\u00b7keln", "seis", "nach\u00b7ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Das war", "tokens": ["Das", "war"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "-+", "measure": "iambic.single"}}}}}