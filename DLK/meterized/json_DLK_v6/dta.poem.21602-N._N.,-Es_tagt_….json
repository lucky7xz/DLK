{"dta.poem.21602": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Es tagt \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.71", "nl:0.28"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Es tagt! Es tagt! Schon wogt\u2019s im Nebelmeer!", "tokens": ["Es", "tagt", "!", "Es", "tagt", "!", "Schon", "wogt's", "im", "Ne\u00b7bel\u00b7meer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die neue Welt, die k\u00e4mpfend wir ersehnen,", "tokens": ["Die", "neu\u00b7e", "Welt", ",", "die", "k\u00e4mp\u00b7fend", "wir", "er\u00b7seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wirft ihre Purpurstrahlen vor sich her:", "tokens": ["Wirft", "ih\u00b7re", "Pur\u00b7pur\u00b7strah\u00b7len", "vor", "sich", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O gr\u00fc\u00dfet sie mit heilgen Freudenthr\u00e4nen!", "tokens": ["O", "gr\u00fc\u00b7\u00dfet", "sie", "mit", "heil\u00b7gen", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht ohne Fehl ist diese neue Welt,", "tokens": ["Nicht", "oh\u00b7ne", "Fehl", "ist", "die\u00b7se", "neu\u00b7e", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht ohne Schuld und ohne tiefe Schmerzen,", "tokens": ["Nicht", "oh\u00b7ne", "Schuld", "und", "oh\u00b7ne", "tie\u00b7fe", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ist ihr Geist von stolzer ", "tokens": ["Doch", "ist", "ihr", "Geist", "von", "stol\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und frisches ", "tokens": ["Und", "fri\u00b7sches"], "token_info": ["word", "word"], "pos": ["KON", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Was sie mit goldnen Siegeskr\u00e4nzen ehrt,", "tokens": ["Was", "sie", "mit", "gold\u00b7nen", "Sie\u00b7ges\u00b7kr\u00e4n\u00b7zen", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bist du, o zwangbefreiter Muth der Jugend,", "tokens": ["Bist", "du", ",", "o", "zwang\u00b7be\u00b7frei\u00b7ter", "Muth", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was sie liebt und laut im Liede lehrt,", "tokens": ["Und", "was", "sie", "liebt", "und", "laut", "im", "Lie\u00b7de", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es ist die frei gewordene, ", "tokens": ["Es", "ist", "die", "frei", "ge\u00b7wor\u00b7de\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}