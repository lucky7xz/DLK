{"textgrid.poem.63142": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Laotse", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ward von einer armen Magd empfangen", "tokens": ["Er", "ward", "von", "ei\u00b7ner", "ar\u00b7men", "Magd", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf hartem Ackerland.", "tokens": ["Auf", "har\u00b7tem", "A\u00b7cker\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der grosse Wandrer kam gegangen", "tokens": ["Der", "gros\u00b7se", "Wand\u00b7rer", "kam", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nahm sie bei der Hand.", "tokens": ["Und", "nahm", "sie", "bei", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vor ihren Augen ward es finster,", "tokens": ["Vor", "ih\u00b7ren", "Au\u00b7gen", "ward", "es", "fins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrem Herzen ward es licht.", "tokens": ["In", "ih\u00b7rem", "Her\u00b7zen", "ward", "es", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versinkend spielte sie noch mit dem Ginster,", "tokens": ["Ver\u00b7sin\u00b7kend", "spiel\u00b7te", "sie", "noch", "mit", "dem", "Gins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Junik\u00e4fer schlug ihr ins Gesicht.", "tokens": ["Ein", "Ju\u00b7ni\u00b7k\u00e4\u00b7fer", "schlug", "ihr", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und als sie um sich sah, war sie erwacht.", "tokens": ["Und", "als", "sie", "um", "sich", "sah", ",", "war", "sie", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PRF", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Mond ber\u00fchrte blinkend ihren Jammer.", "tokens": ["Der", "Mond", "be\u00b7r\u00fchr\u00b7te", "blin\u00b7kend", "ih\u00b7ren", "Jam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und weinend ging sie durch die goldne Nacht", "tokens": ["Und", "wei\u00b7nend", "ging", "sie", "durch", "die", "gold\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ihre schwarze M\u00e4dchenkammer.", "tokens": ["In", "ih\u00b7re", "schwar\u00b7ze", "M\u00e4d\u00b7chen\u00b7kam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Neun Jahre trug durch Fron und Schweiss", "tokens": ["Neun", "Jah\u00b7re", "trug", "durch", "Fron", "und", "Schweiss"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie an dem Kind, das ihr erkoren.", "tokens": ["Sie", "an", "dem", "Kind", ",", "das", "ihr", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stunde kam. Sie hatte einen Greis", "tokens": ["Die", "Stun\u00b7de", "kam", ".", "Sie", "hat\u00b7te", "ei\u00b7nen", "Greis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In silberweissem Haar geboren.", "tokens": ["In", "sil\u00b7ber\u00b7weis\u00b7sem", "Haar", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sein Haupt war spitz und seine Haut war welk,", "tokens": ["Sein", "Haupt", "war", "spitz", "und", "sei\u00b7ne", "Haut", "war", "welk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dass sie erschrak, sooft sie ihn umherzte.", "tokens": ["Dass", "sie", "er\u00b7schrak", ",", "sooft", "sie", "ihn", "um\u00b7herz\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "+--++-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Vor seiner Stirne lag es wie Gew\u00f6lk.", "tokens": ["Vor", "sei\u00b7ner", "Stir\u00b7ne", "lag", "es", "wie", "Ge\u00b7w\u00f6lk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er sprach, als wenn ein Vater mit ihr scherzte.", "tokens": ["Er", "sprach", ",", "als", "wenn", "ein", "Va\u00b7ter", "mit", "ihr", "scherz\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sie sass bei ihm, nicht er bei ihr, und lauschte", "tokens": ["Sie", "sass", "bei", "ihm", ",", "nicht", "er", "bei", "ihr", ",", "und", "lauschte"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PTKNEG", "PPER", "APPR", "PPOSAT", "$,", "KON", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und trug ihr gross und kleines Weh", "tokens": ["Und", "trug", "ihr", "gross", "und", "klei\u00b7nes", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm an sein Ohr, das muschel\u00e4hnlich rauschte.", "tokens": ["Ihm", "an", "sein", "Ohr", ",", "das", "mu\u00b7schel\u00b7\u00e4hn\u00b7lich", "rauschte", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00e4chelnd streichelte sie Laotse.", "tokens": ["Und", "l\u00e4\u00b7chelnd", "strei\u00b7chel\u00b7te", "sie", "Laot\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}