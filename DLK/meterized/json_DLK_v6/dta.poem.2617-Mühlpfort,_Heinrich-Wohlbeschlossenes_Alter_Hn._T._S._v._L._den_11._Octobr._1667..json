{"dta.poem.2617": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wohlbeschlossenes Alter/  \n  Hn. T. S. v. L. den 11. Octobr.  1667.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was klagt/ ihr Sterblichen/ die Wenigkeit der Tage/", "tokens": ["Was", "klagt", "/", "ihr", "Sterb\u00b7li\u00b7chen", "/", "die", "We\u00b7nig\u00b7keit", "der", "Ta\u00b7ge", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df eurer Jahre Ziel so kurtz ist abgefast?", "tokens": ["Da\u00df", "eu\u00b7rer", "Jah\u00b7re", "Ziel", "so", "kurtz", "ist", "ab\u00b7ge\u00b7fast", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Legts Leben und den Brauch des Lebens auf die Wage/", "tokens": ["Legts", "Le\u00b7ben", "und", "den", "Brauch", "des", "Le\u00b7bens", "auf", "die", "Wa\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gewi\u00df/ das letzte macht euch nur die gr\u00f6ste Last.", "tokens": ["Ge\u00b7wi\u00df", "/", "das", "letz\u00b7te", "macht", "euch", "nur", "die", "gr\u00f6s\u00b7te", "Last", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ART", "ADJA", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Viel leben gantz verkehrt/ viel lernen niemals leben/", "tokens": ["Viel", "le\u00b7ben", "gantz", "ver\u00b7kehrt", "/", "viel", "ler\u00b7nen", "nie\u00b7mals", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VVPP", "$(", "ADV", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Theil sch\u00e4tzt sich begl\u00fcckt da\u00df Jahr und Zeit verraucht:", "tokens": ["Ein", "Theil", "sch\u00e4tzt", "sich", "be\u00b7gl\u00fcckt", "da\u00df", "Jahr", "und", "Zeit", "ver\u00b7raucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVPP", "KOUS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn nun die Stunde schl\u00e4gt/ und man soll Abschied geben/", "tokens": ["Wenn", "nun", "die", "Stun\u00b7de", "schl\u00e4gt", "/", "und", "man", "soll", "Ab\u00b7schied", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$(", "KON", "PIS", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So seufftzt man nur umbsonst/ da\u00df dieses Gut verbraucht.", "tokens": ["So", "seufftzt", "man", "nur", "um\u00b7bsonst", "/", "da\u00df", "die\u00b7ses", "Gut", "ver\u00b7braucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "$(", "KOUS", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wir h\u00e4tten langen Raum der Tugend nach zusetzen/", "tokens": ["Wir", "h\u00e4t\u00b7ten", "lan\u00b7gen", "Raum", "der", "Tu\u00b7gend", "nach", "zu\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "ART", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und gar bequeme Frist die Wei\u00dfheit anzuschaun/", "tokens": ["Und", "gar", "be\u00b7que\u00b7me", "Frist", "die", "Wei\u00df\u00b7heit", "an\u00b7zu\u00b7schaun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So lassen wir allhier uns die Begierden hetzen/", "tokens": ["So", "las\u00b7sen", "wir", "all\u00b7hier", "uns", "die", "Be\u00b7gier\u00b7den", "het\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und pflegen Babels Thurn hoch in die Lufft zu baun.", "tokens": ["Und", "pfle\u00b7gen", "Ba\u00b7bels", "Thurn", "hoch", "in", "die", "Lufft", "zu", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der vor der Th\u00fcren steht/ und auf Genaden harret/", "tokens": ["Der", "vor", "der", "Th\u00fc\u00b7ren", "steht", "/", "und", "auf", "Ge\u00b7na\u00b7den", "har\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$(", "KON", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Aempter bettelt aus/ und neue Titul sucht/", "tokens": ["Der", "A\u00b7emp\u00b7ter", "bet\u00b7telt", "aus", "/", "und", "neu\u00b7e", "Ti\u00b7tul", "sucht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Wei\u00df nicht wie liederlich er seine Zeit vernarret/", "tokens": ["Wei\u00df", "nicht", "wie", "lie\u00b7der\u00b7lich", "er", "sei\u00b7ne", "Zeit", "ver\u00b7nar\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "KOKOM", "ADJD", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wie viel Monat ihm zerfliessen sonder Frucht.", "tokens": ["Und", "wie", "viel", "Mo\u00b7nat", "ihm", "zer\u00b7flies\u00b7sen", "son\u00b7der", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIAT", "NN", "PPER", "VVPP", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein ander stirbt verwirrt in zweiffelhafften Rechten;", "tokens": ["Ein", "an\u00b7der", "stirbt", "ver\u00b7wirrt", "in", "zweif\u00b7fel\u00b7haff\u00b7ten", "Rech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die von dem Ehrgeitz frey/ die f\u00e4llt der Gelddurst an.", "tokens": ["Die", "von", "dem", "Ehr\u00b7geitz", "frey", "/", "die", "f\u00e4llt", "der", "Geld\u00b7durst", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "$(", "ART", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Theils leben unvers\u00f6hnt/ und wollen immer fechten/", "tokens": ["Theils", "le\u00b7ben", "un\u00b7ver\u00b7s\u00f6hnt", "/", "und", "wol\u00b7len", "im\u00b7mer", "fech\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$(", "KON", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was hat unreine Brunst bey manchen nicht gethan?", "tokens": ["Was", "hat", "un\u00b7rei\u00b7ne", "Brunst", "bey", "man\u00b7chen", "nicht", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJA", "NN", "APPR", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer so sein Leben nutzt/ da hat es freylich Fl\u00fcgel/", "tokens": ["Wer", "so", "sein", "Le\u00b7ben", "nutzt", "/", "da", "hat", "es", "frey\u00b7lich", "Fl\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ist schneller als ein Pfeil/ der durch die L\u00fcffte streicht", "tokens": ["Ist", "schnel\u00b7ler", "als", "ein", "Pfeil", "/", "der", "durch", "die", "L\u00fcff\u00b7te", "streicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn hilfft uns vor den Tod kein Freybrieff oder Siegel/", "tokens": ["Denn", "hilfft", "uns", "vor", "den", "Tod", "kein", "Frey\u00b7brieff", "o\u00b7der", "Sie\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das Ende/ das man so gesuchet/ ist erreicht.", "tokens": ["Das", "En\u00b7de", "/", "das", "man", "so", "ge\u00b7su\u00b7chet", "/", "ist", "er\u00b7reicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PIS", "ADV", "VVPP", "$(", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Hier kan man endlich wol noch einen F\u00fcrhang machen/", "tokens": ["Hier", "kan", "man", "end\u00b7lich", "wol", "noch", "ei\u00b7nen", "F\u00fcr\u00b7hang", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ADV", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Verstellen Aug\u2019 und Hertz; dort steht die Seele blo\u00df/", "tokens": ["Ver\u00b7stel\u00b7len", "Aug'", "und", "Hertz", ";", "dort", "steht", "die", "See\u00b7le", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wenn di\u00df Wort erschillt: Thu Raitung deiner Sachen/", "tokens": ["Und", "wenn", "di\u00df", "Wort", "er\u00b7schillt", ":", "Thu", "Rai\u00b7tung", "dei\u00b7ner", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "NN", "VVPP", "$.", "NE", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So geht der Aengsten Angst mit vollem Hauffen lo\u00df.", "tokens": ["So", "geht", "der", "A\u00b7engs\u00b7ten", "Angst", "mit", "vol\u00b7lem", "Hauf\u00b7fen", "lo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Gar einen andern Zweck sein Leben wol zu schliessen/", "tokens": ["Gar", "ei\u00b7nen", "an\u00b7dern", "Zweck", "sein", "Le\u00b7ben", "wol", "zu", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Zu dienen GOtt/ und auch dem Nechsten nutz zu seyn/", "tokens": ["Zu", "die\u00b7nen", "Gott", "/", "und", "auch", "dem", "Nechs\u00b7ten", "nutz", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$(", "KON", "ADV", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Hat ihm der ", "tokens": ["Hat", "ihm", "der"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.32": {"text": "Den man nun Lebenssatt dem Grabe sencket ein.", "tokens": ["Den", "man", "nun", "Le\u00b7bens\u00b7satt", "dem", "Gra\u00b7be", "sen\u00b7cket", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Des Geistes Fertigkeit/ der muntern Jahre Kr\u00e4ffte/", "tokens": ["Des", "Geis\u00b7tes", "Fer\u00b7tig\u00b7keit", "/", "der", "mun\u00b7tern", "Jah\u00b7re", "Kr\u00e4ff\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Hat Arbeit au\u00dfgesch\u00e4rfft/ und steter Flei\u00df vergn\u00fcgt;", "tokens": ["Hat", "Ar\u00b7beit", "au\u00df\u00b7ge\u00b7sch\u00e4rfft", "/", "und", "ste\u00b7ter", "Flei\u00df", "ver\u00b7gn\u00fcgt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$(", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es r\u00fchmt ein jederman den Fortgang der Gesch\u00e4ffte/", "tokens": ["Es", "r\u00fchmt", "ein", "je\u00b7der\u00b7man", "den", "Fort\u00b7gang", "der", "Ge\u00b7sch\u00e4ff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIS", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das noch der Stadt gar wol im Angedencken liegt.", "tokens": ["Das", "noch", "der", "Stadt", "gar", "wol", "im", "An\u00b7ge\u00b7den\u00b7cken", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Bi\u00df da\u00df sein Alter ihn hie\u00df auf die Ruhe dencken/", "tokens": ["Bi\u00df", "da\u00df", "sein", "Al\u00b7ter", "ihn", "hie\u00df", "auf", "die", "Ru\u00b7he", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Auf eine solche Ruh/ die Weise stets geliebt/", "tokens": ["Auf", "ei\u00b7ne", "sol\u00b7che", "Ruh", "/", "die", "Wei\u00b7se", "stets", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$(", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die sicher von dem Neid/ die Hoffart nicht kan kr\u00e4ncken/", "tokens": ["Die", "si\u00b7cher", "von", "dem", "Neid", "/", "die", "Hof\u00b7fart", "nicht", "kan", "kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "$(", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "In welcher Helden sich vor jener Zeit ge\u00fcbt.", "tokens": ["In", "wel\u00b7cher", "Hel\u00b7den", "sich", "vor", "je\u00b7ner", "Zeit", "ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PRF", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"},