{"textgrid.poem.58944": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "163. Geistliches Priesteramt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Allerheiligsten inwendig", "tokens": ["Im", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7ten", "in\u00b7wen\u00b7dig"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verricht dein Priesteramt best\u00e4ndig!", "tokens": ["Ver\u00b7richt", "dein", "Pries\u00b7ter\u00b7amt", "be\u00b7st\u00e4n\u00b7dig", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In s\u00fc\u00dfer, dunkler Stille, da", "tokens": ["In", "s\u00fc\u00b7\u00dfer", ",", "dunk\u00b7ler", "Stil\u00b7le", ",", "da"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Gottes Majest\u00e4t dir nah.", "tokens": ["Ist", "Got\u00b7tes", "Ma\u00b7jes\u00b7t\u00e4t", "dir", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}