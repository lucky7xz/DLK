{"textgrid.poem.42178": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Schweig und sei lieb", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als du, mein Held, zum ersten Male mir", "tokens": ["Als", "du", ",", "mein", "Held", ",", "zum", "ers\u00b7ten", "Ma\u00b7le", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im lichterf\u00fcllten Saal entgegentratest", "tokens": ["Im", "lich\u00b7ter\u00b7f\u00fcll\u00b7ten", "Saal", "ent\u00b7ge\u00b7gent\u00b7ra\u00b7test"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und l\u00e4chelnd, fast mit kindlichem Gezier,", "tokens": ["Und", "l\u00e4\u00b7chelnd", ",", "fast", "mit", "kind\u00b7li\u00b7chem", "Ge\u00b7zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um einen Walzer mich verlegen batest,", "tokens": ["Um", "ei\u00b7nen", "Wal\u00b7zer", "mich", "ver\u00b7le\u00b7gen", "ba\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wei\u00dft du, was in des Morgens D\u00e4mmerstunden,", "tokens": ["Wei\u00dft", "du", ",", "was", "in", "des", "Mor\u00b7gens", "D\u00e4m\u00b7mer\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "APPR", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Eh dich mein Traum von neuem mir verbunden", "tokens": ["Eh", "dich", "mein", "Traum", "von", "neu\u00b7em", "mir", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich in mein Tagebuch err\u00f6tend schrieb? \u2013", "tokens": ["Ich", "in", "mein", "Ta\u00b7ge\u00b7buch", "er\u00b7r\u00f6\u00b7tend", "schrieb", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schweig und sei lieb!", "tokens": ["Schweig", "und", "sei", "lieb", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VAFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und als du gestern mir mit raschen Schritten", "tokens": ["Und", "als", "du", "ge\u00b7stern", "mir", "mit", "ra\u00b7schen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nachjagtest \u2013 zum Befehl ward mir dein Ruf;", "tokens": ["Nach\u00b7jag\u00b7test", "\u2013", "zum", "Be\u00b7fehl", "ward", "mir", "dein", "Ruf", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "APPRART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Kind h\u00e4tt ich ihn nie so streng gelitten,", "tokens": ["Als", "Kind", "h\u00e4tt", "ich", "ihn", "nie", "so", "streng", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da stets nur Trotz er mir im Herzen schuf \u2013", "tokens": ["Da", "stets", "nur", "Trotz", "er", "mir", "im", "Her\u00b7zen", "schuf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ahnst du, weshalb in fieberhei\u00dfem Beben,", "tokens": ["Ahnst", "du", ",", "we\u00b7shalb", "in", "fie\u00b7ber\u00b7hei\u00b7\u00dfem", "Be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weshalb ich rettungslos dir preisgegeben,", "tokens": ["We\u00b7shalb", "ich", "ret\u00b7tungs\u00b7los", "dir", "preis\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Weshalb ich stracks wie angekettet blieb? \u2013", "tokens": ["We\u00b7shalb", "ich", "stracks", "wie", "an\u00b7ge\u00b7ket\u00b7tet", "blieb", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADV", "KOKOM", "VVPP", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Schweig und sei lieb!", "tokens": ["Schweig", "und", "sei", "lieb", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VAFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Von Wahnsinnsst\u00fcrmen ward mein Sinn umhallt,", "tokens": ["Von", "Wahn\u00b7sinns\u00b7st\u00fcr\u00b7men", "ward", "mein", "Sinn", "um\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Stolz erstarb, der sonst so siegesfrohe ...", "tokens": ["Mein", "Stolz", "er\u00b7starb", ",", "der", "sonst", "so", "sie\u00b7ges\u00b7fro\u00b7he", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADV", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Begreifst du die d\u00e4monische Gewalt,", "tokens": ["Be\u00b7greifst", "du", "die", "d\u00e4\u00b7mo\u00b7ni\u00b7sche", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mein Held? Begreifst du, welch emp\u00f6rte Lohe,", "tokens": ["Mein", "Held", "?", "Be\u00b7greifst", "du", ",", "welch", "em\u00b7p\u00f6r\u00b7te", "Lo\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df sie nicht sengend Herz und Hirn verzehre,", "tokens": ["Da\u00df", "sie", "nicht", "sen\u00b7gend", "Herz", "und", "Hirn", "ver\u00b7zeh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Mich dir mein Gl\u00fcck, mein Leben, meine Ehre,", "tokens": ["Mich", "dir", "mein", "Gl\u00fcck", ",", "mein", "Le\u00b7ben", ",", "mei\u00b7ne", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mich dir mein alles hinzugeben trieb? \u2013", "tokens": ["Mich", "dir", "mein", "al\u00b7les", "hin\u00b7zu\u00b7ge\u00b7ben", "trieb", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPER", "PPOSAT", "PIS", "VVIZU", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Schweig und sei lieb!", "tokens": ["Schweig", "und", "sei", "lieb", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VAFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}