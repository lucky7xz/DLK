{"textgrid.poem.39552": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es lebe Karl der Gro\u00dfe,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lebe Karl der Gro\u00dfe,", "tokens": ["Es", "le\u00b7be", "Karl", "der", "Gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein \u00e4chter deutscher Mann,", "tokens": ["Ein", "\u00e4ch\u00b7ter", "deut\u00b7scher", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder Deutsche sto\u00dfe", "tokens": ["Und", "je\u00b7der", "Deut\u00b7sche", "sto\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem Becher an!", "tokens": ["Mit", "sei\u00b7nem", "Be\u00b7cher", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er thronte dort in Achen", "tokens": ["Er", "thron\u00b7te", "dort", "in", "A\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem altber\u00fchmten Ort,", "tokens": ["Dem", "alt\u00b7be\u00b7r\u00fchm\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und V\u00f6lker vieler Sprachen", "tokens": ["Und", "V\u00f6l\u00b7ker", "vie\u00b7ler", "Spra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gehorchten seinem Wort.", "tokens": ["Ge\u00b7horch\u00b7ten", "sei\u00b7nem", "Wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es hat der gro\u00dfe Kaiser,", "tokens": ["Es", "hat", "der", "gro\u00b7\u00dfe", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trotz seinem langen Bart, \u2013", "tokens": ["Trotz", "sei\u00b7nem", "lan\u00b7gen", "Bart", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er war um desto weiser \u2013", "tokens": ["Er", "war", "um", "des\u00b7to", "wei\u00b7ser", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Ernst mit Lust gepaart.", "tokens": ["Den", "Ernst", "mit", "Lust", "ge\u00b7paart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er liebte warme Quellen,", "tokens": ["Er", "lieb\u00b7te", "war\u00b7me", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schwamm in manchem Teich;", "tokens": ["Und", "schwamm", "in", "man\u00b7chem", "Teich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An sch\u00f6nen Badestellen", "tokens": ["An", "sch\u00f6\u00b7nen", "Ba\u00b7de\u00b7stel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist Achen durch ihn reich.", "tokens": ["Ist", "A\u00b7chen", "durch", "ihn", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Den edeln Ingelheimer", "tokens": ["Den", "e\u00b7deln", "In\u00b7gel\u00b7hei\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zog er bei seinem Schlo\u00df,", "tokens": ["Zog", "er", "bei", "sei\u00b7nem", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wovon schon mancher Eimer", "tokens": ["Wo\u00b7von", "schon", "man\u00b7cher", "Ei\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Kehl' uns niederflo\u00df.", "tokens": ["Die", "Kehl'", "uns", "nie\u00b7der\u00b7flo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Am r\u00fcdesheimer Berge", "tokens": ["Am", "r\u00fc\u00b7des\u00b7hei\u00b7mer", "Ber\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat er den Wein gepflanzt,", "tokens": ["Hat", "er", "den", "Wein", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Nixen sonst und Zwerge", "tokens": ["Wo", "Ni\u00b7xen", "sonst", "und", "Zwer\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um Hattos Thurm getanzt.", "tokens": ["Um", "Hat\u00b7tos", "Thurm", "ge\u00b7tanzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn wir den Rheinwein trinken,", "tokens": ["Wenn", "wir", "den", "Rhein\u00b7wein", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So werde sein gedacht.", "tokens": ["So", "wer\u00b7de", "sein", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch die westph\u00e4ls-chen S-chinken", "tokens": ["Auch", "die", "west\u00b7ph\u00e4ls\u00b7chen", "S\u00b7\u00b7chin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hat er erst aufgebracht.", "tokens": ["Hat", "er", "erst", "auf\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er taufte ja die Sachsen,", "tokens": ["Er", "tauf\u00b7te", "ja", "die", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es war ein strenges Mu\u00df,", "tokens": ["Es", "war", "ein", "stren\u00b7ges", "Mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er zog sie bei den Fachsen", "tokens": ["Er", "zog", "sie", "bei", "den", "Fach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl in den Weser-Flu\u00df.", "tokens": ["Wohl", "in", "den", "We\u00b7ser\u00b7Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die heidnischen Westphalen", "tokens": ["Die", "heid\u00b7ni\u00b7schen", "West\u00b7pha\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die schlachteten nicht ein;", "tokens": ["Die", "schlach\u00b7te\u00b7ten", "nicht", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die M\u00f6nche drauf befahlen", "tokens": ["Die", "M\u00f6n\u00b7che", "drauf", "be\u00b7fah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein fett Sanct Mertens Schwein.", "tokens": ["Ein", "fett", "Sanct", "Mer\u00b7tens", "Schwein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dem heil'gen Mann zu Ehren", "tokens": ["Dem", "heil'\u00b7gen", "Mann", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hieng man sie in den Rauch,", "tokens": ["Hieng", "man", "sie", "in", "den", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So sah man sich vermehren", "tokens": ["So", "sah", "man", "sich", "ver\u00b7meh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den lobenswerthen Brauch.", "tokens": ["Den", "lo\u00b7bens\u00b7wert\u00b7hen", "Brauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es lebe Karl der Gro\u00dfe,", "tokens": ["Es", "le\u00b7be", "Karl", "der", "Gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein \u00e4chter deutscher Mann!", "tokens": ["Ein", "\u00e4ch\u00b7ter", "deut\u00b7scher", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder Deutsche sto\u00dfe", "tokens": ["Und", "je\u00b7der", "Deut\u00b7sche", "sto\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem Becher an!", "tokens": ["Mit", "sei\u00b7nem", "Be\u00b7cher", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}