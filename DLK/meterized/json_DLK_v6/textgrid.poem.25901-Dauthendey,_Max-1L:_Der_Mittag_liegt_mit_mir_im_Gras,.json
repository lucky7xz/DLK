{"textgrid.poem.25901": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Mittag liegt mit mir im Gras,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mittag liegt mit mir im Gras,", "tokens": ["Der", "Mit\u00b7tag", "liegt", "mit", "mir", "im", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wolken ziehn tiefblaue Stra\u00df,", "tokens": ["Die", "Wol\u00b7ken", "ziehn", "tief\u00b7blau\u00b7e", "Stra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt ist gr\u00fcn und wei\u00df und blau,", "tokens": ["Die", "Welt", "ist", "gr\u00fcn", "und", "wei\u00df", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu mir setzt sich die Herzensfrau.", "tokens": ["Zu", "mir", "setzt", "sich", "die", "Her\u00b7zens\u00b7frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "\u00bbrot,\u00ab spricht sie, \u00bbist die ganze Welt,", "tokens": ["\u00bb", "rot", ",", "\u00ab", "spricht", "sie", ",", "\u00bb", "ist", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn man zum Ku\u00df den Mund hinh\u00e4lt.\u00ab", "tokens": ["Wenn", "man", "zum", "Ku\u00df", "den", "Mund", "hin\u00b7h\u00e4lt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}