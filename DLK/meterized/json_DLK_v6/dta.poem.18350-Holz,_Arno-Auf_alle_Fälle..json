{"dta.poem.18350": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Auf alle F\u00e4lle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ob's wahr, je nun, das wei\u00df ich nicht:", "tokens": ["Ob's", "wahr", ",", "je", "nun", ",", "das", "wei\u00df", "ich", "nicht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADV", "ADV", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eder vielgesuchte Stein der Weisen", "tokens": ["\u201e", "der", "viel\u00b7ge\u00b7such\u00b7te", "Stein", "der", "Wei\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist ein Gemisch aus Blut und Eisen.\u201c", "tokens": ["Ist", "ein", "Ge\u00b7misch", "aus", "Blut", "und", "Ei\u00b7sen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar Standrecht giebt's und Festungsw\u00e4lle,", "tokens": ["Zwar", "Stand\u00b7recht", "giebt's", "und", "Fes\u00b7tungs\u00b7w\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch Eins bleibt wahr auf alle F\u00e4lle:", "tokens": ["Doch", "Eins", "bleibt", "wahr", "auf", "al\u00b7le", "F\u00e4l\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ob der Kanzler zehnmal spricht,", "tokens": ["Und", "ob", "der", "Kanz\u00b7ler", "zehn\u00b7mal", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein braver Kerl, der forcht sich nicht!", "tokens": ["Ein", "bra\u00b7ver", "Kerl", ",", "der", "forcht", "sich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}