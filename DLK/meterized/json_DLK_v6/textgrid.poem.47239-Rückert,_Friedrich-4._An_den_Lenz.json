{"textgrid.poem.47239": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4. An den Lenz", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schm\u00fccke doch, du Hand des Lenzen,", "tokens": ["Schm\u00fc\u00b7cke", "doch", ",", "du", "Hand", "des", "Len\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schm\u00fccke diese Fluren doch,", "tokens": ["Schm\u00fc\u00b7cke", "die\u00b7se", "Flu\u00b7ren", "doch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich sie zuletzt ergl\u00e4nzen", "tokens": ["Da\u00df", "ich", "sie", "zu\u00b7letzt", "er\u00b7gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh' in vollem Glanze noch.", "tokens": ["Seh'", "in", "vol\u00b7lem", "Glan\u00b7ze", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df, wenn ich einst einsam weine,", "tokens": ["Da\u00df", ",", "wenn", "ich", "einst", "ein\u00b7sam", "wei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Ferne dein Gefild'", "tokens": ["Aus", "der", "Fer\u00b7ne", "dein", "Ge\u00b7fild'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00f6stlich l\u00e4chelnd mir erscheine,", "tokens": ["Tr\u00f6st\u00b7lich", "l\u00e4\u00b7chelnd", "mir", "er\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht ein starrend Winterbild.", "tokens": ["Nicht", "ein", "star\u00b7rend", "Win\u00b7ter\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}