{"textgrid.poem.60382": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An eine Witwe", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach einem heftigen Gewitter", "tokens": ["Nach", "ei\u00b7nem", "hef\u00b7ti\u00b7gen", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wandl ich allein im tiefen Haine", "tokens": ["Wandl", "ich", "al\u00b7lein", "im", "tie\u00b7fen", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und blicke durch das nasse Gitter", "tokens": ["Und", "bli\u00b7cke", "durch", "das", "nas\u00b7se", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bl\u00e4tter auf zum Sternenscheine.", "tokens": ["Der", "Bl\u00e4t\u00b7ter", "auf", "zum", "Ster\u00b7nen\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die sturmesm\u00fcden B\u00e4ume schweigen;", "tokens": ["Die", "stur\u00b7mes\u00b7m\u00fc\u00b7den", "B\u00e4u\u00b7me", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur manchmal rauschen Windeshauche,", "tokens": ["Nur", "manch\u00b7mal", "rau\u00b7schen", "Win\u00b7des\u00b7hau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie eine Mahnung, in den Zweigen,", "tokens": ["Wie", "ei\u00b7ne", "Mah\u00b7nung", ",", "in", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann tropft es nach im dunkeln Strauche.", "tokens": ["Dann", "tropft", "es", "nach", "im", "dun\u00b7keln", "Strau\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So fand ich nach den Schmerzgewittern", "tokens": ["So", "fand", "ich", "nach", "den", "Schmerz\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich m\u00fcd versenkt im stillen Grame;", "tokens": ["Dich", "m\u00fcd", "ver\u00b7senkt", "im", "stil\u00b7len", "Gra\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sah ich deine Tr\u00e4nen zittern,", "tokens": ["Doch", "sah", "ich", "dei\u00b7ne", "Tr\u00e4\u00b7nen", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn dir erklang sein teurer Name.", "tokens": ["Wenn", "dir", "er\u00b7klang", "sein", "teu\u00b7rer", "Na\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Fr\u00fchling kam, vor seinem Strahle", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kam", ",", "vor", "sei\u00b7nem", "Strah\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Suchst du des Schmerzes traute Schatten", "tokens": ["Suchst", "du", "des", "Schmer\u00b7zes", "trau\u00b7te", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchrest nach dem fernen Tale", "tokens": ["Und", "f\u00fch\u00b7rest", "nach", "dem", "fer\u00b7nen", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kinder an das Grab des Gatten.", "tokens": ["Die", "Kin\u00b7der", "an", "das", "Grab", "des", "Gat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du wanderst mit den Vaterlosen,", "tokens": ["Du", "wan\u00b7derst", "mit", "den", "Va\u00b7ter\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Tr\u00e4nen neu das Grab zu tr\u00e4nken,", "tokens": ["Mit", "Tr\u00e4\u00b7nen", "neu", "das", "Grab", "zu", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf das du deiner Wangen Rosen", "tokens": ["Auf", "das", "du", "dei\u00b7ner", "Wan\u00b7gen", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gestreut zum treuen Angedenken.", "tokens": ["Ge\u00b7streut", "zum", "treu\u00b7en", "An\u00b7ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O bring zum Grabe deines Lieben", "tokens": ["O", "bring", "zum", "Gra\u00b7be", "dei\u00b7nes", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von mir auch einen Gru\u00df und sage,", "tokens": ["Von", "mir", "auch", "ei\u00b7nen", "Gru\u00df", "und", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df auch mein Herz ihm treu geblieben,", "tokens": ["Da\u00df", "auch", "mein", "Herz", "ihm", "treu", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bring ihm des Jugendfreundes Klage.", "tokens": ["Bring", "ihm", "des", "Ju\u00b7gend\u00b7freun\u00b7des", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn aus dem Aug dir Tr\u00e4nen brechen,", "tokens": ["Wenn", "aus", "dem", "Aug", "dir", "Tr\u00e4\u00b7nen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "M\u00f6cht ich am Grabe dich begr\u00fc\u00dfen,", "tokens": ["M\u00f6cht", "ich", "am", "Gra\u00b7be", "dich", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dir von seiner Jugend sprechen", "tokens": ["Mit", "dir", "von", "sei\u00b7ner", "Ju\u00b7gend", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und m\u00f6chte seine Kinder k\u00fcssen.", "tokens": ["Und", "m\u00f6ch\u00b7te", "sei\u00b7ne", "Kin\u00b7der", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}