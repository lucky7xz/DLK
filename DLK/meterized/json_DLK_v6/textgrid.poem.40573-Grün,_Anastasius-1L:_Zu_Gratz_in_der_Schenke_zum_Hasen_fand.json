{"textgrid.poem.40573": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu Gratz in der Schenke zum Hasen fand", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Gratz in der Schenke zum Hasen fand", "tokens": ["Zu", "Gratz", "in", "der", "Schen\u00b7ke", "zum", "Ha\u00b7sen", "fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sonst frohe Gesellschaft sich ein,", "tokens": ["Sonst", "fro\u00b7he", "Ge\u00b7sell\u00b7schaft", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der Wirth war das lustigste M\u00e4nnlein im Land", "tokens": ["Der", "Wirth", "war", "das", "lus\u00b7tigs\u00b7te", "M\u00e4nn\u00b7lein", "im", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und schenkte den herrlichsten Wein.", "tokens": ["Und", "schenk\u00b7te", "den", "herr\u00b7lichs\u00b7ten", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Still ist's und leer nun, kein Trank und Schwank!", "tokens": ["Still", "ist's", "und", "leer", "nun", ",", "kein", "Trank", "und", "Schwank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "ADV", "$,", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Wirthe verging der Scherz.", "tokens": ["Dem", "Wirt\u00b7he", "ver\u00b7ging", "der", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es liegt ihm zu Hause die Gattin krank", "tokens": ["Es", "liegt", "ihm", "zu", "Hau\u00b7se", "die", "Gat\u00b7tin", "krank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und wimmert im Mutterschmerz.", "tokens": ["Und", "wim\u00b7mert", "im", "Mut\u00b7ter\u00b7schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Er steht am Bette tr\u00f6stend und hebt", "tokens": ["Er", "steht", "am", "Bet\u00b7te", "tr\u00f6s\u00b7tend", "und", "hebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die H\u00e4nde zum Himmel und spricht:", "tokens": ["Die", "H\u00e4n\u00b7de", "zum", "Him\u00b7mel", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u00bbo Mutter de\u00df, der in Ewigkeit lebt,", "tokens": ["\u00bb", "o", "Mut\u00b7ter", "de\u00df", ",", "der", "in", "E\u00b7wig\u00b7keit", "lebt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "ART", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verlasse die Dulderin nicht!", "tokens": ["Ver\u00b7las\u00b7se", "die", "Dul\u00b7de\u00b7rin", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und wenn das Kind, das am Arm ihr einst winkt,", "tokens": ["Und", "wenn", "das", "Kind", ",", "das", "am", "Arm", "ihr", "einst", "winkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kann heben den ersten Stein,", "tokens": ["Kann", "he\u00b7ben", "den", "ers\u00b7ten", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Am Ort, wo der Stein aus der Hand ihm sinkt,", "tokens": ["Am", "Ort", ",", "wo", "der", "Stein", "aus", "der", "Hand", "ihm", "sinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dort will ich ein Kirchlein dir weihn!\u00ab \u2013", "tokens": ["Dort", "will", "ich", "ein", "Kirch\u00b7lein", "dir", "weihn", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$.", "$(", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Einst wallt durch die Flur, die wieder ergr\u00fcnt,", "tokens": ["Einst", "wallt", "durch", "die", "Flur", ",", "die", "wie\u00b7der", "er\u00b7gr\u00fcnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Wirth und sein holdes Weib,", "tokens": ["Der", "Wirth", "und", "sein", "hol\u00b7des", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zur Seite t\u00e4ndelt ein liebliches Kind,", "tokens": ["Zur", "Sei\u00b7te", "t\u00e4n\u00b7delt", "ein", "lieb\u00b7li\u00b7ches", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Geschmiegt an der Mutter Leib.", "tokens": ["Ge\u00b7schmiegt", "an", "der", "Mut\u00b7ter", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Das hebt dort am Bach ein Steinchen auf,", "tokens": ["Das", "hebt", "dort", "am", "Bach", "ein", "Stein\u00b7chen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und tr\u00e4gt's wohl weit noch und lang;", "tokens": ["Und", "tr\u00e4gt's", "wohl", "weit", "noch", "und", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hinunter durch Th\u00e4ler, zu H\u00fcgeln hinauf", "tokens": ["Hin\u00b7un\u00b7ter", "durch", "Th\u00e4\u00b7ler", ",", "zu", "H\u00fc\u00b7geln", "hin\u00b7auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "APPR", "NN", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Geht wechselnd der Wandelnden Gang.", "tokens": ["Geht", "wech\u00b7selnd", "der", "Wan\u00b7deln\u00b7den", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Bis tief in ein Thal, vom Wald umkr\u00f6nt,", "tokens": ["Bis", "tief", "in", "ein", "Thal", ",", "vom", "Wald", "um\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da l\u00e4\u00dft es nicht weiter sie gehn;", "tokens": ["Da", "l\u00e4\u00dft", "es", "nicht", "wei\u00b7ter", "sie", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "PPER", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ein Ruf in den L\u00fcften und Herzen ert\u00f6nt,", "tokens": ["Ein", "Ruf", "in", "den", "L\u00fcf\u00b7ten", "und", "Her\u00b7zen", "er\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gebietend, hier stille zu stehn!", "tokens": ["Ge\u00b7bie\u00b7tend", ",", "hier", "stil\u00b7le", "zu", "stehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Ein Ruf aus rauschendem F\u00f6hrenlaub,", "tokens": ["Ein", "Ruf", "aus", "rau\u00b7schen\u00b7dem", "F\u00f6h\u00b7ren\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus Wellen, die pl\u00e4tschernd ziehn,", "tokens": ["Aus", "Wel\u00b7len", ",", "die", "pl\u00e4t\u00b7schernd", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus Blumen und wehendem Bl\u00fcthenstaub,", "tokens": ["Aus", "Blu\u00b7men", "und", "we\u00b7hen\u00b7dem", "Bl\u00fc\u00b7then\u00b7staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Aus Halmen und Wiesengr\u00fcn!", "tokens": ["Aus", "Hal\u00b7men", "und", "Wie\u00b7sen\u00b7gr\u00fcn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Ein Ruf, der auf Strahlen des Lichtes heran", "tokens": ["Ein", "Ruf", ",", "der", "auf", "Strah\u00b7len", "des", "Lich\u00b7tes", "he\u00b7ran"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "ART", "NN", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und tief in die Herzen f\u00e4hrt,", "tokens": ["Und", "tief", "in", "die", "Her\u00b7zen", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wieder als Dank und Jubel hinan", "tokens": ["Und", "wie\u00b7der", "als", "Dank", "und", "Ju\u00b7bel", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "NN", "KON", "NN", "PTKVZ"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zur strahlenden Heimat kehrt!", "tokens": ["Zur", "strah\u00b7len\u00b7den", "Hei\u00b7mat", "kehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Und wie das Kind die Eltern ersah", "tokens": ["Und", "wie", "das", "Kind", "die", "El\u00b7tern", "er\u00b7sah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hinknieend mit betendem Mund,", "tokens": ["Hin\u00b7kni\u00b7e\u00b7end", "mit", "be\u00b7ten\u00b7dem", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ausspannt es die Arme zum Himmel da,", "tokens": ["Aus\u00b7spannt", "es", "die", "Ar\u00b7me", "zum", "Him\u00b7mel", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Stein \u2013 entsank ihm zum Grund.", "tokens": ["Der", "Stein", "\u2013", "ent\u00b7sank", "ihm", "zum", "Grund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Wohl sieht man zur Stelle ein Kirchlein stehn,", "tokens": ["Wohl", "sieht", "man", "zur", "Stel\u00b7le", "ein", "Kirch\u00b7lein", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Man nennt es Maria Gr\u00fcn,", "tokens": ["Man", "nennt", "es", "Ma\u00b7ria", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch sieht man das Thal so wundersch\u00f6n,", "tokens": ["Noch", "sieht", "man", "das", "Thal", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So gr\u00fcnend und duftend bl\u00fchn. \u2013", "tokens": ["So", "gr\u00fc\u00b7nend", "und", "duf\u00b7tend", "bl\u00fchn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVINF", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Das hat zu Mariens und Gottes Ehr'", "tokens": ["Das", "hat", "zu", "Ma\u00b7ri\u00b7ens", "und", "Got\u00b7tes", "Ehr'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NE", "KON", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor Jahren ein Wirth gethan;", "tokens": ["Vor", "Jah\u00b7ren", "ein", "Wirth", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Enkel doch bauten \u2013 dem Wirth wohl zur Ehr'? \u2013", "tokens": ["Die", "En\u00b7kel", "doch", "bau\u00b7ten", "\u2013", "dem", "Wirth", "wohl", "zur", "Ehr'", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$(", "ART", "NN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Vorl\u00e4ngst eine Schenke daran!", "tokens": ["Vor\u00b7l\u00e4ngst", "ei\u00b7ne", "Schen\u00b7ke", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PAV", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "So mische sich Jauchzen und Becherklang", "tokens": ["So", "mi\u00b7sche", "sich", "Jauch\u00b7zen", "und", "Be\u00b7cher\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit Psalmen und Glockengel\u00e4ut!", "tokens": ["Mit", "Psal\u00b7men", "und", "Glo\u00b7cken\u00b7ge\u00b7l\u00e4ut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "So tanze der schwarze Me\u00dfner entlang", "tokens": ["So", "tan\u00b7ze", "der", "schwar\u00b7ze", "Me\u00df\u00b7ner", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPO"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit rosiger Kellnerin heut!", "tokens": ["Mit", "ro\u00b7si\u00b7ger", "Kell\u00b7ne\u00b7rin", "heut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}