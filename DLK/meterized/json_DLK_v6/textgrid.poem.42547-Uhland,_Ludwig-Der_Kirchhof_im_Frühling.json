{"textgrid.poem.42547": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Kirchhof im Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stiller Garten, eile nur,", "tokens": ["Stil\u00b7ler", "Gar\u00b7ten", ",", "ei\u00b7le", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich mit jungem Gr\u00fcn zu decken,", "tokens": ["Dich", "mit", "jun\u00b7gem", "Gr\u00fcn", "zu", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Bodens letzte Spur", "tokens": ["Und", "des", "Bo\u00b7dens", "letz\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Birg mit dichten Rosenhecken!", "tokens": ["Birg", "mit", "dich\u00b7ten", "Ro\u00b7sen\u00b7he\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schlie\u00dfe fest den schwarzen Grund!", "tokens": ["Schlie\u00b7\u00dfe", "fest", "den", "schwar\u00b7zen", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn sein Anblick macht mir bange,", "tokens": ["Denn", "sein", "An\u00b7blick", "macht", "mir", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob er keines aus dem Bund", "tokens": ["Ob", "er", "kei\u00b7nes", "aus", "dem", "Bund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Liebsten abverlange.", "tokens": ["Mei\u00b7ner", "Liebs\u00b7ten", "ab\u00b7ver\u00b7lan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Will mich selbst die dumpfe Gruft,", "tokens": ["Will", "mich", "selbst", "die", "dump\u00b7fe", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun wohlan, sie mag mich raffen!", "tokens": ["Nun", "wo\u00b7hlan", ",", "sie", "mag", "mich", "raf\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcnkt mir gleich, in frischer Luft", "tokens": ["D\u00fcnkt", "mir", "gleich", ",", "in", "fri\u00b7scher", "Luft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tt ich manches noch zu schaffen.", "tokens": ["H\u00e4tt", "ich", "man\u00b7ches", "noch", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}