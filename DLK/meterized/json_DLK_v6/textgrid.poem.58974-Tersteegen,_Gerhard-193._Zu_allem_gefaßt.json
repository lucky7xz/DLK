{"textgrid.poem.58974": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "193. Zu allem gefa\u00dft", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Wille sei stets \u00fcbergeben", "tokens": ["Dein", "Wil\u00b7le", "sei", "stets", "\u00fc\u00b7ber\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu allen Proben, die Gott schickt,", "tokens": ["Zu", "al\u00b7len", "Pro\u00b7ben", ",", "die", "Gott", "schickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.3": {"text": "In einem blo\u00dfen Gottankleben,", "tokens": ["In", "ei\u00b7nem", "blo\u00b7\u00dfen", "Got\u00b7tan\u00b7kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So find't der Sturm dich unverr\u00fcckt.", "tokens": ["So", "find't", "der", "Sturm", "dich", "un\u00b7ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}