{"textgrid.poem.40328": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Mamma", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber nicht wieder! Nein, nie wieder!", "tokens": ["A\u00b7ber", "nicht", "wie\u00b7der", "!", "Nein", ",", "nie", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "$.", "PTKANT", "$,", "ADV", "ADV", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ja, du wolltest mich begl\u00fccken:", "tokens": ["Ja", ",", "du", "woll\u00b7test", "mich", "be\u00b7gl\u00fc\u00b7cken", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wie sie an dein Fleisch sich dr\u00fccken,", "tokens": ["wie", "sie", "an", "dein", "Fleisch", "sich", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "diese hilflos kleinen Glieder.", "tokens": ["die\u00b7se", "hil\u00b7flos", "klei\u00b7nen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber mir diese Lust beschauen,", "tokens": ["A\u00b7ber", "mir", "die\u00b7se", "Lust", "be\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "ist mir ein Grauen.", "tokens": ["ist", "mir", "ein", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Zu tief sah ich unsrer zahmen Katze", "tokens": ["Zu", "tief", "sah", "ich", "uns\u00b7rer", "zah\u00b7men", "Kat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "in die m\u00fctterlichen Augen,", "tokens": ["in", "die", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sah die t\u00e4ppischen Jungen saugen", "tokens": ["sah", "die", "t\u00e4p\u00b7pi\u00b7schen", "Jun\u00b7gen", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "unter der steifgez\u00fcckten Tatze;", "tokens": ["un\u00b7ter", "der", "steif\u00b7ge\u00b7z\u00fcck\u00b7ten", "Tat\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "und der zarten blinden Brut", "tokens": ["und", "der", "zar\u00b7ten", "blin\u00b7den", "Brut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "schmeckte das alte Raubtier gut.", "tokens": ["schmeck\u00b7te", "das", "al\u00b7te", "Raub\u00b7tier", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Decke die Brust zu, wenn die Lippen", "tokens": ["De\u00b7cke", "die", "Brust", "zu", ",", "wenn", "die", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "KOUS", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "deines Sohnes dich ber\u00fchren!", "tokens": ["dei\u00b7nes", "Soh\u00b7nes", "dich", "be\u00b7r\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "la\u00df ihn andere Wonnen sp\u00fcren", "tokens": ["la\u00df", "ihn", "an\u00b7de\u00b7re", "Won\u00b7nen", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "als den Blick der Ahnen und der Sippen!", "tokens": ["als", "den", "Blick", "der", "Ah\u00b7nen", "und", "der", "Sip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nein, ich wollte dich nicht betr\u00fcben;", "tokens": ["Nein", ",", "ich", "woll\u00b7te", "dich", "nicht", "be\u00b7tr\u00fc\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "nur \u2013 nur anders la\u00df uns lieben!", "tokens": ["nur", "\u2013", "nur", "an\u00b7ders", "la\u00df", "uns", "lie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bebt'ich doch selber, als ich ihn k\u00fc\u00dfte,", "tokens": ["Bebt'\u00b7ich", "doch", "sel\u00b7ber", ",", "als", "ich", "ihn", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "und ich will die Wonnen der Ammen", "tokens": ["und", "ich", "will", "die", "Won\u00b7nen", "der", "Am\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "nicht verdammen;", "tokens": ["nicht", "ver\u00b7dam\u00b7men", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "dunkel ist der Zweck der L\u00fcste.", "tokens": ["dun\u00b7kel", "ist", "der", "Zweck", "der", "L\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber die M\u00fctter \u2013 nein, schweigen wir!", "tokens": ["A\u00b7ber", "die", "M\u00fct\u00b7ter", "\u2013", "nein", ",", "schwei\u00b7gen", "wir", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "PTKANT", "$,", "VVFIN", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "wehe, der Mensch ist ein S\u00e4ugetier.", "tokens": ["we\u00b7he", ",", "der", "Mensch", "ist", "ein", "S\u00e4u\u00b7ge\u00b7tier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.6": {"line.1": {"text": "Einsamer als je begann ich", "tokens": ["Ein\u00b7sa\u00b7mer", "als", "je", "be\u00b7gann", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meine Seele zu belauern.", "tokens": ["mei\u00b7ne", "See\u00b7le", "zu", "be\u00b7lau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wozu sehnte, wozu sann ich?", "tokens": ["Wo\u00b7zu", "sehn\u00b7te", ",", "wo\u00b7zu", "sann", "ich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur um unsern Wollustschauern", "tokens": ["Nur", "um", "un\u00b7sern", "Wol\u00b7lust\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "heilige Masken vorzustecken?", "tokens": ["hei\u00b7li\u00b7ge", "Mas\u00b7ken", "vor\u00b7zu\u00b7ste\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "War dann nicht im Hochzeitskleide", "tokens": ["War", "dann", "nicht", "im", "Hoch\u00b7zeits\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "das Getier der Fr\u00fchlingshecken", "tokens": ["das", "Ge\u00b7tier", "der", "Fr\u00fch\u00b7lings\u00b7he\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "gottbegnadeter als wir beide?", "tokens": ["gott\u00b7be\u00b7gna\u00b7de\u00b7ter", "als", "wir", "bei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPER", "PIS", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Welch ein Jubel der Erh\u00f6rung,", "tokens": ["Welch", "ein", "Ju\u00b7bel", "der", "Er\u00b7h\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dies Geschwirr, Gegirr, Geraune!", "tokens": ["dies", "Ge\u00b7schwirr", ",", "Ge\u00b7girr", ",", "Ge\u00b7rau\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit Best\u00fcrzung, mit Emp\u00f6rung", "tokens": ["Mit", "Be\u00b7st\u00fcr\u00b7zung", ",", "mit", "Em\u00b7p\u00f6\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "lernt ich Deine Macht anstaunen,", "tokens": ["lernt", "ich", "Dei\u00b7ne", "Macht", "an\u00b7stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}