{"textgrid.poem.35641": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4. Auf Michaelis' Tod", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchbraust des Herbstthals \u00d6de gewaltiger,", "tokens": ["Durch\u00b7braust", "des", "Herbstt\u00b7hals", "\u00d6\u00b7de", "ge\u00b7wal\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr k\u00e4ltern Wind'! Und die du mit schwarzem Schau'r", "tokens": ["Ihr", "k\u00e4l\u00b7tern", "Wind'", "!", "Und", "die", "du", "mit", "schwar\u00b7zem", "Schau'r"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "KON", "ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mich \u00fcberdeckst, o Espe, s\u00e4usle", "tokens": ["Mich", "\u00fc\u00b7berd\u00b7eckst", ",", "o", "Es\u00b7pe", ",", "s\u00e4us\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lauter herab mit verwehtem Laube!", "tokens": ["Lau\u00b7ter", "her\u00b7ab", "mit", "ver\u00b7weh\u00b7tem", "Lau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Entsteigt den Gr\u00e4bern, schlummernde Geister, schwebt,", "tokens": ["Ent\u00b7steigt", "den", "Gr\u00e4\u00b7bern", ",", "schlum\u00b7mern\u00b7de", "Geis\u00b7ter", ",", "schwebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In wei\u00dfer H\u00fclle, wimmernd und totenbleich", "tokens": ["In", "wei\u00b7\u00dfer", "H\u00fcl\u00b7le", ",", "wim\u00b7mernd", "und", "to\u00b7ten\u00b7bleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VVPP", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Um meine Klagen, bis der Fr\u00fche", "tokens": ["Um", "mei\u00b7ne", "Kla\u00b7gen", ",", "bis", "der", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "D\u00e4mmerung euch zu der Gruft zur\u00fcckscheucht!", "tokens": ["D\u00e4m\u00b7me\u00b7rung", "euch", "zu", "der", "Gruft", "zu\u00b7r\u00fcck\u00b7scheucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-----+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Ach Michaelis! \u2013 St\u00fcrze die Wang' hinab,", "tokens": ["Ach", "Mic\u00b7ha\u00b7e\u00b7lis", "!", "\u2013", "St\u00fcr\u00b7ze", "die", "Wang'", "hin\u00b7ab", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "$(", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du Thr\u00e4nenstrom! \u2013 Im Schweigen des Grabs auch du,", "tokens": ["Du", "Thr\u00e4\u00b7nen\u00b7strom", "!", "\u2013", "Im", "Schwei\u00b7gen", "des", "Grabs", "auch", "du", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "$(", "APPRART", "NN", "ART", "NN", "ADV", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mein Michaelis? \u2013 Patrioten", "tokens": ["Mein", "Mic\u00b7ha\u00b7e\u00b7lis", "?", "\u2013", "Pat\u00b7ri\u00b7o\u00b7ten"], "token_info": ["word", "word", "punct", "punct", "word"], "pos": ["PPOSAT", "NE", "$.", "$(", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weinen, und Boie zerflie\u00dft in Thr\u00e4nen.", "tokens": ["Wei\u00b7nen", ",", "und", "Boi\u00b7e", "zer\u00b7flie\u00dft", "in", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ha! Tod, du schwarzer M\u00f6rder! Wie viele traf", "tokens": ["Ha", "!", "Tod", ",", "du", "schwar\u00b7zer", "M\u00f6r\u00b7der", "!", "Wie", "vie\u00b7le", "traf"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "NN", "$,", "PPER", "ADJA", "NN", "$.", "PWAV", "PIS", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dein Schlachtschwert schon der bl\u00fchendsten Hoffnungen", "tokens": ["Dein", "Schlacht\u00b7schwert", "schon", "der", "bl\u00fc\u00b7hends\u00b7ten", "Hoff\u00b7nun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im deutschen Eichenhain! Auch diesen", "tokens": ["Im", "deut\u00b7schen", "Ei\u00b7chen\u00b7hain", "!", "Auch", "die\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "ADV", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fcrgtest du, welcher begann, und gro\u00df war?", "tokens": ["W\u00fcrg\u00b7test", "du", ",", "wel\u00b7cher", "be\u00b7gann", ",", "und", "gro\u00df", "war", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "VVFIN", "$,", "KON", "ADJD", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Mein armes Deutschland! Der, mit geweihtem Spiel,", "tokens": ["Mein", "ar\u00b7mes", "Deutschland", "!", "Der", ",", "mit", "ge\u00b7weih\u00b7tem", "Spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ART", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Empfindung deinen T\u00f6chtern und Tugend sang,", "tokens": ["Emp\u00b7fin\u00b7dung", "dei\u00b7nen", "T\u00f6ch\u00b7tern", "und", "Tu\u00b7gend", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der nie mit Gift den Strom des Wohlklangs", "tokens": ["Der", "nie", "mit", "Gift", "den", "Strom", "des", "Wohl\u00b7klangs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00fcckisch vermischte \u2013 der stirbt, ein J\u00fcngling!", "tokens": ["T\u00fc\u00b7ckisch", "ver\u00b7mischte", "\u2013", "der", "stirbt", ",", "ein", "J\u00fcng\u00b7ling", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "ART", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Der, mit dem heitern L\u00e4cheln des Phrygiers,", "tokens": ["Der", ",", "mit", "dem", "hei\u00b7tern", "L\u00e4\u00b7cheln", "des", "Phry\u00b7giers", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+++", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ein Sittenlehrer horchender Knaben war,", "tokens": ["Ein", "Sit\u00b7ten\u00b7leh\u00b7rer", "hor\u00b7chen\u00b7der", "Kna\u00b7ben", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der k\u00fchn den Frevel mit gehobner", "tokens": ["Der", "k\u00fchn", "den", "Fre\u00b7vel", "mit", "ge\u00b7hob\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gei\u00dfel verfolgte \u2013 der stirbt, ein J\u00fcngling! \u2013", "tokens": ["Gei\u00b7\u00dfel", "ver\u00b7folg\u00b7te", "\u2013", "der", "stirbt", ",", "ein", "J\u00fcng\u00b7ling", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$(", "ART", "VVFIN", "$,", "ART", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Doch unwert dieses J\u00fcnglinges warst du, Land,", "tokens": ["Doch", "un\u00b7wert", "die\u00b7ses", "J\u00fcng\u00b7lin\u00b7ges", "warst", "du", ",", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "PDAT", "NN", "VAFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das seines Volkes Ehre verkennt, voll Durst", "tokens": ["Das", "sei\u00b7nes", "Vol\u00b7kes", "Eh\u00b7re", "ver\u00b7kennt", ",", "voll", "Durst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "NN", "VVFIN", "$,", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nach Arouets Geklingel lechzet,", "tokens": ["Nach", "Ar\u00b7ou\u00b7ets", "Ge\u00b7klin\u00b7gel", "lech\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Daniens K\u00f6nigen Klopstocks Lied g\u00f6nnt.", "tokens": ["Da\u00b7ni\u00b7ens", "K\u00f6\u00b7ni\u00b7gen", "Klops\u00b7tocks", "Lied", "g\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Drum trockne, Boie, trockne die Thr\u00e4nen ab,", "tokens": ["Drum", "trock\u00b7ne", ",", "Boi\u00b7e", ",", "trock\u00b7ne", "die", "Thr\u00e4\u00b7nen", "ab", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "$,", "NE", "$,", "ADJA", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn unwert dieses J\u00fcnglinges war dies Land!", "tokens": ["Denn", "un\u00b7wert", "die\u00b7ses", "J\u00fcng\u00b7lin\u00b7ges", "war", "dies", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PDAT", "NN", "VAFIN", "PDS", "NN", "$."], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Ihn rief zur Sternenburg Jehovah,", "tokens": ["Ihn", "rief", "zur", "Ster\u00b7nen\u00b7burg", "Je\u00b7ho\u00b7vah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort in der Strahlenden Chor zu jauchzen!", "tokens": ["Dort", "in", "der", "Strah\u00b7len\u00b7den", "Chor", "zu", "jauch\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}}}}