{"dta.poem.13741": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Symmetrie .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "O Gegenwart, wie bist du schnelle,", "tokens": ["O", "Ge\u00b7gen\u00b7wart", ",", "wie", "bist", "du", "schnel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zukunft, wie bist du morgenhelle,", "tokens": ["Zu\u00b7kunft", ",", "wie", "bist", "du", "mor\u00b7gen\u00b7hel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vergangenheit so abendroth!", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", "so", "a\u00b7ben\u00b7droth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Abendroth soll ewig stehen,", "tokens": ["Das", "A\u00b7ben\u00b7droth", "soll", "e\u00b7wig", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Morgenhelle frisch drein wehen,", "tokens": ["Die", "Mor\u00b7gen\u00b7hel\u00b7le", "frisch", "drein", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So ist die Gegenwart nicht todt.", "tokens": ["