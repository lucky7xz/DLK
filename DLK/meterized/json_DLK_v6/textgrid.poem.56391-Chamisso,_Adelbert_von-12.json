{"textgrid.poem.56391": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "12", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie gingen zu fragen", "tokens": ["Sie", "gin\u00b7gen", "zu", "fra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Fraya, die herrliche,", "tokens": ["Fray\u00b7a", ",", "die", "herr\u00b7li\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und es war Thors Wort,", "tokens": ["Und", "es", "war", "Thors", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Welches zuerst er sprach:", "tokens": ["Wel\u00b7ches", "zu\u00b7erst", "er", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "\u00bbbr\u00e4utliches Leinen", "tokens": ["\u00bb", "br\u00e4ut\u00b7li\u00b7ches", "Lei\u00b7nen"], "token_info": ["punct", "word", "word"], "pos": ["$(", "ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Lege dir an, Fraya,", "tokens": ["Le\u00b7ge", "dir", "an", ",", "Fray\u00b7a", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wir beide wir reisen", "tokens": ["Wir", "bei\u00b7de", "wir", "rei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Nach Riesenheim.\u00ab", "tokens": ["Nach", "Rie\u00b7sen\u00b7heim", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["APPR", "NE", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}