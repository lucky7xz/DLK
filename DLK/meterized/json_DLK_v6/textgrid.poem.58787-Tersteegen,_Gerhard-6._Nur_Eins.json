{"textgrid.poem.58787": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "6. Nur Eins", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur Eins ist not; wer mehr will haben,", "tokens": ["Nur", "Eins", "ist", "not", ";", "wer", "mehr", "will", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ADJD", "$.", "PWS", "ADV", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den kann nicht Welt noch Himmel laben.", "tokens": ["Den", "kann", "nicht", "Welt", "noch", "Him\u00b7mel", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PTKNEG", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kehr aus der Vielheit dich ins Eine,", "tokens": ["Kehr", "aus", "der", "Viel\u00b7heit", "dich", "ins", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So hast du gnug an Gott alleine!", "tokens": ["So", "hast", "du", "gnug", "an", "Gott", "al\u00b7lei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}