{"dta.poem.14592": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und ich wandre aus den Mauern", "tokens": ["Und", "ich", "wand\u00b7re", "aus", "den", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis hinaus in's freie Feld,", "tokens": ["Bis", "hin\u00b7aus", "in's", "frei\u00b7e", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APZR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hehres Gl\u00e4nzen, heil'ges Schauen!", "tokens": ["Heh\u00b7res", "Gl\u00e4n\u00b7zen", ",", "heil'\u00b7ges", "Schau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie so weit und still die Welt!", "tokens": ["Wie", "so", "weit", "und", "still", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}