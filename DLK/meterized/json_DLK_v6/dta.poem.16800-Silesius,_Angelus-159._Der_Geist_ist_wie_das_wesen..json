{"dta.poem.16800": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "159.  Der Geist ist wie das wesen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Seist ist wie ein seyn: er ahnt dem wesen nach/", "tokens": ["Mein", "Seist", "ist", "wie", "ein", "seyn", ":", "er", "ahnt", "dem", "we\u00b7sen", "nach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "VAINF", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem er urgestand/ und Anfangs aufgebrach.", "tokens": ["Von", "dem", "er", "ur\u00b7ge\u00b7stand", "/", "und", "An\u00b7fangs", "auf\u00b7ge\u00b7brach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NE", "$(", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}