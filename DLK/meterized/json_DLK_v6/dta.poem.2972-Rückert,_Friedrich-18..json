{"dta.poem.2972": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein alt bauf\u00e4llig Haus kann man durch Pfeiler st\u00fctzen,", "tokens": ["Ein", "alt", "bau\u00b7f\u00e4l\u00b7lig", "Haus", "kann", "man", "durch", "Pfei\u00b7ler", "st\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "VMFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Durch Balkenwerk, das wird noch eine Zeitlang n\u00fctzen.", "tokens": ["Durch", "Bal\u00b7ken\u00b7werk", ",", "das", "wird", "noch", "ei\u00b7ne", "Zeit\u00b7lang", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PDS", "VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Am Ende f\u00e4llt es doch mit allen seinen Kr\u00fccken,", "tokens": ["Am", "En\u00b7de", "f\u00e4llt", "es", "doch", "mit", "al\u00b7len", "sei\u00b7nen", "Kr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und diese helfen es zu Boden selber dr\u00fccken.", "tokens": ["Und", "die\u00b7se", "hel\u00b7fen", "es", "zu", "Bo\u00b7den", "sel\u00b7ber", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und desto gr\u00f6\u00dfer wird der Tr\u00fcmmerfall dann seyn;", "tokens": ["Und", "des\u00b7to", "gr\u00f6\u00b7\u00dfer", "wird", "der", "Tr\u00fcm\u00b7mer\u00b7fall", "dann", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Niemand rei\u00dft, was er mit M\u00fch gebaut, gern ein.", "tokens": ["Doch", "Nie\u00b7mand", "rei\u00dft", ",", "was", "er", "mit", "M\u00fch", "ge\u00b7baut", ",", "gern", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PWS", "PPER", "APPR", "NN", "VVPP", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im Unbequemen hat man sichs gemacht bequem,", "tokens": ["Im", "Un\u00b7be\u00b7que\u00b7men", "hat", "man", "sichs", "ge\u00b7macht", "be\u00b7quem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "PIS", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und h\u00e4lt, solang man kann, ein unhaltbar System.", "tokens": ["Und", "h\u00e4lt", ",", "so\u00b7lang", "man", "kann", ",", "ein", "un\u00b7halt\u00b7bar", "Sys\u00b7tem", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "VMFIN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}