{"textgrid.poem.24832": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Bekannte Sterne", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da steigen sie strahlend empor aufs neue,", "tokens": ["Da", "stei\u00b7gen", "sie", "strah\u00b7lend", "em\u00b7por", "aufs", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "APPRART", "ADJA", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die altbekannten Sterne, Licht an Licht,", "tokens": ["Die", "alt\u00b7be\u00b7kann\u00b7ten", "Ster\u00b7ne", ",", "Licht", "an", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und gr\u00fc\u00dfen aus der n\u00e4chtlich-dunklen Bl\u00e4ue", "tokens": ["Und", "gr\u00fc\u00b7\u00dfen", "aus", "der", "n\u00e4cht\u00b7lich\u00b7dunk\u00b7len", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach mir mit Freundesangesicht.", "tokens": ["Nach", "mir", "mit", "Freun\u00b7de\u00b7san\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Du dort, der leuchtend durch die Pappelreihen", "tokens": ["Du", "dort", ",", "der", "leuch\u00b7tend", "durch", "die", "Pap\u00b7pel\u00b7rei\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor meines Vaters Haus mir schien, Arktur,", "tokens": ["Vor", "mei\u00b7nes", "Va\u00b7ters", "Haus", "mir", "schien", ",", "Ark\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPER", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem ich, mein Leben hohem Ziel zu weihen,", "tokens": ["Dem", "ich", ",", "mein", "Le\u00b7ben", "ho\u00b7hem", "Ziel", "zu", "wei\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PPOSAT", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In k\u00fchnem Seelendrange schwur;", "tokens": ["In", "k\u00fch\u00b7nem", "See\u00b7len\u00b7dran\u00b7ge", "schwur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Orion du, bei dessen keuschem Strahle", "tokens": ["O\u00b7rion", "du", ",", "bei", "des\u00b7sen", "keu\u00b7schem", "Strah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zuerst an der Geliebten Brust ich sank", "tokens": ["Zu\u00b7erst", "an", "der", "Ge\u00b7lieb\u00b7ten", "Brust", "ich", "sank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "PPER", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und von den Lippen ihr zum erstenmale", "tokens": ["Und", "von", "den", "Lip\u00b7pen", "ihr", "zum", "ers\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den warmen Lebensodem trank;", "tokens": ["Den", "war\u00b7men", "Le\u00b7ben\u00b7so\u00b7dem", "trank", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und du, die halbgeh\u00fcllt in Nebelschleier", "tokens": ["Und", "du", ",", "die", "halb\u00b7ge\u00b7h\u00fcllt", "in", "Ne\u00b7bel\u00b7schlei\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du dort gezogen kommst, so wie du kamst,", "tokens": ["Du", "dort", "ge\u00b7zo\u00b7gen", "kommst", ",", "so", "wie", "du", "kamst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VVFIN", "$,", "ADV", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als du, o Vega, Tr\u00e4gerin der Leier,", "tokens": ["Als", "du", ",", "o", "Ve\u00b7ga", ",", "Tr\u00e4\u00b7ge\u00b7rin", "der", "Lei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "FM", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des J\u00fcnglings erstes Lied vernahmst:", "tokens": ["Des", "J\u00fcng\u00b7lings", "ers\u00b7tes", "Lied", "ver\u00b7nahmst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, alle seid ihr es, geliebte Bilder,", "tokens": ["Ja", ",", "al\u00b7le", "seid", "ihr", "es", ",", "ge\u00b7lieb\u00b7te", "Bil\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VAFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An denen zitternd oft mein Auge hing,", "tokens": ["An", "de\u00b7nen", "zit\u00b7ternd", "oft", "mein", "Au\u00b7ge", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bevor des Himmels mildes Licht in wilder", "tokens": ["Be\u00b7vor", "des", "Him\u00b7mels", "mil\u00b7des", "Licht", "in", "wil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gewitternacht mir unterging.", "tokens": ["Ge\u00b7wit\u00b7ter\u00b7nacht", "mir", "un\u00b7ter\u00b7ging", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Wonnen saht ihr, welche mein einst waren;", "tokens": ["Die", "Won\u00b7nen", "saht", "ihr", ",", "wel\u00b7che", "mein", "einst", "wa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPOSAT", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Saht, wie ich litt und k\u00e4mpfte und verlor \u2013", "tokens": ["Saht", ",", "wie", "ich", "litt", "und", "k\u00e4mpf\u00b7te", "und", "ver\u00b7lor", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr aber zogt seitdem, ihr immerklaren,", "tokens": ["Ihr", "a\u00b7ber", "zogt", "seit\u00b7dem", ",", "ihr", "im\u00b7mer\u00b7kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PAV", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ew'gen Bahnen wie zuvor.", "tokens": ["Die", "ew'\u00b7gen", "Bah\u00b7nen", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Noch strahlt im Glanze, den ihr damals hattet,", "tokens": ["Noch", "strahlt", "im", "Glan\u00b7ze", ",", "den", "ihr", "da\u00b7mals", "hat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Nacht f\u00fcr Nacht am Dach, das droben blaut;", "tokens": ["Ihr", "Nacht", "f\u00fcr", "Nacht", "am", "Dach", ",", "das", "dro\u00b7ben", "blaut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "APPRART", "NN", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch in dem Grame, der mein Aug' umschattet,", "tokens": ["Doch", "in", "dem", "Gra\u00b7me", ",", "der", "mein", "Aug'", "um\u00b7schat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hab' ich euch lange nicht geschaut.", "tokens": ["Hab'", "ich", "euch", "lan\u00b7ge", "nicht", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}