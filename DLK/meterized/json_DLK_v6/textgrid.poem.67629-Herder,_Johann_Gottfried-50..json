{"textgrid.poem.67629": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein M\u00f6nch, der B\u00fccher seines Klosters K\u00fcster,", "tokens": ["Ein", "M\u00f6nch", ",", "der", "B\u00fc\u00b7cher", "sei\u00b7nes", "Klos\u00b7ters", "K\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kam im Register", "tokens": ["Kam", "im", "Re\u00b7gis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Auf ein ebr\u00e4isch Buch, das der Philister,", "tokens": ["Auf", "ein", "eb\u00b7r\u00e4\u00b7isch", "Buch", ",", "das", "der", "Phi\u00b7lis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie billig, nicht verstand.", "tokens": ["Wie", "bil\u00b7lig", ",", "nicht", "ver\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er fr\u00e4gt den Prior: \u00bbHerr, wie wird das Buch genannt?\u00ab", "tokens": ["Er", "fr\u00e4gt", "den", "Pri\u00b7or", ":", "\u00bb", "Herr", ",", "wie", "wird", "das", "Buch", "ge\u00b7nannt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$(", "NN", "$,", "PWAV", "VAFIN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u00bbdas ist,\u00ab antwortet Der, \u00bbdenn auch nicht meine Sache;", "tokens": ["\u00bb", "das", "ist", ",", "\u00ab", "ant\u00b7wor\u00b7tet", "Der", ",", "\u00bb", "denn", "auch", "nicht", "mei\u00b7ne", "Sa\u00b7che", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "$,", "$(", "VVFIN", "ART", "$,", "$(", "KON", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch wei\u00df ich, ist's des lieben Gottes Sprache,", "tokens": ["Doch", "wei\u00df", "ich", ",", "ist's", "des", "lie\u00b7ben", "Got\u00b7tes", "Spra\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die allemal f\u00e4ngt hinten an.\u00ab", "tokens": ["Die", "al\u00b7le\u00b7mal", "f\u00e4ngt", "hin\u00b7ten", "an", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der M\u00f6nch begann:", "tokens": ["Der", "M\u00f6nch", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "\u00bbhier folgt ein Buch, das ich nicht lesen kann.", "tokens": ["\u00bb", "hier", "folgt", "ein", "Buch", ",", "das", "ich", "nicht", "le\u00b7sen", "kann", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Es ist, der Prior sagt's, des lieben Gottes Sprache,", "tokens": ["Es", "ist", ",", "der", "Pri\u00b7or", "sagt's", ",", "des", "lie\u00b7ben", "Got\u00b7tes", "Spra\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die allemal f\u00e4ngt hinten an.\u00ab", "tokens": ["Die", "al\u00b7le\u00b7mal", "f\u00e4ngt", "hin\u00b7ten", "an", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der M\u00f6nch schrieb wahr", "tokens": ["Der", "M\u00f6nch", "schrieb", "wahr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und nannt' in seinem Commentar", "tokens": ["Und", "nannt'", "in", "sei\u00b7nem", "Com\u00b7men\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weit richtiger als mancher Pfarr", "tokens": ["Weit", "rich\u00b7ti\u00b7ger", "als", "man\u00b7cher", "Pfarr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "KOKOM", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mancher Narr.", "tokens": ["Und", "man\u00b7cher", "Narr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}