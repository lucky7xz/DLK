{"dta.poem.23335": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hannoversches Zwangbier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr m\u00fc\u00dft Jahr aus Jahr ein das Bier", "tokens": ["Ihr", "m\u00fc\u00dft", "Jahr", "aus", "Jahr", "ein", "das", "Bier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "APPR", "NN", "ART", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Vom Landesvater kaufen,", "tokens": ["Vom", "Lan\u00b7des\u00b7va\u00b7ter", "kau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch leider! ", "tokens": ["Doch", "lei\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ihr m\u00fc\u00dft das Bier auch saufen.", "tokens": ["Ihr", "m\u00fc\u00dft", "das", "Bier", "auch", "sau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gl\u00fcck zu! trinkt aus, schenkt wacker ein!", "tokens": ["Gl\u00fcck", "zu", "!", "trinkt", "aus", ",", "schenkt", "wa\u00b7cker", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+----+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ich halt' es mit dem Weine:", "tokens": ["Ich", "halt'", "es", "mit", "dem", "Wei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ernst Rex mag euer K\u00f6nig sein,", "tokens": ["Ernst", "Rex", "mag", "eu\u00b7er", "K\u00f6\u00b7nig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Scherz Rex das ist der meine.", "tokens": ["Scherz", "Rex", "das", "ist", "der", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PDS", "VAFIN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}