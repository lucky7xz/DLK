s": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nein, es ist eines Gottes Tat.", "tokens": ["Nein", ",", "es", "ist", "ei\u00b7nes", "Got\u00b7tes", "Tat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Erfreu dich ihrer, rein von S\u00fcnden,", "tokens": ["Er\u00b7freu", "dich", "ih\u00b7rer", ",", "rein", "von", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ehr im Glauben Gottes Rat.", "tokens": ["Und", "ehr", "im", "Glau\u00b7ben", "Got\u00b7tes", "Rat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Bet oft; entdeck am stillen Orte", "tokens": ["Bet", "oft", ";", "ent\u00b7deck", "am", "stil\u00b7len", "Or\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott ohne Zagen deinen Schmerz.", "tokens": ["Gott", "oh\u00b7ne", "Za\u00b7gen", "dei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schlie\u00dft vom Herzen auf die Worte,", "tokens": ["Er", "schlie\u00dft", "vom", "Her\u00b7zen", "auf", "die", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht von den Worten auf das Herz.", "tokens": ["Nicht", "von", "den", "Wor\u00b7ten", "auf", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nicht dein gebognes Knie, nicht Tr\u00e4nen,", "tokens": ["Nicht", "dein", "ge\u00b7bog\u00b7nes", "Knie", ",", "nicht", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht Worte, Seufzer, Psalm und Ton,", "tokens": ["Nicht", "Wor\u00b7te", ",", "Seuf\u00b7zer", ",", "Psalm", "und", "Ton", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht dein Gel\u00fcbd r\u00fchrt Gott; dein Sehnen,", "tokens": ["Nicht", "dein", "Ge\u00b7l\u00fcbd", "r\u00fchrt", "Gott", ";", "dein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVFIN", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Glaub an ihn und seinen Sohn.", "tokens": ["Dein", "Glaub", "an", "ihn", "und", "sei\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Bet oft; Gott wohnt an jeder St\u00e4tte,", "t