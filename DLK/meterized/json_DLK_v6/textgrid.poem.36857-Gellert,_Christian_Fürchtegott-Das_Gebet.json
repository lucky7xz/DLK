: "iambic.tetra"}, "line.7": {"text": "Ist's nicht gerecht: so gelten Z\u00e4hren", "tokens": ["Ist's", "nicht", "ge\u00b7recht", ":", "so", "gel\u00b7ten", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ADJD", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der ganzen Welt nichts vor dem Herrn.", "tokens": ["Der", "gan\u00b7zen", "Welt", "nichts", "vor", "dem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Doch s\u00e4ume nicht, in den Gemeinen", "tokens": ["Doch", "s\u00e4u\u00b7me", "nicht", ",", "in", "den", "Ge\u00b7mei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch \u00f6ffentlich Gott anzuflehn,", "tokens": ["Auch", "\u00f6f\u00b7fent\u00b7lich", "Gott", "an\u00b7zu\u00b7flehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seinen Namen mit den Seinen,", "tokens": ["Und", "sei\u00b7nen", "Na\u00b7men", "mit", "den", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "PPOSS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seinen Br\u00fcdern, zu erh\u00f6hn;", "tokens": ["Mit", "sei\u00b7nen", "Br\u00fc\u00b7dern", ",", "zu", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Herz voll Andacht zu entdecken,", "tokens": ["Dein", "Herz", "voll", "An\u00b7dacht", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie es dein Mitchrist dir entdeckt,", "tokens": ["Wie", "es", "dein", "Mitc\u00b7hrist", "dir", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ihn zur Inbrunst zu erwecken,", "tokens": ["Und", "ihn", "zur", "In\u00b7brunst", "zu", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Wie er zur Inbrunst dich erweckt.", "tokens": ["Wie", "er", "zur", "In\u00b7brunst", "dich", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Bist du ein Herr, dem andre dienen:", "tokens": ["Bist", "du", "ein", "Herr", ",", "dem", "and\u00b7re", "die\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sei ihr Beispiel, sei es stets,", "tokens": ["So", "sei", "ihr", "Bei\u00b7spiel", ",", "sei", "es", "stets", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und feire t\u00e4glich gern mit ihnen", "tokens": ["Und", "fei\u00b7re", "t\u00e4g\u00b7lich", "gern", "mit", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die selge Stunde des Gebets.", "tokens": ["Die", "sel\u00b7ge", "Stun\u00b7de", "des", "Ge\u00b7bets", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nie sch\u00e4me dich des Heils der Seelen,", "tokens": ["Nie", "sch\u00e4\u00b7me", "dich", "des", "Heils", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Gottes Hand dir anvertraut.", "tokens": ["Die", "Got\u00b7tes", "Hand", "dir", "an\u00b7ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Knecht des Hauses m\u00fcsse fehlen;", "tokens": ["Kein", "Knecht", "des", "Hau\u00b7ses", "m\u00fcs\u00b7se", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er ist ein Christ, und werd erbaut!", "tokens": ["Er", "ist", "ein", "Christ", ",", "und", "werd", "er\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Bet oft zu Gott f\u00fcr deine Br\u00fcder,", "tokens": ["Bet", "oft", "zu", "Gott", "f\u00fcr", "dei\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr alle Menschen, als ihr Freund;", "tokens": ["F\u00fcr", "al\u00b7le", "Men\u00b7schen", ",", "als", "ihr", "Freund", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wir sind ", "tokens": ["Denn", "wir", "sind"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VAFIN"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Ein Glied davon ist auch dein Feind.", "tokens": ["Ein", "Glied", "da\u00b7von", "ist", "auch", "dein", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bet oft; so wirst du Glauben halten,", "tokens": ["Bet", "oft", ";", "so", "wirst", "du", "Glau\u00b7ben", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "ADV", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich pr\u00fcfen, und das B\u00f6se scheun,", "tokens": ["Dich", "pr\u00fc\u00b7fen", ",", "und", "das", "B\u00f6\u00b7se", "scheun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An Lieb und Eifer nicht erkalten,", "tokens": ["An", "Lieb", "und", "Ei\u00b7fer", "nicht", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und gern zum Guten weise sein.", "tokens": ["Und", "gern", "zum", "Gu\u00b7ten", "wei\u00b7se", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}