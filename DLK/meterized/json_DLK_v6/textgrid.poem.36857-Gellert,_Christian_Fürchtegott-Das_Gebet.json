{"textgrid.poem.36857": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Das Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Heil, o Christ, nicht zu verscherzen,", "tokens": ["Dein", "Heil", ",", "o", "Christ", ",", "nicht", "zu", "ver\u00b7scher\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei wach und n\u00fcchtern zum Gebet!", "tokens": ["Sei", "wach", "und", "n\u00fcch\u00b7tern", "zum", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Flehn aus reinem guten Herzen", "tokens": ["Ein", "Flehn", "aus", "rei\u00b7nem", "gu\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat Gott, dein Vater, nie verschm\u00e4ht.", "tokens": ["Hat", "Gott", ",", "dein", "Va\u00b7ter", ",", "nie", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erschein vor seinem Angesichte", "tokens": ["Er\u00b7schein", "vor", "sei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Dank, mit Demut, oft und gern,", "tokens": ["Mit", "Dank", ",", "mit", "De\u00b7mut", ",", "oft", "und", "gern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und pr\u00fcfe dich in seinem Lichte,", "tokens": ["Und", "pr\u00fc\u00b7fe", "dich", "in", "sei\u00b7nem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und klage deine Not dem Herrn.", "tokens": ["Und", "kla\u00b7ge", "dei\u00b7ne", "Not", "dem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Welch Gl\u00fcck, so hoch geehrt zu werden", "tokens": ["Welch", "Gl\u00fcck", ",", "so", "hoch", "ge\u00b7ehrt", "zu", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "ADV", "ADJD", "VVPP", "PTKZU", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und im Gebet vor Gott zu stehn!", "tokens": ["Und", "im", "Ge\u00b7bet", "vor", "Gott", "zu", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herr des Himmels und der Erden,", "tokens": ["Der", "Herr", "des", "Him\u00b7mels", "und", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bedarf der eines Menschen Flehn?", "tokens": ["Be\u00b7darf", "der", "ei\u00b7nes", "Men\u00b7schen", "Flehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sagt Gott nicht: Bittet, da\u00df ihr nehmet?", "tokens": ["Sagt", "Gott", "nicht", ":", "Bit\u00b7tet", ",", "da\u00df", "ihr", "neh\u00b7met", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKNEG", "$.", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist des Gebetes Frucht nicht dein?", "tokens": ["Ist", "des", "Ge\u00b7be\u00b7tes", "Frucht", "nicht", "dein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer sich der Pflicht zu beten sch\u00e4met,", "tokens": ["Wer", "sich", "der", "Pflicht", "zu", "be\u00b7ten", "sch\u00e4\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der sch\u00e4mt sich, Gottes Freund zu sein.", "tokens": ["Der", "sch\u00e4mt", "sich", ",", "Got\u00b7tes", "Freund", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "$,", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sein Gl\u00fcck von seinem Gott begehren,", "tokens": ["Sein", "Gl\u00fcck", "von", "sei\u00b7nem", "Gott", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist dies denn eine schwere Pflicht?", "tokens": ["Ist", "dies", "denn", "ei\u00b7ne", "schwe\u00b7re", "Pflicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine W\u00fcnsche Gott erkl\u00e4ren,", "tokens": ["Und", "sei\u00b7ne", "W\u00fcn\u00b7sche", "Gott", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erhebt dies unsre Seele nicht?", "tokens": ["Er\u00b7hebt", "dies", "uns\u00b7re", "See\u00b7le", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich in der Furcht des H\u00f6chsten st\u00e4rken,", "tokens": ["Sich", "in", "der", "Furcht", "des", "H\u00f6chs\u00b7ten", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In dem Vertraun, da\u00df Gott uns liebt,", "tokens": ["In", "dem", "Ver\u00b7traun", ",", "da\u00df", "Gott", "uns", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Flei\u00df zu allen guten Werken,", "tokens": ["Im", "Flei\u00df", "zu", "al\u00b7len", "gu\u00b7ten", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist diese Pflicht f\u00fcr dich betr\u00fcbt?", "tokens": ["Ist", "die\u00b7se", "Pflicht", "f\u00fcr", "dich", "be\u00b7tr\u00fcbt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bet oft in Einfalt deiner Seelen;", "tokens": ["Bet", "oft", "in", "Ein\u00b7falt", "dei\u00b7ner", "See\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott sieht aufs Herz, Gott ist ein Geist.", "tokens": ["Gott", "sieht", "aufs", "Herz", ",", "Gott", "ist", "ein", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie k\u00f6nnen dir die Worte fehlen,", "tokens": ["Wie", "k\u00f6n\u00b7nen", "dir", "die", "Wor\u00b7te", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wofern dein Herz dich beten hei\u00dft?", "tokens": ["Wo\u00b7fern", "dein", "Herz", "dich", "be\u00b7ten", "hei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht T\u00f6ne sind's, die Gott gefallen,", "tokens": ["Nicht", "T\u00f6\u00b7ne", "sin\u00b7d's", ",", "die", "Gott", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nicht Worte, die die Kunst gebeut.", "tokens": ["Nicht", "Wor\u00b7te", ",", "die", "die", "Kunst", "ge\u00b7beut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gott ist kein Mensch. Ein gl\u00e4ubig Lallen,", "tokens": ["Gott", "ist", "kein", "Mensch", ".", "Ein", "gl\u00e4u\u00b7big", "Lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Das ist vor ihm Beredsamkeit.", "tokens": ["Das", "ist", "vor", "ihm", "Be\u00b7red\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer das, was uns zum Frieden dienet,", "tokens": ["Wer", "das", ",", "was", "uns", "zum", "Frie\u00b7den", "die\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Glauben sucht, der ehret Gott.", "tokens": ["Im", "Glau\u00b7ben", "sucht", ",", "der", "eh\u00b7ret", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer das zu bitten sich erk\u00fchnet,", "tokens": ["Wer", "das", "zu", "bit\u00b7ten", "sich", "er\u00b7k\u00fch\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "PTKZU", "VVINF", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was er nicht w\u00fcnscht, entehret Gott.", "tokens": ["Was", "er", "nicht", "w\u00fcnscht", ",", "en\u00b7teh\u00b7ret", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer t\u00e4glich Gott die Treue schw\u00f6ret,", "tokens": ["Wer", "t\u00e4g\u00b7lich", "Gott", "die", "Treu\u00b7e", "schw\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dann vergi\u00dft, was er beschwur;", "tokens": ["Und", "dann", "ver\u00b7gi\u00dft", ",", "was", "er", "be\u00b7schwur", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und klagt, da\u00df Gott ihn nicht erh\u00f6ret,", "tokens": ["Und", "klagt", ",", "da\u00df", "Gott", "ihn", "nicht", "er\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der spottet seines Sch\u00f6pfers nur.", "tokens": ["Der", "spot\u00b7tet", "sei\u00b7nes", "Sch\u00f6p\u00b7fers", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Bet oft zu Gott, und schmeck in Freuden,", "tokens": ["Bet", "oft", "zu", "Gott", ",", "und", "schmeck", "in", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie freundlich er, dein Vater, ist.", "tokens": ["Wie", "freund\u00b7lich", "er", ",", "dein", "Va\u00b7ter", ",", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bet oft zu Gott, und f\u00fchl in Leiden,", "tokens": ["Bet", "oft", "zu", "Gott", ",", "und", "f\u00fchl", "in", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie g\u00f6ttlich er das Leid vers\u00fc\u00dft.", "tokens": ["Wie", "g\u00f6tt\u00b7lich", "er", "das", "Leid", "ver\u00b7s\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bet oft, wenn dich Versuchung qu\u00e4let;", "tokens": ["Bet", "oft", ",", "wenn", "dich", "Ver\u00b7su\u00b7chung", "qu\u00e4\u00b7let", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gott h\u00f6rt's, Gott ist's, der H\u00fclfe schafft.", "tokens": ["Gott", "h\u00f6rt's", ",", "Gott", "ist's", ",", "der", "H\u00fcl\u00b7fe", "schafft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VAFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Bet oft, wenn innrer Trost dir fehlet;", "tokens": ["Bet", "oft", ",", "wenn", "inn\u00b7rer", "Trost", "dir", "feh\u00b7let", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er gibt den M\u00fcden St\u00e4rk und Kraft.", "tokens": ["Er", "gibt", "den", "M\u00fc\u00b7den", "St\u00e4rk", "und", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bet oft, und heiter im Gem\u00fcte", "tokens": ["Bet", "oft", ",", "und", "hei\u00b7ter", "im", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schau dich an seinen Wundern satt.", "tokens": ["Schau", "dich", "an", "sei\u00b7nen", "Wun\u00b7dern", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schau auf den Ernst, schau auf die G\u00fcte,", "tokens": ["Schau", "auf", "den", "Ernst", ",", "schau", "auf", "die", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mit der er dich geleitet hat.", "tokens": ["Mit", "der", "er", "dich", "ge\u00b7lei\u00b7tet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier irrtest du in deiner Jugend,", "tokens": ["Hier", "irr\u00b7test", "du", "in", "dei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Alter dort. Er trug Geduld,", "tokens": ["Im", "Al\u00b7ter", "dort", ".", "Er", "trug", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Rief dich durch Gl\u00fcck und Kreuz zur Tugend;", "tokens": ["Rief", "dich", "durch", "Gl\u00fcck", "und", "Kreuz", "zur", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erkenn und f\u00fchle seine Huld.", "tokens": ["Er\u00b7kenn", "und", "f\u00fch\u00b7le", "sei\u00b7ne", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Bet oft, und schau mit selgen Blicken", "tokens": ["Bet", "oft", ",", "und", "schau", "mit", "sel\u00b7gen", "Bli\