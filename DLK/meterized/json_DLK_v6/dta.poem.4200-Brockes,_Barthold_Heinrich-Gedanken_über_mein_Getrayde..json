{"dta.poem.4200": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken \u00fcber mein Getrayde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kann jetzt nicht nur mit Freuden,", "tokens": ["Ich", "kann", "jetzt", "nicht", "nur", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So wie vor, mein Auge weiden", "tokens": ["So", "wie", "vor", ",", "mein", "Au\u00b7ge", "wei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "$,", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An der Felder Schmuck und Pracht,", "tokens": ["An", "der", "Fel\u00b7der", "Schmuck", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und darinn die weise Macht", "tokens": ["Und", "da\u00b7rinn", "die", "wei\u00b7se", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "De\u00df, Der sie hervorgebracht,", "tokens": ["De\u00df", ",", "Der", "sie", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Loben, r\u00fchmen und erheben;", "tokens": ["Lo\u00b7ben", ",", "r\u00fch\u00b7men", "und", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sondern GOtt hat mir gegeben", "tokens": ["Son\u00b7dern", "Gott", "hat", "mir", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Zu der allgemeinen Gabe,", "tokens": ["Zu", "der", "all\u00b7ge\u00b7mei\u00b7nen", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df ich (Jhm sey Dank daf\u00fcr)", "tokens": ["Da\u00df", "ich", "(", "Jhm", "sey", "Dank", "da\u00b7f\u00fcr", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPER", "VAFIN", "APPR", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Von derselben holden Zier", "tokens": ["Von", "der\u00b7sel\u00b7ben", "hol\u00b7den", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Auch die Frucht zu hoffen habe.", "tokens": ["Auch", "die", "Frucht", "zu", "hof\u00b7fen", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Welches, da es GOtt beschehrt,", "tokens": ["Wel\u00b7ches", ",", "da", "es", "Gott", "be\u00b7schehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Dank und Freude noch vermehrt,", "tokens": ["Dank", "und", "Freu\u00b7de", "noch", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Die ich GOtt in beyden zolle,", "tokens": ["Die", "ich", "Gott", "in", "bey\u00b7den", "zol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Mit dazu gef\u00fcgtem Flehn,", "tokens": ["Mit", "da\u00b7zu", "ge\u00b7f\u00fcg\u00b7tem", "Flehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Da\u00df Er\u2019s ferner wohl ergehn", "tokens": ["Da\u00df", "Er's", "fer\u00b7ner", "wohl", "er\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Und gerahten lassen wolle!", "tokens": ["Und", "ge\u00b7rah\u00b7ten", "las\u00b7sen", "wol\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Da das liebe Korn zumahl", "tokens": ["Da", "das", "lie\u00b7be", "Korn", "zu\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "KOUS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "So vortrefflich steht; so lenke", "tokens": ["So", "vor\u00b7treff\u00b7lich", "steht", ";", "so", "len\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Sich der Sonnen Lebens-Strahl", "tokens": ["Sich", "der", "Son\u00b7nen", "Le\u00b7bens\u00b7Strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Ferner auf mein Feld! Es tr\u00e4nke", "tokens": ["Fer\u00b7ner", "auf", "mein", "Feld", "!", "Es", "tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Den mir hier gezeigten Segen", "tokens": ["Den", "mir", "hier", "ge\u00b7zeig\u00b7ten", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Oefters ein erw\u00fcnschter Regen", "tokens": ["Oef\u00b7ters", "ein", "er\u00b7w\u00fcnschter", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.24": {"text": "Voller fetten Fruchtbarkeit!", "tokens": ["Vol\u00b7ler", "fet\u00b7ten", "Frucht\u00b7bar\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "La\u00df gelinde Winde wehen!", "tokens": ["La\u00df", "ge\u00b7lin\u00b7de", "Win\u00b7de", "we\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "La\u00df mich es zu rechter Zeit", "tokens": ["La\u00df", "mich", "es", "zu", "rech\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "In die Scheuren fahren sehen!", "tokens": ["In", "die", "Scheu\u00b7ren", "fah\u00b7ren", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "La\u00df mich deine Huld ermessen!", "tokens": ["La\u00df", "mich", "dei\u00b7ne", "Huld", "er\u00b7mes\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "La\u00df es mir zum Nutz erspriessen,", "tokens": ["La\u00df", "es", "mir", "zum", "Nutz", "er\u00b7spries\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Und, wenn wirs mit Lust geniessen,", "tokens": ["Und", ",", "wenn", "wirs", "mit", "Lust", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Ja des Dankens nicht vergessen!", "tokens": ["Ja", "des", "Dan\u00b7kens", "nicht", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}