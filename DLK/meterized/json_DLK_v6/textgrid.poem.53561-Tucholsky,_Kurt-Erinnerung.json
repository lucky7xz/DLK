{"textgrid.poem.53561": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Deutschland hingegen", "tokens": ["In", "Deutschland", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "tuns erst lang \u00fcberlegen,", "tokens": ["tuns", "erst", "lang", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "dann schreib'n sie sich ein", "tokens": ["dann", "schreib'n", "sie", "sich", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "in ein' Aufruhrverein,", "tokens": ["in", "ein'", "Auf\u00b7ruhr\u00b7ve\u00b7rein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "w\u00e4hln ein' Obmann, wie's Brauch,", "tokens": ["w\u00e4hln", "ein'", "Ob\u00b7mann", ",", "wie's", "Brauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "zwei Schriftf\u00fchrer auch, ein' Kontrollor, zwei Kassier,", "tokens": ["zwei", "Schrift\u00b7f\u00fch\u00b7rer", "auch", ",", "ein'", "Kont\u00b7rol\u00b7lor", ",", "zwei", "Kas\u00b7sier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$,", "ART", "NN", "$,", "CARD", "NN", "$,"], "meter": "-++-+-+-++-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "Ehrenjungfrauen vier,", "tokens": ["Eh\u00b7ren\u00b7jung\u00b7frau\u00b7en", "vier", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "CARD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "dann schreib'n 's nach'm Buch", "tokens": ["dann", "schreib'n", "'s", "nach'm", "Buch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "ein Freiheitsgesuch: \u2013", "tokens": ["ein", "Frei\u00b7heits\u00b7ge\u00b7such", ":", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "So machen's dort die Revolution,", "tokens": ["So", "ma\u00b7chen's", "dort", "die", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "das liegt im Charakter der Nation!", "tokens": ["das", "liegt", "im", "Cha\u00b7rak\u00b7ter", "der", "Na\u00b7tion", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Charakter? Ich glaube, sie hat keinen.", "tokens": ["Cha\u00b7rak\u00b7ter", "?", "Ich", "glau\u00b7be", ",", "sie", "hat", "kei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PIAT", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}