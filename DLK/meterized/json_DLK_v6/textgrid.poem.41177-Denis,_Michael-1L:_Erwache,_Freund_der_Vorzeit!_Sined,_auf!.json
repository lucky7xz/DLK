{"textgrid.poem.41177": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erwache, Freund der Vorzeit! Sined, auf!", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erwache, Freund der Vorzeit! Sined, auf!", "tokens": ["Er\u00b7wa\u00b7che", ",", "Freund", "der", "Vor\u00b7zeit", "!", "Si\u00b7ned", ",", "auf", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$.", "NE", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Mittag deines Lebens ist vorbei.", "tokens": ["Der", "Mit\u00b7tag", "dei\u00b7nes", "Le\u00b7bens", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sieh mir in's Antlitz! Ich erscheine dir", "tokens": ["Sieh", "mir", "in's", "Ant\u00b7litz", "!", "Ich", "er\u00b7schei\u00b7ne", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht mehr so oft, als ich erschienen bin.", "tokens": ["Nicht", "mehr", "so", "oft", ",", "als", "ich", "er\u00b7schie\u00b7nen", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADV", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer fl\u00fcstert mir in meine Tr\u00e4ume? Graut", "tokens": ["Wer", "fl\u00fcs\u00b7tert", "mir", "in", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "?", "Graut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Morgen? \u2013 Ha, du bist's, du kehrest, Tag!", "tokens": ["Der", "Mor\u00b7gen", "?", "\u2013", "Ha", ",", "du", "bist's", ",", "du", "keh\u00b7rest", ",", "Tag", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ITJ", "$,", "PPER", "NE", "$,", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein erster Tag, als auf Allvaters Wink", "tokens": ["Mein", "ers\u00b7ter", "Tag", ",", "als", "auf", "All\u00b7va\u00b7ters", "Wink"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch mich die Zahl der Erdekinder wuchs!", "tokens": ["Durch", "mich", "die", "Zahl", "der", "Er\u00b7de\u00b7kin\u00b7der", "wuchs", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sey mir willkommen! Fr\u00fcher Harfenklang,", "tokens": ["Sey", "mir", "will\u00b7kom\u00b7men", "!", "Fr\u00fc\u00b7her", "Har\u00b7fen\u00b7klang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und warmer Herzenausbruch feire dich,", "tokens": ["Und", "war\u00b7mer", "Her\u00b7zen\u00b7aus\u00b7bruch", "fei\u00b7re", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da noch kein Liederzeuge mich umsteht.", "tokens": ["Da", "noch", "kein", "Lie\u00b7der\u00b7zeu\u00b7ge", "mich", "um\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sey mir gegr\u00fc\u00dfet, o Tag! an welchem ein z\u00e4rtlicher Vater", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fc\u00b7\u00dfet", ",", "o", "Tag", "!", "an", "wel\u00b7chem", "ein", "z\u00e4rt\u00b7li\u00b7cher", "Va\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "FM", "NN", "$.", "APPR", "PRELS", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "In die gef\u00e4lligen Arme mich nahm,", "tokens": ["In", "die", "ge\u00b7f\u00e4l\u00b7li\u00b7gen", "Ar\u00b7me", "mich", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Einer erwartenden Mutter das erstemal reichte, die freudig", "tokens": ["Ei\u00b7ner", "er\u00b7war\u00b7ten\u00b7den", "Mut\u00b7ter", "das", "ers\u00b7te\u00b7mal", "reich\u00b7te", ",", "die", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADV", "VVFIN", "$,", "PRELS", "ADJD"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Ueber dem Sohne der Wehen verga\u00df,", "tokens": ["Ue\u00b7ber", "dem", "Soh\u00b7ne", "der", "We\u00b7hen", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Freudig den eigenen Busen mir bot. Ich konnt' ihr nicht danken;", "tokens": ["Freu\u00b7dig", "den", "ei\u00b7ge\u00b7nen", "Bu\u00b7sen", "mir", "bot", ".", "Ich", "konnt'", "ihr", "nicht", "dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Aber als itzo mein keimender Geist", "tokens": ["A\u00b7ber", "als", "it\u00b7zo", "mein", "kei\u00b7men\u00b7der", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "PIAT", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Seine Gesch\u00e4fte begann, mein Aug' mit Seele sich umsah,", "tokens": ["Sei\u00b7ne", "Ge\u00b7sch\u00e4f\u00b7te", "be\u00b7gann", ",", "mein", "Aug'", "mit", "See\u00b7le", "sich", "um\u00b7sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Lispel der Liebe mir sprachen in's Herz,", "tokens": ["Lis\u00b7pel", "der", "Lie\u00b7be", "mir", "spra\u00b7chen", "in's", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "Tag! du wei\u00dft es, wie sehr ich sie liebte! So oft ich von ihr ging,", "tokens": ["Tag", "!", "du", "wei\u00dft", "es", ",", "wie", "sehr", "ich", "sie", "lieb\u00b7te", "!", "So", "oft", "ich", "von", "ihr", "ging", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "$,", "PWAV", "ADV", "PPER", "PPER", "VVFIN", "$.", "ADV", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Weisheit zu suchen, so sah mich die Nacht,", "tokens": ["Weis\u00b7heit", "zu", "su\u00b7chen", ",", "so", "sah", "mich", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.11": {"text": "Sah mich der Morgen in Thr\u00e4nen; und dennoch liebt' ich die Weisheit", "tokens": ["Sah", "mich", "der", "Mor\u00b7gen", "in", "Thr\u00e4\u00b7nen", ";", "und", "den\u00b7noch", "liebt'", "ich", "die", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$.", "KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Mehr, als der J\u00fcngling das L\u00e4cheln der Braut.", "tokens": ["Mehr", ",", "als", "der", "J\u00fcng\u00b7ling", "das", "L\u00e4\u00b7cheln", "der", "Braut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.13": {"text": "Aber als itzo vom Himmel das Loos, in weiser Druiden", "tokens": ["A\u00b7ber", "als", "it\u00b7zo", "vom", "Him\u00b7mel", "das", "Loos", ",", "in", "wei\u00b7ser", "Dru\u00b7i\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPRART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Schoo\u00dfe mein Leben zu leben, mir fiel,", "tokens": ["Schoo\u00b7\u00dfe", "mein", "Le\u00b7ben", "zu", "le\u00b7ben", ",", "mir", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.15": {"text": "Als ich die Heimath verlie\u00df, die Donau den R\u00fccken mir anbot,", "tokens": ["Als", "ich", "die", "Hei\u00b7math", "ver\u00b7lie\u00df", ",", "die", "Do\u00b7nau", "den", "R\u00fc\u00b7cken", "mir", "an\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "NE", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Stand sie, die Z\u00e4rtliche, die mich gebar,", "tokens": ["Stand", "sie", ",", "die", "Z\u00e4rt\u00b7li\u00b7che", ",", "die", "mich", "ge\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Weinend am Ufer, und gab mit wehendem Schleier der Liebe", "tokens": ["Wei\u00b7nend", "am", "U\u00b7fer", ",", "und", "gab", "mit", "we\u00b7hen\u00b7dem", "Schlei\u00b7er", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Zeichen dem Sohne, bis endlich der Strom", "tokens": ["Zei\u00b7chen", "dem", "Soh\u00b7ne", ",", "bis", "end\u00b7lich", "der", "Strom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KON", "ADV", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.19": {"text": "Ihren Geliebten vertrug. Ich habe sie nimmer gesehen.", "tokens": ["Ih\u00b7ren", "Ge\u00b7lieb\u00b7ten", "ver\u00b7trug", ".", "Ich", "ha\u00b7be", "sie", "nim\u00b7mer", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Lange schon decken ihr frommes Gebein", "tokens": ["Lan\u00b7ge", "schon", "de\u00b7cken", "ihr", "from\u00b7mes", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.21": {"text": "Herbstliche Blumen; allein in Sineds Seele gegraben", "tokens": ["Herbst\u00b7li\u00b7che", "Blu\u00b7men", ";", "al\u00b7lein", "in", "Si\u00b7neds", "See\u00b7le", "ge\u00b7gra\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "APPR", "NE", "NN", "VVPP"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Lebet verehret ihr ewiges Bild.", "tokens": ["Le\u00b7bet", "ver\u00b7eh\u00b7ret", "ihr", "e\u00b7wi\u00b7ges", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.23": {"text": "Den auch, der mich erzeugte, den hab' ich nimmer gesehen.", "tokens": ["Den", "auch", ",", "der", "mich", "er\u00b7zeug\u00b7te", ",", "den", "hab'", "ich", "nim\u00b7mer", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Lange schon hub sich sein H\u00fcgel empor.", "tokens": ["Lan\u00b7ge", "schon", "hub", "sich", "sein", "H\u00fc\u00b7gel", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.25": {"text": "Ach mein Vater! Du erster, der Liederkenntni\u00df mich lehrte!", "tokens": ["Ach", "mein", "Va\u00b7ter", "!", "Du", "ers\u00b7ter", ",", "der", "Lie\u00b7der\u00b7kennt\u00b7ni\u00df", "mich", "lehr\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$.", "PPER", "ADJA", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Ach noch erblick' ich die Stunde vor mir,", "tokens": ["Ach", "noch", "er\u00b7blick'", "ich", "die", "Stun\u00b7de", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.27": {"text": "Da du dem Knaben vom ewigen Liede des R\u00f6mers erz\u00e4hltest,", "tokens": ["Da", "du", "dem", "Kna\u00b7ben", "vom", "e\u00b7wi\u00b7gen", "Lie\u00b7de", "des", "R\u00f6\u00b7mers", "er\u00b7z\u00e4hl\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Welcher die K\u00fcnste der Bienen besang.", "tokens": ["Wel\u00b7cher", "die", "K\u00fcns\u00b7te", "der", "Bie\u00b7nen", "be\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.29": {"text": "Thiere, die waren mir immer so lieb. O gib mir den S\u00e4nger,", "tokens": ["Thie\u00b7re", ",", "die", "wa\u00b7ren", "mir", "im\u00b7mer", "so", "lieb", ".", "O", "gib", "mir", "den", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$.", "NE", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Vater! ich will ihn, ich mu\u00df ihn versteh'n!", "tokens": ["Va\u00b7ter", "!", "ich", "will", "ihn", ",", "ich", "mu\u00df", "ihn", "ver\u00b7steh'n", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.31": {"text": "Also begann ich. Noch herrschten die Lieder der Fremden. In Erde", "tokens": ["Al\u00b7so", "be\u00b7gann", "ich", ".", "Noch", "herrschten", "die", "Lie\u00b7der", "der", "Frem\u00b7den", ".", "In", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "APPR", "NN"], "meter": 