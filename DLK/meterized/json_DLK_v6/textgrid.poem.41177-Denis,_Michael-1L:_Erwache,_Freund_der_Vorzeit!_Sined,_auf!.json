, "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "H\u00e4ttest du Sined geseh'n im Kreise der Barden, dein Antlitz", "tokens": ["H\u00e4t\u00b7test", "du", "Si\u00b7ned", "ge\u00b7seh'n", "im", "Krei\u00b7se", "der", "Bar\u00b7den", ",", "dein", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "VVPP", "APPRART", "NN", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.38": {"text": "H\u00e4tte vor inniger Wonne gegl\u00e4nzt! \u2013", "tokens": ["H\u00e4t\u00b7te", "vor", "in\u00b7ni\u00b7ger", "Won\u00b7ne", "ge\u00b7gl\u00e4nzt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Aber h\u00e4ngst du denn nicht am Arme von deiner Geliebten", "tokens": ["A\u00b7ber", "h\u00e4ngst", "du", "denn", "nicht", "am", "Ar\u00b7me", "von", "dei\u00b7ner", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-++-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Jetzo den thauenden Himmel herab?", "tokens": ["Jet\u00b7zo", "den", "thau\u00b7en\u00b7den", "Him\u00b7mel", "her\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "H\u00f6rst du mich nicht? \u2013 Ihr h\u00f6ret mich, Aeltern! Allvater der mi\u00dft euch,", "tokens": ["H\u00f6rst", "du", "mich", "nicht", "?", "\u2013", "Ihr", "h\u00f6\u00b7ret", "mich", ",", "A\u00b7el\u00b7tern", "!", "All\u00b7va\u00b7