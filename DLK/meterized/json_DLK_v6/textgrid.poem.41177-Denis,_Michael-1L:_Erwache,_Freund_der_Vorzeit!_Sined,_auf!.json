 "mir", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "ADV", "VVFIN", "$,", "PRELS", "ADV", "PPER", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Wie es Allvater gebeut! Ich will nicht forschen. Er wird es,", "tokens": ["Wie", "es", "All\u00b7va\u00b7ter", "ge\u00b7beut", "!", "Ich", "will", "nicht", "for\u00b7schen", ".", "Er", "wird", "es", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "PPER", "VAFIN", "PPER", "$,"], "meter": "+-++--+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Wie er es anfing, vollenden mit mir,", "tokens": ["Wie", "er", "es", "an\u00b7fing", ",", "voll\u00b7en\u00b7den", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "ADJA", "APPR", "PPER", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Gn\u00e4dig vollenden! Mein Abschied von hier ist lange gesungen,", "tokens": ["Gn\u00e4\u00b7dig", "voll\u00b7en\u00b7den", "!", "Mein", "Ab\u00b7schied", "von", "hier", "ist", "lan\u00b7ge", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$.", "PPOSAT", "NN", "APPR", "ADV", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Lange den Freunden des Liedes bekannt.", "tokens": ["Lan\u00b7ge", "den", "Freun\u00b7den", "des", "Lie\u00b7des", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "me