{"textgrid.poem.53679": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "K\u00e4hne", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du d\u00e4mlicher Hund liegst blutend im Wald.", "tokens": ["Du", "d\u00e4m\u00b7li\u00b7cher", "Hund", "liegst", "blu\u00b7tend", "im", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein preu\u00dfischer Adliger machte dich kalt.", "tokens": ["Ein", "preu\u00b7\u00dfi\u00b7scher", "Ad\u00b7li\u00b7ger", "mach\u00b7te", "dich", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zitternd stand dein Junge dabei \u2013", "tokens": ["Zit\u00b7ternd", "stand", "dein", "Jun\u00b7ge", "da\u00b7bei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "PAV", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mensch, du warst Nummer 103!", "tokens": ["Mensch", ",", "du", "warst", "Num\u00b7mer", "10\u00b73", "!"], "token_info": ["word", "punct", "word", "word", "word", "number", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "CARD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "W\u00e4lz dich im Dreck \u2013 aber mach keine Szene.", "tokens": ["W\u00e4lz", "dich", "im", "Dreck", "\u2013", "a\u00b7ber", "mach", "kei\u00b7ne", "Sze\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$(", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Auf dich scho\u00df nicht schlecht", "tokens": ["Auf", "dich", "scho\u00df", "nicht", "schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PTKNEG", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "waidgerecht", "tokens": ["waid\u00b7ge\u00b7recht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "K\u00e4hne.", "tokens": ["K\u00e4h\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Das treibt er seit f\u00fcnfzehn Jahren so.", "tokens": ["Das", "treibt", "er", "seit", "f\u00fcnf\u00b7zehn", "Jah\u00b7ren", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "CARD", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die braven Sonntagsausfl\u00fcgler sind froh,", "tokens": ["Die", "bra\u00b7ven", "Sonn\u00b7tags\u00b7aus\u00b7fl\u00fcg\u00b7ler", "sind", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wenn sie an seinem Anstand vorbei.", "tokens": ["wenn", "sie", "an", "sei\u00b7nem", "An\u00b7stand", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Einen Schritt zu weit \u2013 Schu\u00df, Fall, Geschrei.", "tokens": ["Ei\u00b7nen", "Schritt", "zu", "weit", "\u2013", "Schu\u00df", ",", "Fall", ",", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKA", "ADJD", "$(", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Der schie\u00dft aus Notwehr den Fraun in die Beene.", "tokens": ["Der", "schie\u00dft", "aus", "Not\u00b7wehr", "den", "Fraun", "in", "die", "Bee\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Weil er bedroht w\u00e4r.", "tokens": ["Weil", "er", "be\u00b7droht", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Immer in Notwehr.", "tokens": ["Im\u00b7mer", "in", "Not\u00b7wehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "K\u00e4hne.", "tokens": ["K\u00e4h\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Mit Landrat und Richtern im Amtsgest\u00fchl", "tokens": ["Mit", "Land\u00b7rat", "und", "Rich\u00b7tern", "im", "Amts\u00b7ge\u00b7st\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zusammengeh\u00f6rigkeitsgef\u00fchl.", "tokens": ["Zu\u00b7sam\u00b7men\u00b7ge\u00b7h\u00f6\u00b7rig\u00b7keits\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kein Gendarm, kein Landj\u00e4ger siehts.", "tokens": ["Kein", "Gen\u00b7darm", ",", "kein", "Land\u00b7j\u00e4\u00b7ger", "siehts", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Herr Hauptmann schie\u00dft auf die ganze Justiz.", "tokens": ["Herr", "Haupt\u00b7mann", "schie\u00dft", "auf", "die", "gan\u00b7ze", "Jus\u00b7tiz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hat Waffen, Freiheit, Helfershelfer", "tokens": ["Hat", "Waf\u00b7fen", ",", "Frei\u00b7heit", ",", "Hel\u00b7fer\u00b7shel\u00b7fer"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "von Potsdam bis Petzow . . .", "tokens": ["von", "Pots\u00b7dam", "bis", "Pet\u00b7zow", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wozu das Gebelfer?", "tokens": ["Wo\u00b7zu", "das", "Ge\u00b7bel\u00b7fer", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr la\u00dft euch peitschen von solchem Kujon?", "tokens": ["Ihr", "la\u00dft", "euch", "peit\u00b7schen", "von", "sol\u00b7chem", "Ku\u00b7jon", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Versammelt euch. Redet. Resolution.", "tokens": ["Ver\u00b7sam\u00b7melt", "euch", ".", "Re\u00b7det", ".", "Re\u00b7so\u00b7lu\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "$.", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Macht Justizreformierungspl\u00e4ne . . .", "tokens": ["Macht", "Jus\u00b7tiz\u00b7re\u00b7for\u00b7mie\u00b7rungs\u00b7pl\u00e4\u00b7ne", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["NN", "NN", "$.", "$.", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Gibt es im ganzen Lande so weit", "tokens": ["Gibt", "es", "im", "gan\u00b7zen", "Lan\u00b7de", "so", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ADV", "ADJD"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "keine, keine Gerechtigkeit \u2013?", "tokens": ["kei\u00b7ne", ",", "kei\u00b7ne", "Ge\u00b7rech\u00b7tig\u00b7keit", "\u2013", "?"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["PIAT", "$,", "PIAT", "NN", "$(", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "K\u00e4hne.", "tokens": ["K\u00e4h\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}