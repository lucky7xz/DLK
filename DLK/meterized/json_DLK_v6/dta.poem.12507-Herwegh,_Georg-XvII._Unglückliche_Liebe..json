{"dta.poem.12507": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XvII.  \n  Ungl\u00fcckliche Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht an den K\u00f6nigen liegt's \u2014 die K\u00f6nige", "tokens": ["Nicht", "an", "den", "K\u00f6\u00b7ni\u00b7gen", "liegt's", "die", "K\u00f6\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$(", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "lieben die Freiheit:", "tokens": ["lie\u00b7ben", "die", "Frei\u00b7heit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Aber die Freiheit liebt leider die K\u00f6nige", "tokens": ["A\u00b7ber", "die", "Frei\u00b7heit", "liebt", "lei\u00b7der", "die", "K\u00f6\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "nicht!", "tokens": ["nicht", "!"], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$."], "meter": "-", "measure": "single.down"}}}}}