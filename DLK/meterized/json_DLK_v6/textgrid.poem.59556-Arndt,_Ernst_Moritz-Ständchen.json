{"textgrid.poem.59556": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "St\u00e4ndchen", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es leuchtet ein Licht durch die weite Welt,", "tokens": ["Es", "leuch\u00b7tet", "ein", "Licht", "durch", "die", "wei\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das sch\u00f6nste und hellste von allen,", "tokens": ["Das", "sch\u00f6ns\u00b7te", "und", "hells\u00b7te", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "APPR", "PIAT", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es ist nicht als Sonne zum Tag bestellt,", "tokens": ["Es", "ist", "nicht", "als", "Son\u00b7ne", "zum", "Tag", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "KOUS", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nicht als Mond die Nacht zu durchstrahlen,", "tokens": ["Nicht", "als", "Mond", "die", "Nacht", "zu", "durch\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Bla\u00df werden die Sternlein, die Kleinen,", "tokens": ["Bla\u00df", "wer\u00b7den", "die", "Stern\u00b7lein", ",", "die", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Vor seinem allm\u00e4chtigen Scheinen.", "tokens": ["Vor", "sei\u00b7nem", "all\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Es dringet wie Blitze durch Eisen und Stahl,", "tokens": ["Es", "drin\u00b7get", "wie", "Blit\u00b7ze", "durch", "Ei\u00b7sen", "und", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Kein Panzer mag fest vor ihm stehen,", "tokens": ["Kein", "Pan\u00b7zer", "mag", "fest", "vor", "ihm", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch dr\u00e4nget sich jeder zum f\u00e4hrlichen Strahl!", "tokens": ["Doch", "dr\u00e4n\u00b7get", "sich", "je\u00b7der", "zum", "f\u00e4hr\u00b7li\u00b7chen", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIS", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und sollt' er zur Stunde vergehen.", "tokens": ["Und", "sollt'", "er", "zur", "Stun\u00b7de", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "O scheine, du s\u00fc\u00dfes Verderben!", "tokens": ["O", "schei\u00b7ne", ",", "du", "s\u00fc\u00b7\u00dfes", "Ver\u00b7der\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wie lieblich, in Flammen zu sterben!", "tokens": ["Wie", "lieb\u00b7lich", ",", "in", "Flam\u00b7men", "zu", "ster\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Geh auf denn, mein Licht! Geh wieder zur Ruh'!", "tokens": ["Geh", "auf", "denn", ",", "mein", "Licht", "!", "Geh", "wie\u00b7der", "zur", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "$,", "PPOSAT", "NN", "$.", "NE", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ich darf dich, du sch\u00f6nstes, nicht nennen;", "tokens": ["Ich", "darf", "dich", ",", "du", "sch\u00f6ns\u00b7tes", ",", "nicht", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "PPER", "ADJA", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch str\u00f6met mein Herze wie Wasser dir zu,", "tokens": ["Doch", "str\u00f6\u00b7met", "mein", "Her\u00b7ze", "wie", "Was\u00b7ser", "dir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "VVFIN", "KOKOM", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Hei\u00df f\u00fchl' ich es lodern und brennen!", "tokens": ["Hei\u00df", "f\u00fchl'", "ich", "es", "lo\u00b7dern", "und", "bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Auf! lasset es schmettern und klingen!", "tokens": ["Auf", "!", "las\u00b7set", "es", "schmet\u00b7tern", "und", "klin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Es will mich die Wehmut bezwingen.", "tokens": ["Es", "will", "mich", "die", "Weh\u00b7mut", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}