{"textgrid.poem.60821": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Manches gewagte Problem und die spr\u00f6desten Stoffe bew\u00e4ltigt", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manches gewagte Problem und die spr\u00f6desten Stoffe bew\u00e4ltigt", "tokens": ["Man\u00b7ches", "ge\u00b7wag\u00b7te", "Prob\u00b7lem", "und", "die", "spr\u00f6\u00b7des\u00b7ten", "Stof\u00b7fe", "be\u00b7w\u00e4l\u00b7tigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "VVPP"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Mein siegreicher Verstand, meine vollendete Kunst;", "tokens": ["Mein", "sieg\u00b7rei\u00b7cher", "Ver\u00b7stand", ",", "mei\u00b7ne", "voll\u00b7en\u00b7de\u00b7te", "Kunst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Doch mir mangelt geschichtlicher Sinn, ich entbehre der Griechen", "tokens": ["Doch", "mir", "man\u00b7gelt", "ge\u00b7schicht\u00b7li\u00b7cher", "Sinn", ",", "ich", "ent\u00b7beh\u00b7re", "der", "Grie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "---+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Milde zu sehr, mir fehlt Ruhe der Seele zu sehr.", "tokens": ["Mil\u00b7de", "zu", "sehr", ",", "mir", "fehlt", "Ru\u00b7he", "der", "See\u00b7le", "zu", "sehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "$,", "PPER", "VVFIN", "NN", "ART", "NN", "APPR", "ADV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}