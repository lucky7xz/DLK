{"dta.poem.435": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An einem so eine Jungfer geschw\u00e4ngert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund er griff zu starck das muntre Pferdgen an/", "tokens": ["Mein", "Freund", "er", "griff", "zu", "starck", "das", "mun\u00b7tre", "Pferd\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "PTKA", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sachen hat er auch dadurch zu viel gethau;", "tokens": ["Der", "Sa\u00b7chen", "hat", "er", "auch", "da\u00b7durch", "zu", "viel", "ge\u00b7thau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PAV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem Pferd/ so willig ist/ und das sich l\u00e4st besteigen/", "tokens": ["Dem", "Pferd", "/", "so", "wil\u00b7lig", "ist", "/", "und", "das", "sich", "l\u00e4st", "be\u00b7stei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADJD", "VAFIN", "$(", "KON", "PRELS", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df man die scharffe Sporn nicht geben/ sondern zeigen.", "tokens": ["Mu\u00df", "man", "die", "scharf\u00b7fe", "Sporn", "nicht", "ge\u00b7ben", "/", "so