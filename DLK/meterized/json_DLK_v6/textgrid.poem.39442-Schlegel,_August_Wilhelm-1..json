{"textgrid.poem.39442": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Jungfrau ruht, nur Demuth ihr Geschmeide,", "tokens": ["Die", "Jung\u00b7frau", "ruht", ",", "nur", "De\u00b7muth", "ihr", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Abendschatten an der H\u00fctte Thor.", "tokens": ["Im", "A\u00b7bend\u00b7schat\u00b7ten", "an", "der", "H\u00fct\u00b7te", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie wei\u00df nicht, da\u00df sie Gott zur Braut erkor,", "tokens": ["Sie", "wei\u00df", "nicht", ",", "da\u00df", "sie", "Gott", "zur", "Braut", "er\u00b7kor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch stilles Sinnen ist ihr Seelenweide.", "tokens": ["Doch", "stil\u00b7les", "Sin\u00b7nen", "ist", "ihr", "See\u00b7len\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sieh! ein J\u00fcngling tritt im lichten Kleide,", "tokens": ["Da", "sieh", "!", "ein", "J\u00fcng\u00b7ling", "tritt", "im", "lich\u00b7ten", "Klei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Palmenzweig in seiner Hand, hervor.", "tokens": ["Den", "Pal\u00b7men\u00b7zweig", "in", "sei\u00b7ner", "Hand", ",", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Voll s\u00fc\u00dfen Schauers bebet sie empor,", "tokens": ["Voll", "s\u00fc\u00b7\u00dfen", "Schau\u00b7ers", "be\u00b7bet", "sie", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Denn seine Stirn ist Morgenroth der Freude.", "tokens": ["Denn", "sei\u00b7ne", "Stirn", "ist", "Mor\u00b7gen\u00b7roth", "der", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gegr\u00fc\u00dft, Maria! t\u00f6nt sein holder Mund,", "tokens": ["Ge\u00b7gr\u00fc\u00dft", ",", "Ma\u00b7ria", "!", "t\u00f6nt", "sein", "hol\u00b7der", "Mund", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NE", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und thut das wundervolle Heil ihr kund,", "tokens": ["Und", "thut", "das", "wun\u00b7der\u00b7vol\u00b7le", "Heil", "ihr", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Kraft von oben her sie soll umwallen.", "tokens": ["Wie", "Kraft", "von", "o\u00b7ben", "her", "sie", "soll", "um\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADV", "APZR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und sie, die Arm' auf ihre Brust gelegt,", "tokens": ["Und", "sie", ",", "die", "Arm'", "auf", "ih\u00b7re", "Brust", "ge\u00b7legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo sich's geheim und innig liebend regt,", "tokens": ["Wo", "sich's", "ge\u00b7heim", "und", "in\u00b7nig", "lie\u00b7bend", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "KON", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Spricht: Mir geschehe nach des Herrn Gefallen.", "tokens": ["Spricht", ":", "Mir", "ge\u00b7sche\u00b7he", "nach", "des", "Herrn", "Ge\u00b7fal\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}