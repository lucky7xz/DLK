{"textgrid.poem.43304": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein holdes Wort, ein weicher Ton", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein holdes Wort, ein weicher Ton", "tokens": ["Ein", "hol\u00b7des", "Wort", ",", "ein", "wei\u00b7cher", "Ton"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zog seltsam durch mein Leben,", "tokens": ["Zog", "selt\u00b7sam", "durch", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Vollmondlicht als Knabe schon", "tokens": ["Im", "Voll\u00b7mond\u00b7licht", "als", "Kna\u00b7be", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KOUS", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt' ich sein leises Weben.", "tokens": ["H\u00f6rt'", "ich", "sein", "lei\u00b7ses", "We\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch j\u00e4hlings ist der Zauber fort,", "tokens": ["Doch", "j\u00e4h\u00b7lings", "ist", "der", "Zau\u00b7ber", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mich so lang umsponnen,", "tokens": ["Der", "mich", "so", "lang", "um\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der weiche Ton ... das holde Wort ...", "tokens": ["Der", "wei\u00b7che", "Ton", "...", "das", "hol\u00b7de", "Wort", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Vollmondlicht zerronnen.", "tokens": ["Im", "Voll\u00b7mond\u00b7licht", "zer\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}