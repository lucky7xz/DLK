{"textgrid.poem.26519": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frohlockt nur, da\u00df ihr mich bezwungen", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frohlockt nur, da\u00df ihr mich bezwungen", "tokens": ["Froh\u00b7lockt", "nur", ",", "da\u00df", "ihr", "mich", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach langem Kampf und Widersteh'n,", "tokens": ["Nach", "lan\u00b7gem", "Kampf", "und", "Wi\u00b7der\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frohlockt nur, da\u00df es euch gelungen,", "tokens": ["Froh\u00b7lockt", "nur", ",", "da\u00df", "es", "euch", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich jetzt in eu'rer Macht zu seh'n.", "tokens": ["Mich", "jetzt", "in", "eu'\u00b7rer", "Macht", "zu", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Tiefsten f\u00fchlt ihr dennoch leise,", "tokens": ["Im", "Tiefs\u00b7ten", "f\u00fchlt", "ihr", "den\u00b7noch", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es war ein falscher, halber Sieg \u2013", "tokens": ["Es", "war", "ein", "fal\u00b7scher", ",", "hal\u00b7ber", "Sieg", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Sieg zu des Besiegten Preise,", "tokens": ["Ein", "Sieg", "zu", "des", "Be\u00b7sieg\u00b7ten", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der durch den Fall nur h\u00f6her stieg.", "tokens": ["Der", "durch", "den", "Fall", "nur", "h\u00f6\u00b7her", "stieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn wollt ihr ferner auch behalten,", "tokens": ["Denn", "wollt", "ihr", "fer\u00b7ner", "auch", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich dahin euch endlich gab:", "tokens": ["Was", "ich", "da\u00b7hin", "euch", "end\u00b7lich", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So d\u00fcrft ihr nicht als Herren walten,", "tokens": ["So", "d\u00fcrft", "ihr", "nicht", "als", "Her\u00b7ren", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr m\u00fc\u00dft euch beugen tief hinab;", "tokens": ["Ihr", "m\u00fc\u00dft", "euch", "beu\u00b7gen", "tief", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hinab zu meinen F\u00fc\u00dfen wieder", "tokens": ["Hin\u00b7ab", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und scheue Demuth wird euch Pflicht \u2013", "tokens": ["Und", "scheu\u00b7e", "De\u00b7muth", "wird", "euch", "Pflicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst klirrt auf's neu' die Kette nieder:", "tokens": ["Sonst", "klirrt", "auf's", "neu'", "die", "Ket\u00b7te", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn meine Seele habt ihr nicht!", "tokens": ["Denn", "mei\u00b7ne", "See\u00b7le", "habt", "ihr", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}