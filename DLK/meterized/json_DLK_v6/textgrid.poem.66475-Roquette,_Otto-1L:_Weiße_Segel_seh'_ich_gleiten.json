{"textgrid.poem.66475": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wei\u00dfe Segel seh' ich gleiten", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dfe Segel seh' ich gleiten", "tokens": ["Wei\u00b7\u00dfe", "Se\u00b7gel", "seh'", "ich", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stromhinab im Windeshauch,", "tokens": ["Strom\u00b7hin\u00b7ab", "im", "Win\u00b7des\u00b7hauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, denselben Weg vor Zeiten", "tokens": ["Ach", ",", "den\u00b7sel\u00b7ben", "Weg", "vor", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PDAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog ich oft befl\u00fcgelt auch!", "tokens": ["Zog", "ich", "oft", "be\u00b7fl\u00fc\u00b7gelt", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fr\u00fchling lachte, Jugend bl\u00fchte,", "tokens": ["Fr\u00fch\u00b7ling", "lach\u00b7te", ",", "Ju\u00b7gend", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Erd und Himmel war voll Licht,", "tokens": ["Erd", "und", "Him\u00b7mel", "war", "voll", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn am holden Ziel ergl\u00fchte", "tokens": ["Wenn", "am", "hol\u00b7den", "Ziel", "er\u00b7gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir der Liebsten Angesicht.", "tokens": ["Mir", "der", "Liebs\u00b7ten", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gru\u00df und Ku\u00df, und ein Willkommen", "tokens": ["Gru\u00df", "und", "Ku\u00df", ",", "und", "ein", "Will\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unerme\u00dflich reicher Lust!", "tokens": ["Un\u00b7er\u00b7me\u00df\u00b7lich", "rei\u00b7cher", "Lust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Lebenskeime glommen", "tokens": ["Al\u00b7le", "Le\u00b7bens\u00b7kei\u00b7me", "glom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammend auf in junger Brust.", "tokens": ["Flam\u00b7mend", "auf", "in", "jun\u00b7ger", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "War's ein Traum? Entr\u00fcckt, zerflossen", "tokens": ["Wa\u00b7r's", "ein", "Traum", "?", "Ent\u00b7r\u00fcckt", ",", "zer\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "ART", "NN", "$.", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie in Todesnebel, schwand", "tokens": ["Wie", "in", "To\u00b7des\u00b7ne\u00b7bel", ",", "schwand"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "APPR", "NN", "$,", "VVFIN"], "meter": "--+---+", "measure": "anapaest.init"}, "line.7": {"text": "Licht und Leben! Aufgeschlossen", "tokens": ["Licht", "und", "Le\u00b7ben", "!", "Auf\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "KON", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "War der Schmerzen finstres Land.", "tokens": ["War", "der", "Schmer\u00b7zen", "finst\u00b7res", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bergesh\u00f6hn, die einst als Pforte", "tokens": ["Ber\u00b7ges\u00b7h\u00f6hn", ",", "die", "einst", "als", "Pfor\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldner Lust sich aufgebaut,", "tokens": ["Gold\u00b7ner", "Lust", "sich", "auf\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehen an demselben Orte", "tokens": ["Ste\u00b7hen", "an", "dem\u00b7sel\u00b7ben", "Or\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun als W\u00e4chter, kalt, ergraut.", "tokens": ["Nun", "als", "W\u00e4ch\u00b7ter", ",", "kalt", ",", "er\u00b7graut", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "NE", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fest gezogen sind die Grenzen,", "tokens": ["Fest", "ge\u00b7zo\u00b7gen", "sind", "die", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur ein Seufzer folget sacht", "tokens": ["Nur", "ein", "Seuf\u00b7zer", "fol\u00b7get", "sacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Den zerri\u00dfnen Jugendkr\u00e4nzen", "tokens": ["Den", "zer\u00b7ri\u00df\u00b7nen", "Ju\u00b7gend\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stromhinab in's Reich der Nacht.", "tokens": ["Strom\u00b7hin\u00b7ab", "in's", "Reich", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}