{"dta.poem.8321": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Arglistigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Laster h\u00fcllen sich oft in den Tu-\ngend-Schein,", "tokens": ["Die", "Las\u00b7ter", "h\u00fcl\u00b7len", "sich", "oft", "in", "den", "Tu", "gen\u00b7dSchein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "TRUNC", "NN", "$,"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Als wie ein gierger Wolf in Scha-", "tokens": ["Als", "wie", "ein", "gier\u00b7ger", "Wolf", "in", "Scha"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NE", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mensche pflegt den Glanz der", "tokens": ["Der", "Men\u00b7sche", "pflegt", "den", "Glanz", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allein er will nicht gern sich in derselben \u00fcben,", "tokens": ["Al\u00b7lein", "er", "will", "nicht", "gern", "sich", "in", "der\u00b7sel\u00b7ben", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PTKNEG", "ADV", "PRF", "APPR", "PDAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum nimmt er ihren Schmuk und zieht denselben", "tokens": ["Drum", "nimmt", "er", "ih\u00b7ren", "Schmuk", "und", "zieht", "den\u00b7sel\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "PDS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf das er ungescheut die Welt ber\u00fckken kan,", "tokens": ["Auf", "das", "er", "un\u00b7ge\u00b7scheut", "die", "Welt", "be\u00b7r\u00fck\u00b7ken", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Will das nicht mehr angehn, so spannt er ihren", "tokens": ["Will", "das", "nicht", "mehr", "an\u00b7gehn", ",", "so", "spannt", "er", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PDS", "PTKNEG", "ADV", "VVINF", "$,", "ADV", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als einen schimmernden mit Gold besezten Nah-", "tokens": ["Als", "ei\u00b7nen", "schim\u00b7mern\u00b7den", "mit", "Gold", "be\u00b7sez\u00b7ten", "Nah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "APPR", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Um seine Thaten her; so ist des Lasters Bild,", "tokens": ["Um", "sei\u00b7ne", "Tha\u00b7ten", "her", ";", "so", "ist", "des", "Las\u00b7ters", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ins sch\u00f6ne Aussenwerk der Tugend eingeh\u00fcllt.", "tokens": ["Ins", "sch\u00f6\u00b7ne", "Aus\u00b7sen\u00b7werk", "der", "Tu\u00b7gend", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Mode ist jezt so, man will von aussen gleissen,", "tokens": ["Die", "Mo\u00b7de", "ist", "jezt", "so", ",", "man", "will", "von", "aus\u00b7sen", "gleis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$,", "PIS", "VMFIN", "APPR", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was B\u00f6se ist soll gut, das Laster Tugend heissen.", "tokens": ["Was", "B\u00f6\u00b7se", "ist", "soll", "gut", ",", "das", "Las\u00b7ter", "Tu\u00b7gend", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "VMFIN", "ADJD", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein Beispiel giebet uns der Menschen arge List,", "tokens": ["Ein", "Bei\u00b7spiel", "gie\u00b7bet", "uns", "der", "Men\u00b7schen", "ar\u00b7ge", "List", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die in der jezgen Welt die weise Klugheit ist.", "tokens": ["Die", "in", "der", "jez\u00b7gen", "Welt", "die", "wei\u00b7se", "Klug\u00b7heit", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wer seinen Wiz gebraucht zu eines andern Scha-", "tokens": ["Wer", "sei\u00b7nen", "Wiz", "ge\u00b7braucht", "zu", "ei\u00b7nes", "an\u00b7dern", "Scha"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wer seine eigne Schuld kan anderen aufladen,", "tokens": ["Wer", "sei\u00b7ne", "eig\u00b7ne", "Schuld", "kan", "an\u00b7de\u00b7ren", "auf\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der heist ein kluger Mann, den lobt die Mode-", "tokens": ["Der", "heist", "ein", "klu\u00b7ger", "Mann", ",", "den", "lobt", "die", "Mo\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Weil ihr das Laster nur, die Klugheit nicht gef\u00e4llt,", "tokens": ["Weil", "ihr", "das", "Las\u00b7ter", "nur", ",", "die", "Klug\u00b7heit", "nicht", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es ist zu unsrer Zeit, die Secte der Ophiten, ", "tokens": ["Es", "ist", "zu", "uns\u00b7rer", "Zeit", ",", "die", "Sec\u00b7te", "der", "O\u00b7phi\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die sonst durchaus verdammt, bei vielen woll ge-", "tokens": ["Die", "sonst", "durc\u00b7haus", "ver\u00b7dammt", ",", "bei", "vie\u00b7len", "woll", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "VVPP", "$,", "APPR", "PIAT", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Man nimt den Lehrsaz an, da\u00df der ein Kluger heist,", "tokens": ["Man", "nimt", "den", "Lehr\u00b7saz", "an", ",", "da\u00df", "der", "ein", "Klu\u00b7ger", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Des Herze voll Betrug, von Gifte \u00fcberfleu\u00dft;", "tokens": ["Des", "Her\u00b7ze", "voll", "Be\u00b7trug", ",", "von", "Gif\u00b7te", "\u00fc\u00b7berf\u00b7leu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn man dadurch zum Ziel, das man gew\u00fcnscht,", "tokens": ["Wenn", "man", "da\u00b7durch", "zum", "Ziel", ",", "das", "man", "ge\u00b7w\u00fcnscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "APPRART", "NN", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "So wird die b\u00f6se That zum Meisterst\u00fck genommen,", "tokens": ["So", "wird", "die", "b\u00f6\u00b7se", "That", "zum", "Meis\u00b7ter\u00b7st\u00fck", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das kluger Wiz erdacht. Und r\u00e4umt man dieses", "tokens": ["Das", "klu\u00b7ger", "Wiz", "er\u00b7dacht", ".", "Und", "r\u00e4umt", "man", "die\u00b7ses"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "KON", "VVFIN", "PIS", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "So kan die Schlang die gl\u00e4nzt, ein Seraphine seyn,", "tokens": ["So", "kan", "die", "Schlang", "die", "gl\u00e4nzt", ",", "ein", "Se\u00b7ra\u00b7phi\u00b7ne", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "VVFIN", "$,", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So k\u00f6nnen die, die nur als falsche Schlangen", "tokens": ["So", "k\u00f6n\u00b7nen", "die", ",", "die", "nur", "als", "fal\u00b7sche", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "$,", "PRELS", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Insk\u00fcnftige nicht mehr des Teuffels Kinder heissen,", "tokens": ["Ins\u00b7k\u00fcnf\u00b7ti\u00b7ge", "nicht", "mehr", "des", "Teuf\u00b7fels", "Kin\u00b7der", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sie sind im Gegentheil als solche anzusehn,", "tokens": ["Sie", "sind", "im", "Ge\u00b7gen\u00b7theil", "als", "sol\u00b7che", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "KOUS", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die sich mit klugen Wiz, nach ihrem Ziele drehn,", "tokens": ["Die", "sich", "mit", "klu\u00b7gen", "Wiz", ",", "nach", "ih\u00b7rem", "Zie\u00b7le", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Darnach ein jeder strebt, und nach der Vorsicht", "tokens": ["Dar\u00b7nach", "ein", "je\u00b7der", "strebt", ",", "und", "nach", "der", "Vor\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ART", "PIS", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Als ein Vern\u00fcnftiger wird billig streben m\u00fcssen.", "tokens": ["Als", "ein", "Ver\u00b7n\u00fcnf\u00b7ti\u00b7ger", "wird", "bil\u00b7lig", "stre\u00b7ben", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Verdammte Sittenlehr! die diesen Grundsaz hegt,", "tokens": ["Ver\u00b7damm\u00b7te", "Sit\u00b7ten\u00b7lehr", "!", "die", "die\u00b7sen", "Grund\u00b7saz", "hegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den hat die alte Schlang, der Teuffel eingepr\u00e4gt,", "tokens": ["Den", "hat", "die", "al\u00b7te", "Schlang", ",", "der", "Teuf\u00b7fel", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df man als Mensch verpflicht, nach seinen Gl\u00fck", "tokens": ["Da\u00df", "man", "als", "Mensch", "ver\u00b7pflicht", ",", "nach", "sei\u00b7nen", "Gl\u00fck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "KOKOM", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Da\u00df andre neben uns in heissen Kummer schmachten.", "tokens": ["Da\u00df", "and\u00b7re", "ne\u00b7ben", "uns", "in", "heis\u00b7sen", "Kum\u00b7mer", "schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wir leben auf der Welt, wir m\u00fcssen uns bem\u00fchn,", "tokens": ["Wir", "le\u00b7ben", "auf", "der", "Welt", ",", "wir", "m\u00fcs\u00b7sen", "uns", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Durch Klugheit uns den Schlam des Elends zu", "tokens": ["Durch", "Klug\u00b7heit", "uns", "den", "Schlam", "des", "E\u00b7lends", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Wir m\u00fcssen uns mit Lust, die Kummer Tage w\u00fcr-", "tokens": ["Wir", "m\u00fcs\u00b7sen", "uns", "mit", "Lust", ",", "die", "Kum\u00b7mer", "Ta\u00b7ge", "w\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "$,", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch dadurch andre nicht in bittres Elend st\u00fcrzen.", "tokens": ["Doch", "da\u00b7durch", "and\u00b7re", "nicht", "in", "bitt\u00b7res", "E\u00b7lend", "st\u00fcr\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PIS", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Das denken viele nicht, drum \u00fcben sie Betrug,", "tokens": ["Das", "den\u00b7ken", "vie\u00b7le", "nicht", ",", "drum", "\u00fc\u00b7ben", "sie", "Be\u00b7trug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PTKNEG", "$,", "PAV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und meinen wer das k\u00f6nn, der sey nur wizzig, klug,", "tokens": ["Und", "mei\u00b7nen", "wer", "das", "k\u00f6nn", ",", "der", "sey", "nur", "wiz\u00b7zig", ",", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "PWS", "PDS", "VMFIN", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die treue Redlichkeit die keinen Mensch betr\u00fcbet,", "tokens": ["Die", "treu\u00b7e", "Red\