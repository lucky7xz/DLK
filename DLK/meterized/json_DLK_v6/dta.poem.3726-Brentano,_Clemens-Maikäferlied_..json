{"dta.poem.3726": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Maik\u00e4ferlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Maik\u00e4fercher, Maik\u00e4ferchen, fliege weg!               ", "tokens": ["Mai\u00b7k\u00e4\u00b7fer\u00b7cher", ",", "Mai\u00b7k\u00e4\u00b7fer\u00b7chen", ",", "flie\u00b7ge", "weg", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Dein H\u00e4usgen brennt,", "tokens": ["Dein", "H\u00e4us\u00b7gen", "brennt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dein M\u00fctterchen flennt,", "tokens": ["Dein", "M\u00fct\u00b7ter\u00b7chen", "flennt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dein Vater sitzt auf der Schwelle,", "tokens": ["Dein", "Va\u00b7ter", "sitzt", "auf", "der", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Flieg in Himmel aus der H\u00f6lle.", "tokens": ["Flieg", "in", "Him\u00b7mel", "aus", "der", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}