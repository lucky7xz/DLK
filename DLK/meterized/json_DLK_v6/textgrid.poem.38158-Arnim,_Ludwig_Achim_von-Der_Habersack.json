{"textgrid.poem.38158": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Habersack", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wollt ihr h\u00f6ren singen,", "tokens": ["Und", "wollt", "ihr", "h\u00f6\u00b7ren", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich sing ein neues Lied,", "tokens": ["Ich", "sing", "ein", "neu\u00b7es", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von einem feinen Fr\u00e4ulein,", "tokens": ["Von", "ei\u00b7nem", "fei\u00b7nen", "Fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Und wie es dem ergieng,", "tokens": ["Und", "wie", "es", "dem", "er\u00b7gieng", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie war genannt der Habersack,", "tokens": ["Sie", "war", "ge\u00b7nannt", "der", "Ha\u00b7ber\u00b7sack", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott geb ihr einen guten Morgen,", "tokens": ["Gott", "geb", "ihr", "ei\u00b7nen", "gu\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und einen guten Tag,", "tokens": ["Und", "ei\u00b7nen", "gu\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Tag und Tag und aber Tag", "tokens": ["Tag", "und", "Tag", "und", "a\u00b7ber", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der ich heut Nacht sprach.", "tokens": ["Mit", "der", "ich", "heut", "Nacht", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Fr\u00e4ulein, das war weise,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", ",", "das", "war", "wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seinen Worten klug,", "tokens": ["Mit", "sei\u00b7nen", "Wor\u00b7ten", "klug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie bald nahm sie den Habersack,", "tokens": ["Wie", "bald", "nahm", "sie", "den", "Ha\u00b7ber\u00b7sack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn zu der M\u00fchle trug,", "tokens": ["Ihn", "zu", "der", "M\u00fch\u00b7le", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun seh, du lieber M\u00fcller mein,", "tokens": ["Nun", "seh", ",", "du", "lie\u00b7ber", "M\u00fcl\u00b7ler", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "ADV", "NE", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Haber sollst du mahlen,", "tokens": ["Den", "Ha\u00b7ber", "sollst", "du", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohl um den Willen mein,", "tokens": ["Wohl", "um", "den", "Wil\u00b7len", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein und mein und aber dein,", "tokens": ["Dein", "und", "mein", "und", "a\u00b7ber", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "PPOSAT", "KON", "ADV", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es soll verschwiegen sein.", "tokens": ["Es", "soll", "ver\u00b7schwie\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der M\u00fcller nahm den Haber", "tokens": ["Der", "M\u00fcl\u00b7ler", "nahm", "den", "Ha\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sch\u00fctt ihn auf die Rell,", "tokens": ["Und", "sch\u00fctt", "ihn", "auf", "die", "Rell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er konnt ihn nie gemahlen,", "tokens": ["Er", "konnt", "ihn", "nie", "ge\u00b7mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es war sein Ungef\u00e4ll,", "tokens": ["Es", "war", "sein", "Un\u00b7ge\u00b7f\u00e4ll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er mahlt die Nacht, bis an den Tag,", "tokens": ["Er", "mahlt", "die", "Nacht", ",", "bis", "an", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott geb ihm einen guten Morgen,", "tokens": ["Gott", "geb", "ihm", "ei\u00b7nen", "gu\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und einen guten Tag,", "tokens": ["Und", "ei\u00b7nen", "gu\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Tag und Tag und aber Tag,", "tokens": ["Tag", "und", "Tag", "und", "a\u00b7ber", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der ich heut Nacht sprach.", "tokens": ["Mit", "der", "ich", "heut", "Nacht", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Der M\u00fcller nahm die Stiefel,", "tokens": ["Der", "M\u00fcl\u00b7ler", "nahm", "die", "Stie\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Streift sie an seine Bein,", "tokens": ["Streift", "sie", "an", "sei\u00b7ne", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er gieng die Gassen auf und ab,", "tokens": ["Er", "gieng", "die", "Gas\u00b7sen", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sang ein Liedlein klein,", "tokens": ["Und", "sang", "ein", "Lied\u00b7lein", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sang ein Lied vom Habersack,", "tokens": ["Er", "sang", "ein", "Lied", "vom", "Ha\u00b7ber\u00b7sack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott geb ihr ein guten Morgen,", "tokens": ["Gott", "geb", "ihr", "ein", "gu\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und einen guten Tag,", "tokens": ["Und", "ei\u00b7nen", "gu\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Tag und Tag, und aber Tag,", "tokens": ["Tag", "und", "Tag", ",", "und", "a\u00b7ber", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der ich heut Nacht sprach.", "tokens": ["Mit", "der", "ich", "heut", "Nacht", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Das h\u00f6rt des M\u00fcllers Knechte", "tokens": ["Das", "h\u00f6rt", "des", "M\u00fcl\u00b7lers", "Knech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In seinem K\u00e4mmerlein,", "tokens": ["In", "sei\u00b7nem", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er dacht in seinem Sinne,", "tokens": ["Er", "dacht", "in", "sei\u00b7nem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es w\u00e4r ein Fr\u00e4ulein fein,", "tokens": ["Es", "w\u00e4r", "ein", "Fr\u00e4u\u00b7lein", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es w\u00e4r ein Fr\u00e4ulein minniglich,", "tokens": ["Es", "w\u00e4r", "ein", "Fr\u00e4u\u00b7lein", "min\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wollt Gott sollt ich sie schauen,", "tokens": ["Wollt", "Gott", "sollt", "ich", "sie", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohl durch den Willen mein,", "tokens": ["Wohl", "durch", "den", "Wil\u00b7len", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dein und mein und aber dein,", "tokens": ["Dein", "und", "mein", "und", "a\u00b7ber", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "PPOSAT", "KON", "ADV", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es sollt verschwiegen sein.", "tokens": ["Es", "sollt", "ver\u00b7schwie\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}