{"textgrid.poem.36832": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In solcher Einsamkeit, wer sollt' es meinen,", "tokens": ["In", "sol\u00b7cher", "Ein\u00b7sam\u00b7keit", ",", "wer", "sollt'", "es", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWS", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df mir zuweilen auch der heitre Eros,", "tokens": ["Da\u00df", "mir", "zu\u00b7wei\u00b7len", "auch", "der", "heit\u00b7re", "E\u00b7ros", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und alle Grazien l\u00e4chelnd mir erscheinen.", "tokens": ["Und", "al\u00b7le", "Gra\u00b7zi\u00b7en", "l\u00e4\u00b7chelnd", "mir", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "J\u00fcngst fuhr ich von Parthenope her\u00fcber,", "tokens": ["J\u00fcngst", "fuhr", "ich", "von", "Par\u00b7the\u00b7no\u00b7pe", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "ADV", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und sieh im engen schweren Capriboote", "tokens": ["Und", "sieh", "im", "en\u00b7gen", "schwe\u00b7ren", "Cap\u00b7ri\u00b7boo\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sa\u00df eine sch\u00f6ne Frau mir gegen\u00fcber.", "tokens": ["Sa\u00df", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Frau", "mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar sah ich meist hinab in Fluth und Wogen,", "tokens": ["Zwar", "sah", "ich", "meist", "hin\u00b7ab", "in", "Fluth", "und", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch l\u00e4ugn' ich nicht, da\u00df manchmal meine Augen", "tokens": ["Doch", "l\u00e4ugn'", "ich", "nicht", ",", "da\u00df", "manch\u00b7mal", "mei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geheime Lust aus ihren Blicken sogen.", "tokens": ["Ge\u00b7hei\u00b7me", "Lust", "aus", "ih\u00b7ren", "Bli\u00b7cken", "so\u00b7gen."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "XY"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und mu\u00dft' ich mich vom Sonnenglanze wenden,", "tokens": ["Und", "mu\u00dft'", "ich", "mich", "vom", "Son\u00b7nen\u00b7glan\u00b7ze", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn's Meer ihn wiederstrahlt, begann auch wieder", "tokens": ["Wenn's", "Meer", "ihn", "wie\u00b7der\u00b7strahlt", ",", "be\u00b7gann", "auch", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr holdes Aetherauge mich zu blenden.", "tokens": ["Ihr", "hol\u00b7des", "A\u00b7e\u00b7ther\u00b7au\u00b7ge", "mich", "zu", "blen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Und nicht so sch\u00f6n erhoben sich die Wellen,", "tokens": ["Und", "nicht", "so", "sch\u00f6n", "er\u00b7ho\u00b7ben", "sich", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sanken, als ich ihren jungen Busen,", "tokens": ["Und", "san\u00b7ken", ",", "als", "ich", "ih\u00b7ren", "jun\u00b7gen", "Bu\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das d\u00fcnne Kleid sah auf und nieder schwellen.", "tokens": ["Das", "d\u00fcn\u00b7ne", "Kleid", "sah", "auf", "und", "nie\u00b7der", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wir sprachen viel, doch eben nur vom Winde,", "tokens": ["Wir", "spra\u00b7chen", "viel", ",", "doch", "e\u00b7ben", "nur", "vom", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sahn die Fische h\u00fcpfen \u00fcbers Wasser,", "tokens": ["Wir", "sahn", "die", "Fi\u00b7sche", "h\u00fcp\u00b7fen", "\u00fc\u00b7bers", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich lachte wohl auch mit dem h\u00fcbschen Kinde.", "tokens": ["Ich", "lach\u00b7te", "wohl", "auch", "mit", "dem", "h\u00fcb\u00b7schen", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Es kam die Nacht, und sie verschwand im Dunkel,", "tokens": ["Es", "kam", "die", "Nacht", ",", "und", "sie", "ver\u00b7schwand", "im", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir freuten uns mit jedem Ruderschlage", "tokens": ["Wir", "freu\u00b7ten", "uns", "mit", "je\u00b7dem", "Ru\u00b7der\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt an des Meeres strahlendem Gefunkel.", "tokens": ["Jetzt", "an", "des", "Mee\u00b7res", "strah\u00b7len\u00b7dem", "Ge\u00b7fun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Da breitete die schwarzen j\u00e4hen W\u00e4nde", "tokens": ["Da", "brei\u00b7te\u00b7te", "die", "schwar\u00b7zen", "j\u00e4\u00b7hen", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Felseneiland um uns aus, \u2013 wir sahen", "tokens": ["Das", "Fel\u00b7sen\u00b7ei\u00b7land", "um", "uns", "aus", ",", "\u2013", "wir", "sa\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Strandes Lichter, unsres Weges Ende.", "tokens": ["Des", "Stran\u00b7des", "Lich\u00b7ter", ",", "uns\u00b7res", "We\u00b7ges", "En\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Schon h\u00f6rte sie des frohen Vaters Rufen,", "tokens": ["Schon", "h\u00f6r\u00b7te", "sie", "des", "fro\u00b7hen", "Va\u00b7ters", "Ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der alte Fischer schlie\u00dft sie in die Arme,", "tokens": ["Der", "al\u00b7te", "Fi\u00b7scher", "schlie\u00dft", "sie", "in", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun gute Nacht! Und meine Felsenstufen", "tokens": ["Nun", "gu\u00b7te", "Nacht", "!", "Und", "mei\u00b7ne", "Fel\u00b7sen\u00b7stu\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wandr' ich empor mit ungetr\u00fcbtem Sinne:", "tokens": ["Wandr'", "ich", "em\u00b7por", "mit", "un\u00b7ge\u00b7tr\u00fcb\u00b7tem", "Sin\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zwar es verliert, wer Kraft hat zu entsagen,", "tokens": ["Zwar", "es", "ver\u00b7liert", ",", "wer", "Kraft", "hat", "zu", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "PWS", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch leicht ist der Verlust vor dem Gewinne.", "tokens": ["Doch", "leicht", "ist", "der", "Ver\u00b7lust", "vor", "dem", "Ge\u00b7win\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}