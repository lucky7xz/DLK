{"textgrid.poem.60123": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn in G\u00e4rten voller Pracht", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn in G\u00e4rten voller Pracht", "tokens": ["Wenn", "in", "G\u00e4r\u00b7ten", "vol\u00b7ler", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Rose nicht, umgl\u00e4nzt von Tulpen, lacht,", "tokens": ["Auch", "die", "Ro\u00b7se", "nicht", ",", "um\u00b7gl\u00e4nzt", "von", "Tul\u00b7pen", ",", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "$,", "VVPP", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "O so giebts an allen B\u00e4chen", "tokens": ["O", "so", "giebts", "an", "al\u00b7len", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frischer Blumen gnug zu brechen;", "tokens": ["Fri\u00b7scher", "Blu\u00b7men", "gnug", "zu", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und im Kranz der Freude stehn", "tokens": ["Und", "im", "Kranz", "der", "Freu\u00b7de", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch die Wiesenbl\u00fcmchen sch\u00f6n.", "tokens": ["Auch", "die", "Wie\u00b7sen\u00b7bl\u00fcm\u00b7chen", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}