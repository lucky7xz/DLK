{"textgrid.poem.40149": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Frieden der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist der letzte Funken hingeschwommen,", "tokens": ["Nun", "ist", "der", "letz\u00b7te", "Fun\u00b7ken", "hin\u00b7ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der um die schwarzen Wipfel noch gespielt, \u2013", "tokens": ["der", "um", "die", "schwar\u00b7zen", "Wip\u00b7fel", "noch", "ge\u00b7spielt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADV", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "im Strome auch der letzte Schein verglommen,", "tokens": ["im", "Stro\u00b7me", "auch", "der", "letz\u00b7te", "Schein", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "den zitternd noch die Welle hielt.", "tokens": ["den", "zit\u00b7ternd", "noch", "die", "Wel\u00b7le", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Tief im Gefild die blauen Nebel schleichen,", "tokens": ["Tief", "im", "Ge\u00b7fild", "die", "blau\u00b7en", "Ne\u00b7bel", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "verm\u00e4hlen Erd' und Himmel sich im Duft; \u2013", "tokens": ["ver\u00b7m\u00e4h\u00b7len", "Erd'", "und", "Him\u00b7mel", "sich", "im", "Duft", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PRF", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und wiegend tauscht die Pappel ihre weichen", "tokens": ["und", "wie\u00b7gend", "tauscht", "die", "Pap\u00b7pel", "ih\u00b7re", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "schwerm\u00fct'gen K\u00fcsse mit der Luft.", "tokens": ["schwer\u00b7m\u00fct'\u00b7gen", "K\u00fcs\u00b7se", "mit", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchle lauschend meine Pulse stocken,", "tokens": ["Ich", "f\u00fch\u00b7le", "lau\u00b7schend", "mei\u00b7ne", "Pul\u00b7se", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "als str\u00f6mt' ich in die Finsternis hinein;", "tokens": ["als", "str\u00f6mt'", "ich", "in", "die", "Fins\u00b7ter\u00b7nis", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Nachtwind taucht mir in die feuchten Locken,", "tokens": ["der", "Nacht\u00b7wind", "taucht", "mir", "in", "die", "feuch\u00b7ten", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und meine Seele schlummert ein.", "tokens": ["und", "mei\u00b7ne", "See\u00b7le", "schlum\u00b7mert", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}