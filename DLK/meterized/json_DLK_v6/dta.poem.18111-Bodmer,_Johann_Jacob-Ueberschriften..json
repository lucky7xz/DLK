{"dta.poem.18111": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Ueberschriften.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fleuch wo dir hingeliebt, wohin du nur kanst kommen,", "tokens": ["Fleuch", "wo", "dir", "hin\u00b7ge\u00b7liebt", ",", "wo\u00b7hin", "du", "nur", "kanst", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Fleuch mein Gem\u00fcthe, fleuch Luft, Feuer, Wasser, Erd,", "tokens": ["Fleuch", "mein", "Ge\u00b7m\u00fc\u00b7the", ",", "fleuch", "Luft", ",", "Feu\u00b7er", ",", "Was\u00b7ser", ",", "Erd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du magst doch nicht entgehn, dein Vorsatz wird verkehrt,", "tokens": ["Du", "magst", "doch", "nicht", "ent\u00b7gehn", ",", "dein", "Vor\u00b7satz", "wird", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil dich mein Lieb in sich schon g\u00e4ntzlich eingenommen.", "tokens": ["Weil", "dich", "mein", "Lieb", "in", "sich", "schon", "g\u00e4ntz\u00b7lich", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}