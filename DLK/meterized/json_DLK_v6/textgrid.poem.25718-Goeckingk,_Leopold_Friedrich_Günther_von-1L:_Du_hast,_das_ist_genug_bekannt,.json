{"textgrid.poem.25718": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast, das ist genug bekannt,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast, das ist genug bekannt,", "tokens": ["Du", "hast", ",", "das", "ist", "ge\u00b7nug", "be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den armen Dichtern selten", "tokens": ["Den", "ar\u00b7men", "Dich\u00b7tern", "sel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ge\u00f6ffnet deine milde Hand,", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "dei\u00b7ne", "mil\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch oft den R\u00fccken zugewandt,", "tokens": ["Doch", "oft", "den", "R\u00fc\u00b7cken", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz Bitten oder Schelten!", "tokens": ["Trotz", "Bit\u00b7ten", "o\u00b7der", "Schel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch ich, ich habe wahrlich dir", "tokens": ["Auch", "ich", ",", "ich", "ha\u00b7be", "wahr\u00b7lich", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PPER", "VAFIN", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sehr wenig zu verdanken;", "tokens": ["Sehr", "we\u00b7nig", "zu", "ver\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch, la\u00df nur, was ich habe, mir,", "tokens": ["Doch", ",", "la\u00df", "nur", ",", "was", "ich", "ha\u00b7be", ",", "mir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "VVIMP", "ADV", "$,", "PWS", "PPER", "VAFIN", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dann will ich, liebes Gl\u00fcck, mit dir", "tokens": ["Dann", "will", "ich", ",", "lie\u00b7bes", "Gl\u00fcck", ",", "mit", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mich um den Rest nicht zanken.", "tokens": ["Mich", "um", "den", "Rest", "nicht", "zan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}