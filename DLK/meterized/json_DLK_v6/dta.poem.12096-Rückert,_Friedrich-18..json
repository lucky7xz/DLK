{"dta.poem.12096": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Weg am Berg empor beschlie\u00dft ein Gitterthor,", "tokens": ["Den", "Weg", "am", "Berg", "em\u00b7por", "be\u00b7schlie\u00dft", "ein", "Git\u00b7ter\u00b7thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur schwankend angelehnt; ein Bettler sitzt davor.", "tokens": ["Nur", "schwan\u00b7kend", "an\u00b7ge\u00b7lehnt", ";", "ein", "Bett\u00b7ler", "sitzt", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$.", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er bettelt nicht, gelehnt auf seinen Bettlerstab,", "tokens": ["Er", "bet\u00b7telt", "nicht", ",", "ge\u00b7lehnt", "auf", "sei\u00b7nen", "Bett\u00b7lers\u00b7tab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Betschnur K\u00fcgelchen betet er schweigend ab.", "tokens": ["Der", "Betsc\u00b7hnur", "K\u00fc\u00b7gel\u00b7chen", "be\u00b7tet", "er", "schwei\u00b7gend", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Er schaut nicht, sondern horcht, denn sein Gesicht ist blind,", "tokens": ["Er", "schaut", "nicht", ",", "son\u00b7dern", "horcht", ",", "denn", "sein", "Ge\u00b7sicht", "ist", "blind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KON", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob sich ein Fu\u00dftritt naht, dann hebt er sich geschwind.", "tokens": ["Ob", "sich", "ein", "Fu\u00df\u00b7tritt", "naht", ",", "dann", "hebt", "er", "sich", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dem Wandrer \u00f6ffnet er die beigelehnte Pforte;", "tokens": ["Dem", "Wand\u00b7rer", "\u00f6ff\u00b7net", "er", "die", "bei\u00b7ge\u00b7lehn\u00b7te", "Pfor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Wandrer geht hindurch, und jener bleibt am Orte.", "tokens": ["Der", "Wand\u00b7rer", "geht", "hin\u00b7durch", ",", "und", "je\u00b7ner", "bleibt", "am", "Or\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "KON", "PDS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch gibst du ihm ein klein Almosen, sagt er drauf:", "tokens": ["Doch", "gibst", "du", "ihm", "ein", "klein", "Al\u00b7mo\u00b7sen", ",", "sagt", "er", "drauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ART", "ADJD", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So thue Gott dir einst das Paradiesthor auf!", "tokens": ["So", "thue", "Gott", "dir", "einst", "das", "Pa\u00b7ra\u00b7diest\u00b7hor", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch wenn du nichts ihm gibst, so sagt er nicht ein Wort,", "tokens": ["Doch", "wenn", "du", "nichts", "ihm", "gibst", ",", "so", "sagt", "er", "nicht", "ein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ohne Segen gehst du von dem Bettler fort.", "tokens": ["Und", "oh\u00b7ne", "Se\u00b7gen", "gehst", "du", "von", "dem", "Bett\u00b7ler", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}