{"textgrid.poem.49585": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsahnung", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrgott, ich rieche Fr\u00fchlingsluft!", "tokens": ["Herr\u00b7gott", ",", "ich", "rie\u00b7che", "Fr\u00fch\u00b7lings\u00b7luft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Es liegt so was wie Veilchenduft", "tokens": ["Es", "liegt", "so", "was", "wie", "Veil\u00b7chen\u00b7duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um alle gr\u00fcnen Str\u00e4uche.", "tokens": ["Um", "al\u00b7le", "gr\u00fc\u00b7nen", "Str\u00e4u\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt kommen vor die Ladent\u00fcr", "tokens": ["Jetzt", "kom\u00b7men", "vor", "die", "La\u00b7den\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kr\u00e4mersleute all herf\u00fcr", "tokens": ["Die", "Kr\u00e4\u00b7mers\u00b7leu\u00b7te", "all", "her\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00e4rmen sich die B\u00e4uche.", "tokens": ["Und", "w\u00e4r\u00b7men", "sich", "die", "B\u00e4u\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun hat die Sonne wieder Kraft.", "tokens": ["Nun", "hat", "die", "Son\u00b7ne", "wie\u00b7der", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist die Zeit der Leidenschaft,", "tokens": ["Das", "ist", "die", "Zeit", "der", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo alle B\u00f6cklein springen.", "tokens": ["Wo", "al\u00b7le", "B\u00f6c\u00b7klein", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will mir ein M\u00e4dchen gn\u00e4dig sein,", "tokens": ["Will", "mir", "ein", "M\u00e4d\u00b7chen", "gn\u00e4\u00b7dig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann k\u00f6nnt es auch dem Dichterlein,", "tokens": ["Dann", "k\u00f6nnt", "es", "auch", "dem", "Dich\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Dichterlein gelingen.", "tokens": ["Dem", "Dich\u00b7ter\u00b7lein", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Teufel wei\u00df, woran das liegt,", "tokens": ["Der", "Teu\u00b7fel", "wei\u00df", ",", "wo\u00b7ran", "das", "liegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df uns die Lust am Zipfel kriegt", "tokens": ["Da\u00df", "uns", "die", "Lust", "am", "Zip\u00b7fel", "kriegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In diesen Fr\u00fchlingszeiten.", "tokens": ["In", "die\u00b7sen", "Fr\u00fch\u00b7lings\u00b7zei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja selbst ein sanfter Mensch wird keck,", "tokens": ["Ja", "selbst", "ein", "sanf\u00b7ter", "Mensch", "wird", "keck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich k\u00f6nnte jetzt ein lieber Schneck", "tokens": ["Mich", "k\u00f6nn\u00b7te", "jetzt", "ein", "lie\u00b7ber", "Schneck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum d\u00fcmmsten Streich verleiten.", "tokens": ["Zum", "d\u00fcmms\u00b7ten", "Streich", "ver\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch wenn es so geschehen m\u00fc\u00dft,", "tokens": ["Doch", "wenn", "es", "so", "ge\u00b7sche\u00b7hen", "m\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df mich kein ledig M\u00e4dchen k\u00fc\u00dft,", "tokens": ["Da\u00df", "mich", "kein", "le\u00b7dig", "M\u00e4d\u00b7chen", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dann ist das allerbeste:", "tokens": ["Dann", "ist", "das", "al\u00b7ler\u00b7bes\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich h\u00fcpf um eines andern Frau", "tokens": ["Ich", "h\u00fcpf", "um", "ei\u00b7nes", "an\u00b7dern", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lebe wie der Kuckuck schlau", "tokens": ["Und", "le\u00b7be", "wie", "der", "Ku\u00b7ckuck", "schlau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergn\u00fcgt im fremden Neste.", "tokens": ["Ver\u00b7gn\u00fcgt", "im", "frem\u00b7den", "Nes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}