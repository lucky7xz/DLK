{"textgrid.poem.48123": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Die Gipfel gl\u00fchen", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und aufw\u00e4rts geht es Schritt vor Schritt,", "tokens": ["Und", "auf\u00b7w\u00e4rts", "geht", "es", "Schritt", "vor", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bei Hunderttausend schreiten mit;", "tokens": ["bei", "Hun\u00b7dert\u00b7tau\u00b7send", "schrei\u00b7ten", "mit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in qualendunkle Seelen bricht", "tokens": ["in", "qua\u00b7len\u00b7dunk\u00b7le", "See\u00b7len", "bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der H\u00f6he klares Sonnenlicht:", "tokens": ["der", "H\u00f6\u00b7he", "kla\u00b7res", "Son\u00b7nen\u00b7licht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die ", "tokens": ["die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Wir lagen tief in Not und Schmach,", "tokens": ["Wir", "la\u00b7gen", "tief", "in", "Not", "und", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bis unsres Wesens Siegel brach,", "tokens": ["bis", "uns\u00b7res", "We\u00b7sens", "Sie\u00b7gel", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "bis \u00fcber uns der Schutt zerstob", "tokens": ["bis", "\u00fc\u00b7ber", "uns", "der", "Schutt", "zer\u00b7stob"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da reckt empor aus dunkler Haft", "tokens": ["Da", "reckt", "em\u00b7por", "aus", "dunk\u00b7ler", "Haft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ihr blondes Haupt die Volkeskraft;", "tokens": ["ihr", "blon\u00b7des", "Haupt", "die", "Vol\u00b7kes\u00b7kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da f\u00fchlten wir an Schlag und Sto\u00df,", "tokens": ["da", "f\u00fchl\u00b7ten", "wir", "an", "Schlag", "und", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "am Strom, der hei\u00df zum Herzen scho\u00df,", "tokens": ["am", "Strom", ",", "der", "hei\u00df", "zum", "Her\u00b7zen", "scho\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Blut in den Adern.", "tokens": ["Blut", "in", "den", "A\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Da lebten wir die schwere Zeit.", "tokens": ["Da", "leb\u00b7ten", "wir", "die", "schwe\u00b7re", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein harter Lehrherr war das Leid:", "tokens": ["Ein", "har\u00b7ter", "Lehr\u00b7herr", "war", "das", "Leid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "es lehrte uns zusammenstehn,", "tokens": ["es", "lehr\u00b7te", "uns", "zu\u00b7sam\u00b7men\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in klirrenden Ketten vorw\u00e4rtsgehn,", "tokens": ["in", "klir\u00b7ren\u00b7den", "Ket\u00b7ten", "vor\u00b7w\u00e4rts\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "uns b\u00e4umen und wehren!", "tokens": ["uns", "b\u00e4u\u00b7men", "und", "weh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Und rasch verrauschte Jahr um Jahr,", "tokens": ["Und", "rasch", "ver\u00b7rauschte", "Jahr", "um", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "verklungen ist, was damals war.", "tokens": ["ver\u00b7klun\u00b7gen", "ist", ",", "was", "da\u00b7mals", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Wundgepeitschte st\u00f6hnten, gellt", "tokens": ["Wo", "Wund\u00b7ge\u00b7peitschte", "st\u00f6hn\u00b7ten", ",", "gellt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "NN", "VVFIN", "$,", "VVPP"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "ein Tubaton durch blaches Feld,", "tokens": ["ein", "Tu\u00b7ba\u00b7ton", "durch", "bla\u00b7ches", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "eine Siegesfanfare.", "tokens": ["ei\u00b7ne", "Sie\u00b7ge\u00b7sfan\u00b7fa\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Heut stehn wir da: in stolzer Wehr,", "tokens": ["Heut", "stehn", "wir", "da", ":", "in", "stol\u00b7zer", "Wehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein kriegsbereit Millionenheer \u2013", "tokens": ["ein", "kriegs\u00b7be\u00b7reit", "Mil\u00b7li\u00b7o\u00b7nen\u00b7heer", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Statt Speer und B\u00fcchse Pflug und Beil,", "tokens": ["Statt", "Speer", "und", "B\u00fcch\u00b7se", "Pflug", "und", "Beil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Zukunft Trost, der Menschheit Heil", "tokens": ["der", "Zu\u00b7kunft", "Trost", ",", "der", "Menschheit", "Heil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "in starken H\u00e4nden!", "tokens": ["in", "star\u00b7ken", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Das Heer der Arbeit! ", "tokens": ["Das", "Heer", "der", "Ar\u00b7beit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wie anders kam's, als ihr gedacht:", "tokens": ["Wie", "an\u00b7ders", "kam's", ",", "als", "ihr", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schicksal sprach sein Donnerwort,", "tokens": ["Das", "Schick\u00b7sal", "sprach", "sein", "Don\u00b7ner\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und \u00fcber eure K\u00f6pfe fort", "tokens": ["und", "\u00fc\u00b7ber", "eu\u00b7re", "K\u00f6p\u00b7fe", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "grollt seine Stimme.", "tokens": ["grollt", "sei\u00b7ne", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Sein ist der Blitz, der z\u00fcngelnd brennt;", "tokens": ["Sein", "ist", "der", "Blitz", ",", "der", "z\u00fcn\u00b7gelnd", "brennt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er schl\u00e4gt in euer Parlament,", "tokens": ["er", "schl\u00e4gt", "in", "eu\u00b7er", "Par\u00b7la\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "er fri\u00dft, was faul und modrig war, \u2013", "tokens": ["er", "fri\u00dft", ",", "was", "faul", "und", "mod\u00b7rig", "war", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "durch Rauch und D\u00fcnste ringt sich klar", "tokens": ["durch", "Rauch", "und", "D\u00fcns\u00b7te", "ringt", "sich", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die ewige Sonne.", "tokens": ["die", "e\u00b7wi\u00b7ge", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Und vorw\u00e4rts geht's mit hartem Schritt,", "tokens": ["Und", "vor\u00b7w\u00e4rts", "geht's", "mit", "har\u00b7tem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und Millionen schreiten mit.", "tokens": ["und", "Mil\u00b7lion\u00b7en", "schrei\u00b7ten", "mit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "aus hei\u00dfen Kehlen bricht ein Schrei:", "tokens": ["aus", "hei\u00b7\u00dfen", "Keh\u00b7len", "bricht", "ein", "Schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das Ziel so nah \u2013 so licht \u2013 so frei!", "tokens": ["das", "Ziel", "so", "nah", "\u2013", "so", "licht", "\u2013", "so", "frei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$(", "ADV", "ADJD", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Gipfel gl\u00fchen!", "tokens": ["Die", "Gip\u00b7fel", "gl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}