{"textgrid.poem.57288": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Goldener Traum, du, den ich nie nicht erf\u00fcllt seh,", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldener Traum, du, den ich nie nicht erf\u00fcllt seh,", "tokens": ["Gol\u00b7de\u00b7ner", "Traum", ",", "du", ",", "den", "ich", "nie", "nicht", "er\u00b7f\u00fcllt", "seh", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVPP", "VVFIN", "$,"], "meter": "+--+-+-+--++", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Strahlengestalt, wie der Tag sch\u00f6n, wenn er aufwacht,", "tokens": ["Strah\u00b7len\u00b7ge\u00b7stalt", ",", "wie", "der", "Tag", "sch\u00f6n", ",", "wenn", "er", "auf\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+--+--++-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Kom du dennoch zur\u00fcck, und schwebe", "tokens": ["Kom", "du", "den\u00b7noch", "zu\u00b7r\u00fcck", ",", "und", "schwe\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir vor dem trunkenen Blick!", "tokens": ["Mir", "vor", "dem", "trun\u00b7ke\u00b7nen", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Decken sie denn Kronen umsonst, dass des Traumes", "tokens": ["De\u00b7cken", "sie", "denn", "Kro\u00b7nen", "um\u00b7sonst", ",", "dass", "des", "Trau\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "NN", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Himlisches Bild sie ins Daseyn nicht verwandeln?", "tokens": ["Him\u00b7li\u00b7sches", "Bild", "sie", "ins", "Da\u00b7seyn", "nicht", "ver\u00b7wan\u00b7deln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Soll ihr Marmor sie auch schon decken,", "tokens": ["Soll", "ihr", "Mar\u00b7mor", "sie", "auch", "schon", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wenn die Verwandlung geschieht?", "tokens": ["Wenn", "die", "Ver\u00b7wand\u00b7lung", "ge\u00b7schieht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "K\u00f6nigessohn! Edelster! dir, ja die sch\u00f6nste", "tokens": ["K\u00f6\u00b7ni\u00b7ges\u00b7sohn", "!", "E\u00b7dels\u00b7ter", "!", "dir", ",", "ja", "die", "sch\u00f6ns\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "PPER", "$,", "ADV", "ART", "ADJA"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Leyer ert\u00f6nt zu dem sch\u00f6nsten der Ges\u00e4nge", "tokens": ["Le\u00b7yer", "er\u00b7t\u00f6nt", "zu", "dem", "sch\u00f6ns\u00b7ten", "der", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Dir, der einst es vollf\u00fchrt! Dein warten", "tokens": ["Dir", ",", "der", "einst", "es", "voll\u00b7f\u00fchrt", "!", "Dein", "war\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "PPER", "VVPP", "$.", "PPOSAT", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ehren der Religion!", "tokens": ["Eh\u00b7ren", "der", "Re\u00b7li\u00b7gi\u00b7on", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Liess mich das Grab; s\u00e4ng ich von dir! Zu der sch\u00f6nsten", "tokens": ["Liess", "mich", "das", "Grab", ";", "s\u00e4ng", "ich", "von", "dir", "!", "Zu", "der", "sch\u00f6ns\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "VVFIN", "PPER", "APPR", "PPER", "$.", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Leyer ert\u00f6nt mein Gesang nicht; doch begeistert", "tokens": ["Le\u00b7yer", "er\u00b7t\u00f6nt", "mein", "Ge\u00b7sang", "nicht", ";", "doch", "be\u00b7geis\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$.", "ADV", "VVPP"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "S\u00e4ng' ich! sch\u00f6pft aus der Freude tiefsten", "tokens": ["S\u00e4ng'", "ich", "!", "sch\u00f6pft", "aus", "der", "Freu\u00b7de", "tiefs\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "VVFIN", "APPR", "ART", "NN", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Str\u00f6men, Vollf\u00fchrer, dein Lob!", "tokens": ["Str\u00f6\u00b7men", ",", "Voll\u00b7f\u00fch\u00b7rer", ",", "dein", "Lob", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Gross ist dein Werk! jetzo mein Wunsch. O es weiss der", "tokens": ["Gross", "ist", "dein", "Werk", "!", "jet\u00b7zo", "mein", "Wunsch", ".", "O", "es", "weiss", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "PPOSAT", "NN", "$.", "NE", "PPER", "VVFIN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Nicht, was es ist, sich verlieren in der Wonne!", "tokens": ["Nicht", ",", "was", "es", "ist", ",", "sich", "ver\u00b7lie\u00b7ren", "in", "der", "Won\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$,", "PRF", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wer die Religion, begleitet", "tokens": ["Wer", "die", "Re\u00b7li\u00b7gi\u00b7on", ",", "be\u00b7glei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWS", "ART", "NN", "$,", "VVPP"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von der geweihten Musik,", "tokens": ["Von", "der", "ge\u00b7weih\u00b7ten", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Und von des Psalms heiligem Flug, nicht gef\u00fchlt hat", "tokens": ["Und", "von", "des", "Psalms", "hei\u00b7li\u00b7gem", "Flug", ",", "nicht", "ge\u00b7f\u00fchlt", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,", "PTKNEG", "VVPP", "VAFIN"], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sanft nicht gebebt, wenn die Schaaren in dem Tempel", "tokens": ["Sanft", "nicht", "ge\u00b7bebt", ",", "wenn", "die", "Schaa\u00b7ren", "in", "dem", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "VVPP", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Feyrend sangen! und, ward diess Meer still,", "tokens": ["Fey\u00b7rend", "san\u00b7gen", "!", "und", ",", "ward", "diess", "Meer", "still", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$.", "KON", "$,", "VAFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ch\u00f6re vom Himmel herab!", "tokens": ["Ch\u00f6\u00b7re", "vom", "Him\u00b7mel", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "T\u00e4usche mich lang, seliger Traum! Ach ich h\u00f6re", "tokens": ["T\u00e4u\u00b7sche", "mich", "lang", ",", "se\u00b7li\u00b7ger", "Traum", "!", "Ach", "ich", "h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$.", "ITJ", "PPER", "VVFIN"], "meter": "+--+-+-++-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Christengesang! Welch ein Volkheer ist versammelt!", "tokens": ["Chris\u00b7ten\u00b7ge\u00b7sang", "!", "Welch", "ein", "Volk\u00b7heer", "ist", "ver\u00b7sam\u00b7melt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PIAT", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "So sah Kephas vordem f\u00fcnf Tausend", "tokens": ["So", "sah", "Ke\u00b7phas", "vor\u00b7dem", "f\u00fcnf", "Tau\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jesus auf Einmal sich weihn.", "tokens": ["Je\u00b7sus", "auf", "Ein\u00b7mal", "sich", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "H\u00f6rt ihr? den Sohn singet sein Volk! mit des Herzens", "tokens": ["H\u00f6rt", "ihr", "?", "den", "Sohn", "sin\u00b7get", "sein", "Volk", "!", "mit", "des", "Her\u00b7zens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Einfalt vereint sich die Einfalt des Gesanges!", "tokens": ["Ein\u00b7falt", "ver\u00b7eint", "sich", "die", "Ein\u00b7falt", "des", "Ge\u00b7san\u00b7ges", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+---+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Und mehr Hoheit, als alle Welt hat,", "tokens": ["Und", "mehr", "Ho\u00b7heit", ",", "als", "al\u00b7le", "Welt", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "KOUS", "PIAT", "NN", "VAFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hebt sie gen Himmel empor!", "tokens": ["Hebt", "sie", "gen", "Him\u00b7mel", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Wonnegef\u00fchl hebt sie empor, und es fliessen", "tokens": ["Won\u00b7ne\u00b7ge\u00b7f\u00fchl", "hebt", "sie", "em\u00b7por", ",", "und", "es", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPER", "VVINF"], "meter": "+--++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Thr\u00e4nen ins Lied! Denn die Kronen an dem Ziele", "tokens": ["Thr\u00e4\u00b7nen", "ins", "Lied", "!", "Denn", "die", "Kro\u00b7nen", "an", "dem", "Zie\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$.", "KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Strahlen ihnen! sie sehn um Sion", "tokens": ["Strah\u00b7len", "ih\u00b7nen", "!", "sie", "sehn", "um", "Si\u00b7on"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "PPER", "VVFIN", "APPR", "NE"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Palmen der Himlischen wehn!", "tokens": ["Pal\u00b7men", "der", "Him\u00b7li\u00b7schen", "wehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Oben beginnt jetzo der Psalm, den die Ch\u00f6re", "tokens": ["O\u00b7ben", "be\u00b7ginnt", "jet\u00b7zo", "der", "Psalm", ",", "den", "die", "Ch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Singen, Musik, als ob kunstlos aus der Seele", "tokens": ["Sin\u00b7gen", ",", "Mu\u00b7sik", ",", "als", "ob", "kunst\u00b7los", "aus", "der", "See\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KOKOM", "KOUS", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Schnell sie str\u00f6me! So leiten Meister", "tokens": ["Schnell", "sie", "str\u00f6\u00b7me", "!", "So", "lei\u00b7ten", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PPER", "VVFIN", "$.", "ADV", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sie, doch in Ufern, daher.", "tokens": ["Sie", ",", "doch", "in", "U\u00b7fern", ",", "da\u00b7her", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ADV", "APPR", "NN", "$,", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Kraftvoll, und tief dringt sie ins Herz! Sie verachtet", "tokens": ["Kraft\u00b7voll", ",", "und", "tief", "dringt", "sie", "ins", "Herz", "!", "Sie", "ver\u00b7ach\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$.", "PPER", "VVFIN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Alles, was uns bis zur Thr\u00e4ne nicht erhebet!", "tokens": ["Al\u00b7les", ",", "was", "uns", "bis", "zur", "Thr\u00e4\u00b7ne", "nicht", "er\u00b7he\u00b7bet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Was nicht f\u00fcllet den Geist mit Schauer!", "tokens": ["Was", "nicht", "f\u00fcl\u00b7let", "den", "Geist", "mit", "Schau\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Oder mit himlischem Ernst.", "tokens": ["O\u00b7der", "mit", "him\u00b7li\u00b7schem", "Ernst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "Himlischer Ernst t\u00f6net herab mit des Festes", "tokens": ["Him\u00b7li\u00b7scher", "Ernst", "t\u00f6\u00b7net", "her\u00b7ab", "mit", "des", "Fes\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+--++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hohem Gesang. Prophezeiung! und Erf\u00fcllung!", "tokens": ["Ho\u00b7hem", "Ge\u00b7sang", ".", "Pro\u00b7phe\u00b7zei\u00b7ung", "!", "und", "Er\u00b7f\u00fcl\u00b7lung", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "$.", "KON", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wechseln Ch\u00f6re, mit Ch\u00f6ren. Gnade!", "tokens": ["Wech\u00b7seln", "Ch\u00f6\u00b7re", ",", "mit", "Ch\u00f6\u00b7ren", ".", "Gna\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "NN", "$.", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Singen sie dann, und Gericht!", "tokens": ["Sin\u00b7gen", "sie", "dann", ",", "und", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.13": {"line.1": {"text": "Ach von des Sohns Liede beseelt, von der Heerschaar", "tokens": ["Ach", "von", "des", "Sohns", "Lie\u00b7de", "be\u00b7seelt", ",", "von", "der", "Heer\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sions entflamt, wie erheben sie ihr Loblied!", "tokens": ["Si\u00b7ons", "ent\u00b7flamt", ",", "wie", "er\u00b7he\u00b7ben", "sie", "ihr", "Lob\u00b7lied", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+--+-++-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Eine Stimme beginnet leise,", "tokens": ["Ei\u00b7ne", "Stim\u00b7me", "be\u00b7gin\u00b7net", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Eine der Harfen mit ihr.", "tokens": ["Ei\u00b7ne", "der", "Har\u00b7fen", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.14": {"line.1": {"text": "Aber es t\u00f6nt m\u00e4chtiger bald in dem Chor fort!", "tokens": ["A\u00b7ber", "es", "t\u00f6nt", "m\u00e4ch\u00b7ti\u00b7ger", "bald", "in", "dem", "Chor", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ch\u00f6re sind nun in dem Strom schon des Gesanges!", "tokens": ["Ch\u00f6\u00b7re", "sind", "nun", "in", "dem", "Strom", "schon", "des", "Ge\u00b7san\u00b7ges", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "APPR", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Schon erzittert das Volk! schon gl\u00fchet", "tokens": ["Schon", "er\u00b7zit\u00b7tert", "das", "Volk", "!", "schon", "gl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Feuer des Himmels in ihm!", "tokens": ["Feu\u00b7er", "des", "Him\u00b7mels", "in", "ihm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+-++", "measure": "iambic.tetra.invert"}}, "stanza.15": {"line.1": {"text": "Wonne! das Volk h\u00e4lt sich noch kaum! Die Posaune", "tokens": ["Won\u00b7ne", "!", "das", "Volk", "h\u00e4lt", "sich", "noch", "kaum", "!", "Die", "Po\u00b7sau\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "PRF", "ADV", "ADV", "$.", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Donnerte schon! und itzt donnert sie von neuem!", "tokens": ["Don\u00b7ner\u00b7te", "schon", "!", "und", "itzt", "don\u00b7nert", "sie", "von", "neu\u00b7em", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "KON", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Aller Ch\u00f6re Triumph erscholl schon!", "tokens": ["Al\u00b7ler", "Ch\u00f6\u00b7re", "Tri\u00b7umph", "er\u00b7scholl", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "ADJD", "ADV", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schallt, dass der Tempel ihm bebt!", "tokens": ["Schallt", ",", "dass", "der", "Tem\u00b7pel", "ihm", "bebt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.16": {"line.1": {"text": "L\u00e4nger nun nicht, l\u00e4nger nicht mehr! Die Gemeine", "tokens": ["L\u00e4n\u00b7ger", "nun", "nicht", ",", "l\u00e4n\u00b7ger", "nicht", "mehr", "!", "Die", "Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "PTKNEG", "$,", "ADJD", "PTKNEG", "ADV", "$.", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Sinket dahin, auf ihr Antlitz zum Altare,", "tokens": ["Sin\u00b7ket", "da\u00b7hin", ",", "auf", "ihr", "Ant\u00b7litz", "zum", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Hell vom Kelche des Bundes! eilt, eilt!", "tokens": ["Hell", "vom", "Kel\u00b7che", "des", "Bun\u00b7des", "!", "eilt", ",", "eilt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$.", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Str\u00f6mt in der Ch\u00f6re Triumph!", "tokens": ["Str\u00f6mt", "in", "der", "Ch\u00f6\u00b7re", "Tri\u00b7umph", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.17": {"line.1": {"text": "Ruhet dereinst dort mein Gebein, an der Tempel", "tokens": ["Ru\u00b7het", "de\u00b7reinst", "dort", "mein", "Ge\u00b7bein", ",", "an", "der", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Einem mein Staub, wo der Chorpsalm den Gemeinen", "tokens": ["Ei\u00b7nem", "mein", "Staub", ",", "wo", "der", "Chor\u00b7psalm", "den", "Ge\u00b7mei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "T\u00f6nt; so bebet mein Grab, und lichter", "tokens": ["T\u00f6nt", ";", "so", "be\u00b7bet", "mein", "Grab", ",", "und", "lich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bl\u00fchet die Blume darauf,", "tokens": ["Bl\u00fc\u00b7het", "die", "Blu\u00b7me", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.18": {"line.1": {"text": "Wenn, an dem Tag', als aus dem Fels der Entschlafne", "tokens": ["Wenn", ",", "an", "dem", "Tag'", ",", "als", "aus", "dem", "Fels", "der", "Ent\u00b7schlaf\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Strahlte, der Preis in dem Jubel sich ihm nachschwingt!", "tokens": ["Strahl\u00b7te", ",", "der", "Preis", "in", "dem", "Ju\u00b7bel", "sich", "ihm", "nach\u00b7schwingt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "APPR", "ART", "NN", "PRF", "PPER", "VVFIN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Denn ich h\u00f6r' es, und: Auferstehung!", "tokens": ["Denn", "ich", "h\u00f6r'", "es", ",", "und", ":", "Auf\u00b7er\u00b7ste\u00b7hung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "KON", "$.", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Lispelt ein Laut aus der Gruft.", "tokens": ["Lis\u00b7pelt", "ein", "Laut", "aus", "der", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "APPR", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}