{"textgrid.poem.48823": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "48. Auf den Sonnenschirm", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht, da\u00df sie den Verdru\u00df der Sonnen ihr benehme,", "tokens": ["Nicht", ",", "da\u00df", "sie", "den", "Ver\u00b7dru\u00df", "der", "Son\u00b7nen", "ihr", "be\u00b7neh\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "braucht meine Sonne dich, o du der Sch\u00f6nheit Schutz", "tokens": ["braucht", "mei\u00b7ne", "Son\u00b7ne", "dich", ",", "o", "du", "der", "Sch\u00f6n\u00b7heit", "Schutz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "$,", "FM", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und Zaum der fremden Glut, nein! dieses ist dein Nutz,", "tokens": ["und", "Zaum", "der", "frem\u00b7den", "Glut", ",", "nein", "!", "die\u00b7ses", "ist", "dein", "Nutz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$,", "PTKANT", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df sich die Sonne nicht f\u00fcr ihrer Klarheit sch\u00e4me", "tokens": ["da\u00df", "sich", "die", "Son\u00b7ne", "nicht", "f\u00fcr", "ih\u00b7rer", "Klar\u00b7heit", "sch\u00e4\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "und sich nicht etwa krank und gar zu Tode gr\u00e4me", "tokens": ["und", "sich", "nicht", "et\u00b7wa", "krank", "und", "gar", "zu", "To\u00b7de", "gr\u00e4\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "PTKNEG", "ADV", "ADJD", "KON", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "f\u00fcr derer Treflichkeit, die ihrer auch beut Trutz.", "tokens": ["f\u00fcr", "de\u00b7rer", "Tref\u00b7lich\u00b7keit", ",", "die", "ih\u00b7rer", "auch", "beut", "Trutz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "$,", "PRELS", "PPOSAT", "ADV", "VVFIN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Drum setzt sie dich vor sich. Dein frommer Schatten tuts,", "tokens": ["Drum", "setzt", "sie", "dich", "vor", "sich", ".", "Dein", "from\u00b7mer", "Schat\u00b7ten", "tuts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "PRF", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df du dem Himmel selbst und ihr auch bist bequ\u00e4me.", "tokens": ["da\u00df", "du", "dem", "Him\u00b7mel", "selbst", "und", "ihr", "auch", "bist", "be\u00b7qu\u00e4\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "KON", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So bleibt die Sonne klar und ihre Sch\u00f6nheit ganz.", "tokens": ["So", "bleibt", "die", "Son\u00b7ne", "klar", "und", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch dich, o Schiedemann, hat jedes seinen Glanz.", "tokens": ["Durch", "dich", ",", "o", "Schie\u00b7de\u00b7mann", ",", "hat", "je\u00b7des", "sei\u00b7nen", "Glanz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach, da\u00df du solchen Dienst mir woltest nicht verschm\u00e4hen!", "tokens": ["Ach", ",", "da\u00df", "du", "sol\u00b7chen", "Dienst", "mir", "wol\u00b7test", "nicht", "ver\u00b7schm\u00e4\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "PIAT", "NN", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Trit zwischen mich und sie. Ihr allzustarkes Licht", "tokens": ["Trit", "zwi\u00b7schen", "mich", "und", "sie", ".", "Ihr", "all\u00b7zu\u00b7star\u00b7kes", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "KON", "PPER", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "kan mein verblendter Schein durchaus vertragen nicht.", "tokens": ["kan", "mein", "ver\u00b7blend\u00b7ter", "Schein", "durc\u00b7haus", "ver\u00b7tra\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Welchs sterblichs Auge kan in diese Sonne sehen?", "tokens": ["Welchs", "sterb\u00b7lichs", "Au\u00b7ge", "kan", "in", "die\u00b7se", "Son\u00b7ne", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}