{"textgrid.poem.64260": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Ro\u00dfbach", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur ernsten Tons gedenkt der Deutsche sonst,", "tokens": ["Nur", "erns\u00b7ten", "Tons", "ge\u00b7denkt", "der", "Deut\u00b7sche", "sonst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Schlacht, der m\u00e4nnermordenden: jedoch", "tokens": ["Der", "Schlacht", ",", "der", "m\u00e4n\u00b7ner\u00b7mor\u00b7den\u00b7den", ":", "je\u00b7doch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$.", "ADV"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Ein leises L\u00e4cheln spielt um seinen Mund", "tokens": ["Ein", "lei\u00b7ses", "L\u00e4\u00b7cheln", "spielt", "um", "sei\u00b7nen", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Beim Namen \u00bbRo\u00dfbach\u00ab. Ei, welch' lust'ger Klang!", "tokens": ["Beim", "Na\u00b7men", "\u00bb", "Ro\u00df\u00b7bach", "\u00ab", ".", "Ei", ",", "welch'", "lust'\u00b7ger", "Klang", "!"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "NN", "$(", "$.", "NN", "$,", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie sausen die Husaren durch das Feld!", "tokens": ["Wie", "sau\u00b7sen", "die", "Hu\u00b7sa\u00b7ren", "durch", "das", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist's eine Schlacht? Ist's nur ein Pferderennen?", "tokens": ["Ist's", "ei\u00b7ne", "Schlacht", "?", "Ist's", "nur", "ein", "Pfer\u00b7de\u00b7ren\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "NE", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Welch' bunte Beute sandte her Paris!", "tokens": ["Welch'", "bun\u00b7te", "Beu\u00b7te", "sand\u00b7te", "her", "Pa\u00b7ris", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Gott Humor hat diesen Sieg gewonnen!", "tokens": ["Der", "Gott", "Hu\u00b7mor", "hat", "die\u00b7sen", "Sieg", "ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}