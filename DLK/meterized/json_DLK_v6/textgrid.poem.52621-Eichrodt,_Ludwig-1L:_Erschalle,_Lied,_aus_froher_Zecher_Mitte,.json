{"textgrid.poem.52621": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erschalle, Lied, aus froher Zecher Mitte,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erschalle, Lied, aus froher Zecher Mitte,", "tokens": ["Er\u00b7schal\u00b7le", ",", "Lied", ",", "aus", "fro\u00b7her", "Ze\u00b7cher", "Mit\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchsch\u00fcttre, Becherklang, den Saal!", "tokens": ["Durch\u00b7sch\u00fctt\u00b7re", ",", "Be\u00b7cher\u00b7klang", ",", "den", "Saal", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So freun wir uns der guten alten Sitte,", "tokens": ["So", "freun", "wir", "uns", "der", "gu\u00b7ten", "al\u00b7ten", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und f\u00fcllen dreimal den Pokal.", "tokens": ["Und", "f\u00fcl\u00b7len", "drei\u00b7mal", "den", "Po\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf! seid mit Herz und Mund dabei,", "tokens": ["Auf", "!", "seid", "mit", "Herz", "und", "Mund", "da\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VAFIN", "APPR", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hebet die Becher, eins, zwei, drei!", "tokens": ["Und", "he\u00b7bet", "die", "Be\u00b7cher", ",", "eins", ",", "zwei", ",", "drei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PIS", "$,", "CARD", "$,", "CARD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du Erster, leuchtend ein verzehrend Gl\u00fchen,", "tokens": ["Du", "Ers\u00b7ter", ",", "leuch\u00b7tend", "ein", "ver\u00b7zeh\u00b7rend", "Gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVPP", "ART", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mach unsre Geister ph\u00f6nixjung,", "tokens": ["Mach", "uns\u00b7re", "Geis\u00b7ter", "ph\u00f6\u00b7nix\u00b7jung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verfl\u00fcchtige an kleine Noth und M\u00fchen", "tokens": ["Ver\u00b7fl\u00fcch\u00b7ti\u00b7ge", "an", "klei\u00b7ne", "Noth", "und", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Nebel der Erinnerung!", "tokens": ["Den", "Ne\u00b7bel", "der", "E\u00b7rin\u00b7ne\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn soll uns der Himmel recht sonnig bl\u00fchn,", "tokens": ["Denn", "soll", "uns", "der", "Him\u00b7mel", "recht", "son\u00b7nig", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "M\u00fcssen die alten Gew\u00f6lke fliehn.", "tokens": ["M\u00fcs\u00b7sen", "die", "al\u00b7ten", "Ge\u00b7w\u00f6l\u00b7ke", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Das andre Glas: der Heuchler soll erblassen!", "tokens": ["Das", "and\u00b7re", "Glas", ":", "der", "Heuch\u00b7ler", "soll", "er\u00b7blas\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Freude ist das Glas geweiht!", "tokens": ["Der", "Freu\u00b7de", "ist", "das", "Glas", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O sagt, wer kann sie all im Busen fassen", "tokens": ["O", "sagt", ",", "wer", "kann", "sie", "all", "im", "Bu\u00b7sen", "fas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PWS", "VMFIN", "PPER", "PIAT", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die s\u00fc\u00dfe Unerme\u00dflichkeit?", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Un\u00b7er\u00b7me\u00df\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir hoffen, wir schw\u00e4rmen, es tr\u00e4umet das Herz,", "tokens": ["Wir", "hof\u00b7fen", ",", "wir", "schw\u00e4r\u00b7men", ",", "es", "tr\u00e4u\u00b7met", "das", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es hat nichts erfahren, kennt keinen Schmerz.", "tokens": ["Es", "hat", "nichts", "er\u00b7fah\u00b7ren", ",", "kennt", "kei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVINF", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Drum noch einmal! Gru\u00df unserm Zechgelage!", "tokens": ["Drum", "noch", "ein\u00b7mal", "!", "Gru\u00df", "un\u00b7serm", "Zech\u00b7ge\u00b7la\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Zukunft \u2013 Gru\u00df! dem Lebensmuth!", "tokens": ["Der", "Zu\u00b7kunft", "\u2013", "Gru\u00df", "!", "dem", "Le\u00b7bens\u00b7muth", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Morgenroth begl\u00fcckter Sonnentage", "tokens": ["Ein", "Mor\u00b7gen\u00b7roth", "be\u00b7gl\u00fcck\u00b7ter", "Son\u00b7nen\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sei dieses Festes Freudengluth!", "tokens": ["Sei", "die\u00b7ses", "Fes\u00b7tes", "Freu\u00b7den\u00b7gluth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00dft nimmermehr ab von deutscher Art,", "tokens": ["La\u00dft", "nim\u00b7mer\u00b7mehr", "ab", "von", "deut\u00b7scher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dann ist euch die Zukunft in Treuen bewahrt.", "tokens": ["Dann", "ist", "euch", "die", "Zu\u00b7kunft", "in", "Treu\u00b7en", "be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}