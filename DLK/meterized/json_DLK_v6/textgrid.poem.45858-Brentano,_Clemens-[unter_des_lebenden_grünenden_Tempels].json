{"textgrid.poem.45858": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[unter des lebenden gr\u00fcnenden Tempels]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter des lebenden", "tokens": ["Un\u00b7ter", "des", "le\u00b7ben\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gr\u00fcnenden Tempels", "tokens": ["Gr\u00fc\u00b7nen\u00b7den", "Tem\u00b7pels"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Fl\u00fcsternde Hallen", "tokens": ["Fl\u00fcs\u00b7tern\u00b7de", "Hal\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Komme ich irrend.", "tokens": ["Kom\u00b7me", "ich", "ir\u00b7rend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie sich die Eiche", "tokens": ["Wie", "sich", "die", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Himmelw\u00e4rts t\u00fcrmet,", "tokens": ["Him\u00b7mel\u00b7w\u00e4rts", "t\u00fcr\u00b7met", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie in dem Gipfel", "tokens": ["Wie", "in", "dem", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ruhet des M\u00e4chtigen", "tokens": ["Ru\u00b7het", "des", "M\u00e4ch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Jupiters Fu\u00df.", "tokens": ["Ju\u00b7pi\u00b7ters", "Fu\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und in dem Herzen", "tokens": ["Und", "in", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "F\u00fchl' ich die N\u00e4he", "tokens": ["F\u00fchl'", "ich", "die", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Heiliger Wesen,", "tokens": ["Hei\u00b7li\u00b7ger", "We\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die durch die Zweige", "tokens": ["Die", "durch", "die", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Zu dem Olympos", "tokens": ["Zu", "dem", "O\u00b7lym\u00b7pos"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Wandeln empor.", "tokens": ["Wan\u00b7deln", "em\u00b7por", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "F\u00fchrt mich ihr friedlichen", "tokens": ["F\u00fchrt", "mich", "ihr", "fried\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Geister des Haines,", "tokens": ["Geis\u00b7ter", "des", "Hai\u00b7nes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die mich umschweben", "tokens": ["Die", "mich", "um\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Lachend und rufend,", "tokens": ["La\u00b7chend", "und", "ru\u00b7fend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "F\u00fchrt mich zur\u00fcck.", "tokens": ["F\u00fchrt", "mich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Irrende, fl\u00fcchtige,", "tokens": ["Ir\u00b7ren\u00b7de", ",", "fl\u00fcch\u00b7ti\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "T\u00f6nende Geister,", "tokens": ["T\u00f6\u00b7nen\u00b7de", "Geis\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die ihr mit sch\u00e4kernden", "tokens": ["Die", "ihr", "mit", "sch\u00e4\u00b7kern\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA"], "meter": "+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Lispelnden Worten", "tokens": ["Lis\u00b7peln\u00b7den", "Wor\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Irr mich gef\u00fchrt.", "tokens": ["Irr", "mich", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Hier wo in mondlichen", "tokens": ["Hier", "wo", "in", "mond\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PWAV", "APPR", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "N\u00e4chten ihr rauschet,", "tokens": ["N\u00e4ch\u00b7ten", "ihr", "rau\u00b7schet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und um die wohnsame", "tokens": ["Und", "um", "die", "wohn\u00b7sa\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Herrliche Eiche", "tokens": ["Herr\u00b7li\u00b7che", "Ei\u00b7che"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Tanzend euch schwingt.", "tokens": ["Tan\u00b7zend", "euch", "schwingt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Wo ich im Taue", "tokens": ["Wo", "ich", "im", "Tau\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Freudigen Grases", "tokens": ["Freu\u00b7di\u00b7gen", "Gra\u00b7ses"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Von euren fl\u00fcchtigen", "tokens": ["Von", "eu\u00b7ren", "fl\u00fcch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Goldenen Sohlen", "tokens": ["Gol\u00b7de\u00b7nen", "Soh\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ehre die Spur. \u2013", "tokens": ["Eh\u00b7re", "die", "Spur", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "H\u00f6rt mich ihr freundlichen,", "tokens": ["H\u00f6rt", "mich", "ihr", "freund\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die ihr verlorene", "tokens": ["Die", "ihr", "ver\u00b7lo\u00b7re\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "G\u00f6tter gepfleget,", "tokens": ["G\u00f6t\u00b7ter", "ge\u00b7pfle\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die ihr die fliehende", "tokens": ["Die", "ihr", "die", "flie\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Daphne umarmt.", "tokens": ["Daph\u00b7ne", "um\u00b7armt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Frohe, geheime,", "tokens": ["Fro\u00b7he", ",", "ge\u00b7hei\u00b7me", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Lindernde Geister,", "tokens": ["Lin\u00b7dern\u00b7de", "Geis\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die in des Waldes", "tokens": ["Die", "in", "des", "Wal\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "R\u00fchrigen Schauer", "tokens": ["R\u00fch\u00b7ri\u00b7gen", "Schau\u00b7er"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Weben den Trost.", "tokens": ["We\u00b7ben", "den", "Trost", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "M\u00e4chtige, lebende,", "tokens": ["M\u00e4ch\u00b7ti\u00b7ge", ",", "le\u00b7ben\u00b7de", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "St\u00e4rkende Geister,", "tokens": ["St\u00e4r\u00b7ken\u00b7de", "Geis\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die in der St\u00e4mme", "tokens": ["Die", "in", "der", "St\u00e4m\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Alter und Jugend", "tokens": ["Al\u00b7ter", "und", "Ju\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Bilden die Kraft.", "tokens": ["Bil\u00b7den", "die", "Kraft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Wenn ich je frevlend", "tokens": ["Wenn", "ich", "je", "frev\u00b7lend"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Eure geheiligten", "tokens": ["Eu\u00b7re", "ge\u00b7hei\u00b7lig\u00b7ten"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "St\u00e4mme verletzet,", "tokens": ["St\u00e4m\u00b7me", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "O! so verdorre", "tokens": ["O", "!", "so", "ver\u00b7dor\u00b7re"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "ADV", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Welkend die Hand.", "tokens": ["Wel\u00b7kend", "die", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Nimmer auch h\u00f6hnt' ich", "tokens": ["Nim\u00b7mer", "auch", "h\u00f6hnt'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Echo die Jungfrau,", "tokens": ["E\u00b7cho", "die", "Jung\u00b7frau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die mit euch wohnet,", "tokens": ["Die", "mit", "euch", "woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Teilt ihr vertraulich", "tokens": ["Teilt", "ihr", "ver\u00b7trau\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Liebe und Schmerz.", "tokens": ["Lie\u00b7be", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.13": {"line.1": {"text": "F\u00fchret mich heimw\u00e4rts!", "tokens": ["F\u00fch\u00b7ret", "mich", "heim\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bin nur ein Wandrer,", "tokens": ["Bin", "nur", "ein", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Bin kein Unsterblicher,", "tokens": ["Bin", "kein", "U\u00b7nsterb\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der mit ambrosischen", "tokens": ["Der", "mit", "am\u00b7bro\u00b7si\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bissen sich n\u00e4hrt.", "tokens": ["Bis\u00b7sen", "sich", "n\u00e4hrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.14": {"line.1": {"text": "Wisset mich hungert,", "tokens": ["Wis\u00b7set", "mich", "hun\u00b7gert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "F\u00fchret mich heimw\u00e4rts,", "tokens": ["F\u00fch\u00b7ret", "mich", "heim\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df ich dem Freunde", "tokens": ["Da\u00df", "ich", "dem", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Von der Dryaden", "tokens": ["Von", "der", "Dry\u00b7a\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "H\u00fclfreicher G\u00fcte", "tokens": ["H\u00fclf\u00b7rei\u00b7cher", "G\u00fc\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bringe die M\u00e4r.", "tokens": ["Brin\u00b7ge", "die", "M\u00e4r."], "token_info": ["word", "word", "abbreviation"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--", "measure": "dactylic.init"}}}}}