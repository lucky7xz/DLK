{"textgrid.poem.67775": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Darstellung Jesu im Tempel", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Sohn im Arm, Maria lag", "tokens": ["Den", "Sohn", "im", "Arm", ",", "Ma\u00b7ria", "lag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "NE", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Auf Knieen am Altar", "tokens": ["Auf", "Kni\u00b7e\u00b7en", "am", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und dankt' und bracht' ihr arm Geschenk,", "tokens": ["Und", "dankt'", "und", "bracht'", "ihr", "arm", "Ge\u00b7schenk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Turteltaubenpaar,", "tokens": ["Ein", "Tur\u00b7tel\u00b7tau\u00b7ben\u00b7paar", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und brachte mehr als alle Welt,", "tokens": ["Und", "brach\u00b7te", "mehr", "als", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott, ihren Liebling, dar.", "tokens": ["Gott", ",", "ih\u00b7ren", "Lieb\u00b7ling", ",", "dar", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und sieh, da trat ein Greis zu ihr", "tokens": ["Und", "sieh", ",", "da", "trat", "ein", "Greis", "zu", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(der Greis hie\u00df Simeon);", "tokens": ["(", "der", "Greis", "hie\u00df", "Si\u00b7me\u00b7on", ")", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "NE", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er weinte Freud' und zitterte", "tokens": ["Er", "wein\u00b7te", "Freud'", "und", "zit\u00b7ter\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kniet' und nahm den Sohn,", "tokens": ["Und", "kniet'", "und", "nahm", "den", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Umarmt' und dr\u00fcckt' ihn an sein Herz", "tokens": ["Um\u00b7armt'", "und", "dr\u00fcckt'", "ihn", "an", "sein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und war im Himmel schon.", "tokens": ["Und", "war", "im", "Him\u00b7mel", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbla\u00df mich, la\u00df, Herr, nun Deinen Knecht,", "tokens": ["\u00bb", "la\u00df", "mich", ",", "la\u00df", ",", "Herr", ",", "nun", "Dei\u00b7nen", "Knecht", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "VVIMP", "$,", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "In Friede la\u00df mich ziehn!", "tokens": ["In", "Frie\u00b7de", "la\u00df", "mich", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe, was Du zu mir sprachst,", "tokens": ["Ich", "ha\u00b7be", ",", "was", "Du", "zu", "mir", "sprachst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sollt' noch sehen ihn.", "tokens": ["Ich", "sollt'", "noch", "se\u00b7hen", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich seh' ihn, meinen Heiland! la\u00df", "tokens": ["Ich", "seh'", "ihn", ",", "mei\u00b7nen", "Hei\u00b7land", "!", "la\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun meinen Blick entfliehn!", "tokens": ["Nun", "mei\u00b7nen", "Blick", "ent\u00b7fliehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Menschen Heil, der V\u00f6lker Licht,", "tokens": ["Der", "Men\u00b7schen", "Heil", ",", "der", "V\u00f6l\u00b7ker", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Israel's Preis und Ruhm!", "tokens": ["Is\u00b7ra\u00b7el's", "Preis", "und", "Ruhm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab', ich schau', ich k\u00fcsse Dich,", "tokens": ["Ich", "hab'", ",", "ich", "schau'", ",", "ich", "k\u00fcs\u00b7se", "Dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erde Heiligthum!", "tokens": ["Der", "Er\u00b7de", "Hei\u00b7lig\u00b7thum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und doch (hier wandt' er tr\u00f6stend sich,", "tokens": ["Und", "doch", "(", "hier", "wandt'", "er", "tr\u00f6s\u00b7tend", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "VVFIN", "PPER", "ADJD", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Prophet, zur Mutter um)", "tokens": ["Pro\u00b7phet", ",", "zur", "Mut\u00b7ter", "um", ")"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sieh, er wird Fels sein! Vieler Fall", "tokens": ["Sieh", ",", "er", "wird", "Fels", "sein", "!", "Vie\u00b7ler", "Fall"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PPER", "VAFIN", "NN", "VAINF", "$.", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und Vieler Auferstehn;", "tokens": ["Und", "Vie\u00b7ler", "Auf\u00b7er\u00b7stehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Panier zum Kampf, und, Mutter, Dir", "tokens": ["Pa\u00b7nier", "zum", "Kampf", ",", "und", ",", "Mut\u00b7ter", ",", "Dir"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "APPRART", "NN", "$,", "KON", "$,", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird Schwert durchs Herz er gehn,", "tokens": ["Wird", "Schwert", "durchs", "Herz", "er", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Vieler, Vieler Menschensinn", "tokens": ["Und", "Vie\u00b7ler", ",", "Vie\u00b7ler", "Men\u00b7schen\u00b7sinn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird an ihm offen stehn.\u00ab", "tokens": ["Wird", "an", "ihm", "of\u00b7fen", "stehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Greis, er sprach's. Des Herren Geist", "tokens": ["Der", "Greis", ",", "er", "sprach'", "s.", "Des", "Her\u00b7ren", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "VVIMP", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weht', was er sprach, ihm zu;", "tokens": ["Weht'", ",", "was", "er", "sprach", ",", "ihm", "zu", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VVFIN", "$,", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der regt' ihn: \u00bbEil in Gottes Haus!", "tokens": ["Der", "regt'", "ihn", ":", "\u00bb", "Eil", "in", "Got\u00b7tes", "Haus", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "$(", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Sohn da findest Du.\u00ab", "tokens": ["Den", "Sohn", "da", "fin\u00b7dest", "Du", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er ging und fand und segnet' ihn", "tokens": ["Er", "ging", "und", "fand", "und", "seg\u00b7net'", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und brach und ging zur Ruh.", "tokens": ["Und", "brach", "und", "ging", "zur", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und Greises Segen sank aufs Kind;", "tokens": ["Und", "Grei\u00b7ses", "Se\u00b7gen", "sank", "aufs", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Wort, es drang ins Herz:", "tokens": ["Sein", "Wort", ",", "es", "drang", "ins", "Herz", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er ward der V\u00f6lker Heil und Licht,", "tokens": ["Er", "ward", "der", "V\u00f6l\u00b7ker", "Heil", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Israel's Ruhm und Schmerz,", "tokens": ["Is\u00b7ra\u00b7el's", "Ruhm", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Fels zum Fall und Auferstehn,", "tokens": ["Ein", "Fels", "zum", "Fall", "und", "Auf\u00b7er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Mutter Schwert ins Herz.", "tokens": ["Der", "Mut\u00b7ter", "Schwert", "ins", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}