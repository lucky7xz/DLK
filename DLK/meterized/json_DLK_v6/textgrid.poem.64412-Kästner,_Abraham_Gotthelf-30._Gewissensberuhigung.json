{"textgrid.poem.64412": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "30. Gewissensberuhigung", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Doktor", "tokens": ["Ein", "Dok\u00b7tor"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "J\u00fcngst von der nat\u00fcrlichen Folge der S\u00fcnde;", "tokens": ["J\u00fcngst", "von", "der", "na\u00b7t\u00fcr\u00b7li\u00b7chen", "Fol\u00b7ge", "der", "S\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Er hat es verschworen", "tokens": ["Er", "hat", "es", "ver\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ja, dacht er, f\u00fcr Andre, nicht selbsten f\u00fcr mich.", "tokens": ["Ja", ",", "dacht", "er", ",", "f\u00fcr", "And\u00b7re", ",", "nicht", "selbs\u00b7ten", "f\u00fcr", "mich", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "APPR", "PIS", "$,", "PTKNEG", "ADV", "APPR", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}