{"textgrid.poem.59133": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "352. Du bist nicht dein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geist, Seel' und Leib, die Glieder auch", "tokens": ["Geist", ",", "Seel'", "und", "Leib", ",", "die", "Glie\u00b7der", "auch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht dein sind, sondern Jesu Sachen.", "tokens": ["Nicht", "dein", "sind", ",", "son\u00b7dern", "Je\u00b7su", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "VAFIN", "$,", "KON", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr ihn und nicht f\u00fcr dich sie brauch;", "tokens": ["F\u00fcr", "ihn", "und", "nicht", "f\u00fcr", "dich", "sie", "brauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PTKNEG", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie er sie braucht, la\u00df du ihn machen!", "tokens": ["Wie", "er", "sie", "braucht", ",", "la\u00df", "du", "ihn", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}