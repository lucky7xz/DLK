{"dta.poem.8660": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Am Grabe eines Ministers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Du fuhrst im goldnen Gl\u00fcckeswagen", "tokens": ["Du", "fuhrst", "im", "gold\u00b7nen", "Gl\u00fc\u00b7ckes\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dahin den raschen Trott,", "tokens": ["Da\u00b7hin", "den", "ra\u00b7schen", "Trott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von keuchenden L\u00fcsten fortgetragen,", "tokens": ["Von", "keu\u00b7chen\u00b7den", "L\u00fcs\u00b7ten", "fort\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und d\u00fcnktest dir ein Gott!", "tokens": ["Und", "d\u00fcnk\u00b7test", "dir", "ein", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie flogen des P\u00f6bels Rabenschw\u00e4rme", "tokens": ["Wie", "flo\u00b7gen", "des", "P\u00f6\u00b7bels", "Ra\u00b7ben\u00b7schw\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir aus dem Weg' so bang,", "tokens": ["Dir", "aus", "dem", "Weg'", "so", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sie h\u00f6rten der Gei\u00dfel wild Gel\u00e4rme,", "tokens": ["Da", "sie", "h\u00f6r\u00b7ten", "der", "Gei\u00b7\u00dfel", "wild", "Ge\u00b7l\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der R\u00e4der Donnerklang!", "tokens": ["Der", "R\u00e4\u00b7der", "Don\u00b7ner\u00b7klang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein weinender Bettler, stand am Wege", "tokens": ["Ein", "wei\u00b7nen\u00b7der", "Bett\u00b7ler", ",", "stand", "am", "We\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das arme Vaterland,", "tokens": ["Das", "ar\u00b7me", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und flehte dich an um milde Pflege", "tokens": ["Und", "fleh\u00b7te", "dich", "an", "um", "mil\u00b7de", "Pfle\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit aufgehobner Hand;", "tokens": ["Mit", "auf\u00b7ge\u00b7hob\u00b7ner", "Hand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch wie auch klagte die bittre Klage,", "tokens": ["Doch", "wie", "auch", "klag\u00b7te", "die", "bitt\u00b7re", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie auch die Thr\u00e4ne rann:", "tokens": ["Wie", "auch", "die", "Thr\u00e4\u00b7ne", "rann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du triebst mit gellendem Gei\u00dfelschlage", "tokens": ["Du", "triebst", "mit", "gel\u00b7len\u00b7dem", "Gei\u00b7\u00dfel\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor\u00fcber dein Gespann! \u2014", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "dein", "Ge\u00b7spann", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201ehalt!\u201c schlug nun eine grause Stimme", "tokens": ["\u201e", "halt", "!", "\u201c", "schlug", "nun", "ei\u00b7ne", "grau\u00b7se", "Stim\u00b7me"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$.", "$(", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An dein entseztes Ohr,", "tokens": ["An", "dein", "ent\u00b7sez\u00b7tes", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es st\u00fcrzt', ein R\u00e4uber, mit Hohn und Grimme", "tokens": ["Es", "st\u00fcrzt'", ",", "ein", "R\u00e4u\u00b7ber", ",", "mit", "Hohn", "und", "Grim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Tod vom Wald hervor;", "tokens": ["Der", "Tod", "vom", "Wald", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und hieb die Str\u00e4nge mit scharfem Schwerde", "tokens": ["Und", "hieb", "die", "Str\u00e4n\u00b7ge", "mit", "schar\u00b7fem", "Schwer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom Wagen, rie\u00df mit Macht", "tokens": ["Vom", "Wa\u00b7gen", ",", "rie\u00df", "mit", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich fort, trotz Flehen und Angstgeberde,", "tokens": ["Dich", "fort", ",", "trotz", "Fle\u00b7hen", "und", "Angst\u00b7ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In seine finstre Nacht. \u2014", "tokens": ["In", "sei\u00b7ne", "finst\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Vaterland mit Lachen und Singen", "tokens": ["Das", "Va\u00b7ter\u00b7land", "mit", "La\u00b7chen", "und", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "H\u00e4lt Wacht an deinem Grab;", "tokens": ["H\u00e4lt", "Wacht", "an", "dei\u00b7nem", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Scheucht Thr\u00e4nen und Seufzer und H\u00e4nderingen", "tokens": ["Scheucht", "Thr\u00e4\u00b7nen", "und", "Seuf\u00b7zer", "und", "H\u00e4n\u00b7de\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "KON", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Fort mit dem Bettelstab!", "tokens": ["Fort", "mit", "dem", "Bet\u00b7tel\u00b7stab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}