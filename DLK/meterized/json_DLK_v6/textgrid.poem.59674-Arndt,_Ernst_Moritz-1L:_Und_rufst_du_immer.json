{"textgrid.poem.59674": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und rufst du immer ", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und rufst du immer ", "tokens": ["Und", "rufst", "du", "im\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und doch, wie bald umrollt der Sand", "tokens": ["Und", "doch", ",", "wie", "bald", "um\u00b7rollt", "der", "Sand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Grabes deinen Leichenkasten!", "tokens": ["Des", "Gra\u00b7bes", "dei\u00b7nen", "Lei\u00b7chen\u00b7kas\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die n\u00e4chste Ladung tr\u00e4gst du schon", "tokens": ["Die", "n\u00e4chs\u00b7te", "La\u00b7dung", "tr\u00e4gst", "du", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geschrieben hell auf wei\u00dfer Scheitel;", "tokens": ["Ge\u00b7schrie\u00b7ben", "hell", "auf", "wei\u00b7\u00dfer", "Schei\u00b7tel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gedenk' des weisen Salomon,", "tokens": ["Ge\u00b7denk'", "des", "wei\u00b7sen", "Sa\u00b7lo\u00b7mon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gedenk' des Spruches: ", "tokens": ["Ge\u00b7denk'", "des", "Spru\u00b7ches", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ja, darum ruf' ich ", "tokens": ["Ja", ",", "da\u00b7rum", "ruf'", "ich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PAV", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wann lange unsrer Gr\u00e4ber Sand", "tokens": ["Wann", "lan\u00b7ge", "uns\u00b7rer", "Gr\u00e4\u00b7ber", "Sand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unsern Staub die Winde treiben;", "tokens": ["Und", "un\u00b7sern", "Staub", "die", "Win\u00b7de", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wann unsrer Namen d\u00fcnner Schall", "tokens": ["Wann", "uns\u00b7rer", "Na\u00b7men", "d\u00fcn\u00b7ner", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Zeitensturme l\u00e4ngst verklungen,", "tokens": ["Im", "Zei\u00b7ten\u00b7stur\u00b7me", "l\u00e4ngst", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sei dieses Namens Widerhall", "tokens": ["Sei", "die\u00b7ses", "Na\u00b7mens", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Millionen nachgesungen!", "tokens": ["Von", "Mil\u00b7lion\u00b7en", "nach\u00b7ge\u00b7sun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, darum, weil wir gleich dem Schein", "tokens": ["Ja", ",", "da\u00b7rum", ",", "weil", "wir", "gleich", "dem", "Schein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PAV", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Morgend\u00e4mmerung verschweben,", "tokens": ["Der", "Mor\u00b7gen\u00b7d\u00e4m\u00b7me\u00b7rung", "ver\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df dies die gro\u00dfe Sonne sein,", "tokens": ["Mu\u00df", "dies", "die", "gro\u00b7\u00dfe", "Son\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Worin wir bl\u00fchn, wodurch wir leben;", "tokens": ["Wo\u00b7rin", "wir", "bl\u00fchn", ",", "wo\u00b7durch", "wir", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum m\u00fcssen wir an diesem Bau", "tokens": ["Drum", "m\u00fcs\u00b7sen", "wir", "an", "die\u00b7sem", "Bau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns hier die Ewigkeit erbauen,", "tokens": ["Uns", "hier", "die", "E\u00b7wig\u00b7keit", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Damit wir aus dem Geistergau", "tokens": ["Da\u00b7mit", "wir", "aus", "dem", "Geis\u00b7ter\u00b7gau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Einst selig k\u00f6nnen niederschauen.", "tokens": ["Einst", "se\u00b7lig", "k\u00f6n\u00b7nen", "nie\u00b7der\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Vaterland! Mein Vaterland!", "tokens": ["O", "Va\u00b7ter\u00b7land", "!", "Mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du heil'ges, das mir Gott gegeben!", "tokens": ["Du", "heil'\u00b7ges", ",", "das", "mir", "Gott", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "PRELS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sei alles eitel, alles Tand,", "tokens": ["Sei", "al\u00b7les", "ei\u00b7tel", ",", "al\u00b7les", "Tand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Name nichts und nichts mein Leben \u2013", "tokens": ["Mein", "Na\u00b7me", "nichts", "und", "nichts", "mein", "Le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIS", "KON", "PIS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst Jahrtausende durchbl\u00fchn", "tokens": ["Du", "wirst", "Jahr\u00b7tau\u00b7sen\u00b7de", "durch\u00b7bl\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In deutschen Treuen, deutschen Ehren:", "tokens": ["In", "deut\u00b7schen", "Treu\u00b7en", ",", "deut\u00b7schen", "Eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir Kurze m\u00fcssen hinnenziehn,", "tokens": ["Wir", "Kur\u00b7ze", "m\u00fcs\u00b7sen", "hin\u00b7nen\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch Liebe wird unsterblich w\u00e4hren.", "tokens": ["Doch", "Lie\u00b7be", "wird", "uns\u00b7terb\u00b7lich", "w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}