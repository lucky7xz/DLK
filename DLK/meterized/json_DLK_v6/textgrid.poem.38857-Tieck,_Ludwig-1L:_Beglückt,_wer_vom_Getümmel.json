{"textgrid.poem.38857": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Begl\u00fcckt, wer vom Get\u00fcmmel", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fcckt, wer vom Get\u00fcmmel", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wer", "vom", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PWS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Welt sein Leben schlie\u00dft,", "tokens": ["Der", "Welt", "sein", "Le\u00b7ben", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das dorten im Gewimmel", "tokens": ["Das", "dor\u00b7ten", "im", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verworren abw\u00e4rts flie\u00dft.", "tokens": ["Ver\u00b7wor\u00b7ren", "ab\u00b7w\u00e4rts", "flie\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier sind wir all' befreundet,", "tokens": ["Hier", "sind", "wir", "all'", "be\u00b7freun\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mensch, Thier und Blumenreich,", "tokens": ["Mensch", ",", "Thier", "und", "Blu\u00b7men\u00b7reich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von keinem angefeindet", "tokens": ["Von", "kei\u00b7nem", "an\u00b7ge\u00b7fein\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Macht uns die Liebe gleich.", "tokens": ["Macht", "uns", "die", "Lie\u00b7be", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Die zarten L\u00e4mmer springen", "tokens": ["Die", "zar\u00b7ten", "L\u00e4m\u00b7mer", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergn\u00fcgt um meinen Fu\u00df,", "tokens": ["Ver\u00b7gn\u00fcgt", "um", "mei\u00b7nen", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Turteltauben singen", "tokens": ["Die", "Tur\u00b7tel\u00b7tau\u00b7ben", "sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und girren Morgengru\u00df.", "tokens": ["Und", "gir\u00b7ren", "Mor\u00b7gen\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Rosenstrauch mit Gr\u00fc\u00dfen", "tokens": ["Der", "Ro\u00b7sen\u00b7strauch", "mit", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beut seine Kinder dar,", "tokens": ["Beut", "sei\u00b7ne", "Kin\u00b7der", "dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Thale dort der s\u00fc\u00dfen", "tokens": ["Im", "Tha\u00b7le", "dort", "der", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Violen blaue Schaar.", "tokens": ["Vi\u00b7o\u00b7len", "blau\u00b7e", "Schaar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn ich Kr\u00e4nze winde", "tokens": ["Und", "wenn", "ich", "Kr\u00e4n\u00b7ze", "win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ert\u00f6nt und rauscht der Hain,", "tokens": ["Er\u00b7t\u00f6nt", "und", "rauscht", "der", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es duftet mir die Linde", "tokens": ["Es", "duf\u00b7tet", "mir", "die", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im goldnen Mondenschein.", "tokens": ["Im", "gold\u00b7nen", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Zwietracht bleibt dahinten,", "tokens": ["Die", "Zwiet\u00b7racht", "bleibt", "da\u00b7hin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Stolz, Verfolgung, Neid,", "tokens": ["Und", "Stolz", ",", "Ver\u00b7fol\u00b7gung", ",", "Neid", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann nicht die Wege finden", "tokens": ["Kann", "nicht", "die", "We\u00b7ge", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hierher zur goldnen Zeit.", "tokens": ["Hier\u00b7her", "zur", "gold\u00b7nen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Vor mir stehn holde Scherze", "tokens": ["Vor", "mir", "stehn", "hol\u00b7de", "Scher\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und tr\u00fcbe Sorge weicht;", "tokens": ["Und", "tr\u00fc\u00b7be", "Sor\u00b7ge", "weicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Allein mein innres Herze", "tokens": ["Al\u00b7lein", "mein", "inn\u00b7res", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wird darum doch nicht leicht.", "tokens": ["Wird", "da\u00b7rum", "doch", "nicht", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Weil ich die Liebe kannte", "tokens": ["Weil", "ich", "die", "Lie\u00b7be", "kann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Blick und Ku\u00df verstand,", "tokens": ["Und", "Blick", "und", "Ku\u00df", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So bin ich nun Verbannte", "tokens": ["So", "bin", "ich", "nun", "Ver\u00b7bann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weit ab im fernen Land.", "tokens": ["Weit", "ab", "im", "fer\u00b7nen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Freude macht mich tr\u00fcbe,", "tokens": ["Die", "Freu\u00b7de", "macht", "mich", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dunkelt den stillen Sinn,", "tokens": ["Dun\u00b7kelt", "den", "stil\u00b7len", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Denn meine zarte Liebe", "tokens": ["Denn", "mei\u00b7ne", "zar\u00b7te", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist nun auf ewig hin. \u2013", "tokens": ["Ist", "nun", "auf", "e\u00b7wig", "hin", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Erinnre und equicke", "tokens": ["E\u00b7rinn\u00b7re", "und", "e\u00b7qui\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich an vergangner Lust,", "tokens": ["Dich", "an", "ver\u00b7gang\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am schwermuthsvollen Gl\u00fccke,", "tokens": ["Am", "schwer\u00b7muths\u00b7vol\u00b7len", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn sonst zerspringt die Brust.", "tokens": ["Denn", "sonst", "zer\u00b7springt", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die Morgenr\u00f6the l\u00e4chelt", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "l\u00e4\u00b7chelt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir zwar noch ofte zu,", "tokens": ["Mir", "zwar", "noch", "of\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und matte Hoffnung f\u00e4chelt", "tokens": ["Und", "mat\u00b7te", "Hoff\u00b7nung", "f\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich dann in sch\u00f6nre Ruh:", "tokens": ["Mich", "dann", "in", "sch\u00f6n\u00b7re", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da\u00df ich ihn wieder finde,", "tokens": ["Da\u00df", "ich", "ihn", "wie\u00b7der", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den ich wohl sonst gekannt,", "tokens": ["Den", "ich", "wohl", "sonst", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und da\u00df sich um uns winde", "tokens": ["Und", "da\u00df", "sich", "um", "uns", "win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein gl\u00fcckgewirktes Band.", "tokens": ["Ein", "gl\u00fcck\u00b7ge\u00b7wirk\u00b7tes", "Band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wer wei\u00df, durch welche Schatten", "tokens": ["Wer", "wei\u00df", ",", "durch", "wel\u00b7che", "Schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Fu\u00df schon heute geht,", "tokens": ["Sein", "Fu\u00df", "schon", "heu\u00b7te", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann k\u00f6mmt er \u00fcber Matten", "tokens": ["Dann", "k\u00f6mmt", "er", "\u00fc\u00b7ber", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles ist verweht,", "tokens": ["Und", "al\u00b7les", "ist", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Die Seufzer und die Thr\u00e4nen,", "tokens": ["Die", "Seuf\u00b7zer", "und", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie l\u00f6scht das neue Gl\u00fcck,", "tokens": ["Sie", "l\u00f6scht", "das", "neu\u00b7e", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Hoffen, F\u00fcrchten, Sehnen", "tokens": ["Und", "Hof\u00b7fen", ",", "F\u00fcrch\u00b7ten", ",", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verschmilzt in Einen Blick.", "tokens": ["Ver\u00b7schmilzt", "in", "Ei\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}