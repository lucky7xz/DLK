{"textgrid.poem.54803": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "17. Wundsch des Dichters als er umb die newe Jahrs-Zeit verlobet 1649", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr/ der du dich mit unserm Fleisch verm\u00e4hlt/", "tokens": ["Herr", "/", "der", "du", "dich", "mit", "un\u00b7serm", "Fleisch", "ver\u00b7m\u00e4hlt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und/ wie wir/ Jahr auff diser Welt gezehlt/", "tokens": ["Und", "/", "wie", "wir", "/", "Jahr", "auff", "di\u00b7ser", "Welt", "ge\u00b7zehlt", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWAV", "PPER", "$(", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gib da\u00df ich/ und die du mir wilst verbinden/", "tokens": ["Gib", "da\u00df", "ich", "/", "und", "die", "du", "mir", "wilst", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "$(", "KON", "PRELS", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich m\u00f6g in mir und ich in ihr dich finden/", "tokens": ["Dich", "m\u00f6g", "in", "mir", "und", "ich", "in", "ihr", "dich", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "KON", "PPER", "APPR", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wo wir zwey in einem Fleische seyn:", "tokens": ["Und", "wo", "wir", "zwey", "in", "ei\u00b7nem", "Flei\u00b7sche", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "CARD", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So gib vilmehr nur einen Geist uns ein.", "tokens": ["So", "gib", "vil\u00b7mehr", "nur", "ei\u00b7nen", "Geist", "uns", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}