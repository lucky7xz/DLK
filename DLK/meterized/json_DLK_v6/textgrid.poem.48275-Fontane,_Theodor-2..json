{"textgrid.poem.48275": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herbst ist da. Die Lust, zu jagen,", "tokens": ["Der", "Herbst", "ist", "da", ".", "Die", "Lust", ",", "zu", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "ART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lockt aus der Stadt nach Windsor-Schlo\u00df,", "tokens": ["Lockt", "aus", "der", "Stadt", "nach", "Wind\u00b7sor\u00b7Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und jetzt, vorbei an Heck' und Hagen,", "tokens": ["Und", "jetzt", ",", "vor\u00b7bei", "an", "Heck'", "und", "Ha\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "APPR", "NE", "KON", "NE", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Bricht Jakob und sein J\u00e4gertro\u00df.", "tokens": ["Bricht", "Ja\u00b7kob", "und", "sein", "J\u00e4\u00b7ger\u00b7tro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Welch Leben das! Die Rosse sch\u00e4umen,", "tokens": ["Welch", "Le\u00b7ben", "das", "!", "Die", "Ros\u00b7se", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "$.", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Meute klafft, die Pfeife gellt,", "tokens": ["Die", "Meu\u00b7te", "klafft", ",", "die", "Pfei\u00b7fe", "gellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Wald erwacht aus seinen Tr\u00e4umen", "tokens": ["Der", "Wald", "er\u00b7wacht", "aus", "sei\u00b7nen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und schauert, wenn ein Opfer f\u00e4llt.", "tokens": ["Und", "schau\u00b7ert", ",", "wenn", "ein", "Op\u00b7fer", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schon dunkelt's. Doch das Blutvergeuden", "tokens": ["Schon", "dun\u00b7kelt'", "s.", "Doch", "das", "Blut\u00b7ver\u00b7geu\u00b7den"], "token_info": ["word", "word", "abbreviation", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es dauert fort bis in die Nacht,", "tokens": ["Es", "dau\u00b7ert", "fort", "bis", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Bis D\u00fcrsten nach des Mahles Freuden", "tokens": ["Bis", "D\u00fcrs\u00b7ten", "nach", "des", "Mah\u00b7les", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dem Durst nach Blut ein Ende macht.", "tokens": ["Dem", "Durst", "nach", "Blut", "ein", "En\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Heim ruft das Horn. Bald in den R\u00e4umen", "tokens": ["Heim", "ruft", "das", "Horn", ".", "Bald", "in", "den", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Schlosses l\u00e4rmt man beim Bankett,", "tokens": ["Des", "Schlos\u00b7ses", "l\u00e4rmt", "man", "beim", "Ban\u00b7kett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man zecht, und statt der Rosse Sch\u00e4umen", "tokens": ["Man", "zecht", ",", "und", "statt", "der", "Ros\u00b7se", "Sch\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00e4umt Wein und Lust jetzt um die Wett',", "tokens": ["Sch\u00e4umt", "Wein", "und", "Lust", "jetzt", "um", "die", "Wett'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Toaste schallen hundertt\u00f6nig,", "tokens": ["Toas\u00b7te", "schal\u00b7len", "hun\u00b7dert\u00b7t\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Wein verschwistert Alt und Jung,", "tokens": ["Der", "Wein", "ver\u00b7schwis\u00b7tert", "Alt", "und", "Jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00fcstern bringt zuletzt der K\u00f6nig", "tokens": ["Und", "l\u00fcs\u00b7tern", "bringt", "zu\u00b7letzt", "der", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Damen seine Huldigung.", "tokens": ["Den", "Da\u00b7men", "sei\u00b7ne", "Hul\u00b7di\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "\u00bbdie Sch\u00f6nen hoch!\u00ab Der trunkne Alte,", "tokens": ["\u00bb", "die", "Sch\u00f6\u00b7nen", "hoch", "!", "\u00ab", "Der", "trunk\u00b7ne", "Al\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJD", "$.", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Matt blinzelnd ruft er's durch den Saal,", "tokens": ["Matt", "blin\u00b7zelnd", "ruft", "er's", "durch", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Dieweil der k\u00f6nigliche Zecher", "tokens": ["Die\u00b7weil", "der", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umsonst nach ihren Z\u00fcgen gafft,", "tokens": ["Um\u00b7sonst", "nach", "ih\u00b7ren", "Z\u00fc\u00b7gen", "gafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Leert sie den ysopbittren Becher", "tokens": ["Leert", "sie", "den", "ysop\u00b7bit\u00b7tren", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Zur\u00fcckgewiesner Leidenschaft.", "tokens": ["Zu\u00b7r\u00fcck\u00b7ge\u00b7wi\u00b7es\u00b7ner", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie, die bei tausend Huldigungen", "tokens": ["Sie", ",", "die", "bei", "tau\u00b7send", "Hul\u00b7di\u00b7gun\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Herz mit kaltem Stolz bew\u00e4hrt,", "tokens": ["Ihr", "Herz", "mit", "kal\u00b7tem", "Stolz", "be\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sieht jeden Sieg, den sie errungen,", "tokens": ["Sieht", "je\u00b7den", "Sieg", ",", "den", "sie", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In Niederlage jetzt verkehrt,", "tokens": ["In", "Nie\u00b7der\u00b7la\u00b7ge", "jetzt", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie gl\u00fcht, und hinter Teppichw\u00e4nden", "tokens": ["Sie", "gl\u00fcht", ",", "und", "hin\u00b7ter", "Tep\u00b7pich\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Hervor aus wohlgeborgnem Schrank", "tokens": ["Her\u00b7vor", "aus", "wohl\u00b7ge\u00b7borg\u00b7nem", "Schrank"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nimmt sie den aus ital'schen H\u00e4nden", "tokens": ["Nimmt", "sie", "den", "aus", "i\u00b7tal'\u00b7schen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Heut erst erkauften Liebestrank.", "tokens": ["Heut", "erst", "er\u00b7kauf\u00b7ten", "Lie\u00b7bes\u00b7trank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "Und schon weiter bauend,", "tokens": ["Und", "schon", "wei\u00b7ter", "bau\u00b7end", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das Fl\u00e4schchen in gekrampfter Hand,", "tokens": ["Das", "Fl\u00e4\u00b7schchen", "in", "ge\u00b7krampf\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stutzt pl\u00f6tzlich sie, sich selbst erschauend", "tokens": ["Stutzt", "pl\u00f6tz\u00b7lich", "sie", ",", "sich", "selbst", "er\u00b7schau\u00b7end"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "$,", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gen\u00fcber in der Spiegelwand.", "tokens": ["Ge\u00b7n\u00fc\u00b7ber", "in", "der", "Spie\u00b7gel\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist, als fasse sie ein Staunen", "tokens": ["Es", "ist", ",", "als", "fas\u00b7se", "sie", "ein", "Stau\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vor ihrem eignen Ebenbild,", "tokens": ["Vor", "ih\u00b7rem", "eig\u00b7nen", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie h\u00f6rt den Stolz im Busen raunen:", "tokens": ["Sie", "h\u00f6rt", "den", "Stolz", "im", "Bu\u00b7sen", "rau\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.10": {"text": "Hin klirrt das Glas in Splitterscherben:", "tokens": ["Hin", "klirrt", "das", "Glas", "in", "Split\u00b7ter\u00b7scher\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbfahr wohl! ... Du k\u00fcmmerlicher Saft", "tokens": ["\u00bb", "fahr", "wohl", "!", "...", "Du", "k\u00fcm\u00b7mer\u00b7li\u00b7cher", "Saft"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "$.", "$(", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sollst nicht um Liebe f\u00fcr mich werben", "tokens": ["Sollst", "nicht", "um", "Lie\u00b7be", "f\u00fcr", "mich", "wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "APPR", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und spotten meiner eignen Kraft.", "tokens": ["Und", "spot\u00b7ten", "mei\u00b7ner", "eig\u00b7nen", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Traun, ob der alte H\u00f6llenmeister", "tokens": ["Traun", ",", "ob", "der", "al\u00b7te", "H\u00f6l\u00b7len\u00b7meis\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Auch selber dich bereitet h\u00e4tt',", "tokens": ["Auch", "sel\u00b7ber", "dich", "be\u00b7rei\u00b7tet", "h\u00e4tt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Gilt's Herrschaft \u00fcber Sinn und Geister,", "tokens": ["Gilt's", "Herr\u00b7schaft", "\u00fc\u00b7ber", "Sinn", "und", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ich biete dir und ihm die Wett';", "tokens": ["Ich", "bie\u00b7te", "dir", "und", "ihm", "die", "Wett'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nur fort der letzte Rest von L\u00fcge,", "tokens": ["Nur", "fort", "der", "letz\u00b7te", "Rest", "von", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "All Schein und Maske fahre hin,", "tokens": ["All", "Schein", "und", "Mas\u00b7ke", "fah\u00b7re", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sehn soll er meine wahren Z\u00fcge,", "tokens": ["Sehn", "soll", "er", "mei\u00b7ne", "wah\u00b7ren", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und siegen werd' ich, wie ich bin ...\u00ab", "tokens": ["Und", "sie\u00b7gen", "werd'", "ich", ",", "wie", "ich", "bin", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}