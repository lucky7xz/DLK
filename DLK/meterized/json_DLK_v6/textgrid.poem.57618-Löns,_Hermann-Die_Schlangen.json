{"textgrid.poem.57618": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlangen", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott Apollo, gib mir T\u00f6ne,", "tokens": ["Gott", "A\u00b7pol\u00b7lo", ",", "gib", "mir", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVIMP", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich diese stolze Sch\u00f6ne", "tokens": ["Da\u00df", "ich", "die\u00b7se", "stol\u00b7ze", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach Geb\u00fchr besingen kann,", "tokens": ["Nach", "Ge\u00b7b\u00fchr", "be\u00b7sin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deren ellenlange Z\u00f6pfe", "tokens": ["De\u00b7ren", "el\u00b7len\u00b7lan\u00b7ge", "Z\u00f6p\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses frommen Nestes K\u00f6pfe", "tokens": ["Die\u00b7ses", "from\u00b7men", "Nes\u00b7tes", "K\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ziehn in ihren Blondhaarbann.", "tokens": ["Ziehn", "in", "ih\u00b7ren", "Blond\u00b7ha\u00b7ar\u00b7bann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Diese semmelblonden, langen", "tokens": ["Die\u00b7se", "sem\u00b7mel\u00b7blon\u00b7den", ",", "lan\u00b7gen"], "token_info": ["word", "word", "punct", "word"], "pos": ["PDAT", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grazi\u00f6sen Riesenschlangen", "tokens": ["Gra\u00b7zi\u00b7\u00f6\u00b7sen", "Rie\u00b7sen\u00b7schlan\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben auch mein Herz ber\u00fcckt,", "tokens": ["Ha\u00b7ben", "auch", "mein", "Herz", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich gesteh, es war abscheulich,", "tokens": ["Ich", "ge\u00b7steh", ",", "es", "war", "ab\u00b7scheu\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Im Konzerte hab' ich neulich", "tokens": ["Im", "Kon\u00b7zer\u00b7te", "hab'", "ich", "neu\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Taub nach ihr nur hingeblickt.", "tokens": ["Taub", "nach", "ihr", "nur", "hin\u00b7ge\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie der Schlangenzwilling wehte,", "tokens": ["Wie", "der", "Schlan\u00b7gen\u00b7zwil\u00b7ling", "weh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der blonde Kopf sich drehte,", "tokens": ["Wenn", "der", "blon\u00b7de", "Kopf", "sich", "dreh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heiliger Antonius!", "tokens": ["Hei\u00b7li\u00b7ger", "An\u00b7to\u00b7ni\u00b7us", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt versteh' ich deine Qualen,", "tokens": ["Jetzt", "ver\u00b7steh'", "ich", "dei\u00b7ne", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als besucht dich dazumalen", "tokens": ["Als", "be\u00b7sucht", "dich", "da\u00b7zu\u00b7ma\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jener fesche Genius.", "tokens": ["Je\u00b7ner", "fe\u00b7sche", "Ge\u00b7nius", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Bibel, Gei\u00dfel, Totenk\u00f6pfe", "tokens": ["Bi\u00b7bel", ",", "Gei\u00b7\u00dfel", ",", "To\u00b7ten\u00b7k\u00f6p\u00b7fe"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Helfen nichts, wenn blonde Z\u00f6pfe", "tokens": ["Hel\u00b7fen", "nichts", ",", "wenn", "blon\u00b7de", "Z\u00f6p\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PIS", "$,", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihnen keck den Krieg erkl\u00e4rt.", "tokens": ["Ih\u00b7nen", "keck", "den", "Krieg", "er\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich bin kein Heiliger, leider,", "tokens": ["Und", "ich", "bin", "kein", "Hei\u00b7li\u00b7ger", ",", "lei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "$,", "ADV", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Trage keine h\u00e4rnen Kleider,", "tokens": ["Tra\u00b7ge", "kei\u00b7ne", "h\u00e4r\u00b7nen", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin nicht d\u00fcrr und abgezehrt.", "tokens": ["Bin", "nicht", "d\u00fcrr", "und", "ab\u00b7ge\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Darum tu ich dir, du Holde,", "tokens": ["Da\u00b7rum", "tu", "ich", "dir", ",", "du", "Hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir und deinem Kopfhaargolde", "tokens": ["Dir", "und", "dei\u00b7nem", "Kopf\u00b7haar\u00b7gol\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Krieg und Kampf zu wissen kund,", "tokens": ["Krieg", "und", "Kampf", "zu", "wis\u00b7sen", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinen Lippen, deinen warmen,", "tokens": ["Dei\u00b7nen", "Lip\u00b7pen", ",", "dei\u00b7nen", "war\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00c4rmelknappen, weichen Armen,", "tokens": ["\u00c4r\u00b7mel\u00b7knap\u00b7pen", ",", "wei\u00b7chen", "Ar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deinem scharfgeschnitt'nen Mund.", "tokens": ["Dei\u00b7nem", "scha\u00b7rf\u00b7ge\u00b7schnitt'\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Erste Schlacht \u2013 je eh'r, je besser,", "tokens": ["Ers\u00b7te", "Schlacht", "\u2013", "je", "eh'r", ",", "je", "bes\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "K\u00e4mpfen will ich bis aufs Messer,", "tokens": ["K\u00e4mp\u00b7fen", "will", "ich", "bis", "aufs", "Mes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Horch, die Trommel wird ger\u00fchrt,", "tokens": ["Horch", ",", "die", "Trom\u00b7mel", "wird", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterbend werde ich verbluten", "tokens": ["Ster\u00b7bend", "wer\u00b7de", "ich", "ver\u00b7blu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder du von Liebesgluten", "tokens": ["O\u00b7der", "du", "von", "Lie\u00b7bes\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Krank mir an das Herz gef\u00fchrt.", "tokens": ["Krank", "mir", "an", "das", "Herz", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}