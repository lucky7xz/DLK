{"textgrid.poem.49919": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Waldplage", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Walde deucht mir alles miteinander sch\u00f6n,", "tokens": ["Im", "Wal\u00b7de", "deucht", "mir", "al\u00b7les", "mi\u00b7tein\u00b7an\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nichts Mi\u00dfliebiges darin, so vielerlei", "tokens": ["Und", "nichts", "Mi\u00df\u00b7lie\u00b7bi\u00b7ges", "da\u00b7rin", ",", "so", "vie\u00b7ler\u00b7lei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "NN", "PAV", "$,", "ADV", "PIAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er hegen mag; es krieche zwischen Gras und Moos", "tokens": ["Er", "he\u00b7gen", "mag", ";", "es", "krie\u00b7che", "zwi\u00b7schen", "Gras", "und", "Moos"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Am Boden, oder jage rei\u00dfend durchs Geb\u00fcsch,", "tokens": ["Am", "Bo\u00b7den", ",", "o\u00b7der", "ja\u00b7ge", "rei\u00b7\u00dfend", "durchs", "Ge\u00b7b\u00fcsch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es singe oder kreische von den Gipfeln hoch,", "tokens": ["Es", "sin\u00b7ge", "o\u00b7der", "krei\u00b7sche", "von", "den", "Gip\u00b7feln", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und hacke mit dem Schnabel in der Fichte Stamm,", "tokens": ["Und", "ha\u00b7cke", "mit", "dem", "Schna\u00b7bel", "in", "der", "Fich\u00b7te", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df lieblich sie ert\u00f6net durch den ganzen Saal.", "tokens": ["Da\u00df", "lieb\u00b7lich", "sie", "er\u00b7t\u00f6\u00b7net", "durch", "den", "gan\u00b7zen", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja machte je sich irgend etwas unbequem,", "tokens": ["Ja", "mach\u00b7te", "je", "sich", "ir\u00b7gend", "et\u00b7was", "un\u00b7be\u00b7quem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ADV", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verdrie\u00dft es nicht, zu suchen einen andern Sitz,", "tokens": ["Ver\u00b7drie\u00dft", "es", "nicht", ",", "zu", "su\u00b7chen", "ei\u00b7nen", "an\u00b7dern", "Sitz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der sch\u00f6ner bald, der allersch\u00f6nste, dich bed\u00fcnkt.", "tokens": ["Der", "sch\u00f6\u00b7ner", "bald", ",", "der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", ",", "dich", "be\u00b7d\u00fcnkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "ART", "ADJA", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein einzig \u00dcbel aber hat der Wald f\u00fcr mich,", "tokens": ["Ein", "ein\u00b7zig", "\u00dc\u00b7bel", "a\u00b7ber", "hat", "der", "Wald", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein grausames und unausweichliches beinah.", "tokens": ["Ein", "grau\u00b7sa\u00b7mes", "und", "un\u00b7aus\u00b7weich\u00b7li\u00b7ches", "bei\u00b7nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sogleich beschreib ich dieses Scheusal, da\u00df ihr's kennt;", "tokens": ["Sog\u00b7leich", "be\u00b7schreib", "ich", "die\u00b7ses", "Scheu\u00b7sal", ",", "da\u00df", "ih\u00b7r's", "kennt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Noch kennt ihr's kaum, und merkt es nicht, bis unversehns", "tokens": ["Noch", "kennt", "ih\u00b7r's", "kaum", ",", "und", "merkt", "es", "nicht", ",", "bis", "un\u00b7ver\u00b7sehns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "NE"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Die Hand euch und, noch schrecklicher, die Wange schmerzt.", "tokens": ["Die", "Hand", "euch", "und", ",", "noch", "schreck\u00b7li\u00b7cher", ",", "die", "Wan\u00b7ge", "schmerzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "$,", "ADV", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gefl\u00fcgelt kommt es, s\u00e4uselnd, fast unh\u00f6rbarlich;", "tokens": ["Ge\u00b7fl\u00fc\u00b7gelt", "kommt", "es", ",", "s\u00e4u\u00b7selnd", ",", "fast", "un\u00b7h\u00f6r\u00b7bar\u00b7lich", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Auf F\u00fc\u00dfen, zweimal dreien, ist es hoch gestellt", "tokens": ["Auf", "F\u00fc\u00b7\u00dfen", ",", "zwei\u00b7mal", "drei\u00b7en", ",", "ist", "es", "hoch", "ge\u00b7stellt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ADV", "CARD", "$,", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "(deswegen ich in Versen es zu schm\u00e4hen auch", "tokens": ["(", "des\u00b7we\u00b7gen", "ich", "in", "Ver\u00b7sen", "es", "zu", "schm\u00e4\u00b7hen", "auch"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PAV", "PPER", "APPR", "NN", "PPER", "PTKZU", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Den klassischen Senarium mit Fug erw\u00e4hlt);", "tokens": ["Den", "klas\u00b7si\u00b7schen", "Se\u00b7na\u00b7ri\u00b7um", "mit", "Fug", "er\u00b7w\u00e4hlt", ")", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$(", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und wie es anfliegt, augenblicklich l\u00e4sset es", "tokens": ["Und", "wie", "es", "an\u00b7fliegt", ",", "au\u00b7gen\u00b7blick\u00b7lich", "l\u00e4s\u00b7set", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.21": {"text": "Den langen R\u00fcssel senkrecht in die zarte Haut;", "tokens": ["Den", "lan\u00b7gen", "R\u00fcs\u00b7sel", "sen\u00b7krecht", "in", "die", "zar\u00b7te", "Haut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Erschrocken schlagt ihr schnell darnach, jedoch umsonst,", "tokens": ["Er\u00b7schro\u00b7cken", "schlagt", "ihr", "schnell", "dar\u00b7nach", ",", "je\u00b7doch", "um\u00b7sonst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PAV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn, grazi\u00f6ser Wendung, schon entschwebet es.", "tokens": ["Denn", ",", "gra\u00b7zi\u00b7\u00f6\u00b7ser", "Wen\u00b7dung", ",", "schon", "ent\u00b7schwe\u00b7bet", "es", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und alsobald, entz\u00fcndet von dem raschen Gift,", "tokens": ["Und", "al\u00b7so\u00b7bald", ",", "ent\u00b7z\u00fcn\u00b7det", "von", "dem", "ra\u00b7schen", "Gift", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Schwillt euch die Hand zum ungestalten Kissen auf,", "tokens": ["Schwillt", "euch", "die", "Hand", "zum", "un\u00b7ge\u00b7stal\u00b7ten", "Kis\u00b7sen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und juckt und spannt und brennet zum Verzweifeln euch", "tokens": ["Und", "juckt", "und", "spannt", "und", "bren\u00b7net", "zum", "Ver\u00b7zwei\u00b7feln", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Viel Stunden, ja zuweilen noch den dritten Tag.", "tokens": ["Viel", "Stun\u00b7den", ",", "ja", "zu\u00b7wei\u00b7len", "noch", "den", "drit\u00b7ten", "Tag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So unter meiner Lieblingsfichte sa\u00df ich j\u00fcngst\u2013", "tokens": ["So", "un\u00b7ter", "mei\u00b7ner", "Lieb\u00b7lings\u00b7fich\u00b7te", "sa\u00df", "ich", "j\u00fcngst", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Zur Lehne wie gedrechselt f\u00fcr den R\u00fccken, steigt", "tokens": ["Zur", "Leh\u00b7ne", "wie", "ge\u00b7drech\u00b7selt", "f\u00fcr", "den", "R\u00fc\u00b7cken", ",", "steigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "KOKOM", "VVPP", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Zwiest\u00e4mmig, nah dem Boden, sie als Gabel auf \u2013", "tokens": ["Zwie\u00b7st\u00e4m\u00b7mig", ",", "nah", "dem", "Bo\u00b7den", ",", "sie", "als", "Ga\u00b7bel", "auf", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ART", "NN", "$,", "PPER", "KOUS", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Den Dichter lesend, den ich jahrelang verga\u00df:", "tokens": ["Den", "Dich\u00b7ter", "le\u00b7send", ",", "den", "ich", "jah\u00b7re\u00b7lang", "ver\u00b7ga\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "An Fanny singt er, Cidli und den Z\u00fcrcher See,", "tokens": ["An", "Fan\u00b7ny", "singt", "er", ",", "Cid\u00b7li", "und", "den", "Z\u00fcr\u00b7cher", "See", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "$,", "XY", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die fr\u00fchen Gr\u00e4ber und des Rheines goldnen Wein", "tokens": ["Die", "fr\u00fc\u00b7hen", "Gr\u00e4\u00b7ber", "und", "des", "Rhei\u00b7nes", "gold\u00b7nen", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "(o sein Gestade br\u00fctet jenes Greuels auch", "tokens": ["(", "o", "sein", "Ge\u00b7sta\u00b7de", "br\u00fc\u00b7tet", "je\u00b7nes", "Greu\u00b7els", "auch"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "PPOSAT", "NN", "VVFIN", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein gr\u00f6\u00dferes Geschlechte noch und schlimmres aus,", "tokens": ["Ein", "gr\u00f6\u00b7\u00dfe\u00b7res", "Ge\u00b7schlech\u00b7te", "noch", "und", "schlimm\u00b7res", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "KON", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ich kenn es wohl, doch h\u00f6flicher dem Gaste war's). \u2013", "tokens": ["Ich", "kenn", "es", "wohl", ",", "doch", "h\u00f6f\u00b7li\u00b7cher", "dem", "Gas\u00b7te", "wa\u00b7r's", ")", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "ART", "NN", "VAFIN", "$(", "$.", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.37": {"text": "Nun aber hatte geigend schon ein kleiner Trupp", "tokens": ["Nun", "a\u00b7ber", "hat\u00b7te", "gei\u00b7gend", "schon", "ein", "klei\u00b7ner", "Trupp"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mich ausgewittert, den geruhig Sitzenden;", "tokens": ["Mich", "aus\u00b7ge\u00b7wit\u00b7tert", ",", "den", "ge\u00b7ru\u00b7hig", "Sit\u00b7zen\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PRELS", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.39": {"text": "Mir um die Schl\u00e4fe tanzet er in L\u00fcsternheit.", "tokens": ["Mir", "um", "die", "Schl\u00e4\u00b7fe", "tan\u00b7zet", "er", "in", "L\u00fcs\u00b7tern\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ein Stich! der erste! er emp\u00f6rt die Galle schon.", "tokens": ["Ein", "Stich", "!", "der", "ers\u00b7te", "!", "er", "em\u00b7p\u00f6rt", "die", "Gal\u00b7le", "schon", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Zerstreuten Sinnes immer schiel ich \u00fcbers Blatt.", "tokens": ["Zer\u00b7streu\u00b7ten", "Sin\u00b7nes", "im\u00b7mer", "schiel", "ich", "\u00fc\u00b7bers", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ein zweiter macht, ein dritter, mich zum Rasenden.", "tokens": ["Ein", "zwei\u00b7ter", "macht", ",", "ein", "drit\u00b7ter", ",", "mich", "zum", "Ra\u00b7sen\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "ART", "ADJA", "$,", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Das holde Zwillings-Nymphen-Paar des Fichtenbaums", "tokens": ["Das", "hol\u00b7de", "Zwil\u00b7lings\u00b7Nym\u00b7phen\u00b7Paar", "des", "Fich\u00b7ten\u00b7baums"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Vernahm da Worte, die es nicht bei mir gesucht;", "tokens": ["Ver\u00b7nahm", "da", "Wor\u00b7te", ",", "die", "es", "nicht", "bei", "mir", "ge\u00b7sucht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,", "PRELS", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Zuletzt geboten sie mir fl\u00fcsternd M\u00e4\u00dfigung:", "tokens": ["Zu\u00b7letzt", "ge\u00b7bo\u00b7ten", "sie", "mir", "fl\u00fcs\u00b7ternd", "M\u00e4\u00b7\u00dfi\u00b7gung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVPP", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "Wo nicht, so sollt ich meiden ihren Ruhbezirk.", "tokens": ["Wo", "nicht", ",", "so", "sollt", "ich", "mei\u00b7den", "ih\u00b7ren", "Ruh\u00b7be\u00b7zirk", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Besch\u00e4mt gehorcht ich, sinnend still auf Grausamtat.", "tokens": ["Be\u00b7sch\u00e4mt", "ge\u00b7horcht", "ich", ",", "sin\u00b7nend", "still", "auf", "Grau\u00b7sam\u00b7tat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ADJD", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ich hielt ge\u00f6ffnet auf der flachen Hand das Buch,", "tokens": ["Ich", "hielt", "ge\u00b7\u00f6ff\u00b7net", "auf", "der", "fla\u00b7chen", "Hand", "das", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Das schwebende Geziefer, wie sich eines naht,", "tokens": ["Das", "schwe\u00b7ben\u00b7de", "Ge\u00b7zie\u00b7fer", ",", "wie", "sich", "ei\u00b7nes", "naht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Mit raschem Klapp zu t\u00f6ten. Ha! da kommt schon eins!", "tokens": ["Mit", "ra\u00b7schem", "Klapp", "zu", "t\u00f6\u00b7ten", ".", "Ha", "!", "da", "kommt", "schon", "eins", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$.", "ITJ", "$.", "ADV", "VVFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "\u00bbdu fliehst! o bleibe, eile nicht, Gedankenfreund!\u00ab", "tokens": ["\u00bb", "du", "fliehst", "!", "o", "blei\u00b7be", ",", "ei\u00b7le", "nicht", ",", "Ge\u00b7dan\u00b7ken\u00b7freund", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "FM", "VVFIN", "$,", "VVFIN", "PTKNEG", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "(dem hohen Mond rief jener Dichter zu dies Wort.)", "tokens": ["(", "dem", "ho\u00b7hen", "Mond", "rief", "je\u00b7ner", "Dich\u00b7ter", "zu", "dies", "Wort", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PDAT", "NN", "APPR", "PDS", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Patsch! Hab ich dich, Kanaille, oder hab ich nicht?", "tokens": ["Patsch", "!", "Hab", "ich", "dich", ",", "Ka\u00b7nail\u00b7le", ",", "o\u00b7der", "hab", "ich", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PPER", "PRF", "$,", "VMFIN", "$,", "KON", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Und hastig \u2013 denn schon hatte meine Mordbegier", "tokens": ["Und", "has\u00b7tig", "\u2013", "denn", "schon", "hat\u00b7te", "mei\u00b7ne", "Mord\u00b7be\u00b7gier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$(", "KON", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Zum stillen Wahnsinn sich verirrt, zum kleinlichen \u2013", "tokens": ["Zum", "stil\u00b7len", "Wahn\u00b7sinn", "sich", "ver\u00b7irrt", ",", "zum", "klein\u00b7li\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVPP", "$,", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Begierig bl\u00e4ttr' ich: ja, da liegst du plattgedr\u00fcckt,", "tokens": ["Be\u00b7gie\u00b7rig", "bl\u00e4t\u00b7tr'", "ich", ":", "ja", ",", "da", "liegst", "du", "platt\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "PTKANT", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.57": {"text": "Bevor du stachst, nun aber stichst du nimmermehr,", "tokens": ["Be\u00b7vor", "du", "stachst", ",", "nun", "a\u00b7ber", "stichst", "du", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Du zierlich Langgebeinetes, Jungfr\u00e4uliches!", "tokens": ["Du", "zier\u00b7lich", "Lang\u00b7ge\u00b7bei\u00b7ne\u00b7tes", ",", "Jung\u00b7fr\u00e4u\u00b7li\u00b7ches", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "\u2013 Also, nicht achtend eines sch\u00f6nen Buchs Verderb,", "tokens": ["\u2013", "Al\u00b7so", ",", "nicht", "ach\u00b7tend", "ei\u00b7nes", "sch\u00f6\u00b7nen", "Buchs", "Ver\u00b7derb", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PTKNEG", "ADJD", "ART", "ADJA", "NN", "NN", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.60": {"text": "Trieb ich erheitert lange noch die schn\u00f6de Jagd,", "tokens": ["Trieb", "ich", "er\u00b7hei\u00b7tert", "lan\u00b7ge", "noch", "die", 