{"textgrid.poem.38331": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "12. Erziehung durch Ueberzeugung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem, welcher unverwirret", "tokens": ["Wohl", "dem", ",", "wel\u00b7cher", "un\u00b7ver\u00b7wir\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der irdischen Unruh", "tokens": ["Von", "der", "ir\u00b7di\u00b7schen", "Un\u00b7ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wie ein einsam T\u00e4ublein girret,", "tokens": ["Wie", "ein", "ein\u00b7sam", "T\u00e4ub\u00b7lein", "gir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und fleugt holen Felsen zu,", "tokens": ["Und", "fleugt", "ho\u00b7len", "Fel\u00b7sen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dessen Herz auf Gott gericht,", "tokens": ["Des\u00b7sen", "Herz", "auf", "Gott", "ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Horchet, was er zu ihm spricht.", "tokens": ["Hor\u00b7chet", ",", "was", "er", "zu", "ihm", "spricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl dem, welcher nimmt die Haue,", "tokens": ["Wohl", "dem", ",", "wel\u00b7cher", "nimmt", "die", "Hau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grabet, hackt mit Lust und Schmerz,", "tokens": ["Gra\u00b7bet", ",", "hackt", "mit", "Lust", "und", "Schmerz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf da\u00df er den Acker baue", "tokens": ["Auf", "da\u00df", "er", "den", "A\u00b7cker", "bau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und noch mehr sein d\u00fcrres Herz,", "tokens": ["Und", "noch", "mehr", "sein", "d\u00fcr\u00b7res", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der die Welt mit ihrer Pracht", "tokens": ["Der", "die", "Welt", "mit", "ih\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ehr, Gem\u00e4chlichkeit verlacht.", "tokens": ["Ehr", ",", "Ge\u00b7m\u00e4ch\u00b7lich\u00b7keit", "ver\u00b7lacht", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl dem, welcher dann alleine", "tokens": ["Wohl", "dem", ",", "wel\u00b7cher", "dann", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt bei einem klaren Bach,", "tokens": ["Sitzt", "bei", "ei\u00b7nem", "kla\u00b7ren", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebet nur, auf da\u00df er weine,", "tokens": ["Le\u00b7bet", "nur", ",", "auf", "da\u00df", "er", "wei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Uebe an sich selber Rach;", "tokens": ["Ue\u00b7be", "an", "sich", "sel\u00b7ber", "Rach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df der keuschen Engel Hauf", "tokens": ["Da\u00df", "der", "keu\u00b7schen", "En\u00b7gel", "Hauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fasset seine Thr\u00e4nen auf.", "tokens": ["Fas\u00b7set", "sei\u00b7ne", "Thr\u00e4\u00b7nen", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohl dem, dessen Aug und Wangen", "tokens": ["Wohl", "dem", ",", "des\u00b7sen", "Aug", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein \u00fcberstr\u00f6mend Fluth", "tokens": ["Wie", "ein", "\u00fc\u00b7ber\u00b7str\u00f6\u00b7mend", "Fluth"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "VVPP", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen Weg, den er gegangen,", "tokens": ["Sei\u00b7nen", "Weg", ",", "den", "er", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Netzet mit dem Herzensblut", "tokens": ["Net\u00b7zet", "mit", "dem", "Her\u00b7zens\u00b7blut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohl der Erde, Holz und Au,", "tokens": ["Wohl", "der", "Er\u00b7de", ",", "Holz", "und", "Au", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses ist ihr Himmelsthau.", "tokens": ["Die\u00b7ses", "ist", "ihr", "Him\u00b7mel\u00b7sthau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}