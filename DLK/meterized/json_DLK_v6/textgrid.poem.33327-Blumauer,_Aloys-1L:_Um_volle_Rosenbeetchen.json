{"textgrid.poem.33327": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um volle Rosenbeetchen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um volle Rosenbeetchen", "tokens": ["Um", "vol\u00b7le", "Ro\u00b7sen\u00b7be\u00b7et\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schw\u00e4rmt' einst zum Zeitvertreib", "tokens": ["Schw\u00e4rmt'", "einst", "zum", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein junges Zephyrettchen,", "tokens": ["Ein", "jun\u00b7ges", "Ze\u00b7phy\u00b7rett\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und suchte sich ein Weib.", "tokens": ["Und", "such\u00b7te", "sich", "ein", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der K\u00f6nigin der Rosen", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "der", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ergab der Freier sich,", "tokens": ["Er\u00b7gab", "der", "Frei\u00b7er", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu lieben und zu kosen", "tokens": ["Zu", "lie\u00b7ben", "und", "zu", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verstand er meisterlich.", "tokens": ["Ver\u00b7stand", "er", "meis\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die besten Fr\u00fchlingsd\u00fcfte", "tokens": ["Die", "bes\u00b7ten", "Fr\u00fch\u00b7lings\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bracht' er zum Morgengru\u00df,", "tokens": ["Bracht'", "er", "zum", "Mor\u00b7gen\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die lau'sten Sommerl\u00fcfte", "tokens": ["Die", "lau'\u00b7sten", "Som\u00b7mer\u00b7l\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nahm er zu seinem Ku\u00df.", "tokens": ["Nahm", "er", "zu", "sei\u00b7nem", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Seufzer stahl und kr\u00e4uselt'", "tokens": ["Und", "Seuf\u00b7zer", "stahl", "und", "kr\u00e4u\u00b7selt'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er hin zu ihrem Ohr,", "tokens": ["Er", "hin", "zu", "ih\u00b7rem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ganze Tage s\u00e4uselt'", "tokens": ["Und", "gan\u00b7ze", "Ta\u00b7ge", "s\u00e4u\u00b7selt'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Er ihr von Liebe vor.", "tokens": ["Er", "ihr", "von", "Lie\u00b7be", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bald h\u00fcpft er auf dem Teiche,", "tokens": ["Bald", "h\u00fcpft", "er", "auf", "dem", "Tei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und am\u00fcsirte sie,", "tokens": ["Und", "am\u00b7\u00fc\u00b7sir\u00b7te", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald schuf er kleine Str\u00e4uche", "tokens": ["Bald", "schuf", "er", "klei\u00b7ne", "Str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Lauben um f\u00fcr sie.", "tokens": ["Zu", "Lau\u00b7ben", "um", "f\u00fcr", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Nachtigallen T\u00f6ne", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Holt' er vom Wald herzu,", "tokens": ["Holt'", "er", "vom", "Wald", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lullte seine Sch\u00f6ne", "tokens": ["Und", "lull\u00b7te", "sei\u00b7ne", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Nachts damit in Ruh.", "tokens": ["Des", "Nachts", "da\u00b7mit", "in", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PAV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und schlief sie nun, so w\u00fchlte", "tokens": ["Und", "schlief", "sie", "nun", ",", "so", "w\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er k\u00fchn in ihrer Brust;", "tokens": ["Er", "k\u00fchn", "in", "ih\u00b7rer", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Rose tr\u00e4umt' und f\u00fchlte", "tokens": ["Die", "Ro\u00b7se", "tr\u00e4umt'", "und", "f\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die nahe G\u00f6tterlust.", "tokens": ["Die", "na\u00b7he", "G\u00f6t\u00b7ter\u00b7lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und ihre s\u00fcssen D\u00fcfte", "tokens": ["Und", "ih\u00b7re", "s\u00fcs\u00b7sen", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verschlang und sammelt' er,", "tokens": ["Ver\u00b7schlang", "und", "sam\u00b7melt'", "er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und trug sie durch die L\u00fcfte", "tokens": ["Und", "trug", "sie", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stolzirend weit umher.", "tokens": ["Stol\u00b7zi\u00b7rend", "weit", "um\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Morgentropfen k\u00fc\u00dfte", "tokens": ["Die", "Mor\u00b7gen\u00b7trop\u00b7fen", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er ihr vom Busen fr\u00fch,", "tokens": ["Er", "ihr", "vom", "Bu\u00b7sen", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und keine Freude mi\u00dfte", "tokens": ["Und", "kei\u00b7ne", "Freu\u00b7de", "mi\u00df\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei seiner Liebe sie.", "tokens": ["Bei", "sei\u00b7ner", "Lie\u00b7be", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Umbuhlt von ihrem Freier,", "tokens": ["Um\u00b7buhlt", "von", "ih\u00b7rem", "Frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4hnt sie sich hochbegl\u00fcckt,", "tokens": ["W\u00e4hnt", "sie", "sich", "hoch\u00b7be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Inde\u00df die Trauungsfeier", "tokens": ["In\u00b7de\u00df", "die", "Trau\u00b7ungs\u00b7fei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tag t\u00e4glich n\u00e4her r\u00fcckt.", "tokens": ["Tag", "t\u00e4g\u00b7lich", "n\u00e4\u00b7her", "r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Den letzten Tag im Lenzen", "tokens": ["Den", "letz\u00b7ten", "Tag", "im", "Len\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ward er Mann, sie Frau;", "tokens": ["Da", "ward", "er", "Mann", ",", "sie", "Frau", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Sang und Freudent\u00e4nzen", "tokens": ["Von", "Sang", "und", "Freu\u00b7den\u00b7t\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ert\u00f6nte Feld und Au.", "tokens": ["Er\u00b7t\u00f6n\u00b7te", "Feld", "und", "Au", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Ehe Sommer gl\u00fchte", "tokens": ["Der", "E\u00b7he", "Som\u00b7mer", "gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwar manchmal hei\u00df, doch sch\u00f6n,", "tokens": ["Zwar", "manch\u00b7mal", "hei\u00df", ",", "doch", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seine Gattin bl\u00fchte", "tokens": ["Und", "sei\u00b7ne", "Gat\u00b7tin", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun noch einmal so sch\u00f6n.", "tokens": ["Nun", "noch", "ein\u00b7mal", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der Herbst kam, und was keimte,", "tokens": ["Der", "Herbst", "kam", ",", "und", "was", "ke\u00b7im\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PWS", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Stand nun in voller Frucht,", "tokens": ["Stand", "nun", "in", "vol\u00b7ler", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Eh'paar sprach und tr\u00e4umte", "tokens": ["Das", "Eh'\u00b7paar", "sprach", "und", "tr\u00e4um\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von sch\u00f6ner Rosenzucht.", "tokens": ["Von", "sch\u00f6\u00b7ner", "Ro\u00b7sen\u00b7zucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Doch k\u00e4lter war das Wehen", "tokens": ["Doch", "k\u00e4l\u00b7ter", "war", "das", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Gatten um sie her,", "tokens": ["Des", "Gat\u00b7ten", "um", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf Auen und auf Seen", "tokens": ["Auf", "Au\u00b7en", "und", "auf", "Seen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gab's keine Freuden mehr.", "tokens": ["Gab's", "kei\u00b7ne", "Freu\u00b7den", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Es r\u00fcckte t\u00e4glich k\u00e4lter", "tokens": ["Es", "r\u00fcck\u00b7te", "t\u00e4g\u00b7lich", "k\u00e4l\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Winter nun heran,", "tokens": ["Der", "Win\u00b7ter", "nun", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die gute Frau ward \u00e4lter,", "tokens": ["Die", "gu\u00b7te", "Frau", "ward", "\u00e4l\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und frostiger der Mann.", "tokens": ["Und", "fros\u00b7ti\u00b7ger", "der", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Sein Hauch, der sonst sie k\u00fchlte,", "tokens": ["Sein", "Hauch", ",", "der", "sonst", "sie", "k\u00fchl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ward nun wie schneidend Eis,", "tokens": ["Ward", "nun", "wie", "schnei\u00b7dend", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In seinem S\u00e4useln f\u00fchlte", "tokens": ["In", "sei\u00b7nem", "S\u00e4u\u00b7seln", "f\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sich dem Sturme preis.", "tokens": ["Sie", "sich", "dem", "Stur\u00b7me", "preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Und sprach er nun, so nahm er", "tokens": ["Und", "sprach", "er", "nun", ",", "so", "nahm", "er"], "token_info": ["word", "word", "word",