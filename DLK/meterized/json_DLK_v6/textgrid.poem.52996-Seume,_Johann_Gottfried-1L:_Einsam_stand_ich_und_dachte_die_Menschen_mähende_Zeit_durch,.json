{"textgrid.poem.52996": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam stand ich und dachte die Menschen m\u00e4hende Zeit durch,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam stand ich und dachte die Menschen m\u00e4hende Zeit durch,", "tokens": ["Ein\u00b7sam", "stand", "ich", "und", "dach\u00b7te", "die", "Men\u00b7schen", "m\u00e4\u00b7hen\u00b7de", "Zeit", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Die ich am Laurenzstrom und an der Weichsel gelebt,", "tokens": ["Die", "ich", "am", "Lau\u00b7renz\u00b7strom", "und", "an", "der", "Weich\u00b7sel", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Z\u00e4hlte der St\u00fcrme sehr viele, die meinen Nachen bedrohten;", "tokens": ["Z\u00e4hl\u00b7te", "der", "St\u00fcr\u00b7me", "sehr", "vie\u00b7le", ",", "die", "mei\u00b7nen", "Na\u00b7chen", "be\u00b7droh\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Halcyonischer Tag blickte dem Pilger nicht oft:", "tokens": ["Hal\u00b7cyo\u00b7ni\u00b7scher", "Tag", "blick\u00b7te", "dem", "Pil\u00b7ger", "nicht", "oft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Doch zuweilen, wenn ihm ein St\u00fcndchen mit Gleim und mit Wei\u00dfe,", "tokens": ["Doch", "zu\u00b7wei\u00b7len", ",", "wenn", "ihm", "ein", "St\u00fcnd\u00b7chen", "mit", "Gleim", "und", "mit", "Wei\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mit Freund G\u00f6schen am Berg, Hedwig und Oeser verstrich.", "tokens": ["Mit", "Freund", "G\u00f6\u00b7schen", "am", "Berg", ",", "Hed\u00b7wig", "und", "O\u00b7e\u00b7ser", "ver\u00b7strich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "APPRART", "NN", "$,", "NE", "KON", "NN", "ADJD", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Hedwig, der Gute, der Menschenfreund, der Christ in der Wahrheit,", "tokens": ["Hed\u00b7wig", ",", "der", "Gu\u00b7te", ",", "der", "Men\u00b7schen\u00b7freund", ",", "der", "Christ", "in", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "War gestorben; ihm half nicht Podalirius Gunst", "tokens": ["War", "ge\u00b7stor\u00b7ben", ";", "ihm", "half", "nicht", "Po\u00b7da\u00b7li\u00b7rius", "Gunst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$.", "PPER", "VVFIN", "PTKNEG", "NE", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Einsam ma\u00df ich den Werth nun erst in seinem Verlust ganz;", "tokens": ["Ein\u00b7sam", "ma\u00df", "ich", "den", "Werth", "nun", "erst", "in", "sei\u00b7nem", "Ver\u00b7lust", "ganz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Und ein Bothe erschien, gab mir ein Blatt und verschwand:", "tokens": ["Und", "ein", "Bo\u00b7the", "er\u00b7schien", ",", "gab", "mir", "ein", "Blatt", "und", "ver\u00b7schwand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "\u00bboeser, auch Oeser ging hin ins Land, aus dem keiner zur\u00fcckkehrt:", "tokens": ["\u00bb", "o\u00b7e\u00b7ser", ",", "auch", "O\u00b7e\u00b7ser", "ging", "hin", "ins", "Land", ",", "aus", "dem", "kei\u00b7ner", "zu\u00b7r\u00fcck\u00b7kehrt", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADV", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,", "APPR", "ART", "PIS", "VVPP", "$."], "meter": "-+---+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Fr\u00fch, ach zu fr\u00fch war, obgleich lange gef\u00fcrchtet, der Schlag.\u00ab", "tokens": ["Fr\u00fch", ",", "ach", "zu", "fr\u00fch", "war", ",", "ob\u00b7gleich", "lan\u00b7ge", "ge\u00b7f\u00fcrch\u00b7tet", ",", "der", "Schlag", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "ADV", "PTKA", "ADJD", "VAFIN", "$,", "KOUS", "ADV", "VVPP", "$,", "ART", "NN", "$.", "$("], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Eine Thr\u00e4ne gl\u00fchete mir an der hei\u00dferen Wimper", "tokens": ["Ei\u00b7ne", "Thr\u00e4\u00b7ne", "gl\u00fc\u00b7he\u00b7te", "mir", "an", "der", "hei\u00b7\u00dfe\u00b7ren", "Wim\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+---+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Langsam steigend herauf, glitt dann die Wimper herab.", "tokens": ["Lang\u00b7sam", "stei\u00b7gend", "her\u00b7auf", ",", "glitt", "dann", "die", "Wim\u00b7per", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKVZ", "$,", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Dort kommt wieder ein Zug zur\u00fcck von dem H\u00fcgel des Kirchhofs;", "tokens": ["Dort", "kommt", "wie\u00b7der", "ein", "Zug", "zu\u00b7r\u00fcck", "von", "dem", "H\u00fc\u00b7gel", "des", "Kirch\u00b7hofs", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "M\u00fccke, der Redliche, starb: heiliges Wandels war er,", "tokens": ["M\u00fc\u00b7cke", ",", "der", "Red\u00b7li\u00b7che", ",", "starb", ":", "hei\u00b7li\u00b7ges", "Wan\u00b7dels", "war", "er", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "VVFIN", "$.", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "+--+---+--+-+-", "measure": "dactylic.di.plus"}, "line.17": {"text": "Wie der Irdischen hier auf Erden nur wenige leben,", "tokens": ["Wie", "der", "Ir\u00b7di\u00b7schen", "hier", "auf", "Er\u00b7den", "nur", "we\u00b7ni\u00b7ge", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "NN", "ADV", "PIS", "VVINF", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Ohne Tadel als Mensch, Vater und Lehrer und Freund,", "tokens": ["Oh\u00b7ne", "Ta\u00b7del", "als", "Mensch", ",", "Va\u00b7ter", "und", "Leh\u00b7rer", "und", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOUS", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Oeser, auch Oeser ging hin ins Land, aus dem keiner zur\u00fcckkehrt;", "tokens": ["O\u00b7e\u00b7ser", ",", "auch", "O\u00b7e\u00b7ser", "ging", "hin", "ins", "Land", ",", "aus", "dem", "kei\u00b7ner", "zu\u00b7r\u00fcck\u00b7kehrt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,", "APPR", "ART", "PIS", "VVPP", "$."], "meter": "-+--++-+--+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.20": {"text": "Fr\u00fch, ach zu fr\u00fch war, obgleich lange gef\u00fcrchtet, der Schlag.", "tokens": ["Fr\u00fch", ",", "ach", "zu", "fr\u00fch", "war", ",", "ob\u00b7gleich", "lan\u00b7ge", "ge\u00b7f\u00fcrch\u00b7tet", ",", "der", "Schlag", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "PTKA", "ADJD", "VAFIN", "$,", "KOUS", "ADV", "VVPP", "$,", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Wie als Knabe vom Grabe des Vaters ich weinend empor sah,", "tokens": ["Wie", "als", "Kna\u00b7be", "vom", "Gra\u00b7be", "des", "Va\u00b7ters", "ich", "wei\u00b7nend", "em\u00b7por", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOUS", "NN", "APPRART", "NN", "ART", "NN", "PPER", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.22": {"text": "Seh' ich wehmuthsvoll, Oeser, von deinem empor.", "tokens": ["Seh'", "ich", "weh\u00b7muths\u00b7voll", ",", "O\u00b7e\u00b7ser", ",", "von", "dei\u00b7nem", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "NN", "$,", "APPR", "PPOSAT", "PTKVZ", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.23": {"text": "Wenige Jahre nur waren unserer frohen Verbindung,", "tokens": ["We\u00b7ni\u00b7ge", "Jah\u00b7re", "nur", "wa\u00b7ren", "un\u00b7se\u00b7rer", "fro\u00b7hen", "Ver\u00b7bin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Liebensw\u00fcrdiger Greis; mehrere Lustra ihr Werth.", "tokens": ["Lie\u00b7bens\u00b7w\u00fcr\u00b7di\u00b7ger", "Greis", ";", "meh\u00b7re\u00b7re", "Lus\u00b7tra", "ihr", "Werth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Ach, wie oft sa\u00df ich bey dir am runden vertraulichen Tische,", "tokens": ["Ach", ",", "wie", "oft", "sa\u00df", "ich", "bey", "dir", "am", "run\u00b7den", "ver\u00b7trau\u00b7li\u00b7chen", "Ti\u00b7sche", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "VVFIN", "PPER", "APPR", "PPER", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "St\u00fcmperte Griechisch dir vor, und du erz\u00e4hltest zum Lohn,", "tokens": ["St\u00fcm\u00b7per\u00b7te", "Grie\u00b7chisch", "dir", "vor", ",", "und", "du", "er\u00b7z\u00e4hl\u00b7test", "zum", "Lohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Und verga\u00dfest im Scherz die Achtzig des silbernen Hauptes;", "tokens": ["Und", "ver\u00b7ga\u00b7\u00dfest", "im", "Scherz", "die", "Acht\u00b7zig", "des", "sil\u00b7ber\u00b7nen", "Haup\u00b7tes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "CARD", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Oder verga\u00dfest sie nicht, ehrtest durch Freude sie mehr", "tokens": ["O\u00b7der", "ver\u00b7ga\u00b7\u00dfest", "sie", "nicht", ",", "ehr\u00b7test", "durch", "Freu\u00b7de", "sie", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "APPR", "NN", "PPER", "ADV"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.29": {"text": "Durch die Erinnerung jung gabst du Geschenke der Vorzeit,", "tokens": ["Durch", "die", "E\u00b7rin\u00b7ne\u00b7rung", "jung", "gabst", "du", "Ge\u00b7schen\u00b7ke", "der", "Vor\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.30": {"text": "Und zur Gegenwart hob sie das lakonische Wort;", "tokens": ["Und", "zur", "Ge\u00b7gen\u00b7wart", "hob", "sie", "das", "la\u00b7ko\u00b7ni\u00b7sche", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+-+---+-+-+", "measure": "anapaest.init"}, "line.31": {"text": "Monumente von Witz und Monumente von Bravheit,", "tokens": ["Mo\u00b7nu\u00b7men\u00b7te", "von", "Witz", "und", "Mo\u00b7nu\u00b7men\u00b7te", "von", "Brav\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+---+--++--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Von dem Marmor herab bis in die H\u00fctte von Stroh.", "tokens": ["Von", "dem", "Mar\u00b7mor", "her\u00b7ab", "bis", "in", "die", "H\u00fct\u00b7te", "von", "Stroh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Sch\u00f6ner Gruppen voll war dir das gro\u00dfe Gem\u00e4hlde des Lebens;", "tokens": ["Sch\u00f6\u00b7ner", "Grup\u00b7pen", "voll", "war", "dir", "das", "gro\u00b7\u00dfe", "Ge\u00b7m\u00e4hl\u00b7de", "des", "Le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VAFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Und zum Geistesgenu\u00df mischten es Bettler und F\u00fcrst.", "tokens": ["Und", "zum", "Geis\u00b7tes\u00b7ge\u00b7nu\u00df", "mischten", "es", "Bett\u00b7ler", "und", "F\u00fcrst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.35": {"text": "M\u00f6gen andre den K\u00fcnstler bewundern, der Geist in die Form schuf!", "tokens": ["M\u00f6\u00b7gen", "and\u00b7re", "den", "K\u00fcnst\u00b7ler", "be\u00b7wun\u00b7dern", ",", "der", "Geist", "in", "die", "Form", "schuf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "VVINF", "$,", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Wahr, der K\u00fcnstler war gro\u00df; aber ich liebte den Mann.", "tokens": ["Wahr", ",", "der", "K\u00fcnst\u00b7ler", "war", "gro\u00df", ";", "a\u00b7ber", "ich", "lieb\u00b7te", "den", "Mann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "ADJD", "$.", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Steh' ich einst vor seiner Auferstehung und hebe", "tokens": ["Steh'", "ich", "einst", "vor", "sei\u00b7ner", "Auf\u00b7er\u00b7ste\u00b7hung", "und", "he\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.38": {"text": "Mich mit magischer Kraft \u00fcber die Wolken empor;", "tokens": ["Mich", "mit", "ma\u00b7gi\u00b7scher", "Kraft", "\u00fc\u00b7ber", "die", "Wol\u00b7ken", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Dann, dann denk' ich ger\u00fchrt doch mehr noch zur\u00fcck an die St\u00e4tte,", "tokens": ["Dann", ",", "dann", "denk'", "ich", "ge\u00b7r\u00fchrt", "doch", "mehr", "noch", "zu\u00b7r\u00fcck", "an", "die", "St\u00e4t\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "VVPP", "ADV", "ADV", "ADV", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-++--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Wo er mir v\u00e4terlich rief; Bleib du nur heute bey mir!", "tokens": ["Wo", "er", "mir", "v\u00e4\u00b7ter\u00b7lich", "rief", ";", "Bleib", "du", "nur", "heu\u00b7te", "bey", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$.", "NN", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "Und blickt zauberisch einst mir seine Sibylle von Endor,", "tokens": ["Und", "blickt", "zau\u00b7be\u00b7risch", "einst", "mir", "sei\u00b7ne", "Si\u00b7byl\u00b7le", "von", "En\u00b7dor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "PPER", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.42": {"text": "Denk' ich doch herzlicher noch an den unendlichen Gang,", "tokens": ["Denk'", "ich", "doch", "herz\u00b7li\u00b7cher", "noch", "an", "den", "un\u00b7end\u00b7li\u00b7chen", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.43": {"text": "Der durch die alternde Burg zu seinem Sorgenlos f\u00fchrte,", "tokens": ["Der", "durch", "die", "al\u00b7tern\u00b7de", "Burg", "zu", "sei\u00b7nem", "Sor\u00b7gen\u00b7los", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Wo er der Freuden so viel immer den Freunden erfand.", "tokens": ["Wo", "er", "der", "Freu\u00b7den", "so", "viel", "im\u00b7mer", "den", "Freun\u00b7den", "er\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.45": {"text": "Pflanzten auch F\u00fcrsten mit Pracht ein Denkmahl dem gl\u00fchenden Mahler,", "tokens": ["Pflanz\u00b7ten", "auch", "F\u00fcrs\u00b7ten", "mit", "Pracht", "ein", "Denk\u00b7mahl", "dem", "gl\u00fc\u00b7hen\u00b7den", "Mah\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "APPR", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.46": {"text": "Gr\u00f6\u00dfer w\u00e4r' es vielleicht, heiliger w\u00e4r' es doch nicht,", "tokens": ["Gr\u00f6\u00b7\u00dfer", "w\u00e4r'", "es", "viel\u00b7leicht", ",", "hei\u00b7li\u00b7ger", "w\u00e4r'", "es", "doch", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,", "ADJD", "VAFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.47": {"text": "Als das Denkmahl, das ihm in vieler Seelen gebaut ist,", "tokens": ["Als", "das", "Denk\u00b7mahl", ",", "das", "ihm", "in", "vie\u00b7ler", "See\u00b7len", "ge\u00b7baut", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PIAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "--+----+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "Die nicht den K\u00fcnstler allein, die auch den Menschen gekannt.", "tokens": ["Die", "nicht", "den", "K\u00fcnst\u00b7ler", "al\u00b7lein", ",", "die", "auch", "den", "Men\u00b7schen", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.49": {"text": "M\u00e4nner, Verkl\u00e4rter, wie du warst, sterben hin\u00fcber zum Leben;", "tokens": ["M\u00e4n\u00b7ner", ",", "Ver\u00b7kl\u00e4r\u00b7ter", ",", "wie", "du", "warst", ",", "ster\u00b7ben", "hin\u00b7\u00fc\u00b7ber", "zum", "Le\u00b7ben", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWAV", "PPER", "VAFIN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.50": {"text": "Denn ihr Wesen ist nicht Ephemeridengeschlechts.", "tokens": ["Denn", "ihr", "We\u00b7sen", "ist", "nicht", "E\u00b7phe\u00b7me\u00b7ri\u00b7den\u