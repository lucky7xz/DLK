{"dta.poem.7624": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00f6nes Streben ist's, den Guten \u00e4hnlich werden,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Stre\u00b7ben", "ist's", ",", "den", "Gu\u00b7ten", "\u00e4hn\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die hier vom h\u00f6chsten Gut Abbilder sind auf Erden.", "tokens": ["Die", "hier", "vom", "h\u00f6chs\u00b7ten", "Gut", "Ab\u00b7bil\u00b7der", "sind", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch immer wird das nur ein Bild vom Bilde seyn;", "tokens": ["Doch", "im\u00b7mer", "wird", "das", "nur", "ein", "Bild", "vom", "Bil\u00b7de", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PDS", "ADV", "ART", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bilde deinem Geist das Urbild selber ein.", "tokens": ["Du", "bil\u00b7de", "dei\u00b7nem", "Geist", "das", "Ur\u00b7bild", "sel\u00b7ber", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "PTKVZ", "$."]