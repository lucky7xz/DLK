{"textgrid.poem.46597": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mild beschwichtendes Element]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mild beschwichtendes Element,", "tokens": ["Mild", "be\u00b7schwich\u00b7ten\u00b7des", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wasser,", "tokens": ["Was\u00b7ser", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Nicht nur l\u00f6schest du Feur, das brennt,", "tokens": ["Nicht", "nur", "l\u00f6\u00b7schest", "du", "Feur", ",", "das", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wasser!", "tokens": ["Was\u00b7ser", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Nicht nur erquickst du den Durst, der lechzt.", "tokens": ["Nicht", "nur", "er\u00b7quickst", "du", "den", "Durst", ",", "der", "lechzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wasser,", "tokens": ["Was\u00b7ser", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Du auch heilest mein Kind, das \u00e4chzt,", "tokens": ["Du", "auch", "hei\u00b7lest", "mein", "Kind", ",", "das", "\u00e4chzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wasser!", "tokens": ["Was\u00b7ser", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Ihm gelegt auf die gl\u00fchende Stirn,", "tokens": ["Ihm", "ge\u00b7legt", "auf", "die", "gl\u00fc\u00b7hen\u00b7de", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wasser,", "tokens": ["Was\u00b7ser", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sch\u00fctze vorm Fieberbrand das Gehirn,", "tokens": ["Sch\u00fct\u00b7ze", "vorm", "Fie\u00b7ber\u00b7brand", "das", "Ge\u00b7hirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wasser!", "tokens": ["Was\u00b7ser", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Auf die welkende Bl\u00fcthe gesprengt,", "tokens": ["Auf", "die", "wel\u00b7ken\u00b7de", "Bl\u00fc\u00b7the", "ge\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wasser,", "tokens": ["Was\u00b7ser", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wie auf Blumen sonnenversengt,", "tokens": ["Wie", "auf", "Blu\u00b7men", "son\u00b7nen\u00b7ver\u00b7sengt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wasser!", "tokens": ["Was\u00b7ser", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Essig m\u00fc\u00dfte mir sein der Wein,", "tokens": ["Es\u00b7sig", "m\u00fc\u00df\u00b7te", "mir", "sein", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PPOSAT", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wasser,", "tokens": ["Was\u00b7ser", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Eh' er d\u00fcrfte gemischt dir sein,", "tokens": ["Eh'", "er", "d\u00fcrf\u00b7te", "ge\u00b7mischt", "dir", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVPP", "PPER", "VAINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Wasser.", "tokens": ["Was\u00b7ser", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}