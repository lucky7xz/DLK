{"textgrid.poem.44170": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun, Bruder, las mich auch in Fried und Freundschaft fort.", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun, Bruder, las mich auch in Fried und Freundschaft fort.", "tokens": ["Nun", ",", "Bru\u00b7der", ",", "las", "mich", "auch", "in", "Fried", "und", "Freund\u00b7schaft", "fort", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Schlesien entdeckt vor mich noch keinen Ort.", "tokens": ["Mein", "Schle\u00b7si\u00b7en", "ent\u00b7deckt", "vor", "mich", "noch", "kei\u00b7nen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So weit ich Ost und West darinnen durchgezogen,", "tokens": ["So", "weit", "ich", "Ost", "und", "West", "da\u00b7rin\u00b7nen", "durch\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So viel Mahl hab ich mich und meinen Wuntsch betrogen.", "tokens": ["So", "viel", "Mahl", "hab", "ich", "mich", "und", "mei\u00b7nen", "Wunt\u00b7sch", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "PRF", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Es kan wohl leichtlich seyn, da\u00df oftmahls eigne Schuld", "tokens": ["Es", "kan", "wohl", "leicht\u00b7lich", "seyn", ",", "da\u00df", "oft\u00b7mahls", "eig\u00b7ne", "Schuld"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$,", "KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "(denn wer begreift sich wohl in Noth und Ungeduld?)", "tokens": ["(", "denn", "wer", "be\u00b7greift", "sich", "wohl", "in", "Noth", "und", "Un\u00b7ge\u00b7duld", "?", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PWS", "VVFIN", "PRF", "ADV", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Viel, wie die Misgunst spricht, zur Unruh beygetragen;", "tokens": ["Viel", ",", "wie", "die", "Mis\u00b7gunst", "spricht", ",", "zur", "Un\u00b7ruh", "bey\u00b7ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jedoch den rechten Grund darf Kiel und Mund nicht sagen.", "tokens": ["Je\u00b7doch", "den", "rech\u00b7ten", "Grund", "darf", "Kiel", "und", "Mund", "nicht", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "NE", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was noch zu \u00e4ndern ist und blos bey mir besteht,", "tokens": ["Was", "noch", "zu", "\u00e4n\u00b7dern", "ist", "und", "blos", "bey", "mir", "be\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKZU", "VVINF", "VAFIN", "KON", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das wird, wo meine Zeit nicht vor der Zeit vergeht,", "tokens": ["Das", "wird", ",", "wo", "mei\u00b7ne", "Zeit", "nicht", "vor", "der", "Zeit", "ver\u00b7geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von nun an ganz gewis der Feinde Spott besch\u00e4men;", "tokens": ["Von", "nun", "an", "ganz", "ge\u00b7wis", "der", "Fein\u00b7de", "Spott", "be\u00b7sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADV", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was am Verh\u00e4ngn\u00fc\u00df liegt, dem lern ich mich bequemen.", "tokens": ["Was", "am", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "liegt", ",", "dem", "lern", "ich", "mich", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Inde\u00dfen geh ich gleich auf meiner Reis allein,", "tokens": ["In\u00b7de\u00b7\u00dfen", "geh", "ich", "gleich", "auf", "mei\u00b7ner", "Reis", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So kan ich allzeit froh und niemahls m\u00fc\u00dfig seyn;", "tokens": ["So", "kan", "ich", "all\u00b7zeit", "froh", "und", "nie\u00b7mahls", "m\u00fc\u00b7\u00dfig", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "KON", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da werd ich bey mir selbst die wunderlichen Sachen", "tokens": ["Da", "werd", "ich", "bey", "mir", "selbst", "die", "wun\u00b7der\u00b7li\u00b7chen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Von Creuzburg, Landeshutt und Brieg zu Nuze machen.", "tokens": ["Von", "Creuz\u00b7burg", ",", "Lan\u00b7des\u00b7hutt", "und", "Brieg", "zu", "Nu\u00b7ze", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJD", "KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bald wird ein weicher Trieb in meiner Brust entstehn", "tokens": ["Bald", "wird", "ein", "wei\u00b7cher", "Trieb", "in", "mei\u00b7ner", "Brust", "ent\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und Thr\u00e4nen wahrer Treu dabey mit untergehn,", "tokens": ["Und", "Thr\u00e4\u00b7nen", "wah\u00b7rer", "Treu", "da\u00b7bey", "mit", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "PAV", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn Phillis und der Tag, mein bester auf der Erden,", "tokens": ["Wenn", "Phil\u00b7lis", "und", "der", "Tag", ",", "mein", "bes\u00b7ter", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "ART", "NN", "$,", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "An dem sie mich erwehlt, mein Blut bewegen werden.", "tokens": ["An", "dem", "sie", "mich", "er\u00b7wehlt", ",", "mein", "Blut", "be\u00b7we\u00b7gen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVPP", "$,", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch da das Gl\u00fccke zwingt, so sto\u00df ich alles hin", "tokens": ["Doch", "da", "das", "Gl\u00fc\u00b7cke", "zwingt", ",", "so", "sto\u00df", "ich", "al\u00b7les", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PIS", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und tr\u00f6ste bald darauf den erst erschrocknen Sinn.", "tokens": ["Und", "tr\u00f6s\u00b7te", "bald", "da\u00b7rauf", "den", "erst", "er\u00b7schrock\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ach lache doch mit mir mit jenem alten Bilde,", "tokens": ["Ach", "la\u00b7che", "doch", "mit", "mir", "mit", "je\u00b7nem", "al\u00b7ten", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das wie ein Kranichhals und Mausehund von Schilde", "tokens": ["Das", "wie", "ein", "Kra\u00b7nich\u00b7hals", "und", "Mau\u00b7se\u00b7hund", "von", "Schil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "ART", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Den muntern Pegasus bisweilen scheu gemacht.", "tokens": ["Den", "mun\u00b7tern", "Pe\u00b7ga\u00b7sus", "bis\u00b7wei\u00b7len", "scheu", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Voraus erg\u00f6zt mich noch die halbberauschte Nacht,", "tokens": ["Vo\u00b7raus", "er\u00b7g\u00f6zt", "mich", "noch", "die", "halb\u00b7be\u00b7rauschte", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "In der du mich, mein Freund, aus Hirschberg weggezwungen,", "tokens": ["In", "der", "du", "mich", ",", "mein", "Freund", ",", "aus", "Hirschberg", "weg\u00b7ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "---+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Nachdem mein Abschiedslied schon um den Queis geklungen.", "tokens": ["Nach\u00b7dem", "mein", "Ab\u00b7schieds\u00b7lied", "schon", "um", "den", "Que\u00b7is", "ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.29": {"text": "Der Dinge sind zu viel, denn Schmiedebergs Pallast", "tokens": ["Der", "Din\u00b7ge", "sind", "zu", "viel", ",", "denn", "Schmie\u00b7de\u00b7bergs", "Pal\u00b7last"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKA", "PIS", "$,", "KON", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und Mephiboseths Haus und was der Reim nicht fast,", "tokens": ["Und", "Me\u00b7phi\u00b7bo\u00b7seths", "Haus", "und", "was", "der", "Reim", "nicht", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "KON", "PWS", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sind, wer die Streiche weis, die ich und du erfahren,", "tokens": ["Sind", ",", "wer", "die", "Strei\u00b7che", "weis", ",", "die", "ich", "und", "du", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "In einen Lachekram nicht ungeschickte Wahren.", "tokens": ["In", "ei\u00b7nen", "La\u00b7che\u00b7kram", "nicht", "un\u00b7ge\u00b7schick\u00b7te", "Wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Das sag ich Landeshutt zu Troz der Feinde Schmach", "tokens": ["Das", "sag", "ich", "Lan\u00b7des\u00b7hutt", "zu", "Troz", "der", "Fein\u00b7de", "Schmach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKZU", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Vor unserm Schlesien in allen St\u00e4dten nach,", "tokens": ["Vor", "un\u00b7serm", "Schle\u00b7si\u00b7en", "in", "al\u00b7len", "St\u00e4d\u00b7ten", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df meine Musen hier mehr Lieb und Schuz getrofen,", "tokens": ["Da\u00df", "mei\u00b7ne", "Mu\u00b7sen", "hier", "mehr", "Lieb", "und", "Schuz", "ge\u00b7tro\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PIAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Als sie wohl nicht so bald in einem Lande hofen.", "tokens": ["Als", "sie", "wohl", "nicht", "so", "bald", "in", "ei\u00b7nem", "Lan\u00b7de", "ho\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wo Narren f\u00e4hig sind, uns in Verdru\u00df zu ziehn,", "tokens": ["Wo", "Nar\u00b7ren", "f\u00e4\u00b7hig", "sind", ",", "uns", "in", "Ver\u00b7dru\u00df", "zu", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VAFIN", "$,", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So ist's der einzige, den mir der Ort verliehn,", "tokens": ["So", "ist's", "der", "ein\u00b7zi\u00b7ge", ",", "den", "mir", "der", "Ort", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-----+-+", "measure": "unknown.measure.tetra"}, "line.39": {"text": "Da\u00df, nehmt die M\u00fczen ab, der Erbprinz von Drey Hasen,", "tokens": ["Da\u00df", ",", "nehmt", "die", "M\u00fc\u00b7zen", "ab", ",", "der", "E\u00b7rbprinz", "von", "Drey", "Ha\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Um mir Gewalt zu thun, die B\u00fcchse leer geblasen.", "tokens": ["Um", "mir", "Ge\u00b7walt", "zu", "thun", ",", "die", "B\u00fcch\u00b7se", "leer", "ge\u00b7bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "NN", "PTKZU", "VVINF", "$,", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wie mancher Topf mit Wein, wie mancher fetter Hecht", "tokens": ["Wie", "man\u00b7cher", "Topf", "mit", "Wein", ",", "wie", "man\u00b7cher", "fet\u00b7ter", "Hecht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "APPR", "NN", "$,", "PWAV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Flog an die Billigkeit und bis ans liebe Recht,", "tokens": ["Flog", "an", "die", "Bil\u00b7lig\u00b7keit", "und", "bis", "ans", "lie\u00b7be", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Den ungezwungnen Neid an meiner Faust zu r\u00e4chen!", "tokens": ["Den", "un\u00b7ge\u00b7zwung\u00b7nen", "Neid", "an", "mei\u00b7ner", "Faust", "zu", "r\u00e4\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Je mehr man Igel w\u00e4lzt, je mehr sie uns auch stechen;", "tokens": ["Je", "mehr", "man", "I\u00b7gel", "w\u00e4lzt", ",", "je", "mehr", "sie", "uns", "auch", "ste\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "NN", "VVFIN", "$,", "ADV", "ADV", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und dies empfand an mir der eingebildte Rath,", "tokens": ["Und", "dies", "emp\u00b7fand", "an", "mir", "der", "ein\u00b7ge\u00b7bild\u00b7te", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Der noch nicht ruhig wird, als bis ein Ne\u00dfelblat", "tokens": ["Der", "noch", "nicht", "ru\u00b7hig", "wird", ",", "als", "bis", "ein", "Ne\u00b7\u00df\u00b7el\u00b7blat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "------+--+--+", "measure": "iambic.tri.relaxed"}, "line.47": {"text": "Vom deutschen Helicon und was von Stacheln\u00fc\u00dfen", "tokens": ["Vom", "deut\u00b7schen", "He\u00b7li\u00b7con", "und", "was", "von", "Sta\u00b7chel\u00b7n\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "PWS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Das ungewaschne Maul mit Schande f\u00fcttern m\u00fc\u00dfen.", "tokens": ["Das", "un\u00b7ge\u00b7waschne", "Maul", "mit", "Schan\u00b7de", "f\u00fct\u00b7tern", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.49": {"text": "Genug von dem, mein Freund. Die lezte gute Nacht,", "tokens": ["Ge\u00b7nug", "von", "dem", ",", "mein", "Freund", ".", "Die", "lez\u00b7te", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "$,", "PPOSAT", "NN", "$.", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "An die mein Sehnsuchtstrieb schon lange Zeit gedacht,", "tokens": ["An", "die", "mein", "Sehn\u00b7sucht\u00b7strieb", "schon", "lan\u00b7ge", "Zeit", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Erlaubt mir hier nicht mehr, die Narren durchzustriegeln.", "tokens": ["Er\u00b7laubt", "mir", "hier", "nicht", "mehr", ",", "die", "Nar\u00b7ren", "durch\u00b7zu\u00b7strie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Ein Ku\u00df und dieses Blat soll unsre Freundschaft siegeln,", "tokens": ["Ein", "Ku\u00df", "und", "die\u00b7ses", "Blat", "soll", "uns\u00b7re", "Freund\u00b7schaft", "sie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PDAT", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Die Leipzig erstlich schlo\u00df, der Boberflu\u00df verst\u00e4rckt.", "tokens": ["Die", "Leip\u00b7zig", "erst\u00b7lich", "schlo\u00df", ",", "der", "Bo\u00b7berf\u00b7lu\u00df", "ver\u00b7st\u00e4rckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJD", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Dies, was du mir gar oft von Schwachheit angemerckt,", "tokens": ["Dies", ",", "was", "du", "mir", "gar", "oft", "von", "Schwach\u00b7heit", "an\u00b7ge\u00b7merckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "PPER", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Das schicke durch die Luft bis an die kalten Gr\u00e4nzen,", "tokens": ["Das", "schi\u00b7cke", "durch", "die", "Luft", "bis", "an", "die", "kal\u00b7ten", "Gr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Wo in der langen Nacht mehr Fisch als Sterne gl\u00e4nzen.", "tokens": ["Wo", "in", "der", "lan\u00b7gen", "Nacht", "mehr", "Fisch", "als", "Ster\u00b7ne", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "PIAT", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Geht es dir k\u00fcnftig wohl, so soll es mich erfreun", "tokens": ["Geht", "es", "dir", "k\u00fcnf\u00b7tig", "wohl", ",", "so", "soll", "es", "mich", "er\u00b7freun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ADV", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Und dein Verh\u00e4ngn\u00fc\u00df mir ein gleich Verh\u00e4ngn\u00fc\u00df seyn.", "tokens": ["Und", "dein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "mir", "ein", "gleich", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ART", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Die Zwietracht unter uns sey ganz und gar verge\u00dfen;", "tokens": ["Die", "Zwiet\u00b7racht", "un\u00b7ter", "uns", "sey", "ganz", "und", "gar", "ver\u00b7ge\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Ein jeder mag sich selbst nach seiner Elle me\u00dfen.", "tokens": ["Ein", "je\u00b7der", "mag", "sich", "selbst", "nach", "sei\u00b7ner", "El\u00b7le", "me\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Das sag ich frey heraus, der Himmel sey davor,", "tokens": ["Das", "sag", "ich", "frey", "he\u00b7raus", ",", "der", "Him\u00b7mel", "sey", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "ART", "NN", "VAFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Verl\u00f6r ich deine Treu, so wie ich die verlor,", "tokens": ["Ver\u00b7l\u00f6r", "ich", "dei\u00b7ne", "Treu", ",", "so", "wie", "ich", "die", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,", "ADV", "KOKOM", "PPER", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Worauf ich mich fast mehr als auf mich selbst verla\u00dfen,", "tokens": ["Wo\u00b7rauf", "ich", "mich", "fast", "mehr", "als", "auf", "mich", "selbst", "ver\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "ADV", "PIAT", "KOKOM", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Nichts br\u00e4chte mich dazu, dich ganz und gar zu ha\u00dfen.", "tokens": ["Nichts", "br\u00e4ch\u00b7te", "mich", "da\u00b7zu", ",", "dich", "ganz", "und", "gar", "zu", "ha\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PAV", "$,", "PPER", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Denn da\u00df du meine Kunst in Beuchelts Huld gebracht,", "tokens": ["Denn", "da\u00df", "du", "mei\u00b7ne", "Kunst", "in", "Beu\u00b7chelts", "Huld", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "NE", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Ist, was mich auf der Welt warhaftig froher macht,", "tokens": ["Ist", ",", "was", "mich", "auf", "der", "Welt", "war\u00b7haf\u00b7tig", "fro\u00b7her", "macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Als wenn ein bl\u00f6der F\u00fcrst mich aus dem P\u00f6bel h\u00fcbe", "tokens": ["Als", "wenn", "ein", "bl\u00f6\u00b7der", "F\u00fcrst", "mich", "aus", "dem", "P\u00f6\u00b7bel", "h\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Und seine Gnade mir ein halbes Land verschriebe.", "tokens": ["Und", "sei\u00b7ne", "Gna\u00b7de", "mir", "ein", "hal\u00b7bes", "Land", "ver\u00b7schrie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Ich schweige, da\u00df durch dich auch Klugens Gro\u00dfmuth kam", "tokens": ["Ich", "schwei\u00b7ge", ",", "da\u00df", "durch", "dich", "auch", "Klu\u00b7gens", "Gro\u00df\u00b7muth", "kam"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "PPER", "ADV", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Und meiner D\u00fcrftigkeit nicht wenig Last benahm,", "tokens": ["Und", "mei\u00b7ner", "D\u00fcrf\u00b7tig\u00b7keit", "nicht", "we\u00b7nig", "Last", "be\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "So da\u00df, wenn meine Pflicht ein Abendopfer brannte,", "tokens": ["So", "da\u00df", ",", "wenn", "mei\u00b7ne", "Pflicht", "ein", "A\u00b7ben\u00b7dop\u00b7fer", "brann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Ich seinen edlen Geist bey solchen Flammen kannte.", "tokens": ["Ich", "sei\u00b7nen", "ed\u00b7len", "Geist", "bey", "sol\u00b7chen", "Flam\u00b7men", "kann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Ich schweige, weil davon auf einen andern Tag", "tokens": ["Ich", "schwei\u00b7ge", ",", "weil", "da\u00b7von", "auf", "ei\u00b7nen", "an\u00b7dern", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Des Himmels Widergelt mit Seegen regnen mag,", "tokens": ["Des", "Him\u00b7mels", "Wi\u00b7der\u00b7gelt", "mit", "See\u00b7gen", "reg\u00b7nen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Und bitte dich nicht mehr als: Las mein Angedencken", "tokens": ["Und", "bit\u00b7te", "dich", "nicht", "mehr", "als", ":", "Las", "mein", "An\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "KOUS", "$.", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Nicht mit der Ofenth\u00fcr im lezten Winckel hencken.", "tokens": ["Nicht", "mit", "der", "O\u00b7fent\u00b7h\u00fcr", "im", "lez\u00b7ten", "Win\u00b7ckel", "hen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Ich werd es auch nicht thun und um den Plei\u00dfenstrand,", "tokens": ["Ich", "werd", "es", "auch", "nicht", "thun", "und", "um", "den", "Plei\u00b7\u00dfen\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Wo mich die Pegniz nicht aus Sachsen gar verbannt,", "tokens": ["Wo", "mich", "die", "Peg\u00b7niz", "nicht", "aus", "Sach\u00b7sen", "gar", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKNEG", "APPR", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Vielleicht mit be\u00dfrer Ruh, ihr werdet's doch wohl f\u00fchlen,", "tokens": ["Viel\u00b7leicht", "mit", "be\u00df\u00b7rer", "Ruh", ",", "ihr", "wer\u00b7det's", "doch", "wohl", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.80": {"text": "Ein frommes Sch\u00e4ferlied von eurer Freundschaft spielen.", "tokens": ["Ein", "from\u00b7mes", "Sch\u00e4\u00b7fer\u00b7lied", "von", "eu\u00b7rer", "Freund\u00b7schaft", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}