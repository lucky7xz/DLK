{"textgrid.poem.38416": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steht auf ihr lieben Kinderlein,", "tokens": ["Steht", "auf", "ihr", "lie\u00b7ben", "Kin\u00b7der\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Morgenstern mit hellem Schein,", "tokens": ["Der", "Mor\u00b7gens\u00b7tern", "mit", "hel\u00b7lem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4\u00dft sich sehn frei gleich wie ein Held,", "tokens": ["L\u00e4\u00dft", "sich", "sehn", "frei", "gleich", "wie", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "ADJD", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leuchtet in die ganze Welt.", "tokens": ["Und", "leuch\u00b7tet", "in", "die", "gan\u00b7ze", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sey willkommen, du lieber Tag,", "tokens": ["Sey", "will\u00b7kom\u00b7men", ",", "du", "lie\u00b7ber", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPER", "ADV", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Vor dir die Nacht nicht bleiben mag,", "tokens": ["Vor", "dir", "die", "Nacht", "nicht", "blei\u00b7ben", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Leucht uns in unsre Herzen fein,", "tokens": ["Leucht", "uns", "in", "uns\u00b7re", "Her\u00b7zen", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinem himmelischen Schein.", "tokens": ["Mit", "dei\u00b7nem", "him\u00b7me\u00b7li\u00b7schen", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}