{"textgrid.poem.39007": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch Betteln, welch Verfolgen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch Betteln, welch Verfolgen,", "tokens": ["Welch", "Bet\u00b7teln", ",", "welch", "Ver\u00b7fol\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Welcher freche Ungest\u00fcm!", "tokens": ["Wel\u00b7cher", "fre\u00b7che", "Un\u00b7ge\u00b7st\u00fcm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur des Itali\u00e4ners Phlegma", "tokens": ["Nur", "des", "I\u00b7ta\u00b7li\u00b7\u00e4\u00b7ners", "Phleg\u00b7ma"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NE"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wei\u00df diese st\u00fcrmischen Wogen zu beschwichtigen.", "tokens": ["Wei\u00df", "die\u00b7se", "st\u00fcr\u00b7mi\u00b7schen", "Wo\u00b7gen", "zu", "be\u00b7schwich\u00b7ti\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Schon ist es dunkel,", "tokens": ["Schon", "ist", "es", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "M\u00fcde schwank' ich \u00fcber den Corso", "tokens": ["M\u00fc\u00b7de", "schwank'", "ich", "\u00fc\u00b7ber", "den", "Cor\u00b7so"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Der Heimath zu.", "tokens": ["Der", "Hei\u00b7math", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Erfa\u00dft mich die Hand eines ehrbaren Alten,", "tokens": ["Er\u00b7fa\u00dft", "mich", "die", "Hand", "ei\u00b7nes", "ehr\u00b7ba\u00b7ren", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "F\u00fchrt mich beiseit,", "tokens": ["F\u00fchrt", "mich", "bei\u00b7seit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Erz\u00e4hlt sein Ungl\u00fcck, von kranken Kindern,", "tokens": ["Er\u00b7z\u00e4hlt", "sein", "Un\u00b7gl\u00fcck", ",", "von", "kran\u00b7ken", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Von Mangel und Noth und den bittern Schmerzen", "tokens": ["Von", "Man\u00b7gel", "und", "Noth", "und", "den", "bit\u00b7tern", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Versch\u00e4mter Armuth.", "tokens": ["Ver\u00b7sch\u00e4m\u00b7ter", "Ar\u00b7muth", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ueberrascht, verlegen, da ich vom feinen Mann", "tokens": ["Ue\u00b7ber\u00b7rascht", ",", "ver\u00b7le\u00b7gen", ",", "da", "ich", "vom", "fei\u00b7nen", "Mann"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVINF", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Nicht dies Gest\u00e4ndni\u00df erwartet,", "tokens": ["Nicht", "dies", "Ge\u00b7st\u00e4nd\u00b7ni\u00df", "er\u00b7war\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "Gleiten ihm einige Paul in die Hand:", "tokens": ["Glei\u00b7ten", "ihm", "ei\u00b7ni\u00b7ge", "Paul", "in", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "Er dr\u00fcckt die meine,", "tokens": ["Er", "dr\u00fcckt", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPOSAT", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Ach! k\u00f6nnt ich, fl\u00fcstert er mit Innigkeit,", "tokens": ["Ach", "!", "k\u00f6nnt", "ich", ",", "fl\u00fcs\u00b7tert", "er", "mit", "In\u00b7nig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Etwas thun f\u00fcr so edlen Geist!", "tokens": ["Et\u00b7was", "thun", "f\u00fcr", "so", "ed\u00b7len", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er hat den hinkenden Gang, den Stab gesehn,", "tokens": ["Er", "hat", "den", "hin\u00b7ken\u00b7den", "Gang", ",", "den", "Stab", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Er fa\u00dft mich r\u00fcstig unter die Achsel,", "tokens": ["Er", "fa\u00dft", "mich", "r\u00fcs\u00b7tig", "un\u00b7ter", "die", "Ach\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und bevor ich noch fragen,", "tokens": ["Und", "be\u00b7vor", "ich", "noch", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.18": {"text": "Bejahen, verneinen kann,", "tokens": ["Be\u00b7ja\u00b7hen", ",", "ver\u00b7nei\u00b7nen", "kann", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Tr\u00e4gt er mich, f\u00fchrt er mich halb,", "tokens": ["Tr\u00e4gt", "er", "mich", ",", "f\u00fchrt", "er", "mich", "halb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.20": {"text": "Die f\u00fcnf, sechs schmalen Stufen hinauf,", "tokens": ["Die", "f\u00fcnf", ",", "sechs", "schma\u00b7len", "Stu\u00b7fen", "hin\u00b7auf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "$,", "CARD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Die an der Stra\u00dfe", "tokens": ["Die", "an", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Sich l\u00e4ngst dem Hause ziehn.", "tokens": ["Sich", "l\u00e4ngst", "dem", "Hau\u00b7se", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Als ich nun oben", "tokens": ["Als", "ich", "nun", "o\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Geb\u00fcckt und \u00e4ngstlich schwanke,", "tokens": ["Ge\u00b7b\u00fcckt", "und", "\u00e4ngst\u00b7lich", "schwan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Verneigt er sich tief,", "tokens": ["Ver\u00b7neigt", "er", "sich", "tief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Im Dunkel seinen Weg hinwandelnd.", "tokens": ["Im", "Dun\u00b7kel", "sei\u00b7nen", "Weg", "hin\u00b7wan\u00b7delnd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Kaum vermag ich scheu und tastend,", "tokens": ["Kaum", "ver\u00b7mag", "ich", "scheu", "und", "tas\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Zitternd, auf den Stab gelehnt,", "tokens": ["Zit\u00b7ternd", ",", "auf", "den", "Stab", "ge\u00b7lehnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Die Stra\u00dfe wieder zu gewinnen,", "tokens": ["Die", "Stra\u00b7\u00dfe", "wie\u00b7der", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Die ich so seltsam verlor.", "tokens": ["Die", "ich", "so", "selt\u00b7sam", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Geschieht nicht vieles so,", "tokens": ["Ge\u00b7schieht", "nicht", "vie\u00b7les", "so", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PIS", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn Manche w\u00e4hnen", "tokens": ["Wenn", "Man\u00b7che", "w\u00e4h\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Gutes zu thun?", "tokens": ["Gu\u00b7tes", "zu", "thun", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}