{"textgrid.poem.43604": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Paukenschl\u00e4gerlied", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun Alle herbei und h\u00f6rt mich an,", "tokens": ["Nun", "Al\u00b7le", "her\u00b7bei", "und", "h\u00f6rt", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie sch\u00f6n ich die Pauken schlagen kann!", "tokens": ["Wie", "sch\u00f6n", "ich", "die", "Pau\u00b7ken", "schla\u00b7gen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kommt Alle herbei, Papa und Mama!", "tokens": ["Kommt", "Al\u00b7le", "her\u00b7bei", ",", "Pa\u00b7pa", "und", "Ma\u00b7ma", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bidibum, bidibum, juchhei\u00dfassa!", "tokens": ["Bi\u00b7di\u00b7bum", ",", "bi\u00b7di\u00b7bum", ",", "juch\u00b7hei\u00b7\u00dfas\u00b7sa", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "FM.la", "$,", "ITJ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Wenn fr\u00fch noch die Andern im Bette sind,", "tokens": ["Wenn", "fr\u00fch", "noch", "die", "An\u00b7dern", "im", "Bet\u00b7te", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ART", "ADJA", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So geh' ich zu meinen Pauken geschwind,", "tokens": ["So", "geh'", "ich", "zu", "mei\u00b7nen", "Pau\u00b7ken", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich schlage drauf bald stark bald sacht:", "tokens": ["Ich", "schla\u00b7ge", "drauf", "bald", "stark", "bald", "sacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bidebum, bidebum, ihr Schl\u00e4fer erwacht!", "tokens": ["Bi\u00b7de\u00b7bum", ",", "bi\u00b7de\u00b7bum", ",", "ihr", "Schl\u00e4\u00b7fer", "er\u00b7wacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Wenn traurig ich bin, so helf' ich mir bald,", "tokens": ["Wenn", "trau\u00b7rig", "ich", "bin", ",", "so", "hel\u00b7f'", "ich", "mir", "bald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So schlag' ich die Pauken, da\u00df es schallt;", "tokens": ["So", "schlag'", "ich", "die", "Pau\u00b7ken", ",", "da\u00df", "es", "schallt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Traurigkeit, die w\u00e4hret nicht lang,", "tokens": ["Die", "Trau\u00b7rig\u00b7keit", ",", "die", "w\u00e4h\u00b7ret", "nicht", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Bidibum, bidibum, bei dem Paukenklang.", "tokens": ["Bi\u00b7di\u00b7bum", ",", "bi\u00b7di\u00b7bum", ",", "bei", "dem", "Pau\u00b7ken\u00b7klang", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenn's drau\u00dfen brauset und sauset und schneit,", "tokens": ["Wenn's", "drau\u00b7\u00dfen", "brau\u00b7set", "und", "sau\u00b7set", "und", "schneit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "KON", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So ist es bei mir doch noch gute Zeit;", "tokens": ["So", "ist", "es", "bei", "mir", "doch", "noch", "gu\u00b7te", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich pauke dann in fr\u00f6hlicher Ruh,", "tokens": ["Ich", "pau\u00b7ke", "dann", "in", "fr\u00f6h\u00b7li\u00b7cher", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Bidibum, bidibum, und lache dazu.", "tokens": ["Bi\u00b7di\u00b7bum", ",", "bi\u00b7di\u00b7bum", ",", "und", "la\u00b7che", "da\u00b7zu", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "$,", "KON", "VVFIN", "PAV", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Wenn unter uns ein Streit mal beginnt,", "tokens": ["Wenn", "un\u00b7ter", "uns", "ein", "Streit", "mal", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So r\u00fchr' ich meine Pauken geschwind:", "tokens": ["So", "r\u00fchr'", "ich", "mei\u00b7ne", "Pau\u00b7ken", "ge\u00b7schwind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der Frieden ist gleich wieder da,", "tokens": ["Der", "Frie\u00b7den", "ist", "gleich", "wie\u00b7der", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bidebum, bidebum, Victoria!", "tokens": ["Bi\u00b7de\u00b7bum", ",", "bi\u00b7de\u00b7bum", ",", "Vic\u00b7to\u00b7ria", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "FM.la", "$,", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Wenn's aber hei\u00dft: ins Bette sogleich!", "tokens": ["Wenn's", "a\u00b7ber", "hei\u00dft", ":", "ins", "Bet\u00b7te", "sog\u00b7leich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "$.", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So schlag' ich noch erst den Zapfenstreich.", "tokens": ["So", "schlag'", "ich", "noch", "erst", "den", "Zap\u00b7fen\u00b7streich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das t\u00f6net so fein, das klinget so nett,", "tokens": ["Das", "t\u00f6\u00b7net", "so", "fein", ",", "das", "klin\u00b7get", "so", "nett", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "PDS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bidibum, bidibum, jetzt geh'n wir zu Bett.", "tokens": ["Bi\u00b7di\u00b7bum", ",", "bi\u00b7di\u00b7bum", ",", "jetzt", "geh'n", "wir", "zu", "Bett", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Bidibum, bidibum, juchhei\u00dfassa!", "tokens": ["Bi\u00b7di\u00b7bum", ",", "bi\u00b7di\u00b7bum", ",", "juch\u00b7hei\u00b7\u00dfas\u00b7sa", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "FM.la", "$,", "ITJ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nun schlafet Alle wohl, Papa und Mama!", "tokens": ["Nun", "schla\u00b7fet", "Al\u00b7le", "wohl", ",", "Pa\u00b7pa", "und", "Ma\u00b7ma", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Papa, Mama, es ist vollbracht,", "tokens": ["Pa\u00b7pa", ",", "Ma\u00b7ma", ",", "es", "ist", "voll\u00b7bracht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bidibum, bidibum, drum gute Nacht!", "tokens": ["Bi\u00b7di\u00b7bum", ",", "bi\u00b7di\u00b7bum", ",", "drum", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "$,", "PAV", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}