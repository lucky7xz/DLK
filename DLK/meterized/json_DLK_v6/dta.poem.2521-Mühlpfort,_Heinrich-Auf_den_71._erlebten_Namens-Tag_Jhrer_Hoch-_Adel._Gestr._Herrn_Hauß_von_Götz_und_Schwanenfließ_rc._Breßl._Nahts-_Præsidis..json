{"dta.poem.2521": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf den 71. erlebten Namens-Tag Jhrer Hoch-  \n Adel. Gestr.  \n Herrn Hau\u00df von G\u00f6tz und Schwanenflie\u00df/ rc.  \n Bre\u00dfl. Nahts- Pr\u00e6sidis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kan noch das Capitol/ das Wunder-Schlo\u00df der Welt", "tokens": ["Kan", "noch", "das", "Ca\u00b7pi\u00b7tol", "/", "das", "Wun\u00b7der\u00b7Schlo\u00df", "der", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "$(", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff seine Tempel bau\u2019n/ mit Siges-Bogen prangen/", "tokens": ["Auff", "sei\u00b7ne", "Tem\u00b7pel", "bau'n", "/", "mit", "Si\u00b7ge\u00b7sBo\u00b7gen", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$(", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Pfeiler melden an/ wo Cronen vor gehangen/", "tokens": ["Die", "Pfei\u00b7ler", "mel\u00b7den", "an", "/", "wo", "Cro\u00b7nen", "vor", "ge\u00b7han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "PWAV", "NN", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ein gro\u00dfm\u00fchtig Geist/ wenn er den Feind gef\u00e4llt/", "tokens": ["Die", "ein", "gro\u00df\u00b7m\u00fch\u00b7tig", "Geist", "/", "wenn", "er", "den", "Feind", "ge\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem Jupiter geweyht/ kan aus den Lorber-Zweigen", "tokens": ["Dem", "Ju\u00b7pi\u00b7ter", "ge\u00b7weyht", "/", "kan", "aus", "den", "Lor\u00b7ber\u00b7Zwei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$(", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein unverwelckter Ruhm noch bey der Nach-Welt steigen:", "tokens": ["Ein", "un\u00b7ver\u00b7welck\u00b7ter", "Ruhm", "noch", "bey", "der", "Nach\u00b7Welt", "stei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bekr\u00f6nt den Fabins ein ewiges Gericht/", "tokens": ["Be\u00b7kr\u00f6nt", "den", "Fa\u00b7bins", "ein", "e\u00b7wi\u00b7ges", "Ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Und mu\u00df noch African aus dem Metalle blitzen/", "tokens": ["Und", "mu\u00df", "noch", "A\u00b7fri\u00b7can", "aus", "dem", "Me\u00b7tal\u00b7le", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "NE", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn Brutus wil sein Blut vors Vater-Land verspritzen/", "tokens": ["Wenn", "Bru\u00b7tus", "wil", "sein", "Blut", "vors", "Va\u00b7ter\u00b7Land", "ver\u00b7sprit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn Cato standhafft bleibt der Wei\u00dfheit Bild und Licht:", "tokens": ["Wenn", "Ca\u00b7to", "stand\u00b7hafft", "bleibt", "der", "Wei\u00df\u00b7heit", "Bild", "und", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Lebt der Marcellus noch/ und bl\u00fcht in den Gem\u00fchtern/", "tokens": ["Lebt", "der", "Mar\u00b7cel\u00b7lus", "noch", "/", "und", "bl\u00fcht", "in", "den", "Ge\u00b7m\u00fch\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ADV", "$(", "KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Steht des Augustus Bild frey von den Ungewittern/", "tokens": ["Steht", "des", "Au\u00b7gus\u00b7tus", "Bild", "frey", "von", "den", "Un\u00b7ge\u00b7wit\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "NN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Und stirbt die Tugend nicht/ wie sie nicht sterben kan;", "tokens": ["Und", "stirbt", "die", "Tu\u00b7gend", "nicht", "/", "wie", "sie", "nicht", "ster\u00b7ben", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$(", "PWAV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Veraltert nie ihr Glantz/ vergehn nie ihre Straalen/", "tokens": ["Ver\u00b7al\u00b7tert", "nie", "ihr", "Glantz", "/", "ver\u00b7gehn", "nie", "ih\u00b7re", "Straa\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$(", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Darff unsre Vor-Welt nicht nur tapfre Helden mahlen/", "tokens": ["Darff", "uns\u00b7re", "Vor\u00b7Welt", "nicht", "nur", "tapf\u00b7re", "Hel\u00b7den", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ist auch zu unser Zeit was grosses je gethan/", "tokens": ["Ist", "auch", "zu", "un\u00b7ser", "Zeit", "was", "gros\u00b7ses", "je", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "PWS", "ADJA", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So heist es Schuld und Pflicht/ da\u00df man erlauchten Seelen", "tokens": ["So", "heist", "es", "Schuld", "und", "Pflicht", "/", "da\u00df", "man", "er\u00b7lauch\u00b7ten", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$(", "KOUS", "PIS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zum Denckmal Stein und Ertz und Marmel aus-sol h\u00f6len.", "tokens": ["Zum", "Denck\u00b7mal", "Stein", "und", "Ertz", "und", "Mar\u00b7mel", "aus\u00b7sol", "h\u00f6\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "KON", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn das ist je gewi\u00df/ da\u00df eine gr\u00f6\u00dfre Macht", "tokens": ["Denn", "das", "ist", "je", "ge\u00b7wi\u00df", "/", "da\u00df", "ei\u00b7ne", "gr\u00f6\u00df\u00b7re", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Gemeinem Heil zu Nutz auch grosse Geister schicket/", "tokens": ["Ge\u00b7mei\u00b7nem", "Heil", "zu", "Nutz", "auch", "gros\u00b7se", "Geis\u00b7ter", "schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dieselben mit Verstand und seltner Wei\u00dfheit schm\u00fccket/", "tokens": ["Die\u00b7sel\u00b7ben", "mit", "Ver\u00b7stand", "und", "selt\u00b7ner", "Wei\u00df\u00b7heit", "schm\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df vor das Vater-Land ihr munter Auge wacht/", "tokens": ["Da\u00df", "vor", "das", "Va\u00b7ter\u00b7Land", "ihr", "mun\u00b7ter", "Au\u00b7ge", "wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und der Gedancken Ziel nur eintzig dahin gehet/", "tokens": ["Und", "der", "Ge\u00b7dan\u00b7cken", "Ziel", "nur", "eint\u00b7zig", "da\u00b7hin", "ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADV", "ADJD", "PAV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Damit das Regimentin sch\u00f6nster Bl\u00fcte stehet.", "tokens": ["Da\u00b7mit", "das", "Re\u00b7gi\u00b7men\u00b7tin", "sch\u00f6ns\u00b7ter", "Bl\u00fc\u00b7te", "ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was aber misch ich viel von frembden Thaten ein?", "tokens": ["Was", "a\u00b7ber", "misch", "ich", "viel", "von", "fremb\u00b7den", "Tha\u00b7ten", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Zeugt unser Bre\u00dflau nicht/ der Brunnquell der Gesetze", "tokens": ["Zeugt", "un\u00b7ser", "Bre\u00df\u00b7lau", "nicht", "/", "der", "Brunn\u00b7quell", "der", "Ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$(", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und Spiegel guter Zucht/ hierinnen seine Sch\u00e4tze/", "tokens": ["Und", "Spie\u00b7gel", "gu\u00b7ter", "Zucht", "/", "hie\u00b7rin\u00b7nen", "sei\u00b7ne", "Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$(", "PAV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "(wo Staats-erfahrne Leut auch so zu nennen seyn:)", "tokens": ["(", "wo", "Staats\u00b7er\u00b7fahr\u00b7ne", "Leut", "auch", "so", "zu", "nen\u00b7nen", "seyn", ":)"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "emoticon"], "pos": ["$(", "PWAV", "NN", "NN", "ADV", "ADV", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Kan unser ", "tokens": ["Kan", "un\u00b7ser"], "token_info": ["word", "word"], "pos": ["VMFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Und sol man es nicht mehr als den Lycurgus preisen?", "tokens": ["Und", "sol", "man", "es", "nicht", "mehr", "als", "den", "Ly\u00b7cur\u00b7gus", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PPER", "PTKNEG", "ADV", "KOKOM", "ART", "NE", "VVFIN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Rom hat offt seinem Raht viel S\u00e4ulen auffgericht/", "tokens": ["Rom", "hat", "offt", "sei\u00b7nem", "Raht", "viel", "S\u00e4u\u00b7len", "auff\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PPOSAT", "NN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und so der Danckbarkeit Kennzeichen hinterlassen.", "tokens": ["Und", "so", "der", "Dan\u00b7ck\u00b7bar\u00b7keit", "Kenn\u00b7zei\u00b7chen", "hin\u00b7ter\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "H\u00f6chstwerthester Patron/ dergleichen Schlu\u00df zu fassen", "tokens": ["H\u00f6chst\u00b7wert\u00b7hes\u00b7ter", "Pat\u00b7ron", "/", "derg\u00b7lei\u00b7chen", "Schlu\u00df", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "PIS", "NN", "PTKZU", "VVINF"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.34": {"text": "Ist zwar der Wille da/ doch das Vollbringen nicht.", "tokens": ["Ist", "zwar", "der", "Wil\u00b7le", "da", "/", "doch", "das", "Voll\u00b7brin\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$(", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich kan nicht Mentors Hand/ nicht Zeuxis Pinsel f\u00fchren/", "tokens": ["Ich", "kan", "nicht", "Men\u00b7tors", "Hand", "/", "nicht", "Zeu\u00b7xis", "Pin\u00b7sel", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "NN", "$(", "PTKNEG", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die wusten jedes Ding mit Leben au\u00dfzuzieren.", "tokens": ["Die", "wus\u00b7ten", "je\u00b7des", "Ding", "mit", "Le\u00b7ben", "au\u00df\u00b7zu\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein schlechtes Lorber-Blat/ das mir Apollo giebt/", "tokens": ["Ein", "schlech\u00b7tes", "Lor\u00b7ber\u00b7Blat", "/", "das", "mir", "A\u00b7pol\u00b7lo", "giebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PPER", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mec", "tokens": ["Mec"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.39": {"text": "Weil aus des Himmels Gunst Er diesen Tag kan gr\u00fcssen/", "tokens": ["Weil", "aus", "des", "Him\u00b7mels", "Gunst", "Er", "die\u00b7sen", "Tag", "kan", "gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PPER", "PDAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der seinen Namen f\u00fchrt/ und den ein jeder liebt", "tokens": ["Der", "sei\u00b7nen", "Na\u00b7men", "f\u00fchrt", "/", "und", "den", "ein", "je\u00b7der", "liebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "KON", "ART", "ART", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Als selbst sein eigen Licht; darob die Stadt sich freuet/", "tokens": ["Als", "selbst", "sein", "ei\u00b7gen", "Licht", ";", "da\u00b7rob", "die", "Stadt", "sich", "freu\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$.", "PAV", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Weil durch ihr ", "tokens": ["Weil", "durch", "ihr"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.43": {"text": "Zwar w\u00fcnscht\u2019 ich mir ein Lied/ das nach dem Himmel",