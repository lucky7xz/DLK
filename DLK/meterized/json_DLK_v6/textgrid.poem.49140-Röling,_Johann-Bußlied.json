{"textgrid.poem.49140": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Bu\u00dflied", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kehr', o Gott der Gnaden,", "tokens": ["Ich", "kehr'", ",", "o", "Gott", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich wiederum zu dir,", "tokens": ["Mich", "wie\u00b7de\u00b7rum", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Schuld mich zu entladen,", "tokens": ["Der", "Schuld", "mich", "zu", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die dich getrennt von mir;", "tokens": ["Die", "dich", "ge\u00b7trennt", "von", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, Vater, h\u00f6r mich doch,", "tokens": ["Ach", ",", "Va\u00b7ter", ",", "h\u00f6r", "mich", "doch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn du mich w\u00fcrdst verschm\u00e4hen,", "tokens": ["Wenn", "du", "mich", "w\u00fcrdst", "ver\u00b7schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So m\u00fcst' ich untergehen", "tokens": ["So", "m\u00fcst'", "ich", "un\u00b7ter\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In diesem schweren Joch.", "tokens": ["In", "die\u00b7sem", "schwe\u00b7ren", "Joch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar hab' ichs nicht verdienet,", "tokens": ["Zwar", "hab'", "ichs", "nicht", "ver\u00b7die\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du mehr gn\u00e4dig seyst,", "tokens": ["Da\u00df", "du", "mehr", "gn\u00e4\u00b7dig", "seyst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil ich, wenn du vers\u00fchnet,", "tokens": ["Weil", "ich", ",", "wenn", "du", "ver\u00b7s\u00fch\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich niemals recht gepreyst;", "tokens": ["Dich", "nie\u00b7mals", "recht", "ge\u00b7preyst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Viel wenger mich ergetzt,", "tokens": ["Viel", "wen\u00b7ger", "mich", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da ich mich be\u00dfern m\u00f6chte,", "tokens": ["Da", "ich", "mich", "be\u00b7\u00dfern", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Noch da\u00df ich deine Rechte", "tokens": ["Noch", "da\u00df", "ich", "dei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir hertzlich vorgesetzt.", "tokens": ["Mir", "hertz\u00b7lich", "vor\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was aber willstu machen", "tokens": ["Was", "a\u00b7ber", "will\u00b7stu", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit einem armen Knecht?", "tokens": ["Mit", "ei\u00b7nem", "ar\u00b7men", "Knecht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Spr\u00e4chst du in meiner Sachen,", "tokens": ["Spr\u00e4chst", "du", "in", "mei\u00b7ner", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So f\u00fchr' ich mehr als schlecht.", "tokens": ["So", "f\u00fchr'", "ich", "mehr", "als", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach Gott, erbarm dich mein,", "tokens": ["Ach", "Gott", ",", "er\u00b7barm", "dich", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADJD", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich hab' es ja geerbet,", "tokens": ["Ich", "hab'", "es", "ja", "ge\u00b7er\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich gantz bin verderbet,", "tokens": ["Da\u00df", "ich", "gantz", "bin", "ver\u00b7der\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie kan ich be\u00dfer seyn?", "tokens": ["Wie", "kan", "ich", "be\u00b7\u00dfer", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Will gleich der Geist das Gute,", "tokens": ["Will", "gleich", "der", "Geist", "das", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So folgt das Fleisch doch nicht,", "tokens": ["So", "folgt", "das", "Fleisch", "doch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin in meinem Muthe", "tokens": ["Ich", "bin", "in", "mei\u00b7nem", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Offt gantz zu dir gericht.", "tokens": ["Offt", "gantz", "zu", "dir", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach aber, ach, wie leicht", "tokens": ["Ach", "a\u00b7ber", ",", "ach", ",", "wie", "leicht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "$,", "ITJ", "$,", "PWAV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Werd', Herr, ich von der Erden", "tokens": ["Werd'", ",", "Herr", ",", "ich", "von", "der", "Er\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "NN", "$,", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit L\u00fcsten und Beschwerden", "tokens": ["Mit", "L\u00fcs\u00b7ten", "und", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von deinem Dienst gescheucht.", "tokens": ["Von", "dei\u00b7nem", "Dienst", "ge\u00b7scheucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Erleb' ich gute Tage,", "tokens": ["Er\u00b7leb'", "ich", "gu\u00b7te", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So reitzet mich mein Blut,", "tokens": ["So", "reit\u00b7zet", "mich", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Qu\u00e4lt mich des Kreutzes Plage,", "tokens": ["Qu\u00e4lt", "mich", "des", "Kreut\u00b7zes", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So f\u00e4llt mir fort der Muth.", "tokens": ["So", "f\u00e4llt", "mir", "fort", "der", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sieh, Gott, was mir gebricht,", "tokens": ["Sieh", ",", "Gott", ",", "was", "mir", "ge\u00b7bricht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es falle, wie es wolle,", "tokens": ["Es", "fal\u00b7le", ",", "wie", "es", "wol\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So thu ich, was ich solle,", "tokens": ["So", "thu", "ich", ",", "was", "ich", "sol\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch das Geringste nicht.", "tokens": ["Doch", "das", "Ge\u00b7rings\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dies geht mir so zu Hertzen,", "tokens": ["Dies", "geht", "mir", "so", "zu", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich f\u00fcr Reu und Leid,", "tokens": ["Da\u00df", "ich", "f\u00fcr", "Reu", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr Angst und Seelen-Schmertzen", "tokens": ["F\u00fcr", "Angst", "und", "See\u00b7len\u00b7Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht von der H\u00f6llen weit;", "tokens": ["Nicht", "von", "der", "H\u00f6l\u00b7len", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ja, w\u00e4re nicht dein Wort,", "tokens": ["Ja", ",", "w\u00e4\u00b7re", "nicht", "dein", "Wort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So m\u00fcst' ich ganz verzagen", "tokens": ["So", "m\u00fcst'", "ich", "ganz", "ver\u00b7za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und gleich dem Kain klagen,", "tokens": ["Und", "gleich", "dem", "Kain", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Du treibst ohn Trost mich fort.", "tokens": ["Du", "treibst", "ohn", "Trost", "mich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nun hastu ja geschworen", "tokens": ["Nun", "has\u00b7tu", "ja", "ge\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Au\u00df gro\u00dfer Lieb' und Huld,", "tokens": ["Au\u00df", "gro\u00b7\u00dfer", "Lieb'", "und", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Keiner sey verloren", "tokens": ["Da\u00df", "Kei\u00b7ner", "sey", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von wegen seiner Schuld,", "tokens": ["Von", "we\u00b7gen", "sei\u00b7ner", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der sich bekehrt zu dir;", "tokens": ["Der", "sich", "be\u00b7kehrt", "zu", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auff diese deine Treue", "tokens": ["Auff", "die\u00b7se", "dei\u00b7ne", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Bitt' ich in wahrer Reue,", "tokens": ["Bitt'", "ich", "in", "wah\u00b7rer", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verzeih, mein Sch\u00f6pffer, mir.", "tokens": ["Ver\u00b7zeih", ",", "mein", "Sch\u00f6pf\u00b7fer", ",", "mir", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Schon' Herr, ach schone meiner,", "tokens": ["Schon'", "Herr", ",", "ach", "scho\u00b7ne", "mei\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ITJ", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ja, schon', o Menschen-Freund,", "tokens": ["Ja", ",", "schon'", ",", "o", "Men\u00b7schen\u00b7Freund", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht minder selber deiner:", "tokens": ["Nicht", "min\u00b7der", "sel\u00b7ber", "dei\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn wie kanst du mein Feind", "tokens": ["Denn", "wie", "kanst", "du", "mein", "Feind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ohn deinen Schaden seyn,", "tokens": ["Ohn", "dei\u00b7nen", "Scha\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da du au\u00df deinem Leben", "tokens": ["Da", "du", "au\u00df", "dei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mir hast den Geist gegeben,", "tokens": ["Mir", "hast", "den", "Geist", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist auch mein Fall ja dein?", "tokens": ["Ist", "auch", "mein", "Fall", "ja", "dein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Bestehstu denn, da\u00df S\u00fcnde", "tokens": ["Be\u00b7steh\u00b7stu", "denn", ",", "da\u00df", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht gut ohn Straffe thu,", "tokens": ["Nicht", "gut", "ohn", "Straf\u00b7fe", "thu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So siehe deinem Kinde", "tokens": ["So", "sie\u00b7he", "dei\u00b7nem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seiner Marter zu:", "tokens": ["In", "sei\u00b7ner", "Mar\u00b7ter", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das fromme Sch\u00e4ffelein", "tokens": ["Das", "from\u00b7me", "Sch\u00e4f\u00b7fe\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hat so viel Pein u