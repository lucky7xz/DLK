{"dta.poem.12868": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bombice d\u2019Amore.  \n  Aus dem welschen des   Marini.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin mein eigner ungl\u00fccks-schmid", "tokens": ["Ich", "bin", "mein", "eig\u00b7ner", "un\u00b7gl\u00fccks\u00b7schmid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und, wie ein seiden-wurm, nur sinnreich mich zu kr\u00e4ncken.", "tokens": ["Und", ",", "wie", "ein", "sei\u00b7den\u00b7wurm", ",", "nur", "sinn\u00b7reich", "mich", "zu", "kr\u00e4n\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich sollt\u2019 auf wahre ruh gedencken,", "tokens": ["Ich", "sollt'", "auf", "wah\u00b7re", "ruh", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bin um lauter angst bem\u00fcht.", "tokens": ["Und", "bin", "um", "lau\u00b7ter", "angst", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein hertze nehret sich von d\u00fcrren hoffnungs-bl\u00e4ttern,", "tokens": ["Mein", "hert\u00b7ze", "neh\u00b7ret", "sich", "von", "d\u00fcr\u00b7ren", "hoff\u00b7nungs\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und meine liebe sucht auf einen baum zu klettern,", "tokens": ["Und", "mei\u00b7ne", "lie\u00b7be", "sucht", "auf", "ei\u00b7nen", "baum", "zu", "klet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der weder frucht noch schatten geben kan.", "tokens": ["Der", "we\u00b7der", "frucht", "noch", "schat\u00b7ten", "ge\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "NN", "ADV", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich spinn\u2019 und fange zwar viel sch\u00f6ne faden an.", "tokens": ["Ich", "spinn'", "und", "fan\u00b7ge", "zwar", "viel", "sch\u00f6\u00b7ne", "fa\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PIAT", "ADJA", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was aber sind es? nichts, als flatternde gedancken,", "tokens": ["Was", "a\u00b7ber", "sind", "es", "?", "nichts", ",", "als", "flat\u00b7tern\u00b7de", "ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "$.", "PIS", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die der begierden hand in meines hauptes schrancken,", "tokens": ["Die", "der", "be\u00b7gier\u00b7den", "hand", "in", "mei\u00b7nes", "haup\u00b7tes", "schran\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nur immer mehr und mehr verwickelt und verstrickt,", "tokens": ["Nur", "im\u00b7mer", "mehr", "und", "mehr", "ver\u00b7wi\u00b7ckelt", "und", "ver\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "KON", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bis sie der tod zerst\u00fcckt.", "tokens": ["Bis", "sie", "der", "tod", "zer\u00b7st\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "So bau ich mir, o seltsames verh\u00e4ngni\u00df!", "tokens": ["So", "bau", "ich", "mir", ",", "o", "selt\u00b7sa\u00b7mes", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Mit eigner hand und kunst ein bangsames gef\u00e4ngni\u00df,", "tokens": ["Mit", "eig\u00b7ner", "hand", "und", "kunst", "ein", "bang\u00b7sa\u00b7mes", "ge\u00b7f\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und endlich gar mein eigen grab.", "tokens": ["Und", "end\u00b7lich", "gar", "mein", "ei\u00b7gen", "grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}