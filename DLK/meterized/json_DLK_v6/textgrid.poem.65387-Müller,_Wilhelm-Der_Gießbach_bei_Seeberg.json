{"textgrid.poem.65387": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Gie\u00dfbach bei Seeberg", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle Felsen will er zerbrechen,", "tokens": ["Al\u00b7le", "Fel\u00b7sen", "will", "er", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und er zerbricht und zersch\u00e4umet nur sich.", "tokens": ["Und", "er", "zer\u00b7bricht", "und", "zer\u00b7sch\u00e4u\u00b7met", "nur", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "PRF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von Klippe zu Klippe", "tokens": ["Von", "Klip\u00b7pe", "zu", "Klip\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Springt er mit Brausen,", "tokens": ["Springt", "er", "mit", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Spritzend und sprudelnd,", "tokens": ["Sprit\u00b7zend", "und", "spru\u00b7delnd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Als h\u00e4tt' er Meere", "tokens": ["Als", "h\u00e4tt'", "er", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "So zu vergeuden.", "tokens": ["So", "zu", "ver\u00b7geu\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Und unten im Thale,", "tokens": ["Und", "un\u00b7ten", "im", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo ist er geblieben?", "tokens": ["Wo", "ist", "er", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Im Sande schleicht er", "tokens": ["Im", "San\u00b7de", "schleicht", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Matt und verschmachtend,", "tokens": ["Matt", "und", "ver\u00b7schmach\u00b7tend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und die Berge", "tokens": ["Und", "die", "Ber\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.6": {"text": "Stehn und schauen", "tokens": ["Stehn", "und", "schau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Stolz und h\u00f6hnend", "tokens": ["Stolz", "und", "h\u00f6h\u00b7nend"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Auf ihn nieder.", "tokens": ["Auf", "ihn", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Oder meinen sie dich,", "tokens": ["O\u00b7der", "mei\u00b7nen", "sie", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Erdens\u00f6hnchen,", "tokens": ["Er\u00b7den\u00b7s\u00f6hn\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Das wie der Gie\u00dfbach", "tokens": ["Das", "wie", "der", "Gie\u00df\u00b7bach"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "St\u00fcrmet und st\u00fcrzet und brauset durch's Leben?", "tokens": ["St\u00fcr\u00b7met", "und", "st\u00fcr\u00b7zet", "und", "brau\u00b7set", "durch's", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}