{"dta.poem.5348": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So Tag als Nacht auf auders nichts gedencken,", "tokens": ["So", "Tag", "als", "Nacht", "auf", "au\u00b7ders", "nichts", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "APPR", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht einen Blick auf sich, auf GOttes Wercke, lencken;", "tokens": ["Nicht", "ei\u00b7nen", "Blick", "auf", "sich", ",", "auf", "Got\u00b7tes", "Wer\u00b7cke", ",", "len\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "PRF", "$,", "APPR", "NN", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So scheinen sie ja wol nichts bessers wehrt,", "tokens": ["So", "schei\u00b7nen", "sie", "ja", "wol", "nichts", "bes\u00b7sers", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als da\u00df sie mit den K\u00e4fern in der Erden,", "tokens": ["Als", "da\u00df", "sie", "mit", "den", "K\u00e4\u00b7fern", "in", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den B\u00fcrgern faulen Mists, verglichen werden.", "tokens": ["Den", "B\u00fcr\u00b7gern", "fau\u00b7len", "Mists", ",", "ver\u00b7gli\u00b7chen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch halt, mich deucht, wie sich Chrysander hier erkl\u00e4hrt:", "tokens": ["Doch", "halt", ",", "mich", "deucht", ",", "wie", "sich", "Chry\u00b7san\u00b7der", "hier", "er\u00b7kl\u00e4hrt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PWAV", "PRF", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie kommt es doch, da\u00df dir so Geld als Mist", "tokens": ["Wie", "kommt", "es", "doch", ",", "da\u00df", "dir", "so", "Geld", "als", "Mist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So scheuslich und ver\u00e4chtlich ist?", "tokens": ["So", "scheus\u00b7lich", "und", "ver\u00b7\u00e4cht\u00b7lich", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da sich jedoch die gantze Welt", "tokens": ["Da", "sich", "je\u00b7doch", "die", "gant\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Durch Geld und Mist allein erh\u00e4lt.", "tokens": ["Durch", "Geld", "und", "Mist", "al\u00b7lein", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Durch Mist wird Fruchtbarkeit im Land\u2019 erreget,", "tokens": ["Durch", "Mist", "wird", "Frucht\u00b7bar\u00b7keit", "im", "Land'", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Das uns die Kost und Nahrung tr\u00e4get;", "tokens": ["Das", "uns", "die", "Kost", "und", "Nah\u00b7rung", "tr\u00e4\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Durch Geld wird alles das erhalten,", "tokens": ["Durch", "Geld", "wird", "al\u00b7les", "das", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Was uns erh\u00e4lt, vergn\u00fcgt und sch\u00fctzt,", "tokens": ["Was", "uns", "er\u00b7h\u00e4lt", ",", "ver\u00b7gn\u00fcgt", "und", "sch\u00fctzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Was uns bey Jungen und bey Alten", "tokens": ["Was", "uns", "bey", "Jun\u00b7gen", "und", "bey", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Gewogenheit erreget, Ansehn giebt,", "tokens": ["Ge\u00b7wo\u00b7gen\u00b7heit", "er\u00b7re\u00b7get", ",", "An\u00b7sehn", "giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wodurch man uns verehrt und liebt:", "tokens": ["Wo\u00b7durch", "man", "uns", "ver\u00b7ehrt", "und", "liebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ist die\u00df denn nicht der M\u00fche wehrt,", "tokens": ["Ist", "die\u00df", "denn", "nicht", "der", "M\u00fc\u00b7he", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da\u00df man es achtet und begehrt?", "tokens": ["Da\u00df", "man", "es", "ach\u00b7tet", "und", "be\u00b7gehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die\u00df ist zwar wahr, Chrysander, aber h\u00f6re,", "tokens": ["Die\u00df", "ist", "zwar", "wahr", ",", "Chry\u00b7san\u00b7der", ",", "a\u00b7ber", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Du hast ja alles dieses nicht.", "tokens": ["Du", "hast", "ja", "al\u00b7les", "die\u00b7ses", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "PDAT", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Dir fehlt Beqvehmlichkeit, Vergn\u00fcgen, Lieb\u2019 und Ehre,", "tokens": ["Dir", "fehlt", "Be\u00b7qvehm\u00b7lich\u00b7keit", ",", "Ver\u00b7gn\u00fc\u00b7gen", ",", "Lieb'", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nichts ist fast, das dir nicht gebricht.", "tokens": ["Nichts", "ist", "fast", ",", "das", "dir", "nicht", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Indem du gar nichts Guts mit deinem Gelde schaffest;", "tokens": ["In\u00b7dem", "du", "gar", "nichts", "Guts", "mit", "dei\u00b7nem", "Gel\u00b7de", "schaf\u00b7fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es blos allein zusammen raffest,", "tokens": ["Es", "blos", "al\u00b7lein", "zu\u00b7sam\u00b7men", "raf\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.26": {"text": "Um Geld auf Geld zu h\u00e4uffen; dich vernarrst,", "tokens": ["Um", "Geld", "auf", "Geld", "zu", "h\u00e4uf\u00b7fen", ";", "dich", "ver\u00b7narrst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPR", "NN", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und blos nur, um zu scharren, scharrst.", "tokens": ["Und", "blos", "nur", ",", "um", "zu", "schar\u00b7ren", ",", "scharrst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Sollt\u2019 alle Kraft von deiner Seelen,", "tokens": ["Sollt'", "al\u00b7le", "Kraft", "von", "dei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Die Absicht, da\u00df du worden bist, allein", "tokens": ["Die", "Ab\u00b7sicht", ",", "da\u00df", "du", "wor\u00b7den", "bist", ",", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VAPP", "VAFIN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Auf Geld zu sammlen und zu zehlen", "tokens": ["Auf", "Geld", "zu", "samm\u00b7len", "und", "zu", "zeh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Bestimmet und genommen seyn?", "tokens": ["Be\u00b7stim\u00b7met", "und", "ge\u00b7nom\u00b7men", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}