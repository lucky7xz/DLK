{"textgrid.poem.31236": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch gestern/ wenn ich es bed\u00e4ncke/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch gestern/ wenn ich es bed\u00e4ncke/", "tokens": ["Noch", "ge\u00b7stern", "/", "wenn", "ich", "es", "be\u00b7d\u00e4n\u00b7cke", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "PPER", "PPER", "VVFIN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "sa\u00df ich mit ", "tokens": ["sa\u00df", "ich", "mit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Itzt mu\u00df ich insgesambt solch Drachten", "tokens": ["Itzt", "mu\u00df", "ich", "ins\u00b7ge\u00b7sambt", "solch", "Drach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "for faule ", "tokens": ["for", "fau\u00b7le"], "token_info": ["word", "word"], "pos": ["FM.la", "FM.la"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Das saufft und raufft und flucht und schwehrt,", "tokens": ["Das", "saufft", "und", "raufft", "und", "flucht", "und", "schwehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "nicht einer ist sein Leder wehrt!", "tokens": ["nicht", "ei\u00b7ner", "ist", "sein", "Le\u00b7der", "wehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "zu sehr nach Budergens und Salben.", "tokens": ["zu", "sehr", "nach", "Bu\u00b7der\u00b7gens", "und", "Sal\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Zobel-P\u00e4ltz mit hundret Schw\u00e4ntzen/", "tokens": ["Ein", "Zo\u00b7bel\u00b7P\u00e4ltz", "mit", "hund\u00b7ret", "Schw\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "das wer so wa\u00df for ihn zum Gl\u00e4ntzen!", "tokens": ["das", "wer", "so", "wa\u00df", "for", "ihn", "zum", "Gl\u00e4nt\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PWS", "ADV", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am lihbsten m\u00fccht er sich verschreiben", "tokens": ["Am", "lihbs\u00b7ten", "m\u00fccht", "er", "sich", "ver\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ein W\u00e4gelgen mit Spihgel-Scheiben.", "tokens": ["ein", "W\u00e4\u00b7gel\u00b7gen", "mit", "Spih\u00b7gel\u00b7Schei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie dihser Tummrian sich zihrt/", "tokens": ["Wie", "dih\u00b7ser", "Tumm\u00b7ri\u00b7an", "sich", "zihrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "hat mich schon offters affrontirt!", "tokens": ["hat", "mich", "schon", "off\u00b7ters", "af\u00b7fron\u00b7tirt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "nur auff die lange Weiber-Kleider.", "tokens": ["nur", "auff", "die", "lan\u00b7ge", "Wei\u00b7ber\u00b7Klei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In allen Winckelgens und Ekken", "tokens": ["In", "al\u00b7len", "Win\u00b7ckel\u00b7gens", "und", "Ek\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mu\u00df er sich mit den M\u00e4gdgens zekken.", "tokens": ["mu\u00df", "er", "sich", "mit", "den", "M\u00e4gd\u00b7gens", "zek\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Zu jeder hoppt er hin und schnoppert/", "tokens": ["Zu", "je\u00b7der", "hoppt", "er", "hin", "und", "schnop\u00b7pert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "wa\u00df ihr wohl hindterm D\u00fcnn-Tuch boppert.", "tokens": ["wa\u00df", "ihr", "wohl", "hind\u00b7term", "D\u00fcnn\u00b7Tuch", "bop\u00b7pert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Darbey so hat das geule Vieh", "tokens": ["Dar\u00b7bey", "so", "hat", "das", "geu\u00b7le", "Vieh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "doch die Frantzosen-", "tokens": ["doch", "die", "Frant\u00b7zo\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "TRUNC"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "sich seinen Magen zu kasteyen.", "tokens": ["sich", "sei\u00b7nen", "Ma\u00b7gen", "zu", "kas\u00b7te\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Befillt mit Sauer-Kraut und Schwarten/", "tokens": ["Be\u00b7fillt", "mit", "Sau\u00b7e\u00b7rKraut", "und", "Schwar\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "kan er der K\u00e4sgens kaum noch warten.", "tokens": ["kan", "er", "der", "K\u00e4s\u00b7gens", "kaum", "noch", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u00dcmb Hammel-Fleisch und gr\u00fcne Bohnen", "tokens": ["\u00dcmb", "Ham\u00b7mel\u00b7Fleisch", "und", "gr\u00fc\u00b7ne", "Boh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "lih\u00df er ein Beutelgen Tublonen.", "tokens": ["lih\u00df", "er", "ein", "Beu\u00b7tel\u00b7gen", "Tub\u00b7lo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Darfor so schwappt ihm schon das Fett/", "tokens": ["Dar\u00b7for", "so", "schwappt", "ihm", "schon", "das", "Fett", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "er daugt nicht mehr al\u00df Ober-Bett!", "tokens": ["er", "daugt", "nicht", "mehr", "al\u00df", "O\u00b7ber\u00b7Bett", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Fast Nacht for Nacht geht er gassaten.", "tokens": ["Fast", "Nacht", "for", "Nacht", "geht", "er", "gas\u00b7sa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Lichtrichins au\u00df den Laternen", "tokens": ["Die", "Licht\u00b7ri\u00b7chins", "au\u00df", "den", "La\u00b7ter\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mu\u00df er ", "tokens": ["mu\u00df", "er"], "token_info": ["word", "word"], "pos": ["VMFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Gleich/ k\u00fckkt ihm einer zu verstohlen/", "tokens": ["Gleich", "/", "k\u00fckkt", "ihm", "ei\u00b7ner", "zu", "ver\u00b7stoh\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "br\u00fcllt er: Herr Bruder/ die Bistohlen!", "tokens": ["br\u00fcllt", "er", ":", "Herr", "Bru\u00b7der", "/", "die", "Bis\u00b7toh\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "NN", "NN", "$(", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Noch keinen sah man auff der Welt/", "tokens": ["Noch", "kei\u00b7nen", "sah", "man", "auff", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "VVFIN", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der so al\u00df Waldt-Schwein sich verstellt!", "tokens": ["der", "so", "al\u00df", "Waldt\u00b7Schwein", "sich", "ver\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOUS", "NN", "PRF", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "for einen halben Schlukk Canari.", "tokens": ["for", "ei\u00b7nen", "hal\u00b7ben", "Schlukk", "Ca\u00b7na\u00b7ri", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht einer sah mit krausen Zeichen", "tokens": ["Nicht", "ei\u00b7ner", "sah", "mit", "krau\u00b7sen", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ihn je schon ein Bappihr bestreichen.", "tokens": ["ihn", "je", "schon", "ein", "Bap\u00b7pihr", "be\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er h\u00e4lt die hohe ", "tokens": ["Er", "h\u00e4lt", "die", "ho\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "for alte Heerings-Kr\u00e4merinnen", "tokens": ["for", "al\u00b7te", "Hee\u00b7rings\u00b7Kr\u00e4\u00b7me\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und lihbt bi\u00df murgens fr\u00fch \u00fcmb Acht", "tokens": ["und", "lihbt", "bi\u00df", "mur\u00b7gens", "fr\u00fch", "\u00fcmb", "Acht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ADJD", "APPR", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "die ohngemein verl\u00e4ngkte Nacht.", "tokens": ["die", "ohn\u00b7ge\u00b7mein", "ver\u00b7l\u00e4ngk\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur ich sizz hihr auff meinem Stertze", "tokens": ["Nur", "ich", "sizz", "hihr", "auff", "mei\u00b7nem", "Stert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "vergn\u00fcgt bey einer Unschlitt-Kertze", "tokens": ["ver\u00b7gn\u00fcgt", "bey", "ei\u00b7ner", "Un\u00b7schlit\u00b7tKert\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und m\u00fche fleissig Hirn und H\u00e4nde", "tokens": ["und", "m\u00fc\u00b7he", "fleis\u00b7sig", "Hirn", "und", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "bloh\u00df for die h\u00f6hern Gegenst\u00e4nde.", "tokens": ["bloh\u00df", "for", "die", "h\u00f6\u00b7hern", "Ge\u00b7gen\u00b7st\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Sammt-Barett mit blancken Tressen", "tokens": ["Mein", "Samm\u00b7tBa\u00b7rett", "mit", "blan\u00b7cken", "Tres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "staubt hindterm Ofen/ fast vergessen;", "tokens": ["staubt", "hind\u00b7term", "O\u00b7fen", "/", "fast", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$(", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "schon schirrt ", "tokens": ["schon", "schirrt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Neid/ halt dein blaues L\u00e4ster-Maul!", "tokens": ["Neid", "/", "halt", "dein", "blau\u00b7es", "L\u00e4s\u00b7ter\u00b7Maul", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}