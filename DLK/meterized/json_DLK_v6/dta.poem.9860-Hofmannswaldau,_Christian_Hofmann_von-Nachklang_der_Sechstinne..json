{"dta.poem.9860": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Nachklang der Sechstinne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der haare sch\u00f6nes gold/ der augen lichter brand/", "tokens": ["Der", "haa\u00b7re", "sch\u00f6\u00b7nes", "gold", "/", "der", "au\u00b7gen", "lich\u00b7ter", "brand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wangen paradie\u00df/ der lippen himmel-wein/", "tokens": ["Der", "wan\u00b7gen", "pa\u00b7ra\u00b7die\u00df", "/", "der", "lip\u00b7pen", "him\u00b7mel\u00b7wein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat mit des halses zier/ ohn allen zwang/ bekannt/", "tokens": ["Hat", "mit", "des", "hal\u00b7ses", "zier", "/", "ohn", "al\u00b7len", "zwang", "/", "be\u00b7kannt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$(", "APPR", "PIS", "VVFIN", "$(", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df auff den br\u00fcsten soll der liebe ruhstatt seyn.", "tokens": ["Da\u00df", "auff", "den", "br\u00fcs\u00b7ten", "soll", "der", "lie\u00b7be", "ruh\u00b7statt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "VMFIN", "ART",