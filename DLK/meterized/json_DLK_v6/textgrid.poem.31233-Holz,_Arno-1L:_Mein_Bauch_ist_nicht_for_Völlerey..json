{"textgrid.poem.31233": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Bauch ist nicht for V\u00f6llerey.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Bauch ist nicht for V\u00f6llerey.", "tokens": ["Mein", "Bauch", "ist", "nicht", "for", "V\u00f6l\u00b7le\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch dih\u00df so mu\u00df ich sagen:", "tokens": ["Doch", "dih\u00df", "so", "mu\u00df", "ich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "der g\u00f6ldne Monahts-K\u00f6nig Mey", "tokens": ["der", "g\u00f6ld\u00b7ne", "Mo\u00b7nahts\u00b7K\u00f6\u00b7nig", "Mey"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "fegt mir nicht bloh\u00df die Leber frey/", "tokens": ["fegt", "mir", "nicht", "bloh\u00df", "die", "Le\u00b7ber", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "ADJD", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "er st\u00e4rckt mir auch den Magen!", "tokens": ["er", "st\u00e4rckt", "mir", "auch", "den", "Ma\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Artschokken/ Bortulak/ Spenat/", "tokens": ["Art\u00b7schok\u00b7ken", "/", "Bor\u00b7tu\u00b7lak", "/", "Spe\u00b7nat", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so nichts br\u00e4ucht man zu schonen;", "tokens": ["so", "nichts", "br\u00e4ucht", "man", "zu", "scho\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Endiwjen gihbts und Kopff-Saulat/", "tokens": ["E\u00b7ndiw\u00b7jen", "gihbts", "und", "Kopf\u00b7fSau\u00b7lat", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "s\u00e4lbst Spargel siht man schon barat/", "tokens": ["s\u00e4lbst", "Spar\u00b7gel", "siht", "man", "schon", "ba\u00b7rat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Rabuntzelgens und Bohnen.", "tokens": ["Ra\u00b7bunt\u00b7zel\u00b7gens", "und", "Boh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "fischt Krebsckens und Forellen/", "tokens": ["fischt", "Kreb\u00b7sckens", "und", "Fo\u00b7rel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und s\u00e4lbst ", "tokens": ["und", "s\u00e4lbst"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "k\u00e4ut Dill und Bimpinellen.", "tokens": ["k\u00e4ut", "Dill", "und", "Bim\u00b7pi\u00b7nel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Itzt schm\u00e4kkt zu Hammel P\u00e4rl-Porree/", "tokens": ["Itzt", "schm\u00e4kkt", "zu", "Ham\u00b7mel", "P\u00e4r\u00b7lPor\u00b7ree", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "itzt neid ich nicht die Dodten/", "tokens": ["itzt", "neid", "ich", "nicht", "die", "Dod\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "itzt halt ich mich nicht retir\u00e9/", "tokens": ["itzt", "halt", "ich", "mich", "nicht", "re\u00b7tir\u00e9", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wenn ich auff einem Deller seh", "tokens": ["wenn", "ich", "auff", "ei\u00b7nem", "Del\u00b7ler", "seh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Butt-H\u00fchngen-Fleisch mit Schoten!", "tokens": ["But\u00b7tH\u00fchn\u00b7gen\u00b7Fleisch", "mit", "Scho\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Darzu so sch\u00e4nck ich mir wa\u00df ein/", "tokens": ["Dar\u00b7zu", "so", "sch\u00e4n\u00b7ck", "ich", "mir", "wa\u00df", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "PPER", "VVFIN", "ART", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "sonst sch\u00e4rfft sich mir mein Bl\u00fcthgen;", "tokens": ["sonst", "sch\u00e4rfft", "sich", "mir", "mein", "Bl\u00fcth\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "doch darffs des offtern auch/ statt Wein/", "tokens": ["doch", "darffs", "des", "off\u00b7tern", "auch", "/", "statt", "Wein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PAV", "ART", "ADV", "ADV", "$(", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Pfund-Bier au\u00df K\u00f6tschenbroda seyn/", "tokens": ["Pfun\u00b7dBier", "au\u00df", "K\u00f6t\u00b7schen\u00b7bro\u00b7da", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "das steigt nicht so ins H\u00fctgen.", "tokens": ["das", "steigt", "nicht", "so", "ins", "H\u00fct\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dikk auffgebluhsterter ", "tokens": ["Dikk", "auff\u00b7ge\u00b7bluhs\u00b7ter\u00b7ter"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "bedrillre deine Meikens!", "tokens": ["be\u00b7drill\u00b7re", "dei\u00b7ne", "Mei\u00b7kens", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Itzt m\u00fcht sich mein gespizzter Kihl", "tokens": ["Itzt", "m\u00fcht", "sich", "mein", "ge\u00b7spizz\u00b7ter", "Kihl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nur for den lihben Betersihl", "tokens": ["nur", "for", "den", "lih\u00b7ben", "Be\u00b7ter\u00b7sihl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und for die Kibitz-Eyckens!", "tokens": ["und", "for", "die", "Ki\u00b7bitz\u00b7Ey\u00b7ckens", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}