{"dta.poem.11919": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem unbeschriebnen Blatt des Geistes in dem Kinde", "tokens": ["Dem", "un\u00b7be\u00b7schrieb\u00b7nen", "Blatt", "des", "Geis\u00b7tes", "in", "dem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schreib unbed\u00e4chtig nicht zu viel ein zu geschwinde.", "tokens": ["Schreib", "un\u00b7be\u00b7d\u00e4ch\u00b7tig", "nicht", "zu", "viel", "ein", "zu", "ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKNEG", "PTKA", "PIS", "ART", "PTKZU", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar wird nie voll das Blatt, stets neu zu \u00fcberschreiben,", "tokens": ["Zwar", "wird", "nie", "voll", "das", "Blatt", ",", "stets", "neu", "zu", "\u00fc\u00b7bersc\u00b7hrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch keine Schrift so fest wird als die erste bleiben.", "tokens": ["Doch", "kei\u00b7ne", "Schrift", "so", "fest", "wird", "als", "die", "ers\u00b7te", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "ADJD", "VAFIN", "KOKOM", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ja keine Kunst vermag sie v\u00f6llig wegzuwischen;", "tokens": ["Ja", "kei\u00b7ne", "Kunst", "ver\u00b7mag", "sie", "v\u00f6l\u00b7lig", "weg\u00b7zu\u00b7wi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was man auch dr\u00fcber schreibt, sie schimmert durch dazwischen.", "tokens": ["Was", "man", "auch", "dr\u00fc\u00b7ber", "schreibt", ",", "sie", "schim\u00b7mert", "durch", "da\u00b7zwi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "PAV", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und manchen Forscher freuts, den Neues wenig freut,", "tokens": ["Und", "man\u00b7chen", "For\u00b7scher", "freuts", ",", "den", "Neu\u00b7es", "we\u00b7nig", "freut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NE", "$,", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn rathend er die halb sichtbare Schrift erneut.", "tokens": ["Wenn", "ra\u00b7thend", "er", "die", "halb", "sicht\u00b7ba\u00b7re", "Schrift", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Du selber m\u00f6gest einst, wann sp\u00e4tre Schriften schwinden,", "tokens": ["Du", "sel\u00b7ber", "m\u00f6\u00b7gest", "einst", ",", "wann", "sp\u00e4t\u00b7re", "Schrif\u00b7ten", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erloschne Kinderz\u00fcg' im Herzen wieder finden.", "tokens": ["Er\u00b7lo\u00b7schne", "Kin\u00b7der\u00b7z\u00fcg'", "im", "Her\u00b7zen", "wie\u00b7der", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}