e,", "tokens": ["Dir", "auf", "den", "Schu\u00b7len", "schmei\u00b7cheln", "mus\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn ohngefehr der Pr\u00fcfungstag", "tokens": ["Wenn", "ohn\u00b7ge\u00b7fehr", "der", "Pr\u00fc\u00b7fungs\u00b7tag"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihm n\u00e4chtlich in Gedancken lag", "tokens": ["Ihm", "n\u00e4cht\u00b7lich", "in", "Ge\u00b7dan\u00b7cken", "lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und nichts als dich zum Helfer wuste.", "tokens": ["Und", "nichts", "als", "dich", "zum", "Hel\u00b7fer", "wus\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Jezt wird er als ein gro\u00dfes Thier", "tokens": ["Jezt", "wird", "er", "als", "ein", "gro\u00b7\u00dfes", "Thier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir kaum noch \u00fcber Achsel dancken.", "tokens": ["Dir", "kaum", "noch", "\u00fc\u00b7ber", "Ach\u00b7sel", "dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach faules Herz, wo bleiben wir?", "tokens": ["Ach", "fau\u00b7les", "Herz", ",", "wo", "blei\u00b7ben", "w