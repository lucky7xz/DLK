{"textgrid.poem.44027": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du wirst noch wohl, verzagtes Herz,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wirst noch wohl, verzagtes Herz,", "tokens": ["Du", "wirst", "noch", "wohl", ",", "ver\u00b7zag\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Unmuth in die Erde sincken.", "tokens": ["Vor", "Un\u00b7muth", "in", "die", "Er\u00b7de", "sin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was helfen Thr\u00e4nen vor den Schmerz?", "tokens": ["Was", "hel\u00b7fen", "Thr\u00e4\u00b7nen", "vor", "den", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du must ganz andern Julep trincken.", "tokens": ["Du", "must", "ganz", "an\u00b7dern", "Ju\u00b7lep", "trin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo ist dein gro\u00dfmuthsvoller Sinn", "tokens": ["Wo", "ist", "dein", "gro\u00df\u00b7muths\u00b7vol\u00b7ler", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mitsamt dem tapfern Vorsaz hin,", "tokens": ["Mit\u00b7samt", "dem", "tap\u00b7fern", "Vor\u00b7saz", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch Fels und M\u00fch emporzubrechen?", "tokens": ["Durch", "Fels", "und", "M\u00fch", "em\u00b7por\u00b7zu\u00b7bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Betrachte doch den Unterscheid", "tokens": ["Be\u00b7trach\u00b7te", "doch", "den", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Lieder alt- und dieser Zeit:", "tokens": ["Der", "Lie\u00b7der", "al\u00b7t", "und", "die\u00b7ser", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "TRUNC", "KON", "PDAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Was wird die Nachwelt von dir sprechen?", "tokens": ["Was", "wird", "die", "Nach\u00b7welt", "von", "dir", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erweck einmahl den faulen Muth,", "tokens": ["Er\u00b7weck", "ein\u00b7mahl", "den", "fau\u00b7len", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Trunck und Wollust eingenommen.", "tokens": ["Den", "Trunck", "und", "Wol\u00b7lust", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du brauchst vorwahr nicht schlechte Glut,", "tokens": ["Du", "brauchst", "vor\u00b7wahr", "nicht", "schlech\u00b7te", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ber\u00fchmten Seelen nachzukommen.", "tokens": ["Be\u00b7r\u00fchm\u00b7ten", "See\u00b7len", "nach\u00b7zu\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier sezt es Schwei\u00df, hier kostet's M\u00fch;", "tokens": ["Hier", "sezt", "es", "Schwei\u00df", ",", "hier", "kos\u00b7tet's", "M\u00fch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du wilst ja, da\u00df dein Nahme bl\u00fch;", "tokens": ["Du", "wilst", "ja", ",", "da\u00df", "dein", "Nah\u00b7me", "bl\u00fch", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Gram verspricht dir schlechte Tittel;", "tokens": ["Der", "Gram", "ver\u00b7spricht", "dir", "schlech\u00b7te", "Tit\u00b7tel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er ist ein Kind der Weichligkeit", "tokens": ["Er", "ist", "ein", "Kind", "der", "Weich\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und ist bei dem, den Ruhm erfreut,", "tokens": ["Und", "ist", "bei", "dem", ",", "den", "Ruhm", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das sch\u00e4dlichste Verhindrungsmittel.", "tokens": ["Das", "sch\u00e4d\u00b7lichs\u00b7te", "Ver\u00b7hin\u00b7drungs\u00b7mit\u00b7tel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du weist vor Ungl\u00fcck nicht wonaus;", "tokens": ["Du", "weist", "vor", "Un\u00b7gl\u00fcck", "nicht", "wo\u00b7naus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedult, die Vorsicht sinnt auf Wege.", "tokens": ["Ge\u00b7dult", ",", "die", "Vor\u00b7sicht", "sinnt", "auf", "We\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist nur selbst dein Marterhaus", "tokens": ["Du", "bist", "nur", "selbst", "dein", "Mar\u00b7ter\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und machst dich selbst zum Guten tr\u00e4ge.", "tokens": ["Und", "machst", "dich", "selbst", "zum", "Gu\u00b7ten", "tr\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, sprichstu, mein Gewi\u00dfen bei\u00dft,", "tokens": ["Ja", ",", "sprichs\u00b7tu", ",", "mein", "Ge\u00b7wi\u00b7\u00dfen", "bei\u00dft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Indem es mir die Fehler weist,", "tokens": ["In\u00b7dem", "es", "mir", "die", "Feh\u00b7ler", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wodurch ich fast schon gar verdorben:", "tokens": ["Wo\u00b7durch", "ich", "fast", "schon", "gar", "ver\u00b7dor\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O halt es nur noch k\u00fcnftig rein,", "tokens": ["O", "halt", "es", "nur", "noch", "k\u00fcnf\u00b7tig", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Hofnung wird bald gr\u00fcner seyn,", "tokens": ["Die", "Hof\u00b7nung", "wird", "bald", "gr\u00fc\u00b7ner", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Du bist ja wohl noch nicht gestorben.", "tokens": ["Du", "bist", "ja", "wohl", "noch", "nicht", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Ernst kommt nimmermehr zu sp\u00e4t,", "tokens": ["Der", "Ernst", "kommt", "nim\u00b7mer\u00b7mehr", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bereute S\u00fcnde l\u00e4st sich h\u00fcten.", "tokens": ["Be\u00b7reu\u00b7te", "S\u00fcn\u00b7de", "l\u00e4st", "sich", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Misgunst, so dich jezo schm\u00e4ht,", "tokens": ["Die", "Mis\u00b7gunst", ",", "so", "dich", "je\u00b7zo", "schm\u00e4ht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df endlich in sich selber w\u00fcten,", "tokens": ["Mu\u00df", "end\u00b7lich", "in", "sich", "sel\u00b7ber", "w\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bald du nehmlich kl\u00fcger gehst", "tokens": ["So", "bald", "du", "nehm\u00b7lich", "kl\u00fc\u00b7ger", "gehst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit geschickter Art verstehst,", "tokens": ["Und", "mit", "ge\u00b7schick\u00b7ter", "Art", "ver\u00b7stehst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die alten Scharten auszuwezen.", "tokens": ["Die", "al\u00b7ten", "Schar\u00b7ten", "aus\u00b7zu\u00b7we\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dies kan nun anders nicht geschehn,", "tokens": ["Dies", "kan", "nun", "an\u00b7ders", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als derer Beyspiel anzusehn,", "tokens": ["Als", "de\u00b7rer", "Bey\u00b7spiel", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Kunst und Wei\u00dfheit h\u00f6her sezen.", "tokens": ["Die", "Kunst", "und", "Wei\u00df\u00b7heit", "h\u00f6\u00b7her", "se\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Schatten macht die Farben sch\u00f6n.", "tokens": ["Der", "Schat\u00b7ten", "macht", "die", "Far\u00b7ben", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wird der Fehler deiner Jugend", "tokens": ["So", "wird", "der", "Feh\u00b7ler", "dei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des reifen Alters Glanz erh\u00f6hn;", "tokens": ["Des", "rei\u00b7fen", "Al\u00b7ters", "Glanz", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bekleid es nur mit wahrer Tugend.", "tokens": ["Be\u00b7kleid", "es", "nur", "mit", "wah\u00b7rer", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Vorwurf hat bey Leuten Statt,", "tokens": ["Kein", "Vor\u00b7wurf", "hat", "bey", "Leu\u00b7ten", "Statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Straf und Zeit gebe\u00dfert hat.", "tokens": ["Die", "Straf", "und", "Zeit", "ge\u00b7be\u00b7\u00dfert", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verschiebe nur den Ernst nicht l\u00e4nger;", "tokens": ["Ver\u00b7schie\u00b7be", "nur", "den", "Ernst", "nicht", "l\u00e4n\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein guter Anfang k\u00fcrzt das Ziel,", "tokens": ["Ein", "gu\u00b7ter", "An\u00b7fang", "k\u00fcrzt", "das", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wer im Laufen s\u00e4umen will,", "tokens": ["Und", "wer", "im", "Lau\u00b7fen", "s\u00e4u\u00b7men", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dem macht die Furcht den Raum gedr\u00e4nger.", "tokens": ["Dem", "macht", "die", "Furcht", "den", "Raum", "ge\u00b7dr\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Erhize dich durch andrer Ruhm,", "tokens": ["Er\u00b7hi\u00b7ze", "dich", "durch", "an\u00b7drer", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Betrachte, sag ich, deines gleichen;", "tokens": ["Be\u00b7trach\u00b7te", ",", "sag", "ich", ",", "dei\u00b7nes", "glei\u00b7chen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie mancher Musen Heiligthum", "tokens": ["Wie", "man\u00b7cher", "Mu\u00b7sen", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00e4nzt schon von ihren Ehrenzeichen.", "tokens": ["Gl\u00e4nzt", "schon", "von", "ih\u00b7ren", "Eh\u00b7ren\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den ziert Astr\u00e4ens Sternenkranz,", "tokens": ["Den", "ziert", "A\u00b7str\u00e4\u00b7ens", "Ster\u00b7nen\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der heist der Canzel Licht und Glanz,", "tokens": ["Der", "heist", "der", "Can\u00b7zel", "Licht", "und", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den f\u00fchrt der Krancken Heil zum Gl\u00fccke,", "tokens": ["Den", "f\u00fchrt", "der", "Kran\u00b7cken", "Heil", "zum", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Drey zieren schon des F\u00fcrsten Stand,", "tokens": ["Drey", "zie\u00b7ren", "schon", "des", "F\u00fcrs\u00b7ten", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und zweyen schickt Bellonens Hand", "tokens": ["Und", "zwe\u00b7yen", "schickt", "Bel\u00b7lo\u00b7nens", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In Ost- und Westen holde Blicke.", "tokens": ["In", "Ost", "und", "Wes\u00b7ten", "hol\u00b7de", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dies sind jez