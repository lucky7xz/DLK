{"dta.poem.761": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlVII.  An die vmbstehenden Freunde.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr libsten/ die jhr mich das wohn-hau\u00df grimmer noht/ ", "tokens": ["Ihr", "libs\u00b7ten", "/", "die", "jhr", "mich", "das", "wohn\u00b7hau\u00df", "grim\u00b7mer", "noht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PRELS", "PPER", "PRF", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den abgezehrten Leib/ vnd die verdorten h\u00e4nde/ ", "tokens": ["Den", "ab\u00b7ge\u00b7zehr\u00b7ten", "Leib", "/", "vnd", "die", "ver\u00b7dor\u00b7ten", "h\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df sterbende gesicht/ vnd mein betr\u00fcbtes ende/ ", "tokens": ["Di\u00df", "ster\u00b7ben\u00b7de", "ge\u00b7sicht", "/", "vnd", "mein", "be\u00b7tr\u00fcb\u00b7tes", "en\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVPP", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit nassen Augen schawt; trawrt nicht vmb meinen Tod: ", "tokens": ["Mit", "nas\u00b7sen", "Au\u00b7gen", "schawt", ";", "trawrt", "nicht", "vmb", "mei\u00b7nen", "Tod", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da als der welt ade der Wehrte Bruder bott/ ", "tokens": ["Da", "als", "der", "welt", "a\u00b7de", "der", "Wehr\u00b7te", "Bru\u00b7der", "bott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "ART", "NN", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach dem ich jeden tag viel tausendt seuftzer sende/", "tokens": ["Nach", "dem", "ich", "je\u00b7den", "tag", "viel", "tau\u00b7sendt", "seuft\u00b7zer", "sen\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "ADV", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit dem die Schwester schied\u2019 als meiner libsten w\u00e4nde", "tokens": ["Mit", "dem", "die", "Schwes\u00b7ter", "schied'", "als", "mei\u00b7ner", "libs\u00b7ten", "w\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In flammen gingen ein/ vnd wurden grau\u00df vnd kot/", "tokens": ["In", "flam\u00b7men", "gin\u00b7gen", "ein", "/", "vnd", "wur\u00b7den", "grau\u00df", "vnd", "kot", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVFIN", "ART", "$(", "KON", "VAFIN", "ADJD", "KON", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da fiel mein leben hin. Mein fleisch der ", "tokens": ["Da", "fiel", "mein", "le\u00b7ben", "hin", ".", "Mein", "fleisch", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "VVFIN", "PTKVZ", "$.", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ligt nunmehr schon vnd fault in nicht nur einem grabe/", "tokens": ["Ligt", "nun\u00b7mehr", "schon", "vnd", "fault", "in", "nicht", "nur", "ei\u00b7nem", "gra\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KON", "VVFIN", "APPR", "PTKNEG", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dis was jhr f\u00fcr euch seht/ ist ein gespenst vnd dunst.", "tokens": ["Dis", "was", "jhr", "f\u00fcr", "euch", "seht", "/", "ist", "ein", "ge\u00b7spenst", "vnd", "dunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$(", "VAFIN", "ART", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Di\u00df Feber kompt zu sp\u00e4t. Der todt wird hier nichts finden/ ", "tokens": ["Di\u00df", "Fe\u00b7ber", "kompt", "zu", "sp\u00e4t", ".", "Der", "todt", "wird", "hier", "nichts", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PTKA", "ADJD", "$.", "ART", "ADJD", "VAFIN", "ADV", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was acht ich seinen pfeil/ ich mu\u00df ohn di\u00df verschwinden.", "tokens": ["Was", "acht", "ich", "sei\u00b7nen", "pfeil", "/", "ich", "mu\u00df", "ohn", "di\u00df", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "PPER", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "APPR", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzt nun mein tag anbricht. Habt danck f\u00fcr ewre gunst.", "tokens": ["Jtzt", "nun", "mein", "tag", "an\u00b7bricht", ".", "Habt", "danck", "f\u00fcr", "ew\u00b7re", "gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$.", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}}}}