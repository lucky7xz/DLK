{"dta.poem.13513": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die fernen Heimathh\u00f6hen,", "tokens": ["Die", "fer\u00b7nen", "Hei\u00b7math\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das stille hohe Haus,", "tokens": ["Das", "stil\u00b7le", "ho\u00b7he", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Berg, von dem ich gesehen", "tokens": ["Der", "Berg", ",", "von", "dem", "ich", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jeden Fr\u00fchling in's Land hinaus,", "tokens": ["Je\u00b7den", "Fr\u00fch\u00b7ling", "in's", "Land", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Mutter, Freunde und Br\u00fcder,", "tokens": ["Mut\u00b7ter", ",", "Freun\u00b7de", "und", "Br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "An die ich so oft gedacht,", "tokens": ["An", "die", "ich", "so", "oft", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Es gr\u00fc\u00dft mich alles wieder,", "tokens": ["Es", "gr\u00fc\u00dft", "mich", "al\u00b7les", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In stiller Mondesnacht.", "tokens": ["In", "stil\u00b7ler", "Mon\u00b7des\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}