{"dta.poem.20945": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ix .  \n K\u00f6nig Ludewigs des  XIII.  in  \n Franckreich.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.85", "fr:0.14"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Rochelle, Perpignan, mit Brisach und Tourin,", "tokens": ["Ro\u00b7chel\u00b7le", ",", "Per\u00b7pig\u00b7nan", ",", "mit", "Bri\u00b7sach", "und", "Tou\u00b7rin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Entdecken aller Welt wer ich gewesen bin.", "tokens": ["Ent\u00b7de\u00b7cken", "al\u00b7ler", "Welt", "wer", "ich", "ge\u00b7we\u00b7sen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PWS", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So hab ich albereit zu sterben angefangen.", "tokens": ["So", "hab", "ich", "al\u00b7be\u00b7reit", "zu", "ster\u00b7ben", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}