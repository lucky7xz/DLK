{"textgrid.poem.46722": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[eine Lerch' in der Luft]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Lerch' in der Luft", "tokens": ["Ei\u00b7ne", "Ler\u00b7ch'", "in", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt verborgen im Duft:", "tokens": ["Singt", "ver\u00b7bor\u00b7gen", "im", "Duft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ist der Fr\u00fchling gekommen?", "tokens": ["Ist", "der", "Fr\u00fch\u00b7ling", "ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Herz, warum so beklommen?", "tokens": ["Herz", ",", "wa\u00b7rum", "so", "be\u00b7klom\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Eine Lerch' in der Luft!", "tokens": ["Ei\u00b7ne", "Ler\u00b7ch'", "in", "der", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Gebirg in der Kluft", "tokens": ["Im", "Ge\u00b7birg", "in", "der", "Kluft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Liegt der Schnee noch in Massen,", "tokens": ["Liegt", "der", "Schnee", "noch", "in", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Will nicht schmelzen sich lassen.", "tokens": ["Will", "nicht", "schmel\u00b7zen", "sich", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVFIN", "PRF", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Eine Lerch' in der Luft!", "tokens": ["Ei\u00b7ne", "Ler\u00b7ch'", "in", "der", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom Gebirg aus der Schluft", "tokens": ["Vom", "Ge\u00b7birg", "aus", "der", "Schluft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "St\u00fcrmt des Nordwindes W\u00fcthen;", "tokens": ["St\u00fcrmt", "des", "Nord\u00b7win\u00b7des", "W\u00fct\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "O wie soll ich da br\u00fcten?", "tokens": ["O", "wie", "soll", "ich", "da", "br\u00fc\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Eine Lerch' in der Luft!", "tokens": ["Ei\u00b7ne", "Ler\u00b7ch'", "in", "der", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "All' die Erd' eine Gruft;", "tokens": ["All'", "die", "Erd'", "ei\u00b7ne", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gl\u00fccklich, wer sich mit Singen", "tokens": ["Gl\u00fcck\u00b7lich", ",", "wer", "sich", "mit", "Sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWS", "PRF", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Kann den Gr\u00fcften entschwingen.", "tokens": ["Kann", "den", "Gr\u00fcf\u00b7ten", "ent\u00b7schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Eine Lerch' in der Luft,", "tokens": ["Ei\u00b7ne", "Ler\u00b7ch'", "in", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist verschwommen im Duft,", "tokens": ["Ist", "ver\u00b7schwom\u00b7men", "im", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ist zum Himmel gestiegen;", "tokens": ["Ist", "zum", "Him\u00b7mel", "ge\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wer ihr nach k\u00f6nnte fliegen!", "tokens": ["Wer", "ihr", "nach", "k\u00f6nn\u00b7te", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}