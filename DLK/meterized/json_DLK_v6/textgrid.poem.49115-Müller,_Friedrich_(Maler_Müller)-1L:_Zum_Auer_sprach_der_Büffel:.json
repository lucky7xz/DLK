{"textgrid.poem.49115": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Auer sprach der B\u00fcffel:", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Auer sprach der B\u00fcffel:", "tokens": ["Zum", "Au\u00b7er", "sprach", "der", "B\u00fcf\u00b7fel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbsag'! Ist nicht Bruder Mastochs,", "tokens": ["\u00bb", "sag'", "!", "Ist", "nicht", "Bru\u00b7der", "Mas\u00b7tochs", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "VAFIN", "PTKNEG", "NN", "NE", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.3": {"text": "F\u00fcr den der Bauer t\u00e4glich", "tokens": ["F\u00fcr", "den", "der", "Bau\u00b7er", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit frischem Futter sorget,", "tokens": ["Mit", "fri\u00b7schem", "Fut\u00b7ter", "sor\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als gl\u00fccklich zu beneiden?\u00ab \u2013", "tokens": ["Als", "gl\u00fcck\u00b7lich", "zu", "be\u00b7nei\u00b7den", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "ADJD", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbo ja! so lang ihm 's Messer", "tokens": ["\u00bb", "o", "ja", "!", "so", "lang", "ihm", "'s", "Mes\u00b7ser"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "ADV", "$.", "ADV", "ADJD", "PPER", "PPER", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nicht in der Kehle sitzet!\u00ab", "tokens": ["Nicht", "in", "der", "Keh\u00b7le", "sit\u00b7zet", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}