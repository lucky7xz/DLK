{"textgrid.poem.64749": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "377. Hallers Jugends\u00fcnde", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Das nennt er, krank und schwach, die S\u00fcnden seiner Jugend.", "tokens": ["Das", "nennt", "er", ",", "krank", "und", "schwach", ",", "die", "S\u00fcn\u00b7den", "sei\u00b7ner", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun, manchen Vers von ihm, den ", "tokens": ["Nun", ",", "man\u00b7chen", "Vers", "von", "ihm", ",", "den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PIAT", "NN", "APPR", "PPER", "$,", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den lies't sein Sohn ", "tokens": ["Den", "lies't", "sein", "Sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "\u00bbmein sel'ger Vater schrieb das nicht!\u00ab", "tokens": ["\u00bb", "mein", "sel'\u00b7ger", "Va\u00b7ter", "schrieb", "das", "nicht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VVFIN", "PDS", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein solcher Sohn ist ", "tokens": ["Ein", "sol\u00b7cher", "Sohn", "ist"], "token_info