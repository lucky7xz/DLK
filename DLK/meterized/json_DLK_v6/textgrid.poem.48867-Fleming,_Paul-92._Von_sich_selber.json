{"textgrid.poem.48867": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "92. Von sich selber", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich feure ganz und brenne lichter Loh.", "tokens": ["Ich", "feu\u00b7re", "ganz", "und", "bren\u00b7ne", "lich\u00b7ter", "Loh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Tr\u00e4nen hier sind meiner Flammen Ammen,", "tokens": ["Die", "Tr\u00e4\u00b7nen", "hier", "sind", "mei\u00b7ner", "Flam\u00b7men", "Am\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die mich nicht l\u00e4\u00dft di\u00df stete Leid vertammen.", "tokens": ["die", "mich", "nicht", "l\u00e4\u00dft", "di\u00df", "ste\u00b7te", "Leid", "ver\u00b7tam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "PDS", "VVFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich kenn' es wol, was mich kan machen froh,", "tokens": ["Ich", "kenn'", "es", "wol", ",", "was", "mich", "kan", "ma\u00b7chen", "froh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VMFIN", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "da\u00df ich fortan nicht d\u00fcrfte weinen so.", "tokens": ["da\u00df", "ich", "for\u00b7tan", "nicht", "d\u00fcrf\u00b7te", "wei\u00b7nen", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo aber ists? So m\u00fcssen nun die Flammen", "tokens": ["Wo", "a\u00b7ber", "ists", "?", "So", "m\u00fcs\u00b7sen", "nun", "die", "Flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "$.", "ADV", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "hier \u00fcber mir nur schlagen frei zusammen.", "tokens": ["hier", "\u00fc\u00b7ber", "mir", "nur", "schla\u00b7gen", "frei", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Schirm ist weg, mein Schutz ist anderswo.", "tokens": ["Mein", "Schirm", "ist", "weg", ",", "mein", "Schutz", "ist", "an\u00b7ders\u00b7wo", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ist ganz Nichts da, daran ich mich mag k\u00fchlen", "tokens": ["Ist", "ganz", "Nichts", "da", ",", "da\u00b7ran", "ich", "mich", "mag", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIS", "ADV", "$,", "PAV", "PPER", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in solcher Glut, die meine Geister f\u00fchlen?", "tokens": ["in", "sol\u00b7cher", "Glut", ",", "die", "mei\u00b7ne", "Geis\u00b7ter", "f\u00fch\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Liebesdurst verzehrt mir Mark und Bein.", "tokens": ["Der", "Lie\u00b7bes\u00b7durst", "ver\u00b7zehrt", "mir", "Mark", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Di\u00df Wa\u00dfer ists, die K\u00fchlung meiner Hitze,", "tokens": ["Di\u00df", "Wa\u00b7\u00dfer", "ists", ",", "die", "K\u00fch\u00b7lung", "mei\u00b7ner", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das ich zum Trunk' aus beiden Augen schwitze.", "tokens": ["das", "ich", "zum", "Trunk'", "aus", "bei\u00b7den", "Au\u00b7gen", "schwit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPRART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich zapfe selbst und Amor schenkt mir ein.", "tokens": ["Ich", "zap\u00b7fe", "selbst", "und", "A\u00b7mor", "schenkt", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}