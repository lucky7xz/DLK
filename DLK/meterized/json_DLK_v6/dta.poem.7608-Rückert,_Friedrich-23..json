{"dta.poem.7608": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Edler auf Besuch kam zu dem Thor des Andern,", "tokens": ["Ein", "Ed\u00b7ler", "auf", "Be\u00b7such", "kam", "zu", "dem", "Thor", "des", "An\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zur Anmeldung lie\u00df hinein dis Blatt er wandern:", "tokens": ["Und", "zur", "An\u00b7mel\u00b7dung", "lie\u00df", "hin\u00b7ein", "dis", "Blatt", "er", "wan\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADV", "PDS", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ist Eintritt mir gew\u00e4hrt? Ich komme wie die Feder,", "tokens": ["Ist", "Ein\u00b7tritt", "mir", "ge\u00b7w\u00e4hrt", "?", "Ich", "kom\u00b7me", "wie", "die", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVPP", "$.", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die herf\u00fchrt jeder Wind, und weiter f\u00fchret jeder.", "tokens": ["Die", "her\u00b7f\u00fchrt", "je\u00b7der", "Wind", ",", "und", "wei\u00b7ter", "f\u00fch\u00b7ret", "je\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,", "KON", "ADV", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und jener schrieb heraus: Zum Eintritt nicht gereicht", "tokens": ["Und", "je\u00b7ner", "schrieb", "he\u00b7raus", ":", "Zum", "Ein\u00b7tritt", "nicht", "ge\u00b7reicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PTKVZ", "$.", "APPRART", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird die Erlaubnis dir, du machtest dich zu leicht.", "tokens": ["Wird", "die", "Er\u00b7laub\u00b7nis", "dir", ",", "du", "mach\u00b7test", "dich", "zu", "leicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und jener schrieb hinein: Ich komme wie der Stein,", "tokens": ["Und", "je\u00b7ner", "schrieb", "hin\u00b7ein", ":", "Ich", "kom\u00b7me", "wie", "der", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo der ins Wasser f\u00e4llt, da bleibt er liegen fein.", "tokens": ["Wo", "der", "ins", "Was\u00b7ser", "f\u00e4llt", ",", "da", "bleibt", "er", "lie\u00b7gen", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPRART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und jener schrieb hinaus: Erlaubnis nimmermehr", "tokens": ["Und", "je\u00b7ner", "schrieb", "hin\u00b7aus", ":", "Er\u00b7laub\u00b7nis", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PTKVZ", "$.", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird dir zum Eintritt hier, du machtest dich zu schwer.", "tokens": ["Wird", "dir", "zum", "Ein\u00b7tritt", "hier", ",", "du", "mach\u00b7test", "dich", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und jener schrieb hinein: Ich komme wie der Reiter,", "tokens": ["Und", "je\u00b7ner", "schrieb", "hin\u00b7ein", ":", "Ich", "kom\u00b7me", "wie", "der", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, wo er sein Gesch\u00e4ft gethan hat, reitet weiter.", "tokens": ["Der", ",", "wo", "er", "sein", "Ge\u00b7sch\u00e4ft", "ge\u00b7than", "hat", ",", "rei\u00b7tet", "wei\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVPP", "VAFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und jener rief hinaus: Das ist ein wahres Wort;", "tokens": ["Und", "je\u00b7ner", "rief", "hin\u00b7aus", ":", "Das", "ist", "ein", "wah\u00b7res", "Wort", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Th\u00fcrh\u00fcter, la\u00df mir ein den edlen Gast sofort!", "tokens": ["Th\u00fcr\u00b7h\u00fc\u00b7ter", ",", "la\u00df", "mir", "ein", "den", "ed\u00b7len", "Gast", "so\u00b7fort", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ART", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}