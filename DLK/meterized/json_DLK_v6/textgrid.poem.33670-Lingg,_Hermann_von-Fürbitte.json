{"textgrid.poem.33670": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcrbitte", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedenke, da\u00df du Schuldner bist", "tokens": ["Ge\u00b7den\u00b7ke", ",", "da\u00df", "du", "Schuld\u00b7ner", "bist"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Armen, die nichts haben,", "tokens": ["Der", "Ar\u00b7men", ",", "die", "nichts", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und deren Recht gleich deinem ist", "tokens": ["Und", "de\u00b7ren", "Recht", "gleich", "dei\u00b7nem", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRELAT", "NN", "ADV", "PPOSAT", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An allen Erdengaben.", "tokens": ["An", "al\u00b7len", "Er\u00b7den\u00b7ga\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn jemals noch zu dir des Lebens", "tokens": ["Wenn", "je\u00b7mals", "noch", "zu", "dir", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gesegnet goldne Str\u00f6me gehn,", "tokens": ["Ge\u00b7seg\u00b7net", "gold\u00b7ne", "Str\u00f6\u00b7me", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df nicht auf deinen Tisch vergebens", "tokens": ["La\u00df", "nicht", "auf", "dei\u00b7nen", "Tisch", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Hungrigen durchs Fenster sehn,", "tokens": ["Den", "Hung\u00b7ri\u00b7gen", "durchs", "Fens\u00b7ter", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Verscheuche nicht die wilde Taube,", "tokens": ["Ver\u00b7scheu\u00b7che", "nicht", "die", "wil\u00b7de", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "La\u00df hinter dir noch \u00c4hren stehn", "tokens": ["La\u00df", "hin\u00b7ter", "dir", "noch", "\u00c4h\u00b7ren", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPER", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und nimm dem Weinstock nicht die letzte Traube!", "tokens": ["Und", "nimm", "dem", "Wein\u00b7stock", "nicht", "die", "letz\u00b7te", "Trau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}