{"textgrid.poem.40706": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "[maienwonne, Maienbl\u00fcthe]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maienwonne, Maienbl\u00fcthe,", "tokens": ["Mai\u00b7en\u00b7won\u00b7ne", ",", "Mai\u00b7en\u00b7bl\u00fct\u00b7he", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Fluren, im Gem\u00fcthe,", "tokens": ["Auf", "den", "Flu\u00b7ren", ",", "im", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, so bald, so schnell vorbei!", "tokens": ["Ach", ",", "so", "bald", ",", "so", "schnell", "vor\u00b7bei", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "$,", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch auch das ist Maiengabe,", "tokens": ["Doch", "auch", "das", "ist", "Mai\u00b7en\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "VAFIN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ging der eigne Lenz zu Grabe,", "tokens": ["Ging", "der", "eig\u00b7ne", "Lenz", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freudig segnen fremden Mai.", "tokens": ["Freu\u00b7dig", "seg\u00b7nen", "frem\u00b7den", "Mai", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}