{"textgrid.poem.33601": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "An meine pompejanische Lampe", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werd' ich von dir mich m\u00fcssen scheiden,", "tokens": ["Werd'", "ich", "von", "dir", "mich", "m\u00fcs\u00b7sen", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trauliche Leuchte, holdes Licht?", "tokens": ["Trau\u00b7li\u00b7che", "Leuch\u00b7te", ",", "hol\u00b7des", "Licht", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wie mild dein Glanz in meine Leiden", "tokens": ["Wie", "mild", "dein", "Glanz", "in", "mei\u00b7ne", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vers\u00f6hnung bringt und ruhig spricht:", "tokens": ["Ver\u00b7s\u00f6h\u00b7nung", "bringt", "und", "ru\u00b7hig", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verzage nicht!", "tokens": ["Ver\u00b7za\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich will mit frischem \u00d6l dich netzen:", "tokens": ["Ich", "will", "mit", "fri\u00b7schem", "\u00d6l", "dich", "net\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es quillt ein Schlummer aus dem Mohn.", "tokens": ["Es", "quillt", "ein", "Schlum\u00b7mer", "aus", "dem", "Mohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was k\u00f6nnte mir dein Licht ersetzen?", "tokens": ["Was", "k\u00f6nn\u00b7te", "mir", "dein", "Licht", "er\u00b7set\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es leuchtet mir zum Helikon", "tokens": ["Es", "leuch\u00b7tet", "mir", "zum", "He\u00b7li\u00b7kon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus dunklem Thon.", "tokens": ["Aus", "dunk\u00b7lem", "Thon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wenn heim der Wandrer vom Vesuve", "tokens": ["Wenn", "heim", "der", "Wand\u00b7rer", "vom", "Ve\u00b7su\u00b7ve"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dich Totenlampe mitgebracht,", "tokens": ["Dich", "To\u00b7ten\u00b7lam\u00b7pe", "mit\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So war's zum freundlichen Berufe,", "tokens": ["So", "wa\u00b7r's", "zum", "freund\u00b7li\u00b7chen", "Be\u00b7ru\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df du ihm leuchtest neuentfacht", "tokens": ["Da\u00df", "du", "ihm", "leuch\u00b7test", "neu\u00b7ent\u00b7facht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In stiller Nacht.", "tokens": ["In", "stil\u00b7ler", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Gedenkst du auch noch deines Hauses?", "tokens": ["Ge\u00b7denkst", "du", "auch", "noch", "dei\u00b7nes", "Hau\u00b7ses", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus einer Marmorlarve sprang", "tokens": ["Aus", "ei\u00b7ner", "Mar\u00b7mor\u00b7lar\u00b7ve", "sprang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Brunnen fr\u00f6hlichen Gebrauses", "tokens": ["Ein", "Brun\u00b7nen", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7brau\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und rauschte sch\u00f6ne N\u00e4chte lang", "tokens": ["Und", "rauschte", "sch\u00f6\u00b7ne", "N\u00e4ch\u00b7te", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Im S\u00e4ulengang.", "tokens": ["Im", "S\u00e4u\u00b7len\u00b7gang", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Erinnerst du dich noch des Alten", "tokens": ["E\u00b7rin\u00b7nerst", "du", "dich", "noch", "des", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Rollen in dem Schlafgemach,", "tokens": ["Vor", "Rol\u00b7len", "in", "dem", "Schlaf\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sorglich dich emporgehalten,", "tokens": ["Der", "sorg\u00b7lich", "dich", "em\u00b7por\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Siegel auf dem Brief erbrach", "tokens": ["Die", "Sie\u00b7gel", "auf", "dem", "Brief", "er\u00b7brach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Griechisch sprach?", "tokens": ["Und", "Grie\u00b7chisch", "sprach", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Bei Schatten, Freundin meiner Mu\u00dfe,", "tokens": ["Bei", "Schat\u00b7ten", ",", "Freun\u00b7din", "mei\u00b7ner", "Mu\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verschliefst du ein Jahrtausend, taub", "tokens": ["Ver\u00b7schliefst", "du", "ein", "Jahr\u00b7tau\u00b7send", ",", "taub"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Licht und seinem holden Gru\u00dfe,", "tokens": ["Dem", "Licht", "und", "sei\u00b7nem", "hol\u00b7den", "Gru\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Grabmal bei der Flammen Raub,", "tokens": ["Im", "Grab\u00b7mal", "bei", "der", "Flam\u00b7men", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Schutt und Staub.", "tokens": ["In", "Schutt", "und", "Staub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Nun horchst du wieder Menschentr\u00e4umen,", "tokens": ["Nun", "horchst", "du", "wie\u00b7der", "Men\u00b7schen\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nachtluft stillem Atemzug.", "tokens": ["Der", "Nacht\u00b7luft", "stil\u00b7lem", "A\u00b7tem\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kommt zu dir aus Bl\u00fcteb\u00e4umen", "tokens": ["Es", "kommt", "zu", "dir", "aus", "Bl\u00fc\u00b7te\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Motte, die zu dir im Flug", "tokens": ["Die", "Mot\u00b7te", ",", "die", "zu", "dir", "im", "Flug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Begierde trug.", "tokens": ["Be\u00b7gier\u00b7de", "trug", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Doch ach, anstatt zu fernen Liedern,", "tokens": ["Doch", "ach", ",", "an\u00b7statt", "zu", "fer\u00b7nen", "Lie\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "KOUI", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Scheinst du vielleicht bald meiner Gruft;", "tokens": ["Scheinst", "du", "viel\u00b7leicht", "bald", "mei\u00b7ner", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den kalten Gru\u00df mu\u00dft du erwidern", "tokens": ["Den", "kal\u00b7ten", "Gru\u00df", "mu\u00dft", "du", "er\u00b7wi\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Leichenkerze, statt dem Duft", "tokens": ["Der", "Lei\u00b7chen\u00b7ker\u00b7ze", ",", "statt", "dem", "Duft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Fr\u00fchlingsluft.", "tokens": ["Der", "Fr\u00fch\u00b7lings\u00b7luft", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Die Seele, der dein Licht jetzt funkelt,", "tokens": ["Die", "See\u00b7le", ",", "der", "dein", "Licht", "jetzt", "fun\u00b7kelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tauscht, kleine Leuchte, dann mit dir", "tokens": ["Tauscht", ",", "klei\u00b7ne", "Leuch\u00b7te", ",", "dann", "mit", "dir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ADV", "APPR", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und wandelt unten, tief umdunkelt,", "tokens": ["Und", "wan\u00b7delt", "un\u00b7ten", ",", "tief", "um\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Indes du oben leuchtest hier", "tokens": ["In\u00b7des", "du", "o\u00b7ben", "leuch\u00b7test", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und zeugst von ihr.", "tokens": ["Und", "zeugst", "von", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Kommt dann ein Schmetterling geflogen,", "tokens": ["Kommt", "dann", "ein", "Schmet\u00b7ter\u00b7ling", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fragst du, wo ist der Freund denn jetzt,", "tokens": ["Fragst", "du", ",", "wo", "ist", "der", "Freund", "denn", "jetzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "VAFIN", "ART", "NN", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dem ich oft Gespr\u00e4ch gepflogen,", "tokens": ["Mit", "dem", "ich", "oft", "Ge\u00b7spr\u00e4ch", "ge\u00b7pflo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sp\u00e4t sich noch zu mir gesetzt", "tokens": ["Der", "sp\u00e4t", "sich", "noch", "zu", "mir", "ge\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PRF", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mich genetzt?", "tokens": ["Und", "mich", "ge\u00b7netzt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Nein, wache nur ob einem Schlummer,", "tokens": ["Nein", ",", "wa\u00b7che", "nur", "ob", "ei\u00b7nem", "Schlum\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "ADV", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tagesm\u00fchen unterbricht!", "tokens": ["Der", "Ta\u00b7ges\u00b7m\u00fc\u00b7hen", "un\u00b7ter\u00b7bricht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Traum versinke Gram und Kummer \u2013", "tokens": ["In", "Traum", "ver\u00b7sin\u00b7ke", "Gram", "und", "Kum\u00b7mer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du traute Leuchte, holdes Licht,", "tokens": ["Du", "trau\u00b7te", "Leuch\u00b7te", ",", "hol\u00b7des", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erlisch noch nicht!", "tokens": ["Er\u00b7lisch", "noch", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}