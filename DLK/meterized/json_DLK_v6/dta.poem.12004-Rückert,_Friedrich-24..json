{"dta.poem.12004": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.71", "nl:0.14", "pl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O sage wo du bist, wo du nicht bist o sage!", "tokens": ["O", "sa\u00b7ge", "wo", "du", "bist", ",", "wo", "du", "nicht", "bist", "o", "sa\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PWAV", "PPER", "VAFIN", "$,", "PWAV", "PPER", "PTKNEG", "VAFIN", "FM", "FM", "$."], "meter": "+--+-+---+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du \u00fcberall in Nacht, und \u00fcberall zu Tage.", "tokens": ["Du", "\u00fc\u00b7be\u00b7rall", "in", "Nacht", ",", "und", "\u00fc\u00b7be\u00b7rall", "zu", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "$,", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Wahrheit du allein, und alles andre Schein,", "tokens": ["Die", "Wahr\u00b7heit", "du", "al\u00b7lein", ",", "und", "al\u00b7les", "and\u00b7re", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "$,", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aller Schein was k\u00f6nnt' er au\u00dfer Wahrheit seyn?", "tokens": ["Und", "al\u00b7ler", "Schein", "was", "k\u00f6nnt'", "er", "au\u00b7\u00dfer", "Wahr\u00b7heit", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PWS", "VMFIN", "PPER", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die liebend suchen dich, sind nicht zu dir gekommen;", "tokens": ["Die", "lie\u00b7bend", "su\u00b7chen", "dich", ",", "sind", "nicht", "zu", "dir", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPER", "$,", "VAFIN", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die dich fliehen, sind nicht deiner Lieb' entnommen.", "tokens": ["Und", "die", "dich", "flie\u00b7hen", ",", "sind", "nicht", "dei\u00b7ner", "Lieb'", "ent\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVINF", "$,", "VAFIN", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die fern sich f\u00fchlen dir, sind drum dir nicht entrissen;", "tokens": ["Die", "fern", "sich", "f\u00fch\u00b7len", "dir", ",", "sind", "drum", "dir", "nicht", "ent\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVFIN", "PPER", "$,", "VAFIN", "PAV", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch selig sind allein, die sich dir nahe wissen.", "tokens": ["Doch", "se\u00b7lig", "sind", "al\u00b7lein", ",", "die", "sich", "dir", "na\u00b7he", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "$,", "PRELS", "PRF", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}