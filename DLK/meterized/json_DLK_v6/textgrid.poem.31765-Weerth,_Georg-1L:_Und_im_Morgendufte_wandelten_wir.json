{"textgrid.poem.31765": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und im Morgendufte wandelten wir", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und im Morgendufte wandelten wir", "tokens": ["Und", "im", "Mor\u00b7gen\u00b7duf\u00b7te", "wan\u00b7del\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Durch die herbstlichen Rebenlauben,", "tokens": ["Durch", "die", "herbst\u00b7li\u00b7chen", "Re\u00b7ben\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Es rauschte m\u00e4rchenmurmelnd der Rhein \u2013", "tokens": ["Es", "rauschte", "m\u00e4r\u00b7chen\u00b7mur\u00b7melnd", "der", "Rhein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NE", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Rubinfarben gl\u00fchten die Trauben.", "tokens": ["Ru\u00b7bin\u00b7far\u00b7ben", "gl\u00fch\u00b7ten", "die", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbin K\u00f6ln war wirklich ein arger Skandal \u2013\u00ab,", "tokens": ["\u00bb", "in", "K\u00f6ln", "war", "wirk\u00b7lich", "ein", "ar\u00b7ger", "Skan\u00b7dal", "\u2013", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "APPR", "NE", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$(", "$(", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Begann ich zum alten Herrn Soherr,", "tokens": ["Be\u00b7gann", "ich", "zum", "al\u00b7ten", "Herrn", "So\u00b7herr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbbarrikaden kamen in Masse, man wu\u00dft", "tokens": ["\u00bb", "bar\u00b7ri\u00b7ka\u00b7den", "ka\u00b7men", "in", "Mas\u00b7se", ",", "man", "wu\u00dft"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "VVFIN", "APPR", "NN", "$,", "PIS", "VVFIN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bei Gott nicht, von wannen und woher.", "tokens": ["Bei", "Gott", "nicht", ",", "von", "wan\u00b7nen", "und", "wo\u00b7her", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "$,", "APPR", "ADJA", "KON", "PWAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sie wurden im Nu emporgebaut", "tokens": ["Sie", "wur\u00b7den", "im", "Nu", "em\u00b7por\u00b7ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADV", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von H\u00e4nden, energischen, raschen,", "tokens": ["Von", "H\u00e4n\u00b7den", ",", "e\u00b7ner\u00b7gi\u00b7schen", ",", "ra\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Dombausteinen und Kirchenst\u00fchln", "tokens": ["Aus", "Dom\u00b7bau\u00b7stei\u00b7nen", "und", "Kir\u00b7chen\u00b7st\u00fchln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und aus ausgetrunkenen Flaschen.", "tokens": ["Und", "aus", "aus\u00b7ge\u00b7trun\u00b7ke\u00b7nen", "Fla\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Es wehte die Fahne der Republik,", "tokens": ["Es", "weh\u00b7te", "die", "Fah\u00b7ne", "der", "Re\u00b7pub\u00b7lik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und ein Tag war's, ein f\u00fcrchterlicher.", "tokens": ["Und", "ein", "Tag", "wa\u00b7r's", ",", "ein", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Steckbrieflich werden die H\u00e4upter verfolgt,", "tokens": ["Steck\u00b7brief\u00b7lich", "wer\u00b7den", "die", "H\u00e4up\u00b7ter", "ver\u00b7folgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kein ehrlicher Mann ist mehr sicher.", "tokens": ["Kein", "ehr\u00b7li\u00b7cher", "Mann", "ist", "mehr", "si\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Die Insurgenten wurden verjagt,", "tokens": ["Die", "In\u00b7sur\u00b7gen\u00b7ten", "wur\u00b7den", "ver\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und proklamiert ward eilig das Standrecht;", "tokens": ["Und", "pro\u00b7kla\u00b7miert", "ward", "ei\u00b7lig", "das", "Stand\u00b7recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das ist wahrhaftig noch schlimmer als", "tokens": ["Das", "ist", "wahr\u00b7haf\u00b7tig", "noch", "schlim\u00b7mer", "als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "ADV", "ADJD", "KOKOM"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das alte preu\u00dfische Landrecht.", "tokens": ["Das", "al\u00b7te", "preu\u00b7\u00dfi\u00b7sche", "Land\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Herr Engels, der ist Diktator von K\u00f6ln,", "tokens": ["Herr", "En\u00b7gels", ",", "der", "ist", "Dik\u00b7ta\u00b7tor", "von", "K\u00f6ln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "VAFIN", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bis wieder die Sonne der Ruh scheint;", "tokens": ["Bis", "wie\u00b7der", "die", "Son\u00b7ne", "der", "Ruh", "scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Stadtkommandant, Herr Engels, der hat", "tokens": ["Der", "Stadt\u00b7kom\u00b7man\u00b7dant", ",", "Herr", "En\u00b7gels", ",", "der", "hat"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "NE", "$,", "PRELS", "VAFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die B\u00fcrgerwehr K\u00f6lns entkuhbeint.", "tokens": ["Die", "B\u00fcr\u00b7ger\u00b7wehr", "K\u00f6lns", "ent\u00b7kuh\u00b7beint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Geschl\u00f6ffelt wird, wer sich r\u00fchrt oder regt,", "tokens": ["Ge\u00b7schl\u00f6f\u00b7felt", "wird", ",", "wer", "sich", "r\u00fchrt", "o\u00b7der", "regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PWS", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gehangen, gebraten, gesotten \u2013", "tokens": ["Ge\u00b7han\u00b7gen", ",", "ge\u00b7bra\u00b7ten", ",", "ge\u00b7sot\u00b7ten", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wohl werd ich mich h\u00fcten, Herrn Engels mit", "tokens": ["Wohl", "werd", "ich", "mich", "h\u00fc\u00b7ten", ",", "Herrn", "En\u00b7gels", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,", "NN", "NE", "APPR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Meinem L\u00e4stermund zu verspotten.", "tokens": ["Mei\u00b7nem", "L\u00e4s\u00b7ter\u00b7mund", "zu", "ver\u00b7spot\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Er w\u00fcrde mich packen mit schrecklicher Faust", "tokens": ["Er", "w\u00fcr\u00b7de", "mich", "pa\u00b7cken", "mit", "schreck\u00b7li\u00b7cher", "Faust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und w\u00fcrde zu Tode mich schie\u00dfen", "tokens": ["Und", "w\u00fcr\u00b7de", "zu", "To\u00b7de", "mich", "schie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mit seinem groben Gesch\u00fctze, und ach,", "tokens": ["Mit", "sei\u00b7nem", "gro\u00b7ben", "Ge\u00b7sch\u00fct\u00b7ze", ",", "und", "ach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "XY", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das w\u00fcrde mich sehr verdrie\u00dfen.", "tokens": ["Das", "w\u00fcr\u00b7de", "mich", "sehr", "ver\u00b7drie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Wie einen Krammsvogel w\u00fcrde er mich", "tokens": ["Wie", "ei\u00b7nen", "Kramms\u00b7vo\u00b7gel", "w\u00fcr\u00b7de", "er", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PPER", "PRF"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An den grauen Domkranen hangen \u2013", "tokens": ["An", "den", "grau\u00b7en", "Dom\u00b7kra\u00b7nen", "han\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.3": {"text": "Doch die Krammsv\u00f6gel lassen am besten sich", "tokens": ["Doch", "die", "Kramms\u00b7v\u00f6\u00b7gel", "las\u00b7sen", "am", "bes\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "PTKA", "ADJD", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In den Nebeln des Herbstes fangen.", "tokens": ["In", "den", "Ne\u00b7beln", "des", "Herbs\u00b7tes", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Und Krammsv\u00f6gel schmecken vortrefflich gut", "tokens": ["Und", "Kramms\u00b7v\u00f6\u00b7gel", "schme\u00b7cken", "vor\u00b7treff\u00b7lich", "gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADJD", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit butterger\u00f6stetem Brote \u2013", "tokens": ["Mit", "but\u00b7ter\u00b7ge\u00b7r\u00f6s\u00b7te\u00b7tem", "Bro\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "O himmlischer Vater, la\u00df manche mich", "tokens": ["O", "himm\u00b7li\u00b7scher", "Va\u00b7ter", ",", "la\u00df", "man\u00b7che", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VVIMP", "PIS", "PRF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Noch essen vor meinem Tode!\u00ab", "tokens": ["Noch", "es\u00b7sen", "vor", "mei\u00b7nem", "To\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}