{"textgrid.poem.58830": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "49. Auf Gott merken", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die wahre Andacht stehet nicht", "tokens": ["Die", "wah\u00b7re", "An\u00b7dacht", "ste\u00b7het", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsrer Macht und eignem Willen;", "tokens": ["In", "uns\u00b7rer", "Macht", "und", "eig\u00b7nem", "Wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bet nur und merke, wann's geschicht,", "tokens": ["Bet", "nur", "und", "mer\u00b7ke", ",", "wann's", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Gott dich r\u00fchrt und selbst will stillen!", "tokens": ["Da\u00df", "Gott", "dich", "r\u00fchrt", "und", "selbst", "will", "stil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "KON", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}