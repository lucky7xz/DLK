{"textgrid.poem.38842": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebe denkt in s\u00fcssen T\u00f6nen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe denkt in s\u00fcssen T\u00f6nen,", "tokens": ["Lie\u00b7be", "denkt", "in", "s\u00fcs\u00b7sen", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn Gedanken stehn zu fern,", "tokens": ["Denn", "Ge\u00b7dan\u00b7ken", "stehn", "zu", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur in T\u00f6nen mag sie gern", "tokens": ["Nur", "in", "T\u00f6\u00b7nen", "mag", "sie", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles, was sie will, versch\u00f6nen.", "tokens": ["Al\u00b7les", ",", "was", "sie", "will", ",", "ver\u00b7sch\u00f6\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "VMFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn im tiefen Schmerz verloren", "tokens": ["Wenn", "im", "tie\u00b7fen", "Schmerz", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Geister in mir klagen,", "tokens": ["Al\u00b7le", "Geis\u00b7ter", "in", "mir", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ger\u00fchrt die Freunde fragen:", "tokens": ["Und", "ge\u00b7r\u00fchrt", "die", "Freun\u00b7de", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbwelch ein Leid ist dir geboren?\u00ab", "tokens": ["\u00bb", "welch", "ein", "Leid", "ist", "dir", "ge\u00b7bo\u00b7ren", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAT", "ART", "NN", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann ich keine Antwort sagen,", "tokens": ["Kann", "ich", "kei\u00b7ne", "Ant\u00b7wort", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob sich Freuden wollen finden,", "tokens": ["Ob", "sich", "Freu\u00b7den", "wol\u00b7len", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Leiden in mein Herz gew\u00f6hnen,", "tokens": ["Lei\u00b7den", "in", "mein", "Herz", "ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Geister, die sich liebend binden", "tokens": ["Geis\u00b7ter", ",", "die", "sich", "lie\u00b7bend", "bin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kann kein Wort niemals verk\u00fcnden,", "tokens": ["Kann", "kein", "Wort", "nie\u00b7mals", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Liebe denkt in s\u00fcssen T\u00f6nen.", "tokens": ["Lie\u00b7be", "denkt", "in", "s\u00fcs\u00b7sen", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Warum hat Gesangess\u00fcsse", "tokens": ["Wa\u00b7rum", "hat", "Ge\u00b7san\u00b7ges\u00b7s\u00fcs\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer sich von mir geschieden?", "tokens": ["Im\u00b7mer", "sich", "von", "mir", "ge\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Zornig hat sie mich vermieden,", "tokens": ["Zor\u00b7nig", "hat", "sie", "mich", "ver\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ich auch die Holde gr\u00fc\u00dfe.", "tokens": ["Wie", "ich", "auch", "die", "Hol\u00b7de", "gr\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So geschieht es, da\u00df ich b\u00fc\u00dfe,", "tokens": ["So", "ge\u00b7schieht", "es", ",", "da\u00df", "ich", "b\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schweigen ist mir vorgeschrieben,", "tokens": ["Schwei\u00b7gen", "ist", "mir", "vor\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich sagte doch so gern", "tokens": ["Und", "ich", "sag\u00b7te", "doch", "so", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was dem Herzen sei sein Lieben,", "tokens": ["Was", "dem", "Her\u00b7zen", "sei", "sein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber stumm bin ich geblieben,", "tokens": ["A\u00b7ber", "stumm", "bin", "ich", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Denn Gedanken stehn zu fern.", "tokens": ["Denn", "Ge\u00b7dan\u00b7ken", "stehn", "zu", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, wo kann ich doch ein Zeichen,", "tokens": ["Ach", ",", "wo", "kann", "ich", "doch", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Liebe ew'ges Leben", "tokens": ["Mei\u00b7ner", "Lie\u00b7be", "ew'\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir nur selber kund zu geben,", "tokens": ["Mir", "nur", "sel\u00b7ber", "kund", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Lebenswort erreichen?", "tokens": ["Wie", "ein", "Le\u00b7bens\u00b7wort", "er\u00b7rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn dann alles will entweichen", "tokens": ["Wenn", "dann", "al\u00b7les", "will", "ent\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIS", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df ich oft in Trauer w\u00e4hnen", "tokens": ["Mu\u00df", "ich", "oft", "in", "Trau\u00b7er", "w\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe sei dem Herzen fern,", "tokens": ["Lie\u00b7be", "sei", "dem", "Her\u00b7zen", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dann weckt sie das tiefste Sehnen,", "tokens": ["Dann", "weckt", "sie", "das", "tiefs\u00b7te", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Sprechen mag sie nur in Thr\u00e4nen,", "tokens": ["Spre\u00b7chen", "mag", "sie", "nur", "in", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nur in T\u00f6nen mag sie gern.", "tokens": ["Nur", "in", "T\u00f6\u00b7nen", "mag", "sie", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Will die Liebe in mir weinen,", "tokens": ["Will", "die", "Lie\u00b7be", "in", "mir", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt sie Jammer, bringt sie Wonne,", "tokens": ["Bringt", "sie", "Jam\u00b7mer", ",", "bringt", "sie", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will sie Nacht sein, oder Sonne,", "tokens": ["Will", "sie", "Nacht", "sein", ",", "o\u00b7der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VAINF", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollen Gl\u00fcckessterne scheinen?", "tokens": ["Sol\u00b7len", "Gl\u00fc\u00b7ckess\u00b7ter\u00b7ne", "schei\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Tausend Wunder sich vereinen:", "tokens": ["Tau\u00b7send", "Wun\u00b7der", "sich", "ver\u00b7ei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr Gedanken schweiget stille,", "tokens": ["Ihr", "Ge\u00b7dan\u00b7ken", "schwei\u00b7get", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn die Liebe will mich kr\u00f6nen,", "tokens": ["Denn", "die", "Lie\u00b7be", "will", "mich", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und was sich an mir erf\u00fclle", "tokens": ["Und", "was", "sich", "an", "mir", "er\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wei\u00df ich das, es wird ihr Wille", "tokens": ["Wei\u00df", "ich", "das", ",", "es", "wird", "ihr", "Wil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Alles, was sie will, versch\u00f6nen.", "tokens": ["Al\u00b7les", ",", "was", "sie", "will", ",", "ver\u00b7sch\u00f6\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "VMFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}