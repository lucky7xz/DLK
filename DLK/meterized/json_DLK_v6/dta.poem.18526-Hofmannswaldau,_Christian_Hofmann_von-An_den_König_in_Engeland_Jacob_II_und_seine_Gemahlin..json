{"dta.poem.18526": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den K\u00f6nig in Engeland Jacob  \n  II  und seine Gemahlin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Euch beyden r\u00e4umete GOtt Edens garten ein/", "tokens": ["Euch", "bey\u00b7den", "r\u00e4u\u00b7me\u00b7te", "Gott", "E\u00b7dens", "gar\u00b7ten", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "NE", "VVFIN", "ART", "$("], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ich meine Engeland/ da\u00df ihr ihn soltet bauen/", "tokens": ["Ich", "mei\u00b7ne", "En\u00b7ge\u00b7land", "/", "da\u00df", "ihr", "ihn", "sol\u00b7tet", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$(", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und die verbotne frucht des Papstes ja nicht schauen/", "tokens": ["Und", "die", "ver\u00b7bot\u00b7ne", "frucht", "des", "Paps\u00b7tes", "ja", "nicht", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die R\u00f6msche schlange kont doch euer meister seyn;", "tokens": ["Die", "R\u00f6m\u00b7sche", "schlan\u00b7ge", "kont", "doch", "eu\u00b7er", "meis\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Pater Peter war die ursach eures falls/", "tokens": ["Der", "Pa\u00b7ter", "Pe\u00b7ter", "war", "die", "ur\u00b7sach", "eu\u00b7res", "falls", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit nichten werdet ihr/ sprach er/ des todes sterben/", "tokens": ["Mit", "nich\u00b7ten", "wer\u00b7det", "ihr", "/", "sprach", "er", "/", "des", "to\u00b7des", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "$(", "VVFIN", "PPER", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die Souverainit\u00e4t k\u00f6nt ihr dadurch erwerben/", "tokens": ["Die", "Sou\u00b7ve\u00b7rai\u00b7ni\u00b7t\u00e4t", "k\u00f6nt", "ihr", "da\u00b7durch", "er\u00b7wer\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Parla mente kostt es k\u00fcnfftig kopff und hal\u00df.", "tokens": ["Die", "Par\u00b7la", "men\u00b7te", "kostt", "es", "k\u00fcnff\u00b7tig", "kopff", "und", "hal\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VVFIN", "PPER", "ADJD", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jhr a\u00dft die schn\u00f6de frucht mit sehnlicher begier/", "tokens": ["Ihr", "a\u00dft", "die", "schn\u00f6\u00b7de", "frucht", "mit", "sehn\u00b7li\u00b7cher", "be\u00b7gier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bald wardt ihr nackt und blo\u00df und eures reichs entbl\u00f6sset/", "tokens": ["Bald", "wardt", "ihr", "nackt", "und", "blo\u00df", "und", "eu\u00b7res", "reichs", "ent\u00b7bl\u00f6s\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADV", "KON", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Daraus euch GOttes hand durch einen Cherub st\u00f6sset/", "tokens": ["Da\u00b7raus", "euch", "Got\u00b7tes", "hand", "durch", "ei\u00b7nen", "Che\u00b7rub", "st\u00f6s\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "NN", "APPR", "ART", "NE", "VVFIN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Printz von Vranien steht mit dem schwerdte hier.", "tokens": ["Printz", "von", "Vra\u00b7ni\u00b7en", "steht", "mit", "dem", "schwerd\u00b7te", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "VVFIN", "APPR", "ART", "ADJA", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Der ersten eltern fluch trifft euch zwar eben nicht/", "tokens": ["Der", "ers\u00b7ten", "el\u00b7tern", "fluch", "trifft", "euch", "zwar", "e\u00b7ben", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Printz ", "tokens": ["Der", "Printz"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Und beede habet ihr das Lilgen-feld erkohren/", "tokens": ["Und", "bee\u00b7de", "ha\u00b7bet", "ihr", "das", "Lil\u00b7gen\u00b7feld", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wo/ eurer meinung nach/ euch keine distel sticht.", "tokens": ["Wo", "/", "eu\u00b7rer", "mei\u00b7nung", "nach", "/", "euch", "kei\u00b7ne", "dis\u00b7tel", "sticht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "PPOSAT", "NN", "APPR", "$(", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}