{"dta.poem.11193": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen kamm aus elephanten-z\u00e4hnen.  \n  R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vjel z\u00e4hne hab ich itzt, vor war ich nur ein zahn:", "tokens": ["Vjel", "z\u00e4h\u00b7ne", "hab", "ich", "itzt", ",", "vor", "war", "ich", "nur", "ein", "zahn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VAFIN", "PPER", "ADV", "$,", "APPR", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Genung, da\u00df ich den zanck der haar\u2019 entscheiden kan.", "tokens": ["Ge\u00b7nung", ",", "da\u00df", "ich", "den", "zanck", "der", "haa\u00b7r'", "ent\u00b7schei\u00b7den", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}