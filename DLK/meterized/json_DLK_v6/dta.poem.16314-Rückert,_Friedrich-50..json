{"dta.poem.16314": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man reist, damit es uns zuhaus erst recht gefalle;", "tokens": ["Man", "reist", ",", "da\u00b7mit", "es", "uns", "zu\u00b7haus", "erst", "recht", "ge\u00b7fal\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer durchs Leben reist, der ist im gleichen Falle.", "tokens": ["Und", "wer", "durchs", "Le\u00b7ben", "reist", ",", "der", "ist", "im", "glei\u00b7chen", "Fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "VVFIN", "$,", "PRELS", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur da\u00df der Reisende hier nicht die Heimath kennt,", "tokens": ["Nur", "da\u00df", "der", "Rei\u00b7sen\u00b7de", "hier", "nicht", "die", "Hei\u00b7math", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur am Heimweh f\u00fchlt, er ist von ihr getrennt.", "tokens": ["Und", "nur", "am", "Heim\u00b7weh", "f\u00fchlt", ",", "er", "ist", "von", "ihr", "ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gereist zu seyn, wie wird dich's in der Heimat laben;", "tokens": ["Ge\u00b7reist", "zu", "seyn", ",", "wie", "wird", "dich's", "in", "der", "Hei\u00b7mat", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$,", "PWAV", "VAFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einst wie lieblich wird es seyn gelebt zu haben.", "tokens": ["Und", "einst", "wie", "lieb\u00b7lich", "wird", "es", "seyn", "ge\u00b7lebt", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "ADJD", "VAFIN", "PPER", "VAINF", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}