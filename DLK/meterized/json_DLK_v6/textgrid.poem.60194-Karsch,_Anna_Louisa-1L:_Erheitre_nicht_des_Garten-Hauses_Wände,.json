{"textgrid.poem.60194": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erheitre nicht des Garten-Hauses W\u00e4nde,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erheitre nicht des Garten-Hauses W\u00e4nde,", "tokens": ["Er\u00b7hei\u00b7tre", "nicht", "des", "Gar\u00b7ten\u00b7Hau\u00b7ses", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00e4lle nicht, um einer Handbreit Raum,", "tokens": ["Und", "f\u00e4l\u00b7le", "nicht", ",", "um", "ei\u00b7ner", "Hand\u00b7breit", "Raum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUI", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch Eisen und durch zwey gedungne H\u00e4nde,", "tokens": ["Durch", "Ei\u00b7sen", "und", "durch", "zwey", "ge\u00b7dung\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den schattigten Baum.", "tokens": ["Den", "schat\u00b7tig\u00b7ten", "Baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Selbst der Prophet, der Ninivens Verderben", "tokens": ["Selbst", "der", "Pro\u00b7phet", ",", "der", "Ni\u00b7ni\u00b7vens", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hartn\u00e4ckig foderte, ganz Menschenfeind,", "tokens": ["Hart\u00b7n\u00e4\u00b7ckig", "fo\u00b7der\u00b7te", ",", "ganz", "Men\u00b7schen\u00b7feind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hat einst, ger\u00fchrt von einer Pflanze Sterben,", "tokens": ["Hat", "einst", ",", "ge\u00b7r\u00fchrt", "von", "ei\u00b7ner", "Pflan\u00b7ze", "Ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den K\u00fcrbis beweint.", "tokens": ["Den", "K\u00fcr\u00b7bis", "be\u00b7weint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und du, ganz Menschenfreund, du willst die Hiebe", "tokens": ["Und", "du", ",", "ganz", "Men\u00b7schen\u00b7freund", ",", "du", "willst", "die", "Hie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ADV", "NN", "$,", "PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im hohen Baum? auf dessen Zweigen oft", "tokens": ["Im", "ho\u00b7hen", "Baum", "?", "auf", "des\u00b7sen", "Zwei\u00b7gen", "oft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "APPR", "PRELAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Vogel singt, der lockend, seiner Liebe", "tokens": ["Ein", "Vo\u00b7gel", "singt", ",", "der", "lo\u00b7ckend", ",", "sei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "VVFIN", "$,", "PRELS", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Befriedigung hofft?", "tokens": ["Be\u00b7frie\u00b7di\u00b7gung", "hofft", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Das willst du nicht. Denn wann auf weichem Sitze", "tokens": ["Das", "willst", "du", "nicht", ".", "Denn", "wann", "auf", "wei\u00b7chem", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$.", "KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wie ein F\u00fcrst, in selbst geschaffner Ruh", "tokens": ["Du", "wie", "ein", "F\u00fcrst", ",", "in", "selbst", "ge\u00b7schaff\u00b7ner", "Ruh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "KOKOM", "ART", "NN", "$,", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dich hier verbirgst, dann decket vor der Hitze", "tokens": ["Dich", "hier", "ver\u00b7birgst", ",", "dann", "de\u00b7cket", "vor", "der", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Schatten dich zu.", "tokens": ["Sein", "Schat\u00b7ten", "dich", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Er ist ein Herzog im Bezirk des Gartens.", "tokens": ["Er", "ist", "ein", "Her\u00b7zog", "im", "Be\u00b7zirk", "des", "Gar\u00b7tens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Pyramiden-B\u00e4ume wuchsen nur", "tokens": ["Die", "Py\u00b7ra\u00b7mi\u00b7den\u00b7B\u00e4u\u00b7me", "wuch\u00b7sen", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So durch die Kunst. Er spottete des Wartens,", "tokens": ["So", "durch", "die", "Kunst", ".", "Er", "spot\u00b7te\u00b7te", "des", "War\u00b7tens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihn zog die Natur!", "tokens": ["Ihn", "zog", "die", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "O welch ein Leib! mit was f\u00fcr starken Gliedern", "tokens": ["O", "welch", "ein", "Leib", "!", "mit", "was", "f\u00fcr", "star\u00b7ken", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "ART", "NN", "$.", "APPR", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versah sie ihn! So stand in Priams Stadt", "tokens": ["Ver\u00b7sah", "sie", "ihn", "!", "So", "stand", "in", "Pri\u00b7ams", "Stadt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$.", "ADV", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Einst Hector unter allen seinen Br\u00fcdern,", "tokens": ["Einst", "Hec\u00b7tor", "un\u00b7ter", "al\u00b7len", "sei\u00b7nen", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Kampfe nicht matt.", "tokens": ["Von", "Kamp\u00b7fe", "nicht", "matt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Dein Baum, der Held, steht, wann der Frost dem Leben", "tokens": ["Dein", "Baum", ",", "der", "Held", ",", "steht", ",", "wann", "der", "Frost", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Weinstocks und des Pfirsich-Baumes droht,", "tokens": ["Des", "Wein\u00b7stocks", "und", "des", "Pfir\u00b7sich\u00b7Bau\u00b7mes", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann steht er von Pomonens Schutz umgeben,", "tokens": ["Dann", "steht", "er", "von", "Po\u00b7mo\u00b7nens", "Schutz", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht f\u00fcrchtend den Tod.", "tokens": ["Nicht", "f\u00fcrch\u00b7tend", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Mit andern Trauben als der Weinstock tr\u00e4get", "tokens": ["Mit", "an\u00b7dern", "Trau\u00b7ben", "als", "der", "Wein\u00b7stock", "tr\u00e4\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Prangt er im Herbst; und liefert seinem Herrn", "tokens": ["Prangt", "er", "im", "Herbst", ";", "und", "lie\u00b7fert", "sei\u00b7nem", "Herrn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indem ein Holz ihn unbarmherzig schl\u00e4get", "tokens": ["In\u00b7dem", "ein", "Holz", "ihn", "un\u00b7barm\u00b7her\u00b7zig", "schl\u00e4\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den lieblichen Kern,", "tokens": ["Den", "lieb\u00b7li\u00b7chen", "Kern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Gewachsen in dem Umfang harter Schalen.", "tokens": ["Ge\u00b7wach\u00b7sen", "in", "dem", "Um\u00b7fang", "har\u00b7ter", "Scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So liegt im schlechten C\u00f6rper oft versteckt", "tokens": ["So", "liegt", "im", "schlech\u00b7ten", "C\u00f6r\u00b7per", "oft", "ver\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Herz, nicht mit dem Glanze zu bezahlen", "tokens": ["Ein", "Herz", ",", "nicht", "mit", "dem", "Glan\u00b7ze", "zu", "be\u00b7zah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mi\u00dfgunst erweckt.", "tokens": ["Der", "Mi\u00df\u00b7gunst", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "So hart wie sie, soll gegen fremde L\u00fcste", "tokens": ["So", "hart", "wie", "sie", ",", "soll", "ge\u00b7gen", "frem\u00b7de", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "$,", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein M\u00e4dchen seyn, f\u00fcr dich allein nur sch\u00f6n.", "tokens": ["Dein", "M\u00e4d\u00b7chen", "seyn", ",", "f\u00fcr", "dich", "al\u00b7lein", "nur", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "$,", "APPR", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weyh ihr den Baum, und sag einst: du Gek\u00fc\u00dfte!", "tokens": ["Weyh", "ihr", "den", "Baum", ",", "und", "sag", "einst", ":", "du", "Ge\u00b7k\u00fc\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "$.", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir lie\u00df ich ihn stehn!", "tokens": ["Dir", "lie\u00df", "ich", "ihn", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}