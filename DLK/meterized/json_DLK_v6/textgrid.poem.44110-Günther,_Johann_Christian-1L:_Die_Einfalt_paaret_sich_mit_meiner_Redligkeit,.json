{"textgrid.poem.44110": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Einfalt paaret sich mit meiner Redligkeit,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Einfalt paaret sich mit meiner Redligkeit,", "tokens": ["Die", "Ein\u00b7falt", "paa\u00b7ret", "sich", "mit", "mei\u00b7ner", "Red\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die jezt, mein Vater, dir ein schlechtes Opfer weiht.", "tokens": ["Die", "jezt", ",", "mein", "Va\u00b7ter", ",", "dir", "ein", "schlech\u00b7tes", "Op\u00b7fer", "weiht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PPOSAT", "NN", "$,", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nimm mit geneigter Hand den Erstling meiner Lieder", "tokens": ["Nimm", "mit", "ge\u00b7neig\u00b7ter", "Hand", "den", "Erst\u00b7ling", "mei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor deine Vatertreu von meiner Unschuld wieder!", "tokens": ["Vor", "dei\u00b7ne", "Va\u00b7ter\u00b7treu", "von", "mei\u00b7ner", "Un\u00b7schuld", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du siehst das gute Herz, du kennest meinen Sinn,", "tokens": ["Du", "siehst", "das", "gu\u00b7te", "Herz", ",", "du", "ken\u00b7nest", "mei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich aber weis auch dies, da\u00df ich dein Schuldner bin,", "tokens": ["Ich", "a\u00b7ber", "weis", "auch", "dies", ",", "da\u00df", "ich", "dein", "Schuld\u00b7ner", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "ADV", "PDS", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und w\u00fcrde, k\u00f6nt ich gleich dein Bild in Marmor graben,", "tokens": ["Und", "w\u00fcr\u00b7de", ",", "k\u00f6nt", "ich", "gleich", "dein", "Bild", "in", "Mar\u00b7mor", "gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dir doch das wenigste dadurch vergolten haben.", "tokens": ["Dir", "doch", "das", "we\u00b7nigs\u00b7te", "da\u00b7durch", "ver\u00b7gol\u00b7ten", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "PIS", "PAV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Leben, welches ich nechst Gott von dir empfing,", "tokens": ["Das", "Le\u00b7ben", ",", "wel\u00b7ches", "ich", "nechst", "Gott", "von", "dir", "emp\u00b7fing", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Bo\u00dfheit, der mein Fu\u00df durch deinen Flei\u00df entgieng,", "tokens": ["Die", "Bo\u00df\u00b7heit", ",", "der", "mein", "Fu\u00df", "durch", "dei\u00b7nen", "Flei\u00df", "ent\u00b7gieng", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sind Ursach, da\u00df mein Geist die Warheit angenommen:", "tokens": ["Sind", "Ur\u00b7sach", ",", "da\u00df", "mein", "Geist", "die", "War\u00b7heit", "an\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Im Lieben weis kein Kind den Eltern beyzukommen.", "tokens": ["Im", "Lie\u00b7ben", "weis", "kein", "Kind", "den", "El\u00b7tern", "bey\u00b7zu\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PTKVZ", "PIAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie manchen Sommertag, wie manche Winternacht", "tokens": ["Wie", "man\u00b7chen", "Som\u00b7mer\u00b7tag", ",", "wie", "man\u00b7che", "Win\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hat meine Kindheit dir betr\u00fcbt und schwer gemacht!", "tokens": ["Hat", "mei\u00b7ne", "Kind\u00b7heit", "dir", "be\u00b7tr\u00fcbt", "und", "schwer", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kein Tropfen saurer Schwei\u00df, der oft so unverdro\u00dfen", "tokens": ["Kein", "Trop\u00b7fen", "sau\u00b7rer", "Schwei\u00df", ",", "der", "oft", "so", "un\u00b7ver\u00b7dro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Bey \u00fcberh\u00e4ufter M\u00fch mir auf das Buch geflo\u00dfen,", "tokens": ["Bey", "\u00fc\u00b7berh\u00b7\u00e4uf\u00b7ter", "M\u00fch", "mir", "auf", "das", "Buch", "ge\u00b7flo\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Gereute deinen Mund, wenn ich zur Seiten sa\u00df,", "tokens": ["Ge\u00b7reu\u00b7te", "dei\u00b7nen", "Mund", ",", "wenn", "ich", "zur", "Sei\u00b7ten", "sa\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und meinen . . . . . . . . den goldnen Maro las.", "tokens": ["Und", "mei\u00b7nen", ".", ".", ".", ".", ".", ".", ".", ".", "den", "gold\u00b7nen", "Ma\u00b7ro", "las", "."], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "ART", "ADJA", "NE", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Ach Vater, Vater ach, ich w\u00fcntschte deine Lehren", "tokens": ["Ach", "Va\u00b7ter", ",", "Va\u00b7ter", "ach", ",", "ich", "w\u00fcnt\u00b7schte", "dei\u00b7ne", "Leh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "NN", "XY", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Anjezo wie zuvor mit Wollust anzuh\u00f6ren.", "tokens": ["An\u00b7je\u00b7zo", "wie", "zu\u00b7vor", "mit", "Wol\u00b7lust", "an\u00b7zu\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Kern der Gottesfurcht, den du mir eingepr\u00e4gt,", "tokens": ["Der", "Kern", "der", "Got\u00b7tes\u00b7furcht", ",", "den", "du", "mir", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Hat mir den rechten Grund des Christenthums gelegt,", "tokens": ["Hat", "mir", "den", "rech\u00b7ten", "Grund", "des", "Chris\u00b7ten\u00b7thums", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und deine Wachsamkeit war fr\u00fch und sp\u00e4t befli\u00dfen,", "tokens": ["Und", "dei\u00b7ne", "Wach\u00b7sam\u00b7keit", "war", "fr\u00fch", "und", "sp\u00e4t", "be\u00b7fli\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Wei\u00dfheit und ihr Oel mir in das Herz zu gie\u00dfen.", "tokens": ["Die", "Wei\u00df\u00b7heit", "und", "ihr", "O\u00b7el", "mir", "in", "das", "Herz", "zu", "gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Was der Befehl gebaut, rei\u00dft oft der Wandel ein.", "tokens": ["Was", "der", "Be\u00b7fehl", "ge\u00b7baut", ",", "rei\u00dft", "oft", "der", "Wan\u00b7del", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Eltern Bo\u00dfheit pflegt der Kinder Spiel zu seyn,", "tokens": ["Der", "El\u00b7tern", "Bo\u00df\u00b7heit", "pflegt", "der", "Kin\u00b7der", "Spiel", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die sich das wilde Fleisch zum B\u00f6sen f\u00fchren la\u00dfen", "tokens": ["Die", "sich", "das", "wil\u00b7de", "Fleisch", "zum", "B\u00f6\u00b7sen", "f\u00fch\u00b7ren", "la\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und hundert Laster eh als eine Tugend fa\u00dfen.", "tokens": ["Und", "hun\u00b7dert", "Las\u00b7ter", "eh", "als", "ei\u00b7ne", "Tu\u00b7gend", "fa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "KOUS", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Weil deinen Worten nun die That zu H\u00fclfe kam", "tokens": ["Weil", "dei\u00b7nen", "Wor\u00b7ten", "nun", "die", "That", "zu", "H\u00fcl\u00b7fe", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und Lehr und Leben stets mit in die Schule nahm,", "tokens": ["Und", "Lehr", "und", "Le\u00b7ben", "stets", "mit", "in", "die", "Schu\u00b7le", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So will der Zweifel sich mit meiner Be\u00dfrung zancken,", "tokens": ["So", "will", "der", "Zwei\u00b7fel", "sich", "mit", "mei\u00b7ner", "Be\u00df\u00b7rung", "zan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wem doch von beiden ich mein Wohlergehn zu dancken.", "tokens": ["Wem", "doch", "von", "bei\u00b7den", "ich", "mein", "Woh\u00b7ler\u00b7gehn", "zu", "dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PIAT", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mein Vater, glaube nicht, da\u00df mir die Schmeicheley", "tokens": ["Mein", "Va\u00b7ter", ",", "glau\u00b7be", "nicht", ",", "da\u00df", "mir", "die", "Schmei\u00b7che\u00b7ley"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Zu dieser treuen Schrift den falschen Kiel verleih;", "tokens": ["Zu", "die\u00b7ser", "treu\u00b7en", "Schrift", "den", "fal\u00b7schen", "Kiel", "ver\u00b7leih", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Himmel zeuget mir, da\u00df mich die reine Liebe", "tokens": ["Der", "Him\u00b7mel", "zeu\u00b7get", "mir", ",", "da\u00df", "mich", "die", "rei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Vor dich zur Danckbarkeit durch Fluth und Feuer triebe.", "tokens": ["Vor", "dich", "zur", "Dan\u00b7ck\u00b7bar\u00b7keit", "durch", "Fluth", "und", "Feu\u00b7er", "trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPRART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.37": {"text": "Der Seegen breite sich auf dich und unser Haus", "tokens": ["Der", "See\u00b7gen", "brei\u00b7te", "sich", "auf", "dich", "und", "un\u00b7ser", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So wie ein Feigenbaum in Jacobs Erbtheil aus", "tokens": ["So", "wie", "ein", "Fei\u00b7gen\u00b7baum", "in", "Ja\u00b7cobs", "E\u00b7rbtheil", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NE", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und la\u00dfe dich so viel vergn\u00fcgter Jahre zehlen,", "tokens": ["Und", "la\u00b7\u00dfe", "dich", "so", "viel", "ver\u00b7gn\u00fcg\u00b7ter", "Jah\u00b7re", "zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Als Stunden mir noch jezt zu Nestors Alter fehlen.", "tokens": ["Als", "Stun\u00b7den", "mir", "noch", "jezt", "zu", "Nes\u00b7tors", "Al\u00b7ter", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "ADV", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich k\u00fc\u00dfe dir nunmehr die wohlbetagte Hand,", "tokens": ["Ich", "k\u00fc\u00b7\u00dfe", "dir", "nun\u00b7mehr", "die", "wohl\u00b7be\u00b7tag\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die mir in Lieb und Ernst die erste Ruthe band,", "tokens": ["Die", "mir", "in", "Lieb", "und", "Ernst", "die", "ers\u00b7te", "Ru\u00b7the", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Sie hat mich in der Furcht des H\u00f6chsten auferzogen,", "tokens": ["Sie", "hat", "mich", "in", "der", "Furcht", "des", "H\u00f6chs\u00b7ten", "auf\u00b7er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da\u00df mich die Eitelkeit durch keinen Schein betrogen.", "tokens": ["Da\u00df", "mich", "die", "Ei\u00b7tel\u00b7keit", "durch", "kei\u00b7nen", "Schein", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "So lange dir und mir die Augen ofen stehn,", "tokens": ["So", "lan\u00b7ge", "dir", "und", "mir", "die", "Au\u00b7gen", "o\u00b7fen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "KON", "PPER", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "So lange soll dein Lob mir von der Zunge gehn;", "tokens": ["So", "lan\u00b7ge", "soll", "dein", "Lob", "mir", "von", "der", "Zun\u00b7ge", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wird endlich mich der Tod aus dieser Welt verweisen,", "tokens": ["Wird", "end\u00b7lich", "mich", "der", "Tod", "aus", "die\u00b7ser", "Welt", "ver\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "So will ich deinen Flei\u00df vor jenem Richter preisen.", "tokens": ["So", "will", "ich", "dei\u00b7nen", "Flei\u00df", "vor", "je\u00b7nem", "Rich\u00b7ter", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}