{"textgrid.poem.42316": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Einladung", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid doch einmal mein Gast, Herr Plitt!", "tokens": ["Seid", "doch", "ein\u00b7mal", "mein", "Gast", ",", "Herr", "Plitt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADV", "PPOSAT", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon bitt' ich euch zu hundert Malen.", "tokens": ["Schon", "bitt'", "ich", "euch", "zu", "hun\u00b7dert", "Ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bringt ihr etwa euer Essen mit,", "tokens": ["Bringt", "ihr", "et\u00b7wa", "eu\u00b7er", "Es\u00b7sen", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "So sollt ihr nur den Wein bezahlen.", "tokens": ["So", "sollt", "ihr", "nur", "den", "Wein", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}