{"textgrid.poem.43854": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "O M\u00e4uselein!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O M\u00e4uselein, o M\u00e4uselein,", "tokens": ["O", "M\u00e4u\u00b7sel\u00b7ein", ",", "o", "M\u00e4u\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O stelle doch das Naschen ein!", "tokens": ["O", "stel\u00b7le", "doch", "das", "Na\u00b7schen", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir warnen dich, wir meinen's gut:", "tokens": ["Wir", "war\u00b7nen", "dich", ",", "wir", "mei\u00b7nen's", "gut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sei k\u00fcnftig mehr auf deiner Hut!", "tokens": ["Sei", "k\u00fcnf\u00b7tig", "mehr", "auf", "dei\u00b7ner", "Hut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O M\u00e4uselein, o M\u00e4uselein,", "tokens": ["O", "M\u00e4u\u00b7sel\u00b7ein", ",", "o", "M\u00e4u\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie wird es dir ergehen!", "tokens": ["Wie", "wird", "es", "dir", "er\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O M\u00e4uselein, o M\u00e4uselein,", "tokens": ["O", "M\u00e4u\u00b7sel\u00b7ein", ",", "o", "M\u00e4u\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh' in die Ecke nicht hinein!", "tokens": ["Geh'", "in", "die", "E\u00b7cke", "nicht", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es stehet eine Falle da,", "tokens": ["Es", "ste\u00b7het", "ei\u00b7ne", "Fal\u00b7le", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die aufgestellt hat der Papa.", "tokens": ["Die", "auf\u00b7ge\u00b7stellt", "hat", "der", "Pa\u00b7pa", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O M\u00e4uselein, o M\u00e4uselein,", "tokens": ["O", "M\u00e4u\u00b7sel\u00b7ein", ",", "o", "M\u00e4u\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie wird es dir ergehen!", "tokens": ["Wie", "wird", "es", "dir", "er\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das M\u00e4uselein, das M\u00e4uselein,", "tokens": ["Das", "M\u00e4u\u00b7sel\u00b7ein", ",", "das", "M\u00e4u\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schl\u00fcpfet in die Fall' hinein.", "tokens": ["Das", "schl\u00fcp\u00b7fet", "in", "die", "Fall'", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wipp wapp! da f\u00e4llt die Falle zu,", "tokens": ["Wipp", "wapp", "!", "da", "f\u00e4llt", "die", "Fal\u00b7le", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gefangen ist es da im Nu.", "tokens": ["Ge\u00b7fan\u00b7gen", "ist", "es", "da", "im", "Nu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O M\u00e4uselein, o M\u00e4uselein,", "tokens": ["O", "M\u00e4u\u00b7sel\u00b7ein", ",", "o", "M\u00e4u\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun ist's um dich geschehen!", "tokens": ["Nun", "ist's", "um", "dich", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}