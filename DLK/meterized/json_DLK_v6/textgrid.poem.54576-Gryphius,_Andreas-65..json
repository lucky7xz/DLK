{"textgrid.poem.54576": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch gr\u00fcnt die frische Lieb/ ob Blum und Baum erbleicht/", "tokens": ["Doch", "gr\u00fcnt", "die", "fri\u00b7sche", "Lieb", "/", "ob", "Blum", "und", "Baum", "er\u00b7bleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$(", "KOUS", "NE", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe/ die sich mir in einem Krantz verehret/", "tokens": ["Die", "Lie\u00b7be", "/", "die", "sich", "mir", "in", "ei\u00b7nem", "Krantz", "ver\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem Bild der Ewigkeit/ die durch kein End auffh\u00f6ret/", "tokens": ["Dem", "Bild", "der", "E\u00b7wig\u00b7keit", "/", "die", "durch", "kein", "End", "auff\u00b7h\u00f6\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und keiner Zeiten Grimm/ ja nicht dem Tode weicht/", "tokens": ["Und", "kei\u00b7ner", "Zei\u00b7ten", "Grimm", "/", "ja", "nicht", "dem", "To\u00b7de", "weicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NE", "$(", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr keuschestes Gem\u00fcth/ das reinem Silber gleicht/", "tokens": ["Ihr", "keu\u00b7sches\u00b7tes", "Ge\u00b7m\u00fcth", "/", "das", "rei\u00b7nem", "Sil\u00b7ber", "gleicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Licht/ hat ihr Geschenck mit Perlen noch vermehret/", "tokens": ["Mein", "Licht", "/", "hat", "ihr", "Ge\u00b7schenck", "mit", "Per\u00b7len", "noch", "ver\u00b7meh\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die/ wenn das rauhe Saltz der Wellen sich emp\u00f6ret/", "tokens": ["Die", "/", "wenn", "das", "rau\u00b7he", "Saltz", "der", "Wel\u00b7len", "sich", "em\u00b7p\u00f6\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kein scharffer Schlag der See/ kein sch\u00e4umend Fleck erreicht/", "tokens": ["Kein", "scharf\u00b7fer", "Schlag", "der", "See", "/", "kein", "sch\u00e4u\u00b7mend", "Fleck", "er\u00b7reicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "$(", "PIAT", "ADJD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So bleibt ihr hoher Geist doch rein in tr\u00fcben Schmertzen/", "tokens": ["So", "bleibt", "ihr", "ho\u00b7her", "Geist", "doch", "rein", "in", "tr\u00fc\u00b7ben", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ihr Geist/ den rechte Treu aus unverf\u00e4lschtem Hertzen", "tokens": ["Ihr", "Geist", "/", "den", "rech\u00b7te", "Treu", "aus", "un\u00b7ver\u00b7f\u00e4lschtem", "Hert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Durch ihrer Seuffzer West in meinen C\u00f6rper schickt.", "tokens": ["Durch", "ih\u00b7rer", "Seuff\u00b7zer", "West", "in", "mei\u00b7nen", "C\u00f6r\u00b7per", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich wil zwar ihr Gem\u00fcth aus dem Geschencke sch\u00e4tzen/", "tokens": ["Ich", "wil", "zwar", "ihr", "Ge\u00b7m\u00fcth", "aus", "dem", "Ge\u00b7schen\u00b7cke", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df Wort doch/ das sie lie\u00df auf dieses Silber etzen/", "tokens": ["Di\u00df", "Wort", "doch", "/", "das", "sie", "lie\u00df", "auf", "die\u00b7ses", "Sil\u00b7ber", "et\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "$(", "PRELS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ist was den Krantz recht ziert und mich allein erquickt.", "tokens": ["Ist", "was", "den", "Krantz", "recht", "ziert", "und", "mich", "al\u00b7lein", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "ADJD", "VVFIN", "KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}