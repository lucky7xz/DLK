{"textgrid.poem.43374": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sag an, du \u00f6ffentliche Meinung,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag an, du \u00f6ffentliche Meinung,", "tokens": ["Sag", "an", ",", "du", "\u00f6f\u00b7fent\u00b7li\u00b7che", "Mei\u00b7nung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sag an, wie lange schweigst du still?", "tokens": ["Sag", "an", ",", "wie", "lan\u00b7ge", "schweigst", "du", "still", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann bringst du endlich zur Erscheinung", "tokens": ["Wann", "bringst", "du", "end\u00b7lich", "zur", "Er\u00b7schei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was Deutschland soll und mu\u00df und will?", "tokens": ["Was", "Deutschland", "soll", "und", "mu\u00df", "und", "will", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VMFIN", "KON", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Zeig deines Volkes Wundenmale,", "tokens": ["Zeig", "dei\u00b7nes", "Vol\u00b7kes", "Wun\u00b7den\u00b7ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeig seine Blut- und Thr\u00e4nensaat,", "tokens": ["Zeig", "sei\u00b7ne", "Blut", "und", "Thr\u00e4\u00b7nen\u00b7saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wieg auf deiner Wageschale", "tokens": ["Und", "wieg", "auf", "dei\u00b7ner", "Wa\u00b7ge\u00b7scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Volkes Lohn, der F\u00fcrsten That!", "tokens": ["Des", "Vol\u00b7kes", "Lohn", ",", "der", "F\u00fcrs\u00b7ten", "That", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du willst nicht Aufruhr, nicht Zerw\u00fcrfni\u00df,", "tokens": ["Du", "willst", "nicht", "Auf\u00b7ruhr", ",", "nicht", "Zer\u00b7w\u00fcrf\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4umst nicht von Hochverrath und Mord \u2013", "tokens": ["Tr\u00e4umst", "nicht", "von", "Hoch\u00b7ver\u00b7rath", "und", "Mord", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Wunsch ist nur das ", "tokens": ["Dein", "Wunsch", "ist", "nur", "das"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}