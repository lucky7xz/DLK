{"textgrid.poem.24405": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Invita Minerva nihil facies", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer etwas lernen will/ befrage sich zuvor", "tokens": ["Wer", "et\u00b7was", "ler\u00b7nen", "will", "/", "be\u00b7fra\u00b7ge", "sich", "zu\u00b7vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PIS", "VVINF", "VMFIN", "$(", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worzu ihn die Natur vor andern f\u00e4hig machet/", "tokens": ["Wor\u00b7zu", "ihn", "die", "Na\u00b7tur", "vor", "an\u00b7dern", "f\u00e4\u00b7hig", "ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PIS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sonst segelt wieder Wind und Wellen nur ein Thor/", "tokens": ["Sonst", "se\u00b7gelt", "wie\u00b7der", "Wind", "und", "Wel\u00b7len", "nur", "ein", "Thor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und von ", "tokens": ["Und", "von"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sie ist/ wie Frauenzimmer sind/", "tokens": ["Sie", "ist", "/", "wie", "Frau\u00b7en\u00b7zim\u00b7mer", "sind", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KOKOM", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein gut doch eigensinnig Kind/", "tokens": ["Ein", "gut", "doch", "ei\u00b7gen\u00b7sin\u00b7nig", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer sie nach Lust bedient/ hat viele Gunst zu hoffen/", "tokens": ["Wer", "sie", "nach", "Lust", "be\u00b7dient", "/", "hat", "vie\u00b7le", "Gunst", "zu", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVPP", "$(", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den andern steht auch nicht die Hinter-Th\u00fcre offen.", "tokens": ["Den", "an\u00b7dern", "steht", "auch", "nicht", "die", "Hin\u00b7ter\u00b7T\u00b7h\u00fc\u00b7re", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}