{"dta.poem.21748": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Nachtsturm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sturm der gellenden Nacht, f&#252;rchterlich klingt und", "tokens": ["Sturm", "der", "gel\u00b7len\u00b7den", "Nacht", ",", "f", "&#252;", "rch\u00b7ter\u00b7lich", "klingt", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "XML_entity", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "NE", "$(", "ADJD", "VVFIN", "KON"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "sch\u00f6n", "tokens": ["sch\u00f6n"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Durch die Nacht dein Gel\u00e4ut, klingt mir begeis-", "tokens": ["Durch", "die", "Nacht", "dein", "Ge\u00b7l\u00e4ut", ",", "klingt", "mir", "be\u00b7geis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "TRUNC"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Als der lydischen Fl\u00f6te", "tokens": ["Als", "der", "ly\u00b7di\u00b7schen", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Weiche Wirbel im Abendk\u00fchl.", "tokens": ["Wei\u00b7che", "Wir\u00b7bel", "im", "A\u00b7bend\u00b7k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Rabenschwarz ist die Nacht. Durch die Erebische", "tokens": ["Ra\u00b7ben\u00b7schwarz", "ist", "die", "Nacht", ".", "Durch", "die", "E\u00b7re\u00b7bi\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "W\u00e4lzt der M\u00e4chtige sich s\u00e4uselnden Schwungs", "tokens": ["W\u00e4lzt", "der", "M\u00e4ch\u00b7ti\u00b7ge", "sich", "s\u00e4u\u00b7seln\u00b7den", "Schwungs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PRF", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "daher,", "tokens": ["da\u00b7her", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Beugt die Grossen der Sch\u00f6pfung,", "tokens": ["Beugt", "die", "Gros\u00b7sen", "der", "Sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "St\u00e4upt die H\u00f6hen und zaust den Wald.", "tokens": ["St\u00e4upt", "die", "H\u00f6\u00b7hen", "und", "zaust", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Orkan, Orkan, gegr\u00fcsst sey mir in deiner Kraft.", "tokens": ["Or\u00b7kan", ",", "Or\u00b7kan", ",", "ge\u00b7gr\u00fcs\u00b7st", "sey", "mir", "in", "dei\u00b7ner", "Kraft", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "VVPP", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+----+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Orkan, Orkan, dir lauscht gerne des J\u00fcnglings Ohr,", "tokens": ["Or\u00b7kan", ",", "Or\u00b7kan", ",", "dir", "lauscht", "ger\u00b7ne", "des", "J\u00fcng\u00b7lings", "Ohr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wenn Allfadern dein P\u00e4an", "tokens": ["Wenn", "All\u00b7fa\u00b7dern", "dein", "P\u00e4\u00b7an"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Harfe der Waldnacht singt.", "tokens": ["Auf", "der", "Har\u00b7fe", "der", "Wald\u00b7nacht", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Minder feyerlich f\u00fcrwahr wallet anbetender", "tokens": ["Min\u00b7der", "fey\u00b7er\u00b7lich", "f\u00fcr\u00b7wahr", "wal\u00b7let", "an\u00b7be\u00b7ten\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Myriaden Gesang rings durch des M\u00fcnsters Schiff,", "tokens": ["My\u00b7ria\u00b7den", "Ge\u00b7sang", "rings", "durch", "des", "M\u00fcns\u00b7ters", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als dein Hymnus im Dunkeln", "tokens": ["Als", "dein", "Hym\u00b7nus", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Durch den Tempel der Sch\u00f6pfung wallt.", "tokens": ["Durch", "den", "Tem\u00b7pel", "der", "Sch\u00f6p\u00b7fung", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6n und f\u00fcrchterlich ists, wenn du die Wei-", "tokens": ["Sch\u00f6n", "und", "f\u00fcrch\u00b7ter\u00b7lich", "ists", ",", "wenn", "du", "die", "Wei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ART", "TRUNC"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Niederm\u00e4hst, wenn dein Arm geisselt den stolzen", "tokens": ["Nie\u00b7der\u00b7m\u00e4hst", ",", "wenn", "dein", "Arm", "geis\u00b7selt", "den", "stol\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Forst,", "tokens": ["Forst", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und mit Pappeln und Eiche,", "tokens": ["Und", "mit", "Pap\u00b7peln", "und", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie ein Knabe mit Diesteln, spielt.", "tokens": ["Wie", "ein", "Kna\u00b7be", "mit", "Dies\u00b7teln", ",", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6n und f\u00fcrchterlich ists, wenn du das Meer er-", "tokens": ["Sch\u00f6n", "und", "f\u00fcrch\u00b7ter\u00b7lich", "ists", ",", "wenn", "du", "das", "Meer", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "TRUNC"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sein Verm\u00f6gen zerst\u00e4upst, Schiffe, wie Kr\u00e4usel", "tokens": ["Sein", "Ver\u00b7m\u00f6\u00b7gen", "zer\u00b7st\u00e4u\u00b7pst", ",", "Schif\u00b7fe", ",", "wie", "Kr\u00e4u\u00b7sel"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "NN", "$,", "PWAV", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "drehst,", "tokens": ["drehst", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Masten knickest, wie Binsen,", "tokens": ["Mas\u00b7ten", "kni\u00b7ckest", ",", "wie", "Bin\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Taue reissest, wie m\u00fcrben Zwirn.", "tokens": ["Tau\u00b7e", "reis\u00b7sest", ",", "wie", "m\u00fcr\u00b7ben", "Zwirn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Sch\u00f6n und f\u00fcrchterlich ists, wenn du die Wolken ballst.", "tokens": ["Sch\u00f6n", "und", "f\u00fcrch\u00b7ter\u00b7lich", "ists", ",", "wenn", "du", "die", "Wol\u00b7ken", "ballst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Manches Riesengebild segelt in weiter Luft.", "tokens": ["Man\u00b7ches", "Rie\u00b7sen\u00b7ge\u00b7bild", "se\u00b7gelt", "in", "wei\u00b7ter", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Lunens Silber verblasset.", "tokens": ["Lu\u00b7nens", "Sil\u00b7ber", "ver\u00b7blas\u00b7set", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Rings erblindet der Sterne Gold.", "tokens": ["Rings", "er\u00b7blin\u00b7det", "der", "Ster\u00b7ne", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Westges\u00e4usel behagt l\u00fcsternen Weichlingen.", "tokens": ["West\u00b7ge\u00b7s\u00e4u\u00b7sel", "be\u00b7hagt", "l\u00fcs\u00b7ter\u00b7nen", "Weich\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+--++---+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Bass behagt mir, Orkan, dein dithyrambisch Lied.", "tokens": ["Bass", "be\u00b7hagt", "mir", ",", "Or\u00b7kan", ",", "dein", "dit\u00b7hy\u00b7ram\u00b7bisch", "Lied", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NE", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Jeden glimmenden Funken", "tokens": ["Je\u00b7den", "glim\u00b7men\u00b7den", "Fun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Fachst du, straff'st den erschlafften Nerv.", "tokens": ["Fachst", "du", ",", "straff'st", "den", "er\u00b7schlaff\u00b7ten", "Nerv", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "In das Dunkel hinaus st\u00fcrm' ich, in schwarzer Nacht", "tokens": ["In", "das", "Dun\u00b7kel", "hin\u00b7aus", "st\u00fcrm'", "ich", ",", "in", "schwar\u00b7zer", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APZR", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Klimm' ich Felsen hinan, schaue vom stickeln Fels", "tokens": ["Klimm'", "ich", "Fel\u00b7sen", "hi\u00b7nan", ",", "schau\u00b7e", "vom", "sti\u00b7ckeln", "Fels"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$,", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "In das g\u00e4hrende Chaos,", "tokens": ["In", "das", "g\u00e4h\u00b7ren\u00b7de", "Chaos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In die w\u00fchlende Nacht hinaus.", "tokens": ["In", "die", "w\u00fch\u00b7len\u00b7de", "Nacht", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Erd' und Himmel und Meer zittern dir, Freudiger!", "tokens": ["Erd'", "und", "Him\u00b7mel", "und", "Meer", "zit\u00b7tern", "dir", ",", "Freu\u00b7di\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Freudigeres, denn du, hebet des Menschen Brust,", "tokens": ["Freu\u00b7di\u00b7ge\u00b7res", ",", "denn", "du", ",", "he\u00b7bet", "des", "Men\u00b7schen", "Brust", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Triumphirt in des Daseyns", "tokens": ["Tri\u00b7um\u00b7phirt", "in", "des", "Da\u00b7seyns"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "St\u00fcrmen, frohlockt im Untergang.", "tokens": ["St\u00fcr\u00b7men", ",", "froh\u00b7lockt", "im", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}