{"textgrid.poem.55794": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Trostlos-weinende Sehnsucht,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trostlos-weinende Sehnsucht,", "tokens": ["Trost\u00b7los\u00b7wei\u00b7nen\u00b7de", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Du geleitest", "tokens": ["Du", "ge\u00b7lei\u00b7test"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Einzig meine verirrte,", "tokens": ["Ein\u00b7zig", "mei\u00b7ne", "ver\u00b7irr\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wundenblutige Seele,", "tokens": ["Wun\u00b7den\u00b7blu\u00b7ti\u00b7ge", "See\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Und Dein hilflos", "tokens": ["Und", "Dein", "hil\u00b7flos"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Kindisch-th\u00f6richtes Weinen", "tokens": ["Kin\u00b7disch\u00b7th\u00f6\u00b7rich\u00b7tes", "Wei\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Mehrt ihre Qualen.", "tokens": ["Mehrt", "ih\u00b7re", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Weinende Sehnsucht,", "tokens": ["Wei\u00b7nen\u00b7de", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Lass', o lass' meine Seele", "tokens": ["Lass'", ",", "o", "lass'", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "FM", "VVFIN", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Pilgern und irren,", "tokens": ["Pil\u00b7gern", "und", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Fallen, verderben,", "tokens": ["Fal\u00b7len", ",", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Aber g\u00f6nn' ihr", "tokens": ["A\u00b7ber", "g\u00f6nn'", "ihr"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVIMP", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Hoffnungsloses,", "tokens": ["Hoff\u00b7nungs\u00b7lo\u00b7ses", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Selig-schweigendes Sterben.", "tokens": ["Se\u00b7lig\u00b7schwei\u00b7gen\u00b7des", "Ster\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}