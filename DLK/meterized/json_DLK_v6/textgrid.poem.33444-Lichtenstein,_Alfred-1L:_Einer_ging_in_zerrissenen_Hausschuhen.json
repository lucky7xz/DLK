{"textgrid.poem.33444": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einer ging in zerrissenen Hausschuhen", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einer ging in zerrissenen Hausschuhen", "tokens": ["Ei\u00b7ner", "ging", "in", "zer\u00b7ris\u00b7se\u00b7nen", "Haus\u00b7schu\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+---+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Hin und her durch das kleine Zimmer,", "tokens": ["Hin", "und", "her", "durch", "das", "klei\u00b7ne", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Das er bewohnte.", "tokens": ["Das", "er", "be\u00b7wohn\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Er sann \u00fcber die Geschehnisse,", "tokens": ["Er", "sann", "\u00fc\u00b7ber", "die", "Ge\u00b7scheh\u00b7nis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Von denen in dem Abendblatt berichtet war.", "tokens": ["Von", "de\u00b7nen", "in", "dem", "A\u00b7bend\u00b7blatt", "be\u00b7rich\u00b7tet", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und g\u00e4hnte traurig, wie nur jemand g\u00e4hnt,", "tokens": ["Und", "g\u00e4hn\u00b7te", "trau\u00b7rig", ",", "wie", "nur", "je\u00b7mand", "g\u00e4hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der viel und Seltsames gelesen hat \u2013", "tokens": ["Der", "viel", "und", "Selt\u00b7sa\u00b7mes", "ge\u00b7le\u00b7sen", "hat", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KON", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und der Gedanke \u00fcberkam ihn pl\u00f6tzlich,", "tokens": ["Und", "der", "Ge\u00b7dan\u00b7ke", "\u00fc\u00b7ber\u00b7kam", "ihn", "pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie wohl den Furchtsamen die G\u00e4nsehaut", "tokens": ["Wie", "wohl", "den", "Furcht\u00b7sa\u00b7men", "die", "G\u00e4n\u00b7se\u00b7haut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und wie das Aufsto\u00dfen den \u00dcbers\u00e4ttigten,", "tokens": ["Und", "wie", "das", "Auf\u00b7sto\u00b7\u00dfen", "den", "\u00dc\u00b7bers\u00b7\u00e4t\u00b7tig\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Wie Mutterwehen:", "tokens": ["Wie", "Mut\u00b7ter\u00b7we\u00b7hen", ":"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Das gro\u00dfe G\u00e4hnen sei vielleicht ein Zeichen,", "tokens": ["Das", "gro\u00b7\u00dfe", "G\u00e4h\u00b7nen", "sei", "viel\u00b7leicht", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ein Wink des Schicksals, sich zur Ruh zu legen.", "tokens": ["Ein", "Wink", "des", "Schick\u00b7sals", ",", "sich", "zur", "Ruh", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und der Gedanke lie\u00df ihn nicht mehr los.", "tokens": ["Und", "der", "Ge\u00b7dan\u00b7ke", "lie\u00df", "ihn", "nicht", "mehr", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und also fing er an, sich zu entkleiden ...", "tokens": ["Und", "al\u00b7so", "fing", "er", "an", ",", "sich", "zu", "ent\u00b7klei\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als er ganz nackt war, hantelte er etwas.", "tokens": ["Als", "er", "ganz", "nackt", "war", ",", "han\u00b7tel\u00b7te", "er", "et\u00b7was", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-++-+-+++-", "measure": "unknown.measure.septa"}}}}}