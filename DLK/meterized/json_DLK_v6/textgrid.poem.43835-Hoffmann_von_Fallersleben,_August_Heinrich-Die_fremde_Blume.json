{"textgrid.poem.43835": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die fremde Blume", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern aus der Heimath Land", "tokens": ["Fern", "aus", "der", "Hei\u00b7math", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bin ich hieher verbannt \u2013", "tokens": ["Bin", "ich", "hie\u00b7her", "ver\u00b7bannt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "VVPP", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Schmerzt mich so sehr.", "tokens": ["Schmerzt", "mich", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Fremd ist mir Alles hier,", "tokens": ["Fremd", "ist", "mir", "Al\u00b7les", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Alles so seltsam mir,", "tokens": ["Al\u00b7les", "so", "selt\u00b7sam", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Traurig und leer.", "tokens": ["Trau\u00b7rig", "und", "leer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Als ich daheim noch war,", "tokens": ["Als", "ich", "da\u00b7heim", "noch", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hatt' ich das ganze Jahr", "tokens": ["Hatt'", "ich", "das", "gan\u00b7ze", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sommer zumal;", "tokens": ["Som\u00b7mer", "zu\u00b7mal", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-++", "measure": "unknown.measure.tri"}, "line.4": {"text": "Kannte nicht kalte Zeit,", "tokens": ["Kann\u00b7te", "nicht", "kal\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Stubenluft, Winterleid,", "tokens": ["Stu\u00b7ben\u00b7luft", ",", "Win\u00b7ter\u00b7leid", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Sorgen und Qual.", "tokens": ["Sor\u00b7gen", "und", "Qual", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und in Verbannung noch", "tokens": ["Und", "in", "Ver\u00b7ban\u00b7nung", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gr\u00fcn' ich und bl\u00fch' ich doch", "tokens": ["Gr\u00fcn'", "ich", "und", "bl\u00fch'", "ich", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wie vorher;", "tokens": ["So", "wie", "vor\u00b7her", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "'s Ist oft im Winter hier", "tokens": ["'s", "Ist", "oft", "im", "Win\u00b7ter", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als ob es Fr\u00fchling mir", "tokens": ["Als", "ob", "es", "Fr\u00fch\u00b7ling", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Immer noch w\u00e4r'.", "tokens": ["Im\u00b7mer", "noch", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Denk' ich, o Heimath, dein,", "tokens": ["Denk'", "ich", ",", "o", "Hei\u00b7math", ",", "dein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$,", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kenn' ich nicht Gram und Pein,", "tokens": ["Kenn'", "ich", "nicht", "Gram", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Kenn' ich nicht Schmerz.", "tokens": ["Kenn'", "ich", "nicht", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Tr\u00e4um' ich von dir die Nacht,", "tokens": ["Tr\u00e4um'", "ich", "von", "dir", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist mir als Bl\u00fcth' erwacht", "tokens": ["Ist", "mir", "als", "Bl\u00fcth'", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "KOUS", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Morgens das Herz.", "tokens": ["Mor\u00b7gens", "das", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}