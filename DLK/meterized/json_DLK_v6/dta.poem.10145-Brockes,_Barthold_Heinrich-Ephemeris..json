{"dta.poem.10145": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ephemeris.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' die kleinen Enlchen schweben,", "tokens": ["Ich", "seh'", "die", "klei\u00b7nen", "Enl\u00b7chen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die man Ephemeris sonst heisst;", "tokens": ["Die", "man", "E\u00b7phe\u00b7me\u00b7ris", "sonst", "heisst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "NE", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die einen einzgen Tag nur leben.", "tokens": ["Die", "ei\u00b7nen", "einz\u00b7gen", "Tag", "nur", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey dem Gesch\u00f6pfe denckt mein Geist:", "tokens": ["Bey", "dem", "Ge\u00b7sch\u00f6p\u00b7fe", "denckt", "mein", "Geist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Da aber dieses Thier, indem es munter flieget,", "tokens": ["Da", "a\u00b7ber", "die\u00b7ses", "Thier", ",", "in\u00b7dem", "es", "mun\u00b7ter", "flie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDAT", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem Ansehn nach vergn\u00fcgt ist, und sich freut;", "tokens": ["Dem", "An\u00b7sehn", "nach", "ver\u00b7gn\u00fcgt", "ist", ",", "und", "sich", "freut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVPP", "VAFIN", "$,", "KON", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So hat es, ungeacht der kurtzen Lebens-Zeit,", "tokens": ["So", "hat", "es", ",", "un\u00b7ge\u00b7acht", "der", "kurt\u00b7zen", "Le\u00b7bens\u00b7Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich l\u00e4nger auf der Welt, als mancher Mensch, vergn\u00fcget.", "tokens": ["Sich", "l\u00e4n\u00b7ger", "auf", "der", "Welt", ",", "als", "man\u00b7cher", "Mensch", ",", "ver\u00b7gn\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "$,", "KOUS", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}