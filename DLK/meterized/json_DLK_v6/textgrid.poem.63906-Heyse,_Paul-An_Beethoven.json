{"textgrid.poem.63906": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "An Beethoven", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wer gekostet hat vom Zauberkraut", "tokens": ["Wie", "wer", "ge\u00b7kos\u00b7tet", "hat", "vom", "Zau\u00b7ber\u00b7kraut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PWS", "VVPP", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nun versteht der Elemente Lallen,", "tokens": ["Und", "nun", "ver\u00b7steht", "der", "E\u00b7le\u00b7men\u00b7te", "Lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Stammeln der Natur, den Klagelaut", "tokens": ["Das", "Stam\u00b7meln", "der", "Na\u00b7tur", ",", "den", "Kla\u00b7ge\u00b7laut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der stummen Wesen, die dem Tod verfallen,", "tokens": ["Der", "stum\u00b7men", "We\u00b7sen", ",", "die", "dem", "Tod", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So gingst du durch die Welt. Dir klang vertraut,", "tokens": ["So", "gingst", "du", "durch", "die", "Welt", ".", "Dir", "klang", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was allen schwieg, und das verworrne Schallen", "tokens": ["Was", "al\u00b7len", "schwieg", ",", "und", "das", "ver\u00b7worr\u00b7ne", "Schal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vom ew'gen Flu\u00df der Dinge \u2013 deiner Seele", "tokens": ["Vom", "ew'\u00b7gen", "Flu\u00df", "der", "Din\u00b7ge", "\u2013", "dei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "War's Wohllaut, wie ein Lied aus Vogelkehle.", "tokens": ["Wa\u00b7r's", "Wohl\u00b7laut", ",", "wie", "ein", "Lied", "aus", "Vo\u00b7gel\u00b7keh\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wer das H\u00f6chst' und Fremdeste ergr\u00fcndet,", "tokens": ["Doch", "wer", "das", "H\u00f6chst'", "und", "Frem\u00b7des\u00b7te", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Fremdling wird er in der eignen Welt.", "tokens": ["Ein", "Fremd\u00b7ling", "wird", "er", "in", "der", "eig\u00b7nen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wann h\u00e4tt' ein Mensch D\u00e4monen sich verb\u00fcndet", "tokens": ["Wann", "h\u00e4tt'", "ein", "Mensch", "D\u00e4\u00b7mo\u00b7nen", "sich", "ver\u00b7b\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dann zu frohen Menschen sich gesellt?", "tokens": ["Und", "dann", "zu", "fro\u00b7hen", "Men\u00b7schen", "sich", "ge\u00b7sellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vom Schmerz des Daseins, den dein Lied verk\u00fcndet,", "tokens": ["Vom", "Schmerz", "des", "Da\u00b7seins", ",", "den", "dein", "Lied", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ward jede fl\u00fccht'ge Wonne dir verg\u00e4llt;", "tokens": ["Ward", "je\u00b7de", "fl\u00fccht'\u00b7ge", "Won\u00b7ne", "dir", "ver\u00b7g\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Einklang, der dir t\u00f6nt' im Flug der Sterne,", "tokens": ["Der", "Ein\u00b7klang", ",", "der", "dir", "t\u00f6nt'", "im", "Flug", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Blieb, wie du k\u00e4mpftest, deinem Busen ferne.", "tokens": ["Blieb", ",", "wie", "du", "k\u00e4mpf\u00b7test", ",", "dei\u00b7nem", "Bu\u00b7sen", "fer\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So r\u00e4chen sich an dem, der sie belauscht,", "tokens": ["So", "r\u00e4\u00b7chen", "sich", "an", "dem", ",", "der", "sie", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Die \u00dcberirdischen, die furchtbar Hehren.", "tokens": ["Die", "\u00dc\u00b7be\u00b7rir\u00b7di\u00b7schen", ",", "die", "furcht\u00b7bar", "Heh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer sich am Urquell alles Seins berauscht,", "tokens": ["Wer", "sich", "am", "Ur\u00b7quell", "al\u00b7les", "Seins", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Soll nicht den Becher ird'scher Freude leeren.", "tokens": ["Soll", "nicht", "den", "Be\u00b7cher", "ird'\u00b7scher", "Freu\u00b7de", "lee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Einsam, wenn alles Seel' um Seele tauscht,", "tokens": ["Ein\u00b7sam", ",", "wenn", "al\u00b7les", "Seel'", "um", "See\u00b7le", "tauscht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PIS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Mu\u00df er die Glut des eignen Herds entbehren,", "tokens": ["Mu\u00df", "er", "die", "Glut", "des", "eig\u00b7nen", "Herds", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und, ihm den Stachel recht ins Blut zu m\u00fchlen,", "tokens": ["Und", ",", "ihm", "den", "Sta\u00b7chel", "recht", "ins", "Blut", "zu", "m\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "ART", "NN", "ADJD", "APPRART", "NN", "PTKZU", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Lehrt Phantasie ihn das Versagte f\u00fchlen.", "tokens": ["Lehrt", "Phan\u00b7ta\u00b7sie", "ihn", "das", "Ver\u00b7sag\u00b7te", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er wei\u00df von allem Traulichen und S\u00fc\u00dfen,", "tokens": ["Er", "wei\u00df", "von", "al\u00b7lem", "Trau\u00b7li\u00b7chen", "und", "S\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das armer Menschen Niedrigkeit verkl\u00e4rt,", "tokens": ["Das", "ar\u00b7mer", "Men\u00b7schen", "Nied\u00b7rig\u00b7keit", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sieht in des Weibes Blick die Liebe gr\u00fc\u00dfen,", "tokens": ["Sieht", "in", "des", "Wei\u00b7bes", "Blick", "die", "Lie\u00b7be", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Treue, die das heil'ge Feuer n\u00e4hrt,", "tokens": ["Die", "Treu\u00b7e", ",", "die", "das", "heil'\u00b7ge", "Feu\u00b7er", "n\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Glauben: endlich werde siegen m\u00fcssen", "tokens": ["Den", "Glau\u00b7ben", ":", "end\u00b7lich", "wer\u00b7de", "sie\u00b7gen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "VAFIN", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Unschuld und Recht, mit Ketten selbst beschwert,", "tokens": ["Un\u00b7schuld", "und", "Recht", ",", "mit", "Ket\u00b7ten", "selbst", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und h\u00f6rt nach Kerkernacht und bangem Leide", "tokens": ["Und", "h\u00f6rt", "nach", "Ker\u00b7ker\u00b7nacht", "und", "ban\u00b7gem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Himmelsstimmen namenloser Freude.", "tokens": ["Die", "Him\u00b7mels\u00b7stim\u00b7men", "na\u00b7men\u00b7lo\u00b7ser", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So sprachst du aus in reinen Melodien,", "tokens": ["So", "sprachst", "du", "aus", "in", "rei\u00b7nen", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was du im Traum der Sehnsucht nur erfahren.", "tokens": ["Was", "du", "im", "Traum", "der", "Sehn\u00b7sucht", "nur", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den goldnen Schatz, den Ahnung dir verliehn,", "tokens": ["Den", "gold\u00b7nen", "Schatz", ",", "den", "Ah\u00b7nung", "dir", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gabst du den Gl\u00fccklichern, ihn zu bewahren.", "tokens": ["Gabst", "du", "den", "Gl\u00fcck\u00b7li\u00b7chern", ",", "ihn", "zu", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn die Neunzahl hoher Sinfonien", "tokens": ["Und", "wenn", "die", "Neun\u00b7zahl", "ho\u00b7her", "Sin\u00b7fo\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Weltgeheimnis strebt zu offenbaren,", "tokens": ["Das", "Welt\u00b7ge\u00b7heim\u00b7nis", "strebt", "zu", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Fidelio klagt und jauchzt das ewig neue", "tokens": ["Fi\u00b7de\u00b7lio", "klagt", "und", "jauchzt", "das", "e\u00b7wig", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "ART", "ADJD", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Uralte Trostlied ew'ger Lieb' und Treue.", "tokens": ["Ur\u00b7al\u00b7te", "Trost\u00b7lied", "ew'\u00b7ger", "Lieb'", "und", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dank, da\u00df du dies Verm\u00e4chtnis uns gelassen!", "tokens": ["Dank", ",", "da\u00df", "du", "dies", "Ver\u00b7m\u00e4cht\u00b7nis", "uns", "ge\u00b7las\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PDS", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon vor dem \u00dcbermenschen will den Geist", "tokens": ["Schon", "vor", "dem", "\u00dc\u00b7ber\u00b7men\u00b7schen", "will", "den", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Schwindel ehrfurchtsvollen Grauns erfassen,", "tokens": ["Ein", "Schwin\u00b7del", "ehr\u00b7furchts\u00b7vol\u00b7len", "Grauns", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ob zu hoch du unsrer Liebe seist:", "tokens": ["Als", "ob", "zu", "hoch", "du", "uns\u00b7rer", "Lie\u00b7be", "seist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PTKA", "ADJD", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da teilst du unser Dulden, Lieben, Hassen,", "tokens": ["Da", "teilst", "du", "un\u00b7ser", "Dul\u00b7den", ",", "Lie\u00b7ben", ",", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADJA", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der hohe Fremdling ist nicht mehr verwaist,", "tokens": ["Der", "ho\u00b7he", "Fremd\u00b7ling", "ist", "nicht", "mehr", "ver\u00b7waist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und der vereinsamt ging auf ird'schen Wegen,", "tokens": ["Und", "der", "ver\u00b7ein\u00b7samt", "ging", "auf", "ird'\u00b7schen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Das Herz der ", "tokens": ["Das", "Herz", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}