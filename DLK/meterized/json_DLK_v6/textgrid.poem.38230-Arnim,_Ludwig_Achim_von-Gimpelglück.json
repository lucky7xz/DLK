{"textgrid.poem.38230": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Gimpelgl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich that einmal spazieren gehn,", "tokens": ["Ich", "that", "ein\u00b7mal", "spa\u00b7zie\u00b7ren", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da h\u00f6rt ich also singen sch\u00f6n", "tokens": ["Da", "h\u00f6rt", "ich", "al\u00b7so", "sin\u00b7gen", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der V\u00f6glein viel und mancherlei,", "tokens": ["Der", "V\u00f6\u00b7glein", "viel", "und", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz lieblich war ihr Melodei;", "tokens": ["Ganz", "lieb\u00b7lich", "war", "ihr", "Me\u00b7lo\u00b7dei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da kam ich auch zu einem Nest,", "tokens": ["Da", "kam", "ich", "auch", "zu", "ei\u00b7nem", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das war geziert aufs allerbest,", "tokens": ["Das", "war", "ge\u00b7ziert", "aufs", "al\u00b7ler\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPRART", "ADJD", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Konnt mich aber nicht richten drein,", "tokens": ["Konnt", "mich", "a\u00b7ber", "nicht", "rich\u00b7ten", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Was doch dies f\u00fcr ein Nest m\u00f6cht seyn.", "tokens": ["Was", "doch", "dies", "f\u00fcr", "ein", "Nest", "m\u00f6cht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PDS", "APPR", "ART", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nahm mir drum also wohl der Weil,", "tokens": ["Nahm", "mir", "drum", "al\u00b7so", "wohl", "der", "Weil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ei da sah ich im Nest ein Eul,", "tokens": ["Ei", "da", "sah", "ich", "im", "Nest", "ein", "Eul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dieselb erzeigt sich sch\u00f6n geziert,", "tokens": ["Die\u00b7selb", "er\u00b7zeigt", "sich", "sch\u00f6n", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gro\u00df und klein V\u00f6gelein sie vexirt,", "tokens": ["Gro\u00df", "und", "klein", "V\u00f6\u00b7ge\u00b7lein", "sie", "ve\u00b7xirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "NN", "PPER", "VVPP", "$,"], "meter": "+-++-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Des must ich mirs lachen in Still,", "tokens": ["Des", "must", "ich", "mirs", "la\u00b7chen", "in", "Still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "NE", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Dieweil deren warn vorhanden viel,", "tokens": ["Die\u00b7weil", "de\u00b7ren", "warn", "vor\u00b7han\u00b7den", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und jeder wolt der N\u00e4chste seyn,", "tokens": ["Und", "je\u00b7der", "wolt", "der", "N\u00e4chs\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und durft doch keiner ins Nest hinein.", "tokens": ["Und", "durft", "doch", "kei\u00b7ner", "ins", "Nest", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Endlich gar bald ich einen ersah,", "tokens": ["End\u00b7lich", "gar", "bald", "ich", "ei\u00b7nen", "er\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "PIS", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der zu dem Nest gieng dreist und nah,", "tokens": ["Der", "zu", "dem", "Nest", "gieng", "dreist", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dieser flog geschwind hinein,", "tokens": ["Und", "die\u00b7ser", "flog", "ge\u00b7schwind", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich dacht bei mir: Wer mag dies seyn?", "tokens": ["Ich", "dacht", "bei", "mir", ":", "Wer", "mag", "dies", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "PWS", "VMFIN", "PDS", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df es ohn Scheu der andern alle,", "tokens": ["Da\u00df", "es", "ohn", "Scheu", "der", "an\u00b7dern", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "ADJA", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Eulen also wohl that gefallen,", "tokens": ["Der", "Eu\u00b7len", "al\u00b7so", "wohl", "that", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "An Federn ich ihn gleich erkannt,", "tokens": ["An", "Fe\u00b7dern", "ich", "ihn", "gleich", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df er der Gimpel ward genannt.", "tokens": ["Da\u00df", "er", "der", "Gim\u00b7pel", "ward", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie ihr nun weiter h\u00f6ren werdt,", "tokens": ["Wie", "ihr", "nun", "wei\u00b7ter", "h\u00f6\u00b7ren", "werdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Gimpel, der ist lobenswerth,", "tokens": ["Vom", "Gim\u00b7pel", ",", "der", "ist", "lo\u00b7bens\u00b7werth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum will ich jezt verhalten nicht", "tokens": ["Drum", "will", "ich", "jezt", "ver\u00b7hal\u00b7ten", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Lob in diesem kleinen Gedicht:", "tokens": ["Sein", "Lob", "in", "die\u00b7sem", "klei\u00b7nen", "Ge\u00b7dicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Der Gimpel ist ein Vogel schon,", "tokens": ["Der", "Gim\u00b7pel", "ist", "ein", "Vo\u00b7gel", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der n\u00e4chste bei den Eulen dran;", "tokens": ["Der", "n\u00e4chs\u00b7te", "bei", "den", "Eu\u00b7len", "dran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein andrer darf sich nahen frey,", "tokens": ["Kein", "an\u00b7drer", "darf", "sich", "na\u00b7hen", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VMFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hin zu dem Nest, wer es auch sey.", "tokens": ["Hin", "zu", "dem", "Nest", ",", "wer", "es", "auch", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PWS", "PPER", "ADV", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Du Gimpel aber magst nach Lust", "tokens": ["Du", "Gim\u00b7pel", "a\u00b7ber", "magst", "nach", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ADV", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei der Eule seyn ganz wohl bewu\u00dft;", "tokens": ["Bei", "der", "Eu\u00b7le", "seyn", "ganz", "wohl", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Drum ich forthin werd haben keine Ruh,", "tokens": ["Drum", "ich", "for\u00b7thin", "werd", "ha\u00b7ben", "kei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VAFIN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis da\u00df ich ein Gimpel werd wie du;", "tokens": ["Bis", "da\u00df", "ich", "ein", "Gim\u00b7pel", "werd", "wie", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ART", "NN", "VAFIN", "KOKOM", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Kein sch\u00f6nern Gimpel sah ich nie,", "tokens": ["Kein", "sch\u00f6\u00b7nern", "Gim\u00b7pel", "sah", "ich", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn dich jezt gegenw\u00e4rtig hie,", "tokens": ["Denn", "dich", "jezt", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Art bist du ganz wohl geziert,", "tokens": ["Von", "Art", "bist", "du", "ganz", "wohl", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gleichwie eim Gimpel sich geb\u00fchrt.", "tokens": ["Gleich\u00b7wie", "eim", "Gim\u00b7pel", "sich", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Magst darum wohl ein Gimpel bleiben,", "tokens": ["Magst", "da\u00b7rum", "wohl", "ein", "Gim\u00b7pel", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn dich wohl keiner wird vertreiben,", "tokens": ["Denn", "dich", "wohl", "kei\u00b7ner", "wird", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PIS", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dessen darfst dich doch f\u00fcrchten nicht,", "tokens": ["Des\u00b7sen", "darfst", "dich", "doch", "f\u00fcrch\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Denn dies wohl nimmermehr geschicht,", "tokens": ["Denn", "dies", "wohl", "nim\u00b7mer\u00b7mehr", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr rechter Gimpel du bist allein,", "tokens": ["Ihr", "rech\u00b7ter", "Gim\u00b7pel", "du", "bist", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den sie vor andern liebt gemein,", "tokens": ["Den", "sie", "vor", "an\u00b7dern", "liebt", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch wegen deines s\u00fc\u00dfen Gesangs,", "tokens": ["Auch", "we\u00b7gen", "dei\u00b7nes", "s\u00fc\u00b7\u00dfen", "Ge\u00b7sangs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Bleibst du ein Gimpel dein lebenlang.", "tokens": ["Bleibst", "du", "ein", "Gim\u00b7pel", "dein", "le\u00b7ben\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Drum billig bist du lobenswerth,", "tokens": ["Drum", "bil\u00b7lig", "bist", "du", "lo\u00b7bens\u00b7werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bleibst ein Gimpel wohl heur als sehr,", "tokens": ["Du", "bleibst", "ein", "Gim\u00b7pel", "wohl", "heur", "als", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie gern wolt ich ein Gimpel seyn,", "tokens": ["Wie", "gern", "wolt", "ich", "ein", "Gim\u00b7pel", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit ich d\u00fcrft ins Nest hinein,", "tokens": ["Da\u00b7mit", "ich", "d\u00fcrft", "ins", "Nest", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob dich schon vexirt jedermann,", "tokens": ["Ob", "dich", "schon", "ve\u00b7xirt", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So la\u00df nur Red vor Ohren gehn,", "tokens": ["So", "la\u00df", 