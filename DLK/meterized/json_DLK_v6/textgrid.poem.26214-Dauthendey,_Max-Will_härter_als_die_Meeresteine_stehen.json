{"textgrid.poem.26214": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Will h\u00e4rter als die Meeresteine stehen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei gro\u00dfe Adler schaukeln in der Morgenluft ums Schiff.", "tokens": ["Zwei", "gro\u00b7\u00dfe", "Ad\u00b7ler", "schau\u00b7keln", "in", "der", "Mor\u00b7gen\u00b7luft", "ums", "Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Vielleicht aufs Meer mein klopfend Herz die gro\u00dfen V\u00f6gel rief.", "tokens": ["Viel\u00b7leicht", "aufs", "Meer", "mein", "klop\u00b7fend", "Herz", "die", "gro\u00b7\u00dfen", "V\u00f6\u00b7gel", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "ADJD", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Die Adler m\u00f6cht' ich senden \u00fcber Ozeane zur Bruderschar", "tokens": ["Die", "Ad\u00b7ler", "m\u00f6cht'", "ich", "sen\u00b7den", "\u00fc\u00b7ber", "O\u00b7zea\u00b7ne", "zur", "Bru\u00b7der\u00b7schar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "APPR", "NE", "APPRART", "NN"], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Mit meinem hei\u00dfen Herzen hin zum deutschen Aar.", "tokens": ["Mit", "mei\u00b7nem", "hei\u00b7\u00dfen", "Her\u00b7zen", "hin", "zum", "deut\u00b7schen", "Aar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Will h\u00e4rter als die harten Meeressteine stehen", "tokens": ["Will", "h\u00e4r\u00b7ter", "als", "die", "har\u00b7ten", "Mee\u00b7res\u00b7stei\u00b7ne", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "KOKOM", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und keinen Herzschlag sp\u00fcren mehr,", "tokens": ["Und", "kei\u00b7nen", "Herz\u00b7schlag", "sp\u00fc\u00b7ren", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u2013 Sollt' Deutschland untergehen.", "tokens": ["\u2013", "Sollt'", "Deutschland", "un\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "NE", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}