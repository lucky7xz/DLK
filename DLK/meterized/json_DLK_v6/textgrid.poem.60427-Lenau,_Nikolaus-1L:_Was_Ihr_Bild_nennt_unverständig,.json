{"textgrid.poem.60427": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was Ihr Bild nennt unverst\u00e4ndig,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Ihr Bild nennt unverst\u00e4ndig,", "tokens": ["Was", "Ihr", "Bild", "nennt", "un\u00b7ver\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nur Gleichnis, kalt und hohl,", "tokens": ["Ist", "nur", "Gleich\u00b7nis", ",", "kalt", "und", "hohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der Geist nicht ein Symbol", "tokens": ["Wo", "der", "Geist", "nicht", "ein", "Sym\u00b7bol"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Sprache zeugt lebendig.", "tokens": ["Mit", "der", "Spra\u00b7che", "zeugt", "le\u00b7ben\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und das Ringlein Salomonis,", "tokens": ["Und", "das", "Rin\u00b7glein", "Sa\u00b7lo\u00b7mo\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das die Diwen zwinget ein,", "tokens": ["Das", "die", "Di\u00b7wen", "zwin\u00b7get", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zauberm\u00e4chtig, es ist kein", "tokens": ["Zau\u00b7ber\u00b7m\u00e4ch\u00b7tig", ",", "es", "ist", "kein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PPER", "VAFIN", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tertium comparationis.", "tokens": ["Ter\u00b7ti\u00b7um", "com\u00b7pa\u00b7ra\u00b7ti\u00b7o\u00b7nis", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}