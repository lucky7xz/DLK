{"textgrid.poem.66051": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus gr\u00fcnem Waldesd\u00e4mmerdunkel", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus gr\u00fcnem Waldesd\u00e4mmerdunkel", "tokens": ["Aus", "gr\u00fc\u00b7nem", "Wal\u00b7des\u00b7d\u00e4m\u00b7mer\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tret pl\u00f6tzlich ich in helles Licht,", "tokens": ["Tret", "pl\u00f6tz\u00b7lich", "ich", "in", "hel\u00b7les", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da gr\u00fc\u00dft aus goldnem Glanzgefunkel", "tokens": ["Da", "gr\u00fc\u00dft", "aus", "gold\u00b7nem", "Glanz\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich ein entz\u00fcckendes Gedicht:", "tokens": ["Mich", "ein", "ent\u00b7z\u00fc\u00b7cken\u00b7des", "Ge\u00b7dicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Marmorhaus in lauter Rosen,", "tokens": ["Ein", "Mar\u00b7mor\u00b7haus", "in", "lau\u00b7ter", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein S\u00e4ulenrund, wo Schaft und Schaft", "tokens": ["Ein", "S\u00e4u\u00b7len\u00b7rund", ",", "wo", "Schaft", "und", "Schaft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verstrickt in eines leichten losen", "tokens": ["Ver\u00b7strickt", "in", "ei\u00b7nes", "leich\u00b7ten", "lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gerankes holder Liebeshaft.", "tokens": ["Ge\u00b7ran\u00b7kes", "hol\u00b7der", "Lie\u00b7bes\u00b7haft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und in der stillen Tempelgrotte", "tokens": ["Und", "in", "der", "stil\u00b7len", "Tem\u00b7pel\u00b7grot\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hebt sich ein schlankes Postament,", "tokens": ["Hebt", "sich", "ein", "schlan\u00b7kes", "Pos\u00b7ta\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darauf sternblank dem Liebesgotte", "tokens": ["Da\u00b7rauf", "stern\u00b7blank", "dem", "Lie\u00b7bes\u00b7got\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Erzbild in der Sonne brennt.", "tokens": ["Ein", "Erz\u00b7bild", "in", "der", "Son\u00b7ne", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Pfeil auf dem erhobenen Bogen,", "tokens": ["Den", "Pfeil", "auf", "dem", "er\u00b7ho\u00b7be\u00b7nen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dar\u00fcber er sein Ziel er\u00e4ugt,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "er", "sein", "Ziel", "er\u00b7\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Steht er, die Sehne straff gezogen", "tokens": ["Steht", "er", ",", "die", "Seh\u00b7ne", "straff", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Schu\u00df, ein wenig vorgebeugt.", "tokens": ["Zum", "Schu\u00df", ",", "ein", "we\u00b7nig", "vor\u00b7ge\u00b7beugt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und vorn an des Geschosses Spitze,", "tokens": ["Und", "vorn", "an", "des", "Ge\u00b7schos\u00b7ses", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie man den Heiligen Opfer bringt,", "tokens": ["Wie", "man", "den", "Hei\u00b7li\u00b7gen", "Op\u00b7fer", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "An einer schlichten, wollnen Litze", "tokens": ["An", "ei\u00b7ner", "schlich\u00b7ten", ",", "woll\u00b7nen", "Lit\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein w\u00e4chsern Herz im Winde schwingt.", "tokens": ["Ein", "w\u00e4ch\u00b7sern", "Herz", "im", "Win\u00b7de", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das zeigt von warmen Fingermalen", "tokens": ["Das", "zeigt", "von", "war\u00b7men", "Fin\u00b7ger\u00b7ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im weichen Wachs ein Konterfei,", "tokens": ["Im", "wei\u00b7chen", "Wachs", "ein", "Kon\u00b7ter\u00b7fei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und eine Spur, als ob in Qualen", "tokens": ["Und", "ei\u00b7ne", "Spur", ",", "als", "ob", "in", "Qua\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "KOKOM", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Weinen drauf gefallen sei.", "tokens": ["Ein", "Wei\u00b7nen", "drauf", "ge\u00b7fal\u00b7len", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und eine abgepfl\u00fcckte Rose,", "tokens": ["Und", "ei\u00b7ne", "ab\u00b7ge\u00b7pfl\u00fcck\u00b7te", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein verlornes Liebespfand,", "tokens": ["Wie", "ein", "ver\u00b7lor\u00b7nes", "Lie\u00b7be\u00b7spfand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liegt da, und Stapfen rings im Moose", "tokens": ["Liegt", "da", ",", "und", "Stap\u00b7fen", "rings", "im", "Moo\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KON", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und weiterhin im gl\u00fchenden Sand.", "tokens": ["Und", "wei\u00b7ter\u00b7hin", "im", "gl\u00fc\u00b7hen\u00b7den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die tauchen in die Buchenschatten", "tokens": ["Die", "tau\u00b7chen", "in", "die", "Bu\u00b7chen\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und finden ungesehn nach Haus,", "tokens": ["Und", "fin\u00b7den", "un\u00b7ge\u00b7sehn", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und niemals plaudern diese Matten", "tokens": ["Und", "nie\u00b7mals", "plau\u00b7dern", "die\u00b7se", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das z\u00e4rtliche Geheimnis aus.", "tokens": ["Das", "z\u00e4rt\u00b7li\u00b7che", "Ge\u00b7heim\u00b7nis", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und einsam in des Mittags Gluten", "tokens": ["Und", "ein\u00b7sam", "in", "des", "Mit\u00b7tags", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Pfeil des Gottes schmilzt das Herz", "tokens": ["Am", "Pfeil", "des", "Got\u00b7tes", "schmilzt", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tropft, ein langsames Verbluten,", "tokens": ["Und", "tropft", ",", "ein", "lang\u00b7sa\u00b7mes", "Ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In roten Tr\u00e4nen niederw\u00e4rts,", "tokens": ["In", "ro\u00b7ten", "Tr\u00e4\u00b7nen", "nie\u00b7der\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und tropft in roten, hei\u00dfen Tr\u00e4nen", "tokens": ["Und", "tropft", "in", "ro\u00b7ten", ",", "hei\u00b7\u00dfen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf wei\u00dfen Marmors kalt Geleucht,", "tokens": ["Auf", "wei\u00b7\u00dfen", "Mar\u00b7mors", "kalt", "Ge\u00b7leucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von ungestillter Liebe Sehnen", "tokens": ["Von", "un\u00b7ge\u00b7still\u00b7ter", "Lie\u00b7be", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein r\u00fchrend Gleichnis, wie mir deucht.", "tokens": ["Ein", "r\u00fch\u00b7rend", "Gleich\u00b7nis", ",", "wie", "mir", "deucht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}