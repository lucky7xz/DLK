{"textgrid.poem.43695": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eines schickt sich nicht f\u00fcr Alle", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eichk\u00e4tzchen, machst dir gro\u00dfe Last:", "tokens": ["Eich\u00b7k\u00e4tz\u00b7chen", ",", "machst", "dir", "gro\u00b7\u00dfe", "Last", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Was h\u00fcpfst du so in wilder Hast", "tokens": ["Was", "h\u00fcpfst", "du", "so", "in", "wil\u00b7der", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem Ast zum andern Ast?", "tokens": ["Von", "ei\u00b7nem", "Ast", "zum", "an\u00b7dern", "Ast", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was willst du doch im Walde hier?", "tokens": ["Was", "willst", "du", "doch", "im", "Wal\u00b7de", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach unserm Hause komm mit mir!", "tokens": ["Nach", "un\u00b7serm", "Hau\u00b7se", "komm", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tagt\u00e4glich geb' ich N\u00fcsse dir. \u2013", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "geb'", "ich", "N\u00fcs\u00b7se", "dir", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbund bietest du mir Gut und Geld,", "tokens": ["\u00bb", "und", "bie\u00b7test", "du", "mir", "Gut", "und", "Geld", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "PPER", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bleibe, wo's mir wohlgef\u00e4llt:", "tokens": ["Ich", "blei\u00b7be", ",", "wo's", "mir", "wohl\u00b7ge\u00b7f\u00e4llt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lieb Kind, der Wald ist meine Welt.", "tokens": ["Lieb", "Kind", ",", "der", "Wald", "ist", "mei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier kann ich tanzen kreuz und quer,", "tokens": ["Hier", "kann", "ich", "tan\u00b7zen", "kreuz", "und", "quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frei bin ich hier, was will ich mehr?", "tokens": ["Frei", "bin", "ich", "hier", ",", "was", "will", "ich", "mehr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lieb Kind, leb wohl! ich danke sehr.\u00ab", "tokens": ["Lieb", "Kind", ",", "leb", "wohl", "!", "ich", "dan\u00b7ke", "sehr", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}