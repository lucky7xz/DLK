{"dta.poem.12921": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die mann-gierige Fulvia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So offt als Fulvia von ihrem fenster blickt,", "tokens": ["So", "offt", "als", "Ful\u00b7via", "von", "ih\u00b7rem", "fens\u00b7ter", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und einen s\u00fc\u00dfen strahl auf unsre stra\u00dfe schickt,", "tokens": ["Und", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "strahl", "auf", "uns\u00b7re", "stra\u00b7\u00dfe", "schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So offte denck ich auch: Die blum\u2019 ist reiff zum pfl\u00fccken;", "tokens": ["So", "off\u00b7te", "denck", "ich", "auch", ":", "Die", "blum'", "ist", "reiff", "zum", "pfl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "ADV", "$.", "ART", "NN", "VAFIN", "ADJD", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst w\u00fcrde sie ihr haupt nicht nach der erde b\u00fccken.", "tokens": ["Sonst", "w\u00fcr\u00b7de", "sie", "ihr", "haupt", "nicht", "nach", "der", "er\u00b7de", "b\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}