{"textgrid.poem.45918": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[trippel Trippel trap, trap, trap]", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trippel Trippel trap, trap, trap", "tokens": ["Trip\u00b7pel", "Trip\u00b7pel", "trap", ",", "trap", ",", "trap"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "VVFIN", "$,", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heut schlie\u00df' ich die T\u00fcr' nicht ab", "tokens": ["Heut", "schlie\u00df'", "ich", "die", "T\u00fcr'", "nicht", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "PTKVZ"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wenn ich dich erst bei mir hab'", "tokens": ["Wenn", "ich", "dich", "erst", "bei", "mir", "hab'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcss' ich dich recht t\u00fcchtig ab.", "tokens": ["K\u00fcss'", "ich", "dich", "recht", "t\u00fcch\u00b7tig", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weck' mir nicht die Mutter auf", "tokens": ["Weck'", "mir", "nicht", "die", "Mut\u00b7ter", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur nicht hust', nicht nies', nicht schnauf',", "tokens": ["Nur", "nicht", "hust'", ",", "nicht", "nies'", ",", "nicht", "schnauf'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "$,", "PTKNEG", "PTKVZ", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht zu stolz renn' mir herauf,", "tokens": ["Nicht", "zu", "stolz", "renn'", "mir", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer hoff\u00e4rtig f\u00e4llt leicht drauf.", "tokens": ["Wer", "hof\u00b7f\u00e4r\u00b7tig", "f\u00e4llt", "leicht", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Weck' mir nicht die Martinsgans,", "tokens": ["Weck'", "mir", "nicht", "die", "Mar\u00b7tins\u00b7gans", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt dem Hund nicht auf den Schwanz,", "tokens": ["Tritt", "dem", "Hund", "nicht", "auf", "den", "Schwanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schleiche wie der Mondenglanz,", "tokens": ["Schlei\u00b7che", "wie", "der", "Mon\u00b7den\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Floh im Hochzeitskranz.", "tokens": ["Wie", "ein", "Floh", "im", "Hoch\u00b7zeits\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sto\u00df' mir nicht die K\u00fcbel um", "tokens": ["Sto\u00df'", "mir", "nicht", "die", "K\u00fc\u00b7bel", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebster Schatz, ich bitt' dich drum", "tokens": ["Liebs\u00b7ter", "Schatz", ",", "ich", "bitt'", "dich", "drum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PRF", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rumpelt er rumpidipum", "tokens": ["Rum\u00b7pelt", "er", "rum\u00b7pi\u00b7di\u00b7pum"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Liebster Schatz, das w\u00e4re dumm.", "tokens": ["Liebs\u00b7ter", "Schatz", ",", "das", "w\u00e4\u00b7re", "dumm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und vor allem ich dich bitt'", "tokens": ["Und", "vor", "al\u00b7lem", "ich", "dich", "bitt'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "PPER", "PRF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Treppe in der Mitt'", "tokens": ["Auf", "der", "Trep\u00b7pe", "in", "der", "Mitt'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mache einen gro\u00dfen Schritt", "tokens": ["Ma\u00b7che", "ei\u00b7nen", "gro\u00b7\u00dfen", "Schritt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von vier Stufen fehlt die dritt'.", "tokens": ["Von", "vier", "Stu\u00b7fen", "fehlt", "die", "dritt'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In das Maul nimm deine Schuh'", "tokens": ["In", "das", "Maul", "nimm", "dei\u00b7ne", "Schuh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVIMP", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6mmt die Magd, so fahr' drauf zu", "tokens": ["K\u00f6mmt", "die", "Magd", ",", "so", "fahr'", "drauf", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PAV", "PTKZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann glaubt sie, du seist Wu Wu", "tokens": ["Dann", "glaubt", "sie", ",", "du", "seist", "Wu", "Wu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "NE", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kriecht ins Bett und l\u00e4\u00dft uns Ruh'.", "tokens": ["Kriecht", "ins", "Bett", "und", "l\u00e4\u00dft", "uns", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gehe links, ach geh nicht recht", "tokens": ["Ge\u00b7he", "links", ",", "ach", "geh", "nicht", "recht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "ITJ", "VVFIN", "PTKNEG", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonst k\u00f6mmst du zum Oberknecht", "tokens": ["Sonst", "k\u00f6mmst", "du", "zum", "O\u00b7berk\u00b7necht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und da kriegst du ein Gefecht", "tokens": ["Und", "da", "kriegst", "du", "ein", "Ge\u00b7fecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und der Jockel schmei\u00dft nicht schlecht.", "tokens": ["Und", "der", "Jo\u00b7ckel", "schmei\u00dft", "nicht", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Steig auch nicht bis unters Dach", "tokens": ["Steig", "auch", "nicht", "bis", "un\u00b7ters", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6mmst du in das Taubenfach,", "tokens": ["K\u00f6mmst", "du", "in", "das", "Tau\u00b7ben\u00b7fach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da wird gleich mein Bruder wach,", "tokens": ["Da", "wird", "gleich", "mein", "Bru\u00b7der", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eilet schnell dem Marder nach.", "tokens": ["Ei\u00b7let", "schnell", "dem", "Mar\u00b7der", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Bist du vor der Kammert\u00fcr", "tokens": ["Bist", "du", "vor", "der", "Kam\u00b7mer\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Klage deinen Jammer mir,", "tokens": ["Kla\u00b7ge", "dei\u00b7nen", "Jam\u00b7mer", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann schieb' ich die Klammer f\u00fcr", "tokens": ["Dann", "schieb'", "ich", "die", "Klam\u00b7mer", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schrei', wer ist, Potz Hammer, hier.", "tokens": ["Schrei'", ",", "wer", "ist", ",", "Potz", "Ham\u00b7mer", ",", "hier", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PWS", "VAFIN", "$,", "NE", "NE", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und da wachet alles auf", "tokens": ["Und", "da", "wa\u00b7chet", "al\u00b7les", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutter, Bruder, Knecht im Lauf", "tokens": ["Mut\u00b7ter", ",", "Bru\u00b7der", ",", "Knecht", "im", "Lauf"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nahn, es wird 'ne Pr\u00fcgeltrauf", "tokens": ["Nahn", ",", "es", "wird", "'ne", "Pr\u00fc\u00b7gel\u00b7trauf"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Besser als 'ne Kindertauf.", "tokens": ["Bes\u00b7ser", "als", "'ne", "Kin\u00b7der\u00b7tauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch es gieng 'nen andern Gang,", "tokens": ["Doch", "es", "gieng", "'nen", "an\u00b7dern", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutter nach neun Monden sang", "tokens": ["Mut\u00b7ter", "nach", "neun", "Mon\u00b7den", "sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "CARD", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4del, 's wird mir angst und bang,", "tokens": ["M\u00e4\u00b7del", ",", "'s", "wird", "mir", "angst", "und", "bang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sonst war ja dein R\u00f6ckchen lang.", "tokens": ["Sonst", "war", "ja", "dein", "R\u00f6ck\u00b7chen", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}