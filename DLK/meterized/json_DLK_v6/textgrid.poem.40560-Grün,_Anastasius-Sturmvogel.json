{"textgrid.poem.40560": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Sturmvogel", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Gewande der Trauer", "tokens": ["Im", "Ge\u00b7wan\u00b7de", "der", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Schreit' ich \u00fcber die Meere,", "tokens": ["Schreit'", "ich", "\u00fc\u00b7ber", "die", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Aufrecht, wie einst der Glaube", "tokens": ["Auf\u00b7recht", ",", "wie", "einst", "der", "Glau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schritt zum Nachen des Herrn.", "tokens": ["Schritt", "zum", "Na\u00b7chen", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Unterm Fl\u00fcgel die K\u00fcchlein", "tokens": ["Un\u00b7term", "Fl\u00fc\u00b7gel", "die", "K\u00fcch\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Br\u00fct' ich, und wie den Glauben,", "tokens": ["Br\u00fct'", "ich", ",", "und", "wie", "den", "Glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Tr\u00e4gt den Schmerz auch die Welle,", "tokens": ["Tr\u00e4gt", "den", "Schmerz", "auch", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Tr\u00e4gt auch des Schmerzes Brut.", "tokens": ["Tr\u00e4gt", "auch", "des", "Schmer\u00b7zes", "Brut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Fern dort gleitet ein Schifflein,", "tokens": ["Fern", "dort", "glei\u00b7tet", "ein", "Schif\u00b7flein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Jubelnd mit Bechern und Harfen,", "tokens": ["Ju\u00b7belnd", "mit", "Be\u00b7chern", "und", "Har\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Gr\u00fc\u00dfend mit Wimpfeln und Flaggen!", "tokens": ["Gr\u00fc\u00b7\u00dfend", "mit", "Wim\u00b7pfeln", "und", "Flag\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schonst du der Lust auch, o Meer?", "tokens": ["Schonst", "du", "der", "Lust", "auch", ",", "o", "Meer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "FM", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "H\u00e4tt'st du, Schifflein, mein Auge,", "tokens": ["H\u00e4tt'st", "du", ",", "Schif\u00b7flein", ",", "mein", "Au\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "In die Tiefe zu blicken,", "tokens": ["In", "die", "Tie\u00b7fe", "zu", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dir verstummten die Harfen,", "tokens": ["Dir", "ver\u00b7stumm\u00b7ten", "die", "Har\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dir ents\u00e4nke die Fahn'!", "tokens": ["Dir", "ent\u00b7s\u00e4n\u00b7ke", "die", "Fahn'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Wie langweilt ihr mich wieder:", "tokens": ["Wie", "lang\u00b7weilt", "ihr", "mich", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schweigende Meeresruhe,", "tokens": ["Schwei\u00b7gen\u00b7de", "Mee\u00b7res\u00b7ru\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Endlose todte Haide,", "tokens": ["End\u00b7lo\u00b7se", "tod\u00b7te", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ewiger Sonnenschein.", "tokens": ["E\u00b7wi\u00b7ger", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Vater Sturm, dich beschw\u00f6r' ich", "tokens": ["Va\u00b7ter", "Sturm", ",", "dich", "be\u00b7schw\u00f6r'", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "PPER"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und gebiete dir, hauche", "tokens": ["Und", "ge\u00b7bie\u00b7te", "dir", ",", "hau\u00b7che"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Scharfen, st\u00e4hlenden Nordhauch", "tokens": ["Schar\u00b7fen", ",", "st\u00e4h\u00b7len\u00b7den", "Nord\u00b7hauch"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Meinen Jungen ums Herz!", "tokens": ["Mei\u00b7nen", "Jun\u00b7gen", "ums", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "La\u00df durchwandeln mich jauchzend", "tokens": ["La\u00df", "durch\u00b7wan\u00b7deln", "mich", "jauch\u00b7zend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PPER", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Gr\u00fcnenden Wellenh\u00fcgel,", "tokens": ["Gr\u00fc\u00b7nen\u00b7den", "Wel\u00b7len\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dessen Gipfel ein Garten", "tokens": ["Des\u00b7sen", "Gip\u00b7fel", "ein", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wei\u00dfer Bl\u00fcthen umsch\u00e4umt!", "tokens": ["Wei\u00b7\u00dfer", "Bl\u00fc\u00b7then", "um\u00b7sch\u00e4umt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "La\u00df mich klimmen frohlockend", "tokens": ["La\u00df", "mich", "klim\u00b7men", "froh\u00b7lo\u00b7ckend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ueber wogende Alpen,", "tokens": ["Ue\u00b7ber", "wo\u00b7gen\u00b7de", "Al\u00b7pen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren H\u00e4upter die Brandung", "tokens": ["De\u00b7ren", "H\u00e4up\u00b7ter", "die", "Bran\u00b7dung"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Kr\u00f6nt mit ewigem Schnee!", "tokens": ["Kr\u00f6nt", "mit", "e\u00b7wi\u00b7gem", "Schnee", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Spalte die Tiefen der Fluthen,", "tokens": ["Spal\u00b7te", "die", "Tie\u00b7fen", "der", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Da\u00df am Grunde die Leiche", "tokens": ["Da\u00df", "am", "Grun\u00b7de", "die", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wieder k\u00fcsse den Lichthauch,", "tokens": ["Wie\u00b7der", "k\u00fcs\u00b7se", "den", "Licht\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sauge die Schimmer des Tags!", "tokens": ["Sau\u00b7ge", "die", "Schim\u00b7mer", "des", "Tags", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Tr\u00e4gst du gleich mir, o Schifflein,", "tokens": ["Tr\u00e4gst", "du", "gleich", "mir", ",", "o", "Schif\u00b7flein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Liebe Brut unterm Fittig,", "tokens": ["Lie\u00b7be", "Brut", "un\u00b7term", "Fit\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Kinder der Lust, die das Meer nicht", "tokens": ["Kin\u00b7der", "der", "Lust", ",", "die", "das", "Meer", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "PTKNEG"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schont, wie die Kinder vom Schmerz?", "tokens": ["Schont", ",", "wie", "die", "Kin\u00b7der", "vom", "Schmerz", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.11": {"line.1": {"text": "Will dich warnend umkreisen,", "tokens": ["Will", "dich", "war\u00b7nend", "um\u00b7krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Rufen vom Mast dir: Wehe!", "tokens": ["Ru\u00b7fen", "vom", "Mast", "dir", ":", "We\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "$.", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Schreien vom Kiel dir: Wehe!", "tokens": ["Schrei\u00b7en", "vom", "Kiel", "dir", ":", "We\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "$.", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ob auch das Herz mir jauchzt.", "tokens": ["Ob", "auch", "das", "Herz", "mir", "jauchzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ha, die Harfen verstummen", "tokens": ["Ha", ",", "die", "Har\u00b7fen", "ver\u00b7stum\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und die Becher, sie sinken,", "tokens": ["Und", "die", "Be\u00b7cher", ",", "sie", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und die Segel, sie fallen,", "tokens": ["Und", "die", "Se\u00b7gel", ",", "sie", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bleich ist der jubelnde Mund!", "tokens": ["Bleich", "ist", "der", "ju\u00b7beln\u00b7de", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Blitz, nun flattre dein Wimpel,", "tokens": ["Blitz", ",", "nun", "flatt\u00b7re", "dein", "Wim\u00b7pel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Donner, r\u00fchre die Harfe,", "tokens": ["Don\u00b7ner", ",", "r\u00fch\u00b7re", "die", "Har\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sturm, nimm mich in die Arme,", "tokens": ["Sturm", ",", "nimm", "mich", "in", "die", "Ar\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wieg' in Wonne dein Kind!", "tokens": ["Wieg'", "in", "Won\u00b7ne", "dein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}