{"textgrid.poem.61272": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sah den Wald sich f\u00e4rben,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah den Wald sich f\u00e4rben,", "tokens": ["Ich", "sah", "den", "Wald", "sich", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Luft war grau und stumm;", "tokens": ["Die", "Luft", "war", "grau", "und", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir war betr\u00fcbt zum Sterben,", "tokens": ["Mir", "war", "be\u00b7tr\u00fcbt", "zum", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wu\u00dft' es kaum, warum.", "tokens": ["Und", "wu\u00dft'", "es", "kaum", ",", "wa\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Durchs Feld vom Herbstgest\u00e4ude", "tokens": ["Durchs", "Feld", "vom", "Herbst\u00b7ge\u00b7st\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hertrieb das d\u00fcrre Laub;", "tokens": ["Her\u00b7trieb", "das", "d\u00fcr\u00b7re", "Laub", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da dacht' ich: Deine Freude", "tokens": ["Da", "dacht'", "ich", ":", "Dei\u00b7ne", "Freu\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ward so des Windes Raub.", "tokens": ["Ward", "so", "des", "Win\u00b7des", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dein Lenz, der bl\u00fctenvolle,", "tokens": ["Dein", "Lenz", ",", "der", "bl\u00fc\u00b7ten\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein reicher Sommer schwand;", "tokens": ["Dein", "rei\u00b7cher", "Som\u00b7mer", "schwand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An die gefrorne Scholle", "tokens": ["An", "die", "ge\u00b7fror\u00b7ne", "Schol\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bist du nun festgebannt.", "tokens": ["Bist", "du", "nun", "fest\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da pl\u00f6tzlich flo\u00df ein klares", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "flo\u00df", "ein", "kla\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Get\u00f6n in L\u00fcften hoch:", "tokens": ["Ge\u00b7t\u00f6n", "in", "L\u00fcf\u00b7ten", "hoch", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Wandervogel war es,", "tokens": ["Ein", "Wan\u00b7der\u00b7vo\u00b7gel", "war", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der nach dem S\u00fcden zog.", "tokens": ["Der", "nach", "dem", "S\u00fc\u00b7den", "zog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Ach, wie der Schlag der Schwingen,", "tokens": ["Ach", ",", "wie", "der", "Schlag", "der", "Schwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Lied ins Ohr mir kam,", "tokens": ["Das", "Lied", "ins", "Ohr", "mir", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchlt ich's wie Trost mir dringen", "tokens": ["F\u00fchlt", "ich's", "wie", "Trost", "mir", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "KOKOM", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Herzen wundersam.", "tokens": ["Zum", "Her\u00b7zen", "wun\u00b7der\u00b7sam", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es mahnt' aus heller Kehle", "tokens": ["Es", "mahnt'", "aus", "hel\u00b7ler", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich ja der fl\u00fccht'ge Gast:", "tokens": ["Mich", "ja", "der", "fl\u00fccht'\u00b7ge", "Gast", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergi\u00df, o Menschenseele,", "tokens": ["Ver\u00b7gi\u00df", ",", "o", "Men\u00b7schen\u00b7see\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht, da\u00df du Fl\u00fcgel hast!", "tokens": ["Nicht", ",", "da\u00df", "du", "Fl\u00fc\u00b7gel", "hast", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}