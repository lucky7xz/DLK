{"dta.poem.12662": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die tugendhaffte Lidia.  \n S. D.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf! ihr meine g\u00f6ldne seiten!", "tokens": ["Auf", "!", "ihr", "mei\u00b7ne", "g\u00f6ld\u00b7ne", "sei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Raffet meinen geist von hier,", "tokens": ["Raf\u00b7fet", "mei\u00b7nen", "geist", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lidia will neben mir", "tokens": ["Li\u00b7dia", "will", "ne\u00b7ben", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "APPR", "PPER"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Uber lufft und himmel schreiten,", "tokens": ["U\u00b7ber", "lufft", "und", "him\u00b7mel", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist durch meiner sinnen macht", "tokens": ["Ist", "durch", "mei\u00b7ner", "sin\u00b7nen", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf ein ewig lob bedacht.", "tokens": ["Auf", "ein", "e\u00b7wig", "lob", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie erkennt, da\u00df pracht und jugend", "tokens": ["Sie", "er\u00b7kennt", ",", "da\u00df", "pracht", "und", "ju\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein dampff verrauchen mu\u00df,", "tokens": ["Wie", "ein", "dampff", "ver\u00b7rau\u00b7chen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum stellt sie ihren fu\u00df", "tokens": ["Da\u00b7rum", "stellt", "sie", "ih\u00b7ren", "fu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den pfad standhaffter tugend,", "tokens": ["Auf", "den", "pfad", "stand\u00b7haff\u00b7ter", "tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Will durch meiner gaben schein", "tokens": ["Will", "durch", "mei\u00b7ner", "ga\u00b7ben", "schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jmmer jung und sch\u00f6ne seyn.", "tokens": ["Jm\u00b7mer", "jung", "und", "sch\u00f6\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJA", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schau, ich reisse mich von hinnen!", "tokens": ["Schau", ",", "ich", "reis\u00b7se", "mich", "von", "hin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey beseelt, o meine hand!", "tokens": ["Sey", "be\u00b7seelt", ",", "o", "mei\u00b7ne", "hand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "FM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fleuch, du feuriger verstand", "tokens": ["Fleuch", ",", "du", "feu\u00b7ri\u00b7ger", "ver\u00b7stand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Uber des gestirnes zinnen,", "tokens": ["U\u00b7ber", "des", "ge\u00b7stir\u00b7nes", "zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Suche da hinauf zu gehn,", "tokens": ["Su\u00b7che", "da", "hin\u00b7auf", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo di\u00df sch\u00f6ne mensch soll stehn.", "tokens": ["Wo", "di\u00df", "sch\u00f6\u00b7ne", "mensch", "soll", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jhre sonuen-rothe wangen,", "tokens": ["Ih\u00b7re", "so\u00b7nu\u00b7en\u00b7ro\u00b7the", "wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Jhrer augen g\u00f6ldnes licht", "tokens": ["Ih\u00b7rer", "au\u00b7gen", "g\u00f6ld\u00b7nes", "licht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr himmel-rund gesicht,", "tokens": ["Und", "ihr", "him\u00b7mel\u00b7rund", "ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll hie neue pracht erlangen,", "tokens": ["Soll", "hie", "neu\u00b7e", "pracht", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Pracht, die ewig nicht verbl\u00fcht", "tokens": ["Pracht", ",", "die", "e\u00b7wig", "nicht", "ver\u00b7bl\u00fcht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD", "PTKNEG", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und nicht herbst noch winter sieht.", "tokens": ["Und", "nicht", "herbst", "noch", "win\u00b7ter", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Freue dich, du prei\u00df der sch\u00f6nen!", "tokens": ["Freu\u00b7e", "dich", ",", "du", "prei\u00df", "der", "sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hie soll deiner gaben schaar", "tokens": ["Hie", "soll", "dei\u00b7ner", "ga\u00b7ben", "schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich vor aller zeit gefahr", "tokens": ["Sich", "vor", "al\u00b7ler", "zeit", "ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der ewigkeit becr\u00f6nen:", "tokens": ["Mit", "der", "e\u00b7wig\u00b7keit", "be\u00b7cr\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine feindliche gewalt", "tokens": ["Kei\u00b7ne", "feind\u00b7li\u00b7che", "ge\u00b7walt"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll dir rauben die gestalt.", "tokens": ["Soll", "dir", "rau\u00b7ben", "die", "ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dieses, was ich von dir schreibe,", "tokens": ["Die\u00b7ses", ",", "was", "ich", "von", "dir", "schrei\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hebt mein Ph\u00f6bus selber auf,", "tokens": ["Hebt", "mein", "Ph\u00f6\u00b7bus", "sel\u00b7ber", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df es von der zeiten lauff", "tokens": ["Da\u00df", "es", "von", "der", "zei\u00b7ten", "lauff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig unbetastet bleibe,", "tokens": ["E\u00b7wig", "un\u00b7be\u00b7tas\u00b7tet", "blei\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Legt es bey, wo gluth und wind", "tokens": ["Legt", "es", "bey", ",", "wo", "gluth", "und", "wind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "NN", "KON", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Erd und see verbannet sind.", "tokens": ["Erd", "und", "see", "ver\u00b7ban\u00b7net", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Starcke w\u00e4lle, th\u00fcrn und mauren", "tokens": ["Star\u00b7cke", "w\u00e4l\u00b7le", ",", "th\u00fcrn", "und", "mau\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fallen mit den jahren ein,", "tokens": ["Fal\u00b7len", "mit", "den", "jah\u00b7ren", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ertzt und eisen, stahl und stein,", "tokens": ["Ertzt", "und", "ei\u00b7sen", ",", "stahl", "und", "stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnen vor der zeit nicht tauren,", "tokens": ["K\u00f6n\u00b7nen", "vor", "der", "zeit", "nicht", "tau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber deine pracht und zier", "tokens": ["A\u00b7ber", "dei\u00b7ne", "pracht", "und", "zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lidia, bleibt f\u00fcr und f\u00fcr.", "tokens": ["Li\u00b7dia", ",", "bleibt", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}