{"textgrid.poem.54059": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Ortskrankenkasse", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich komme in eine fremde Stadt", "tokens": ["Ich", "kom\u00b7me", "in", "ei\u00b7ne", "frem\u00b7de", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u2013 Kasolz oder Ober-Crammin \u2013", "tokens": ["\u2013", "Ka\u00b7solz", "o\u00b7der", "O\u00b7ber\u00b7Cram\u00b7min", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "KON", "NN", "$("], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.3": {"text": "und nehme im Hotel ein Bad,", "tokens": ["und", "neh\u00b7me", "im", "Ho\u00b7tel", "ein", "Bad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "dann tu ich den Mantel anziehn", "tokens": ["dann", "tu", "ich", "den", "Man\u00b7tel", "an\u00b7ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVIZU"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "und gehe durch den fremden Ort", "tokens": ["und", "ge\u00b7he", "durch", "den", "frem\u00b7den", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "an L\u00e4den und Kirchen vorbei", "tokens": ["an", "L\u00e4\u00b7den", "und", "Kir\u00b7chen", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "und gucke hier und da und dort", "tokens": ["und", "gu\u00b7cke", "hier", "und", "da", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und seh eine Metzgerei,", "tokens": ["und", "seh", "ei\u00b7ne", "Metz\u00b7ge\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "das Postamt . . . eine Bilderschau . . .", "tokens": ["das", "Post\u00b7amt", ".", ".", ".", "ei\u00b7ne", "Bil\u00b7der\u00b7schau", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$.", "$.", "$.", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "und immer, in jeder Stadt,", "tokens": ["und", "im\u00b7mer", ",", "in", "je\u00b7der", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "steht ein gro\u00dfer, pr\u00e4chtiger, neuer Bau,", "tokens": ["steht", "ein", "gro\u00b7\u00dfer", ",", "pr\u00e4ch\u00b7ti\u00b7ger", ",", "neu\u00b7er", "Bau", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "den man grade errichtet hat.", "tokens": ["den", "man", "gra\u00b7de", "er\u00b7rich\u00b7tet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVPP", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Und dann frag ich. Und in jeder Stadt,", "tokens": ["Und", "dann", "frag", "ich", ".", "Und", "in", "je\u00b7der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "KON", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "die einen turnenden Schutzmann hat,", "tokens": ["die", "ei\u00b7nen", "tur\u00b7nen\u00b7den", "Schutz\u00b7mann", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "sagt er auf, wie das brave Kind in der Klasse:", "tokens": ["sagt", "er", "auf", ",", "wie", "das", "bra\u00b7ve", "Kind", "in", "der", "Klas\u00b7se", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "\u00bbdas? ist die neue Ortskrankenkasse.\u00ab", "tokens": ["\u00bb", "das", "?", "ist", "die", "neu\u00b7e", "Orts\u00b7kran\u00b7ken\u00b7kas\u00b7se", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "$.", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So ein gro\u00dfes Haus . . . ! Sieh mal einer an . . . !", "tokens": ["So", "ein", "gro\u00b7\u00dfes", "Haus", ".", ".", ".", "!", "Sieh", "mal", "ei\u00b7ner", "an", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "$.", "$.", "$.", "NE", "ADV", "PIS", "PTKVZ", "$.", "$.", "$.", "$."], "meter": "--+-++-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Ein riesiger Kasten. Ja, wer so kann!", "tokens": ["Ein", "rie\u00b7si\u00b7ger", "Kas\u00b7ten", ".", "Ja", ",", "wer", "so", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PTKANT", "$,", "PWS", "ADV", "VMFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das tut jede Verwaltung, die auf sich h\u00e4lt;", "tokens": ["Das", "tut", "je\u00b7de", "Ver\u00b7wal\u00b7tung", ",", "die", "auf", "sich", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,", "PRELS", "APPR", "PRF", "VVFIN", "$."], "meter": "-++--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die Herren haben wohl sehr viel Geld.", "tokens": ["die", "Her\u00b7ren", "ha\u00b7ben", "wohl", "sehr", "viel", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wenn zwei Deutsche im Hof n\u00e4mlich Holz zerspalten,", "tokens": ["Wenn", "zwei", "Deut\u00b7sche", "im", "Hof", "n\u00e4m\u00b7lich", "Holz", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "APPRART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "stehn drei andere herum, die das verwalten.", "tokens": ["stehn", "drei", "an\u00b7de\u00b7re", "he\u00b7rum", ",", "die", "das", "ver\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "PIS", "PTKVZ", "$,", "PRELS", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Und ich seh an dem feuchten Neubau hinauf,", "tokens": ["Und", "ich", "seh", "an", "dem", "feuch\u00b7ten", "Neu\u00b7bau", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "und dies steigt vor meinem Auge auf:", "tokens": ["und", "dies", "steigt", "vor", "mei\u00b7nem", "Au\u00b7ge", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Korridore mit vielen T\u00fcren,", "tokens": ["Kor\u00b7ri\u00b7do\u00b7re", "mit", "vie\u00b7len", "T\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "die alle in kleine B\u00fcrozimmer f\u00fchren.", "tokens": ["die", "al\u00b7le", "in", "klei\u00b7ne", "B\u00fc\u00b7ro\u00b7zim\u00b7mer", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "In den Zimmern ist nichts Besondres los . . .", "tokens": ["In", "den", "Zim\u00b7mern", "ist", "nichts", "Be\u00b7sond\u00b7res", "los", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIS", "NN", "PTKVZ", "$.", "$.", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Und es gibt zweierlei Sorten von B\u00fcros:", "tokens": ["Und", "es", "gibt", "zwei\u00b7er\u00b7lei", "Sor\u00b7ten", "von", "B\u00fc\u00b7ros", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "APPR", "NE", "$."], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Solche, in denen die Buchhaltungsfritzen,", "tokens": ["Sol\u00b7che", ",", "in", "de\u00b7nen", "die", "Buch\u00b7hal\u00b7tungs\u00b7frit\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "APPR", "PRELS", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.14": {"text": "die gew\u00f6hnlichen Schreiber sitzen;", "tokens": ["die", "ge\u00b7w\u00f6hn\u00b7li\u00b7chen", "Schrei\u00b7ber", "sit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "die bebr\u00fcten Akten und f\u00fchren Listen.", "tokens": ["die", "be\u00b7br\u00fc\u00b7ten", "Ak\u00b7ten", "und", "f\u00fch\u00b7ren", "Lis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.16": {"text": "Das sind die gemeinen Papier-Infanteristen.", "tokens": ["Das", "sind", "die", "ge\u00b7mei\u00b7nen", "Pa\u00b7pier\u00b7In\u00b7fan\u00b7te\u00b7ris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Kino, Kollegenklatsch, etwas Sport . . .", "tokens": ["Ki\u00b7no", ",", "Kol\u00b7le\u00b7gen\u00b7klatsch", ",", "et\u00b7was", "Sport", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "$,", "NN", "$,", "PIAT", "NN", "$.", "$.", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "wie schnell das Klassenbewu\u00dftsein verdorrt!", "tokens": ["wie", "schnell", "das", "Klas\u00b7sen\u00b7be\u00b7wu\u00df\u00b7tsein", "ver\u00b7dorrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.19": {"text": "F\u00fcr eine Handlungsvollmacht, f\u00fcr einen Posten", "tokens": ["F\u00fcr", "ei\u00b7ne", "Hand\u00b7lungs\u00b7voll\u00b7macht", ",", "f\u00fcr", "ei\u00b7nen", "Pos\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "tun sie alles, wobei sie die Chefs nichts kosten.", "tokens": ["tun", "sie", "al\u00b7les", ",", "wo\u00b7bei", "sie", "die", "Chefs", "nichts", "kos\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "PWAV", "PPER", "ART", "NN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "Und es haben die M\u00e4dels in den Buchhalterein", "tokens": ["Und", "es", "ha\u00b7ben", "die", "M\u00e4\u00b7dels", "in", "den", "Buch\u00b7hal\u00b7ter\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.22": {"text": "einen Wunsch:", "tokens": ["ei\u00b7nen", "Wunsch", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Hier raus und geheiratet sein!", "tokens": ["Hier", "raus", "und", "ge\u00b7hei\u00b7ra\u00b7tet", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "VVPP", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "Und alle schreiben und schreiben und schreiben", "tokens": ["Und", "al\u00b7le", "schrei\u00b7ben", "und", "schrei\u00b7ben", "und", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVINF", "KON", "VVINF", "KON", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "und m\u00fcssen ewig hinter den Pulten bleiben.", "tokens": ["und", "m\u00fcs\u00b7sen", "e\u00b7wig", "hin\u00b7ter", "den", "Pul\u00b7ten", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Die schuften ihr ganzes Dasein vergebens.", "tokens": ["Die", "schuf\u00b7ten", "ihr", "gan\u00b7zes", "Da\u00b7sein", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch in den andern B\u00fcros", "tokens": ["Doch", "in", "den", "an\u00b7dern", "B\u00fc\u00b7ros"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "hockt dick und gro\u00df", "tokens": ["hockt", "dick", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "das Ideal des Wirtschaftslebens:", "tokens": ["das", "I\u00b7deal", "des", "Wirt\u00b7schafts\u00b7le\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da sitzt der Mann an der Arbeitsstatt,", "tokens": ["Da", "sitzt", "der", "Mann", "an", "der", "Ar\u00b7beits\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der ein Sekretariat und ein Vorzimmer hat,", "tokens": ["der", "ein", "Se\u00b7kre\u00b7ta\u00b7ri\u00b7at", "und", "ein", "Vor\u00b7zim\u00b7mer", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "(\u00fcber jenen, die an ihren Arbeitsst\u00e4tten", "tokens": ["(", "\u00fc\u00b7ber", "je\u00b7nen", ",", "die", "an", "ih\u00b7ren", "Ar\u00b7beits\u00b7st\u00e4t\u00b7ten"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PDAT", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "gern ein Sekretariat und ein Vorzimmer h\u00e4tten).", "tokens": ["gern", "ein", "Se\u00b7kre\u00b7ta\u00b7ri\u00b7at", "und", "ein", "Vor\u00b7zim\u00b7mer", "h\u00e4t\u00b7ten", ")", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "VAFIN", "$(", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Hier wird der Deutsche erst richtig heiter:", "tokens": ["Hier", "wird", "der", "Deut\u00b7sche", "erst", "rich\u00b7tig", "hei\u00b7ter", ":"], "token_info":