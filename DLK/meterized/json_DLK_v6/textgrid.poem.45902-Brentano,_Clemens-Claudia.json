{"textgrid.poem.45902": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Claudia", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch gr\u00fcne Auen wollt' ich mit dir schweifen,", "tokens": ["Durch", "gr\u00fc\u00b7ne", "Au\u00b7en", "wollt'", "ich", "mit", "dir", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4rst du des s\u00fc\u00dfen Maien frohes Kind,", "tokens": ["W\u00e4rst", "du", "des", "s\u00fc\u00b7\u00dfen", "Mai\u00b7en", "fro\u00b7hes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wollte sinnreich nach den Blumen greifen,", "tokens": ["Und", "woll\u00b7te", "sinn\u00b7reich", "nach", "den", "Blu\u00b7men", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu flechten dir ein z\u00e4rtliches Gewind',", "tokens": ["Zu", "flech\u00b7ten", "dir", "ein", "z\u00e4rt\u00b7li\u00b7ches", "Ge\u00b7wind'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir Bl\u00fcten werden all in Liebe reifen,", "tokens": ["Wir", "Bl\u00fc\u00b7ten", "wer\u00b7den", "all", "in", "Lie\u00b7be", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "PIAT", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So spr\u00e4ch' der Kranz, weil wir dir \u00e4hnlich sind.", "tokens": ["So", "spr\u00e4ch'", "der", "Kranz", ",", "weil", "wir", "dir", "\u00e4hn\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch keine Blume ist vor dir entsprungen,", "tokens": ["Doch", "kei\u00b7ne", "Blu\u00b7me", "ist", "vor", "dir", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der ungeteilten Kraft bist du gelungen.", "tokens": ["Der", "un\u00b7ge\u00b7teil\u00b7ten", "Kraft", "bist", "du", "ge\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In leisem Schlummer tr\u00e4umend sinnt die Erde,", "tokens": ["In", "lei\u00b7sem", "Schlum\u00b7mer", "tr\u00e4u\u00b7mend", "sinnt", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sie die Junge Zeit erfreuen soll,", "tokens": ["Wie", "sie", "die", "Jun\u00b7ge", "Zeit", "er\u00b7freu\u00b7en", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da sieht sie dich, in z\u00fcchtiger Geberde", "tokens": ["Da", "sieht", "sie", "dich", ",", "in", "z\u00fcch\u00b7ti\u00b7ger", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stehst du vor ihr so sinnend, liebevoll,", "tokens": ["Stehst", "du", "vor", "ihr", "so", "sin\u00b7nend", ",", "lie\u00b7be\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und jungfr\u00e4ulich begr\u00fc\u00dfte Dich ihr Werde,", "tokens": ["Und", "jung\u00b7fr\u00e4u\u00b7lich", "be\u00b7gr\u00fc\u00df\u00b7te", "Dich", "ihr", "Wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Der keine Blume noch am Busen schwoll.", "tokens": ["Der", "kei\u00b7ne", "Blu\u00b7me", "noch", "am", "Bu\u00b7sen", "schwoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch bald die Einsamkeit dir zu vers\u00fc\u00dfen,", "tokens": ["Doch", "bald", "die", "Ein\u00b7sam\u00b7keit", "dir", "zu", "ver\u00b7s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "L\u00e4\u00dft als Gespielen sie dich Veilchen gr\u00fc\u00dfen.", "tokens": ["L\u00e4\u00dft", "als", "Ge\u00b7spie\u00b7len", "sie", "dich", "Veil\u00b7chen", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "PPER", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So fehlen Blumen, Blume dich zu kr\u00e4nzen,", "tokens": ["So", "feh\u00b7len", "Blu\u00b7men", ",", "Blu\u00b7me", "dich", "zu", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die selbst des Jahres fr\u00fchste Blume bl\u00fcht,", "tokens": ["Die", "selbst", "des", "Jah\u00b7res", "fr\u00fchs\u00b7te", "Blu\u00b7me", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch in des Lebens Garten ohne Grenzen,", "tokens": ["Doch", "in", "des", "Le\u00b7bens", "Gar\u00b7ten", "oh\u00b7ne", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dem der Fr\u00fchling ewig kehrt und flieht,", "tokens": ["In", "dem", "der", "Fr\u00fch\u00b7ling", "e\u00b7wig", "kehrt", "und", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Seh' eine edle Blume fern ich gl\u00e4nzen,", "tokens": ["Seh'", "ei\u00b7ne", "ed\u00b7le", "Blu\u00b7me", "fern", "ich", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die bis zum Namen selbst dir \u00e4hnlich sieht,", "tokens": ["Die", "bis", "zum", "Na\u00b7men", "selbst", "dir", "\u00e4hn\u00b7lich", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPRART", "NN", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Herrliche kehrt ewig zu dem Leben,", "tokens": ["Das", "Herr\u00b7li\u00b7che", "kehrt", "e\u00b7wig", "zu", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und jeder Sommer mu\u00df uns Lilien geben.", "tokens": ["Und", "je\u00b7der", "Som\u00b7mer", "mu\u00df", "uns", "Li\u00b7li\u00b7en", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Dich R\u00f6merin, Vestale seh' ich wieder,", "tokens": ["Dich", "R\u00f6\u00b7me\u00b7rin", ",", "Ves\u00b7ta\u00b7le", "seh'", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich ", "tokens": ["Dich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Keusch h\u00fcllt ein reiner Schleier dir die Glieder,", "tokens": ["Keusch", "h\u00fcllt", "ein", "rei\u00b7ner", "Schlei\u00b7er", "dir", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die aller Liebe reine Flamme n\u00e4hrt.", "tokens": ["Die", "al\u00b7ler", "Lie\u00b7be", "rei\u00b7ne", "Flam\u00b7me", "n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es priesen uns noch keines S\u00e4ngers Lieder,", "tokens": ["Es", "prie\u00b7sen", "uns", "noch", "kei\u00b7nes", "S\u00e4n\u00b7gers", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den hohen Sinn, den uns dein Leben lehrt,", "tokens": ["Den", "ho\u00b7hen", "Sinn", ",", "den", "uns", "dein", "Le\u00b7ben", "lehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bescheidne, z\u00fcrne nicht, la\u00df es gelingen,", "tokens": ["Be\u00b7scheid\u00b7ne", ",", "z\u00fcr\u00b7ne", "nicht", ",", "la\u00df", "es", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die R\u00f6merin will der Barbare singen.", "tokens": ["Die", "R\u00f6\u00b7me\u00b7rin", "will", "der", "Bar\u00b7ba\u00b7re", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da ", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Will er, als Sieger soll ihn Roma sehn,", "tokens": ["Will", "er", ",", "als", "Sie\u00b7ger", "soll", "ihn", "Ro\u00b7ma", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der in der eignen Tat den R\u00f6mer ehrte,", "tokens": ["Der", "in", "der", "eig\u00b7nen", "Tat", "den", "R\u00f6\u00b7mer", "ehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Will im Triumphe auch die Tat erh\u00f6hn,", "tokens": ["Will", "im", "Tri\u00b7um\u00b7phe", "auch", "die", "Tat", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Doch ein Tribun, der tiefen Ha\u00df ihm n\u00e4hrte,", "tokens": ["Doch", "ein", "Tri\u00b7bun", ",", "der", "tie\u00b7fen", "Ha\u00df", "ihm", "n\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Will ungepriesen soll sein Werk vergehn:", "tokens": ["Will", "un\u00b7ge\u00b7prie\u00b7sen", "soll", "sein", "Werk", "ver\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es l\u00e4\u00dft der M\u00e4chtige dem Sieger sagen,", "tokens": ["Es", "l\u00e4\u00dft", "der", "M\u00e4ch\u00b7ti\u00b7ge", "dem", "Sie\u00b7ger", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du sollst durch Rom nicht deine Lorbeern tragen.", "tokens": ["Du", "sollst", "durch", "Rom", "nicht", "dei\u00b7ne", "Lor\u00b7beern", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NE", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch achtet, trotzend auf des Sieges Fl\u00fcgel,", "tokens": ["Doch", "ach\u00b7tet", ",", "trot\u00b7zend", "auf", "des", "Sie\u00b7ges", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Feldherr nicht des Richters ernsten Stab,", "tokens": ["Der", "Feld\u00b7herr", "nicht", "des", "Rich\u00b7ters", "erns\u00b7ten", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Heeresprunk gr\u00fc\u00dft er die sieben H\u00fcgel,", "tokens": ["Im", "Hee\u00b7res\u00b7prunk", "gr\u00fc\u00dft", "er", "die", "sie\u00b7ben", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "CARD", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von seines Wagens goldner H\u00f6h' herab,", "tokens": ["Von", "sei\u00b7nes", "Wa\u00b7gens", "gold\u00b7ner", "H\u00f6h'", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und tausendfach in heller Waffen Spiegel", "tokens": ["Und", "tau\u00b7send\u00b7fach", "in", "hel\u00b7ler", "Waf\u00b7fen", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gr\u00fcnt ihm der Lorbeer, den der Sieg ihm gab,", "tokens": ["Gr\u00fcnt", "ihm", "der", "Lor\u00b7beer", ",", "den", "der", "Sieg", "ihm", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Es lenket durch des Volkes laute Mitte,", "tokens": ["Es", "len\u00b7ket", "durch", "des", "Vol\u00b7kes", "lau\u00b7te", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Zug zum Kapitole hin die Schritte.", "tokens": ["Der", "Zug", "zum", "Ka\u00b7pi\u00b7to\u00b7le", "hin", "die", "Schrit\u00b7te", "."], "token_info": ["word", "word", "word", "word", 