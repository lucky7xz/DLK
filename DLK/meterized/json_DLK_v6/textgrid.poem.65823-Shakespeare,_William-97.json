{"textgrid.poem.65823": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "97", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist von dir, dem Stern des fl\u00fccht'gen Jahrs,", "tokens": ["Wie", "ist", "von", "dir", ",", "dem", "Stern", "des", "fl\u00fccht'\u00b7gen", "Jahrs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPR", "PPER", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Trennung mir zum \u00f6den Winter worden!", "tokens": ["Die", "Tren\u00b7nung", "mir", "zum", "\u00f6\u00b7den", "Win\u00b7ter", "wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sch\u00fcttelte mich Frost, wie dunkel war's,", "tokens": ["Wie", "sch\u00fct\u00b7tel\u00b7te", "mich", "Frost", ",", "wie", "dun\u00b7kel", "wa\u00b7r's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "$,", "PWAV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wie d\u00fcrr dezemberschaurig aller Orten!", "tokens": ["Wie", "d\u00fcrr", "de\u00b7zem\u00b7ber\u00b7schau\u00b7rig", "al\u00b7ler", "Or\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und doch war Sommer diese Trennungszeit,", "tokens": ["Und", "doch", "war", "Som\u00b7mer", "die\u00b7se", "Tren\u00b7nungs\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Fruchtbarer Herbst, der schwellend \u00fcberflo\u00df,", "tokens": ["Frucht\u00b7ba\u00b7rer", "Herbst", ",", "der", "schwel\u00b7lend", "\u00fc\u00b7ber\u00b7flo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Beladen mit des Fr\u00fchlings \u00dcppigkeit,", "tokens": ["Be\u00b7la\u00b7den", "mit", "des", "Fr\u00fch\u00b7lings", "\u00dcp\u00b7pig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie nach des Gatten Tod der Witwe Scho\u00df.", "tokens": ["Wie", "nach", "des", "Gat\u00b7ten", "Tod", "der", "Wit\u00b7we", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch vaterlose Frucht, Verwaisungszeichen", "tokens": ["Doch", "va\u00b7ter\u00b7lo\u00b7se", "Frucht", ",", "Ver\u00b7wai\u00b7sungs\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sah ich in dieser Segensf\u00fclle nur:", "tokens": ["Sah", "ich", "in", "die\u00b7ser", "Se\u00b7gens\u00b7f\u00fcl\u00b7le", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Denn dir folgt Sommer und sein Gl\u00fcck; es schweigen", "tokens": ["Denn", "dir", "folgt", "Som\u00b7mer", "und", "sein", "Gl\u00fcck", ";", "es", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "PPOSAT", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wo du fehlst, selbst die V\u00f6gel auf der Flur.", "tokens": ["Wo", "du", "fehlst", ",", "selbst", "die", "V\u00f6\u00b7gel", "auf", "der", "Flur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und s\u00e4ngen sie, es w\u00e4r so bang zu h\u00f6ren,", "tokens": ["Und", "s\u00e4n\u00b7gen", "sie", ",", "es", "w\u00e4r", "so", "bang", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df Bl\u00e4tter, winterscheu, ihr Gr\u00fcn verl\u00f6ren.", "tokens": ["Da\u00df", "Bl\u00e4t\u00b7ter", ",", "win\u00b7ter\u00b7scheu", ",", "ihr", "Gr\u00fcn", "ver\u00b7l\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADJD", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}