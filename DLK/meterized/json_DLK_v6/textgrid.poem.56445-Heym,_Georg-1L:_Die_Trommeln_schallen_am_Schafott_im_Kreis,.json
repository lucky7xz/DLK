{"textgrid.poem.56445": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Trommeln schallen am Schafott im Kreis,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Trommeln schallen am Schafott im Kreis,", "tokens": ["Die", "Trom\u00b7meln", "schal\u00b7len", "am", "Scha\u00b7fott", "im", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das wie ein Sarg steht, schwarz mit Tuch verschlagen.", "tokens": ["Das", "wie", "ein", "Sarg", "steht", ",", "schwarz", "mit", "Tuch", "ver\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "VVFIN", "$,", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Darauf steht der Block. Dabei der offene Schragen", "tokens": ["Da\u00b7rauf", "steht", "der", "Block", ".", "Da\u00b7bei", "der", "of\u00b7fe\u00b7ne", "Schra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "$.", "PAV", "ART", "ADJA", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "F\u00fcr seinen Leib. Das Fallbeil glitzert wei\u00df.", "tokens": ["F\u00fcr", "sei\u00b7nen", "Leib", ".", "Das", "Fall\u00b7beil", "glit\u00b7zert", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von vollen D\u00e4chern flattern rot Standarten.", "tokens": ["Von", "vol\u00b7len", "D\u00e4\u00b7chern", "flat\u00b7tern", "rot", "Stand\u00b7ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Rufer schrein der Fensterpl\u00e4tze Preis.", "tokens": ["Die", "Ru\u00b7fer", "schrein", "der", "Fens\u00b7ter\u00b7pl\u00e4t\u00b7ze", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Winter ist es. Doch dem Volk wird hei\u00df,", "tokens": ["Im", "Win\u00b7ter", "ist", "es", ".", "Doch", "dem", "Volk", "wird", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "$.", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es dr\u00e4ngt sich murrend vor. Man l\u00e4\u00dft es warten.", "tokens": ["Es", "dr\u00e4ngt", "sich", "mur\u00b7rend", "vor", ".", "Man", "l\u00e4\u00dft", "es", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "$.", "PIS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da h\u00f6rt man L\u00e4rm. Er steigt. Das Schreien braust.", "tokens": ["Da", "h\u00f6rt", "man", "L\u00e4rm", ".", "Er", "steigt", ".", "Das", "Schrei\u00b7en", "braust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$.", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf seinem Karren kommt Capet, bedreckt,", "tokens": ["Auf", "sei\u00b7nem", "Kar\u00b7ren", "kommt", "Ca\u00b7pet", ",", "be\u00b7dreckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Kot beworfen, und das Haar zerzaust.", "tokens": ["Mit", "Kot", "be\u00b7wor\u00b7fen", ",", "und", "das", "Haar", "zer\u00b7zaust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Man schleift ihn schnell herauf. Er wird gestreckt.", "tokens": ["Man", "schleift", "ihn", "schnell", "her\u00b7auf", ".", "Er", "wird", "ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Kopf liegt auf dem Block. Das Fallbeil saust.", "tokens": ["Der", "Kopf", "liegt", "auf", "dem", "Block", ".", "Das", "Fall\u00b7beil", "saust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Blut speit sein Hals, der fest im Loche steckt.", "tokens": ["Blut", "speit", "sein", "Hals", ",", "der", "fest", "im", "Lo\u00b7che", "steckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}