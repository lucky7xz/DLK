{"dta.poem.16110": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "164.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Messer, wenn es auch ist oben noch so scharf,", "tokens": ["Das", "Mes\u00b7ser", ",", "wenn", "es", "auch", "ist", "o\u00b7ben", "noch", "so", "scharf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat unten einen Stiel, wo man's anfassen darf.", "tokens": ["Hat", "un\u00b7ten", "ei\u00b7nen", "Stiel", ",", "wo", "man's", "an\u00b7fas\u00b7sen", "darf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das alte Sprichwort sagt: Wie scharf das Messer sei,", "tokens": ["Das", "al\u00b7te", "Sprich\u00b7wort", "sagt", ":", "Wie", "scharf", "das", "Mes\u00b7ser", "sei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PWAV", "VVFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es schneidet niemals doch den eignen Stiel entzwei.", "tokens": ["Es", "schnei\u00b7det", "nie\u00b7mals", "doch", "den", "eig\u00b7nen", "Stiel", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "