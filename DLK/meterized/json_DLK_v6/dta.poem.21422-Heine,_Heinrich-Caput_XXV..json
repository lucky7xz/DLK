{"dta.poem.21422": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott, o G\u00f6ttin! \u2013 rief ich entz\u00fcckt \u2013", "tokens": ["Mein", "Gott", ",", "o", "G\u00f6t\u00b7tin", "!", "\u2013", "rief", "ich", "ent\u00b7z\u00fcckt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$.", "$(", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das w\u00e4re mein gr\u00f6\u00dftes Vergn\u00fcgen,", "tokens": ["Das", "w\u00e4\u00b7re", "mein", "gr\u00f6\u00df\u00b7tes", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "La\u00df mich das k\u00fcnftige Deutschland sehn \u2013", "tokens": ["La\u00df", "mich", "das", "k\u00fcnf\u00b7ti\u00b7ge", "Deutschland", "sehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich bin ein Mann und verschwiegen.", "tokens": ["Ich", "bin", "ein", "Mann", "und", "ver\u00b7schwie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich will dir schw\u00f6ren jeden Eid,", "tokens": ["Ich", "will", "dir", "schw\u00f6\u00b7ren", "je\u00b7den", "Eid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den du nur magst begehren,", "tokens": ["Den", "du", "nur", "magst", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Schweigen zu verb\u00fcrgen dir \u2013", "tokens": ["Mein", "Schwei\u00b7gen", "zu", "ver\u00b7b\u00fcr\u00b7gen", "dir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sag an, wie soll ich schw\u00f6ren?", "tokens": ["Sag", "an", ",", "wie", "soll", "ich", "schw\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch jene erwiederte: \u201eSchw\u00f6re mir", "tokens": ["Doch", "je\u00b7ne", "er\u00b7wie\u00b7der\u00b7te", ":", "\u201e", "Schw\u00f6\u00b7re", "mir"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "$.", "$(", "VVFIN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In Vater Abrahams Weise,", "tokens": ["In", "Va\u00b7ter", "Ab\u00b7ra\u00b7hams", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie er Eliesern schw\u00f6ren lie\u00df,", "tokens": ["Wie", "er", "E\u00b7lie\u00b7sern", "schw\u00f6\u00b7ren", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als dieser sich gab auf die Reise.", "tokens": ["Als", "die\u00b7ser", "sich", "gab", "auf", "die", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201eheb\u2019 auf das Gewand und lege die Hand", "tokens": ["\u201e", "heb'", "auf", "das", "Ge\u00b7wand", "und", "le\u00b7ge", "die", "Hand"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hier unten an meine H\u00fcften,", "tokens": ["Hier", "un\u00b7ten", "an", "mei\u00b7ne", "H\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und schw\u00f6re mir Verschwiegenheit", "tokens": ["Und", "schw\u00f6\u00b7re", "mir", "Ver\u00b7schwie\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Reden und in Schriften!\u201c", "tokens": ["In", "Re\u00b7den", "und", "in", "Schrif\u00b7ten", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}