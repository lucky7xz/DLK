{"textgrid.poem.43383": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deutschland, Deutschland \u00fcber Alles,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutschland, Deutschland \u00fcber Alles,", "tokens": ["Deutschland", ",", "Deutschland", "\u00fc\u00b7ber", "Al\u00b7les", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "APPR", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ueber Alles in der Welt,", "tokens": ["Ue\u00b7ber", "Al\u00b7les", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn es stets zu Schutz und Trutze", "tokens": ["Wenn", "es", "stets", "zu", "Schutz", "und", "Trut\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00fcderlich zusammenh\u00e4lt,", "tokens": ["Br\u00fc\u00b7der\u00b7lich", "zu\u00b7sam\u00b7men\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von der Maas bis an die Memel,", "tokens": ["Von", "der", "Maas", "bis", "an", "die", "Me\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Etsch bis an den Belt \u2013", "tokens": ["Von", "der", "Etsch", "bis", "an", "den", "Belt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deutschland, Deutschland \u00fcber Alles,", "tokens": ["Deutschland", ",", "Deutschland", "\u00fc\u00b7ber", "Al\u00b7les", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "APPR", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ueber Alles in der Welt!", "tokens": ["Ue\u00b7ber", "Al\u00b7les", "in", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deutsche Frauen, deutsche Treue,", "tokens": ["Deut\u00b7sche", "Frau\u00b7en", ",", "deut\u00b7sche", "Treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutscher Wein und deutscher Sang", "tokens": ["Deut\u00b7scher", "Wein", "und", "deut\u00b7scher", "Sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollen in der Welt behalten", "tokens": ["Sol\u00b7len", "in", "der", "Welt", "be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren alten sch\u00f6nen Klang,", "tokens": ["Ih\u00b7ren", "al\u00b7ten", "sch\u00f6\u00b7nen", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Uns zu edler That begeistern", "tokens": ["Uns", "zu", "ed\u00b7ler", "That", "be\u00b7geis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unser ganzes Leben lang \u2013", "tokens": ["Un\u00b7ser", "gan\u00b7zes", "Le\u00b7ben", "lang", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deutsche Frauen, deutsche Treue,", "tokens": ["Deut\u00b7sche", "Frau\u00b7en", ",", "deut\u00b7sche", "Treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deutscher Wein und deutscher Sang!", "tokens": ["Deut\u00b7scher", "Wein", "und", "deut\u00b7scher", "Sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einigkeit und Recht und Freiheit", "tokens": ["Ei\u00b7nig\u00b7keit", "und", "Recht", "und", "Frei\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr das deutsche Vaterland!", "tokens": ["F\u00fcr", "das", "deut\u00b7sche", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Danach la\u00dft uns alle streben", "tokens": ["Da\u00b7nach", "la\u00dft", "uns", "al\u00b7le", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PIS", "VVFIN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Br\u00fcderlich mit Herz und Hand!", "tokens": ["Br\u00fc\u00b7der\u00b7lich", "mit", "Herz", "und", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einigkeit und Recht und Freiheit", "tokens": ["Ei\u00b7nig\u00b7keit", "und", "Recht", "und", "Frei\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind des Gl\u00fcckes Unterpfand \u2013", "tokens": ["Sind", "des", "Gl\u00fc\u00b7ckes", "Un\u00b7ter\u00b7pfand", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bl\u00fch' im Glanze dieses Gl\u00fcckes,", "tokens": ["Bl\u00fch'", "im", "Glan\u00b7ze", "die\u00b7ses", "Gl\u00fc\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bl\u00fche deutsches Vaterland!", "tokens": ["Bl\u00fc\u00b7he", "deut\u00b7sches", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}