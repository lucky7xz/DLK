{"textgrid.poem.66391": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Palinodie an Bacchus", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du mit deinen Tigern an dem Wagen", "tokens": ["Der", "du", "mit", "dei\u00b7nen", "Ti\u00b7gern", "an", "dem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einst Indien durchzogst,", "tokens": ["Einst", "In\u00b7di\u00b7en", "durch\u00b7zogst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und dich, dem Erebus entstiegen,", "tokens": ["Und", "dich", ",", "dem", "E\u00b7re\u00b7bus", "ent\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hochaufgeschwellt von deinen Siegen", "tokens": ["Hoch\u00b7auf\u00b7ge\u00b7schwellt", "von", "dei\u00b7nen", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Gotte des Olympos logst!", "tokens": ["Zum", "Got\u00b7te", "des", "O\u00b7lym\u00b7pos", "logst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dich sing' ich nicht, wie Dichter, deine Sklaven,", "tokens": ["Dich", "sing'", "ich", "nicht", ",", "wie", "Dich\u00b7ter", ",", "dei\u00b7ne", "Skla\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erst vollgef\u00fcllt aus deinem Horn;", "tokens": ["Erst", "voll\u00b7ge\u00b7f\u00fcllt", "aus", "dei\u00b7nem", "Horn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann hoch die Thyrsusst\u00e4be schwingend,", "tokens": ["Dann", "hoch", "die", "Thyr\u00b7sus\u00b7st\u00e4\u00b7be", "schwin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Evoe im wilden Rausche singend \u2013", "tokens": ["Und", "E\u00b7voe", "im", "wil\u00b7den", "Rau\u00b7sche", "sin\u00b7gend", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Ich singe, Bacchus, dich im Zorn.", "tokens": ["Ich", "sin\u00b7ge", ",", "Bac\u00b7chus", ",", "dich", "im", "Zorn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "$,", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Zorne, da\u00df du auch Thuiskons W\u00e4lder", "tokens": ["Im", "Zor\u00b7ne", ",", "da\u00df", "du", "auch", "Thu\u00b7is\u00b7kons", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zertratst in deinem Drachenzug;", "tokens": ["Zer\u00b7tratst", "in", "dei\u00b7nem", "Dra\u00b7chen\u00b7zug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df du die weingef\u00fcllten R\u00f6mersch\u00e4del", "tokens": ["Da\u00df", "du", "die", "wein\u00b7ge\u00b7f\u00fcll\u00b7ten", "R\u00f6\u00b7mer\u00b7sch\u00e4\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Volke botst, ehmals so gro\u00df, so edel,", "tokens": ["Dem", "Vol\u00b7ke", "botst", ",", "eh\u00b7mals", "so", "gro\u00df", ",", "so", "e\u00b7del", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Varus Legionen schlug;", "tokens": ["Das", "Va\u00b7rus", "Le\u00b7gi\u00b7o\u00b7nen", "schlug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df du mit deinen Giften ihre Knochen,", "tokens": ["Da\u00df", "du", "mit", "dei\u00b7nen", "Gif\u00b7ten", "ih\u00b7re", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ehmals wie Erz, in Brei verkocht,", "tokens": ["Eh\u00b7mals", "wie", "Erz", ",", "in", "Brei", "ver\u00b7kocht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und den zum Siechling umgeschaffen,", "tokens": ["Und", "den", "zum", "Siech\u00b7ling", "um\u00b7ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem sonst beim eisern Klang der Waffen", "tokens": ["Dem", "sonst", "beim", "ei\u00b7sern", "Klang", "der", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Busen aufgepocht.", "tokens": ["Der", "Bu\u00b7sen", "auf\u00b7ge\u00b7pocht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer lehrt das Biedervolk im Eichendunkel schwelgen?", "tokens": ["Wer", "lehrt", "das", "Bie\u00b7der\u00b7volk", "im", "Ei\u00b7chen\u00b7dun\u00b7kel", "schwel\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer hat mit toller Trunkenheit,", "tokens": ["Wer", "hat", "mit", "tol\u00b7ler", "Trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Klubbe rasender Bacchanten,", "tokens": ["Im", "Klub\u00b7be", "ra\u00b7sen\u00b7der", "Bac\u00b7chan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Schl\u00e4uchen, Flaschen, vollen Kanten", "tokens": ["Mit", "Schl\u00e4u\u00b7chen", ",", "Fla\u00b7schen", ",", "vol\u00b7len", "Kan\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Hain Germaniens entweiht?", "tokens": ["Den", "Hain", "Ger\u00b7ma\u00b7ni\u00b7ens", "ent\u00b7weiht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer machte Menschen rei\u00dfender als Tiger,", "tokens": ["Wer", "mach\u00b7te", "Men\u00b7schen", "rei\u00b7\u00dfen\u00b7der", "als", "Ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die deinen Wagen ziehn?", "tokens": ["Die", "dei\u00b7nen", "Wa\u00b7gen", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer lehrt das trunkene Geschlechte,", "tokens": ["Wer", "lehrt", "das", "trun\u00b7ke\u00b7ne", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Dolch des Aufruhrs in der Rechte,", "tokens": ["Den", "Dolch", "des", "Auf\u00b7ruhrs", "in", "der", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von H\u00f6llenmordlust gl\u00fchn?", "tokens": ["Von", "H\u00f6l\u00b7len\u00b7mord\u00b7lust", "gl\u00fchn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer lockt zum L\u00e4rm bei ekeln Saufgelagen,", "tokens": ["Wer", "lockt", "zum", "L\u00e4rm", "bei", "e\u00b7keln", "Sauf\u00b7ge\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als, Schreier Bacchus, du?", "tokens": ["Als", ",", "Schrei\u00b7er", "Bac\u00b7chus", ",", "du", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "$,", "NN", "NE", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir br\u00fcllen deine Taumelschaaren", "tokens": ["Dir", "br\u00fcl\u00b7len", "dei\u00b7ne", "Tau\u00b7mel\u00b7schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit borstigen und wildzerzausten Haaren", "tokens": ["Mit", "bors\u00b7ti\u00b7gen", "und", "wild\u00b7zer\u00b7zaus\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr Evoe bacchantisch zu.", "tokens": ["Ihr", "E\u00b7voe", "bac\u00b7chan\u00b7tisch", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ha! wer zerst\u00f6rt die k\u00f6stliche Behausung", "tokens": ["Ha", "!", "wer", "zer\u00b7st\u00f6rt", "die", "k\u00f6st\u00b7li\u00b7che", "Be\u00b7hau\u00b7sung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Des Menschengeistes? Wessen Gluth", "tokens": ["Des", "Men\u00b7schen\u00b7geis\u00b7tes", "?", "Wes\u00b7sen", "Gluth"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Befleckt den Blick mit dieser blut'gen R\u00f6the,", "tokens": ["Be\u00b7fleckt", "den", "Blick", "mit", "die\u00b7ser", "blut'\u00b7gen", "R\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und pre\u00dft die Augen, wie der Kr\u00f6te,", "tokens": ["Und", "pre\u00dft", "die", "Au\u00b7gen", ",", "wie", "der", "Kr\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit giftgetr\u00e4nkter Wuth?", "tokens": ["Mit", "gift\u00b7ge\u00b7tr\u00e4nk\u00b7ter", "Wuth", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wer schuf die Bl\u00e4ue auf des J\u00fcnglings Lippe?", "tokens": ["Wer", "schuf", "die", "Bl\u00e4u\u00b7e", "auf", "des", "J\u00fcng\u00b7lings", "Lip\u00b7pe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer hat der Wangen Blume abgestreift?", "tokens": ["Wer", "hat", "der", "Wan\u00b7gen", "Blu\u00b7me", "ab\u00b7ge\u00b7streift", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Blume, ach, so farbig sonst, so heiter!", "tokens": ["Die", "Blu\u00b7me", ",", "ach", ",", "so", "far\u00b7big", "sonst", ",", "so", "hei\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$,", "ADV", "ADJD", "ADV", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer zeugt der Hektik faulen Eiter,", "tokens": ["Wer", "zeugt", "der", "Hek\u00b7tik", "fau\u00b7len", "Ei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der aus der Lunge pfeift?", "tokens": ["Der", "aus", "der", "Lun\u00b7ge", "pfeift", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Noch schrecklicher \u2013 wer mordet Geister,", "tokens": ["Noch", "schreck\u00b7li\u00b7cher", "\u2013", "wer", "mor\u00b7det", "Geis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "PWS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als du, als D\u00e4mon Bacchus, du?", "tokens": ["Als", "du", ",", "als", "D\u00e4\u00b7mon", "Bac\u00b7chus", ",", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "NN", "NE", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer gei\u00dfelt sie in einer schwarzen Stunde,", "tokens": ["Wer", "gei\u00b7\u00dfelt", "sie", "in", "ei\u00b7ner", "schwar\u00b7zen", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Geister deiner Sklaven \u2013 ha! dem Schlunde", "tokens": ["Die", "Geis\u00b7ter", "dei\u00b7ner", "Skla\u00b7ven", "\u2013", "ha", "!", "dem", "Schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ITJ", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des g\u00e4hnenden Abyssus zu?", "tokens": ["Des", "g\u00e4h\u00b7nen\u00b7den", "A\u00b7bys\u00b7sus", "zu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Einst kannt' ich einen J\u00fcngling, bl\u00fchend,", "tokens": ["Einst", "kannt'", "ich", "ei\u00b7nen", "J\u00fcng\u00b7ling", ",", "bl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Eros war des J\u00fcnglings Blick,", "tokens": ["Wie", "E\u00b7ros", "war", "des", "J\u00fcng\u00b7lings", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm senkte Gott Gesang der Musen,", "tokens": ["Ihm", "senk\u00b7te", "Gott", "Ge\u00b7sang", "der", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word"