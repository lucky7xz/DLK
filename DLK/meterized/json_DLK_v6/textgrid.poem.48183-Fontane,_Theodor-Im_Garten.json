{"textgrid.poem.48183": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Im Garten", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die hohen Himbeerw\u00e4nde", "tokens": ["Die", "ho\u00b7hen", "Him\u00b7beer\u00b7w\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trennten dich und mich,", "tokens": ["Trenn\u00b7ten", "dich", "und", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch im Laubwerk unsre H\u00e4nde", "tokens": ["Doch", "im", "Laub\u00b7werk", "uns\u00b7re", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fanden von selber sich.", "tokens": ["Fan\u00b7den", "von", "sel\u00b7ber", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "PRF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Die Hecke konnt' es nicht wehren,", "tokens": ["Die", "He\u00b7cke", "konnt'", "es", "nicht", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie hoch sie immer stund:", "tokens": ["Wie", "hoch", "sie", "im\u00b7mer", "stund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich reichte dir die Beeren,", "tokens": ["Ich", "reich\u00b7te", "dir", "die", "Bee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und du reichtest mir deinen Mund.", "tokens": ["Und", "du", "reich\u00b7test", "mir", "dei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ach, schrittest du durch den Garten", "tokens": ["Ach", ",", "schrit\u00b7test", "du", "durch", "den", "Gar\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Noch einmal im raschen Gang,", "tokens": ["Noch", "ein\u00b7mal", "im", "ra\u00b7schen", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie gerne wollt' ich warten,", "tokens": ["Wie", "ger\u00b7ne", "wollt'", "ich", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warten stundenlang.", "tokens": ["War\u00b7ten", "stun\u00b7den\u00b7lang", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}