{"textgrid.poem.39551": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Epistel an einen Freund", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Freund, du hast mir da f\u00fcrwahr", "tokens": ["O", "Freund", ",", "du", "hast", "mir", "da", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Evangelium verk\u00fcndet:", "tokens": ["Ein", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "ver\u00b7k\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In deinem Haus' ist hell und klar", "tokens": ["In", "dei\u00b7nem", "Haus'", "ist", "hell", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein neues Lebenslicht entz\u00fcndet,", "tokens": ["Ein", "neu\u00b7es", "Le\u00b7bens\u00b7licht", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df mit der holden T\u00f6chter Paar", "tokens": ["Da\u00df", "mit", "der", "hol\u00b7den", "T\u00f6ch\u00b7ter", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Kn\u00e4bchen nun sich noch verbindet,", "tokens": ["Ein", "Kn\u00e4b\u00b7chen", "nun", "sich", "noch", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und bald die ganze frohe Schaar", "tokens": ["Und", "bald", "die", "gan\u00b7ze", "fro\u00b7he", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit zarten Armen dich umwindet;", "tokens": ["Mit", "zar\u00b7ten", "Ar\u00b7men", "dich", "um\u00b7win\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df wenn dir manches fl\u00fccht'ge Jahr", "tokens": ["Da\u00df", "wenn", "dir", "man\u00b7ches", "fl\u00fccht'\u00b7ge", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bei'm Ku\u00df der besten Gattin schwindet,", "tokens": ["Bei'm", "Ku\u00df", "der", "bes\u00b7ten", "Gat\u00b7tin", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ihr Beide dennoch immerdar", "tokens": ["Ihr", "Bei\u00b7de", "den\u00b7noch", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In ihnen eure Jugend findet.", "tokens": ["In", "ih\u00b7nen", "eu\u00b7re", "Ju\u00b7gend", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Es ist gewi\u00df nicht wunderbar,", "tokens": ["Es", "ist", "ge\u00b7wi\u00df", "nicht", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da solche Lust dein Herz empfindet,", "tokens": ["Da", "sol\u00b7che", "Lust", "dein", "Herz", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df, was du schreibst und sprichst sogar,", "tokens": ["Da\u00df", ",", "was", "du", "schreibst", "und", "sprichst", "so\u00b7gar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sich, wie von selbst, in Versen r\u00fcndet.", "tokens": ["Sich", ",", "wie", "von", "selbst", ",", "in", "Ver\u00b7sen", "r\u00fcn\u00b7det", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "PWAV", "APPR", "ADV", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Gern s\u00e4ng' ich Jubellieder zwar,", "tokens": ["Gern", "s\u00e4ng'", "ich", "Ju\u00b7bel\u00b7lie\u00b7der", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wenn ihr, o Musen, bei mir st\u00fcndet.", "tokens": ["Wenn", "ihr", ",", "o", "Mu\u00b7sen", ",", "bei", "mir", "st\u00fcn\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Allein ich krau' umsonst mein Haar,", "tokens": ["Al\u00b7lein", "ich", "krau'", "um\u00b7sonst", "mein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Ro\u00dfquell ist mir zugesp\u00fcndet;", "tokens": ["Der", "Ro\u00df\u00b7quell", "ist", "mir", "zu\u00b7ge\u00b7sp\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "So flink ich sonst zur Dichtkunst war,", "tokens": ["So", "flink", "ich", "sonst", "zur", "Dicht\u00b7kunst", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die ich in H\u00fchners Buch ergr\u00fcndet,", "tokens": ["Die", "ich", "in", "H\u00fch\u00b7ners", "Buch", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Scheint heute doch vom grauen Staar", "tokens": ["Scheint", "heu\u00b7te", "doch", "vom", "grau\u00b7en", "Staar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Mein geistig Auge ganz erblindet:", "tokens": ["Mein", "geis\u00b7tig", "Au\u00b7ge", "ganz", "er\u00b7blin\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die Reime bieten sich nicht dar,", "tokens": ["Die", "Rei\u00b7me", "bie\u00b7ten", "sich", "nicht", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ersch\u00f6pft sind alle schon auf ", "tokens": ["Er\u00b7sch\u00f6pft", "sind", "al\u00b7le", "schon", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PIS", "ADV", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Wird meine Schande offenbar,", "tokens": ["Wird", "mei\u00b7ne", "Schan\u00b7de", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "So lauf' ich vom Apoll Gefahr,", "tokens": ["So", "lauf'", "ich", "vom", "A\u00b7poll", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.29": {"text": "Da\u00df er wie Marsyas mich schindet.", "tokens": ["Da\u00df", "er", "wie", "Mar\u00b7syas", "mich", "schin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}