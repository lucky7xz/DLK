{"textgrid.poem.43227": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die letzte Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die letzte Nacht", "tokens": ["Die", "letz\u00b7te", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hab' ich durchwacht", "tokens": ["Hab'", "ich", "durch\u00b7wacht"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf Deiner kleinen Terrasse. \u2013", "tokens": ["Auf", "Dei\u00b7ner", "klei\u00b7nen", "Ter\u00b7ras\u00b7se", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So nah' Dir \u2013 so nah'!", "tokens": ["So", "nah'", "Dir", "\u2013", "so", "nah'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ADV", "PTKVZ", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Durch den Spalt ich sah", "tokens": ["Durch", "den", "Spalt", "ich", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Dein Antlitz, das liebe, blasse.", "tokens": ["Dein", "Ant\u00b7litz", ",", "das", "lie\u00b7be", ",", "blas\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Als Dein Gatte kam,", "tokens": ["Als", "Dein", "Gat\u00b7te", "kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Als er leise nahm", "tokens": ["Als", "er", "lei\u00b7se", "nahm"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Deine kleinen weichen H\u00e4nde,", "tokens": ["Dei\u00b7ne", "klei\u00b7nen", "wei\u00b7chen", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Als er k\u00fc\u00dfte die Stirn,", "tokens": ["Als", "er", "k\u00fc\u00df\u00b7te", "die", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Da gl\u00fchte mein Hirn,", "tokens": ["Da", "gl\u00fch\u00b7te", "mein", "Hirn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Da war mir, als schwankten die W\u00e4nde;", "tokens": ["Da", "war", "mir", ",", "als", "schwank\u00b7ten", "die", "W\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Er verlie\u00df Dein Gemach,", "tokens": ["Er", "ver\u00b7lie\u00df", "Dein", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Du schautest ihm nach", "tokens": ["Du", "schau\u00b7test", "ihm", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Mit m\u00fcdem, trostlosem Nicken;", "tokens": ["Mit", "m\u00fc\u00b7dem", ",", "trost\u00b7lo\u00b7sem", "Ni\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Auf Deinem Gesicht", "tokens": ["Auf", "Dei\u00b7nem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Kein Gl\u00fcck, kein Licht \u2013", "tokens": ["Kein", "Gl\u00fcck", ",", "kein", "Licht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Mir bangte vor Deinen Blicken.", "tokens": ["Mir", "bang\u00b7te", "vor", "Dei\u00b7nen", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Es knurrte Dein Hund,", "tokens": ["Es", "knurr\u00b7te", "Dein", "Hund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Da bebte Dein Mund,", "tokens": ["Da", "beb\u00b7te", "Dein", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "In's Dunkel sp\u00e4hte Dein Auge;", "tokens": ["In's", "Dun\u00b7kel", "sp\u00e4h\u00b7te", "Dein", "Au\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Ich stand wie im Bann", "tokens": ["Ich", "stand", "wie", "im", "Bann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "Und lauschte \u2013 und sann:", "tokens": ["Und", "lauschte", "\u2013", "und", "sann", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Was uns das Leben noch tauge? ...", "tokens": ["Was", "uns", "das", "Le\u00b7ben", "noch", "tau\u00b7ge", "?", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}