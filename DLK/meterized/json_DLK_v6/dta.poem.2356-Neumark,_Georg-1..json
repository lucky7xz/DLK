{"dta.poem.2356": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "An Deinem Wort' am h\u00f6chsten Gut'/", "tokens": ["An", "Dei\u00b7nem", "Wort'", "am", "h\u00f6chs\u00b7ten", "Gut'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' Ich Gott mein Ergetzen/", "tokens": ["Hab'", "Ich", "Gott", "mein", "Er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PPOSAT", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Denn wenn mich wil mein Fleisch und Blut", "tokens": ["Denn", "wenn", "mich", "wil", "mein", "Fleisch", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins S\u00fcndenfinstre setzen/", "tokens": ["Ins", "S\u00fcn\u00b7den\u00b7finst\u00b7re", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00df es eintzig und allein", "tokens": ["So", "mu\u00df", "es", "eint\u00b7zig", "und", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Lebenslicht und Fakkel seyn.", "tokens": ["Mein", "Le\u00b7bens\u00b7licht", "und", "Fak\u00b7kel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach diesem soll mein Hertze sich", "tokens": ["Nach", "die\u00b7sem", "soll", "mein", "Hert\u00b7ze", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VMFIN", "PPOSAT", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In wahrer Demuht richten/", "tokens": ["In", "wah\u00b7rer", "De\u00b7muht", "rich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein guter Geist nur leite mich/", "tokens": ["Dein", "gu\u00b7ter", "Geist", "nur", "lei\u00b7te", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit mein Thun und Tichten/", "tokens": ["Da\u00b7mit", "mein", "Thun", "und", "Tich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sey eintzig und allein geziehlt", "tokens": ["Sey", "eint\u00b7zig", "und", "al\u00b7lein", "ge\u00b7ziehlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dahin/ wo mir dein Wort befiehlt.", "tokens": ["Da\u00b7hin", "/", "wo", "mir", "dein", "Wort", "be\u00b7fiehlt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "BaRBArisch auf die S\u00fcndenbahn/", "tokens": ["BaR\u00b7BA\u00b7risch", "auf", "die", "S\u00fcn\u00b7den\u00b7bahn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Pflegt mich die Welt zu leiten/", "tokens": ["Pflegt", "mich", "die", "Welt", "zu", "lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn aber Ich Dein Wort seh' an/", "tokens": ["Wenn", "a\u00b7ber", "Ich", "Dein", "Wort", "seh'", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Merk' Ich die Eitelkeiten/", "tokens": ["Merk'", "Ich", "die", "Ei\u00b7tel\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dadurch die Seele wird gekr\u00e4nkt/", "tokens": ["Da\u00b7durch", "die", "See\u00b7le", "wird", "ge\u00b7kr\u00e4nkt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und in den hellschen Pfuhl versenkt.", "tokens": ["Und", "in", "den", "hell\u00b7schen", "Pfuhl", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Raubt Mir ein Ungl\u00fck die Geduld/", "tokens": ["Raubt", "Mir", "ein", "Un\u00b7gl\u00fck", "die", "Ge\u00b7duld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df mein Vertrauen hinket", "tokens": ["Da\u00df", "mein", "Ver\u00b7trau\u00b7en", "hin\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An Deiner v\u00e4terlichen Huld/", "tokens": ["An", "Dei\u00b7ner", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Huld", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mein Gem\u00fchte sinket/", "tokens": ["Und", "mein", "Ge\u00b7m\u00fch\u00b7te", "sin\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So ist Mir dein liebreiches Wort/", "tokens": ["So", "ist", "Mir", "dein", "lieb\u00b7rei\u00b7ches", "Wort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein s\u00fc\u00dfer Trost und starker Hort.", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Trost", "und", "star\u00b7ker", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "VoN manchem groben S\u00fcndenwust'", "tokens": ["VoN", "man\u00b7chem", "gro\u00b7ben", "S\u00fcn\u00b7den\u00b7wust'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kan befreyet bleiben/", "tokens": ["Ich", "kan", "be\u00b7fre\u00b7yet", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des frechen Fleisches schn\u00f6de Lust/", "tokens": ["Des", "fre\u00b7chen", "Flei\u00b7sches", "schn\u00f6\u00b7de", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan Ich dadurch vertreiben.", "tokens": ["Kan", "Ich", "da\u00b7durch", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn wenn durchs Wort der Geist obsiegt/", "tokens": ["Denn", "wenn", "durchs", "Wort", "der", "Geist", "ob\u00b7siegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gar bald das S\u00fcndenfteisch erliegt.", "tokens": ["Gar", "bald", "das", "S\u00fcn\u00b7den\u00b7fteisch", "er\u00b7liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}