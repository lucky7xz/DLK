{"textgrid.poem.55523": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Lyde", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euer Beifall macht mich freier,", "tokens": ["Eu\u00b7er", "Bei\u00b7fall", "macht", "mich", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchen, h\u00f6rt ein neues Lied.", "tokens": ["M\u00e4d\u00b7chen", ",", "h\u00f6rt", "ein", "neu\u00b7es", "Lied", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch verzeiht, wenn meine Leier", "tokens": ["Doch", "ver\u00b7zeiht", ",", "wenn", "mei\u00b7ne", "Lei\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht von jenem heil'gen Feuer", "tokens": ["Nicht", "von", "je\u00b7nem", "heil'\u00b7gen", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der geweihten Dichter gl\u00fcht.", "tokens": ["Der", "ge\u00b7weih\u00b7ten", "Dich\u00b7ter", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt von mir, was wenig wissen,", "tokens": ["H\u00f6rt", "von", "mir", ",", "was", "we\u00b7nig", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt's, und denket nach dabei:", "tokens": ["H\u00f6rt's", ",", "und", "den\u00b7ket", "nach", "da\u00b7bei", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "APPR", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df, wenn zwei sich z\u00e4rtlich k\u00fcssen,", "tokens": ["Da\u00df", ",", "wenn", "zwei", "sich", "z\u00e4rt\u00b7lich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "CARD", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gern sich sehn und ungern missen,", "tokens": ["Gern", "sich", "sehn", "und", "un\u00b7gern", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVINF", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Es nicht stets aus Liebe sei.", "tokens": ["Es", "nicht", "stets", "aus", "Lie\u00b7be", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lyde brannt von einem Blicke", "tokens": ["Ly\u00b7de", "brannt", "von", "ei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr Aminen, er f\u00fcr sie;", "tokens": ["F\u00fcr", "A\u00b7mi\u00b7nen", ",", "er", "f\u00fcr", "sie", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ein widriges Geschicke", "tokens": ["Doch", "ein", "wid\u00b7ri\u00b7ges", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Hinderte noch beider Gl\u00fccke,", "tokens": ["Hin\u00b7der\u00b7te", "noch", "bei\u00b7der", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihre Eltern schliefen nie.", "tokens": ["Ih\u00b7re", "El\u00b7tern", "schlie\u00b7fen", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wachsamkeit wird euch nichts taugen,", "tokens": ["Wach\u00b7sam\u00b7keit", "wird", "euch", "nichts", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wenn die T\u00f6chter unser sind;", "tokens": ["Wenn", "die", "T\u00f6ch\u00b7ter", "un\u00b7ser", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eltern, habet hundert Augen,", "tokens": ["El\u00b7tern", ",", "ha\u00b7bet", "hun\u00b7dert", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4dchen, wenn sie List gebrauchen,", "tokens": ["M\u00e4d\u00b7chen", ",", "wenn", "sie", "List", "ge\u00b7brau\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Machen hundert Augen blind.", "tokens": ["Ma\u00b7chen", "hun\u00b7dert", "Au\u00b7gen", "blind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Listig hofft sie, eine Stunde", "tokens": ["Lis\u00b7tig", "hofft", "sie", ",", "ei\u00b7ne", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre W\u00e4chter los zu sein.", "tokens": ["Ih\u00b7re", "W\u00e4ch\u00b7ter", "los", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich kommt die Sch\u00e4ferstunde,", "tokens": ["End\u00b7lich", "kommt", "die", "Sch\u00e4\u00b7fer\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von ihrem hei\u00dfen Munde", "tokens": ["Und", "von", "ih\u00b7rem", "hei\u00b7\u00dfen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Saugt Amin die Wollust ein.", "tokens": ["Saugt", "A\u00b7min", "die", "Wol\u00b7lust", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "So geno\u00df, entfernt vom Neide,", "tokens": ["So", "ge\u00b7no\u00df", ",", "ent\u00b7fernt", "vom", "Nei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er noch manchen s\u00fc\u00dfen Ku\u00df.", "tokens": ["Er", "noch", "man\u00b7chen", "s\u00fc\u00b7\u00dfen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch er ward so vieler Beute", "tokens": ["Doch", "er", "ward", "so", "vie\u00b7ler", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberdr\u00fcssig. Jede Freude", "tokens": ["\u00dc\u00b7berd\u00b7r\u00fcs\u00b7sig", ".", "Je\u00b7de", "Freu\u00b7de"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Endigt sich mit dem Genu\u00df.", "tokens": ["En\u00b7digt", "sich", "mit", "dem", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Ist wohl bei des Blutes Wallen,", "tokens": ["Ist", "wohl", "bei", "des", "Blu\u00b7tes", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Denkt er, immer Liebe da?", "tokens": ["Denkt", "er", ",", "im\u00b7mer", "Lie\u00b7be", "da", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebt sie mich denn wohl vor allen?", "tokens": ["Liebt", "sie", "mich", "denn", "wohl", "vor", "al\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder hab ich ihr gefallen,", "tokens": ["O\u00b7der", "hab", "ich", "ihr", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil sie mich am ersten sah?", "tokens": ["Weil", "sie", "mich", "am", "ers\u00b7ten", "sah", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Einst spricht er, dies auszusp\u00fcren:", "tokens": ["Einst", "spricht", "er", ",", "dies", "aus\u00b7zu\u00b7sp\u00fc\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PDS", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbach, wie qu\u00e4lt mein Vater mich!", "tokens": ["\u00bb", "ach", ",", "wie", "qu\u00e4lt", "mein", "Va\u00b7ter", "mich", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern soll ich die Herde f\u00fchren \u2013", "tokens": ["Fern", "soll", "ich", "die", "Her\u00b7de", "f\u00fch\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmel! Dich soll ich verlieren!", "tokens": ["Him\u00b7mel", "!", "Dich", "soll", "ich", "ver\u00b7lie\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ha, das Leben eh'r als dich!", "tokens": ["Ha", ",", "das", "Le\u00b7ben", "eh'r", "als", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "ADV", "KOUS", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Liebste, nein, ich komme wieder,", "tokens": ["Liebs\u00b7te", ",", "nein", ",", "ich", "kom\u00b7me", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch der beste Freund von mir\u00ab", "tokens": ["Doch", "der", "bes\u00b7te", "Freund", "von", "mir", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "(hier sah sie zur Erde nieder)", "tokens": ["(", "hier", "sah", "sie", "zur", "Er\u00b7de", "nie\u00b7der", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbsinget angenehme Lieder,", "tokens": ["\u00bb", "sin\u00b7get", "an\u00b7ge\u00b7neh\u00b7me", "Lie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Diesen Freund, den la\u00df ich dir.\u00ab", "tokens": ["Die\u00b7sen", "Freund", ",", "den", "la\u00df", "ich", "dir", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDAT", "NN", "$,", "ART", "VVIMP", "PPER", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Lyde denkt an keine T\u00fccke,", "tokens": ["Ly\u00b7de", "denkt", "an", "kei\u00b7ne", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weint und geht es weinend ein.", "tokens": ["Weint", "und", "geht", "es", "wei\u00b7nend", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ungern flieht Amin sein Gl\u00fccke,", "tokens": ["Un\u00b7gern", "flieht", "A\u00b7min", "sein", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Listig bleibt der Freund zur\u00fccke,", "tokens": ["Lis\u00b7tig", "bleibt", "der", "Freund", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oft ist er mit ihr allein.", "tokens": ["Oft", "ist", "er", "mit", "ihr", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Viel singt er von Glut und Liebe,", "tokens": ["Viel", "singt", "er", "von", "Glut", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie wird feurig, er wird k\u00fchn.", "tokens": ["Sie", "wird", "feu\u00b7rig", ",", "er", "wird", "k\u00fchn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie empfindet neue Triebe,", "tokens": ["Sie", "emp\u00b7fin\u00b7det", "neu\u00b7e", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Gelegenheit macht Diebe.", "tokens": ["Und", "Ge\u00b7le\u00b7gen\u00b7heit", "macht", "Die\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Endlich \u2013 Gute Nacht, Amin.", "tokens": ["End\u00b7lich", "\u2013", "Gu\u00b7te", "Nacht", ",", "A\u00b7min", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$(", "ADJA", "NN", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Kinder, seht, da m\u00fc\u00dft ihr wachen,", "tokens": ["Kin\u00b7der", ",", "seht", ",", "da", "m\u00fc\u00dft", "ihr", "wa\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch vom Irrtum zu befrein.", "tokens": ["Euch", "vom", "Irr\u00b7tum", "zu", "be\u00b7fr\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaubet nie den Schein der Sachen,", "tokens": ["Glau\u00b7bet", "nie", "den", "Schein", "der", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sucht euch ja gewi\u00df zu machen,", "tokens": ["Sucht", "euch", "ja", "ge\u00b7wi\u00df", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh ihr glaubt, geliebt zu sein.", "tokens": ["Eh", "ihr", "glaubt", ",", "ge\u00b7liebt", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}