{"dta.poem.3884": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "9) Als er einem Freund etwas \u00fcbersendet.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Bruder! lebst du noch? bey nahe glaub", "tokens": ["Herr", "Bru\u00b7der", "!", "lebst", "du", "noch", "?", "bey", "na\u00b7he", "glaub"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$.", "VVFIN", "PPER", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ichs nicht,", "tokens": ["ichs", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "PTKNEG", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Weil seit acht Wochen her dein Bund den Wech-", "tokens": ["Weil", "seit", "acht", "Wo\u00b7chen", "her", "dein", "Bund", "den", "Wech"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "CARD", "NN", "APZR", "PPOSAT", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "sel bricht", "tokens": ["sel", "bricht"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Von Antwort, Brief und Schrifft; nun qu\u00e4lt sich", "tokens": ["Von", "Ant\u00b7wort", ",", "Brief", "und", "Schrifft", ";", "nun", "qu\u00e4lt", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "mein Verlangen,", "tokens": ["mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Mit nechsten (wo du lebst) die Antwort zu empfange\u0303.", "tokens": ["Mit", "nechs\u00b7ten", "(", "wo", "du", "lebst", ")", "die", "Ant\u00b7wort", "zu", "emp\u00b7fang\u1ebd", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$(", "PWAV", "PPER", "VVFIN", "$(", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "O! s\u00e4ume nicht, ", "tokens": ["O", "!", "s\u00e4u\u00b7me", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und breche halb entz\u00fcckt davon das Siegel auf.", "tokens": ["Und", "bre\u00b7che", "halb", "ent\u00b7z\u00fcckt", "da\u00b7von", "das", "Sie\u00b7gel", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVPP", "PAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vors erste nimm von mir ein P\u00e4ckgen feine Sachen,", "tokens": ["Vors", "ers\u00b7te", "nimm", "von", "mir", "ein", "P\u00e4ck\u00b7gen", "fei\u00b7ne", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du kanst sie dir vielleicht noch wohl zu Nutze mache\u0303.", "tokens": ["Du", "kanst", "sie", "dir", "viel\u00b7leicht", "noch", "wohl", "zu", "Nut\u00b7ze", "mach\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn, wo du itzo wohnst, bl\u00fcht keine Wissenschafft,", "tokens": ["Denn", ",", "wo", "du", "it\u00b7zo", "wohnst", ",", "bl\u00fcht", "kei\u00b7ne", "Wis\u00b7sen\u00b7schafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ADV", "ADV", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vielmehr, wie man so Geld als G\u00fcter an sich rafft.", "tokens": ["Viel\u00b7mehr", ",", "wie", "man", "so", "Geld", "als", "G\u00fc\u00b7ter", "an", "sich", "rafft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIS", "ADV", "NN", "KOUS", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da schweigt der Musen Lust mit ihren sanften Th\u00f6-", "tokens": ["Da", "schweigt", "der", "Mu\u00b7sen", "Lust", "mit", "ih\u00b7ren", "sanf\u00b7ten", "Th\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "nen,", "tokens": ["nen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Und du geh\u00f6rst entfernt zu ihren frommen S\u00f6hnen.", "tokens": ["Und", "du", "ge\u00b7h\u00f6rst", "ent\u00b7fernt", "zu", "ih\u00b7ren", "from\u00b7men", "S\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Verzeihe, liebster ", "tokens": ["Ver\u00b7zei\u00b7he", ",", "liebs\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Weil itzt der Bettelvoigt in meiner Tasch regiert;", "tokens": ["Weil", "itzt", "der", "Bet\u00b7tel\u00b7voigt", "in", "mei\u00b7ner", "Tasch", "re\u00b7giert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Jzt bin ich warrlich arm, ich bin ein armer Schlucker,", "tokens": ["Jzt", "bin", "ich", "warr\u00b7lich", "arm", ",", "ich", "bin", "ein", "ar\u00b7mer", "Schlu\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "ADJD", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Jtzt trinck ich gleich ", "tokens": ["Jtzt", "trinck", "ich", "gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Mein Wechsel ist gehemmt, und dieses mit Gewalt,", "tokens": ["Mein", "Wech\u00b7sel", "ist", "ge\u00b7hemmt", ",", "und", "die\u00b7ses", "mit", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "KON", "PDS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Weil auser unsrer Me\u00df kein Mensch denselben zahlt.", "tokens": ["Weil", "au\u00b7ser", "uns\u00b7rer", "Me\u00df", "kein", "Mensch", "den\u00b7sel\u00b7ben", "zahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PIAT", "NN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn sich mein Reichthum mehrt, denn solst du", "tokens": ["Wenn", "sich", "mein", "Reicht\u00b7hum", "mehrt", ",", "denn", "solst", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,", "KON", "VMFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "auch erfahren,", "tokens": ["auch", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Ich will zu deiner Lust gar keine Kosten spahren:", "tokens": ["Ich", "will", "zu", "dei\u00b7ner", "Lust", "gar", "kei\u00b7ne", "Kos\u00b7ten", "spah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Alleine itzo ist das Gl\u00fcck auf mich erbost.", "tokens": ["Al\u00b7lei\u00b7ne", "it\u00b7zo", "ist", "das", "Gl\u00fcck", "auf", "mich", "er\u00b7bost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Leb wohl, mein Hertzens Freund, geliebter Reben-", "tokens": ["Leb", "wohl", ",", "mein", "Hert\u00b7zens", "Freund", ",", "ge\u00b7lieb\u00b7ter", "Re\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "NN", "$,", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Trost!", "tokens": ["Trost", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}