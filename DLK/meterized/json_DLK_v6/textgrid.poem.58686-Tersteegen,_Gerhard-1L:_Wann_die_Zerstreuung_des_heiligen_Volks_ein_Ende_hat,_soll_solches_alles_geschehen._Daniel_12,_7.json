{"textgrid.poem.58686": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann die Zerstreuung des heiligen Volks ein Ende hat, soll solches alles geschehen. Daniel 12, 7", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann die Zerstreuung des heiligen Volks ein Ende hat, soll solches alles geschehen. Daniel 12, 7", "tokens": ["Wann", "die", "Zer\u00b7streu\u00b7ung", "des", "hei\u00b7li\u00b7gen", "Volks", "ein", "En\u00b7de", "hat", ",", "soll", "sol\u00b7ches", "al\u00b7les", "ge\u00b7sche\u00b7hen", ".", "Da\u00b7ni\u00b7el", "12", ",", "7"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "$,", "VMFIN", "PIAT", "PIS", "VVPP", "$.", "NE", "CARD", "$,", "CARD"], "meter": "+--+--+--+-+-+-+-+--+-+-+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ihr Zeitenforscher, merkt die Zeichen letzter Zeiten,", "tokens": ["Ihr", "Zei\u00b7ten\u00b7for\u00b7scher", ",", "merkt", "die", "Zei\u00b7chen", "letz\u00b7ter", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann die Erf\u00fcllung kommt der Wunder, die man liest;", "tokens": ["Wann", "die", "Er\u00b7f\u00fcl\u00b7lung", "kommt", "der", "Wun\u00b7der", ",", "die", "man", "liest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Glaubt's, Christi herrlich' Reich wird er euch nicht bereiten,", "tokens": ["Glaubt's", ",", "Chris\u00b7ti", "herr\u00b7lich'", "Reich", "wird", "er", "euch", "nicht", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "VVFIN", "NN", "VAFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Solange Herz und Sinn von ihm zerstreuet ist!", "tokens": ["So\u00b7lan\u00b7ge", "Herz", "und", "Sinn", "von", "ihm", "zer\u00b7streu\u00b7et", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verrechnet euch doch nicht, ihr schreibt viel Zahl und Jahren,", "tokens": ["Ver\u00b7rech\u00b7net", "euch", "doch", "nicht", ",", "ihr", "schreibt", "viel", "Zahl", "und", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PPER", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Viele bringt ins Eins, im Nun bleibt stille stehn;", "tokens": ["Das", "Vie\u00b7le", "bringt", "ins", "Eins", ",", "im", "Nun", "bleibt", "stil\u00b7le", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "APPRART", "NN", "$,", "APPRART", "ADV", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer ganz vereinigt ist, kann stets in sich erfahren", "tokens": ["Wer", "ganz", "ver\u00b7ei\u00b7nigt", "ist", ",", "kann", "stets", "in", "sich", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "VMFIN", "ADV", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das tausendj\u00e4hr'ge Reich und alle Wunder sehn!", "tokens": ["Das", "tau\u00b7send\u00b7j\u00e4hr'\u00b7ge", "Reich", "und", "al\u00b7le", "Wun\u00b7der", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}