, "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "PDS", "$,", "PWS", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nichts widersprechendes und tiefe Weisheit finden.", "tokens": ["Nichts", "wi\u00b7der\u00b7spre\u00b7chen\u00b7des", "und", "tie\u00b7fe", "Weis\u00b7heit", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Genug, da\u00df unser Geist die Lehren h\u00f6h'rer Art,", "tokens": ["Ge\u00b7nug", ",", "da\u00df", "un\u00b7ser", "Geist", "die", "Leh\u00b7ren", "h\u00f6h'\u00b7rer", "Art", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die er noch st\u00fcckweis lernt, auf eine Zeit bewahrt,", "tokens": ["Die", "er", "noch", "st\u00fcck\u00b7weis", "lernt", ",", "auf", "ei\u00b7ne", "Zeit", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wo er, vom Nebel frey, der itzund ihn umringet,", "tokens": ["Wo", "er", ",", "vom", "Ne\u00b7bel", "frey", ",", "der", "it\u00b7zund", "ihn", "um\u00b7rin\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPRART", "NN", "PTKVZ", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In den Zusammenhang mit sch\u00e4rfern Blicken dringet.", "tokens": ["In", "den", "Zu\u00b7sam\u00b7men\u00b7hang", "mit", "sch\u00e4r\u00b7fern", "Bli\u00b7cken", "drin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.21": {"text": "So fa\u00dft ein zartes Kind des Vaters Unterricht,", "tokens": ["So", "fa\u00dft", "ein", "zar\u00b7tes", "Kind", "des", "Va\u00b7ters", "Un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Erreicht fein schwacher Sinn der Lehren Grund noch nicht;", "tokens": ["Er\u00b7reicht", "fein", "schwa\u00b7cher", "Sinn", "der", "Leh\u00b7ren", "Grund", "noch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "ART", "NN", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es glaubt, und wird dadurch nur jetzo vorbereitet,", "tokens": ["Es", "glaubt", ",", "und", "wird", "da\u00b7durch", "nur", "jet\u00b7zo", "vor\u00b7be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VAFIN", "PAV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bis es mehr Einsicht einst bey reiferm Alter leitet.", "tokens": ["Bis", "es", "mehr", "Ein\u00b7sicht", "einst", "bey", "rei\u00b7ferm", "Al\u00b7ter", "lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "O gl\u00fccklich, wen Vernunft, so wie der Glaube lenkt!", "tokens": ["O", "gl\u00fcck\u00b7lich", ",", "wen", "Ver\u00b7nunft", ",", "so", "wie", "der", "Glau\u00b7be", "lenkt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWS", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der beyder Werth verehrt; und wenn er glaubt, auch denkt.", "tokens": ["Der", "bey\u00b7der", "Werth", "ver\u00b7ehrt", ";", "und", "wenn", "er", "glaubt", ",", "auch", "denkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$.", "KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Als g\u00f6ttlich wird bey ihm nicht alte Meynung gelten,", "tokens": ["Als", "g\u00f6tt\u00b7lich", "wird", "bey", "ihm", "nicht", "al\u00b7te", "Mey\u00b7nung", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "APPR", "PPER", "PTKNEG", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.28": {"text": "Die Freunde der Vernunft wird er nicht Ketzer schelten;", "tokens": ["Die", "Freun\u00b7de", "der", "Ver\u00b7nunft", "wird", "er", "nicht", "Ket\u00b7zer", "schel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "D