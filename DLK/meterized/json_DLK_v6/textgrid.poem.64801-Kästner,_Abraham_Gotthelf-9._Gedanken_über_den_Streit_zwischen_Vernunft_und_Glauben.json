{"textgrid.poem.64801": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "9. Gedanken \u00fcber den Streit zwischen Vernunft und Glauben", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wirkt, o ", "tokens": ["Was", "wirkt", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "$,", "FM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Der Glauben und Vernunft so lange schon entzweyt?", "tokens": ["Der", "Glau\u00b7ben", "und", "Ver\u00b7nunft", "so", "lan\u00b7ge", "schon", "ent\u00b7zweyt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie ist's, vertr\u00e4gt das Buch, das wir als g\u00f6ttlich preisen,", "tokens": ["Wie", "ist's", ",", "ver\u00b7tr\u00e4gt", "das", "Buch", ",", "das", "wir", "als", "g\u00f6tt\u00b7lich", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "KOUS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die scharfe Probe nicht von Schl\u00fcssen tiefer Weisen?", "tokens": ["Die", "schar\u00b7fe", "Pro\u00b7be", "nicht", "von", "Schl\u00fcs\u00b7sen", "tie\u00b7fer", "Wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verdient man eher nicht, da\u00df uns der Sch\u00f6pfer lehrt,", "tokens": ["Ver\u00b7di\u00b7ent", "man", "e\u00b7her", "nicht", ",", "da\u00df", "uns", "der", "Sch\u00f6p\u00b7fer", "lehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Als bis man die Vernunft, die er uns gab, verschw\u00f6rt?", "tokens": ["Als", "bis", "man", "die", "Ver\u00b7nunft", ",", "die", "er", "uns", "gab", ",", "ver\u00b7schw\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "PIS", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit Rechte wird bey euch des Zankes Grund gesuchet,", "tokens": ["Mit", "Rech\u00b7te", "wird", "bey", "euch", "des", "Zan\u00b7kes", "Grund", "ge\u00b7su\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du, der des Glaubens lacht, du, der der Weisheit fluchet;", "tokens": ["Du", ",", "der", "des", "Glau\u00b7bens", "lacht", ",", "du", ",", "der", "der", "Weis\u00b7heit", "flu\u00b7chet", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der, wenn er die Vernunft mit steifen F\u00fc\u00dfen tritt,", "tokens": ["Der", ",", "wenn", "er", "die", "Ver\u00b7nunft", "mit", "stei\u00b7fen", "F\u00fc\u00b7\u00dfen", "tritt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Meynt, es geschehe Gott ein gro\u00dfer Dienst damit:", "tokens": ["Meynt", ",", "es", "ge\u00b7sche\u00b7he", "Gott", "ein", "gro\u00b7\u00dfer", "Dienst", "da\u00b7mit", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "NN", "ART", "ADJA", "NN", "PAV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Und Der will muntern Witz und scharfes Denken zeigen.", "tokens": ["Und", "Der", "will", "mun\u00b7tern", "Witz", "und", "schar\u00b7fes", "Den\u00b7ken", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Glaubens Sp\u00f6tter seyn ist gro\u00dfen Geistern eigen;", "tokens": ["Des", "Glau\u00b7bens", "Sp\u00f6t\u00b7ter", "seyn", "ist", "gro\u00b7\u00dfen", "Geis\u00b7tern", "ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "VAFIN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und beyde folgen nur, wohin ihr Wahn sie rei\u00dft,", "tokens": ["Und", "bey\u00b7de", "fol\u00b7gen", "nur", ",", "wo\u00b7hin", "ihr", "Wahn", "sie", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "$,", "PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Der Vernunft benennt, und Jener Glauben hei\u00dft.", "tokens": ["Den", "Der", "Ver\u00b7nunft", "be\u00b7nennt", ",", "und", "Je\u00b7ner", "Glau\u00b7ben", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "KON", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verlangt nicht, wollt ihr uns der Gottheit Willen lehren,", "tokens": ["Ver\u00b7langt", "nicht", ",", "wollt", "ihr", "uns", "der", "Got\u00b7theit", "Wil\u00b7len", "leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "VMFIN", "PPER", "PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als ein prophetisch Wort, was ihr nur sagt, zu ehren:", "tokens": ["Als", "ein", "pro\u00b7phe\u00b7tisch", "Wort", ",", "was", "ihr", "nur", "sagt", ",", "zu", "eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gleicht euren Feinden nicht, die ihr den Glauben schm\u00e4ht,", "tokens": ["Gleicht", "eu\u00b7ren", "Fein\u00b7den", "nicht", ",", "die", "ihr", "den", "Glau\u00b7ben", "schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und richtet nicht davon, wovon ihr nichts versteht.", "tokens": ["Und", "rich\u00b7tet", "nicht", "da\u00b7von", ",", "wo\u00b7von", "ihr", "nichts", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PAV", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Sch\u00f6pfer will uns nicht die Macht zu denken rauben,", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "will", "uns", "nicht", "die", "Macht", "zu", "den\u00b7ken", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch heischt er nicht von uns zu wissen, nur zu glauben;", "tokens": ["Doch", "heischt", "er", "nicht", "von", "uns", "zu", "wis\u00b7sen", ",", "nur", "zu", "glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wozu er uns bestimmt, wodurch man ihm gef\u00e4llt,", "tokens": ["Wo\u00b7zu", "er", "uns", "be\u00b7stimmt", ",", "wo\u00b7durch", "man", "ihm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "PWAV", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie das Gewissen Ruh', die Seele Trost erh\u00e4lt,", "tokens": ["Wie", "das", "Ge\u00b7wis\u00b7sen", "Ruh'", ",", "die", "See\u00b7le", "Trost", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bem\u00fcht sich die Vernunft vergebens zu ergr\u00fcnden,", "tokens": ["Be\u00b7m\u00fcht", "sich", "die", "Ver\u00b7nunft", "ver\u00b7ge\u00b7bens", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wird, entdeckt sie was, nur \u00e4ngstlich Zweifeln finden.", "tokens": ["Und", "wird", ",", "ent\u00b7deckt", "sie", "was", ",", "nur", "\u00e4ngst\u00b7lich", "Zwei\u00b7feln", "fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "VVFIN", "PPER", "PIS", "$,", "ADV", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie um den Himmel sich der lichte Milchweg zieht,", "tokens": ["Wie", "um", "den", "Him\u00b7mel", "sich", "der", "lich\u00b7te", "Milch\u00b7weg", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sieht unser Auge dort, und wei\u00df nicht, was es sieht;", "tokens": ["Sieht", "un\u00b7ser", "Au\u00b7ge", "dort", ",", "und", "wei\u00df", "nicht", ",", "was", "es", "sieht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,", "KON", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und da\u00df den wei\u00dfen Glanz ein Heer von Sonnen schicket,", "tokens": ["Und", "da\u00df", "den", "wei\u00b7\u00dfen", "Glanz", "ein", "Heer", "von", "Son\u00b7nen", "schi\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hat ein ", "tokens": ["Hat", "ein"], "token_info": ["word", "word"], "pos": ["VAFIN", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "So ward auch manchem Geist, der \u00fcber andre steigt,", "tokens": ["So", "ward", "auch", "man\u00b7chem", "Geist", ",", "der", "\u00fc\u00b7ber", "and\u00b7re", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "$,", "PRELS", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Viel, was der Christe glaubt, durch die Vernunft gezeigt,", "tokens": ["Viel", ",", "was", "der", "Chris\u00b7te", "glaubt", ",", "durch", "die", "Ver\u00b7nunft", "ge\u00b7zeigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch dunkel, ungewi\u00df, nicht wie der Glaube lehret,", "tokens": ["Doch", "dun\u00b7kel", ",", "un\u00b7ge\u00b7wi\u00df", ",", "nicht", "wie", "der", "Glau\u00b7be", "leh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,", "PTKNEG", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den selbsten die Vernunft mit eignem Lallen ehret.", "tokens": ["Den", "selbs\u00b7ten", "die", "Ver\u00b7nunft", "mit", "eig\u00b7nem", "Lal\u00b7len", "eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wem gleicht ihr, die ihr hier Vernunft alleine nennt?", "tokens": ["Wem", "gleicht", "ihr", ",", "die", "ihr", "hier", "Ver\u00b7nunft", "al\u00b7lei\u00b7ne", "nennt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dem, dessen blo\u00dfes Aug' des Milchwegs Sterne kennt;", "tokens": ["Dem", ",", "des\u00b7sen", "blo\u00b7\u00dfes", "Aug'", "des", "Milch\u00b7wegs", "Ster\u00b7ne", "kennt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELAT", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hier m\u00fc\u00dft ihr euch so gut, als eure Gegner finden,", "tokens": ["Hier", "m\u00fc\u00dft", "ihr", "euch", "so", "gut", ",", "als", "eu\u00b7re", "Geg\u00b7ner", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die sich, um mehr zu sehn, die Augen fest verbinden.", "tokens": ["Die", "sich", ",", "um", "mehr", "zu", "sehn", ",", "die", "Au\u00b7gen", "fest", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein forschender Verstand, der sich im Denken \u00fcbt,", "tokens": ["Ein", "for\u00b7schen\u00b7der", "Ver\u00b7stand", ",", "der", "sich", "im", "Den\u00b7ken", "\u00fcbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wahrheit untersucht, und nur Beweise liebt,", "tokens": ["Die", "Wahr\u00b7heit", "un\u00b7ter\u00b7sucht", ",", "und", "nur", "Be\u00b7wei\u00b7se", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "W\u00fcrd' oft ein Christe seyn, und unsrer Kirche n\u00fctzen,", "tokens": ["W\u00fcrd'", "oft", "ein", "Chris\u00b7te", "seyn", ",", "und", "uns\u00b7rer", "Kir\u00b7che", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VAINF", "$,", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und gr\u00fcndlich und geschickt die Lehren unterst\u00fctzen,", "tokens": ["Und", "gr\u00fcnd\u00b7lich", "und", "ge\u00b7schickt", "die", "Leh\u00b7ren", "un\u00b7ter\u00b7st\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die wahr und richtig sind: so aber, wenn er h\u00f6rt,", "tokens": ["Die", "wahr", "und", "rich\u00b7tig", "sind", ":", "so", "a\u00b7ber", ",", "wenn", "er", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VAFIN", "$.", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df unser Glaube nichts, als solche S\u00e4tze lehrt,", "tokens": ["Da\u00df", "un\u00b7ser", "Glau\u00b7be", "nichts", ",", "als", "sol\u00b7che", "S\u00e4t\u00b7ze", "lehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bey denen die Vernunft, mit Gr\u00fcndlichkeit und Wissen", "tokens": ["Bey", "de\u00b7nen", "die", "Ver\u00b7nunft", ",", "mit", "Gr\u00fcnd\u00b7lich\u00b7keit", "und", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vertrieben und verdammt, dem Glauben weichen m\u00fcssen,", "tokens": ["Ver\u00b7trie\u00b7ben", "und", "ver\u00b7dammt", ",", "dem", "Glau\u00b7ben", "wei\u00b7chen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Tritt er so bald zur\u00fcck. Denn das ist offenbar,", "tokens": ["Tritt", "er", "so", "bald", "zu\u00b7r\u00fcck", ".", "Denn", "das", "ist", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$.", "KON", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was die Vernunft ihm sagt, h\u00e4lt er gewi\u00df f\u00fcr wahr;", "tokens": ["Was", "die", "Ver\u00b7nunft", "ihm", "sagt", ",", "h\u00e4lt", "er", "ge\u00b7wi\u00df", "f\u00fcr", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn Glauben und Vernunft einander widersprechen,", "tokens": ["Wenn", "Glau\u00b7ben", "und", "Ver\u00b7nunft", "ein\u00b7an\u00b7der", "wi\u00b7der\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So ist der Glaube falsch. Die Schuld von dem Verbrechen,", "tokens": ["So", "ist", "der", "Glau\u00b7be", "falsch", ".", "Die", "Schuld", "von", "dem", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$.", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In das der Freygeist f\u00e4llt, geh\u00f6rt dem Lehrer zu,", "tokens": ["In", "das", "der", "Frey\u00b7geist", "f\u00e4llt", ",", "ge\u00b7h\u00f6rt", "dem", "Leh\u00b7rer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der auf den Freygeist schm\u00e4ht; o lie\u00df er ihn in Ruh!", "tokens": ["Der", "auf", "den", "Frey\u00b7geist", "schm\u00e4ht", ";", "o", "lie\u00df", "er", "ihn", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "FM", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "O h\u00f6rt' er einstens auf, die Weisheit zu verfluchen!", "tokens": ["O", "h\u00f6rt'", "er", "eins\u00b7tens", "auf", ",", "die", "Weis\u00b7heit", "zu", "ver\u00b7flu\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Freygeist w\u00fcrde bald die Lehren untersuchen,", "tokens": ["Der", "Frey\u00b7geist", "w\u00fcr\u00b7de", "bald", "die", "Leh\u00b7ren", "un\u00b7ter\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die man f\u00fcr heilig h\u00e4lt: und ihrer Wahrheit Licht", "tokens": ["Die", "man", "f\u00fcr", "hei\u00b7lig", "h\u00e4lt", ":", "und", "ih\u00b7rer", "Wahr\u00b7heit", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "ADJD", "VVFIN", "$.", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Durchstrahlte seine Brust. Doch so geschieht es nicht,", "tokens": ["Durch\u00b7strahl\u00b7te", "sei\u00b7ne", "Brust", ".", "Doch", "so", "ge\u00b7schieht", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So scheut er sich sogar, die Bibel anzur\u00fchren,", "tokens": ["So", "scheut", "er", "sich", "so\u00b7gar", ",", "die", "Bi\u00b7bel", "an\u00b7zu\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Aus Furcht, der Menschheit Werth auf einmal zu verlieren.", "tokens": ["Aus", "Furcht", ",", "der", "Menschheit", "Werth", "auf", "ein\u00b7mal", "zu", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "NN", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Nein, g\u00f6ttliche Vernunft, nie ward dein wahrer Freund", "tokens": ["Nein", ",", "g\u00f6tt\u00b7li\u00b7che", "Ver\u00b7nunft", ",", "nie", "ward", "dein", "wah\u00b7rer", "Freund"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf eigne Kr\u00e4fte k\u00fchn, der Offenbarung feind.", "tokens": ["Auf", "eig\u00b7ne", "Kr\u00e4f\u00b7te", "k\u00fchn", ",", "der", "Of\u00b7fen\u00b7ba\u00b7rung", "feind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geleitet durch dein Licht, bis an der Menschheit Grenzen,", "tokens": ["Ge\u00b7lei\u00b7tet", "durch", "dein", "Licht", ",", "bis", "an", "der", "Menschheit", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,", "KOUS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sieht er, wo du ihm fehlst, der Gnade Strahlen gl\u00e4nzen.", "tokens": ["Sieht", "er", ",", "wo", "du", "ihm", "fehlst", ",", "der", "Gna\u00b7de", "Strah\u00b7len", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch ungelehrter Stolz, den du nur selten f\u00fchrst,", "tokens": ["Doch", "un\u00b7ge\u00b7lehr\u00b7ter", "Stolz", ",", "den", "du", "nur", "sel\u00b7ten", "f\u00fchrst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Merkt nicht, wie weit du gehst, noch wo du dich verlierst.", "tokens": ["Merkt", "nicht", ",", "wie", "weit", "du", "gehst", ",", "noch", "wo", "du", "dich", "ver\u00b7lierst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,", "KON", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du lehrst vom Menschentand die Offenbarung trennen,", "tokens": ["Du", "lehrst", "vom", "Men\u00b7schen\u00b7tand", "die", "Of\u00b7fen\u00b7ba\u00b7rung", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du lehrst uns ihren Zweck und ihren Werth erkennen.", "tokens": ["Du", "lehrst", "uns", "ih\u00b7ren", "Zweck", "und", "ih\u00b7ren", "Werth", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nicht darum gab sie uns der Sch\u00f6pfer, der uns liebt,", "tokens": ["Nicht", "da\u00b7rum", "gab", "sie", "uns", "der", "Sch\u00f6p\u00b7fer", ",", "der", "uns", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PAV", "VVFIN", "PPER", "PRF", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ein Erfinder sich an ihr in Schl\u00fcssen \u00fcbt;", "tokens": ["Da\u00df", "ein", "Er\u00b7fin\u00b7der", "sich", "an", "ihr", "in", "Schl\u00fcs\u00b7sen", "\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im weiten Raum der Welt, im Abgrund unsrer Seelen", "tokens": ["Im", "wei\u00b7ten", "Raum", "der", "Welt", ",", "im", "Ab\u00b7grund", "uns\u00b7rer", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "La\u00dft uns den Gegenstand von unserm Forschen w\u00e4hlen.", "tokens": ["La\u00dft", "uns", "den", "Ge\u00b7gen\u00b7stand", "von", "un\u00b7serm", "For\u00b7schen", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Lehre, welche selbst die Einfalt f\u00fchren soll,", "tokens": ["Die", "Leh\u00b7re", ",", "wel\u00b7che", "selbst", "die", "Ein\u00b7falt", "f\u00fch\u00b7ren", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Macht k\u00fchne Neugier nicht von schweren Fragen voll.", "tokens": ["Macht", "k\u00fch\u00b7ne", "Neu\u00b7gier", "nicht", "von", "schwe\u00b7ren", "Fra\u00b7gen", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKNEG", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch Seelen, welche nicht gleich Weisen denken k\u00f6nnen,", "tokens": ["Auch", "See\u00b7len", ",", "wel\u00b7che", "nicht", "gleich", "Wei\u00b7sen", "den\u00b7ken", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "PTKNEG", "ADV", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Will unumschr\u00e4nkte Huld ein ewig Gl\u00fccke g\u00f6nnen.", "tokens": ["Will", "un\u00b7um\u00b7schr\u00e4nk\u00b7te", "Huld", "ein", "e\u00b7wig", "Gl\u00fc\u00b7cke", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In einem Labyrinth, wo ihm der Faden fehlt,", "tokens": ["In", "ei\u00b7nem", "La\u00b7by\u00b7rinth", ",", "wo", "ihm", "der", "Fa\u00b7den", "fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Irrt der verwegne Sinn, der sich mit Forschen qu\u00e4lt.", "tokens": ["Irrt", "der", "ver\u00b7weg\u00b7ne", "Sinn", ",", "der", "sich", "mit", "For\u00b7schen", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Glaube, unbem\u00fcht, die Kenntni\u00df zu vergr\u00f6\u00dfern,", "tokens": ["Der", "Glau\u00b7be", ",", "un\u00b7be\u00b7m\u00fcht", ",", "die", "Kennt\u00b7ni\u00df", "zu", "ver\u0