{"textgrid.poem.23947": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das hat Gott Vater gut gemacht,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das hat Gott Vater gut gemacht,", "tokens": ["Das", "hat", "Gott", "Va\u00b7ter", "gut", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er zum Herbst den Wein gebracht,", "tokens": ["Da\u00df", "er", "zum", "Herbst", "den", "Wein", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den wei\u00dfen und den roten.", "tokens": ["Den", "wei\u00b7\u00dfen", "und", "den", "ro\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt wird alt, der Wein ist jung,", "tokens": ["Die", "Welt", "wird", "alt", ",", "der", "Wein", "ist", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herz bringt und Beine er in Schwung;", "tokens": ["Herz", "bringt", "und", "Bei\u00b7ne", "er", "in", "Schwung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir tanzen ohne Noten.", "tokens": ["Wir", "tan\u00b7zen", "oh\u00b7ne", "No\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir tanzen nach dem \u00e4ltsten Takt,", "tokens": ["Wir", "tan\u00b7zen", "nach", "dem", "\u00e4lts\u00b7ten", "Takt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach dem im Paradiese nackt", "tokens": ["Nach", "dem", "im", "Pa\u00b7ra\u00b7die\u00b7se", "nackt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Beiden schon sich drehten,", "tokens": ["Die", "Bei\u00b7den", "schon", "sich", "dreh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die unser Aller Eltern sind;", "tokens": ["Die", "un\u00b7ser", "Al\u00b7ler", "El\u00b7tern", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir tanzen zum Oktoberwind", "tokens": ["Wir", "tan\u00b7zen", "zum", "Ok\u00b7to\u00b7ber\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie trunkene Propheten.", "tokens": ["Wie", "trun\u00b7ke\u00b7ne", "Pro\u00b7phe\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da\u00df ihr mir nicht dem Herbste glaubt,", "tokens": ["Da\u00df", "ihr", "mir", "nicht", "dem", "Herbs\u00b7te", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sei nun alles abgelaubt,", "tokens": ["Es", "sei", "nun", "al\u00b7les", "ab\u00b7ge\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Keime schliefen!", "tokens": ["Und", "al\u00b7le", "Kei\u00b7me", "schlie\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seht unsern Kranz und unsern Tanz", "tokens": ["Seht", "un\u00b7sern", "Kranz", "und", "un\u00b7sern", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und unsrer Augen gl\u00fchen Glanz:", "tokens": ["Und", "uns\u00b7rer", "Au\u00b7gen", "gl\u00fc\u00b7hen", "Glanz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es wird was in den Tiefen!", "tokens": ["Es", "wird", "was", "in", "den", "Tie\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie dieser junge Wein im Fa\u00df", "tokens": ["Wie", "die\u00b7ser", "jun\u00b7ge", "Wein", "im", "Fa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich g\u00e4hrend regt ohn Unterla\u00df", "tokens": ["Sich", "g\u00e4h\u00b7rend", "regt", "ohn", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis zu der klaren St\u00e4rke,", "tokens": ["Bis", "zu", "der", "kla\u00b7ren", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So braut in uns gesunder Sinn", "tokens": ["So", "braut", "in", "uns", "ge\u00b7sun\u00b7der", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch Winternis und Starre hin", "tokens": ["Durch", "Win\u00b7ter\u00b7nis", "und", "Star\u00b7re", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu neuem Fr\u00fchlingswerke.", "tokens": ["Zu", "neu\u00b7em", "Fr\u00fch\u00b7lings\u00b7wer\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Gl\u00e4ser alle an den Mund!", "tokens": ["Die", "Gl\u00e4\u00b7ser", "al\u00b7le", "an", "den", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Glaubt nicht dem Herbst! Wir sind gesund", "tokens": ["Glaubt", "nicht", "dem", "Herbst", "!", "Wir", "sind", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$.", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wollens auch beweisen:", "tokens": ["Und", "wol\u00b7lens", "auch", "be\u00b7wei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Herrgott hoch! Hats gut gemacht,", "tokens": ["Der", "Herr\u00b7gott", "hoch", "!", "Hats", "gut", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$.", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df er zum Herbst den Wein gebracht,", "tokens": ["Da\u00df", "er", "zum", "Herbst", "den", "Wein", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den roten und den wei\u00dfen.", "tokens": ["Den", "ro\u00b7ten", "und", "den", "wei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}