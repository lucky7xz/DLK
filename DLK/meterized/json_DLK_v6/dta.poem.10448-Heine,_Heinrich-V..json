{"dta.poem.10448": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist feucht und st\u00fcrmisch,", "tokens": ["Die", "Nacht", "ist", "feucht", "und", "st\u00fcr\u00b7misch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Himmel sternenleer;", "tokens": ["Der", "Him\u00b7mel", "ster\u00b7nen\u00b7leer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Wald, unter rauschenden B\u00e4umen,", "tokens": ["Im", "Wald", ",", "un\u00b7ter", "rau\u00b7schen\u00b7den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wandle ich schweigend einher.", "tokens": ["Wand\u00b7le", "ich", "schwei\u00b7gend", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Es flimmert fern ein Lichtchen", "tokens": ["Es", "flim\u00b7mert", "fern", "ein", "Licht\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus dem einsamen J\u00e4gerhaus';", "tokens": ["Aus", "dem", "ein\u00b7sa\u00b7men", "J\u00e4\u00b7ger\u00b7haus'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es soll mich nicht hin verlocken,", "tokens": ["Es", "soll", "mich", "nicht", "hin", "ver\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dort sieht es verdrie\u00dflich aus.", "tokens": ["Dort", "sieht", "es", "ver\u00b7drie\u00df\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die blinde Gro\u00dfmutter sitzt ja", "tokens": ["Die", "blin\u00b7de", "Gro\u00df\u00b7mut\u00b7ter", "sitzt", "ja"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im ledernen Lehnstuhl dort,", "tokens": ["Im", "le\u00b7der\u00b7nen", "Lehn\u00b7stuhl", "dort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unheimlich und starr, wie ein Steinbild,", "tokens": ["Un\u00b7heim\u00b7lich", "und", "starr", ",", "wie", "ein", "Stein\u00b7bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und spricht kein einziges Wort.", "tokens": ["Und", "spricht", "kein", "ein\u00b7zi\u00b7ges", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Fluchend geht auf und nieder", "tokens": ["Flu\u00b7chend", "geht", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Des F\u00f6rsters rothk\u00f6pfiger Sohn,", "tokens": ["Des", "F\u00f6rs\u00b7ters", "roth\u00b7k\u00f6p\u00b7fi\u00b7ger", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und wirft an die Wand die B\u00fcchse,", "tokens": ["Und", "wirft", "an", "die", "Wand", "die", "B\u00fcch\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und lacht vor Wuth und Hohn.", "tokens": ["Und", "lacht", "vor", "Wuth", "und", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die sch\u00f6ne Spinnerin weinet,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Spin\u00b7ne\u00b7rin", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und feuchtet mit Thr\u00e4nen den Flachs;", "tokens": ["Und", "feuch\u00b7tet", "mit", "Thr\u00e4\u00b7nen", "den", "Flachs", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wimmernd zu ihren F\u00fc\u00dfen", "tokens": ["Wim\u00b7mernd", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Schmiegt sich des Vaters Dachs.", "tokens": ["Schmiegt", "sich", "des", "Va\u00b7ters", "Dachs", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}