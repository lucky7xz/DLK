{"textgrid.poem.40982": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An den Hund des Toten", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tod den edlen Herrn dir nahm,", "tokens": ["Der", "Tod", "den", "ed\u00b7len", "Herrn", "dir", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergebens suchst du seine Wege.", "tokens": ["Ver\u00b7ge\u00b7bens", "suchst", "du", "sei\u00b7ne", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du blickst mich an, ja, komm und lege", "tokens": ["Du", "blickst", "mich", "an", ",", "ja", ",", "komm", "und", "le\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PTKANT", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf meinen Scho\u00df dein Haupt voll Gram.", "tokens": ["Auf", "mei\u00b7nen", "Scho\u00df", "dein", "Haupt", "voll", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus deinen Augen, treues Tier!", "tokens": ["Aus", "dei\u00b7nen", "Au\u00b7gen", ",", "treu\u00b7es", "Tier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schaut eine stumme, tiefe Klage,", "tokens": ["Schaut", "ei\u00b7ne", "stum\u00b7me", ",", "tie\u00b7fe", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und geht an mich die ernste Frage:", "tokens": ["Und", "geht", "an", "mich", "die", "erns\u00b7te", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbwo find' ich ihn? Mensch! sag' es mir!\u00ab", "tokens": ["\u00bb", "wo", "find'", "ich", "ihn", "?", "Mensch", "!", "sag'", "es", "mir", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PPER", "$.", "NN", "$.", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wend' ab dein fragend Auge nur!", "tokens": ["Wend'", "ab", "dein", "fra\u00b7gend", "Au\u00b7ge", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJD", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was k\u00f6nnt' ein armer Mensch dir sagen?", "tokens": ["Was", "k\u00f6nnt'", "ein", "ar\u00b7mer", "Mensch", "dir", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Antwortet ja auf solche Fragen", "tokens": ["Ant\u00b7wor\u00b7tet", "ja", "auf", "sol\u00b7che", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Selbst ", "tokens": ["Selbst"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}}}}