{"textgrid.poem.36083": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XlIV. Schwermuth.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft liegt des Sommers Schw\u00fcle schwerdr\u00fcckend auf der Flur;", "tokens": ["Oft", "liegt", "des", "Som\u00b7mers", "Schw\u00fc\u00b7le", "schwerd\u00b7r\u00fc\u00b7ckend", "auf", "der", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In dunstge Schleier h\u00fcllt sich des Firmaments Azur.", "tokens": ["In", "dunst\u00b7ge", "Schlei\u00b7er", "h\u00fcllt", "sich", "des", "Fir\u00b7ma\u00b7ments", "A\u00b7zur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dr\u00e4uend niederh\u00e4ngend ziehn Wolken, schwarz und schwer,", "tokens": ["Und", "dr\u00e4u\u00b7end", "nie\u00b7der\u00b7h\u00e4n\u00b7gend", "ziehn", "Wol\u00b7ken", ",", "schwarz", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein banges Schweigen waltet, kein Leben regt sich mehr.", "tokens": ["Ein", "ban\u00b7ges", "Schwei\u00b7gen", "wal\u00b7tet", ",", "kein", "Le\u00b7ben", "regt", "sich", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PIAT", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "So liegt ein dr\u00fcckend Bangen auf Faustus Seele hart,", "tokens": ["So", "liegt", "ein", "dr\u00fc\u00b7ckend", "Ban\u00b7gen", "auf", "Faus\u00b7tus", "See\u00b7le", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "APPR", "NE", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vom Trauerflor umhangen ist seine Gegenwart.", "tokens": ["Vom", "Trau\u00b7er\u00b7flor", "um\u00b7han\u00b7gen", "ist", "sei\u00b7ne", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und Grausen macht ihn zittern, wenn er zur Zukunft blickt,", "tokens": ["Und", "Grau\u00b7sen", "macht", "ihn", "zit\u00b7tern", ",", "wenn", "er", "zur", "Zu\u00b7kunft", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Die finster, gleich Gewittern, schwer, furchtbar, n\u00e4her r\u00fcckt.", "tokens": ["Die", "fins\u00b7ter", ",", "gleich", "Ge\u00b7wit\u00b7tern", ",", "schwer", ",", "furcht\u00b7bar", ",", "n\u00e4\u00b7her", "r\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Es wandelt \u00fcbern Erdkreis ein stilles Weib voll Granen,", "tokens": ["Es", "wan\u00b7delt", "\u00fc\u00b7bern", "Erd\u00b7kreis", "ein", "stil\u00b7les", "Weib", "voll", "Gra\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gespensterbleich und schweigsam; weh denen, die sie schauen.", "tokens": ["Ge\u00b7spens\u00b7ter\u00b7bleich", "und", "schweig\u00b7sam", ";", "weh", "de\u00b7nen", ",", "die", "sie", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$.", "ADV", "PDS", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Um ihren Frieden ist es auf immer dann gethan;", "tokens": ["Um", "ih\u00b7ren", "Frie\u00b7den", "ist", "es", "auf", "im\u00b7mer", "dann", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VAFIN", "PPER", "APPR", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Sie stiert mit Wahnsinnblicken die Menschenkinder an.", "tokens": ["Sie", "stiert", "mit", "Wahn\u00b7sinn\u00b7bli\u00b7cken", "die", "Men\u00b7schen\u00b7kin\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie tr\u00e4gt nicht Wehr, nicht Waffen, sie wandelt ganz allein;", "tokens": ["Sie", "tr\u00e4gt", "nicht", "Wehr", ",", "nicht", "Waf\u00b7fen", ",", "sie", "wan\u00b7delt", "ganz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch schon ihr Anblick wandelt das Leben fast in Stein.", "tokens": ["Doch", "schon", "ihr", "An\u00b7blick", "wan\u00b7delt", "das", "Le\u00b7ben", "fast", "in", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie macht die St\u00e4rke zittern, macht schwer Muth, Kraft und Geist,", "tokens": ["Sie", "macht", "die", "St\u00e4r\u00b7ke", "zit\u00b7tern", ",", "macht", "schwer", "Muth", ",", "Kraft", "und", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Daher nicht unbedeutsam ", "tokens": ["Da\u00b7her", "nicht", "un\u00b7be\u00b7deut\u00b7sam"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PTKNEG", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und Schwermuth tritt zu Faustus mit dem Eumenidenblick,", "tokens": ["Und", "Schwer\u00b7muth", "tritt", "zu", "Faus\u00b7tus", "mit", "dem", "Eu\u00b7me\u00b7ni\u00b7den\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Mit dem Medusenantlitz, furchtbar, wie das Geschick.", "tokens": ["Mit", "dem", "Me\u00b7du\u00b7sen\u00b7ant\u00b7litz", ",", "furcht\u00b7bar", ",", "wie", "das", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Er sitzt in d\u00fcstrer Kammer, dumpfbr\u00fctend, tr\u00fcb und tr\u00fcber;", "tokens": ["Er", "sitzt", "in", "d\u00fcst\u00b7rer", "Kam\u00b7mer", ",", "dumpf\u00b7br\u00fc\u00b7tend", ",", "tr\u00fcb", "und", "tr\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und langsam setzt sich Schwermuth dem Stillen gegen\u00fcber.", "tokens": ["Und", "lang\u00b7sam", "setzt", "sich", "Schwer\u00b7muth", "dem", "Stil\u00b7len", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Und hebt die welke Hand auf aus Schleiern silbergrau;", "tokens": ["Und", "hebt", "die", "wel\u00b7ke", "Hand", "auf", "aus", "Schlei\u00b7ern", "sil\u00b7ber\u00b7grau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die blassen Lippen zittern der geisterbleichen Frau.", "tokens": ["Die", "blas\u00b7sen", "Lip\u00b7pen", "zit\u00b7tern", "der", "geis\u00b7ter\u00b7blei\u00b7chen", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie fl\u00fcstert leise, leise, doch was sie fl\u00fcsternd sprach,", "tokens": ["Sie", "fl\u00fcs\u00b7tert", "lei\u00b7se", ",", "lei\u00b7se", ",", "doch", "was", "sie", "fl\u00fcs\u00b7ternd", "sprach", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "$,", "KON", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Das braust wie Windsbrautheulen kalt, schneidend durchs Gemach.", "tokens": ["Das", "braust", "wie", "Winds\u00b7brau\u00b7theu\u00b7len", "kalt", ",", "schnei\u00b7dend", "durchs", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "NN", "ADJD", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}}, "stanza.7": {"line.1": {"text": "\u00bbdu bist verworfen, Faustus! Hast Aussicht nur auf Qualen;", "tokens": ["\u00bb", "du", "bist", "ver\u00b7wor\u00b7fen", ",", "Faus\u00b7tus", "!", "Hast", "Aus\u00b7sicht", "nur", "auf", "Qua\u00b7len", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$,", "NE", "$.", "VAFIN", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Richter im Gerichte reicht Dir des Zornes Schalen.", "tokens": ["Der", "Rich\u00b7ter", "im", "Ge\u00b7rich\u00b7te", "reicht", "Dir", "des", "Zor\u00b7nes", "Scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Staubgebornen Keiner hat sich gleich Dir, vermessen,", "tokens": ["Der", "Staub\u00b7ge\u00b7bor\u00b7nen", "Kei\u00b7ner", "hat", "sich", "gleich", "Dir", ",", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PIS", "VAFIN", "PRF", "ADV", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und wie Du Deinen Sch\u00f6pfer, so wird er Dein vergessen!\u00ab", "tokens": ["Und", "wie", "Du", "Dei\u00b7nen", "Sch\u00f6p\u00b7fer", ",", "so", "wird", "er", "Dein", "ver\u00b7ges\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbdein ganzes Sein war L\u00e4strung, war Frevel, nie zu s\u00fchnen!", "tokens": ["\u00bb", "dein", "gan\u00b7zes", "Sein", "war", "L\u00e4st\u00b7rung", ",", "war", "Fre\u00b7vel", ",", "nie", "zu", "s\u00fch\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$,", "VAFIN", "NN", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was Du gethan, dess durften sich Teufel kaum erk\u00fchnen!", "tokens": ["Was", "Du", "ge\u00b7than", ",", "dess", "durf\u00b7ten", "sich", "Teu\u00b7fel", "kaum", "er\u00b7k\u00fch\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PDS", "VMFIN", "PRF", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Den Teufeln bleibt noch Hoffnung dereinst erl\u00f6st zu werden;", "tokens": ["Den", "Teu\u00b7feln", "bleibt", "noch", "Hoff\u00b7nung", "de\u00b7reinst", "er\u00b7l\u00f6st", "zu", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "F\u00fcr Dich giebt's keine Hoffnung, nicht droben, nicht auf Erden!\u00ab", "tokens": ["F\u00fcr", "Dich", "giebt's", "kei\u00b7ne", "Hoff\u00b7nung", ",", "nicht", "dro\u00b7ben", ",", "nicht", "auf", "Er\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "$,", "PTKNEG", "ADV", "$,", "PTKNEG", "APPR", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbdu bist gerichtet, Faustus, darfst nie zu hoffen wagen!", "tokens": ["\u00bb", "du", "bist", "ge\u00b7rich\u00b7tet", ",", "Faus\u00b7tus", ",", "darfst", "nie", "zu", "hof\u00b7fen", "wa\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$,", "NE", "$,", "PAV", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du l\u00e4stertest den Vater, Du hast den Sohn geschlagen!", "tokens": ["Du", "l\u00e4s\u00b7ter\u00b7test", "den", "Va\u00b7ter", ",", "Du", "hast", "den", "Sohn", "ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Den heilgen Geist der Liebe, den Gott der Welt verliehen,", "tokens": ["Den", "heil\u00b7gen", "Geist", "der", "Lie\u00b7be", ",", "den", "Gott", "der", "Welt", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hast teuflisch Du verh\u00f6hnet, Du hast ihn angespieen!\u00ab", "tokens": ["Hast", "teuf\u00b7lisch", "Du", "ver\u00b7h\u00f6h\u00b7net", ",", "Du", "hast", "ihn", "an\u00b7ge\u00b7spi\u00b7een", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbdu kannst nicht beten, Faustus, nicht glauben mehr, nicht hoffen!", "tokens": ["\u00bb", "du", "kannst", "nicht", "be\u00b7ten", ",", "Faus\u00b7tus", ",", "nicht", "glau\u00b7ben", "mehr", ",", "nicht", "hof\u00b7fen", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "NE", "$,", "PTKNEG", "VVFIN", "ADV", "$,", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "Verschlossen ist der Himmel, die H\u00f6lle steht Dir offen.", "tokens": ["Ver\u00b7schlos\u00b7sen", "ist", "der", "Him\u00b7mel", ",", "die", "H\u00f6l\u00b7le", "steht", "Dir", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Erl\u00f6st war Deine Seele \u2013 Du hast sie losgerissen", "tokens": ["Er\u00b7l\u00f6st", "war", "Dei\u00b7ne", "See\u00b7le", "\u2013", "Du", "hast", "sie", "los\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vom Lichtschooss der Erl\u00f6sung zu Grabesfinsternissen.\u00ab", "tokens": ["Vom", "Licht\u00b7scho\u00b7oss", "der", "Er\u00b7l\u00f6\u00b7sung", "zu", "Gra\u00b7bes\u00b7fins\u00b7ter\u00b7nis\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbdu bist ein Bild, das blutig Verzweiflung \u00fcbermalet;", "tokens": ["\u00bb", "du", "bist", "ein", "Bild", ",", "das", "blu\u00b7tig", "Ver\u00b7zwei\u00b7flung", "\u00fc\u00b7berm\u00b7a\u00b7let", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du bist ein Stern der ausl\u00f6scht, und nimmer wieder strahlet!", "tokens": ["Du", "bist", "ein", "Stern", "der", "aus\u00b7l\u00f6scht", ",", "und", "nim\u00b7mer", "wie\u00b7der", "strah\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "VVPP", "$,", "KON", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der tief zum Abgrund sinket, wo Tagglanz muss erblinden,", "tokens": ["Der", "tief", "zum", "Ab\u00b7grund", "sin\u00b7ket", ",", "wo", "Tag\u00b7glanz", "muss", "er\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$,", "PWAV", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und ob ihn Engel suchten, sie k\u00f6nnen ihn nicht finden.\u00ab", "tokens": ["Und", "ob", "ihn", "En\u00b7gel", "such\u00b7ten", ",", "sie", "k\u00f6n\u00b7nen", "ihn", "nicht", "fin\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "\u00bbund ob Dich Tr\u00f6stung suchte mit ihrer sanften Leuchte,", "tokens": ["\u00bb", "und", "ob", "Dich", "Tr\u00f6s\u00b7tung", "such\u00b7te", "mit", "ih\u00b7rer", "sanf\u00b7ten", "Leuch\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dein Anblick, der verfluchte, selbst Tr\u00f6stung von Dir scheuchte.", "tokens": ["Dein", "An\u00b7blick", ",", "der", "ver\u00b7fluch\u00b7te", ",", "selbst", "Tr\u00f6s\u00b7tung", "von", "Dir", "scheuch\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "ADV", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Fahr hin, fahr hin in Schrecken, Gott will kein Theil an Dir haben!", "tokens": ["Fahr", "hin", ",", "fahr", "hin", "in", "Schre\u00b7cken", ",", "Gott", "will", "kein", "Theil", "an", "Dir", "ha\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "ADV", "APPR", "NN", "$,", "NN", "VMFIN", "PIAT", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "+-+--+-+-+++-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "O m\u00f6chten Dich Berge decken, und Welten Dich begraben!\u00ab \u2013", "tokens": ["O", "m\u00f6ch\u00b7ten", "Dich", "Ber\u00b7ge", "de\u00b7cken", ",", "und", "Wel\u00b7ten", "Dich", "be\u00b7gra\u00b7ben", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VMFIN", "PPER", "NN", "VVINF", "$,", "KON", "NN", "PPER", "VVPP", "$.", "$(", "$("], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "So spricht die d\u00fcstre Schwermuth, und was die Schwermuth sprach,", "tokens": ["So", "spricht", "die", "d\u00fcst\u00b7re", "Schwer\u00b7muth", ",", "und", "was", "die", "Schwer\u00b7muth", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Das seufzt in Faustus Herzen ein wimmernd Echo nach.", "tokens": ["Das", "seufzt", "in", "Faus\u00b7tus", "Her\u00b7zen", "ein", "wim\u00b7mernd", "E\u00b7cho", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NE", "NN", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er weiss nicht, wo die Stimmen? Ob in, ob ausser ihm?", "tokens": ["Er", "weiss", "nicht", ",", "wo", "die", "Stim\u00b7men", "?", "Ob", "in", ",", "ob", "aus\u00b7ser", "ihm", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "NN", "$.", "KOUS", "APPR", "$,", "KOUS", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sein Haupt scheint zu umrauschen zornvoll ein Cherubim.", "tokens": ["Sein", "Haupt", "scheint", "zu", "um\u00b7rau\u00b7schen", "zorn\u00b7voll", "ein", "Che\u00b7ru\u00b7bim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "VVFIN", "ART", "NE", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Stumm sitzt er, wie ein Steinbild auf einem Sarkophag;", "tokens": ["Stumm", "sitzt", "er", ",", "wie", "ein", "Stein\u00b7bild", "auf", "ei\u00b7nem", "Sar\u00b7ko\u00b7phag", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und mehr als Marmor dr\u00fcckt ihn was lastend auf ihm lag.", "tokens": ["Und", "mehr", "als", "Mar\u00b7mor", "dr\u00fcckt", "ihn", "was", "las\u00b7tend", "auf", "ihm", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "NN", "VVFIN", "PPER", "PIS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mehr als Gedanken fassen ist seine Brust voll Qual;", "tokens": ["Mehr", "als", "Ge\u00b7dan\u00b7ken", "fas\u00b7sen", "ist", "sei\u00b7ne", "Brust", "voll", "Qual", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "VVINF", "VAFIN", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ver\u00f6det, ganz verlassen, zerrissen tausendmal.", "tokens": ["Ver\u00b7\u00f6\u00b7det", ",", "ganz", "ver\u00b7las\u00b7sen", ",", "zer\u00b7ris\u00b7sen", "tau\u00b7send\u00b7mal", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVINF", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Sein Auge hat nicht Thr\u00e4nen, nicht Seufzer mehr sein Mund;", "tokens": ["Sein", "Au\u00b7ge", "hat", "nicht", "Thr\u00e4\u00b7nen", ",", "nicht", "Seuf\u00b7zer", "mehr", "sein", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sein Herz hat keinen Balsam, und ist so todeswund,", "tokens": ["Sein", "Herz", "hat", "kei\u00b7nen", "Bal\u00b7sam", ",", "und", "ist", "so", "to\u00b7des\u00b7wund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,", "KON", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und klopft so laut und heftig, und ist doch todesmatt;", "tokens": ["Und", "klopft", "so", "laut", "und", "hef\u00b7tig", ",", "und", "ist", "doch", "to\u00b7des\u00b7matt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er zittert vor dem Sterben, und ist doch lebenssatt.", "tokens": ["Er", "zit\u00b7tert", "vor", "dem", "Ster\u00b7ben", ",", "und", "ist", "doch", "le\u00b7bens\u00b7satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Es klopfet dumpf die Schwermuth an einen alten Sarg,", "tokens": ["Es", "klop\u00b7fet", "dumpf", "die", "Schwer\u00b7muth", "an", "ei\u00b7nen", "al\u00b7ten", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Darin der S\u00fcnder lange still sein Gewissen barg.", "tokens": ["Da\u00b7rin", "der", "S\u00fcn\u00b7der", "lan\u00b7ge", "still", "sein", "Ge\u00b7wis\u00b7sen", "barg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jetzt birst er, und die Leiche steigt lebend aus dem Schrein,", "tokens": ["Jetzt", "birst", "er", ",", "und", "die", "Lei\u00b7che", "steigt", "le\u00b7bend", "aus", "dem", "Schrein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Da meint vor Angst der S\u00fcnder, er muss des Todes sein!", "tokens": ["Da", "meint", "vor", "Angst", "der", "S\u00fcn\u00b7der", ",", "er", "muss", "des", "To\u00b7des", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "Und schreit laut auf beim Anblick der ernsten Richterin,", "tokens": ["Und", "schreit", "laut", "auf", "beim", "An\u00b7blick", "der", "erns\u00b7ten", "Rich\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sinkt vor seinem Lager starr in Bet\u00e4ubung hin.", "tokens": ["Und", "sinkt", "vor", "sei\u00b7nem", "La\u00b7ger", "starr", "in", "Be\u00b7t\u00e4u\u00b7bung", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihm ist als flirren Fl\u00fcgel von Larven um ihn her,", "tokens": ["Ihm", "ist", "als", "flir\u00b7ren", "Fl\u00fc\u00b7gel", "von", "Lar\u00b7ven", "um", "ihn", "her", ","], "token