{"dta.poem.14446": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Geheimni\u00dfvoll geh'n oben gold'ne Sterne,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7voll", "geh'n", "o\u00b7ben", "gold'\u00b7ne", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unten erbraust viel Land in dunk'len Wogen \u2014", "tokens": ["Un\u00b7ten", "er\u00b7braust", "viel", "Land", "in", "dunk'\u00b7len", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Was z\u00f6gerst Du am unbekannten Rande?", "tokens": ["Was", "z\u00f6\u00b7gerst", "Du", "am", "un\u00b7be\u00b7kann\u00b7ten", "Ran\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}