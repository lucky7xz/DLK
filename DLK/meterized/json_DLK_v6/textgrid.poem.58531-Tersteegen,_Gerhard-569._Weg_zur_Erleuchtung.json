{"textgrid.poem.58531": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "569. Weg zur Erleuchtung", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du einig lautres Licht, der Seelenblindheit Kur,", "tokens": ["Du", "ei\u00b7nig", "laut\u00b7res", "Licht", ",", "der", "See\u00b7len\u00b7blind\u00b7heit", "Kur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das alle Wahrheit lehrt, wenn ich mich b\u00fccke nur", "tokens": ["Das", "al\u00b7le", "Wahr\u00b7heit", "lehrt", ",", "wenn", "ich", "mich", "b\u00fc\u00b7cke", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ganz nackt und blind", "tokens": ["Ganz", "nackt", "und", "blind"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Gleichwie ein Kind,", "tokens": ["Gleich\u00b7wie", "ein", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das gar nichts wei\u00df und auch nichts wissen will,", "tokens": ["Das", "gar", "nichts", "wei\u00df", "und", "auch", "nichts", "wis\u00b7sen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIS", "VVFIN", "KON", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und halt' dir so in meinem Grunde still!", "tokens": ["Und", "halt'", "dir", "so", "in", "mei\u00b7nem", "Grun\u00b7de", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann giebet mir dein Angesicht", "tokens": ["Dann", "gie\u00b7bet", "mir", "dein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Genugsam Licht,", "tokens": ["Ge\u00b7nug\u00b7sam", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Genugsam Licht, wiewohl ich Eins nur seh,", "tokens": ["Ge\u00b7nug\u00b7sam", "Licht", ",", "wie\u00b7wohl", "ich", "Eins", "nur", "seh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Doch in dem Eins ich alles leicht versteh.", "tokens": ["Doch", "in", "dem", "Eins", "ich", "al\u00b7les", "leicht", "ver\u00b7steh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Drum ist's nicht mancherlei, nicht Bilder, sondern Wahrheit,", "tokens": ["Drum", "ist's", "nicht", "man\u00b7cher\u00b7lei", ",", "nicht", "Bil\u00b7der", ",", "son\u00b7dern", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PTKNEG", "PIS", "$,", "PTKNEG", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auch f\u00fchrt mich all's gerad hinein zur Einen Klarheit.", "tokens": ["Auch", "f\u00fchrt", "mich", "all's", "ge\u00b7rad", "hin\u00b7ein", "zur", "Ei\u00b7nen", "Klar\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "ADV", "APPRART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}