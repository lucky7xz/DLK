{"dta.poem.6068": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat die Erde durch den Schatten", "tokens": ["Es", "hat", "die", "Er\u00b7de", "durch", "den", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch auf den Mond dieselbe Macht.", "tokens": ["Auch", "auf", "den", "Mond", "die\u00b7sel\u00b7be", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie sich zwischen ihm und unsrer Sonnen lencket;", "tokens": ["Wenn", "sie", "sich", "zwi\u00b7schen", "ihm", "und", "uns\u00b7rer", "Son\u00b7nen", "len\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wirfft der Erden Rund auch eine Schatten-Nacht.", "tokens": ["So", "wirfft", "der", "Er\u00b7den", "Rund", "auch", "ei\u00b7ne", "Schat\u00b7ten\u00b7Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wodurch der Mond so gleich bedecket,", "tokens": ["Wo\u00b7durch", "der", "Mond", "so", "gleich", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sich f\u00fcr uns verstecket.", "tokens": ["Und", "sich", "f\u00fcr", "uns", "ver\u00b7ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So bald die Sonn\u2019 ihm keinen Schein mehr schenckt,", "tokens": ["So", "bald", "die", "Sonn'", "ihm", "kei\u00b7nen", "Schein", "mehr", "schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ist er in Finsterni\u00df und Dunckelheit versenckt.", "tokens": ["Ist", "er", "in", "Fins\u00b7ter\u00b7ni\u00df", "und", "Dun\u00b7ckel\u00b7heit", "ver\u00b7senckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}