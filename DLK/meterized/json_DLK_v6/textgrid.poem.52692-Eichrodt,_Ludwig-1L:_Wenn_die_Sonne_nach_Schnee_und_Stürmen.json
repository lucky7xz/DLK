{"textgrid.poem.52692": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn die Sonne nach Schnee und St\u00fcrmen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Sonne nach Schnee und St\u00fcrmen", "tokens": ["Wenn", "die", "Son\u00b7ne", "nach", "Schnee", "und", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Pl\u00f6tzlich im blauen Himmel steht,", "tokens": ["Pl\u00f6tz\u00b7lich", "im", "blau\u00b7en", "Him\u00b7mel", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wandelt dich an ein Gef\u00fchl der R\u00fchrung,", "tokens": ["Wan\u00b7delt", "dich", "an", "ein", "Ge\u00b7f\u00fchl", "der", "R\u00fch\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie wenn ein Gru\u00df von geliebten Menschen", "tokens": ["Wie", "wenn", "ein", "Gru\u00df", "von", "ge\u00b7lieb\u00b7ten", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aus der Ferne her\u00fcberweht.", "tokens": ["Aus", "der", "Fer\u00b7ne", "her\u00b7\u00fc\u00b7ber\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Wenn nach schw\u00fclen, t\u00f6dtenden Tagen", "tokens": ["Wenn", "nach", "schw\u00fc\u00b7len", ",", "t\u00f6d\u00b7ten\u00b7den", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "VVINF", "$,", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Niederschauert ein Regen mild,", "tokens": ["Nie\u00b7der\u00b7schau\u00b7ert", "ein", "Re\u00b7gen", "mild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wandelt dich an ein Gef\u00fchl der St\u00e4rkung,", "tokens": ["Wan\u00b7delt", "dich", "an", "ein", "Ge\u00b7f\u00fchl", "der", "St\u00e4r\u00b7kung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie wenn aus dumpfen Zweifeln gerissen", "tokens": ["Wie", "wenn", "aus", "dum\u00b7pfen", "Zwei\u00b7feln", "ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dich Gewi\u00dfheit erquickt, erf\u00fcllt.", "tokens": ["Dich", "Ge\u00b7wi\u00df\u00b7heit", "er\u00b7quickt", ",", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Also mit unz\u00e4hligen Griffen", "tokens": ["Al\u00b7so", "mit", "un\u00b7z\u00e4h\u00b7li\u00b7gen", "Grif\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Fa\u00dft Natur ins Menschengem\u00fcth,", "tokens": ["Fa\u00dft", "Na\u00b7tur", "ins", "Men\u00b7schen\u00b7ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Also raget die dichtende Seele", "tokens": ["Al\u00b7so", "ra\u00b7get", "die", "dich\u00b7ten\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "M\u00e4chtig in die unendliche Sch\u00f6pfung", "tokens": ["M\u00e4ch\u00b7tig", "in", "die", "un\u00b7end\u00b7li\u00b7che", "Sch\u00f6p\u00b7fung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "\u2013 Und geboren wird das Lied.", "tokens": ["\u2013", "Und", "ge\u00b7bo\u00b7ren", "wird", "das", "Lied", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}