{"textgrid.poem.37866": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Cedron's Klage", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da nun Abends in dem Garten,", "tokens": ["Da", "nun", "A\u00b7bends", "in", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Daphnis \u00fcberfallen war,", "tokens": ["Daph\u00b7nis", "\u00fc\u00b7berf\u00b7al\u00b7len", "war", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun keinen Grimm ersparte,", "tokens": ["Und", "nun", "kei\u00b7nen", "Grimm", "er\u00b7spar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stark bewehrte M\u00f6rderschaar,", "tokens": ["Stark", "be\u00b7wehr\u00b7te", "M\u00f6r\u00b7der\u00b7schaar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hube s\u00fc\u00dflich an zu weinen,", "tokens": ["Hu\u00b7be", "s\u00fc\u00df\u00b7lich", "an", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein so gar ber\u00fchmter Bach,", "tokens": ["Ein", "so", "gar", "be\u00b7r\u00fchm\u00b7ter", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lie\u00df die liebe Sternen scheinen,", "tokens": ["Lie\u00df", "die", "lie\u00b7be", "Ster\u00b7nen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Er dem Daphnis trauret nach.", "tokens": ["Er", "dem", "Daph\u00b7nis", "trau\u00b7ret", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Cedron hie\u00df der Bach mit Namen,", "tokens": ["Ce\u00b7dron", "hie\u00df", "der", "Bach", "mit", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohnt an einem hohen Stein:", "tokens": ["Wohnt", "an", "ei\u00b7nem", "ho\u00b7hen", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft zu ihm Gesellen kamen,", "tokens": ["Oft", "zu", "ihm", "Ge\u00b7sel\u00b7len", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Damals war er doch allein,", "tokens": ["Da\u00b7mals", "war", "er", "doch", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sa\u00df in seinen gr\u00fcnen Gr\u00fcften,", "tokens": ["Sa\u00df", "in", "sei\u00b7nen", "gr\u00fc\u00b7nen", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Str\u00e4hlet seine Binsenhaar,", "tokens": ["Str\u00e4h\u00b7let", "sei\u00b7ne", "Bin\u00b7sen\u00b7haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Spielet gar mit sanften L\u00fcften,", "tokens": ["Spie\u00b7let", "gar", "mit", "sanf\u00b7ten", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dacht an keine Kriegsgefahr.", "tokens": ["Dacht", "an", "kei\u00b7ne", "Kriegs\u00b7ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Rohr, und Gras, und Wasserbl\u00e4tter,", "tokens": ["Rohr", ",", "und", "Gras", ",", "und", "Was\u00b7ser\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deckten seine Schulter blo\u00df,", "tokens": ["Deck\u00b7ten", "sei\u00b7ne", "Schul\u00b7ter", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stark er sich bey feuchtem Wetter,", "tokens": ["Stark", "er", "sich", "bey", "feuch\u00b7tem", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lehnt auf seinen Eimer gro\u00df,", "tokens": ["Lehnt", "auf", "sei\u00b7nen", "Ei\u00b7mer", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch weil er fast m\u00fcd gelaufen,", "tokens": ["Doch", "weil", "er", "fast", "m\u00fcd", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dazumal in starkem Trab,", "tokens": ["Da\u00b7zu\u00b7mal", "in", "star\u00b7kem", "Trab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Er ein wenig wollt verschnaufen,", "tokens": ["Er", "ein", "we\u00b7nig", "wollt", "ver\u00b7schnau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Go\u00df den Eimer langsam ab.", "tokens": ["Go\u00df", "den", "Ei\u00b7mer", "lang\u00b7sam", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nahm ein R\u00f6hrlein wohl geschnitten,", "tokens": ["Nahm", "ein", "R\u00f6hr\u00b7lein", "wohl", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielet seinen W\u00e4sserlein,", "tokens": ["Spie\u00b7let", "sei\u00b7nen", "W\u00e4s\u00b7ser\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie zum Schlafen th\u00e4t er bitten,", "tokens": ["Sie", "zum", "Schla\u00b7fen", "th\u00e4t", "er", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt sie s\u00fc\u00dflich sausen ein:", "tokens": ["Wollt", "sie", "s\u00fc\u00df\u00b7lich", "sau\u00b7sen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eya, meine W\u00e4sser schlafet,", "tokens": ["Ey\u00b7a", ",", "mei\u00b7ne", "W\u00e4s\u00b7ser", "schla\u00b7fet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlafet meine W\u00e4sserlein,", "tokens": ["Schla\u00b7fet", "mei\u00b7ne", "W\u00e4s\u00b7ser\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht mit Augen immer gaffet,", "tokens": ["Nicht", "mit", "Au\u00b7gen", "im\u00b7mer", "gaf\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eya, schlafet, schlafet ein.", "tokens": ["Ey\u00b7a", ",", "schla\u00b7fet", ",", "schla\u00b7fet", "ein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kaum nun waren eingeschlafen", "tokens": ["Kaum", "nun", "wa\u00b7ren", "ein\u00b7ge\u00b7schla\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine matten W\u00e4sserlein,", "tokens": ["Sei\u00b7ne", "mat\u00b7ten", "W\u00e4s\u00b7ser\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald erklungen Wehr und Waffen,", "tokens": ["Bald", "er\u00b7klun\u00b7gen", "Wehr", "und", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flamm und Fackel gaben Schein,", "tokens": ["Flamm", "und", "Fa\u00b7ckel", "ga\u00b7ben", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur von tollen vollen Knechten,", "tokens": ["Nur", "von", "tol\u00b7len", "vol\u00b7len", "Knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Voll war alles \u00fcberall,", "tokens": ["Voll", "war", "al\u00b7les", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur von Jauchzen, Springen, Fechten,", "tokens": ["Nur", "von", "Jauch\u00b7zen", ",", "Sprin\u00b7gen", ",", "Fech\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Thal und Ufer gaben Schall.", "tokens": ["Thal", "und", "U\u00b7fer", "ga\u00b7ben", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Cedron erstens gar erschrecket,", "tokens": ["Ce\u00b7dron", "ers\u00b7tens", "gar", "er\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War der Waffen ungewohnt,", "tokens": ["War", "der", "Waf\u00b7fen", "un\u00b7ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald er seine Wasser wecket,", "tokens": ["Bald", "er", "sei\u00b7ne", "Was\u00b7ser", "we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollte der Gefahr entgehn,", "tokens": ["Woll\u00b7te", "der", "Ge\u00b7fahr", "ent\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Pfeil vom Bogen zielen,", "tokens": ["Wie", "die", "Pfeil", "vom", "Bo\u00b7gen", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lief er ab, auf nasser Meil,", "tokens": ["Lief", "er", "ab", ",", "auf", "nas\u00b7ser", "Meil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rohr und Eimer ihm entfielen,", "tokens": ["Rohr", "und", "Ei\u00b7mer", "ihm", "ent\u00b7fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fiel auch selbst in blinder Eil.", "tokens": ["Fiel", "auch", "selbst", "in", "blin\u00b7der", "Eil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch weil nachmals er versp\u00fcret,", "tokens": ["Doch", "weil", "nach\u00b7mals", "er", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es nicht wider ihn gemeint,", "tokens": ["Es", "nicht", "wi\u00b7der", "ihn", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nur Daphnis werd gef\u00fchret,", "tokens": ["Und", "nur", "Daph\u00b7nis", "werd", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Daphnis vom bekannten Feind;", "tokens": ["Daph\u00b7nis", "vom", "be\u00b7kann\u00b7ten", "Feind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lie\u00df er ab von strengem Laufen,", "tokens": ["Lie\u00df", "er", "ab", "von", "stren\u00b7gem", "Lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fasset eine Weidenruth,", "tokens": ["Fas\u00b7set", "ei\u00b7ne", "Wei\u00b7den\u00b7ruth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine Wasser trieb zu Haufen", "tokens": ["Sei\u00b7ne", "Was\u00b7ser", "trieb", "zu", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und beklagt das junge Blut.", "tokens": ["Und", "be\u00b7klagt", "das", "jun\u00b7ge", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Traurig hub er an zu klagen,", "tokens": ["Trau\u00b7rig", "hub", "er", "an", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blie\u00df auf einem holen Ried,", "tokens": ["Blie\u00df", "auf", "ei\u00b7nem", "ho\u00b7len", "Ried", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herz und Muth ihm war zerschlagen,", "tokens": ["Herz", "und", "Muth", "ihm", "war", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN",