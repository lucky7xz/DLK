{"textgrid.poem.60014": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr T\u00e4ubchen, welche beyde", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr T\u00e4ubchen, welche beyde", "tokens": ["Ihr", "T\u00e4ub\u00b7chen", ",", "wel\u00b7che", "bey\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Amor einst gepaart,", "tokens": ["Mein", "A\u00b7mor", "einst", "ge\u00b7paart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ihr, auf jener Weide,", "tokens": ["Als", "ihr", ",", "auf", "je\u00b7ner", "Wei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Knaben beste Freude,", "tokens": ["Des", "Kna\u00b7ben", "bes\u00b7te", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Spiel der Nymphen war't!", "tokens": ["Das", "Spiel", "der", "Nym\u00b7phen", "wa\u00b7r't", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "O gr\u00fc\u00dft, mit jedem Morgen,", "tokens": ["O", "gr\u00fc\u00dft", ",", "mit", "je\u00b7dem", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den frommen Dichter hier:", "tokens": ["Den", "from\u00b7men", "Dich\u00b7ter", "hier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Euch z\u00e4rtlich zu versorgen", "tokens": ["Euch", "z\u00e4rt\u00b7lich", "zu", "ver\u00b7sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Befahl Cythere mir.", "tokens": ["Be\u00b7fahl", "Cy\u00b7the\u00b7re", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Seh' ich, zu meinen F\u00fc\u00dfen,", "tokens": ["Seh'", "ich", ",", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch froh und sicher k\u00fcssen,", "tokens": ["Euch", "froh", "und", "si\u00b7cher", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Unschuldvollen ihr!", "tokens": ["Ihr", "Un\u00b7schuld\u00b7vol\u00b7len", "ihr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dann denk' ich an Belinden;", "tokens": ["Dann", "denk'", "ich", "an", "Be\u00b7lin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie lie\u00df in diesen Gr\u00fcnden", "tokens": ["Sie", "lie\u00df", "in", "die\u00b7sen", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich auch die Liebe finden.", "tokens": ["Mich", "auch", "die", "Lie\u00b7be", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Allein, bedauert mich;", "tokens": ["Al\u00b7lein", ",", "be\u00b7dau\u00b7ert", "mich", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Weit gl\u00fccklicher, als ich,", "tokens": ["Weit", "gl\u00fcck\u00b7li\u00b7cher", ",", "als", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Seyd ihr, geliebte T\u00e4ubchen,", "tokens": ["Seyd", "ihr", ",", "ge\u00b7lieb\u00b7te", "T\u00e4ub\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wenn ihr im Haine girrt,", "tokens": ["Wenn", "ihr", "im", "Hai\u00b7ne", "girrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und das getreue Weibchen", "tokens": ["Und", "das", "ge\u00b7treu\u00b7e", "Weib\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Um seinen Gatten irrt.", "tokens": ["Um", "sei\u00b7nen", "Gat\u00b7ten", "irrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wie ruhig k\u00f6nnt ihr spielen,", "tokens": ["Wie", "ru\u00b7hig", "k\u00f6nnt", "ihr", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Wie ruhig, dort im K\u00fchlen,", "tokens": ["Wie", "ru\u00b7hig", ",", "dort", "im", "K\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "In wollustreicher Nacht,", "tokens": ["In", "wol\u00b7lust\u00b7rei\u00b7cher", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Wo keine Mutter wacht,", "tokens": ["Wo", "kei\u00b7ne", "Mut\u00b7ter", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Wo neben eurem Bettchen", "tokens": ["Wo", "ne\u00b7ben", "eu\u00b7rem", "Bett\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Bekr\u00e4nzte Freyheit lacht,", "tokens": ["Be\u00b7kr\u00e4nz\u00b7te", "Frey\u00b7heit", "lacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Und kein bewegtes Bl\u00e4ttchen", "tokens": ["Und", "kein", "be\u00b7weg\u00b7tes", "Bl\u00e4tt\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Die Liebe sch\u00fcchtern macht!", "tokens": ["Die", "Lie\u00b7be", "sch\u00fcch\u00b7tern", "macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Hier, unter \u00f6den B\u00e4umen,", "tokens": ["Hier", ",", "un\u00b7ter", "\u00f6\u00b7den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Hier, auf verla\u00dfner Flur,", "tokens": ["Hier", ",", "auf", "ver\u00b7la\u00df\u00b7ner", "Flur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Von ihrem Kusse tr\u00e4umen,", "tokens": ["Von", "ih\u00b7rem", "Kus\u00b7se", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Ihr T\u00e4ubchen, darf ich nur:", "tokens": ["Ihr", "T\u00e4ub\u00b7chen", ",", "darf", "ich", "nur", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Denn ach! Belinde fliehet", "tokens": ["Denn", "ach", "!", "Be\u00b7lin\u00b7de", "flie\u00b7het"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "XY", "$.", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Das Thal, den Wasserfall,", "tokens": ["Das", "Thal", ",", "den", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Die Grotte selbst, und siehet", "tokens": ["Die", "Grot\u00b7te", "selbst", ",", "und", "sie\u00b7het"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Verr\u00e4ther \u00fcberall.", "tokens": ["Ver\u00b7r\u00e4\u00b7ther", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O glaubte nur Belinde", "tokens": ["O", "glaub\u00b7te", "nur", "Be\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem guten G\u00f6tterkinde,", "tokens": ["Dem", "gu\u00b7ten", "G\u00f6t\u00b7ter\u00b7kin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Liebe treuem Ruf,", "tokens": ["Der", "Lie\u00b7be", "treu\u00b7em", "Ruf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die sie, mit sanftem Herzen,", "tokens": ["Die", "sie", ",", "mit", "sanf\u00b7tem", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu K\u00fcssen und zu Scherzen,", "tokens": ["Zu", "K\u00fcs\u00b7sen", "und", "zu", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie euch, ihr T\u00e4ubchen, schuf!", "tokens": ["Wie", "euch", ",", "ihr", "T\u00e4ub\u00b7chen", ",", "schuf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}