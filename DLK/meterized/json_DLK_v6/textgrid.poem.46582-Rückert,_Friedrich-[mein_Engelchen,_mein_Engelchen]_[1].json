{"textgrid.poem.46582": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mein Engelchen, mein Engelchen] [1]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Engelchen, mein Engelchen,", "tokens": ["Mein", "En\u00b7gel\u00b7chen", ",", "mein", "En\u00b7gel\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+---+--", "measure": "dactylic.init"}, "line.2": {"text": "Ach hol' dir zum Gespielen", "tokens": ["Ach", "hol'", "dir", "zum", "Ge\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nur keines von den Bengelchen,", "tokens": ["Nur", "kei\u00b7nes", "von", "den", "Ben\u00b7gel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Den Br\u00fcderchen, den vielen.", "tokens": ["Den", "Br\u00fc\u00b7der\u00b7chen", ",", "den", "vie\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du schl\u00e4ngest wohl auf jener Flur", "tokens": ["Du", "schl\u00e4n\u00b7gest", "wohl", "auf", "je\u00b7ner", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Br\u00fcdern gern den Reigen;", "tokens": ["Mit", "Br\u00fc\u00b7dern", "gern", "den", "Rei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch kostet dich's ja wenig nur", "tokens": ["Doch", "kos\u00b7tet", "dich's", "ja", "we\u00b7nig", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von dort herab zu steigen", "tokens": ["Von", "dort", "her\u00b7ab", "zu", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nach Neuse\u00df, wo ich dir das Grab", "tokens": ["Nach", "Neu\u00b7se\u00df", ",", "wo", "ich", "dir", "das", "Grab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bereitet h\u00e4tte gerne;", "tokens": ["Be\u00b7rei\u00b7tet", "h\u00e4t\u00b7te", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch du von oben kommst herab,", "tokens": ["Doch", "du", "von", "o\u00b7ben", "kommst", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ist nicht Nah noch Ferne.", "tokens": ["Da", "ist", "nicht", "Nah", "noch", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da sollst du, wo sie stehn und gehn,", "tokens": ["Da", "sollst", "du", ",", "wo", "sie", "stehn", "und", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Oft in ihr Spiel dich mischen;", "tokens": ["Oft", "in", "ihr", "Spiel", "dich", "mi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn sie selbst dich auch nicht sehn,", "tokens": ["Und", "wenn", "sie", "selbst", "dich", "auch", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seh' ich dich doch dazwischen.", "tokens": ["Seh'", "ich", "dich", "doch", "da\u00b7zwi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}