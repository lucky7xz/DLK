{"dta.poem.11865": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die meisten V\u00f6gel bau'n f\u00fcr sich allein kein Nest,", "tokens": ["Die", "meis\u00b7ten", "V\u00f6\u00b7gel", "bau'n", "f\u00fcr", "sich", "al\u00b7lein", "kein", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "PRF", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr ihre Jungen nur bau'n sie's bequem und fest.", "tokens": ["F\u00fcr", "ih\u00b7re", "Jun\u00b7gen", "nur", "bau'n", "sie's", "be\u00b7quem", "und", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So viele Menschen auch, sie w\u00fcrden ihre Kraft", "tokens": ["So", "vie\u00b7le", "Men\u00b7schen", "auch", ",", "sie", "w\u00fcr\u00b7den", "ih\u00b7re", "Kraft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "ADV", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht nutzen, th\u00e4ten sie's nicht f\u00fcr Nachkommenschaft.", "tokens": ["Nicht", "nut\u00b7zen", ",", "th\u00e4\u00b7ten", "sie's", "nicht", "f\u00fcr", "Nach\u00b7kom\u00b7men\u00b7schaft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$