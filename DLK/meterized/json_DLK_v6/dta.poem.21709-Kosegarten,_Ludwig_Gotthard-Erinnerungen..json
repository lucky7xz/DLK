{"dta.poem.21709": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm, Schwester meines Herzens,", "tokens": ["Komm", ",", "Schwes\u00b7ter", "mei\u00b7nes", "Her\u00b7zens", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lass von des ", "tokens": ["Lass", "von", "des"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Uns staunend in die Ferne,", "tokens": ["Uns", "stau\u00b7nend", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins Ungemessne sehn.", "tokens": ["Ins", "Un\u00b7ge\u00b7mess\u00b7ne", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sieh wie im Abendschimmer,", "tokens": ["Sieh", "wie", "im", "A\u00b7bend\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von dunkler Fluth umkr\u00e4nzt,", "tokens": ["Von", "dunk\u00b7ler", "Fluth", "um\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dein m\u00fctterliches Eyland", "tokens": ["Dein", "m\u00fct\u00b7ter\u00b7li\u00b7ches", "Ey\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Uns hold entgegen gl\u00e4nzt.", "tokens": ["Uns", "hold", "ent\u00b7ge\u00b7gen", "gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sieh, sieh im Kranz der Berge", "tokens": ["Sieh", ",", "sieh", "im", "Kranz", "der", "Ber\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Stadt, die dich gebar;", "tokens": ["Die", "Stadt", ",", "die", "dich", "ge\u00b7bar", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich, Schwester, und das M\u00e4dchen", "tokens": ["Dich", ",", "Schwes\u00b7ter", ",", "und", "das", "M\u00e4d\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit goldbest\u00e4ubtem Haar.", "tokens": ["Mit", "gold\u00b7be\u00b7st\u00e4ub\u00b7tem", "Haar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Am donnernden Visurgis", "tokens": ["Am", "don\u00b7nern\u00b7den", "Vi\u00b7sur\u00b7gis"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wallt, die ich meine, nun;", "tokens": ["Wallt", ",", "die", "ich", "mei\u00b7ne", ",", "nun", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Friede soll und Tr\u00f6stung", "tokens": ["Und", "Frie\u00b7de", "soll", "und", "Tr\u00f6s\u00b7tung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf ihrer Scheitel ruhn.", "tokens": ["Auf", "ih\u00b7rer", "Schei\u00b7tel", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo rauscht ihr, heil'ge Hayne,", "tokens": ["Wo", "rauscht", "ihr", ",", "heil'\u00b7ge", "Hay\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo ich im Mondenblitz", "tokens": ["Wo", "ich", "im", "Mon\u00b7den\u00b7blitz"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Sternged\u00e4mmer schw\u00e4rmte,", "tokens": ["Und", "Stern\u00b7ge\u00b7d\u00e4m\u00b7mer", "schw\u00e4rm\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Hayne ", "tokens": ["Ihr", "Hay\u00b7ne"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Mir hat in euren Schatten", "tokens": ["Mir", "hat", "in", "eu\u00b7ren", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Urania sich verkl\u00e4rt,", "tokens": ["U\u00b7ra\u00b7nia", "sich", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PRF", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Hat mir in ernsten N\u00e4chten", "tokens": ["Hat", "mir", "in", "erns\u00b7ten", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Manch hohes Lied gelehrt.", "tokens": ["Manch", "ho\u00b7hes", "Lied", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sieh, sieh im fernen S\u00fcden", "tokens": ["Sieh", ",", "sieh", "im", "fer\u00b7nen", "S\u00fc\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Berge ", "tokens": ["Die", "Ber\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "In jener Berge Schr\u00fcnden,", "tokens": ["In", "je\u00b7ner", "Ber\u00b7ge", "Schr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In jener Waldnacht Graun,", "tokens": ["In", "je\u00b7ner", "Wald\u00b7nacht", "Graun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Rund der H\u00fcnenmale,", "tokens": ["Im", "Rund", "der", "H\u00fc\u00b7nen\u00b7ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo Hirsch und Hindinn gras't,", "tokens": ["Wo", "Hirsch", "und", "Hin\u00b7dinn", "gras't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hab' ich im Stolz der Jugend", "tokens": ["Hab'", "ich", "im", "Stolz", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Monden viel verras't", "tokens": ["Der", "Mon\u00b7den", "viel", "ver\u00b7ras't"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wo gl\u00e4nzen deine Wellen,", "tokens": ["Wo", "gl\u00e4n\u00b7zen", "dei\u00b7ne", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Friedsel'ger ", "tokens": ["Fried\u00b7sel'\u00b7ger"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "An dessen Kalmusufern", "tokens": ["An", "des\u00b7sen", "Kal\u00b7mu\u00b7su\u00b7fern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich sinnend wandelte,", "tokens": ["Ich", "sin\u00b7nend", "wan\u00b7del\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo ich der ", "tokens": ["Wo", "ich", "der"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und lauschend ihren T\u00f6nen", "tokens": ["Und", "lau\u00b7schend", "ih\u00b7ren", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von allem Harm genass.", "tokens": ["Von", "al\u00b7lem", "Harm", "ge\u00b7nass", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wo s\u00e4useln deine Pappeln,", "tokens": ["Wo", "s\u00e4u\u00b7seln", "dei\u00b7ne", "Pap\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kleeduftend ", "tokens": ["Klee\u00b7duf\u00b7tend"], "token_info": ["word"], "pos": ["NN"], "meter": "++-", "measure": "unknown.measure.di"}, "line.3": {"text": "In dessen Thymiangr\u00fcnden,", "tokens": ["In", "des\u00b7sen", "Thy\u00b7mi\u00b7an\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Einfalt stillem Sitz,", "tokens": ["Der", "Ein\u00b7falt", "stil\u00b7lem", "Sitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein bl\u00f6des M\u00e4gdlein sprosste,", "tokens": ["Ein", "bl\u00f6\u00b7des", "M\u00e4gd\u00b7lein", "spross\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das sch\u00fcchtern mich umschlang,", "tokens": ["Das", "sch\u00fcch\u00b7tern", "mich", "um\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und liebend mir versch\u00f6nert", "tokens": ["Und", "lie\u00b7bend", "mir", "ver\u00b7sch\u00f6\u00b7nert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den rauhen Lebensgang.", "tokens": ["Den", "rau\u00b7hen", "Le\u00b7bens\u00b7gang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sieh dort, am Saum des Osten,", "tokens": ["Sieh", "dort", ",", "am", "Saum", "des", "Os\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umsch\u00fcrzt vom Ozean", "tokens": ["Um\u00b7sch\u00fcrzt", "vom", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hebt ", "tokens": ["Hebt"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Titanisch mondhinan.", "tokens": ["Ti\u00b7ta\u00b7nisch", "mond\u00b7hi\u00b7nan", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl seyd ihr, ", "tokens": ["Wohl", "seyd", "ihr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Hoch Seelow's Wolkenheerd,", "tokens": ["Hoch", "See\u00b7low's", "Wol\u00b7ken\u00b7heerd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "