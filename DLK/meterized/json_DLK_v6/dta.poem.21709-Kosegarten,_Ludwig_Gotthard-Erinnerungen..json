RT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und seine Kraft gez\u00fcgelt,", "tokens": ["Und", "sei\u00b7ne", "Kraft", "ge\u00b7z\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und seinen Trotz gel\u00e4hmt?", "tokens": ["Und", "sei\u00b7nen", "Trotz", "ge\u00b7l\u00e4hmt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sey mir gegr\u00fcsst im Liede,", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fcs\u00b7st", "im", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Vertraulich Uferland!", "tokens": ["Ver\u00b7trau\u00b7lich", "U\u00b7fer\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geheime Kr\u00e4fte walten", "tokens": ["Ge\u00b7hei\u00b7me", "Kr\u00e4f\u00b7te", "wal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An deinem \u00f6den Strand.", "tokens": ["An", "dei\u00b7nem", "\u00f6\u00b7den", "Strand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In deinen Uferschr\u00fcnden", "tokens": ["In", "dei\u00b7nen", "U\u00b7fer\u00b7schr\u00fcn\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohnt namenlose Ruh,", "tokens": ["Wohnt", "na\u00b7men\