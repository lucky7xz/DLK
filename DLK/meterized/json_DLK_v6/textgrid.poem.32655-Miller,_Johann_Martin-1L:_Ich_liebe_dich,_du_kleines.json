{"textgrid.poem.32655": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich liebe dich, du kleines", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe dich, du kleines", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "du", "klei\u00b7nes"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und stilles Veilchenthal;", "tokens": ["Und", "stil\u00b7les", "Veil\u00b7chen\u00b7thal", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dennoch schuf mir keines,", "tokens": ["Und", "den\u00b7noch", "schuf", "mir", "kei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie du, so viele Qual.", "tokens": ["Wie", "du", ",", "so", "vie\u00b7le", "Qual", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dich liebet auch Seline,", "tokens": ["Dich", "lie\u00b7bet", "auch", "Se\u00b7li\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die junge Sch\u00e4ferin,", "tokens": ["Die", "jun\u00b7ge", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit der bescheidnen Miene", "tokens": ["Mit", "der", "be\u00b7scheid\u00b7nen", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und, ach! dem harten Sinn.", "tokens": ["Und", ",", "ach", "!", "dem", "har\u00b7ten", "Sinn", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "J\u00fcngst sa\u00df ich hier und spielte,", "tokens": ["J\u00fcngst", "sa\u00df", "ich", "hier", "und", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beim letzten Sonnenlicht;", "tokens": ["Beim", "letz\u00b7ten", "Son\u00b7nen\u00b7licht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zufrieden, denn ich f\u00fchlte", "tokens": ["Zu\u00b7frie\u00b7den", ",", "denn", "ich", "f\u00fchl\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch dich, o Liebe, nicht.", "tokens": ["Noch", "dich", ",", "o", "Lie\u00b7be", ",", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "$,", "FM", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Freiheit war's alleine,", "tokens": ["Die", "Frei\u00b7heit", "wa\u00b7r's", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die mein Gesang erhob;", "tokens": ["Die", "mein", "Ge\u00b7sang", "er\u00b7hob", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nie h\u00f6rten diese Haine", "tokens": ["Nie", "h\u00f6r\u00b7ten", "die\u00b7se", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von mir der Liebe Lob.", "tokens": ["Von", "mir", "der", "Lie\u00b7be", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nie sollst du mich bezwingen,", "tokens": ["Nie", "sollst", "du", "mich", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sang ich allzu k\u00fchn;", "tokens": ["So", "sang", "ich", "all\u00b7zu", "k\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Liebe, deinen Schlingen", "tokens": ["O", "Lie\u00b7be", ",", "dei\u00b7nen", "Schlin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will ich gewi\u00df entfliehn!", "tokens": ["Will", "ich", "ge\u00b7wi\u00df", "ent\u00b7fliehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du \u00e4nderst alle Herzen,", "tokens": ["Du", "\u00e4n\u00b7derst", "al\u00b7le", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zerst\u00f6rest ihre Lust;", "tokens": ["Zer\u00b7st\u00f6\u00b7rest", "ih\u00b7re", "Lust", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schaffest Gram und Schmerzen", "tokens": ["Und", "schaf\u00b7fest", "Gram", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der jugendlichen Brust.", "tokens": ["Der", "ju\u00b7gend\u00b7li\u00b7chen", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So sang ich dir, o Liebe,", "tokens": ["So", "sang", "ich", "dir", ",", "o", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit stolzem Herzen Hohn;", "tokens": ["Mit", "stol\u00b7zem", "Her\u00b7zen", "Hohn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und trotzte deinem Triebe,", "tokens": ["Und", "trotz\u00b7te", "dei\u00b7nem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem ich bisher entflohn.", "tokens": ["Dem", "ich", "bis\u00b7her", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Als pl\u00f6tzlich meinen Blicken", "tokens": ["Als", "pl\u00f6tz\u00b7lich", "mei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich eine Hirtin wies,", "tokens": ["Sich", "ei\u00b7ne", "Hir\u00b7tin", "wies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Sehnsucht und Entz\u00fccken", "tokens": ["Und", "Sehn\u00b7sucht", "und", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Ruhe mir entri\u00df.", "tokens": ["Die", "Ru\u00b7he", "mir", "ent\u00b7ri\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dorine sa\u00df und pfl\u00fcckte", "tokens": ["Do\u00b7ri\u00b7ne", "sa\u00df", "und", "pfl\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich Bl\u00fcmchen, las sie aus,", "tokens": ["Sich", "Bl\u00fcm\u00b7chen", ",", "las", "sie", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vereinte sie und schm\u00fcckte", "tokens": ["Ver\u00b7ein\u00b7te", "sie", "und", "schm\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich mit dem bunten Strau\u00df.", "tokens": ["Sich", "mit", "dem", "bun\u00b7ten", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Gern h\u00e4tt' ich sprechen wollen;", "tokens": ["Gern", "h\u00e4tt'", "ich", "spre\u00b7chen", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umsonst bem\u00fcht' ich mich:", "tokens": ["Um\u00b7sonst", "be\u00b7m\u00fcht'", "ich", "mich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Wort drang aus dem vollen", "tokens": ["Kein", "Wort", "drang", "aus", "dem", "vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Beklemmten Herzen sich.", "tokens": ["Be\u00b7klemm\u00b7ten", "Her\u00b7zen", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Noch lange stand ich, blickte", "tokens": ["Noch", "lan\u00b7ge", "stand", "ich", ",", "blick\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit trunknen Augen hin,", "tokens": ["Mit", "trunk\u00b7nen", "Au\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und immer mehr entz\u00fcckte", "tokens": ["Und", "im\u00b7mer", "mehr", "ent\u00b7z\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich diese Zauberin.", "tokens": ["Mich", "die\u00b7se", "Zau\u00b7be\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}