{"dta.poem.7506": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bedenke da\u00df ein Gott in deinem Leibe wohnt,", "tokens": ["Be\u00b7den\u00b7ke", "da\u00df", "ein", "Gott", "in", "dei\u00b7nem", "Lei\u00b7be", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vor Entweihung sei der Tempel stets verschont.", "tokens": ["Und", "vor", "Ent\u00b7wei\u00b7hung", "sei", "der", "Tem\u00b7pel", "stets", "ver\u00b7schont", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du kr\u00e4nkst den Gott in dir, wenn du den L\u00fcsten fr\u00f6hnest,", "tokens": ["Du", "kr\u00e4nkst", "den", "Gott", "in", "dir", ",", "wenn", "du", "den", "L\u00fcs\u00b7ten", "fr\u00f6h\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mehr noch wenn du in verkehrter Selbstqual st\u00f6hnest.", "tokens": ["Und", "mehr", "noch", "wenn", "du", "in", "ver\u00b7kehr\u00b7ter", "Selbst\u00b7qual", "st\u00f6h\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gott stieg herab, die Welt zu schaun mit deinen Augen;", "tokens": ["Gott", "stieg", "her\u00b7ab", ",", "die", "Welt", "zu", "schaun", "mit", "dei\u00b7nen", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm sollst du Opferduft mit reinen Sinnen saugen.", "tokens": ["Ihm", "sollst", "du", "Op\u00b7fer\u00b7duft", "mit", "rei\u00b7nen", "Sin\u00b7nen", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er ist, der in dir schaut und f\u00fchlt und denkt und spricht;", "tokens": ["Er", "ist", ",", "der", "in", "dir", "schaut", "und", "f\u00fchlt", "und", "denkt", "und", "spricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum was du schaust, f\u00fchlst, denkst und sprichst, sei g\u00f6ttlich licht.", "tokens": ["Drum", "was", "du", "schaust", ",", "f\u00fchlst", ",", "denkst", "und", "sprichst", ",", "sei", "g\u00f6tt\u00b7lich", "licht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}