{"dta.poem.18109": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Ku\u00df.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach schicke mir doch zu, ein K\u00fcssichen, mein Leben,", "tokens": ["Ach", "schi\u00b7cke", "mir", "doch", "zu", ",", "ein", "K\u00fcs\u00b7si\u00b7chen", ",", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcrchtst du, da\u00df auf dem Weg es jemand m\u00f6cht aufheben?", "tokens": ["F\u00fcrchtst", "du", ",", "da\u00df", "auf", "dem", "Weg", "es", "je\u00b7mand", "m\u00f6cht", "auf\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "PPER", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ey druck auf meinen Mund dein zartes M\u00fcndelein,", "tokens": ["Ey", "druck", "auf", "mei\u00b7nen", "Mund", "dein", "zar\u00b7tes", "M\u00fcn\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird es vor Gefahr der Diebe sicher seyn.", "tokens": ["So", "wird", "es", "vor", "Ge\u00b7fahr", "der", "Die\u00b7be", "si\u00b7cher", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}