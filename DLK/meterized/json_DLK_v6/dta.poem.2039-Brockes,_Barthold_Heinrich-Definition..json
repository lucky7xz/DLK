{"dta.poem.2039": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Definition.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist demnach ein Thier ein Wesen, das lebt, das", "tokens": ["Es", "ist", "dem\u00b7nach", "ein", "Thier", "ein", "We\u00b7sen", ",", "das", "lebt", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PAV", "ART", "NN", "ART", "NN", "$,", "PDS", "VVFIN", "$,", "PRELS"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "w\u00e4chst, das sich ern\u00e4hrt,", "tokens": ["w\u00e4chst", ",", "das", "sich", "er\u00b7n\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das sich beweget, das empfindet, und welches sein Ge-", "tokens": ["Das", "sich", "be\u00b7we\u00b7get", ",", "das", "emp\u00b7fin\u00b7det", ",", "und", "wel\u00b7ches", "sein", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "VVFIN", "$,", "PDS", "VVFIN", "$,", "KON", "PWAT", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "schlecht vermehrt.", "tokens": ["schlecht", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}