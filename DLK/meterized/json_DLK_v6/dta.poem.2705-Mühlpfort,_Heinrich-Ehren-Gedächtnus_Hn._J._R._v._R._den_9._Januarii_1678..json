{"dta.poem.2705": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ehren-Ged\u00e4chtnus/  \n  Hn. J. R. v. R. den 9. Januarii  1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sey des Orpheus Grab an Ehren so begl\u00fcckt/", "tokens": ["Es", "sey", "des", "Or\u00b7pheus", "Grab", "an", "Eh\u00b7ren", "so", "be\u00b7gl\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "NN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df j\u00e4hrlich Schwanen ihm die Sterbe-Lieder singen/", "tokens": ["Da\u00df", "j\u00e4hr\u00b7lich", "Schwa\u00b7nen", "ihm", "die", "Ster\u00b7be\u00b7Lie\u00b7der", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die B\u00fcrger in der Luft die sch\u00f6nsten Stimmen bringen/", "tokens": ["Die", "B\u00fcr\u00b7ger", "in", "der", "Luft", "die", "sch\u00f6ns\u00b7ten", "Stim\u00b7men", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df seiner Aschen-Rest die Nachtigal erquickt/", "tokens": ["Da\u00df", "sei\u00b7ner", "A\u00b7schen\u00b7Rest", "die", "Nach\u00b7ti\u00b7gal", "er\u00b7quickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und durch den Wunder-Thon mit Kunst ge\u00fcbter Zungen", "tokens": ["Und", "durch", "den", "Wun\u00b7der\u00b7Thon", "mit", "Kunst", "ge\u00b7\u00fcb\u00b7ter", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Beklagt den Leyer-Printz/ der Thier und Fels bezwungen.", "tokens": ["Be\u00b7klagt", "den", "Leyer\u00b7Printz", "/", "der", "Thier", "und", "Fels", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Es sey de\u00df Memnons Brand/ ein Bild der Ewigkeit", "tokens": ["Es", "sey", "de\u00df", "Mem\u00b7nons", "Brand", "/", "ein", "Bild", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn mitten aus der Glut der Sonnen-V\u00f6gel steigen;", "tokens": ["Wenn", "mit\u00b7ten", "aus", "der", "Glut", "der", "Son\u00b7nen\u00b7V\u00f6\u00b7gel", "stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn gar die Morgenr\u00f6th ihr Beyleid will bezeigen", "tokens": ["Wenn", "gar", "die", "Mor\u00b7gen\u00b7r\u00f6th", "ihr", "Bey\u00b7leid", "will", "be\u00b7zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und bey erwachtem Tag die nasse Perlen streut.", "tokens": ["Und", "bey", "er\u00b7wach\u00b7tem", "Tag", "die", "nas\u00b7se", "Per\u00b7len", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja selbst der Ph\u00f6nix sterb\u2019 auf W\u00fcrtz und Zimmet-R\u00f6hren", "tokens": ["Ja", "selbst", "der", "Ph\u00f6\u00b7nix", "sterb'", "auf", "W\u00fcrtz", "und", "Zim\u00b7met\u00b7R\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und lasse sich zuletzt durch Klang und Lieder ehren.", "tokens": ["Und", "las\u00b7se", "sich", "zu\u00b7letzt", "durch", "Klang", "und", "Lie\u00b7der", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So hat ", "tokens": ["So", "hat"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Sie Zunge Schlestens/ der Mund de\u00df Vater-\nlandes", "tokens": ["Sie", "Zun\u00b7ge", "Schles\u00b7tens", "/", "der", "Mund", "de\u00df", "Va\u00b7ter", "lan\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "NN", "$(", "ART", "NN", "ART", "TRUNC", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Ser F\u00fcrsten treuer Rath/ der Redner jedes Stan-\ndes;", "tokens": ["Ser", "F\u00fcrs\u00b7ten", "treu\u00b7er", "Rath", "/", "der", "Red\u00b7ner", "je\u00b7des", "Stan", "des", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$(", "ART", "NN", "PIAT", "NN", "ART", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es bricht der Nachruhm ihm schon Palm und Lorbern ab", "tokens": ["Es", "bricht", "der", "Nach\u00b7ruhm", "ihm", "schon", "Palm", "und", "Lor\u00b7bern", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADV", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Umb einen Helicon auff seine Grufft zu setzen/", "tokens": ["Umb", "ei\u00b7nen", "He\u00b7li\u00b7con", "auff", "sei\u00b7ne", "Grufft", "zu", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und will den hohen Ruhm in Ertz und Marmel \u00e4tzen.", "tokens": ["Und", "will", "den", "ho\u00b7hen", "Ruhm", "in", "Ertz", "und", "Mar\u00b7mel", "\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Geister Schlesiens so l\u00e4ngst die Sternen-Rey", "tokens": ["Die", "Geis\u00b7ter", "Schle\u00b7si\u00b7ens", "so", "l\u00e4ngst", "die", "Ster\u00b7nen\u00b7Rey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mit ihrem Glantz vermehrt/ der Zeiten Uberwinder", "tokens": ["Mit", "ih\u00b7rem", "Glantz", "ver\u00b7mehrt", "/", "der", "Zei\u00b7ten", "U\u00b7ber\u00b7win\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$(", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und unser Poesie sinnreicher Kunst-Erfinder/", "tokens": ["Und", "un\u00b7ser", "Poe\u00b7sie", "sinn\u00b7rei\u00b7cher", "Kunst\u00b7Er\u00b7fin\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Die f\u00fcgten/ Seeligster/ so eine Lobschrifft bey", "tokens": ["Die", "f\u00fcg\u00b7ten", "/", "See\u00b7ligs\u00b7ter", "/", "so", "ei\u00b7ne", "Lob\u00b7schrifft", "bey"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$(", "NN", "$(", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die deinen W\u00fcrden gleich und geben uns zu lesen/", "tokens": ["Die", "dei\u00b7nen", "W\u00fcr\u00b7den", "gleich", "und", "ge\u00b7ben", "uns", "zu", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie Tugend und Verstand unm\u00f6glich kan verwesen.", "tokens": ["Wie", "Tu\u00b7gend", "und", "Ver\u00b7stand", "un\u00b7m\u00f6g\u00b7lich", "kan", "ver\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Hat Orpheus Flu\u00df und Wald gezogen durch den Klang/", "tokens": ["Hat", "Or\u00b7pheus", "Flu\u00df", "und", "Wald", "ge\u00b7zo\u00b7gen", "durch", "den", "Klang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "KON", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So bunden vieler Hertz die Ketten deiner Zungen.", "tokens": ["So", "bun\u00b7den", "vie\u00b7ler", "Hertz", "die", "Ket\u00b7ten", "dei\u00b7ner", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ist von der Vogel-Schaar de\u00df Memnons Grab erklungen/", "tokens": ["Ist", "von", "der", "Vo\u00b7gel\u00b7Schaar", "de\u00df", "Mem\u00b7nons", "Grab", "er\u00b7klun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So w\u00fcrde manch Poet Geist/ Leben und Gesang", "tokens": ["So", "w\u00fcr\u00b7de", "manch", "Po\u00b7et", "Geist", "/", "Le\u00b7ben", "und", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Noch deiner Aschen weyhn/ Apollo mit den Neunen", "tokens": ["Noch", "dei\u00b7ner", "A\u00b7schen", "weyhn", "/", "A\u00b7pol\u00b7lo", "mit", "den", "Neu\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$(", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Umb seinen ", "tokens": ["Umb", "sei\u00b7nen"], "token_info": ["word", "word"], "pos": ["KOUI", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Allein ", "tokens": ["Al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.32": {"text": "Vergr\u00f6ssert deinen Ruhm/ du hast dir in den Hertzen", "tokens": ["Ver\u00b7gr\u00f6s\u00b7sert", "dei\u00b7nen", "Ruhm", "/", "du", "hast", "dir", "in", "den", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "L\u00e4ngst Tempel aufgebaut/ der lichten Ehren Kertzen", "tokens": ["L\u00e4ngst", "Tem\u00b7pel", "auf\u00b7ge\u00b7baut", "/", "der", "lich\u00b7ten", "Eh\u00b7ren", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVPP", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Verd\u00fcstert keine Nacht/ so lang als Menschen seyn", "tokens": ["Ver\u00b7d\u00fcs\u00b7tert", "kei\u00b7ne", "Nacht", "/", "so", "lang", "als", "Men\u00b7schen", "seyn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$(", "ADV", "ADJD", "KOKOM", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und Schlesien wird stehn/ mu\u00df auch der Neid bekennen", "tokens": ["Und", "Schle\u00b7si\u00b7en", "wird", "stehn", "/", "mu\u00df", "auch", "der", "Neid", "be\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "VVINF", "$(", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df dir bey vielen noch Ged\u00e4chtn\u00fc\u00df-Ampeln brennen.", "tokens": ["Da\u00df", "dir", "bey", "vie\u00b7len", "noch", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df\u00b7Am\u00b7peln", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Offt wird der Marmelstein f\u00fcr falschem Lobe roth/", "tokens": ["Offt", "wird", "der", "Mar\u00b7mel\u00b7stein", "f\u00fcr", "fal\u00b7schem", "Lo\u00b7be", "roth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und die erdichte Schrifft zersprenget ihn in Ritze.", "tokens": ["Und", "die", "er\u00b7dich\u00b7te", "Schrifft", "zer\u00b7spren\u00b7get", "ihn", "in", "Rit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was ist das uns f\u00fcr F\u00e4ul\u2019 und Untergang besch\u00fctze?", "tokens": ["Was", "ist", "das", "uns", "f\u00fcr", "F\u00e4ul'", "und", "Un\u00b7ter\u00b7gang", "be\u00b7sch\u00fct\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Zerst\u00f6rt die Pyramis und das Mausol der Tod?", "tokens": ["Zer\u00b7st\u00f6rt", "die", "Py\u00b7ra\u00b7mis", "und", "das", "Mau\u00b7sol", "der", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Macht seine Tyranney auch Leichen aus den Steinen?", "tokens": ["Macht", "sei\u00b7ne", "Ty\u00b7ran\u00b7ney", "auch", "Lei\u00b7chen", "aus", "den", "Stei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Zerbricht er Ertzt und Stahl gleich unsern d\u00fcrren Beinen?", "tokens": ["Zer\u00b7bricht", "er", "Ertzt", "und", "Stahl", "gleich", "un\u00b7sern", "d\u00fcr\u00b7ren", "Bei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "KON", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So ist ja di\u00df nur blo\u00df was von uns \u00fcbrig bleibt/", "tokens": ["So", "ist", "ja", "di\u00df", "nur", "blo\u00df", "was", "von", "uns", "\u00fcb\u00b7rig", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDS", "ADV", "ADV", "PWS", "APPR", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text