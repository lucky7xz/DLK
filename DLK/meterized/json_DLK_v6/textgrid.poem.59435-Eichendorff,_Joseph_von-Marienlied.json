{"textgrid.poem.59435": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Marienlied", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ins Land die Wetter h\u00e4ngen", "tokens": ["Wenn", "ins", "Land", "die", "Wet\u00b7ter", "h\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Mensch erschrocken steht,", "tokens": ["Und", "der", "Mensch", "er\u00b7schro\u00b7cken", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wendet, wie mit Glockenkl\u00e4ngen", "tokens": ["Wen\u00b7det", ",", "wie", "mit", "Glo\u00b7cken\u00b7kl\u00e4n\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Gewitter dein Gebet,", "tokens": ["Die", "Ge\u00b7wit\u00b7ter", "dein", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Und wo aus den grauen Wogen", "tokens": ["Und", "wo", "aus", "den", "grau\u00b7en", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Weinend auftaucht das Gefild,", "tokens": ["Wei\u00b7nend", "auf\u00b7taucht", "das", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Segnest du's vom Regenbogen \u2013", "tokens": ["Seg\u00b7nest", "du's", "vom", "Re\u00b7gen\u00b7bo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mutter, ach wie bist du mild!", "tokens": ["Mut\u00b7ter", ",", "ach", "wie", "bist", "du", "mild", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn's einst dunkelt auf den Gipfeln", "tokens": ["Wenn's", "einst", "dun\u00b7kelt", "auf", "den", "Gip\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der k\u00fchle Abend sacht", "tokens": ["Und", "der", "k\u00fch\u00b7le", "A\u00b7bend", "sacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niederrauschet in den Wipfeln:", "tokens": ["Nie\u00b7der\u00b7rau\u00b7schet", "in", "den", "Wip\u00b7feln", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O Maria, heil'ge Nacht!", "tokens": ["O", "Ma\u00b7ria", ",", "heil'\u00b7ge", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df mich nimmer wie die andern,", "tokens": ["La\u00df", "mich", "nim\u00b7mer", "wie", "die", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "KOKOM", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Decke zu der letzten Ruh", "tokens": ["De\u00b7cke", "zu", "der", "letz\u00b7ten", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00fctterlich den m\u00fcden Wandrer", "tokens": ["M\u00fct\u00b7ter\u00b7lich", "den", "m\u00fc\u00b7den", "Wand\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem Sternenmantel zu.", "tokens": ["Mit", "dem", "Ster\u00b7nen\u00b7man\u00b7tel", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}