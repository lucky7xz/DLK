{"textgrid.poem.31617": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du starker Donner-Gott! gib deinem Donner-Krafft /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du starker Donner-Gott! gib deinem Donner-Krafft /", "tokens": ["Du", "star\u00b7ker", "Don\u00b7ner\u00b7Gott", "!", "gib", "dei\u00b7nem", "Don\u00b7ner\u00b7Krafft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VVIMP", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dem Herz durchdringungs-Wort; da\u00df man die Geistes-Blitze", "tokens": ["dem", "Herz", "durch\u00b7drin\u00b7gungs\u00b7Wort", ";", "da\u00df", "man", "die", "Geis\u00b7tes\u00b7Blit\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "darauf erblick' / und f\u00fchl' auch die Einschlagungs Hitze /", "tokens": ["da\u00b7rauf", "er\u00b7blick'", "/", "und", "f\u00fchl'", "auch", "die", "Ein\u00b7schla\u00b7gungs", "Hit\u00b7ze", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$(", "KON", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "da\u00df allen Herzen-Stolz es strack danider rafft!", "tokens": ["da\u00df", "al\u00b7len", "Her\u00b7zen\u00b7Stolz", "es", "strack", "da\u00b7ni\u00b7der", "rafft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVFIN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "das Donner-prastlen hat Bekehrungs-Eigenschafft /", "tokens": ["das", "Don\u00b7ner\u00b7prast\u00b7len", "hat", "Be\u00b7keh\u00b7rungs\u00b7Ei\u00b7gen\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "weil Gottes Gegenwart im Schrecken hat den Sitze.", "tokens": ["weil", "Got\u00b7tes", "Ge\u00b7gen\u00b7wart", "im", "Schre\u00b7cken", "hat", "den", "Sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "APPRART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist voll Fruchtbarkeit di\u00df schr\u00f6cklich Lufft gesch\u00fctze:", "tokens": ["Es", "ist", "voll", "Frucht\u00b7bar\u00b7keit", "di\u00df", "schr\u00f6ck\u00b7lich", "Lufft", "ge\u00b7sch\u00fct\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "PDS", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So ist sein Eyfer auch mit Gnaden-Krafft behafft.", "tokens": ["So", "ist", "sein", "Ey\u00b7fer", "auch", "mit", "Gna\u00b7den\u00b7Krafft", "be\u00b7hafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Wunder-Strahl / sein Wort / verletzt der Seelen klingen /", "tokens": ["Der", "Wun\u00b7der\u00b7Strahl", "/", "sein", "Wort", "/", "ver\u00b7letzt", "der", "See\u00b7len", "klin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPOSAT", "NN", "$(", "VVPP", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "dem Leib die scheiden nicht; das stark' ist nur sein Ziel.", "tokens": ["dem", "Leib", "die", "schei\u00b7den", "nicht", ";", "das", "stark'", "ist", "nur", "sein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "PTKNEG", "$.", "PDS", "VVFIN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sein Geist-Subtiligkeit kan unvermerkt durchdringen.", "tokens": ["Sein", "Geist\u00b7Sub\u00b7ti\u00b7lig\u00b7keit", "kan", "un\u00b7ver\u00b7merkt", "durch\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zu zeiten durch den Schall zu f\u00e4llen ihm gefiel.", "tokens": ["Zu", "zei\u00b7ten", "durch", "den", "Schall", "zu", "f\u00e4l\u00b7len", "ihm", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Beh\u00fct uns nur / O Gott / vor Wolken Donnerschl\u00e4gen:", "tokens": ["Be\u00b7h\u00fct", "uns", "nur", "/", "O", "Gott", "/", "vor", "Wol\u00b7ken", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$(", "NE", "NN", "$(", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch deine Wortstreich wollst bekehrend uns erlegen!", "tokens": ["Durch", "dei\u00b7ne", "Wort\u00b7streich", "wollst", "be\u00b7keh\u00b7rend", "uns", "er\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVPP", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}