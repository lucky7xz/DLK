{"textgrid.poem.37530": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Maulwurf", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In seinem Garten freudevoll", "tokens": ["In", "sei\u00b7nem", "Gar\u00b7ten", "freu\u00b7de\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht hier ein G\u00e4rtner namens Knoll.", "tokens": ["Geht", "hier", "ein", "G\u00e4rt\u00b7ner", "na\u00b7mens", "Knoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch seine Freudigkeit vergeht.", "tokens": ["Doch", "sei\u00b7ne", "Freu\u00b7dig\u00b7keit", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Maulwurf w\u00fchlt im Pflanzenbeet.", "tokens": ["Ein", "Maul\u00b7wurf", "w\u00fchlt", "im", "Pflan\u00b7zen\u00b7beet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schnell eilt er fort und holt die Hacke,", "tokens": ["Schnell", "eilt", "er", "fort", "und", "holt", "die", "Ha\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er den schwarzen W\u00fchler packe.", "tokens": ["Da\u00df", "er", "den", "schwar\u00b7zen", "W\u00fch\u00b7ler", "pa\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt ist vor allem an der Zeit", "tokens": ["Jetzt", "ist", "vor", "al\u00b7lem", "an", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die listige Verschwiegenheit.", "tokens": ["Die", "lis\u00b7ti\u00b7ge", "Ver\u00b7schwie\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Aha! Schon hebt sich was im Beet,", "tokens": ["A\u00b7ha", "!", "Schon", "hebt", "sich", "was", "im", "Beet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PRF", "PRELS", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und Knoll erhebt sein Jagdger\u00e4t.", "tokens": ["Und", "Knoll", "er\u00b7hebt", "sein", "Jagd\u00b7ge\u00b7r\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schwupp! da \u2013 und Knoll verfehlt das Ziel.", "tokens": ["Schwupp", "!", "da", "\u2013", "und", "Knoll", "ver\u00b7fehlt", "das", "Ziel", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "$(", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Hacke trennt sich von dem Stiel.", "tokens": ["Die", "Ha\u00b7cke", "trennt", "sich", "von", "dem", "Stiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Instrument ist schnell geheilt;", "tokens": ["Das", "Inst\u00b7ru\u00b7ment", "ist", "schnell", "ge\u00b7heilt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Nagel wird hineingekeilt.", "tokens": ["Ein", "Na\u00b7gel", "wird", "hin\u00b7ein\u00b7ge\u00b7keilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und wieder steht er ernst und krumm", "tokens": ["Und", "wie\u00b7der", "steht", "er", "ernst", "und", "krumm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaut nach keiner Seite um.", "tokens": ["Und", "schaut", "nach", "kei\u00b7ner", "Sei\u00b7te", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Klabumm! \u2013 So krieg die Schwerenot!", "tokens": ["Kla\u00b7bumm", "!", "\u2013", "So", "krieg", "die", "Schwe\u00b7re\u00b7not", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nachbar schie\u00dft die Spatzen tot.", "tokens": ["Der", "Nach\u00b7bar", "schie\u00dft", "die", "Spat\u00b7zen", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch immerhin und einerlei!", "tokens": ["Doch", "im\u00b7mer\u00b7hin", "und", "ei\u00b7ner\u00b7lei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Flintenschu\u00df ist schnell vorbei.", "tokens": ["Ein", "Flin\u00b7ten\u00b7schu\u00df", "ist", "schnell", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Schon wieder w\u00fchlt das Ungetier.", "tokens": ["Schon", "wie\u00b7der", "w\u00fchlt", "das", "Un\u00b7ge\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwart!\u00ab \u2013 denkt sich Knoll \u2013 \u00bbJetzt kommen wir!\u00ab", "tokens": ["\u00bb", "wart", "!", "\u00ab", "\u2013", "denkt", "sich", "Knoll", "\u2013", "\u00bb", "Jetzt", "kom\u00b7men", "wir", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "$(", "VVFIN", "PRF", "ADJD", "$(", "$(", "ADV", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Er schwingt die Hacke voller Hast \u2013", "tokens": ["Er", "schwingt", "die", "Ha\u00b7cke", "vol\u00b7ler", "Hast", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Radatsch! \u2013 o sch\u00f6ner Birnenast!", "tokens": ["Ra\u00b7datsch", "!", "\u2013", "o", "sch\u00f6\u00b7ner", "Bir\u00b7ne\u00b7nast", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Die Hacke \u00e4rgert ihn doch sehr,", "tokens": ["Die", "Ha\u00b7cke", "\u00e4r\u00b7gert", "ihn", "doch", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum holt er jetzt den Spaten her.", "tokens": ["Drum", "holt", "er", "jetzt", "den", "Spa\u00b7ten", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Nun, Alter, sei gescheit und weise,", "tokens": ["Nun", ",", "Al\u00b7ter", ",", "sei", "ge\u00b7scheit", "und", "wei\u00b7se", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mache leise, leise, leise!", "tokens": ["Und", "ma\u00b7che", "lei\u00b7se", ",", "lei\u00b7se", ",", "lei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Schnarr\u00e4ng!! \u2013 Da t\u00f6nt ihm in das Ohr", "tokens": ["Schnar\u00b7r\u00e4ng", "!!", "\u2013", "Da", "t\u00f6nt", "ihm", "in", "das", "Ohr"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Bettelmusikantenchor.", "tokens": ["Ein", "Bet\u00b7tel\u00b7mu\u00b7si\u00b7kan\u00b7ten\u00b7chor", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Musik wird oft nicht sch\u00f6n gefunden,", "tokens": ["Mu\u00b7sik", "wird", "oft", "nicht", "sch\u00f6n", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil sie stets mit Ger\u00e4usch verbunden.", "tokens": ["Weil", "sie", "stets", "mit", "Ge\u00b7r\u00e4usch", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.17": {"line.1": {"text": "Kaum ist's vorbei mit dem Trara,", "tokens": ["Kaum", "ist's", "vor\u00b7bei", "mit", "dem", "Tra\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So ist der W\u00fchler wieder da.", "tokens": ["So", "ist", "der", "W\u00fch\u00b7ler", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Schnupp! dringt die Schaufel, wie der Blitz,", "tokens": ["Schnupp", "!", "dringt", "die", "Schau\u00b7fel", ",", "wie", "der", "Blitz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Maulwurf unter seinen Sitz.", "tokens": ["Dem", "Maul\u00b7wurf", "un\u00b7ter", "sei\u00b7nen", "Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Und mit Hurra in einem Bogen", "tokens": ["Und", "mit", "Hur\u00b7ra", "in", "ei\u00b7nem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wird er herauf ans Licht gezogen.", "tokens": ["Wird", "er", "her\u00b7auf", "ans", "Licht", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Aujau! Man setzt sich in den Rechen", "tokens": ["Au\u00b7jau", "!", "Man", "setzt", "sich", "in", "den", "Re\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PIS", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll spitzer Stacheln, welche stechen.", "tokens": ["Voll", "spit\u00b7zer", "Sta\u00b7cheln", ",", "wel\u00b7che", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Und Knoll zieht f\u00fcr den Augenblick", "tokens": ["Und", "Knoll", "zieht", "f\u00fcr", "den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich schmerzlich in sich selbst zur\u00fcck.", "tokens": ["Sich", "schmerz\u00b7lich", "in", "sich", "selbst", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Schon hat der Maulwurf sich derweil", "tokens": ["Schon", "hat", "der", "Maul\u00b7wurf", "sich", "der\u00b7weil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Loch gescharrt in Angst und Eil.", "tokens": ["Ein", "Loch", "ge\u00b7scharrt", "in", "Angst", "und", "Eil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Doch Knoll, der sich emporgerafft,", "tokens": ["Doch", "Knoll", ",", "der", "sich", "em\u00b7por\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beraubt ihn seiner Lebenskraft.", "tokens": ["Be\u00b7raubt", "ihn", "sei\u00b7ner", "Le\u00b7bens\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "Da liegt der schwarze B\u00f6sewicht", "tokens": ["Da", "liegt", "der", "schwar\u00b7ze", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00fchlte gern und kann doch nicht;", "tokens": ["Und", "w\u00fchl\u00b7te", "gern", "und", "kann", "doch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KON", "VMFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn hinderlich, wie \u00fcberall,", "tokens": ["Denn", "hin\u00b7der\u00b7lich", ",", "wie", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist hier der eigne Todesfall.", "tokens": ["Ist", "hier", "der", "eig\u00b7ne", "To\u00b7des\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}