{"dta.poem.18266": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Offener Brief.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Bei mir habt ihr den K\u00fcrzern gezogen!", "tokens": ["Bei", "mir", "habt", "ihr", "den", "K\u00fcr\u00b7zern", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Drei winzige J\u00e4hrchen erst ist es her,", "tokens": ["Drei", "win\u00b7zi\u00b7ge", "J\u00e4hr\u00b7chen", "erst", "ist", "es", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da habt ihr geflucht die Kreuz und Quer:", "tokens": ["Da", "habt", "ihr", "ge\u00b7flucht", "die", "Kreuz", "und", "Quer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Kerl, der hat zu lange Ohren,", "tokens": ["Der", "Kerl", ",", "der", "hat", "zu", "lan\u00b7ge", "Oh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An dem ist Hopfen und Malz verloren!", "tokens": ["An", "dem", "ist", "Hop\u00b7fen", "und", "Malz", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Und heute? Donner und Doria!", "tokens": ["Und", "heu\u00b7te", "?", "Don\u00b7ner", "und", "Do\u00b7ria", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Grenzt das nicht schamlos an einen Eclat?", "tokens": ["Grenzt", "das", "nicht", "scham\u00b7los", "an", "ei\u00b7nen", "E\u00b7clat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwar, was er wei\u00df, ist nur autodidaktisch,", "tokens": ["Zwar", ",", "was", "er", "wei\u00df", ",", "ist", "nur", "au\u00b7to\u00b7di\u00b7dak\u00b7tisch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aber das Factum ist eben faktisch:", "tokens": ["A\u00b7ber", "das", "Fac\u00b7tum", "ist", "e\u00b7ben", "fak\u00b7tisch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Er capirte die deutsche Poesie", "tokens": ["Er", "ca\u00b7pir\u00b7te", "die", "deut\u00b7sche", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Auch ohne die griechischen Verba auf mi!", "tokens": ["Auch", "oh\u00b7ne", "die", "grie\u00b7chi\u00b7schen", "Ver\u00b7ba", "auf", "mi", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}