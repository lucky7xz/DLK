{"dta.poem.8923": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LvI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Um uns zu hindern und getrennt zu halten,", "tokens": ["Um", "uns", "zu", "hin\u00b7dern", "und", "ge\u00b7trennt", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "KON", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch z\u00e4hl' ich nicht dich zum Geschlecht der Kalten,", "tokens": ["Noch", "z\u00e4hl'", "ich", "nicht", "dich", "zum", "Ge\u00b7schlecht", "der", "Kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Noch geht ein Weg von deinem Blick in meinen.", "tokens": ["Noch", "geht", "ein", "Weg", "von", "dei\u00b7nem", "Blick", "in", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch allzuselten seh' ich dich erscheinen,", "tokens": ["Doch", "all\u00b7zu\u00b7sel\u00b7ten", "seh'", "ich", "dich", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn ich rings das Auge lasse walten,", "tokens": ["Und", "wenn", "ich", "rings", "das", "Au\u00b7ge", "las\u00b7se", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vermiss' ich stets die liebste der Gestalten,", "tokens": ["Ver\u00b7miss'", "ich", "stets", "die", "liebs\u00b7te", "der", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die liebsten Z\u00fcge fehlen stets, die deinen!", "tokens": ["Die", "liebs\u00b7ten", "Z\u00fc\u00b7ge", "feh\u00b7len", "stets", ",", "die", "dei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,", "PRELS", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ermanne dich, und lege nicht die Z\u00e4ume", "tokens": ["Er\u00b7man\u00b7ne", "dich", ",", "und", "le\u00b7ge", "nicht", "die", "Z\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Liebe furchtsam in die Hand des Neides,", "tokens": ["Der", "Lie\u00b7be", "furcht\u00b7sam", "in", "die", "Hand", "des", "Nei\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der gern uns schiede durch entlegne R\u00e4ume!", "tokens": ["Der", "gern", "uns", "schie\u00b7de", "durch", "ent\u00b7leg\u00b7ne", "R\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sey ganz du selbst, dann wird die Zeit des Leides", "tokens": ["Sey", "ganz", "du", "selbst", ",", "dann", "wird", "die", "Zeit", "des", "Lei\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verronnen seyn, dann werden unsre Tr\u00e4ume", "tokens": ["Ver\u00b7ron\u00b7nen", "seyn", ",", "dann", "wer\u00b7den", "uns\u00b7re", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAINF", "$,", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verk\u00f6rpert werden. Wir verdienen beydes.", "tokens": ["Ver\u00b7k\u00f6r\u00b7pert", "wer\u00b7den", ".", "Wir", "ver\u00b7die\u00b7nen", "bey\u00b7des", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "$.", "PPER", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}