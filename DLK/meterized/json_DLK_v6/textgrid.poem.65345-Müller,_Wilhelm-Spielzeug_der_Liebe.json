{"textgrid.poem.65345": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Spielzeug der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ein stummes Kindlein ward meine Liebe j\u00fcngst geboren,", "tokens": ["Als", "ein", "stum\u00b7mes", "Kin\u00b7dlein", "ward", "mei\u00b7ne", "Lie\u00b7be", "j\u00fcngst", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Schreien hat es bald gelernt und bet\u00e4ubt dir nun die Ohren.", "tokens": ["Schrei\u00b7en", "hat", "es", "bald", "ge\u00b7lernt", "und", "be\u00b7t\u00e4ubt", "dir", "nun", "die", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "O so stopf' ihm doch den Mund mit dem Zucker deiner K\u00fcsse,", "tokens": ["O", "so", "stopf'", "ihm", "doch", "den", "Mund", "mit", "dem", "Zu\u00b7cker", "dei\u00b7ner", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und zum Spielen gieb ihm hin deines K\u00f6pfchens harte N\u00fcsse.", "tokens": ["Und", "zum", "Spie\u00b7len", "gieb", "ihm", "hin", "dei\u00b7nes", "K\u00f6pf\u00b7chens", "har\u00b7te", "N\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVIMP", "PPER", "ADV", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}