{"dta.poem.21458": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Meine Gottheit bist du in Ewigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O Anna glaub\u2019, ich lieb\u2019 nur dich,", "tokens": ["O", "An\u00b7na", "glaub'", ",", "ich", "lieb'", "nur", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur dich sucht meine Seele,", "tokens": ["Nur", "dich", "sucht", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du bist der Sch\u00f6nheit Stern f\u00fcr mich:", "tokens": ["Du", "bist", "der", "Sch\u00f6n\u00b7heit", "Stern", "f\u00fcr", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz Licht, ganz ohne Fehle.", "tokens": ["Ganz", "Licht", ",", "ganz", "oh\u00b7ne", "Feh\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Wort mein d\u00fcrstend Herz nur f\u00fcllt,", "tokens": ["Dein", "Wort", "mein", "d\u00fcrs\u00b7tend", "Herz", "nur", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "ADJD", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Gl\u00fcck hellt meine Nacht \u2014", "tokens": ["Dein", "Gl\u00fcck", "hellt", "mei\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "K\u00e4mpf\u2019 ich auch jetzt nebelumh\u00fcllt:", "tokens": ["K\u00e4mpf'", "ich", "auch", "jetzt", "ne\u00b7be\u00b7lum\u00b7h\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Als Sieger end\u2019 ich die Schlacht.", "tokens": ["Als", "Sie\u00b7ger", "end'", "ich", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Als Sieger kehr\u2019 ich zu dir mein Kind,", "tokens": ["Als", "Sie\u00b7ger", "kehr'", "ich", "zu", "dir", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Meiner Tage Sehnsucht und Traum,", "tokens": ["Mei\u00b7ner", "Ta\u00b7ge", "Sehn\u00b7sucht", "und", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Dein Odem umweht mich friedenslind,", "tokens": ["Dein", "O\u00b7dem", "um\u00b7weht", "mich", "frie\u00b7dens\u00b7lind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Da\u00df ich fern je \u2014 wei\u00df ich kaum.", "tokens": ["Da\u00df", "ich", "fern", "je", "wei\u00df", "ich", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "$(", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "An deiner Brust stirbt der D\u00e4mon Schmerz \u2026", "tokens": ["An", "dei\u00b7ner", "Brust", "stirbt", "der", "D\u00e4\u00b7mon", "Schmerz", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "O holde M\u00e4hrchenstund\u2019!", "tokens": ["O", "hol\u00b7de", "M\u00e4hr\u00b7chen\u00b7stund'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Immer wieder k\u00fcss\u2019 ich, du einziges Herz,", "tokens": ["Im\u00b7mer", "wie\u00b7der", "k\u00fcss'", "ich", ",", "du", "ein\u00b7zi\u00b7ges", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Deinen viels\u00fc\u00dfen Kindermund.", "tokens": ["Dei\u00b7nen", "viel\u00b7s\u00fc\u00b7\u00dfen", "Kin\u00b7der\u00b7mund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Ich frage dich nicht; ich wei\u00df es genau:", "tokens": ["Ich", "fra\u00b7ge", "dich", "nicht", ";", "ich", "wei\u00df", "es", "ge\u00b7nau", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Mein ist dein reiches Gem\u00fcth,", "tokens": ["Mein", "ist", "dein", "rei\u00b7ches", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Mein der seligleuchtende Fr\u00fchlingsthau,", "tokens": ["Mein", "der", "se\u00b7lig\u00b7leuch\u00b7ten\u00b7de", "Fr\u00fch\u00b7lingst\u00b7hau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Der deinen Augen entspr\u00fcht.", "tokens": ["Der", "dei\u00b7nen", "Au\u00b7gen", "ent\u00b7spr\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Mein bist du; mein, o Seligkeit!", "tokens": ["Mein", "bist", "du", ";", "mein", ",", "o", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "$.", "PPOSAT", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Einzigmein in Lust und Graus!", "tokens": ["Ein\u00b7zig\u00b7mein", "in", "Lust", "und", "Graus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Meine Gottheit bist du in Ewigkeit!", "tokens": ["Mei\u00b7ne", "Got\u00b7theit", "bist", "du", "in", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.24": {"text": "Und stirbst du \u2014 dann Sonne lisch\u2019 aus.", "tokens": ["Und", "stirbst", "du", "dann", "Son\u00b7ne", "lisch'", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "ADV", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}