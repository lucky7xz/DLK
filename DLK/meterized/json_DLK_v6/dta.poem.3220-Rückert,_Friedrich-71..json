{"dta.poem.3220": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du f\u00fcr dich allein und deinen Frieden sorgtest;", "tokens": ["Wenn", "du", "f\u00fcr", "dich", "al\u00b7lein", "und", "dei\u00b7nen", "Frie\u00b7den", "sorg\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wozu da\u00df von der Welt du noch die Flitter borgtest?", "tokens": ["Wo\u00b7zu", "da\u00df", "von", "der", "Welt", "du", "noch", "die", "Flit\u00b7ter", "borg\u00b7test", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOUS", "APPR", "ART", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du h\u00e4ttest andres nun und bess'res nicht zu thun", "tokens": ["Du", "h\u00e4t\u00b7test", "and\u00b7res", "nun", "und", "bess'\u00b7res", "nicht", "zu", "thun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "KON", "ADJA", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als abzuthun die Welt und still in Gott zu ruhn.", "tokens": ["Als", "ab\u00b7zu\u00b7thun", "die", "Welt", "und", "still", "in", "Gott", "zu", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVIZU", "ART", "NN", "KON", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Allein dein Streben ist nicht f\u00fcr dein enges Zelt,", "tokens": ["Al\u00b7lein", "dein", "Stre\u00b7ben", "ist", "nicht", "f\u00fcr", "dein", "en\u00b7ges", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Streben ist zugleich f\u00fcr Gottes weite Welt.", "tokens": ["Dein", "Stre\u00b7ben", "ist", "zu\u00b7gleich", "f\u00fcr", "Got\u00b7tes", "wei\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dein heilig Streben sei, das Sinnliche zum Sch\u00f6nen", "tokens": ["Dein", "hei\u00b7lig", "Stre\u00b7ben", "sei", ",", "das", "Sinn\u00b7li\u00b7che", "zum", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu l\u00e4utern, um Gesch\u00f6pf und Sch\u00f6pfung zu vers\u00f6hnen.", "tokens": ["Zu", "l\u00e4u\u00b7tern", ",", "um", "Ge\u00b7sch\u00f6pf", "und", "Sch\u00f6p\u00b7fung", "zu", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUI", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "L\u00f6s' auf den Widerspruch, gleich' aus den Zahlenbruch,", "tokens": ["L\u00f6s'", "auf", "den", "Wi\u00b7der\u00b7spruch", ",", "gleich'", "aus", "den", "Zah\u00b7len\u00b7bruch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "O Geist der Lieb', und wandl' in Segen nun den Fluch.", "tokens": ["O", "Geist", "der", "Lieb'", ",", "und", "wandl'", "in", "Se\u00b7gen", "nun", "den", "Fluch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In Zukunfts Furchen wird der Ernte Segen sprossen,", "tokens": ["In", "Zu\u00b7kunfts", "Fur\u00b7chen", "wird", "der", "Ern\u00b7te", "Se\u00b7gen", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in das Heil der Welt ist meins miteingeschlossen.", "tokens": ["Und", "in", "das", "Heil", "der", "Welt", "ist", "meins", "mi\u00b7tein\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}