{"textgrid.poem.53512": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Krethi und Plethi", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vater Liebert hat eine Rede vom Stapel gelassen,", "tokens": ["Va\u00b7ter", "Lie\u00b7bert", "hat", "ei\u00b7ne", "Re\u00b7de", "vom", "Sta\u00b7pel", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "in der er sagte, der Reichstag t\u00e4te ihm nicht mehr passen.", "tokens": ["in", "der", "er", "sag\u00b7te", ",", "der", "Reichs\u00b7tag", "t\u00e4\u00b7te", "ihm", "nicht", "mehr", "pas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Denn in diesen durchaus traurigen Verein", "tokens": ["Denn", "in", "die\u00b7sen", "durc\u00b7haus", "trau\u00b7ri\u00b7gen", "Ver\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "k\u00e4men ja sogar Krethi und Plethi hinein.", "tokens": ["k\u00e4\u00b7men", "ja", "so\u00b7gar", "Kre\u00b7thi", "und", "Ple\u00b7thi", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "KON", "NE", "PTKVZ", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df nun nicht genau, wer Krethi und Plethi sind;", "tokens": ["Ich", "wei\u00df", "nun", "nicht", "ge\u00b7nau", ",", "wer", "Kre\u00b7thi", "und", "Ple\u00b7thi", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADJD", "$,", "PWS", "NE", "KON", "NE", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "vielleicht meint er damit meinen Vater oder dein Enkelkind.", "tokens": ["viel\u00b7leicht", "meint", "er", "da\u00b7mit", "mei\u00b7nen", "Va\u00b7ter", "o\u00b7der", "dein", "En\u00b7kel\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Aber das wei\u00df ich: die Schlacht bei Warschau und in den Argonnen,", "tokens": ["A\u00b7ber", "das", "wei\u00df", "ich", ":", "die", "Schlacht", "bei", "Warsc\u00b7hau", "und", "in", "den", "Ar\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$.", "ART", "NN", "APPR", "NE", "KON", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-++-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "die haben Deutschlands Krethi und Plethi gewonnen.", "tokens": ["die", "ha\u00b7ben", "Deutschlands", "Kre\u00b7thi", "und", "Ple\u00b7thi", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "NE", "KON", "NE", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Vielleicht hat Vater Liebert in Hannover gro\u00dfen Applaus.", "tokens": ["Viel\u00b7leicht", "hat", "Va\u00b7ter", "Lie\u00b7bert", "in", "Han\u00b7no\u00b7ver", "gro\u00b7\u00dfen", "Ap\u00b7plaus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NE", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihm h\u00e4ngt aber nicht nur der Reichstag zum Halse heraus.", "tokens": ["Ihm", "h\u00e4ngt", "a\u00b7ber", "nicht", "nur", "der", "Reichs\u00b7tag", "zum", "Hal\u00b7se", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}}, "stanza.6": {"line.1": {"text": "Da h\u00e4ngt auch ein hoher, preu\u00dfischer, bunter Orden.", "tokens": ["Da", "h\u00e4ngt", "auch", "ein", "ho\u00b7her", ",", "preu\u00b7\u00dfi\u00b7scher", ",", "bun\u00b7ter", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der ist ihm f\u00fcr viel Blut deutscher Krethis und Plethis verliehen worden.", "tokens": ["Der", "ist", "ihm", "f\u00fcr", "viel", "Blut", "deut\u00b7scher", "Kret\u00b7his", "und", "Plet\u00b7his", "ver\u00b7lie\u00b7hen", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "PIAT", "NN", "ADJA", "NN", "KON", "NN", "VVPP", "VAPP", "$."], "meter": "-+-+-+--+--+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "Und der eine Krethi ist Kr\u00fcppel, und der andere Plethi ist krank.", "tokens": ["Und", "der", "ei\u00b7ne", "Kre\u00b7thi", "ist", "Kr\u00fcp\u00b7pel", ",", "und", "der", "an\u00b7de\u00b7re", "Ple\u00b7thi", "ist", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VAFIN", "NN", "$,", "KON", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "--+--+-+-+-+--++-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Tausend blasse Lippen fl\u00fcstern: \u00bbDank, Herr General! Dank!\u00ab", "tokens": ["Tau\u00b7send", "blas\u00b7se", "Lip\u00b7pen", "fl\u00fcs\u00b7tern", ":", "\u00bb", "Dank", ",", "Herr", "Ge\u00b7ne\u00b7ral", "!", "Dank", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["CARD", "ADJA", "NN", "VVINF", "$.", "$(", "NN", "$,", "NN", "NN", "$.", "NN", "$.", "$("], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}}}}}