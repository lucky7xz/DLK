{"textgrid.poem.43580": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm, mein liebes Fr\u00e4nzchen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, mein liebes Fr\u00e4nzchen,", "tokens": ["Komm", ",", "mein", "lie\u00b7bes", "Fr\u00e4nz\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mach mit mir ein T\u00e4nzchen,", "tokens": ["Mach", "mit", "mir", "ein", "T\u00e4nz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tanze mit mir!", "tokens": ["Tan\u00b7ze", "mit", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Komm! Hinauf, hinunter,", "tokens": ["Komm", "!", "Hin\u00b7auf", ",", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "$,", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Immer frisch und munter", "tokens": ["Im\u00b7mer", "frisch", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Tanz' ich mit dir!", "tokens": ["Tanz'", "ich", "mit", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie gr\u00fcn ist das Feld,", "tokens": ["Wie", "gr\u00fcn", "ist", "das"