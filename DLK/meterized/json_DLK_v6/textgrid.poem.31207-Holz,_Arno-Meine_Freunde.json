{"textgrid.poem.31207": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Meine Freunde", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der eine irrt mit Pinsel und Pallette", "tokens": ["Der", "ei\u00b7ne", "irrt", "mit", "Pin\u00b7sel", "und", "Pal\u00b7let\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Maler jetzt in Minnesota rum,", "tokens": ["Als", "Ma\u00b7ler", "jetzt", "in", "Min\u00b7ne\u00b7so\u00b7ta", "rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist stets verliebt, schreibt englische Billette", "tokens": ["Ist", "stets", "ver\u00b7liebt", ",", "schreibt", "eng\u00b7li\u00b7sche", "Bil\u00b7let\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und pfeift als Motto lustig: Homo sum!", "tokens": ["Und", "pfeift", "als", "Mot\u00b7to", "lus\u00b7tig", ":", "Ho\u00b7mo", "sum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ADV", "ADJD", "$.", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der andre wieder weihte sein Genie", "tokens": ["Der", "and\u00b7re", "wie\u00b7der", "weih\u00b7te", "sein", "Ge\u00b7nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der urmodernen Eisenindustrie", "tokens": ["Der", "ur\u00b7mo\u00b7der\u00b7nen", "Ei\u00b7se\u00b7nin\u00b7dust\u00b7rie"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und harmonirt als rother Jakobiner", "tokens": ["Und", "har\u00b7mo\u00b7nirt", "als", "ro\u00b7ther", "Ja\u00b7ko\u00b7bi\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Aufs pr\u00e4chtigste mit seinem Mediziner!", "tokens": ["Aufs", "pr\u00e4ch\u00b7tigs\u00b7te", "mit", "sei\u00b7nem", "Me\u00b7di\u00b7zi\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das ist kein staubgen\u00e4hrter Dutzendgeist,", "tokens": ["Das", "ist", "kein", "staub\u00b7ge\u00b7n\u00e4hr\u00b7ter", "Dut\u00b7zend\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das ist ein Mensch, wie man ihn gerne leidet,", "tokens": ["Das", "ist", "ein", "Mensch", ",", "wie", "man", "ihn", "ger\u00b7ne", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und manchmal, wenn er trockne Witze reisst,", "tokens": ["Und", "manch\u00b7mal", ",", "wenn", "er", "trock\u00b7ne", "Wit\u00b7ze", "reisst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ein Kerl, um den man selber sich beneidet!", "tokens": ["Ein", "Kerl", ",", "um", "den", "man", "sel\u00b7ber", "sich", "be\u00b7nei\u00b7det", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "PIS", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch hat zum L\u00e4cheln bitterwenig Zeit", "tokens": ["Doch", "hat", "zum", "L\u00e4\u00b7cheln", "bit\u00b7ter\u00b7we\u00b7nig", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die eselsgraue Rechtsgelehrsamkeit,", "tokens": ["Die", "e\u00b7sels\u00b7grau\u00b7e", "Rechts\u00b7ge\u00b7lehr\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ochst Justinian und hilft mir Verse kl\u00fcgeln", "tokens": ["Ochst", "Jus\u00b7ti\u00b7ni\u00b7an", "und", "hilft", "mir", "Ver\u00b7se", "kl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "KON", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und wird wohl n\u00e4chstens ihren Lehrer pr\u00fcgeln.", "tokens": ["Und", "wird", "wohl", "n\u00e4chs\u00b7tens", "ih\u00b7ren", "Leh\u00b7rer", "pr\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Bescheidner schon ist jener Chemikus,", "tokens": ["Be\u00b7scheid\u00b7ner", "schon", "ist", "je\u00b7ner", "Che\u00b7mi\u00b7kus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der, schwarz bepinselt mit Retortenruss,", "tokens": ["Der", ",", "schwarz", "be\u00b7pin\u00b7selt", "mit", "Re\u00b7tor\u00b7ten\u00b7russ", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die \u00fcblen Folgen geistiger Di\u00e4t", "tokens": ["Die", "\u00fcb\u00b7len", "Fol\u00b7gen", "geis\u00b7ti\u00b7ger", "Di\u00b7\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Nur im historischen Roman entl\u00e4dt.", "tokens": ["Nur", "im", "his\u00b7to\u00b7ri\u00b7schen", "Ro\u00b7man", "ent\u00b7l\u00e4dt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Doch unbek\u00fcmmert um die ganze Blase,", "tokens": ["Doch", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "um", "die", "gan\u00b7ze", "Bla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "In einem Nichts die einzige Oase,", "tokens": ["In", "ei\u00b7nem", "Nichts", "die", "ein\u00b7zi\u00b7ge", "O\u00b7a\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Denkt still die Gottgelahrtheit nur: Pfui Deibel!", "tokens": ["Denkt", "still", "die", "Gott\u00b7ge\u00b7lahr\u00b7theit", "nur", ":", "Pfui", "Dei\u00b7bel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ADV", "$.", "NE", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und schreibt ein B\u00fcchlein \u00fcber \u2013 E. von Geibel!", "tokens": ["Und", "schreibt", "ein", "B\u00fcch\u00b7lein", "\u00fc\u00b7ber", "\u2013", "E.", "von", "Gei\u00b7bel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "abbreviation", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "$(", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}