{"textgrid.poem.25453": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast mich zwar", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mich zwar", "tokens": ["Du", "hast", "mich", "zwar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Seit einem Jahr'", "tokens": ["Seit", "ei\u00b7nem", "Jahr'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wohl zwanzigmal", "tokens": ["Wohl", "zwan\u00b7zig\u00b7mal"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Besuchen wollen;", "tokens": ["Be\u00b7su\u00b7chen", "wol\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Doch, Berg und Thal", "tokens": ["Doch", ",", "Berg", "und", "Thal"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "M\u00f6cht' eher sich", "tokens": ["M\u00f6cht'", "e\u00b7her", "sich"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "PRF"], "meter": "-+--", "measure": "dactylic.init"}, "line.7": {"text": "Zusammen rollen,", "tokens": ["Zu\u00b7sam\u00b7men", "rol\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Als du und ich,", "tokens": ["Als", "du", "und", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Uns, wie es scheint,", "tokens": ["Uns", ",", "wie", "es", "scheint", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Hier k\u00fcssen sollen.", "tokens": ["Hier", "k\u00fcs\u00b7sen", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Wir aber, Freund,", "tokens": ["Wir", "a\u00b7ber", ",", "Freund", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Trotz unsrer Liebe!", "tokens": ["Trotz", "uns\u00b7rer", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Sind nicht gemeint,", "tokens": ["Sind", "nicht", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Dem Aufgeschiebe", "tokens": ["Dem", "Auf\u00b7ge\u00b7schie\u00b7be"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "So nach zu sehn;", "tokens": ["So", "nach", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Versprechen halten,", "tokens": ["Ver\u00b7spre\u00b7chen", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Das steht bei Alten,", "tokens": ["Das", "steht", "bei", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Und jungen, sch\u00f6n,", "tokens": ["Und", "jun\u00b7gen", ",", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Entschuldigungen", "tokens": ["Ent\u00b7schul\u00b7di\u00b7gun\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Sind, wie du weist,", "tokens": ["Sind", ",", "wie", "du", "weist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Dir oft gelungen;", "tokens": ["Dir", "oft", "ge\u00b7lun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Doch nun zerrei\u00dft", "tokens": ["Doch", "nun", "zer\u00b7rei\u00dft"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Der Frau Geduld", "tokens": ["Der", "Frau", "Ge\u00b7duld"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Am Spinner\u00e4dchen,", "tokens": ["Am", "Spin\u00b7ne\u00b7r\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Das letzte F\u00e4dchen,", "tokens": ["Das", "letz\u00b7te", "F\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Ob deiner Schuld.", "tokens": ["Ob", "dei\u00b7ner", "Schuld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Zwar, wenn ich recht", "tokens": ["Zwar", ",", "wenn", "ich", "recht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Es \u00fcberlege:", "tokens": ["Es", "\u00fc\u00b7berl\u00b7e\u00b7ge", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Die Felsenwege", "tokens": ["Die", "Fel\u00b7sen\u00b7we\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Sind herzlich schlecht;", "tokens": ["Sind", "herz\u00b7lich", "schlecht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "Man wird im Wagen", "tokens": ["Man", "wird", "im", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "So braun und blau,", "tokens": ["So", "braun", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Wie manche Frau", "tokens": ["Wie", "man\u00b7che", "Frau"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.34": {"text": "Vom Mann', geschlagen.", "tokens": ["Vom", "Mann'", ",", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "F\u00fcrwahr! so was", "tokens": ["F\u00fcr\u00b7wahr", "!", "so", "was"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ADV", "PWS"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Verlangt der Ha\u00df", "tokens": ["Ver\u00b7langt", "der", "Ha\u00df"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Und nicht die Liebe;", "tokens": ["Und", "nicht", "die", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.38": {"text": "Ja! w\u00e4r' ich ", "tokens": ["Ja", "!", "w\u00e4r'", "ich"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.39": {"text": "Ich selber bliebe", "tokens": ["Ich", "sel\u00b7ber", "blie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.40": {"text": "Wohl h\u00fcbsch daheim.", "tokens": ["Wohl", "h\u00fcbsch", "da\u00b7heim", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.41": {"text": "Doch steht geschrieben:", "tokens": ["Doch", "steht", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.42": {"text": "Die b\u00f6se Sieben", "tokens": ["Die", "b\u00f6\u00b7se", "Sie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.43": {"text": "Hypochondrie,", "tokens": ["Hy\u00b7po\u00b7chond\u00b7rie", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.44": {"text": "Aus Kreuz und Magen", "tokens": ["Aus", "Kreuz", "und", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.45": {"text": "In einem Huy!", "tokens": ["In", "ei\u00b7nem", "Huy", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "Heraus zu jagen,", "tokens": ["He\u00b7raus", "zu", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.47": {"text": "Sey in der Welt,", "tokens": ["Sey", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.48": {"text": "Wem's sonst gef\u00e4llt,", "tokens": ["Wem's", "sonst", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.49": {"text": "Kein besser Mittel,", "tokens": ["Kein", "bes\u00b7ser", "Mit\u00b7tel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.50": {"text": "Als solch Gesch\u00fcttel.", "tokens": ["Als", "solch", "Ge\u00b7sch\u00fct\u00b7tel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.51": {"text": "Nun wollt' ich gern", "tokens": ["Nun", "wollt'", "ich", "gern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.52": {"text": "Den lieben Herrn", "tokens": ["Den", "lie\u00b7ben", "Herrn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.53": {"text": "Canonicus,", "tokens": ["Ca\u00b7no\u00b7ni\u00b7cus", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.54": {"text": "Der immer sitzen,", "tokens": ["Der", "im\u00b7mer", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.55": {"text": "Bei Acten schwitzen", "tokens": ["Bei", "Ac\u00b7ten", "schwit\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.56": {"text": "Und doctern mu\u00df,", "tokens": ["Und", "doc\u00b7tern", "mu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.57": {"text": "Recht frisch und munter", "tokens": ["Recht", "frisch", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.58": {"text": "Im Fr\u00fchling' sehn:", "tokens": ["Im", "Fr\u00fch\u00b7ling'", "sehn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.59": {"text": "Drum la\u00df es gehn", "tokens": ["Drum", "la\u00df", "es", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.60": {"text": "Berg auf, Berg unter,", "tokens": ["Berg", "auf", ",", "Berg", "un\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "APPR", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.61": {"text": "Dem Harz hinein!", "tokens": ["Dem", "Harz", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.62": {"text": "Denn die Gew\u00e4sser", "tokens": ["Denn", "die", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.63": {"text": "Sind wieder klein,", "tokens": ["Sind", "wie\u00b7der", "klein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.64": {"text": "Die Wege besser,", "tokens": ["Die", "We\u00b7ge", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.65": {"text": "Die Luft so rein!", "tokens": ["Die", "Luft", "so", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.66": {"text": "Der Nachtigallen", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.67": {"text": "Ges\u00e4nge schallen", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", "schal\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.68": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.69": {"text": "Und Blumen d\u00fcften", "tokens": ["Und", "Blu\u00b7men", "d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.70": {"text": "Auf jeder H\u00f6h',", "tokens": ["Auf", "je\u00b7der", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.71": {"text": "Die Herr ", "tokens": ["Die", "Herr"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.72": {"text": "In seinen Schriften,", "tokens": ["In", "sei\u00b7nen", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.73": {"text": "So viel er kennt,", "tokens": ["So", "viel", "er", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.74": {"text": "Nicht alle nennt;", "tokens": ["Nicht", "al\u00b7le", "nennt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.75": {"text": "Und Felsen schimmern", "tokens": ["Und", "Fel\u00b7sen", "schim\u00b7mern"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.76": {"text": "Hoch auf dem Harz", "tokens": ["Hoch", "auf", "dem", "Harz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.77": {"text": "Besonnt, und flimmern", "tokens": ["Be\u00b7sonnt", ",", "und", "flim\u00b7mern"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.78": {"text": "Von Spath und Quarz;", "tokens": ["Von", "Spath", "und", "Quarz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.79": {"text": "Und hohe Schl\u00f6sser,", "tokens": ["Und", "ho\u00b7he", "Schl\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.80": {"text": "Die manchen Fresser", "tokens": ["Die", "man\u00b7chen", "Fres\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.81": {"text": "Und Dieb gen\u00e4hrt,", "tokens": [