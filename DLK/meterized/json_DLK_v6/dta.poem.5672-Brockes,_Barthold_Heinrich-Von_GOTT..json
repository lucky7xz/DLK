{"dta.poem.5672": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ist aus solchem Nichts ein Etwas doch entstanden?", "tokens": ["Wie", "ist", "aus", "sol\u00b7chem", "Nichts", "ein", "Et\u00b7was", "doch", "ent\u00b7stan\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPR", "PIAT", "PIS", "ART", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Nichts ist niemals was hervorgegangen,", "tokens": ["Aus", "Nichts", "ist", "nie\u00b7mals", "was", "her\u00b7vor\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur ein Seyn hat das Seyn, und hat es nicht empfangen,", "tokens": ["Nur", "ein", "Seyn", "hat", "das", "Seyn", ",", "und", "hat", "es", "nicht", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "$,", "KON", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und blo\u00df von Jhm kommt alles. Er allein", "tokens": ["Und", "blo\u00df", "von", "Jhm", "kommt", "al\u00b7les", ".", "Er", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "VVFIN", "PIS", "$.", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist GOTT. K\u00f6nnt\u2019 Er wol seyn,", "tokens": ["Ist", "GoTT", ".", "K\u00f6nnt'", "Er", "wol", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$.", "VMFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wofern ", "tokens": ["Wo\u00b7fern"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Man m\u00fcst\u2019 ein ander Wesen ja,", "tokens": ["Man", "m\u00fcst'", "ein", "an\u00b7der", "We\u00b7sen", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So dann zu dem, der JHR gemacht\u2019, erlesen.", "tokens": ["So", "dann", "zu", "dem", ",", "der", "JhR", "ge\u00b7macht'", ",", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Selbst ins Unendliche mu\u00df unser Dencken gehn,", "tokens": ["Selbst", "ins", "Un\u00b7end\u00b7li\u00b7che", "mu\u00df", "un\u00b7ser", "Den\u00b7cken", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bis man begreiffet: GOTT, mu\u00df aus sich selbst entstehn:", "tokens": ["Bis", "man", "be\u00b7greif\u00b7fet", ":", "GoTT", ",", "mu\u00df", "aus", "sich", "selbst", "ent\u00b7stehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$.", "NE", "$,", "VMFIN", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Einfolglich mu\u00df ER unerschaffen, EJN,", "tokens": ["Ein\u00b7folg\u00b7lich", "mu\u00df", "Er", "un\u00b7er\u00b7schaf\u00b7fen", ",", "EjN", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ADJD", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und unmaterialisch seyn.", "tokens": ["Und", "un\u00b7ma\u00b7te\u00b7ri\u00b7a\u00b7lisch", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nur ER vereint und h\u00e4lt in Sich", "tokens": ["Nur", "Er", "ver\u00b7eint", "und", "h\u00e4lt", "in", "Sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVPP", "KON", "VVFIN", "APPR", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Creatur Zusammenhang,", "tokens": ["Der", "Crea\u00b7tur", "Zu\u00b7sam\u00b7men\u00b7hang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Der Gr\u00fcnde lieblichen Zusammenklang.", "tokens": ["Der", "Gr\u00fcn\u00b7de", "lieb\u00b7li\u00b7chen", "Zu\u00b7sam\u00b7men\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Nur ER allein, der nicht hervor gebracht,", "tokens": ["Nur", "Er", "al\u00b7lein", ",", "der", "nicht", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$,", "PRELS", "PTKNEG", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Hat alle Ding hervorbracht und gemacht:", "tokens": ["Hat", "al\u00b7le", "Ding", "her\u00b7vor\u00b7bracht", "und", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Ein Anfang ohn Beginn, stets, unver\u00e4nderlich.", "tokens": ["Ein", "An\u00b7fang", "ohn", "Be\u00b7ginn", ",", "stets", ",", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ADV", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}