{"dta.poem.12663": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er w\u00fcnschet zu heyrathen.  \n S. D.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll denn mein junges leben,", "tokens": ["Soll", "denn", "mein", "jun\u00b7ges", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da alles liebt und freyt,", "tokens": ["Da", "al\u00b7les", "liebt", "und", "freyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Alleine sich ergeben", "tokens": ["Al\u00b7lei\u00b7ne", "sich", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der langen einsamkeit?", "tokens": ["Der", "lan\u00b7gen", "ein\u00b7sam\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bleibt denn die freud und lust", "tokens": ["Bleibt", "denn", "die", "freud", "und", "lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der schleyer-weissen brust,", "tokens": ["Der", "schleyer\u00b7weis\u00b7sen", "brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Nach der wir alle streben,", "tokens": ["Nach", "der", "wir", "al\u00b7le", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir ewig unbewust?", "tokens": ["Mir", "e\u00b7wig", "un\u00b7be\u00b7wust", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die w\u00fcrme, die nur schleichen,", "tokens": ["Die", "w\u00fcr\u00b7me", ",", "die", "nur", "schlei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schnellen fisch im meer,", "tokens": ["Die", "schnel\u00b7len", "fisch", "im", "meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das wild in den gestr\u00e4uchen,", "tokens": ["Das", "wild", "in", "den", "ge\u00b7str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der vogel leichtes heer,", "tokens": ["Der", "vo\u00b7gel", "leich\u00b7tes", "heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und was sich in der welt", "tokens": ["Und", "was", "sich", "in", "der", "welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch lufft und fluth erh\u00e4lt,", "tokens": ["Durch", "lufft", "und", "fluth", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kriegt iedes seines gleichen,", "tokens": ["Kriegt", "ie\u00b7des", "sei\u00b7nes", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So bald es ihm gef\u00e4llt.", "tokens": ["So", "bald", "es", "ihm", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nur ich mu\u00df nicht geniessen", "tokens": ["Nur", "ich", "mu\u00df", "nicht", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Worauf di\u00df leben geht,", "tokens": ["Wo\u00b7rauf", "di\u00df", "le\u00b7ben", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PDS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das gl\u00fcck will mir verschliessen,", "tokens": ["Das", "gl\u00fcck", "will", "mir", "ver\u00b7schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was andern offen steht;", "tokens": ["Was", "an\u00b7dern", "of\u00b7fen", "steht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der fr\u00fchling meiner zier", "tokens": ["Der", "fr\u00fch\u00b7ling", "mei\u00b7ner", "zier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ist ferne schon von hier,", "tokens": ["Ist", "fer\u00b7ne", "schon", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gleich wie die b\u00e4che fl\u00fcssen,", "tokens": ["Gleich", "wie", "die", "b\u00e4\u00b7che", "fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So eilt mein herbst zu mir.", "tokens": ["So", "eilt", "mein", "herbst", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich aber mu\u00df noch bleiben", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "noch", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So, wie ich vormals war,", "tokens": ["So", ",", "wie", "ich", "vor\u00b7mals", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll nimmer mich beweiben,", "tokens": ["Soll", "nim\u00b7mer", "mich", "be\u00b7wei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit keiner seyn ein paar;", "tokens": ["Mit", "kei\u00b7ner", "seyn", "ein", "paar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "ART", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das s\u00fcsse wangen roth", "tokens": ["Das", "s\u00fcs\u00b7se", "wan\u00b7gen", "roth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Soll nimmer mir die noth", "tokens": ["Soll", "nim\u00b7mer", "mir", "die", "noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der einsamkeit vertreiben,", "tokens": ["Der", "ein\u00b7sam\u00b7keit", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Solch leben ist ein tod.", "tokens": ["Solch", "le\u00b7ben", "ist", "ein", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du k\u00f6nigin, Dione!", "tokens": ["Du", "k\u00f6\u00b7ni\u00b7gin", ",", "Dio\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von der es einig r\u00fchrt,", "tokens": ["Von", "der", "es", "ei\u00b7nig", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df meiner zeiten crone", "tokens": ["Da\u00df", "mei\u00b7ner", "zei\u00b7ten", "cro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir keine lust gebiehrt;", "tokens": ["Mir", "kei\u00b7ne", "lust", "ge\u00b7biehrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist di\u00df der liebe danck,", "tokens": ["Ist", "di\u00df", "der", "lie\u00b7be", "danck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die ich mein lebenlang", "tokens": ["Die", "ich", "mein", "le\u00b7ben\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von dir und deinem sohne", "tokens": ["Von", "dir", "und", "dei\u00b7nem", "soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In meine laute sang?", "tokens": ["In", "mei\u00b7ne", "lau\u00b7te", "sang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es hat mich nie gefangen,", "tokens": ["Es", "hat", "mich", "nie", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was mir verbothen ist,", "tokens": ["Was", "mir", "ver\u00b7bo\u00b7then", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bin nie dem nachgegangen,", "tokens": ["Bin", "nie", "dem", "nach\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was leib und seele b\u00fcst;", "tokens": ["Was", "leib", "und", "see\u00b7le", "b\u00fcst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will keine wilde brunst:", "tokens": ["Will", "kei\u00b7ne", "wil\u00b7de", "brunst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur eines menschen gunst", "tokens": ["Nur", "ei\u00b7nes", "men\u00b7schen", "gunst"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In ehren zu erlangen,", "tokens": ["In", "eh\u00b7ren", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Versuch ich alle kunst.", "tokens": ["Ver\u00b7such", "ich", "al\u00b7le", "kunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Soll ich mir denn erst rathen,", "tokens": ["Soll", "ich", "mir", "denn", "erst", "ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn schon mein winter schneyt,", "tokens": ["Wenn", "schon", "mein", "win\u00b7ter", "schneyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was thu ich dann vor thaten", "tokens": ["Was", "thu", "ich", "dann", "vor", "tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jm s\u00fc\u00dfen liebes-streit?", "tokens": ["Jm", "s\u00fc\u00b7\u00dfen", "lie\u00b7bes\u00b7streit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer jung ist, liebt den krieg,", "tokens": ["Wer", "jung", "ist", ",", "liebt", "den", "krieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein alter bleibt zur\u00fcck,", "tokens": ["Ein", "al\u00b7ter", "bleibt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn solcher art soldaten", "tokens": ["Denn", "sol\u00b7cher", "art", "sol\u00b7da\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erhalten schlechten sieg.", "tokens": ["Er\u00b7hal\u00b7ten", "schlech\u00b7ten", "sieg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nein, itzund will ich haben", "tokens": ["Nein", ",", "it\u00b7zund", "will", "ich", "ha\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VMFIN", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was auf mein leiden dient,", "tokens": ["Was", "auf", "mein", "lei\u00b7den", "dient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil noch die f\u00fc\u00dfe traben", "tokens": ["Weil", "noch", "die", "f\u00fc\u00b7\u00dfe", "tra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und noch mein alter gr\u00fcnt;", "tokens": ["Und", "noch", "mein", "al\u00b7ter", "gr\u00fcnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm, Venus! schleu\u00df mich ein,", "tokens": ["Komm", ",", "Ve\u00b7nus", "!", "schleu\u00df", "mich", "ein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der liebsten, die ich meyn,", "tokens": ["Der", "liebs\u00b7ten", ",", "die", "ich", "meyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich will von deinen gaben", "tokens": ["Ich", "will", "von", "dei\u00b7nen", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Recht satt und truncken seyn.", "tokens": ["Recht", "satt", "und", "trun\u00b7cken", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}