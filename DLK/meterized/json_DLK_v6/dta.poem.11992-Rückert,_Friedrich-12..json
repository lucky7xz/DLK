{"dta.poem.11992": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele h\u00e4tte nicht des Leibs bedurft, sie h\u00e4tte", "tokens": ["Die", "See\u00b7le", "h\u00e4t\u00b7te", "nicht", "des", "Leibs", "be\u00b7durft", ",", "sie", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ART", "NN", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zufrieden k\u00f6nnen seyn mit freier Aetherst\u00e4tte.", "tokens": ["Zu\u00b7frie\u00b7den", "k\u00f6n\u00b7nen", "seyn", "mit", "frei\u00b7er", "A\u00b7e\u00b7ther\u00b7st\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VAINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Allein der Seele hat bedurft der Leib zu leben;", "tokens": ["Al\u00b7lein", "der", "See\u00b7le", "hat", "be\u00b7durft", "der", "Leib", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ohne Seele konnt' er sich vom Staub erheben?", "tokens": ["Wie", "oh\u00b7ne", "See\u00b7le", "konnt'", "er", "sich", "vom", "Staub", "er\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weil nun der Leib, beseelt von einer Seel', ist sch\u00f6n,", "tokens": ["Weil", "nun", "der", "Leib", ",", "be\u00b7seelt", "von", "ei\u00b7ner", "Seel'", ",", "ist", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dank sei der Seele, die herabkam von den H\u00f6hn!", "tokens": ["Dank", "sei", "der", "See\u00b7le", ",", "die", "her\u00b7ab\u00b7kam", "von", "den", "H\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und m\u00f6g' ein Weilchen hier zu wohnen ihr gefallen,", "tokens": ["Und", "m\u00f6g'", "ein", "Weil\u00b7chen", "hier", "zu", "woh\u00b7nen", "ihr", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis lieber k\u00f6rperlos sie will im Aether wallen.", "tokens": ["Bis", "lie\u00b7ber", "k\u00f6r\u00b7per\u00b7los", "sie", "will", "im", "A\u00b7e\u00b7ther", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}