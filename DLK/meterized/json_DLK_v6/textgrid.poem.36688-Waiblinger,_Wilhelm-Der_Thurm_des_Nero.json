{"textgrid.poem.36688": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Thurm des Nero", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gerne, wenn der Abend aus Schattenth\u00e4lern", "tokens": ["Ger\u00b7ne", ",", "wenn", "der", "A\u00b7bend", "aus", "Schat\u00b7tent\u00b7h\u00e4\u00b7lern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Aus dem Felde steigt, das des Capitoles", "tokens": ["Aus", "dem", "Fel\u00b7de", "steigt", ",", "das", "des", "Ca\u00b7pi\u00b7to\u00b7les"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Majest\u00e4t und finster des Kaiserh\u00fcgels", "tokens": ["Ma\u00b7jes\u00b7t\u00e4t", "und", "fins\u00b7ter", "des", "Kai\u00b7ser\u00b7h\u00fc\u00b7gels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Tr\u00fcmmer umragen,", "tokens": ["Tr\u00fcm\u00b7mer", "um\u00b7ra\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Gerne dann im einsamen Hause sitz' ich", "tokens": ["Ger\u00b7ne", "dann", "im", "ein\u00b7sa\u00b7men", "Hau\u00b7se", "sitz'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "So das m\u00fcde Haupt mit dem Arme st\u00fctzend,", "tokens": ["So", "das", "m\u00fc\u00b7de", "Haupt", "mit", "dem", "Ar\u00b7me", "st\u00fct\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wie es oft die Trauernden thun, die Freunde", "tokens": ["Wie", "es", "oft", "die", "Trau\u00b7ern\u00b7den", "thun", ",", "die", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVINF", "$,", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ernster Gedanken.", "tokens": ["Erns\u00b7ter", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Und hin\u00fcber blick' ich, wo alter Sag' ein", "tokens": ["Und", "hin\u00b7\u00fc\u00b7ber", "blick'", "ich", ",", "wo", "al\u00b7ter", "Sag'", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ADJA", "NN", "ART"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schaurig Denkmal, mitten in grauer Kirchen", "tokens": ["Schau\u00b7rig", "Denk\u00b7mal", ",", "mit\u00b7ten", "in", "grau\u00b7er", "Kir\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Frommen Kreis der Thurm des Tyrannen noch zum", "tokens": ["From\u00b7men", "Kreis", "der", "Thurm", "des", "Ty\u00b7ran\u00b7nen", "noch", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "ART", "NN", "ADV", "APPRART"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Himmel emporstarrt;", "tokens": ["Him\u00b7mel", "em\u00b7por\u00b7starrt", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Schon entschwand die goldene Sonn', es dunkelt", "tokens": ["Schon", "ent\u00b7schwand", "die", "gol\u00b7de\u00b7ne", "Sonn'", ",", "es", "dun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Abendblau in d\u00fcsterem Duft um Berg und", "tokens": ["A\u00b7bend\u00b7blau", "in", "d\u00fcs\u00b7te\u00b7rem", "Duft", "um", "Berg", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "APPR", "NN", "KON"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Thurm und Kirch' und schwarzen Ruin, die Erde", "tokens": ["Thurm", "und", "Kirch'", "und", "schwar\u00b7zen", "Ru\u00b7in", ",", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dunkelt, die Nacht kommt.", "tokens": ["Dun\u00b7kelt", ",", "die", "Nacht", "kommt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Flammen aber r\u00f6then die L\u00fcfte noch, und \u00fcber'm", "tokens": ["Flam\u00b7men", "a\u00b7ber", "r\u00f6\u00b7then", "die", "L\u00fcf\u00b7te", "noch", ",", "und", "\u00fc\u00b7ber'm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "ADV", "$,", "KON", "APPRART"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Weiten graunerweckenden Bilde Roma's", "tokens": ["Wei\u00b7ten", "grau\u00b7ner\u00b7we\u00b7cken\u00b7den", "Bil\u00b7de", "Ro\u00b7ma's"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "NE"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gl\u00fcht in langen purpurnen Str\u00f6men noch ein", "tokens": ["Gl\u00fcht", "in", "lan\u00b7gen", "pur\u00b7pur\u00b7nen", "Str\u00f6\u00b7men", "noch", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "ADJA", "NN", "ADV", "ART"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Blendendes Feuer.", "tokens": ["Blen\u00b7den\u00b7des", "Feu\u00b7er", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Da nun mein' ich, hoch auf dem Schattenthurme", "tokens": ["Da", "nun", "mein'", "ich", ",", "hoch", "auf", "dem", "Schat\u00b7ten\u00b7thur\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sitze der Tyrann mit der Laut', und s\u00e4nge", "tokens": ["Sit\u00b7ze", "der", "Ty\u00b7rann", "mit", "der", "Laut'", ",", "und", "s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Troja's Schicksallied, und der Himmel spr\u00fche", "tokens": ["Tro\u00b7ja's", "Schick\u00b7sal\u00b7lied", ",", "und", "der", "Him\u00b7mel", "spr\u00fc\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nur, weil der Erde", "tokens": ["Nur", ",", "weil", "der", "Er\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "K\u00f6nigin entbrannt. Da erschallt der Abend-", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "ent\u00b7brannt", ".", "Da", "er\u00b7schallt", "der", "A\u00b7ben\u00b7d"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "TRUNC"], "meter": "+-+-+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Glocken tausendstimmig Gel\u00e4ut: als ob des", "tokens": ["Glo\u00b7cken", "tau\u00b7sends\u00b7tim\u00b7mig", "Ge\u00b7l\u00e4ut", ":", "als", "ob", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "CARD", "NN", "$.", "KOKOM", "KOUS", "ART"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Kaisers furchtbar Lied in die Flammen t\u00f6nte,", "tokens": ["Kai\u00b7sers", "furcht\u00b7bar", "Lied", "in", "die", "Flam\u00b7men", "t\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "D\u00fcnkt mir dann oftmals;", "tokens": ["D\u00fcnkt", "mir", "dann", "oft\u00b7mals", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Und allm\u00e4hlich schweigt es in Todtenstille,", "tokens": ["Und", "all\u00b7m\u00e4h\u00b7lich", "schweigt", "es", "in", "Tod\u00b7ten\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Selbst die Gluth des Himmels erbleicht, die Nebel", "tokens": ["Selbst", "die", "Gluth", "des", "Him\u00b7mels", "er\u00b7bleicht", ",", "die", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVPP", "$,", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Rauchen aus den Th\u00e4lern, die Nacht deckt Rom, es", "tokens": ["Rau\u00b7chen", "aus", "den", "Th\u00e4\u00b7lern", ",", "die", "Nacht", "deckt", "Rom", ",", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "NE", "$,", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Schlummert im Grabe.", "tokens": ["Schlum\u00b7mert", "im", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}