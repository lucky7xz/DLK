{"textgrid.poem.45805": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ihr hab ich Beethoven hoch geehrt,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ihr hab ich Beethoven hoch geehrt,", "tokens": ["Wie", "ihr", "hab", "ich", "Beet\u00b7ho\u00b7ven", "hoch", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "PPER", "NE", "ADJD", "VVPP", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wobei jedoch als Unterschied sich anh\u00e4ngt,", "tokens": ["Wo\u00b7bei", "je\u00b7doch", "als", "Un\u00b7ter\u00b7schied", "sich", "an\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df, wo eure Bewundrung erst recht anf\u00e4ngt,", "tokens": ["Da\u00df", ",", "wo", "eu\u00b7re", "Be\u00b7wund\u00b7rung", "erst", "recht", "an\u00b7f\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Die meinige schon wieder aufh\u00f6rt.", "tokens": ["Die", "mei\u00b7ni\u00b7ge", "schon", "wie\u00b7der", "auf\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}