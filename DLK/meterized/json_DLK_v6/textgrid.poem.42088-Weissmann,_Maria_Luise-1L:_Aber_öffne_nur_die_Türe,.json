{"textgrid.poem.42088": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aber \u00f6ffne nur die T\u00fcre,", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber \u00f6ffne nur die T\u00fcre,", "tokens": ["A\u00b7ber", "\u00f6ff\u00b7ne", "nur", "die", "T\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber tritt nur auf die Schwelle,", "tokens": ["A\u00b7ber", "tritt", "nur", "auf", "die", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebe kaum den Blick und sp\u00fcre", "tokens": ["He\u00b7be", "kaum", "den", "Blick", "und", "sp\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon die ungeheure Helle,", "tokens": ["Schon", "die", "un\u00b7ge\u00b7heu\u00b7re", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schon den Glanz der leeren R\u00e4ume,", "tokens": ["Schon", "den", "Glanz", "der", "lee\u00b7ren", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die wie Wiese rasch erbl\u00fchten,", "tokens": ["Die", "wie", "Wie\u00b7se", "rasch", "er\u00b7bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schon den Tanz der schweren Tr\u00e4ume,", "tokens": ["Schon", "den", "Tanz", "der", "schwe\u00b7ren", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die sich hoben, die ergl\u00fchten...", "tokens": ["Die", "sich", "ho\u00b7ben", ",", "die", "er\u00b7gl\u00fch\u00b7ten", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "$,", "PRELS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Z\u00e4rtliche beschwingte Welle,", "tokens": ["Z\u00e4rt\u00b7li\u00b7che", "be\u00b7schwing\u00b7te", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sieh, kein Lufthauch, der nicht r\u00fchre \u2013 \u2013", "tokens": ["Sieh", ",", "kein", "Luft\u00b7hauch", ",", "der", "nicht", "r\u00fch\u00b7re", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PIAT", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Aber tritt nur auf die Schwelle,", "tokens": ["A\u00b7ber", "tritt", "nur", "auf", "die", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Aber \u00f6ffne nur die T\u00fcre!", "tokens": ["A\u00b7ber", "\u00f6ff\u00b7ne", "nur", "die", "T\u00fc\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}