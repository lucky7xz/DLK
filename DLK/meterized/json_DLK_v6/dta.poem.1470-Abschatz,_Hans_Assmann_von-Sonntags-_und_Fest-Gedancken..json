{"dta.poem.1470": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonntags- und Fest-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist di\u00df f\u00fcr ein Gru\u00df? f\u00fcr Mutter zu erkennen/", "tokens": ["Was", "ist", "di\u00df", "f\u00fcr", "ein", "Gru\u00df", "?", "f\u00fcr", "Mut\u00b7ter", "zu", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$.", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die keinen Mann erkennt/ mich Bruder zu benennen/", "tokens": ["die", "kei\u00b7nen", "Mann", "er\u00b7kennt", "/", "mich", "Bru\u00b7der", "zu", "be\u00b7nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$(", "PPER", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was f\u00fcr ein Wunder-Schlu\u00df? der S\u00fcnder Heyl zu seyn/", "tokens": ["Was", "f\u00fcr", "ein", "Wun\u00b7der\u00b7Schlu\u00df", "?", "der", "S\u00fcn\u00b7der", "Heyl", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$.", "ART", "NN", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich GOtt von GOtt/ durch GOtt ins Fleisch zu kleiden", "tokens": ["Sich", "Gott", "von", "Gott", "/", "durch", "Gott", "ins", "Fleisch", "zu", "klei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "NN", "APPR", "NN", "$(", "APPR", "NN", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "ein?", "tokens": ["ein", "?"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Erblinde du Vernunfft/ und Glaube tritt herbey/", "tokens": ["Er\u00b7blin\u00b7de", "du", "Ver\u00b7nunfft", "/", "und", "Glau\u00b7be", "tritt", "her\u00b7bey", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$(", "KON", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zu sehn/ wie GOtt ein Mensch/ und Jungfrau Mutter sey!", "tokens": ["Zu", "sehn", "/", "wie", "Gott", "ein", "Mensch", "/", "und", "Jung\u00b7frau", "Mut\u00b7ter", "sey", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "KOKOM", "NN", "ART", "NN", "$(", "KON", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}