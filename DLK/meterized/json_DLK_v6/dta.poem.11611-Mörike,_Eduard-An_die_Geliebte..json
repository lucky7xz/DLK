{"dta.poem.11611": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An die Geliebte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich, von deinem Anschaun tief gestillt,", "tokens": ["Wenn", "ich", ",", "von", "dei\u00b7nem", "An\u00b7schaun", "tief", "ge\u00b7stillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich stumm an deinem heil'gen Werth vergn\u00fcge,", "tokens": ["Mich", "stumm", "an", "dei\u00b7nem", "heil'\u00b7gen", "Werth", "ver\u00b7gn\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann h\u00f6r' ich recht die leisen Athemz\u00fcge", "tokens": ["Dann", "h\u00f6r'", "ich", "recht", "die", "lei\u00b7sen", "A\u00b7them\u00b7z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Engels, welcher sich in dir verh\u00fcllt.", "tokens": ["Des", "En\u00b7gels", ",", "wel\u00b7cher", "sich", "in", "dir", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und ein erstaunt, ein fragend L\u00e4cheln quillt", "tokens": ["Und", "ein", "er\u00b7staunt", ",", "ein", "fra\u00b7gend", "L\u00e4\u00b7cheln", "quillt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "$,", "ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf meinem Mund, ob mich kein Traum betr\u00fcge,", "tokens": ["Auf", "mei\u00b7nem", "Mund", ",", "ob", "mich", "kein", "Traum", "be\u00b7tr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nun in dir, zu ewiger Gen\u00fcge,", "tokens": ["Da\u00df", "nun", "in", "dir", ",", "zu", "e\u00b7wi\u00b7ger", "Ge\u00b7n\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein k\u00fchnster Wunsch, mein einz'ger sich erf\u00fcllt?", "tokens": ["Mein", "k\u00fchns\u00b7ter", "Wunsch", ",", "mein", "einz'\u00b7ger", "sich", "er\u00b7f\u00fcllt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von Tiefe dann zu Tiefen st\u00fcrzt mein Sinn,", "tokens": ["Von", "Tie\u00b7fe", "dann", "zu", "Tie\u00b7fen", "st\u00fcrzt", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich h\u00f6re aus der Gottheit n\u00e4cht'ger Ferne", "tokens": ["Ich", "h\u00f6\u00b7re", "aus", "der", "Got\u00b7theit", "n\u00e4cht'\u00b7ger", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Quellen des Geschicks melodisch rauschen.", "tokens": ["Die", "Quel\u00b7len", "des", "Ge\u00b7schicks", "me\u00b7lo\u00b7disch", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bet\u00e4ubt kehr' ich den Blick nach Oben hin,", "tokens": ["Be\u00b7t\u00e4ubt", "kehr'", "ich", "den", "Blick", "nach", "O\u00b7ben", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum Himmel auf \u2014 da l\u00e4cheln alle Sterne;", "tokens": ["Zum", "Him\u00b7mel", "auf", "da", "l\u00e4\u00b7cheln", "al\u00b7le", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "$(", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich kniee, ihrem Lichtgesang zu lauschen.", "tokens": ["Ich", "kni\u00b7ee", ",", "ih\u00b7rem", "Licht\u00b7ge\u00b7sang", "zu", "lau\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}