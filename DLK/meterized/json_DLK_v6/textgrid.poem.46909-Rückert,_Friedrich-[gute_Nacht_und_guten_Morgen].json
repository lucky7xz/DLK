{"textgrid.poem.46909": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[gute Nacht und guten Morgen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gute Nacht und guten Morgen", "tokens": ["Gu\u00b7te", "Nacht", "und", "gu\u00b7ten", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich t\u00e4glich dir gebracht,", "tokens": ["Hab'", "ich", "t\u00e4g\u00b7lich", "dir", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du, heiter ohne Sorgen,", "tokens": ["Und", "du", ",", "hei\u00b7ter", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast dazu mich angelacht.", "tokens": ["Hast", "da\u00b7zu", "mich", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gute Nacht und guten Morgen,", "tokens": ["Gu\u00b7te", "Nacht", "und", "gu\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt dein L\u00e4cheln, hab' ich schon,", "tokens": ["Sagt", "dein", "L\u00e4\u00b7cheln", ",", "hab'", "ich", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dir selbst in Gram und Sorgen", "tokens": ["Und", "dir", "selbst", "in", "Gram", "und", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00f6nn' ich einen Theil davon.", "tokens": ["G\u00f6nn'", "ich", "ei\u00b7nen", "Theil", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, es ist mir unverborgen,", "tokens": ["Ja", ",", "es", "ist", "mir", "un\u00b7ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gute, wenn ich w\u00fcnsche dir", "tokens": ["Gu\u00b7te", ",", "wenn", "ich", "w\u00fcn\u00b7sche", "dir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gute Nacht und guten Morgen,", "tokens": ["Gu\u00b7te", "Nacht", "und", "gu\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir nicht w\u00fcnsch' ich's, sondern mir.", "tokens": ["Dir", "nicht", "w\u00fcn\u00b7sch'", "ich's", ",", "son\u00b7dern", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVFIN", "PIS", "$,", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}