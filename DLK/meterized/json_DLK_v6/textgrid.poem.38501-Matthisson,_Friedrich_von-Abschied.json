{"textgrid.poem.38501": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Quellenrauschendes Thal! in deinem Schatten,", "tokens": ["Quel\u00b7len\u00b7rau\u00b7schen\u00b7des", "Thal", "!", "in", "dei\u00b7nem", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wo sich schwesterlich Ruh' und Unschuld k\u00fcssen,", "tokens": ["Wo", "sich", "schwes\u00b7ter\u00b7lich", "Ruh'", "und", "Un\u00b7schuld", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJD", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Flohn die s\u00fcssesten meiner J\u00fcnglingsstunden,", "tokens": ["Flohn", "die", "s\u00fcs\u00b7ses\u00b7ten", "mei\u00b7ner", "J\u00fcng\u00b7lings\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mit serafischem L\u00e4cheln, mir vor\u00fcber!", "tokens": ["Mit", "se\u00b7ra\u00b7fi\u00b7schem", "L\u00e4\u00b7cheln", ",", "mir", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn, mit rosiger Hand, auf deine Wipfel", "tokens": ["Wenn", ",", "mit", "ro\u00b7si\u00b7ger", "Hand", ",", "auf", "dei\u00b7ne", "Wip\u00b7fel"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Seine Bl\u00fcten der junge Maimond streute,", "tokens": ["Sei\u00b7ne", "Bl\u00fc\u00b7ten", "der", "jun\u00b7ge", "Mai\u00b7mond", "streu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und die Nachtigall jeder Abendd\u00e4mrung", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gall", "je\u00b7der", "A\u00b7bend\u00b7d\u00e4m\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Ihren Zaubergesang entgegent\u00f6nte!", "tokens": ["Ih\u00b7ren", "Zau\u00b7ber\u00b7ge\u00b7sang", "ent\u00b7ge\u00b7gen\u00b7t\u00f6n\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Ach! ein tr\u00fcbes Geschik entrei\u00dft mich ewig", "tokens": ["Ach", "!", "ein", "tr\u00fc\u00b7bes", "Ge\u00b7schik", "ent\u00b7rei\u00dft", "mich", "e\u00b7wig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Deinen d\u00fcstergew\u00f6lbten Lindeng\u00e4ngen,", "tokens": ["Dei\u00b7nen", "d\u00fcs\u00b7ter\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Lin\u00b7den\u00b7g\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Wo dein l\u00e4chelnder Engelblik, o Maja!", "tokens": ["Wo", "dein", "l\u00e4\u00b7cheln\u00b7der", "En\u00b7gel\u00b7blik", ",", "o", "Ma\u00b7ja", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,", "FM", "FM", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Oft den Himmel mir in die Seele stralte,", "tokens": ["Oft", "den", "Him\u00b7mel", "mir", "in", "die", "See\u00b7le", "stral\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Und mich h\u00f6her zu meinem Gott entz\u00fckte!", "tokens": ["Und", "mich", "h\u00f6\u00b7her", "zu", "mei\u00b7nem", "Gott", "ent\u00b7z\u00fck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.14": {"text": "La\u00df, elisisches Thal, noch diese Thr\u00e4nen,", "tokens": ["La\u00df", ",", "e\u00b7li\u00b7si\u00b7sches", "Thal", ",", "noch", "die\u00b7se", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Meines wachsenden Kummers stille Zeugen,", "tokens": ["Mei\u00b7nes", "wach\u00b7sen\u00b7den", "Kum\u00b7mers", "stil\u00b7le", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Mich an deinem bebl\u00fcmten Busen weinen,", "tokens": ["Mich", "an", "dei\u00b7nem", "be\u00b7bl\u00fcm\u00b7ten", "Bu\u00b7sen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Eh', mit ehernem Arm, die Scheidestunde", "tokens": ["Eh'", ",", "mit", "e\u00b7her\u00b7nem", "Arm", ",", "die", "Schei\u00b7des\u00b7tun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Meinen z\u00f6gernden Fu\u00dftrit pl\u00f6zlich fl\u00fcgelt!", "tokens": ["Mei\u00b7nen", "z\u00f6\u00b7gern\u00b7den", "Fu\u00df\u00b7trit", "pl\u00f6z\u00b7lich", "fl\u00fc\u00b7gelt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}