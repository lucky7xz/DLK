{"dta.poem.17402": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "23.  Die zeit die ist nicht schnell.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt die Zeit ist schnell: wer hat sie sehen fliegen?", "tokens": ["Man", "sagt", "die", "Zeit", "ist", "schnell", ":", "wer", "hat", "sie", "se\u00b7hen", "flie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VAFIN", "ADJD", "$.", "PWS", "VAFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie bleibt ja unverruckt im Welt-begrieffe liegen!", "tokens": ["Sie", "bleibt", "ja", "un\u00b7ver\u00b7ruckt", "im", "Welt\u00b7be\u00b7grief\u00b7fe", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}