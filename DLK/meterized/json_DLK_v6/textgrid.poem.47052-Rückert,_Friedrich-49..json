{"textgrid.poem.47052": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, es ist keine Kunst, wenn Wald und Heiden", "tokens": ["Ach", ",", "es", "ist", "kei\u00b7ne", "Kunst", ",", "wenn", "Wald", "und", "Hei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Berg' und Str\u00f6me, die dazwischen rollen,", "tokens": ["Und", "Ber\u00b7g'", "und", "Str\u00f6\u00b7me", ",", "die", "da\u00b7zwi\u00b7schen", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "PRELS", "PAV", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Meeresfluten, die, im Sturm erschwollen,", "tokens": ["Und", "Mee\u00b7res\u00b7flu\u00b7ten", ",", "die", ",", "im", "Sturm", "er\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dazwischen brausen, dich von Liebe scheiden;", "tokens": ["Da\u00b7zwi\u00b7schen", "brau\u00b7sen", ",", "dich", "von", "Lie\u00b7be", "schei\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch eine Kunst ist's, eine Kunst zu leiden", "tokens": ["Doch", "ei\u00b7ne", "Kunst", "ist's", ",", "ei\u00b7ne", "Kunst", "zu", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist's, wenn von ihr nichts als dein eignes Wollen", "tokens": ["Ist's", ",", "wenn", "von", "ihr", "nichts", "als", "dein", "eig\u00b7nes", "Wol\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "APPR", "PPER", "PIS", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Dich scheidet, und die stillen W\u00fcnsche sollen", "tokens": ["Dich", "schei\u00b7det", ",", "und", "die", "stil\u00b7len", "W\u00fcn\u00b7sche", "sol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Scheidewand zu \u00fcberspringen meiden.", "tokens": ["Die", "Schei\u00b7de\u00b7wand", "zu", "\u00fc\u00b7bers\u00b7prin\u00b7gen", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja eine Kunst ist's, \u00fcber alle K\u00fcnste,", "tokens": ["Ja", "ei\u00b7ne", "Kunst", "ist's", ",", "\u00fc\u00b7ber", "al\u00b7le", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VAFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In also freigew\u00e4hlter Selbstverdammung,", "tokens": ["In", "al\u00b7so", "frei\u00b7ge\u00b7w\u00e4hl\u00b7ter", "Selbst\u00b7ver\u00b7dam\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So fern von ihr zu sein in solcher N\u00e4he,", "tokens": ["So", "fern", "von", "ihr", "zu", "sein", "in", "sol\u00b7cher", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKZU", "VAINF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In solcher N\u00e4he, da\u00df, wenn diese Br\u00fcnste", "tokens": ["In", "sol\u00b7cher", "N\u00e4\u00b7he", ",", "da\u00df", ",", "wenn", "die\u00b7se", "Br\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "$,", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Haus hier setzen k\u00f6nnten in Entflammung,", "tokens": ["Mein", "Haus", "hier", "set\u00b7zen", "k\u00f6nn\u00b7ten", "in", "Ent\u00b7flam\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ganz gut aus ihrem obern Stock sie's s\u00e4he.", "tokens": ["Ganz", "gut", "aus", "ih\u00b7rem", "o\u00b7bern", "Stock", "sie's", "s\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}