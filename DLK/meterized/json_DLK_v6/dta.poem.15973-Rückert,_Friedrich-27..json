{"dta.poem.15973": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich, der Gefangne, der mit seinen Ketten spielt,", "tokens": ["Ich", ",", "der", "Ge\u00b7fang\u00b7ne", ",", "der", "mit", "sei\u00b7nen", "Ket\u00b7ten", "spielt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der blinde Sch\u00fctze, der nach hohem Ziele zielt;", "tokens": ["Der", "blin\u00b7de", "Sch\u00fct\u00b7ze", ",", "der", "nach", "ho\u00b7hem", "Zie\u00b7le", "zielt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der, Geistern anverwandt, ans Thier gebundene,", "tokens": ["Der", ",", "Geis\u00b7tern", "an\u00b7ver\u00b7wandt", ",", "ans", "Thier", "ge\u00b7bun\u00b7de\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "VVPP", "$,", "APPRART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Sich selber suchend, stets sich selbst entschwundene;", "tokens": ["Sich", "sel\u00b7ber", "su\u00b7chend", ",", "stets", "sich", "selbst", "ent\u00b7schwun\u00b7de\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "$,", "VVFIN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Der nicht wei\u00df, was er ist, war oder werde seyn:", "tokens": ["Der", "nicht", "wei\u00df", ",", "was", "er", "ist", ",", "war", "o\u00b7der", "wer\u00b7de", "seyn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$,", "VAFIN", "KON", "VAFIN", "VAINF", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Was w\u00e4r' ich denn, wenn ich nichts w\u00e4r' als ich allein?", "tokens": ["Was", "w\u00e4r'", "ich", "denn", ",", "wenn", "ich", "nichts", "w\u00e4r'", "als", "ich", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PIS", "VAFIN", "KOUS", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich bin auch du, weil du das bist, was in mir ist;", "tokens": ["Ich", "bin", "auch", "du", ",", "weil", "du", "das", "bist", ",", "was", "in", "mir", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "$,", "KOUS", "PPER", "PDS", "VAFIN", "$,", "PRELS", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin mehr als ich bin, weil du mein Alles bist.", "tokens": ["Ich", "bin", "mehr", "als", "ich", "bin", ",", "weil", "du", "mein", "Al\u00b7les", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "PPER", "VAFIN", "$,", "KOUS", "PPER", "PPOSAT", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}