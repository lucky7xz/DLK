{"textgrid.poem.46989": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei Gott, ich mu\u00df mich zum Empfang", "tokens": ["Bei", "Gott", ",", "ich", "mu\u00df", "mich", "zum", "Emp\u00b7fang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PPER", "VMFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdes alten Helden schicken,", "tokens": ["\u00bb", "des", "al\u00b7ten", "Hel\u00b7den", "schi\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den ich verfolgt hab' oft und lang", "tokens": ["Den", "ich", "ver\u00b7folgt", "hab'", "oft", "und", "lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVPP", "VAFIN", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von hier mit meinen Blicken.", "tokens": ["Von", "hier", "mit", "mei\u00b7nen", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich hab' gesehn in mancher Schlacht", "tokens": ["Ich", "hab'", "ge\u00b7sehn", "in", "man\u00b7cher", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl seine Blitzesschnelle,", "tokens": ["Wohl", "sei\u00b7ne", "Blit\u00b7zes\u00b7schnel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jetzund, eh' ich es gedacht,", "tokens": ["Und", "je\u00b7tzund", ",", "eh'", "ich", "es", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist er auch hier zur Stelle.", "tokens": ["Ist", "er", "auch", "hier", "zur", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weit dr\u00fcben, dacht' ich, sei er noch,", "tokens": ["Weit", "dr\u00fc\u00b7ben", ",", "dacht'", "ich", ",", "sei", "er", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dazwischen weite Kl\u00fcfte,", "tokens": ["Da\u00b7zwi\u00b7schen", "wei\u00b7te", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er aber ist hin dr\u00fcber hoch", "tokens": ["Er", "a\u00b7ber", "ist", "hin", "dr\u00fc\u00b7ber", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "PAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gesprungen durch die L\u00fcfte.", "tokens": ["Ge\u00b7sprun\u00b7gen", "durch", "die", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Als ob im Dampf er vor sich hab'", "tokens": ["Als", "ob", "im", "Dampf", "er", "vor", "sich", "hab'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPRART", "NN", "PPER", "APPR", "PRF", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Graben einer Schanze,", "tokens": ["Den", "Gra\u00b7ben", "ei\u00b7ner", "Schan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist er gesprungen \u00fcbers Grab", "tokens": ["Ist", "er", "ge\u00b7sprun\u00b7gen", "\u00fc\u00b7bers", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist schon nah' im Glanze.\u00ab", "tokens": ["Und", "ist", "schon", "nah'", "im", "Glan\u00b7ze", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Im Himmel sprach's der alte Fritz", "tokens": ["Im", "Him\u00b7mel", "sprach's", "der", "al\u00b7te", "Fritz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hob des Bl\u00fcchers wegen", "tokens": ["Und", "hob", "des", "Bl\u00fc\u00b7chers", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich von dem hohen Heldensitz", "tokens": ["Sich", "von", "dem", "ho\u00b7hen", "Hel\u00b7den\u00b7sitz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gieng ihm stracks entgegen.", "tokens": ["Und", "gieng", "ihm", "stracks", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Bl\u00fccher kam ihm doch zuvor,", "tokens": ["Der", "Bl\u00fc\u00b7cher", "kam", "ihm", "doch", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eintrat er gleich dem Blitze,", "tokens": ["Ein\u00b7trat", "er", "gleich", "dem", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und senkte, schreitend durch das Thor,", "tokens": ["Und", "senk\u00b7te", ",", "schrei\u00b7tend", "durch", "das", "Thor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ihm des Degens Spitze.", "tokens": ["Vor", "ihm", "des", "De\u00b7gens", "Spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Vorbei schritt er dem alten Fritz", "tokens": ["Vor\u00b7bei", "schritt", "er", "dem", "al\u00b7ten", "Fritz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trat, ohn' umzuschauen,", "tokens": ["Und", "trat", ",", "ohn'", "um\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUI", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hin, wo er sah auf ihrem Sitz", "tokens": ["Hin", ",", "wo", "er", "sah", "auf", "ih\u00b7rem", "Sitz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die K\u00f6nigin der Frauen.", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "der", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da bracht' er seinen ersten Gru\u00df", "tokens": ["Da", "bracht'", "er", "sei\u00b7nen", "ers\u00b7ten", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der preu\u00dfischen Luise,", "tokens": ["Der", "preu\u00b7\u00dfi\u00b7schen", "Lu\u00b7i\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und beugte vor ihr seinen Fu\u00df,", "tokens": ["Und", "beug\u00b7te", "vor", "ihr", "sei\u00b7nen", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er ihr Ehr' erwiese.", "tokens": ["Da\u00df", "er", "ihr", "Ehr'", "er\u00b7wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Worauf er den Bericht ihr gab", "tokens": ["Wo\u00b7rauf", "er", "den", "Be\u00b7richt", "ihr", "gab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gr\u00fc\u00dfen, die ihr Gatte,", "tokens": ["Von", "Gr\u00fc\u00b7\u00dfen", ",", "die", "ihr", "Gat\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein K\u00f6nig, f\u00fcr sie \u00fcbers Grab", "tokens": ["Sein", "K\u00f6\u00b7nig", ",", "f\u00fcr", "sie", "\u00fc\u00b7bers", "Grab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm anbefohlen hatte.", "tokens": ["Ihm", "an\u00b7be\u00b7foh\u00b7len", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie dankt' ihm mit Holdseligkeit;", "tokens": ["Sie", "dankt'", "ihm", "mit", "Hold\u00b7se\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und so, nach abgethanen", "tokens": ["Und", "so", ",", "nach", "ab\u00b7ge\u00b7tha\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesch\u00e4ften, trat er dienstbereit", "tokens": ["Ge\u00b7sch\u00e4f\u00b7ten", ",", "trat", "er", "dienst\u00b7be\u00b7reit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seines K\u00f6nigs Ahnen.", "tokens": ["Zu", "sei\u00b7nes", "K\u00f6\u00b7nigs", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}