{"dta.poem.3968": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Brief aus fremder Hand scheint dir vielleicht", "tokens": ["Ein", "Brief", "aus", "frem\u00b7der", "Hand", "scheint", "dir", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zu k\u00fchne,", "tokens": ["zu", "k\u00fch\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wahr ists, ich habe viel, und allzu viel gethan.", "tokens": ["Wahr", "ists", ",", "ich", "ha\u00b7be", "viel", ",", "und", "all\u00b7zu", "viel", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "PPER", "VAFIN", "ADV", "$,", "KON", "PTKA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch, da ich dir, o Herr! in tiefster Ehrfurcht diene,", "tokens": ["Doch", ",", "da", "ich", "dir", ",", "o", "Herr", "!", "in", "tiefs\u00b7ter", "Ehr\u00b7furcht", "die\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPER", "$,", "FM", "NN", "$.", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So nimm di\u00df fromme Blat zu einem Opffer an.", "tokens": ["So", "nimm", "di\u00df", "from\u00b7me", "Blat", "zu", "ei\u00b7nem", "Opf\u00b7fer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PDS", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch, soltest du vielleicht der Reue Frucht verachten,", "tokens": ["Doch", ",", "sol\u00b7test", "du", "viel\u00b7leicht", "der", "Reu\u00b7e", "Frucht", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So m\u00fcste ich gewi\u00df vor Furcht und Angst ver-", "tokens": ["So", "m\u00fcs\u00b7te", "ich", "ge\u00b7wi\u00df", "vor", "Furcht", "und", "Angst", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "schmachten.", "tokens": ["schmach\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}