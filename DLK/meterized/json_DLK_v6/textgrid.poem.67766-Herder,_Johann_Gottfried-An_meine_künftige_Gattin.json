{"textgrid.poem.67766": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An meine k\u00fcnftige Gattin", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Du, die Zeus im sch\u00f6nsten Morgenbilde", "tokens": ["O", "Du", ",", "die", "Zeus", "im", "sch\u00f6ns\u00b7ten", "Mor\u00b7gen\u00b7bil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Jugendgattin mir beschied,", "tokens": ["Zur", "Ju\u00b7gend\u00b7gat\u00b7tin", "mir", "be\u00b7schied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei, wo Du immer seist, Dir sing' ich; l\u00e4chle milde,", "tokens": ["Sei", ",", "wo", "Du", "im\u00b7mer", "seist", ",", "Dir", "sing'", "ich", ";", "l\u00e4ch\u00b7le", "mil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erkenne Dich und k\u00fcsse hold dies Lied!", "tokens": ["Er\u00b7ken\u00b7ne", "Dich", "und", "k\u00fcs\u00b7se", "hold", "dies", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ADJD", "PDS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du Sch\u00f6nheitsbild, das aus der ersten Bl\u00fcthe", "tokens": ["Du", "Sch\u00f6n\u00b7heits\u00b7bild", ",", "das", "aus", "der", "ers\u00b7ten", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Zeugenslust zur Lust entsprang,", "tokens": ["Aus", "Zeu\u00b7gens\u00b7lust", "zur", "Lust", "ent\u00b7sprang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du, die als Grazie im Aug' der Mutter gl\u00fchte", "tokens": ["Du", ",", "die", "als", "Gra\u00b7zie", "im", "Aug'", "der", "Mut\u00b7ter", "gl\u00fch\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "KOUS", "NN", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und an ihr Herz mit frohem Wallen drang!", "tokens": ["Und", "an", "ihr", "Herz", "mit", "fro\u00b7hem", "Wal\u00b7len", "drang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du, die vom Himmel her die sch\u00f6nste Seele", "tokens": ["Du", ",", "die", "vom", "Him\u00b7mel", "her", "die", "sch\u00f6ns\u00b7te", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "APZR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Heirathskrone mit bekam,", "tokens": ["Zur", "Hei\u00b7raths\u00b7kro\u00b7ne", "mit", "be\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bekam zur Bildnerin, die in der Herzensh\u00f6hle", "tokens": ["Be\u00b7kam", "zur", "Bild\u00b7ne\u00b7rin", ",", "die", "in", "der", "Her\u00b7zens\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das sch\u00f6nste Blut der Mutter um sich nahm!", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Blut", "der", "Mut\u00b7ter", "um", "sich", "nahm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als Bildnerin, die aus den Othemz\u00fcgen", "tokens": ["Als", "Bild\u00b7ne\u00b7rin", ",", "die", "aus", "den", "O\u00b7them\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mutter sanften Stoff geno\u00df", "tokens": ["Der", "Mut\u00b7ter", "sanf\u00b7ten", "Stoff", "ge\u00b7no\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, freie Sch\u00f6pfrin, sich zur Ehre voll Vergn\u00fcgen", "tokens": ["Und", ",", "frei\u00b7e", "Sch\u00f6pf\u00b7rin", ",", "sich", "zur", "Eh\u00b7re", "voll", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "PRF", "APPRART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den sch\u00f6nsten Leib als H\u00fclle um sich schlo\u00df,", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Leib", "als", "H\u00fcl\u00b7le", "um", "sich", "schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Den Leib, sch\u00f6n wie sie selbst, voll freier Gr\u00f6\u00dfe,", "tokens": ["Den", "Leib", ",", "sch\u00f6n", "wie", "sie", "selbst", ",", "voll", "frei\u00b7er", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "PPER", "ADV", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ein Gedank empfindungszart,", "tokens": ["Wie", "ein", "Ge\u00b7dank", "emp\u00b7fin\u00b7dungs\u00b7zart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dessen Blick und Stirn sie sich in holder Bl\u00f6\u00dfe", "tokens": ["In", "des\u00b7sen", "Blick", "und", "Stirn", "sie", "sich", "in", "hol\u00b7der", "Bl\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "KON", "NN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als K\u00f6nigin der Minnen offenbart.", "tokens": ["Als", "K\u00f6\u00b7ni\u00b7gin", "der", "Min\u00b7nen", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wo wohnst Du, G\u00f6ttin, da\u00df durch edle Thaten", "tokens": ["Wo", "wohnst", "Du", ",", "G\u00f6t\u00b7tin", ",", "da\u00df", "durch", "ed\u00b7le", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der J\u00fcngling um Dich gl\u00fch', um Dich", "tokens": ["Der", "J\u00fcng\u00b7ling", "um", "Dich", "gl\u00fch'", ",", "um", "Dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$,", "KOUI", "PPER"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als seine Lorbeerkrone eifre? \u2013", "tokens": ["Als", "sei\u00b7ne", "Lor\u00b7beer\u00b7kro\u00b7ne", "eif\u00b7re", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}