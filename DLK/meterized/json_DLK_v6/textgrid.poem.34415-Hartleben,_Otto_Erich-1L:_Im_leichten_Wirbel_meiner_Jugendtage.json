{"textgrid.poem.34415": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im leichten Wirbel meiner Jugendtage", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im leichten Wirbel meiner Jugendtage", "tokens": ["Im", "leich\u00b7ten", "Wir\u00b7bel", "mei\u00b7ner", "Ju\u00b7gend\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schweigt, was ich Schweres still im Busen trage:", "tokens": ["schweigt", ",", "was", "ich", "Schwe\u00b7res", "still", "im", "Bu\u00b7sen", "tra\u00b7ge", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "die unverwehte, leise Totenklage.", "tokens": ["die", "un\u00b7ver\u00b7weh\u00b7te", ",", "lei\u00b7se", "To\u00b7ten\u00b7kla\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Auge weinte, meine Lippen flehten:", "tokens": ["Mein", "Au\u00b7ge", "wein\u00b7te", ",", "mei\u00b7ne", "Lip\u00b7pen", "fleh\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "im Traume bist du vor mich hingetreten,", "tokens": ["im", "Trau\u00b7me", "bist", "du", "vor", "mich", "hin\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und um dein Leben hab ich Gott gebeten.", "tokens": ["und", "um", "dein", "Le\u00b7ben", "hab", "ich", "Gott", "ge\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}