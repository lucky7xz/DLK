{"dta.poem.2354": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Wje kan der Freundschafft Pflicht die Seelen doch ver-", "tokens": ["Wie", "kan", "der", "Freund\u00b7schafft", "Pflicht", "die", "See\u00b7len", "doch", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ART", "NN", "NN", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "binden!", "tokens": ["bin\u00b7den", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ich such? offt hie und da/ und weis nicht was ich thu;", "tokens": ["Ich", "such", "?", "offt", "hie", "und", "da", "/", "und", "weis", "nicht", "was", "ich", "thu", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADV", "KON", "ADV", "$(", "KON", "PTKVZ", "PTKNEG", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Ob ich dich irgend kan in deinen W\u00e4ldern finden/", "tokens": ["Ob", "ich", "dich", "ir\u00b7gend", "kan", "in", "dei\u00b7nen", "W\u00e4l\u00b7dern", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Seh\u2019 ich die Reinigkeit und deiner Schrifften Ziehr/", "tokens": ["Seh'", "ich", "die", "Rei\u00b7nig\u00b7keit", "und", "dei\u00b7ner", "Schriff\u00b7ten", "Ziehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So seh\u2019 ich als mir d\u00fcnkt/ dein reines Hertz bey mir.", "tokens": ["So", "seh'", "ich", "als", "mir", "d\u00fcnkt", "/", "dein", "rei\u00b7nes", "Hertz", "bey", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PPER", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}