{"textgrid.poem.34794": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Emma, sage mir die Wahrheit:", "tokens": ["Em\u00b7ma", ",", "sa\u00b7ge", "mir", "die", "Wahr\u00b7heit", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward ich n\u00e4rrisch durch die Liebe?", "tokens": ["Ward", "ich", "n\u00e4r\u00b7risch", "durch", "die", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder ist die Liebe selber", "tokens": ["O\u00b7der", "ist", "die", "Lie\u00b7be", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die Folge meiner Narrheit?", "tokens": ["Nur", "die", "Fol\u00b7ge", "mei\u00b7ner", "Nar\u00b7rheit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ach! mich qu\u00e4let, teure Emma,", "tokens": ["Ach", "!", "mich", "qu\u00e4\u00b7let", ",", "teu\u00b7re", "Em\u00b7ma", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$,", "ADJA", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Au\u00dfer meiner tollen Liebe,", "tokens": ["Au\u00b7\u00dfer", "mei\u00b7ner", "tol\u00b7len", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Au\u00dfer meiner Liebestollheit,", "tokens": ["Au\u00b7\u00dfer", "mei\u00b7ner", "Lie\u00b7be\u00b7stoll\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Obendrein noch dies Dilemma.", "tokens": ["O\u00b7ben\u00b7drein", "noch", "dies", "Di\u00b7lem\u00b7ma", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}