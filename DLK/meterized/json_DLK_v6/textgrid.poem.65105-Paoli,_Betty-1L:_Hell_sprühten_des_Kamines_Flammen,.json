{"textgrid.poem.65105": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hell spr\u00fchten des Kamines Flammen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hell spr\u00fchten des Kamines Flammen,", "tokens": ["Hell", "spr\u00fch\u00b7ten", "des", "Ka\u00b7mi\u00b7nes", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Frisch war die Lampe angefacht,", "tokens": ["Frisch", "war", "die", "Lam\u00b7pe", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Kreise sa\u00dfen wir beisammen", "tokens": ["Im", "Krei\u00b7se", "sa\u00b7\u00dfen", "wir", "bei\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einer stillen Winternacht.", "tokens": ["In", "ei\u00b7ner", "stil\u00b7len", "Win\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein heit'rer Austausch von Gedanken", "tokens": ["Ein", "heit'\u00b7rer", "Aus\u00b7tausch", "von", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Hingaukelnd zwischen Ernst und Scherz,", "tokens": ["Hin\u00b7gau\u00b7kelnd", "zwi\u00b7schen", "Ernst", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Flocht seine duft'gen Bl\u00fcthenranken", "tokens": ["Flocht", "sei\u00b7ne", "duft'\u00b7gen", "Bl\u00fc\u00b7then\u00b7ran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Um jeden Sinn, um jedes Herz.", "tokens": ["Um", "je\u00b7den", "Sinn", ",", "um", "je\u00b7des", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "$,", "KOUI", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir sprachen von der Menschheit Loosen,", "tokens": ["Wir", "spra\u00b7chen", "von", "der", "Menschheit", "Loo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wann sie den froh'sten Sieg errang,", "tokens": ["Wann", "sie", "den", "froh'\u00b7sten", "Sieg", "er\u00b7rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und welche Zeit die sch\u00f6nsten Rosen", "tokens": ["Und", "wel\u00b7che", "Zeit", "die", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr kr\u00f6nend um die Stirne schlang;", "tokens": ["Ihr", "kr\u00f6\u00b7nend", "um", "die", "Stir\u00b7ne", "schlang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann von der Gottheit Angesichte", "tokens": ["Wann", "von", "der", "Got\u00b7theit", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Strahlen hellster auf sie flo\u00df,", "tokens": ["Der", "Strah\u00b7len", "hells\u00b7ter", "auf", "sie", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wann sie am Quell vom Gl\u00fcck und Lichte", "tokens": ["Wann", "sie", "am", "Quell", "vom", "Gl\u00fcck", "und", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den reinsten Labetrunk geno\u00df.", "tokens": ["Den", "reins\u00b7ten", "La\u00b7be\u00b7trunk", "ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und Einer rief: \u00bbM\u00f6gt ihr noch fragen?", "tokens": ["Und", "Ei\u00b7ner", "rief", ":", "\u00bb", "M\u00f6gt", "ihr", "noch", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "$(", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seid ihr der Antwort nicht gewi\u00df?", "tokens": ["Seid", "ihr", "der", "Ant\u00b7wort", "nicht", "ge\u00b7wi\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es war in jenen heitern Tagen,", "tokens": ["Es", "war", "in", "je\u00b7nen", "hei\u00b7tern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Perikles die seinen hie\u00df.", "tokens": ["Die", "Pe\u00b7rik\u00b7les", "die", "sei\u00b7nen", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPOSAT", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "O Hellas! Land der Heldens\u00f6hne!", "tokens": ["O", "Hel\u00b7las", "!", "Land", "der", "Hel\u00b7den\u00b7s\u00f6h\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Kunst geliebtes Vaterland!", "tokens": ["Der", "Kunst", "ge\u00b7lieb\u00b7tes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du hieltst des Lebens Kraft und Sch\u00f6ne", "tokens": ["Du", "hieltst", "des", "Le\u00b7bens", "Kraft", "und", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In deinen Zauberkreis gebannt.", "tokens": ["In", "dei\u00b7nen", "Zau\u00b7ber\u00b7kreis", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da war das Sein noch nicht gespalten", "tokens": ["Da", "war", "das", "Sein", "noch", "nicht", "ge\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Geist und K\u00f6rper nicht im Streit,", "tokens": ["Und", "Geist", "und", "K\u00f6r\u00b7per", "nicht", "im", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Stoff war von des Geistes Walten", "tokens": ["Der", "Stoff", "war", "von", "des", "Geis\u00b7tes", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beseelt, veredelt und geweiht!", "tokens": ["Be\u00b7seelt", ",", "ver\u00b7e\u00b7delt", "und", "ge\u00b7weiht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den wir umsonst zu l\u00f6sen streben,", "tokens": ["Den", "wir", "um\u00b7sonst", "zu", "l\u00f6\u00b7sen", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Zwiespalt zwischen hier und dort,", "tokens": ["Der", "Zwies\u00b7palt", "zwi\u00b7schen", "hier", "und", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er blieb dir fremd und all dein Leben", "tokens": ["Er", "blieb", "dir", "fremd", "und", "all", "dein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein voller, seliger Accord!\u00ab \u2013", "tokens": ["Ein", "vol\u00b7ler", ",", "se\u00b7li\u00b7ger", "Ac\u00b7cord", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Zweiter sprach: \u00bbNicht jenem Alter", "tokens": ["Ein", "Zwei\u00b7ter", "sprach", ":", "\u00bb", "Nicht", "je\u00b7nem", "Al\u00b7ter"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PTKNEG", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Menschheit klagt mein Sehnen nach!", "tokens": ["Der", "Menschheit", "klagt", "mein", "Seh\u00b7nen", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Dem sp\u00e4tern, wo sie gleich dem Falter,", "tokens": ["Dem", "sp\u00e4\u00b7tern", ",", "wo", "sie", "gleich", "dem", "Fal\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch ihrer Tr\u00e4ume H\u00fcllen brach,", "tokens": ["Durch", "ih\u00b7rer", "Tr\u00e4u\u00b7me", "H\u00fcl\u00b7len", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo vor des Kindes armer Krippe", "tokens": ["Wo", "vor", "des", "Kin\u00b7des", "ar\u00b7mer", "Krip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie betend auf die Kniee sank,", "tokens": ["Sie", "be\u00b7tend", "auf", "die", "Kni\u00b7ee", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und von des Menschgeword'nen Lippe", "tokens": ["Und", "von", "des", "Menschge\u00b7word'\u00b7nen", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Das Heil und die Erl\u00f6sung trank.", "tokens": ["Das", "Heil", "und", "die", "Er\u00b7l\u00f6\u00b7sung", "trank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo eine fremde Macht, der Glaube,", "tokens": ["Wo", "ei\u00b7ne", "frem\u00b7de", "Macht", ",", "der", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beseligend ihr Herz durchdrang,", "tokens": ["Be\u00b7se\u00b7li\u00b7gend", "ihr", "Herz", "durch\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sich dem niedern Erdenstaube", "tokens": ["Wo", "sich", "dem", "nie\u00b7dern", "Er\u00b7dens\u00b7tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein heil'ger Bl\u00fcthenflor entrang,", "tokens": ["Ein", "heil'\u00b7ger", "Bl\u00fc\u00b7then\u00b7flor", "ent\u00b7rang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo von dem Strahl der Geistersonne", "tokens": ["Wo", "von", "dem", "Strahl", "der", "Geis\u00b7ter\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Heraufgef\u00fchrt der Weihetag,", "tokens": ["Her\u00b7auf\u00b7ge\u00b7f\u00fchrt", "der", "Wei\u00b7he\u00b7tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo in dem Schmerz die h\u00f6chste Wonne", "tokens": ["Wo", "in", "dem", "Schmerz", "die", "h\u00f6chs\u00b7te", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Hoffnung in dem Tode lag!", "tokens": ["Und", "Hoff\u00b7nung", "in", "dem", "To\u00b7de", "lag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr allzufr\u00fch entschwund'nen Zeiten,", "tokens": ["Ihr", "all\u00b7zu\u00b7fr\u00fch", "ent\u00b7schwun\u00b7d'\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie sehnt nach euer'm Friedensgl\u00fcck", "tokens": ["Wie", "sehnt", "nach", "eu\u00b7er'm", "Frie\u00b7dens\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Geist, ersch\u00f6pft vom Kampf und Streiten", "tokens": ["Mein", "Geist", ",", "er\u00b7sch\u00f6pft", "vom", "Kampf", "und", "Strei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich bang und wehmuthsvoll zur\u00fcck!", "tokens": ["Sich", "bang", "und", "weh\u00b7muths\u00b7voll", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da gr\u00fcnte schattenreich der Glaube", "tokens": ["Da", "gr\u00fcn\u00b7te", "schat\u00b7ten\u00b7reich", "der", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Eiche, die gebietend steht \u2013", "tokens": ["Als", "Ei\u00b7che", ",", "die", "ge\u00b7bie\u00b7tend", "steht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch unser Denken gleicht dem Laube,", "tokens": ["Doch", "un\u00b7ser", "Den\u00b7ken", "gleicht", "dem", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das jeder Windeshauch verweht.\u00ab \u2013", "tokens": ["Das", "je\u00b7der", "Win\u00b7des\u00b7hauch", "ver\u00b7weht", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein Dritter: \u00bbWardst du noch nicht inne,", "tokens": ["Ein", "Drit\u00b7ter", ":", "\u00bb", "Wardst", "du", "noch", "nicht", "in\u00b7ne", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "VAFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Glaube ein erborgter Strahl?", "tokens": ["Da\u00df", "Glau\u00b7be", "ein", "er\u00b7borg\u00b7ter", "Strahl", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr heil'ger gilt in meinem Sinne", "tokens": ["F\u00fcr", "heil'\u00b7ger", "gilt", "in", "mei\u00b7nem", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das selbsterrung'ne Ideal!", "tokens": ["Das", "selbs\u00b7ter\u00b7rung'\u00b7ne", "I\u00b7deal", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum preis' ich jener Zeit Zerw\u00fcrfni\u00df,", "tokens": ["Drum", "preis'", "ich", "je\u00b7ner", "Zeit", "Zer\u00b7w\u00fcrf\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo sich die Menschheit unverzagt", "tokens": ["Wo", "sich", "die", "Menschheit", "un\u00b7ver\u00b7zagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "ADJD"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Ihr tiefstes, flammendstes Bed\u00fcrfni\u00df,", "tokens": ["Ihr", "tiefs\u00b7tes", ",", "flam\u00b7mends\u00b7tes", "Be\u00b7d\u00fcrf\u00b7ni\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr schmerzlichst Missen abgefragt.", "tokens": ["Ihr", "schmerz\u00b7lichst", "Mis\u00b7sen", "ab\u00b7ge\u00b7fragt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wo sie in stillen Mittern\u00e4chten", "tokens": ["Wo", "sie", "in", "stil\u00b7len", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Quell selbsteignen Forschens drang,", "tokens": ["Zum", "Quell", "selbs\u00b7teig\u00b7nen", "For\u00b7schens", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo k\u00fchn sie mit des Zweifels M\u00e4chten,", "tokens": ["Wo", "k\u00fchn", "sie", "mit", "des", "Zwei\u00b7fels", "M\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"},