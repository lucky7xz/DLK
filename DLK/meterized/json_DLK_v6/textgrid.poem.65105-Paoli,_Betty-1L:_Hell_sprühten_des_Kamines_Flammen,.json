": ["ART", "PPER", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit ihrem Hauch so tief durchdrungen,", "tokens": ["Mit", "ih\u00b7rem", "Hauch", "so", "tief", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie sie der meine tief durchdringt.", "tokens": ["Wie", "sie", "der", "mei\u00b7ne", "tief", "durch\u00b7dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ich bin ihr Kind und nicht ihr Richter!", "tokens": ["Ich", "bin", "ihr", "Kind", "und", "nicht", "ihr", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meinen Adern wallt ihr Blut,", "tokens": ["In", "mei\u00b7nen", "A\u00b7dern", "wallt", "ihr", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entbrannt sind meines Geistes Lichter", "tokens": ["Ent\u00b7brannt", "sind", "mei\u00b7nes", "Geis\u00b7tes", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Widerscheine ihrer Gluth,", "tokens": ["Am", "Wi\u00b7der\u00b7schei\u00b7ne", "ih\u00b7rer", "Gluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie ist an jedem Keim betheiligt,", "tokens": ["Sie", "ist", "an", "je\u00b7dem", "Keim", "be\u00b7thei\u00b7ligt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der sich in meiner Brust erschlo\u00df,", "tokens": ["Der", "sich", "in", "mei\u00b7ner", "Brust", "er\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als Mutter ist sie mir geheiligt,", "tokens": ["Als", "Mut\u00b7ter", "ist", "sie", "mir", "ge\u00b7hei\u00b7ligt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ihr Geschick, es ist mein Loos!", "tokens": ["Und", "ihr", "Ge\u00b7schick", ",", "es", "ist", "mein", "Loos", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "O mater dolorosa! Thr\u00e4nen", "tokens": ["O", "ma\u00b7ter", "do\u00b7lo\u00b7ro\u00b7sa", "!", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "FM", "FM", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verdunkeln deiner Augen Licht,", "tokens": ["Ver\u00b7dun\u00b7keln", "dei\u00b7ner", "Au\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deinem tiefsten frommsten Sehnen", "tokens": ["Und", "dei\u00b7nem", "tiefs\u00b7ten", "fromms\u00b7ten", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ward ihm die Erf\u00fcllung nicht,", "tokens": ["Noch", "ward", "ihm", "die", "Er\u00b7f\u00fcl\u00b7lung", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein geheimni\u00dfvolle Zeichen", "tokens": ["Al\u00b7lein", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7le", "Zei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gewahr' ich rings, entz\u00fcckt und bang,", "tokens": ["Ge\u00b7wahr'", "ich", "rings", ",", "ent\u00b7z\u00fcckt", "und", "bang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Ahnung will mich froh beschleichen,", "tokens": ["Und", "Ah\u00b7nung", "will", "mich", "froh", "be\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df keinem Schoo\u00df das Heil entsprang! \u2013", "tokens": ["Da\u00df", "kei\u00b7nem", "Schoo\u00df", "das", "Heil", "ent\u00b7sprang", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "So nimm mich hin als dir zu eigen,", "tokens": ["So", "nimm", "mich", "hin", "als", "dir", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "KOUS", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meines Wesens tiefstem Sinn!", "tokens": ["Mit", "mei\u00b7nes", "We\u00b7sens", "tiefs\u00b7tem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df mich in Wort und Thaten zeigen,", "tokens": ["La\u00df", "mich", "in", "Wort", "und", "Tha\u00b7ten", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich dein Kind, dein treues bin!", "tokens": ["Da\u00df", "ich", "dein", "Kind", ",", "dein", "treu\u00b7es", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mag ich gleich dem Laub zerstieben,", "tokens": ["Und", "mag", "ich", "gleich", "dem", "Laub", "zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das jeder Windeshauch verstreut,", "tokens": ["Das", "je\u00b7der", "Win\u00b7des\u00b7hauch", "ver\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Stamm, der zeugend mich getrieben,", "tokens": ["Der", "Stamm", ",", "der", "zeu\u00b7gend", "mich", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er wurzelt in der Ewigkeit! \u2013", "tokens": ["Er", "wur\u00b7zelt", "in", "der", "E\u00b7wig\u00b7keit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}