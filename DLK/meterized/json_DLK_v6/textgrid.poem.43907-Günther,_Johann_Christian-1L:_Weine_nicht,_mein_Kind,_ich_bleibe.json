{"textgrid.poem.43907": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weine nicht, mein Kind, ich bleibe", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weine nicht, mein Kind, ich bleibe", "tokens": ["Wei\u00b7ne", "nicht", ",", "mein", "Kind", ",", "ich", "blei\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir bis in den Tod getreu.", "tokens": ["Dir", "bis", "in", "den", "Tod", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Glaube, was ich denck und schreibe,", "tokens": ["Glau\u00b7be", ",", "was", "ich", "denck", "und", "schrei\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist und heist stets einerley,", "tokens": ["Ist", "und", "heist", "stets", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil die Redligkeit zum Lieben", "tokens": ["Weil", "die", "Red\u00b7lig\u00b7keit", "zum", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir Geseze vorgeschrieben.", "tokens": ["Mir", "Ge\u00b7se\u00b7ze", "vor\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott und Himmel k\u00f6nnen zeugen,", "tokens": ["Gott", "und", "Him\u00b7mel", "k\u00f6n\u00b7nen", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich dir best\u00e4ndig bin;", "tokens": ["Da\u00df", "ich", "dir", "be\u00b7st\u00e4n\u00b7dig", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eher wird die Warheit schweigen", "tokens": ["E\u00b7her", "wird", "die", "War\u00b7heit", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als mein falsch und leichter Sinn", "tokens": ["Als", "mein", "falsch", "und", "leich\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJD", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das gekn\u00fcpfte Band zerrei\u00dfen", "tokens": ["Das", "ge\u00b7kn\u00fcpf\u00b7te", "Band", "zer\u00b7rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und des Meineids schuldig hei\u00dfen.", "tokens": ["Und", "des", "Mein\u00b7eids", "schul\u00b7dig", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Las die Wetter unterde\u00dfen", "tokens": ["Las", "die", "Wet\u00b7ter", "un\u00b7ter\u00b7de\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber unsrer Unschuld stehn.", "tokens": ["\u00dc\u00b7ber", "uns\u00b7rer", "Un\u00b7schuld", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mustu bittre Mandeln e\u00dfen", "tokens": ["Mus\u00b7tu", "bitt\u00b7re", "Man\u00b7deln", "e\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vorjezt auf Dornen gehn,", "tokens": ["Und", "vor\u00b7jezt", "auf", "Dor\u00b7nen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "So bedencke, das Vergn\u00fcgen", "tokens": ["So", "be\u00b7den\u00b7cke", ",", "das", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird uns einst zusammen f\u00fcgen.", "tokens": ["Wird", "uns", "einst", "zu\u00b7sam\u00b7men", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Warthe mit Gedult der Freude", "tokens": ["Wart\u00b7he", "mit", "Ge\u00b7dult", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der ungemeinen Lust,", "tokens": ["Und", "der", "un\u00b7ge\u00b7mei\u00b7nen", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche du mit diesem Leide", "tokens": ["Wel\u00b7che", "du", "mit", "die\u00b7sem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir zuvor verdienen must!", "tokens": ["Dir", "zu\u00b7vor", "ver\u00b7die\u00b7nen", "must", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Endlich werden deine Thr\u00e4nen", "tokens": ["End\u00b7lich", "wer\u00b7den", "dei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir den Weg zur Wollust b\u00e4hnen.", "tokens": ["Dir", "den", "Weg", "zur", "Wol\u00b7lust", "b\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jezo geb ich deinem Ku\u00dfe", "tokens": ["Je\u00b7zo", "geb", "ich", "dei\u00b7nem", "Ku\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine kurze gute Nacht", "tokens": ["Ei\u00b7ne", "kur\u00b7ze", "gu\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gehorche diesem Schlu\u00dfe,", "tokens": ["Und", "ge\u00b7hor\u00b7che", "die\u00b7sem", "Schlu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welchen das Verh\u00e4ngn\u00fc\u00df macht;", "tokens": ["Wel\u00b7chen", "das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch ich will in wenig Tagen", "tokens": ["Doch", "ich", "will", "in", "we\u00b7nig", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir die Ankunft wieder sagen.", "tokens": ["Dir", "die", "An\u00b7kunft", "wie\u00b7der", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lebe wohl! Die Zunge stammlet,", "tokens": ["Le\u00b7be", "wohl", "!", "Die", "Zun\u00b7ge", "stamm\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Augen na\u00dfes Heer,", "tokens": ["Und", "der", "Au\u00b7gen", "na\u00b7\u00dfes", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das die Wehmuth schon versammlet,", "tokens": ["Das", "die", "Weh\u00b7muth", "schon", "ver\u00b7samm\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht so Hand als Feder schwer", "tokens": ["Macht", "so", "Hand", "als", "Fe\u00b7der", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "NN", "KOUS", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und verbiethet meinem Willen,", "tokens": ["Und", "ver\u00b7bie\u00b7thet", "mei\u00b7nem", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diesen Bogen anzuf\u00fcllen.", "tokens": ["Die\u00b7sen", "Bo\u00b7gen", "an\u00b7zu\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}