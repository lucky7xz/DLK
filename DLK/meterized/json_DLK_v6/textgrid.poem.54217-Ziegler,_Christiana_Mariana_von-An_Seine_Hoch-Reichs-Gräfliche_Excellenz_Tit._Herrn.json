{"textgrid.poem.54217": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "An Seine Hoch-Reichs-Gr\u00e4fliche Excellenz Tit. Herrn", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag, ", "tokens": ["Der", "Tag", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "An dem Du ehemahls das Licht der Welt erblickt,", "tokens": ["An", "dem", "Du", "e\u00b7he\u00b7mahls", "das", "Licht", "der", "Welt", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sieht am Horizont viel tausend Sternen brennen,", "tokens": ["Man", "sieht", "am", "Ho\u00b7ri\u00b7zont", "viel", "tau\u00b7send", "Ster\u00b7nen", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "ADV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch Dein Geburths-Gestirn macht unsre Stadt entz\u00fcckt.", "tokens": ["Doch", "Dein", "Ge\u00b7burths\u00b7Ge\u00b7stirn", "macht", "uns\u00b7re", "Stadt", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Musen machen sich bereit zum sing- und dichten,", "tokens": ["Die", "Mu\u00b7sen", "ma\u00b7chen", "sich", "be\u00b7reit", "zum", "sing", "und", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPRART", "TRUNC", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dieweil man wohl mit Recht Dich ihren Schutz-Gott hei\u00dft,", "tokens": ["Die\u00b7weil", "man", "wohl", "mit", "Recht", "Dich", "ih\u00b7ren", "Schutz\u00b7Gott", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es kan ihr emb\u00dfger Kiel nichts herrlichers verrichten,", "tokens": ["Es", "kan", "ihr", "emb\u00df\u00b7ger", "Kiel", "nichts", "herr\u00b7li\u00b7chers", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als da\u00df er heute Dir geb\u00fcckt sein Opfer wei\u00dft.", "tokens": ["Als", "da\u00df", "er", "heu\u00b7te", "Dir", "ge\u00b7b\u00fcckt", "sein", "Op\u00b7fer", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht wundre Dich, ", "tokens": ["Nicht", "wund\u00b7re", "Dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Erlesne Schaar den Saum von Deinen Purpur k\u00fcst,", "tokens": ["Er\u00b7les\u00b7ne", "Schaar", "den", "Saum", "von", "Dei\u00b7nen", "Pur\u00b7pur", "k\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn ob sie gleich darbey mit Ehrfurcht sich besinnen,", "tokens": ["Denn", "ob", "sie", "gleich", "dar\u00b7bey", "mit", "Ehr\u00b7furcht", "sich", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PAV", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df Du ein Krieges-Gott nach Famens Ausspruch bist,", "tokens": ["Da\u00df", "Du", "ein", "Krie\u00b7ge\u00b7sGott", "nach", "Fa\u00b7mens", "Aus\u00b7spruch", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So kan Dein Anblick doch sie nicht so leicht erschrecken,", "tokens": ["So", "kan", "Dein", "An\u00b7blick", "doch", "sie", "nicht", "so", "leicht", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "KON", "PPER", "PTKNEG", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Pallas, welche sie besch\u00fctzt und auch regiert,", "tokens": ["Die", "Pal\u00b7las", ",", "wel\u00b7che", "sie", "be\u00b7sch\u00fctzt", "und", "auch", "re\u00b7giert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Pflegt sich mit einen Helm und Pantzer zu bedecken,", "tokens": ["Pflegt", "sich", "mit", "ei\u00b7nen", "Helm", "und", "Pant\u00b7zer", "zu", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So offt sie diese Schaar auf Ph\u00f6bens H\u00e4yne f\u00fchrt.", "tokens": ["So", "offt", "sie", "die\u00b7se", "Schaar", "auf", "Ph\u00f6\u00b7bens", "H\u00e4y\u00b7ne", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PDAT", "NN", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Man sieht auf dem Casquet von Dir die Feder blitzen,", "tokens": ["Man", "sieht", "auf", "dem", "Cas\u00b7quet", "von", "Dir", "die", "Fe\u00b7der", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch f\u00fchrst Du selbge mit zugleich auch in der Hand,", "tokens": ["Doch", "f\u00fchrst", "Du", "selb\u00b7ge", "mit", "zu\u00b7gleich", "auch", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "APPR", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein Arm wei\u00df Krieges-Volck und Musen zu besch\u00fctzen,", "tokens": ["Dein", "Arm", "wei\u00df", "Krie\u00b7ge\u00b7sVolck", "und", "Mu\u00b7sen", "zu", "be\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Denn beydes ist der Welt mehr als zu wohl bekannt;", "tokens": ["Denn", "bey\u00b7des", "ist", "der", "Welt", "mehr", "als", "zu", "wohl", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "PIAT", "KOKOM", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Di\u00df aber m\u00f6chte Dich vielleicht in Wunder setzen,", "tokens": ["Di\u00df", "a\u00b7ber", "m\u00f6ch\u00b7te", "Dich", "viel\u00b7leicht", "in", "Wun\u00b7der", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df meine schwache Hand den Ohnmachts-vollen Kiel,", "tokens": ["Da\u00df", "mei\u00b7ne", "schwa\u00b7che", "Hand", "den", "Ohn\u00b7machts\u00b7vol\u00b7len", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Am Tag, den Sachsen-Land vor k\u00fcssens-werth mu\u00df sch\u00e4tzen,", "tokens": ["Am", "Tag", ",", "den", "Sach\u00b7sen\u00b7Land", "vor", "k\u00fcs\u00b7sens\u00b7werth", "mu\u00df", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "APPR", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dir aus besondern Trieb zur Seiten legen will.", "tokens": ["Dir", "aus", "be\u00b7son\u00b7dern", "Trieb", "zur", "Sei\u00b7ten", "le\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was d\u00fcnckt Dich, ", "tokens": ["Was", "d\u00fcnckt", "Dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "Wagt sich, es ist zu frey, an Deine Tafel hin,", "tokens": ["Wagt", "sich", ",", "es", "ist", "zu", "frey", ",", "an", "Dei\u00b7ne", "Ta\u00b7fel", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Allwo Dir M\u00e4nner nur gebeugt ihr Opfer bringen,", "tokens": ["All\u00b7wo", "Dir", "M\u00e4n\u00b7ner", "nur", "ge\u00b7beugt", "ihr", "Op\u00b7fer", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Zu deren Orden ich nicht mit zu zehlen bin.", "tokens": ["Zu", "de\u00b7ren", "Or\u00b7den", "ich", "nicht", "mit", "zu", "zeh\u00b7len", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "PTKNEG", "APPR", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch mein Beginnen ist nicht strafbar zu benennen,", "tokens": ["Doch", "mein", "Be\u00b7gin\u00b7nen", "ist", "nicht", "straf\u00b7bar", "zu", "be\u00b7nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Schau nur, ", "tokens": ["Schau", "nur", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Und sag, ob wir uns nicht darzu mit stellen k\u00f6nnen?", "tokens": ["Und", "sag", ",", "ob", "wir", "uns", "nicht", "dar\u00b7zu", "mit", "stel\u00b7len", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "PAV", "APPR", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sie seynd mit gleicher Tracht, wie wir, auch angethan.", "tokens": ["Sie", "seynd", "mit", "glei\u00b7cher", "Tracht", ",", "wie", "wir", ",", "auch", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,", "PWAV", "PPER", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Kan meine Muse gleich Dir keinen Weyrauch zeigen,", "tokens": ["Kan", "mei\u00b7ne", "Mu\u00b7se", "gleich", "Dir", "kei\u00b7nen", "Wey\u00b7rauch", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den man bey jauchzenden und jubel-vollen Schreyn,", "tokens": ["Den", "man", "bey", "jauch\u00b7zen\u00b7den", "und", "ju\u00b7bel\u00b7vol\u00b7len", "Schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Sieht bi\u00df an Baldachin erhabner Seelen steigen,", "tokens": ["Sieht", "bi\u00df", "an", "Bal\u00b7da\u00b7ch\u00b7in", "er\u00b7hab\u00b7ner", "See\u00b7len", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "So la\u00df den Willen doch der That hier \u00e4hnlich seyn.", "tokens": ["So", "la\u00df", "den", "Wil\u00b7len", "doch", "der", "That", "hier", "\u00e4hn\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "ADV", "ART", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es m\u00f6gen andre Dir, die nettre Fl\u00f6ten f\u00fchren,", "tokens": ["Es", "m\u00f6\u00b7gen", "and\u00b7re", "Dir", ",", "die", "nett\u00b7re", "Fl\u00f6\u00b7ten", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PPER", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mit einen Helden-Lied erfreut entgegen gehn,", "tokens": ["Mit", "ei\u00b7nen", "Hel\u00b7den\u00b7Lied", "er\u00b7freut", "ent\u00b7ge\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich scheue mich vor Dir mein Saitenspiel zu r\u00fchren,", "tokens": ["Ich", "scheu\u00b7e", "mich", "vor", "Dir", "mein", "Sai\u00b7ten\u00b7spiel", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Drum soll ein schlechter Wunsch hier statt des Opfers stehn:", "tokens": ["Drum", "soll", "ein", "schlech\u00b7ter", "Wunsch", "hier", "statt", "des", "Op\u00b7fers", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Himmel m\u00fcsse Dich auf Adlers Fl\u00fcgeln tragen,", "tokens": ["Der", "Him\u00b7mel", "m\u00fcs\u00b7se", "Dich", "auf", "Ad\u00b7lers", "Fl\u00fc\u00b7geln", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Damit Dein edler Fu\u00df nichts zu bef\u00fcrchten hat,", "tokens": ["Da\u00b7mit", "Dein", "ed\u00b7ler", "Fu\u00df", "nichts", "zu", "be\u00b7f\u00fcrch\u00b7ten", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "PIS", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Er \u00fcberstreue Dir bey froh