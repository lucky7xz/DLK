{"textgrid.poem.51275": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Kind der Welt, erkenne deine Plagen,", "tokens": ["Du", "Kind", "der", "Welt", ",", "er\u00b7ken\u00b7ne", "dei\u00b7ne", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sinnen, die dich sch\u00fctzen und verjagen:", "tokens": ["Die", "Sin\u00b7nen", ",", "die", "dich", "sch\u00fct\u00b7zen", "und", "ver\u00b7ja\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn was du suchst Lust, Ehre Gut und Geld,", "tokens": ["Denn", "was", "du", "suchst", "Lust", ",", "Eh\u00b7re", "Gut", "und", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "NN", "$,", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dis ist ein Trost, der selbst sich qu\u00e4lt und f\u00e4llt,", "tokens": ["Dis", "ist", "ein", "Trost", ",", "der", "selbst", "sich", "qu\u00e4lt", "und", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du Kind der Welt.", "tokens": ["Du", "Kind", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ein Rauch und Traum ist sonst dein Wolbehagen,", "tokens": ["Ein", "Rauch", "und", "Traum", "ist", "sonst", "dein", "Wol\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der eitle Wind pflegt es stracks zu verjagen,", "tokens": ["Der", "eit\u00b7le", "Wind", "pflegt", "es", "stracks", "zu", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und dich darzu: So schlecht ist es bestellt,", "tokens": ["Und", "dich", "dar\u00b7zu", ":", "So", "schlecht", "ist", "es", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "$.", "ADV", "ADJD", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Drumb eile fort, die Tugend sey dein Zelt,", "tokens": ["Drumb", "ei\u00b7le", "fort", ",", "die", "Tu\u00b7gend", "sey", "dein", "Zelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du Kind der Welt.", "tokens": ["Du", "Kind", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Drumb auff und komm der Tugend nachzujagen,", "tokens": ["Drumb", "auff", "und", "komm", "der", "Tu\u00b7gend", "nach\u00b7zu\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "KON", "VVFIN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Tugend ist, wiltu recht nach ihr fragen,", "tokens": ["Die", "Tu\u00b7gend", "ist", ",", "wil\u00b7tu", "recht", "nach", "ihr", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein fester Thurm, den keine Macht zerschellt,", "tokens": ["Ein", "fes\u00b7ter", "Thurm", ",", "den", "kei\u00b7ne", "Macht", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Kriegs Mann, welcher siegt und allzeit Platz beh\u00e4lt.", "tokens": ["Ein", "Kriegs", "Mann", ",", "wel\u00b7cher", "siegt", "und", "all\u00b7zeit", "Platz", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADJD", "KON", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du Kind der Welt.", "tokens": ["Du", "Kind", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}