{"textgrid.poem.54640": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "54. Uber die Tauffe", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Flecken deiner Schuld/ die Licht entbrandte Glutt", "tokens": ["Die", "Fle\u00b7cken", "dei\u00b7ner", "Schuld", "/", "die", "Licht", "ent\u00b7brand\u00b7te", "Glutt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der H\u00f6llen Gottes Zorn/ des heissen Eyvers Flutt", "tokens": ["Der", "H\u00f6l\u00b7len", "Got\u00b7tes", "Zorn", "/", "des", "heis\u00b7sen", "Ey\u00b7vers", "Flutt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "NN", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lescht dises Wasser aus/ ob denn die Flamme schwebet/", "tokens": ["Lescht", "di\u00b7ses", "Was\u00b7ser", "aus", "/", "ob", "denn", "die", "Flam\u00b7me", "schwe\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "PTKVZ", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Flamme reiner Brunst die ewig w\u00e4chst und lebet.", "tokens": ["Die", "Flam\u00b7me", "rei\u00b7ner", "Brunst", "die", "e\u00b7wig", "w\u00e4chst", "und", "le\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "mea