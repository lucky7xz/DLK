{"textgrid.poem.27657": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Alten konten fr\u00f6lich singen", "tokens": ["Die", "Al\u00b7ten", "kon\u00b7ten", "fr\u00f6\u00b7lich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von tapffern, deutschen Heldens-Dingen,", "tokens": ["Von", "tapf\u00b7fern", ",", "deut\u00b7schen", "Hel\u00b7den\u00b7sDin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die ihre V\u00e4ter au\u00dfge\u00fcbet.", "tokens": ["Die", "ih\u00b7re", "V\u00e4\u00b7ter", "au\u00df\u00b7ge\u00b7\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Gott noch uns ie Kinder gibet,", "tokens": ["Wo", "Gott", "noch", "uns", "ie", "Kin\u00b7der", "gi\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die werden unsrer Zeit Beginnen", "tokens": ["Die", "wer\u00b7den", "uns\u00b7rer", "Zeit", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Beheulen, nicht besingen k\u00f6nnen.", "tokens": ["Be\u00b7heu\u00b7len", ",", "nicht", "be\u00b7sin\u00b7gen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}