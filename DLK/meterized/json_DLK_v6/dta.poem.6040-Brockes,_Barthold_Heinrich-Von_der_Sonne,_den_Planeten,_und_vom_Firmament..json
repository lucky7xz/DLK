{"dta.poem.6040": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem die Erde sich nun t\u00e4glich dreht, so wendet", "tokens": ["In\u00b7dem", "die", "Er\u00b7de", "sich", "nun", "t\u00e4g\u00b7lich", "dreht", ",", "so", "wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "ADJD", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sich auch in der Zeit, worinn das Jahr sich endet.", "tokens": ["Sie", "sich", "auch", "in", "der", "Zeit", ",", "wo\u00b7rinn", "das", "Jahr", "sich", "en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie l\u00e4ufft den Himmel durch, um unsre Sonn\u2019, im Creyse,", "tokens": ["Sie", "l\u00e4ufft", "den", "Him\u00b7mel", "durch", ",", "um", "uns\u00b7re", "Sonn'", ",", "im", "Crey\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUI", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und rollet vorwerts stets, in ihrer grossen Reise,", "tokens": ["Und", "rol\u00b7let", "vor\u00b7werts", "stets", ",", "in", "ih\u00b7rer", "gros\u00b7sen", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von Westen, Ostenwerts: die beyden Angel stehn", "tokens": ["Von", "Wes\u00b7ten", ",", "Os\u00b7ten\u00b7werts", ":", "die", "bey\u00b7den", "An\u00b7gel", "stehn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NE", "$.", "ART", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Best\u00e4ndig gegen zwo am Himmel feste Stellen.", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "ge\u00b7gen", "zwo", "am", "Him\u00b7mel", "fes\u00b7te", "Stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "CARD", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}