{"textgrid.poem.29390": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonne, die das Land vergoldte, wo das frome Strelitz steht,", "tokens": ["Son\u00b7ne", ",", "die", "das", "Land", "ver\u00b7gold\u00b7te", ",", "wo", "das", "fro\u00b7me", "Stre\u00b7litz", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zog in eine Fahrt von neuem, wo den Oder-rand erh\u00f6ht", "tokens": ["Zog", "in", "ei\u00b7ne", "Fahrt", "von", "neu\u00b7em", ",", "wo", "den", "O\u00b7der\u00b7rand", "er\u00b7h\u00f6ht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "$,", "PWAV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Brieg, das Piasteer-Haus; allda steht sie lieblich stille,", "tokens": ["Brieg", ",", "das", "Pias\u00b7teer\u00b7Haus", ";", "all\u00b7da", "steht", "sie", "lieb\u00b7lich", "stil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$.", "PAV", "VVFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Streuet lauter G\u00fct und Gaben, f\u00fcnckelt, strahlet in der V\u00f6lle.", "tokens": ["Streu\u00b7et", "lau\u00b7ter", "G\u00fct", "und", "Ga\u00b7ben", ",", "f\u00fcn\u00b7ckelt", ",", "strah\u00b7let", "in", "der", "V\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Geh, o Sonne, feine Sonne, geh uns nun und nie zur Ruh!", "tokens": ["Geh", ",", "o", "Son\u00b7ne", ",", "fei\u00b7ne", "Son\u00b7ne", ",", "geh", "uns", "nun", "und", "nie", "zur", "Ruh", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "KON", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Sonnen-Ziraht, selbsten Sonne, wirff uns immer Strahlen zu!", "tokens": ["Son\u00b7nen\u00b7Zi\u00b7raht", ",", "selbs\u00b7ten", "Son\u00b7ne", ",", "wirff", "uns", "im\u00b7mer", "Strah\u00b7len", "zu", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Sonne, die am Himmel lacht, lachet dieser Sonne wegen,", "tokens": ["Son\u00b7ne", ",", "die", "am", "Him\u00b7mel", "lacht", ",", "la\u00b7chet", "die\u00b7ser", "Son\u00b7ne", "we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "PDAT", "NN", "APPR", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Gibt der Schwester halben uns kl\u00e4rern Blick und reichern Segen;", "tokens": ["Gibt", "der", "Schwes\u00b7ter", "hal\u00b7ben", "uns", "kl\u00e4\u00b7rern", "Blick", "und", "rei\u00b7chern", "Se\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "PPER", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Sonne, die die Zeiten theilet, theilet Amt und Regiment", "tokens": ["Son\u00b7ne", ",", "die", "die", "Zei\u00b7ten", "thei\u00b7let", ",", "thei\u00b7let", "Amt", "und", "Re\u00b7gi\u00b7ment"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Mit der Sonne, die von Strelitz g\u00fctig sich zu uns gewendt.", "tokens": ["Mit", "der", "Son\u00b7ne", ",", "die", "von", "Stre\u00b7litz", "g\u00fc\u00b7tig", "sich", "zu", "uns", "ge\u00b7wendt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NE", "ADJD", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}