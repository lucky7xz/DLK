{"textgrid.poem.44193": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[ich will lachen, ich will scherzen]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will lachen, ich will scherzen,", "tokens": ["Ich", "will", "la\u00b7chen", ",", "ich", "will", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob es gleich den Neid verdreust,", "tokens": ["Ob", "es", "gleich", "den", "Neid", "ver\u00b7dreust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Andre m\u00f6gen Grillen fangen,", "tokens": ["And\u00b7re", "m\u00f6\u00b7gen", "Gril\u00b7len", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts ermuntert mein Verlangen,", "tokens": ["Nichts", "er\u00b7mun\u00b7tert", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts bek\u00fcmmert meinen Geist", "tokens": ["Nichts", "be\u00b7k\u00fcm\u00b7mert", "mei\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als der Wechsel treuer Herzen.", "tokens": ["Als", "der", "Wech\u00b7sel", "treu\u00b7er", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eilt man nicht in Rosenbrechen,", "tokens": ["Eilt", "man", "nicht", "in", "Ro\u00b7sen\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauft der Vortheil aus der Hand;", "tokens": ["Lauft", "der", "Vor\u00b7theil", "aus", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Jugend Fr\u00fchlingsjahren", "tokens": ["In", "der", "Ju\u00b7gend", "Fr\u00fch\u00b7lings\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steckt der Kram verliebter Wahren,", "tokens": ["Steckt", "der", "Kram", "ver\u00b7lieb\u00b7ter", "Wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber auch der Unbestand.", "tokens": ["A\u00b7ber", "auch", "der", "Un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Brecht, eh Reu und D\u00f6rner stechen!", "tokens": ["Brecht", ",", "eh", "Reu", "und", "D\u00f6r\u00b7ner", "ste\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NE", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eh noch Glut und Kraft verrauchen,", "tokens": ["Eh", "noch", "Glut", "und", "Kraft", "ver\u00b7rau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt der Ku\u00df Zufriedenheit;", "tokens": ["Tr\u00e4gt", "der", "Ku\u00df", "Zu\u00b7frie\u00b7den\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute lebt man ohne Sorgen,", "tokens": ["Heu\u00b7te", "lebt", "man", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott und Vorsicht weis, ob morgen;", "tokens": ["Gott", "und", "Vor\u00b7sicht", "weis", ",", "ob", "mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$,", "KOUS", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ey, so lerne man der Zeit", "tokens": ["Ey", ",", "so", "ler\u00b7ne", "man", "der", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PIS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bey Gesellschaft recht gebrauchen.", "tokens": ["Bey", "Ge\u00b7sell\u00b7schaft", "recht", "ge\u00b7brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ohne Lieben ist das Gl\u00fccke", "tokens": ["Oh\u00b7ne", "Lie\u00b7ben", "ist", "das", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier auf Erden nichts als Dunst;", "tokens": ["Hier", "auf", "Er\u00b7den", "nichts", "als", "Dunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PIS", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reichthum kan den Gram nicht lindern,", "tokens": ["Reicht\u00b7hum", "kan", "den", "Gram", "nicht", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ehre kan den Schmerz nicht mindern,", "tokens": ["Eh\u00b7re", "kan", "den", "Schmerz", "nicht", "min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur die Liebe kan die Kunst.", "tokens": ["Nur", "die", "Lie\u00b7be", "kan", "die", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eitle W\u00fcntsche, bleibt zur\u00fccke!", "tokens": ["Eit\u00b7le", "W\u00fcnt\u00b7sche", ",", "bleibt", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus der Liebe quillt Vergn\u00fcgen", "tokens": ["Aus", "der", "Lie\u00b7be", "quillt", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Nachschmack g\u00fcldner Zeit;", "tokens": ["Und", "der", "Nach\u00b7schmack", "g\u00fcld\u00b7ner", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein galant und treu Gem\u00fcthe", "tokens": ["Ein", "ga\u00b7lant", "und", "treu", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reizt uns nebst der Sch\u00f6nheit Bl\u00fcthe,", "tokens": ["Reizt", "uns", "nebst", "der", "Sch\u00f6n\u00b7heit", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis die Wollust Flammen streut.", "tokens": ["Bis", "die", "Wol\u00b7lust", "Flam\u00b7men", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, mein Herz, halt dies verschwiegen!", "tokens": ["Ach", ",", "mein", "Herz", ",", "halt", "dies", "ver\u00b7schwie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PDS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In des Mundes Purpurh\u00f6hlen", "tokens": ["In", "des", "Mun\u00b7des", "Pur\u00b7pur\u00b7h\u00f6h\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmt der Ku\u00df noch gr\u00f6\u00dfre Kraft.", "tokens": ["Nimmt", "der", "Ku\u00df", "noch", "gr\u00f6\u00df\u00b7re", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Warthen w\u00e4chst der Zunder,", "tokens": ["Von", "dem", "Wart\u00b7hen", "w\u00e4chst", "der", "Zun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie viel Entz\u00fcckungswunder", "tokens": ["O", "wie", "viel", "Ent\u00b7z\u00fc\u00b7ckungs\u00b7wun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "N\u00e4hren nicht die Leidenschaft", "tokens": ["N\u00e4h\u00b7ren", "nicht", "die", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich und klug verliebter Seelen.", "tokens": ["Gleich", "und", "klug", "ver\u00b7lieb\u00b7ter", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "R\u00fchmt mir auch nicht blos das Prangen", "tokens": ["R\u00fchmt", "mir", "auch", "nicht", "blos", "das", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer Haut, die ausw\u00e4rts glei\u00dft!", "tokens": ["Ei\u00b7ner", "Haut", ",", "die", "aus\u00b7w\u00e4rts", "glei\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Farben ohne Leben", "tokens": ["In", "den", "Far\u00b7ben", "oh\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Find ich lauter Eckel kleben;", "tokens": ["Find", "ich", "lau\u00b7ter", "E\u00b7ckel", "kle\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Find ich aber Wiz und Geist,", "tokens": ["Find", "ich", "a\u00b7ber", "Wiz", "und", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ey, so bin ich gleich gefangen.", "tokens": ["Ey", ",", "so", "bin", "ich", "gleich", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Es erwehlt mein Herz zwo Lippen,", "tokens": ["Es", "er\u00b7wehlt", "mein", "Herz", "zwo", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur es h\u00e4lt sich annoch still;", "tokens": ["Nur", "es", "h\u00e4lt", "sich", "an\u00b7noch", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bergt ihrs auch, ihr losen Augen,", "tokens": ["Bergt", "ihrs", "auch", ",", "ihr", "lo\u00b7sen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euer stetig Feuersaugen", "tokens": ["Eu\u00b7er", "ste\u00b7tig", "Feu\u00b7er\u00b7sau\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Redet so bereits zu viel.", "tokens": ["Re\u00b7det", "so", "be\u00b7reits", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Grade zu st\u00f6\u00dft oft an Klippen.", "tokens": ["Gra\u00b7de", "zu", "st\u00f6\u00dft", "oft", "an", "Klip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}