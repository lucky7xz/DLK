{"textgrid.poem.61905": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Die S\u00e4ngerin", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor andern kalt zu scheinen", "tokens": ["Vor", "an\u00b7dern", "kalt", "zu", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich mich l\u00e4ngst gew\u00f6hnt,", "tokens": ["Hab'", "ich", "mich", "l\u00e4ngst", "ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch halt' ich kaum das Weinen,", "tokens": ["Doch", "halt'", "ich", "kaum", "das", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn diese Stimme t\u00f6nt.", "tokens": ["Wenn", "die\u00b7se", "Stim\u00b7me", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die goldnen Weisen triefen", "tokens": ["Die", "gold\u00b7nen", "Wei\u00b7sen", "trie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins Herz wie Vollmondschein", "tokens": ["Ins", "Herz", "wie", "Voll\u00b7mond\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KOKOM", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ziehn in alle Tiefen", "tokens": ["Und", "ziehn", "in", "al\u00b7le", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Wehmut mich hinein.", "tokens": ["Der", "Weh\u00b7mut", "mich", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das sind gesungene Tr\u00e4nen;", "tokens": ["Das", "sind", "ge\u00b7sun\u00b7ge\u00b7ne", "Tr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es klagt und flutet drin", "tokens": ["Es", "klagt", "und", "flu\u00b7tet", "drin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ganze Leiden und Sehnen", "tokens": ["Das", "gan\u00b7ze", "Lei\u00b7den", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der kranken S\u00e4ngerin.", "tokens": ["Der", "kran\u00b7ken", "S\u00e4n\u00b7ge\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schon brennt auf ihrem blassen", "tokens": ["Schon", "brennt", "auf", "ih\u00b7rem", "blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gesicht ein fliegend Rot;", "tokens": ["Ge\u00b7sicht", "ein", "flie\u00b7gend", "Rot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie kann das Singen nicht lassen", "tokens": ["Sie", "kann", "das", "Sin\u00b7gen", "nicht", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wei\u00df, es ist ihr Tod.", "tokens": ["Und", "wei\u00df", ",", "es", "ist", "ihr", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}