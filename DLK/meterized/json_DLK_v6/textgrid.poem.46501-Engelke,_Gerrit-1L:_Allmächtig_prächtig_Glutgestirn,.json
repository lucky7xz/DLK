{"textgrid.poem.46501": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Allm\u00e4chtig pr\u00e4chtig Glutgestirn,", "genre": "verse", "period": "N.A.", "pub_year": 1918, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allm\u00e4chtig pr\u00e4chtig Glutgestirn,", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "pr\u00e4ch\u00b7tig", "Glut\u00b7ge\u00b7stirn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00dcberw\u00e4ltigend emporwirbelnd, aufdonnernd vor Licht \u00fcber Wolkenfirn", "tokens": ["\u00dc\u00b7berw\u00b7\u00e4l\u00b7ti\u00b7gend", "em\u00b7por\u00b7wir\u00b7belnd", ",", "auf\u00b7don\u00b7nernd", "vor", "Licht", "\u00fc\u00b7ber", "Wol\u00b7ken\u00b7firn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "VVPP", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+--++-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "In flutblau sch\u00e4umende Himmelshallen,", "tokens": ["In", "flut\u00b7blau", "sch\u00e4u\u00b7men\u00b7de", "Him\u00b7mels\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die aus unendlichem ewig herniederfallen:", "tokens": ["Die", "aus", "un\u00b7end\u00b7li\u00b7chem", "e\u00b7wig", "her\u00b7nie\u00b7der\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Unter dir sind: Waldmeere, der Fl\u00fcsse Ge\u00e4der, Felsballen,", "tokens": ["Un\u00b7ter", "dir", "sind", ":", "Wald\u00b7mee\u00b7re", ",", "der", "Fl\u00fcs\u00b7se", "Ge\u00b7\u00e4\u00b7der", ",", "Fels\u00b7bal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "$.", "NN", "$,", "ART", "NN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+--+-+--", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Und der grenzenlos hindunstende Tag", "tokens": ["Und", "der", "gren\u00b7zen\u00b7los", "hin\u00b7duns\u00b7ten\u00b7de", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Von Anfang zu Anfang.", "tokens": ["Von", "An\u00b7fang", "zu", "An\u00b7fang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Erster Tag der Farnw\u00e4lder und Saurier; ersten Blutes, Pulses Schlag,", "tokens": ["Ers\u00b7ter", "Tag", "der", "Farn\u00b7w\u00e4l\u00b7der", "und", "Sau\u00b7rier", ";", "ers\u00b7ten", "Blu\u00b7tes", ",", "Pul\u00b7ses", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "KON", "NN", "$.", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-++--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "Da aus der Mutter gew\u00f6lbtem Leib ein Kind den Erdenodem trank!", "tokens": ["Da", "aus", "der", "Mut\u00b7ter", "ge\u00b7w\u00f6lb\u00b7tem", "Leib", "ein", "Kind", "den", "Er\u00b7de\u00b7no\u00b7dem", "trank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "Oh! wie da aus aller Runde orgelnd: Leben! Leben! sang \u2013", "tokens": ["Oh", "!", "wie", "da", "aus", "al\u00b7ler", "Run\u00b7de", "or\u00b7gelnd", ":", "Le\u00b7ben", "!", "Le\u00b7ben", "!", "sang", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADV", "APPR", "PIAT", "NN", "VVPP", "$.", "NN", "$.", "NN", "$.", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "M\u00e4chtig aufrauschten die Vaterstimmen der Fluten dem Geb\u00e4rten,", "tokens": ["M\u00e4ch\u00b7tig", "auf\u00b7rauschten", "die", "Va\u00b7ter\u00b7stim\u00b7men", "der", "Flu\u00b7ten", "dem", "Ge\u00b7b\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Die grauen Ur-Steingebirge schauerten in ihren B\u00e4rten,", "tokens": ["Die", "grau\u00b7en", "Ur\u00b7Stein\u00b7ge\u00b7bir\u00b7ge", "schau\u00b7er\u00b7ten", "in", "ih\u00b7ren", "B\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-++-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.13": {"text": "Und Bl\u00fcten, Bl\u00fcten fielen tausendfroh aus Blumenhainen", "tokens": ["Und", "Bl\u00fc\u00b7ten", ",", "Bl\u00fc\u00b7ten", "fie\u00b7len", "tau\u00b7send\u00b7froh", "aus", "Blu\u00b7men\u00b7hai\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Und ein kindlich Lallen und erhaben Weinen. \u2013 \u2013 \u2013", "tokens": ["Und", "ein", "kind\u00b7lich", "Lal\u00b7len", "und", "er\u00b7ha\u00b7ben", "Wei\u00b7nen", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "KON", "ADJD", "NN", "$.", "$(", "$(", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Und regten sich tief unter deinem Feuerangesicht:", "tokens": ["Und", "reg\u00b7ten", "sich", "tief", "un\u00b7ter", "dei\u00b7nem", "Feu\u00b7e\u00b7ran\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "Der Heerscharen Gewimmel, Aufruhr und Kampfgericht,", "tokens": ["Der", "Heer\u00b7scha\u00b7ren", "Ge\u00b7wim\u00b7mel", ",", "Auf\u00b7ruhr", "und", "Kampf\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Gelage bei Leichen, Sturzwassernot, Meucheltod \u2013", "tokens": ["Ge\u00b7la\u00b7ge", "bei", "Lei\u00b7chen", ",", "Sturz\u00b7was\u00b7ser\u00b7not", ",", "Meu\u00b7chel\u00b7tod", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Schwarzqualmender St\u00e4dtemord, wild und funkenrot, \u2013", "tokens": ["Schwarz\u00b7qual\u00b7men\u00b7der", "St\u00e4d\u00b7te\u00b7mord", ",", "wild", "und", "fun\u00b7ken\u00b7rot", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "VVFIN", "$,", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Vom Ha\u00df, vom Leid zerpfl\u00fcgtes und zerfleischtes Land. \u2013 \u2013", "tokens": ["Vom", "Ha\u00df", ",", "vom", "Leid", "zer\u00b7pfl\u00fcg\u00b7tes", "und", "zer\u00b7fleischtes", "Land", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "ADJA", "KON", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Und wieder bricht dein Feuerkn\u00e4ul durch Nacht und Wetterwand:", "tokens": ["Und", "wie\u00b7der", "bricht", "dein", "Feu\u00b7er\u00b7kn\u00e4ul", "durch", "Nacht", "und", "Wet\u00b7ter\u00b7wand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "Seht da: London! Tower-Bridge, Dom, Westminster,", "tokens": ["Seht", "da", ":", "Lon\u00b7don", "!", "Tow\u00b7er\u00b7Bridge", ",", "Dom", ",", "West\u00b7mins\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "NE", "$.", "NE", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.22": {"text": "Palastfronten von grauem Nebel triefend, morgenfinster \u2013", "tokens": ["Pa\u00b7last\u00b7fron\u00b7ten", "von", "grau\u00b7em", "Ne\u00b7bel", "trie\u00b7fend", ",", "mor\u00b7gen\u00b7fins\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVPP", "$,", "ADV", "$("], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Auf einmal: brennend, auflodernd, T\u00fcrme gl\u00fchen,", "tokens": ["Auf", "ein\u00b7mal", ":", "bren\u00b7nend", ",", "auf\u00b7lo\u00b7dernd", ",", "T\u00fcr\u00b7me", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "VVPP", "$,", "VVPP", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Park, Alleestra\u00dfen, Fu\u00dfg\u00e4nger, Volk, Volk spr\u00fchen,", "tokens": ["Park", ",", "Al\u00b7lees\u00b7tra\u00b7\u00dfen", ",", "Fu\u00df\u00b7g\u00e4n\u00b7ger", ",", "Volk", ",", "Volk", "spr\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "$,", "NN", "$,", "NN", "$,", "NN", "VVINF", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.25": {"text": "Aufquirlend, hingerissen im glei\u00dfenden Mittagsgold!", "tokens": ["Auf\u00b7quir\u00b7lend", ",", "hin\u00b7ge\u00b7ris\u00b7sen", "im", "glei\u00b7\u00dfen\u00b7den", "Mit\u00b7tags\u00b7gold", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Und Wagen-Strom schiebt, knattert unendlich \u2013 rollt, rollt \u2013", "tokens": ["Und", "Wa\u00b7gen\u00b7Strom", "schiebt", ",", "knat\u00b7tert", "un\u00b7end\u00b7lich", "\u2013", "rollt", ",", "rollt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "$(", "VVFIN", "$,", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Und wieder, seht: Berlin! H\u00e4usergevierte, Warenhausbl\u00f6cke,", "tokens": ["Und", "wie\u00b7der", ",", "seht", ":", "Ber\u00b7lin", "!", 