{"textgrid.poem.55242": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Hypochonder", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Teufel hol das Menschengeschlecht!", "tokens": ["Der", "Teu\u00b7fel", "hol", "das", "Men\u00b7schen\u00b7ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Man m\u00f6chte rasend werden!", "tokens": ["Man", "m\u00f6ch\u00b7te", "ra\u00b7send", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da nehm ich mir so eifrig vor:", "tokens": ["Da", "nehm", "ich", "mir", "so", "eif\u00b7rig", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will niemand weiter sehen,", "tokens": ["Will", "nie\u00b7mand", "wei\u00b7ter", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Will all das Volk Gott und sich selbst", "tokens": ["Will", "all", "das", "Volk", "Gott", "und", "sich", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "ART", "NN", "NN", "KON", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dem Teufel \u00fcberlassen!", "tokens": ["Und", "dem", "Teu\u00b7fel", "\u00fc\u00b7ber\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und kaum seh ich ein Menschengesicht,", "tokens": ["Und", "kaum", "seh", "ich", "ein", "Men\u00b7schen\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "So hab ich's wieder lieb.", "tokens": ["So", "hab", "ich's", "wie\u00b7der", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}