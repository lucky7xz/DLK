{"dta.poem.4355": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung g\u00f6ttlicher Wunder  \n in  \n  Erhaltung des Saamen-Korns  \n im Frost.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ich, in einem starken Frost, die, nur in einem", "tokens": ["Wie", "ich", ",", "in", "ei\u00b7nem", "star\u00b7ken", "Frost", ",", "die", ",", "nur", "in", "ei\u00b7nem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "$,", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "engen Raum,", "tokens": ["en\u00b7gen", "Raum", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Durchs Eis verschr\u00e4nkte Schw\u00e4hne f\u00fcttern, und ihnen", "tokens": ["Durchs", "Eis", "ver\u00b7schr\u00e4nk\u00b7te", "Schw\u00e4h\u00b7ne", "f\u00fct\u00b7tern", ",", "und", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVINF", "$,", "KON", "PPER"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Haber geben lie\u00df,", "tokens": ["Ha\u00b7ber", "ge\u00b7ben", "lie\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Der alsobald mit Eis belief, wann er noch in dem Wasser", "tokens": ["Der", "al\u00b7so\u00b7bald", "mit", "Eis", "be\u00b7lief", ",", "wann", "er", "noch", "in", "dem", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$,", "PWAV", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "kaum,", "tokens": ["kaum", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Und dennoch an sich selbst nicht fror; erwog ich, was mir", "tokens": ["Und", "den\u00b7noch", "an", "sich", "selbst", "nicht", "fror", ";", "er\u00b7wog", "ich", ",", "was", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "APPR", "PRF", "ADV", "PTKNEG", "VVFIN", "$.", "VVFIN", "PPER", "$,", "PWS", "PPER"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "dieses wies:", "tokens": ["die\u00b7ses", "wies", ":"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Ein g\u00fctig Wunder, welches man wohl eben nicht so oft", "tokens": ["Ein", "g\u00fc\u00b7tig", "Wun\u00b7der", ",", "wel\u00b7ches", "man", "wohl", "e\u00b7ben", "nicht", "so", "oft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "PIS", "ADV", "ADV", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "bedenket,", "tokens": ["be\u00b7den\u00b7ket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Und das gewi\u00df, aus weiser Absicht, zum Nutz der Crea-", "tokens": ["Und", "das", "ge\u00b7wi\u00df", ",", "aus", "wei\u00b7ser", "Ab\u00b7sicht", ",", "zum", "Nutz", "der", "Crea"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "tur geschenket.", "tokens": ["tur", "ge\u00b7schen\u00b7ket", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Da nemlich solch ein Saamen-Korn, wie hart es friert,", "tokens": ["Da", "nem\u00b7lich", "solch", "ein", "Saa\u00b7men\u00b7Korn", ",", "wie", "hart", "es", "friert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "ART", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "doch nicht erfriert.", "tokens": ["doch", "nicht", "er\u00b7friert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Wovon die ganze Creatur und Menschheit solchen Nutz", "tokens": ["Wo\u00b7von", "die", "gan\u00b7ze", "Crea\u00b7tur", "und", "Menschheit", "sol\u00b7chen", "Nutz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "KON", "NN", "PIAT", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "versp\u00fchrt,", "tokens": ["ver\u00b7sp\u00fchrt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Da\u00df, wenn es fr\u00f6r, als andre Fr\u00fcchte, die w\u00e4ssericher, als", "tokens": ["Da\u00df", ",", "wenn", "es", "fr\u00f6r", ",", "als", "and\u00b7re", "Fr\u00fcch\u00b7te", ",", "die", "w\u00e4s\u00b7se\u00b7ri\u00b7cher", ",", "als"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADJD", "$,", "KOUS", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "KOUS"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "wie sie,", "tokens": ["wie", "sie", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Nicht nur die Nahrung aller Menschen verd\u00fcrb\u2019, und auch", "tokens": ["Nicht", "nur", "die", "Nah\u00b7rung", "al\u00b7ler", "Men\u00b7schen", "ver\u00b7d\u00fcrb'", ",", "und", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "PIAT", "NN", "VVFIN", "$,", "KON", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "mit aller M\u00fch'", "tokens": ["mit", "al\u00b7ler", "M\u00fch'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Nicht w\u00fcrd\u2019 erhalten werden k\u00f6nnen; nein, auch die Saat", "tokens": ["Nicht", "w\u00fcrd'", "er\u00b7hal\u00b7ten", "wer\u00b7den", "k\u00f6n\u00b7nen", ";", "nein", ",", "auch", "die", "Saat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "VVPP", "VAINF", "VMFIN", "$.", "PTKANT", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "f\u00fcrs k\u00fcnft'ge Jahr", "tokens": ["f\u00fcrs", "k\u00fcnft'\u00b7ge", "Jahr"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "In augenscheinlicher Gefahr,", "tokens": ["In", "au\u00b7gen\u00b7schein\u00b7li\u00b7cher", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}