{"dta.poem.14099": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fromme .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es sa\u00df ein Kind gebunden und gefangen,", "tokens": ["Es", "sa\u00df", "ein", "Kind", "ge\u00b7bun\u00b7den", "und", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo vor der Menschen eitlem Thun und Schallen", "tokens": ["Wo", "vor", "der", "Men\u00b7schen", "eit\u00b7lem", "Thun", "und", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Vorzeit Wunderlaute tr\u00fcb verhallen;", "tokens": ["Der", "Vor\u00b7zeit", "Wun\u00b7der\u00b7lau\u00b7te", "tr\u00fcb", "ver\u00b7hal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der alten Heimath dacht' sie voll Verlangen.", "tokens": ["Der", "al\u00b7ten", "Hei\u00b7math", "dacht'", "sie", "voll", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}