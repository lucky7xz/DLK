{"textgrid.poem.39403": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Mondschein-Nacht ward stumm, und immer stummer,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mondschein-Nacht ward stumm, und immer stummer,", "tokens": ["Die", "Mond\u00b7schein\u00b7Nacht", "ward", "stumm", ",", "und", "im\u00b7mer", "stum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich h\u00f6rte nur der Rosse scharfen Trab.", "tokens": ["Ich", "h\u00f6r\u00b7te", "nur", "der", "Ros\u00b7se", "schar\u00b7fen", "Trab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein jeder Tritt, so sagte mir mein Kummer,", "tokens": ["Ein", "je\u00b7der", "Tritt", ",", "so", "sag\u00b7te", "mir", "mein", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchrt von der Holden weit und weiter ab.", "tokens": ["F\u00fchrt", "von", "der", "Hol\u00b7den", "weit", "und", "wei\u00b7ter", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und stets verscheucht' ein Zweifel mir den Schlummer:", "tokens": ["Und", "stets", "ver\u00b7scheucht'", "ein", "Zwei\u00b7fel", "mir", "den", "Schlum\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gedenkt sie wohl der Zeichen, die sie gab?", "tokens": ["Ge\u00b7denkt", "sie", "wohl", "der", "Zei\u00b7chen", ",", "die", "sie", "gab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Erst als mein Haupt am Lorbeerkranze ruhte,", "tokens": ["Erst", "als", "mein", "Haupt", "am", "Lor\u00b7beer\u00b7kran\u00b7ze", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den sie verschwiegen, still, mit sinn'ger Hand", "tokens": ["Den", "sie", "ver\u00b7schwie\u00b7gen", ",", "still", ",", "mit", "sinn'\u00b7ger", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "$,", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Als Talisman mir f\u00fcr die Reise wand,", "tokens": ["Als", "Ta\u00b7lis\u00b7man", "mir", "f\u00fcr", "die", "Rei\u00b7se", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "F\u00fchlt' ich Erquickung in dem tr\u00fcben Muthe.", "tokens": ["F\u00fchlt'", "ich", "Er\u00b7quic\u00b7kung", "in", "dem", "tr\u00fc\u00b7ben", "Mu\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Mir wies ihr l\u00e4chelnd Bild ein heitrer Traum:", "tokens": ["Mir", "wies", "ihr", "l\u00e4\u00b7chelnd", "Bild", "ein", "hei\u00b7trer", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch ach! zu fr\u00fch erschien Aurorens Purpursaum.", "tokens": ["Doch", "ach", "!", "zu", "fr\u00fch", "er\u00b7schien", "Au\u00b7ro\u00b7rens", "Pur\u00b7pur\u00b7saum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PTKA", "ADJD", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}