{"textgrid.poem.57897": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Voreiligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Pattensen, in Pattensen", "tokens": ["In", "Pat\u00b7ten\u00b7sen", ",", "in", "Pat\u00b7ten\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schrie man: \u00bbMehr Licht, mehr Licht!", "tokens": ["Schrie", "man", ":", "\u00bb", "Mehr", "Licht", ",", "mehr", "Licht", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$.", "$(", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Elektrisch mu\u00df hier alles sein,", "tokens": ["E\u00b7lekt\u00b7risch", "mu\u00df", "hier", "al\u00b7les", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PIS", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Gas\u00f6l gen\u00fcgt uns nicht!\u00ab", "tokens": ["Das", "Ga\u00b7s\u00f6l", "ge\u00b7n\u00fcgt", "uns", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und ehe die Leitung fertig war,", "tokens": ["Und", "e\u00b7he", "die", "Lei\u00b7tung", "fer\u00b7tig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nahm man die Laternen fort,", "tokens": ["Nahm", "man", "die", "La\u00b7ter\u00b7nen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "+-----+", "measure": "dactylic.init"}, "line.3": {"text": "Drum h\u00fcllt jetzt schwarze Finsternis", "tokens": ["Drum", "h\u00fcllt", "jetzt", "schwar\u00b7ze", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Abends ein den Ort.", "tokens": ["Des", "A\u00b7bends", "ein", "den", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn nicht gerade Mondschein ist", "tokens": ["Und", "wenn", "nicht", "ge\u00b7ra\u00b7de", "Mond\u00b7schein", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PTKNEG", "ADV", "NN", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und man mu\u00df abends aus,", "tokens": ["Und", "man", "mu\u00df", "a\u00b7bends", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann nimmt man sich 'ne Laterne mit,", "tokens": ["Dann", "nimmt", "man", "sich", "'ne", "La\u00b7ter\u00b7ne", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn man verl\u00e4\u00dft das Haus.", "tokens": ["Wenn", "man", "ver\u00b7l\u00e4\u00dft", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch unbedingt n\u00f6tig ist das nicht,", "tokens": ["Doch", "un\u00b7be\u00b7dingt", "n\u00f6\u00b7tig", "ist", "das", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PDS", "PTKNEG", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denn, Kinder, wie mir deuchte,", "tokens": ["Denn", ",", "Kin\u00b7der", ",", "wie", "mir", "deuch\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ist es bei euch immer hell:", "tokens": ["Da", "ist", "es", "bei", "euch", "im\u00b7mer", "hell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Daf\u00fcr habt ihr ja \u00bbDie Leuchte!\u00ab", "tokens": ["Da\u00b7f\u00fcr", "habt", "ihr", "ja", "\u00bb", "Die", "Leuch\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "$(", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}