{"textgrid.poem.46425": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An Eglen", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du giebst dir Egle M\u00fch die Sommersprossen zu vertreiben?", "tokens": ["Du", "giebst", "dir", "Eg\u00b7le", "M\u00fch", "die", "Som\u00b7mer\u00b7spros\u00b7sen", "zu", "ver\u00b7trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "NE", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Du bist vergeblich klug!", "tokens": ["Du", "bist", "ver\u00b7geb\u00b7lich", "klug", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zwar wirst du minder he\u00dflich bleiben,", "tokens": ["Zwar", "wirst", "du", "min\u00b7der", "he\u00df\u00b7lich", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allein, noch he\u00dflich gnug.", "tokens": ["Al\u00b7lein", ",", "noch", "he\u00df\u00b7lich", "gnug", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}