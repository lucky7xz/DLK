{"dta.poem.1416": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Pathe/ freue dich: Denn deines Sch\u00f6pffers Huld/", "tokens": ["Mein", "Pa\u00b7the", "/", "freu\u00b7e", "dich", ":", "Denn", "dei\u00b7nes", "Sch\u00f6pf\u00b7fers", "Huld", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PPER", "$.", "KON", "PPOSAT", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dich zur Welt gebracht/ befreyt dich izt der Schuld", "tokens": ["Die", "dich", "zur", "Welt", "ge\u00b7bracht", "/", "be\u00b7freyt", "dich", "izt", "der", "Schuld"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$(", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Adam angeerbt in heilger Tauffe Flutt/", "tokens": ["Von", "A\u00b7dam", "an\u00b7ge\u00b7erbt", "in", "heil\u00b7ger", "Tauf\u00b7fe", "Flutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schreibt dich in seine Hand durch JEsu Christi Blutt/", "tokens": ["Schreibt", "dich", "in", "sei\u00b7ne", "Hand", "durch", "Je\u00b7su", "Chris\u00b7ti", "Blutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "APPR", "NE", "NE", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Darinn du wirst getrost im gantzen Leben stehn/", "tokens": ["Da\u00b7rinn", "du", "wirst", "ge\u00b7trost", "im", "gant\u00b7zen", "Le\u00b7ben", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VAFIN", "VVPP", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und freudig durch die Welt ins Reich der Ehren gehn.", "tokens": ["Und", "freu\u00b7dig", "durch", "die", "Welt", "ins", "Reich", "der", "Eh\u00b7ren", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}