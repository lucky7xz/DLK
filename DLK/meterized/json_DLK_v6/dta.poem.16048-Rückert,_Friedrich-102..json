{"dta.poem.16048": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "102.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat doch jede Geburt des Lebens ihre Wehn!", "tokens": ["Hat", "doch", "je\u00b7de", "Ge\u00b7burt", "des", "Le\u00b7bens", "ih\u00b7re", "Wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sie sind zu \u00fcberstehn, weil sie vor\u00fcbergehn.", "tokens": ["Sie", "sind", "zu", "\u00fc\u00b7bers\u00b7tehn", ",", "weil", "sie", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O w\u00e4re jedes Gl\u00fcck mit Schmerzen nur geboren,", "tokens": ["O", "w\u00e4\u00b7re", "je\u00b7des", "Gl\u00fcck", "mit", "Schmer\u00b7zen", "nur", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIAT", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht einst mit schmerzlichern Gef\u00fchlen auch verloren!", "tokens": ["Nicht", "einst", "mit", "schmerz\u00b7li\u00b7chern", "Ge\u00b7f\u00fch\u00b7len", "auch", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}