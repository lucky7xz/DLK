{"textgrid.poem.58178": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "216. Der g\u00f6ttlichen Liebe Art", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es mu\u00df die wahre Liebe sein", "tokens": ["Es", "mu\u00df", "die", "wah\u00b7re", "Lie\u00b7be", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Best\u00e4ndig, innig, frei und rein,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", ",", "in\u00b7nig", ",", "frei", "und", "rein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist der Seelen Speis' und Leben,", "tokens": ["Sie", "ist", "der", "See\u00b7len", "Speis'", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der H\u00f6lle Gift, der S\u00fcnden Tod,", "tokens": ["Der", "H\u00f6l\u00b7le", "Gift", ",", "der", "S\u00fcn\u00b7den", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie st\u00e4rkt und tr\u00f6st't in aller Not;", "tokens": ["Sie", "st\u00e4rkt", "und", "tr\u00f6st't", "in", "al\u00b7ler", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Leer aus dein Herz, Gott kann sie geben!", "tokens": ["Leer", "aus", "dein", "Herz", ",", "Gott", "kann", "sie", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}