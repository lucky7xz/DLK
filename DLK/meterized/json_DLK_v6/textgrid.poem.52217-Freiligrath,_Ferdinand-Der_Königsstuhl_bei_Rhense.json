{"textgrid.poem.52217": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00f6nigsstuhl bei Rhense", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neu gebaut beim alten Rhense", "tokens": ["Neu", "ge\u00b7baut", "beim", "al\u00b7ten", "Rhen\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht der Wahlstuhl wiederum,", "tokens": ["Steht", "der", "Wahl\u00b7stuhl", "wie\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Enten, ach! und G\u00e4nse", "tokens": ["A\u00b7ber", "En\u00b7ten", ",", "ach", "!", "und", "G\u00e4n\u00b7se"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "ITJ", "$.", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiden schnatternd drum herum.", "tokens": ["Wei\u00b7den", "schnat\u00b7ternd", "drum", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo einst Wahlen hielt das Wahlreich,", "tokens": ["Wo", "einst", "Wah\u00b7len", "hielt", "das", "Wahl\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Reichsaar trotzig schrie,", "tokens": ["Und", "der", "Reich\u00b7saar", "trot\u00b7zig", "schrie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Dorten, feierlich und zahlreich,", "tokens": ["Dor\u00b7ten", ",", "fei\u00b7er\u00b7lich", "und", "zahl\u00b7reich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grast nun zahmes Federvieh.", "tokens": ["Grast", "nun", "zah\u00b7mes", "Fe\u00b7der\u00b7vieh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach! und aus den Weidenb\u00fcschen", "tokens": ["Ach", "!", "und", "aus", "den", "Wei\u00b7den\u00b7b\u00fc\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilt kein Kurf\u00fcrst mut'gen Schritts;", "tokens": ["Eilt", "kein", "Kur\u00b7f\u00fcrst", "mut'\u00b7gen", "Schritts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den sieben hohen Nischen", "tokens": ["In", "den", "sie\u00b7ben", "ho\u00b7hen", "Ni\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leer und \u00f6de jeder Sitz!", "tokens": ["Leer", "und", "\u00f6\u00b7de", "je\u00b7der", "Sitz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dennoch freut es, ihn zu schauen,", "tokens": ["Den\u00b7noch", "freut", "es", ",", "ihn", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stattlich, wie er vormals stand,", "tokens": ["Statt\u00b7lich", ",", "wie", "er", "vor\u00b7mals", "stand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als aus nah und fernen Gauen", "tokens": ["Als", "aus", "nah", "und", "fer\u00b7nen", "Gau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJD", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutschland Boten ihm gesandt;", "tokens": ["Deutschland", "Bo\u00b7ten", "ihm", "ge\u00b7sandt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Als man Kampf beriet und Schlachten", "tokens": ["Als", "man", "Kampf", "be\u00b7riet", "und", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier im offnen Steingemach", "tokens": ["Hier", "im", "off\u00b7nen", "Stein\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und geschickt mit selbstgemachten", "tokens": ["Und", "ge\u00b7schickt", "mit", "selbst\u00b7ge\u00b7mach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6n'gen spielte hohes Schach;", "tokens": ["K\u00f6n'\u00b7gen", "spiel\u00b7te", "ho\u00b7hes", "Schach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Als ins Banner schwarzrotgolden", "tokens": ["Als", "ins", "Ban\u00b7ner", "schwarz\u00b7rot\u00b7gol\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frisch und frei der Rheinwind blies;", "tokens": ["Frisch", "und", "frei", "der", "Rhein\u00b7wind", "blies", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als man einen Trunkenbolden", "tokens": ["Als", "man", "ei\u00b7nen", "Trun\u00b7ken\u00b7bol\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach Verdienst vom Throne stie\u00df.", "tokens": ["Nach", "Ver\u00b7dienst", "vom", "Thro\u00b7ne", "stie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Fauler Wenzel! nimmer sehnen", "tokens": ["Fau\u00b7ler", "Wen\u00b7zel", "!", "nim\u00b7mer", "seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir uns heut nach dir zur\u00fcck!", "tokens": ["Wir", "uns", "heut", "nach", "dir", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch am K\u00f6nigsstuhl zu lehnen,", "tokens": ["Auch", "am", "K\u00f6\u00b7nigs\u00b7stuhl", "zu", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deucht uns kein besonder Gl\u00fcck!", "tokens": ["Deucht", "uns", "kein", "be\u00b7son\u00b7der", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Unterdessen, da bei Rhense", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", ",", "da", "bei", "Rhen\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er zu schaun ist wiederum,", "tokens": ["Er", "zu", "schaun", "ist", "wie\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nehmen willig, trotz der G\u00e4nse,", "tokens": ["Neh\u00b7men", "wil\u00b7lig", ",", "trotz", "der", "G\u00e4n\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir ihn als Augurium;", "tokens": ["Wir", "ihn", "als", "Au\u00b7gu\u00b7ri\u00b7um", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Als ein Zeichen, uns zum Frommen", "tokens": ["Als", "ein", "Zei\u00b7chen", ",", "uns", "zum", "From\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufgericht't am Rheinesstrand:", "tokens": ["Auf\u00b7ge\u00b7richt't", "am", "Rhei\u00b7nes\u00b7strand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du wirst zu Stuhle kommen", "tokens": ["Da\u00df", "du", "wirst", "zu", "Stuh\u00b7le", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonsten auch, o deutsches Land!", "tokens": ["Sons\u00b7ten", "auch", ",", "o", "deut\u00b7sches", "Land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}