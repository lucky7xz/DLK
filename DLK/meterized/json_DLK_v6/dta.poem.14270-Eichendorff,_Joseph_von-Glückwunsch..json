{"dta.poem.14270": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckwunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Brich der lustige Sonnenschein", "tokens": ["Brich", "der", "lus\u00b7ti\u00b7ge", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mit der Th\u00fcr Euch in's Haus hinein,", "tokens": ["Mit", "der", "Th\u00fcr", "Euch", "in's", "Haus", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Da\u00df alle Stuben so fr\u00fchlingshelle!", "tokens": ["Da\u00df", "al\u00b7le", "Stu\u00b7ben", "so", "fr\u00fch\u00b7lings\u00b7hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ADJA", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Engel auf des Hauses Schwelle", "tokens": ["Ein", "En\u00b7gel", "auf", "des", "Hau\u00b7ses", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit seinem Glanze s\u00e4ume", "tokens": ["Mit", "sei\u00b7nem", "Glan\u00b7ze", "s\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hof, Garten, Feld und B\u00e4ume,", "tokens": ["Hof", ",", "Gar\u00b7ten", ",", "Feld", "und", "B\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und geht die Sonne Abends aus,", "tokens": ["Und", "geht", "die", "Son\u00b7ne", "A\u00b7bends", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fchr' er die M\u00fcden mild nach Haus.", "tokens": ["F\u00fchr'", "er", "die", "M\u00fc\u00b7den", "mild", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}