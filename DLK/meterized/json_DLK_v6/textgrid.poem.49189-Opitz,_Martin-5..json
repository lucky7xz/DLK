{"textgrid.poem.49189": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht, meine Seufftzen, hin,", "tokens": ["Geht", ",", "mei\u00b7ne", "Seufft\u00b7zen", ",", "hin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erweichet derer Sinn", "tokens": ["Er\u00b7wei\u00b7chet", "de\u00b7rer", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PDS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die meinen Sinn mir plaget,", "tokens": ["Die", "mei\u00b7nen", "Sinn", "mir", "pla\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und reget unverwandt", "tokens": ["Und", "re\u00b7get", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Hertze von Demant,", "tokens": ["Ein", "Hert\u00b7ze", "von", "De\u00b7mant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das stets mich naget.", "tokens": ["Das", "stets", "mich", "na\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schaut, da\u00df ihr sie bewegt,", "tokens": ["Schaut", ",", "da\u00df", "ihr", "sie", "be\u00b7wegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die taube Sinnen tregt", "tokens": ["Die", "tau\u00b7be", "Sin\u00b7nen", "tregt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nichts von mir will wissen,", "tokens": ["Und", "nichts", "von", "mir", "will", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wiewohl die Threnen mir", "tokens": ["Wie\u00b7wohl", "die", "Thre\u00b7nen", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Au\u00df Wehmut f\u00fcr und f\u00fcr", "tokens": ["Au\u00df", "Weh\u00b7mut", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "KON", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Al\u00df Wasser fliessen.", "tokens": ["Al\u00df", "Was\u00b7ser", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ach, Feindinn meiner Brunst", "tokens": ["Ach", ",", "Fein\u00b7dinn", "mei\u00b7ner", "Brunst"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und aller Liebesgunst,", "tokens": ["Und", "al\u00b7ler", "Lie\u00b7bes\u00b7gunst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was gl\u00e4ntzet dein Gesichte", "tokens": ["Was", "gl\u00e4nt\u00b7zet", "dein", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Stralen weit und breit,", "tokens": ["Mit", "Stra\u00b7len", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann du durch H\u00e4rtigkeit", "tokens": ["Wann", "du", "durch", "H\u00e4r\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie machst zu nichte?", "tokens": ["Sie", "machst", "zu", "nich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "O meine Lust und Pein,", "tokens": ["O", "mei\u00b7ne", "Lust", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sch\u00f6n und auch grausam sein", "tokens": ["Sch\u00f6n", "und", "auch", "grau\u00b7sam", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADV", "ADJD", "VAINF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das schickt sich nicht zusammen;", "tokens": ["Das", "schickt", "sich", "nicht", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vermische dann dein Liecht,", "tokens": ["Ver\u00b7mi\u00b7sche", "dann", "dein", "Liecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das mir mein Hertze bricht", "tokens": ["Das", "mir", "mein", "Hert\u00b7ze", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "PPOSAT", "VVFIN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Liebesflammen.", "tokens": ["Mit", "Lie\u00b7bes\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}