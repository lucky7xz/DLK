{"textgrid.poem.48749": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "36. \u00dcber den Zusammenflu\u00df der Wolgen und Kamen, 20. Werste unter Samaren", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwimmt n\u00e4her zu uns her und stellt euch furchtsam nicht,", "tokens": ["Schwimmt", "n\u00e4\u00b7her", "zu", "uns", "her", "und", "stellt", "euch", "furcht\u00b7sam", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihr wilden F\u00fcrstinnen des \u00f6den ", "tokens": ["ihr", "wil\u00b7den", "F\u00fcrs\u00b7tin\u00b7nen", "des", "\u00f6\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-----+-", "measure": "dactylic.init"}, "line.3": {"text": "Kommt Nymphen an den Port, das Ufer dieses Randes", "tokens": ["Kommt", "Nym\u00b7phen", "an", "den", "Port", ",", "das", "U\u00b7fer", "die\u00b7ses", "Ran\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "APPR", "ART", "NE", "$,", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ist p\u00fcschig, k\u00fchl und frisch, da keine Sonne sticht.", "tokens": ["ist", "p\u00fc\u00b7schig", ",", "k\u00fchl", "und", "frisch", ",", "da", "kei\u00b7ne", "Son\u00b7ne", "sticht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kommt, schauet dieses Schiff, von dem ganz", "tokens": ["Kommt", ",", "schau\u00b7et", "die\u00b7ses", "Schiff", ",", "von", "dem", "ganz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PDAT", "NN", "$,", "APPR", "ART", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "auch di\u00df ist eine Zier der ersten meines Landes,", "tokens": ["auch", "di\u00df", "ist", "ei\u00b7ne", "Zier", "der", "ers\u00b7ten", "mei\u00b7nes", "Lan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "des treuen ", "tokens": ["des", "treu\u00b7en"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "das zwischen mir und ihm in Ewigkeit nicht bricht.", "tokens": ["das", "zwi\u00b7schen", "mir", "und", "ihm", "in", "E\u00b7wig\u00b7keit", "nicht", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "KON", "PPER", "APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und du, o Vater ", "tokens": ["Und", "du", ",", "o", "Va\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "FM", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "mit v\u00f6llern Kr\u00fcgen aus, da\u00df unsern f\u00f6hrnen Fu\u00df", "tokens": ["mit", "v\u00f6l\u00b7lern", "Kr\u00fc\u00b7gen", "aus", ",", "da\u00df", "un\u00b7sern", "f\u00f6hr\u00b7nen", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "kein blinder Sand halt' auf, kein falscher Grund vers\u00e4ume.", "tokens": ["kein", "blin\u00b7der", "Sand", "halt'", "auf", ",", "kein", "fal\u00b7scher", "Grund", "ver\u00b7s\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "beut auf gut Gl\u00fcck und Heil, setzt Wolfahrt ein und schreit,", "tokens": ["beut", "auf", "gut", "Gl\u00fcck", "und", "Heil", ",", "setzt", "Wol\u00b7fahrt", "ein", "und", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "NN", "KON", "NN", "$,", "VVFIN", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df Anfall, Mord und Raub ihr beides Ufer r\u00e4ume.", "tokens": ["da\u00df", "An\u00b7fall", ",", "Mord", "und", "Raub", "ihr", "bei\u00b7des", "U\u00b7fer", "r\u00e4u\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}