{"dta.poem.21373": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kaiser fiel mir in die Red:", "tokens": ["Der", "Kai\u00b7ser", "fiel", "mir", "in", "die", "Red", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eschweig still, von deiner Maschine", "tokens": ["\u201e", "schweig", "still", ",", "von", "dei\u00b7ner", "Ma\u00b7schi\u00b7ne"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "PTKVZ", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Will ich nichts wissen, Gott bewahr\u2019,", "tokens": ["Will", "ich", "nichts", "wis\u00b7sen", ",", "Gott", "be\u00b7wahr'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "VVINF", "$,", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich mich ihrer bediene!", "tokens": ["Da\u00df", "ich", "mich", "ih\u00b7rer", "be\u00b7die\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201eder K\u00f6nig und die K\u00f6niginn!", "tokens": ["\u201e", "der", "K\u00f6\u00b7nig", "und", "die", "K\u00f6\u00b7ni\u00b7ginn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschnallt! an einem Brette!", "tokens": ["Ge\u00b7schnallt", "!", "an", "ei\u00b7nem", "Bret\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist ja gegen allen Respekt", "tokens": ["Das", "ist", "ja", "ge\u00b7gen", "al\u00b7len", "Res\u00b7pekt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und alle Etiquette!", "tokens": ["Und", "al\u00b7le", "E\u00b7ti\u00b7quet\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201eund du, wer bist du, da\u00df du es wagst", "tokens": ["\u201e", "und", "du", ",", "wer", "bist", "du", ",", "da\u00df", "du", "es", "wagst"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "PPER", "$,", "PWS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mich so vertraulich zu dutzen?", "tokens": ["Mich", "so", "ver\u00b7trau\u00b7lich", "zu", "dut\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Warte, du B\u00fcrschchen, ich werde dir schon", "tokens": ["War\u00b7te", ",", "du", "B\u00fcr\u00b7schchen", ",", "ich", "wer\u00b7de", "dir", "schon"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die kecken Fl\u00fcgel stutzen!", "tokens": ["Die", "ke\u00b7cken", "Fl\u00fc\u00b7gel", "stut\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201ees regt mir die innerste Galle auf,", "tokens": ["\u201e", "es", "regt", "mir", "die", "in\u00b7ners\u00b7te", "Gal\u00b7le", "auf", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich dich h\u00f6re sprechen,", "tokens": ["Wenn", "ich", "dich", "h\u00f6\u00b7re", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Odem schon ist Hochverrath", "tokens": ["Dein", "O\u00b7dem", "schon", "ist", "Hoch\u00b7ver\u00b7rath"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Majest\u00e4tsverbrechen!\u201c", "tokens": ["Und", "Ma\u00b7jes\u00b7t\u00e4ts\u00b7ver\u00b7bre\u00b7chen", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}