{"textgrid.poem.48074": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Spaziergang", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber der tauigen Wiese liegt", "tokens": ["Ue\u00b7ber", "der", "tau\u00b7i\u00b7gen", "Wie\u00b7se", "liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "ein feiner, d\u00e4mmernder Nebelstreif;", "tokens": ["ein", "fei\u00b7ner", ",", "d\u00e4m\u00b7mern\u00b7der", "Ne\u00b7bel\u00b7streif", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "um deine tr\u00e4umende Stirne schmiegt", "tokens": ["um", "dei\u00b7ne", "tr\u00e4u\u00b7men\u00b7de", "Stir\u00b7ne", "schmiegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "sich ein schmaler sonniger Reif.", "tokens": ["sich", "ein", "schma\u00b7ler", "son\u00b7ni\u00b7ger", "Reif", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und k\u00fc\u00dft die Sonne den Nebel bleich,", "tokens": ["Und", "k\u00fc\u00dft", "die", "Son\u00b7ne", "den", "Ne\u00b7bel", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "dann w\u00e4chst aus der Wiese ein goldnes Haus,", "tokens": ["dann", "w\u00e4chst", "aus", "der", "Wie\u00b7se", "ein", "gold\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "dann breitet ein bl\u00fchendes K\u00f6nigreich", "tokens": ["dann", "brei\u00b7tet", "ein", "bl\u00fc\u00b7hen\u00b7des", "K\u00f6\u00b7nig\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "sich bis an die blauenden Berge aus.", "tokens": ["sich", "bis", "an", "die", "blau\u00b7en\u00b7den", "Ber\u00b7ge", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Eine flammende Fahne weht vom Turm,", "tokens": ["Ei\u00b7ne", "flam\u00b7men\u00b7de", "Fah\u00b7ne", "weht", "vom", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "aus den Hallen klingt es wie Jubelschrein,", "tokens": ["aus", "den", "Hal\u00b7len", "klingt", "es", "wie", "Ju\u00b7bel\u00b7schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u2013 ein Sonnenrausch, ein Freudensturm", "tokens": ["\u2013", "ein", "Son\u00b7nen\u00b7rausch", ",", "ein", "Freu\u00b7den\u00b7sturm"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "bricht \u00fcber das harrende Land herein.", "tokens": ["bricht", "\u00fc\u00b7ber", "das", "har\u00b7ren\u00b7de", "Land", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Hoch \u00fcber die H\u00e4upter der Berge fliegt,", "tokens": ["Hoch", "\u00fc\u00b7ber", "die", "H\u00e4up\u00b7ter", "der", "Ber\u00b7ge", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "im Blau verzitternd, ein blasser Streif . . .", "tokens": ["im", "Blau", "ver\u00b7zit\u00b7ternd", ",", "ein", "blas\u00b7ser", "Streif", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "um deine leuchtende Stirne liegt", "tokens": ["um", "dei\u00b7ne", "leuch\u00b7ten\u00b7de", "Stir\u00b7ne", "liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "glitzernd der goldene Reif.", "tokens": ["glit\u00b7zernd", "der", "gol\u00b7de\u00b7ne", "Reif", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}