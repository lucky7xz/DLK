{"textgrid.poem.59444": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein zartes V\u00f6gelein,", "tokens": ["Es", "war", "ein", "zar\u00b7tes", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sa\u00df in Lieb gefangen,", "tokens": ["Das", "sa\u00df", "in", "Lieb", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Knabe hegt' und pflegt' sich's fein", "tokens": ["Ein", "Kna\u00b7be", "hegt'", "und", "pflegt'", "sich's", "fein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PIS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl hinter goldnen Stangen.", "tokens": ["Wohl", "hin\u00b7ter", "gold\u00b7nen", "Stan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und drau\u00dfen h\u00f6rt's auf gr\u00fcnem Plan", "tokens": ["Und", "drau\u00b7\u00dfen", "h\u00f6rt's", "auf", "gr\u00fc\u00b7nem", "Plan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verschiedner V\u00f6gel Weisen,", "tokens": ["Ver\u00b7schied\u00b7ner", "V\u00f6\u00b7gel", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sah Tag und Nacht den Knaben an,", "tokens": ["Sah", "Tag", "und", "Nacht", "den", "Kna\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mocht nicht mit ihnen reisen.", "tokens": ["Mocht", "nicht", "mit", "ih\u00b7nen", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und als der Fr\u00fchling weit und breit", "tokens": ["Und", "als", "der", "Fr\u00fch\u00b7ling", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von neuem schien und schw\u00e4rmte,", "tokens": ["Von", "neu\u00b7em", "schien", "und", "schw\u00e4rm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da tat dem Knaben 's V\u00f6glein leid,", "tokens": ["Da", "tat", "dem", "Kna\u00b7ben", "'s", "V\u00f6\u00b7glein", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df es kein Strahl erw\u00e4rmte.", "tokens": ["Da\u00df", "es", "kein", "Strahl", "er\u00b7w\u00e4rm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da nahm er aus dem stillen Haus", "tokens": ["Da", "nahm", "er", "aus", "dem", "stil\u00b7len", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das V\u00f6glein fromm und treue,", "tokens": ["Das", "V\u00f6\u00b7glein", "fromm", "und", "treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schweift' mit ihm durchs Feld hinaus", "tokens": ["Und", "schweift'", "mit", "ihm", "durchs", "Feld", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins himmelblaue Freie.", "tokens": ["Ins", "him\u00b7mel\u00b7blau\u00b7e", "Frei\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er setzt' es vor sich auf die Hand,", "tokens": ["Er", "setzt'", "es", "vor", "sich", "auf", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wend't und putzt sich's feine,", "tokens": ["Da", "wend't", "und", "putzt", "sich's", "fei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In bunten Farben spielt' und brannt", "tokens": ["In", "bun\u00b7ten", "Far\u00b7ben", "spielt'", "und", "brannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Kleid im Sonnenscheine.", "tokens": ["Sein", "Kleid", "im", "Son\u00b7nen\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch aus dem Wald ein Singen rief,", "tokens": ["Doch", "aus", "dem", "Wald", "ein", "Sin\u00b7gen", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bunt' V\u00f6glein ziehn und reisen,", "tokens": ["Bunt'", "V\u00f6\u00b7glein", "ziehn", "und", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das lockt so hell, das lockt so tief", "tokens": ["Das", "lockt", "so", "hell", ",", "das", "lockt", "so", "tief"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "PDS", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In wunders\u00fc\u00dfen Weisen.", "tokens": ["In", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfen", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das V\u00f6glein frisch die Fl\u00fcgel r\u00fchrt \u2013", "tokens": ["Das", "V\u00f6\u00b7glein", "frisch", "die", "Fl\u00fc\u00b7gel", "r\u00fchrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ruft: \u00bbKommst du nicht balde?\u00ab \u2013", "tokens": ["Es", "ruft", ":", "\u00bb", "Kommst", "du", "nicht", "bal\u00b7de", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das hat das V\u00f6gelein verf\u00fchrt,", "tokens": ["Das", "hat", "das", "V\u00f6\u00b7ge\u00b7lein", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fort flog's zum gr\u00fcnen Walde \u2013", "tokens": ["Fort", "flo\u00b7g's", "zum", "gr\u00fc\u00b7nen", "Wal\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun mu\u00df der Knabe einsam gehn,", "tokens": ["Nun", "mu\u00df", "der", "Kna\u00b7be", "ein\u00b7sam", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Klagt \u00fcber Tal und H\u00fcgel:", "tokens": ["Klagt", "\u00fc\u00b7ber", "Tal", "und", "H\u00fc\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbs\u00fc\u00df' Lieb, s\u00fc\u00df' Lieb, wie bist du sch\u00f6n:", "tokens": ["\u00bb", "s\u00fc\u00df'", "Lieb", ",", "s\u00fc\u00df'", "Lieb", ",", "wie", "bist", "du", "sch\u00f6n", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "NN", "$,", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, h\u00e4ttst du keine Fl\u00fcgel!\u00ab \u2013", "tokens": ["Ach", ",", "h\u00e4ttst", "du", "kei\u00b7ne", "Fl\u00fc\u00b7gel", "!", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "PIAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}