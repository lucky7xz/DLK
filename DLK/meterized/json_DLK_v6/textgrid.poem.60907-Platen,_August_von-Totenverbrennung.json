{"textgrid.poem.60907": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Totenverbrennung", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heilige Flammen, o kehrt, kehrt wieder zur\u00fcck, und gereinigt", "tokens": ["Hei\u00b7li\u00b7ge", "Flam\u00b7men", ",", "o", "kehrt", ",", "kehrt", "wie\u00b7der", "zu\u00b7r\u00fcck", ",", "und", "ge\u00b7rei\u00b7nigt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "FM", "VVFIN", "$,", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "VVPP"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Werde des Tods hinfort schn\u00f6de verpestende Luft!", "tokens": ["Wer\u00b7de", "des", "Tods", "hin\u00b7fort", "schn\u00f6\u00b7de", "ver\u00b7pes\u00b7ten\u00b7de", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "M\u00f6ge zu Staub der Bestattende wieder die Leiche des Freundes", "tokens": ["M\u00f6\u00b7ge", "zu", "Staub", "der", "Be\u00b7stat\u00b7ten\u00b7de", "wie\u00b7der", "die", "Lei\u00b7che", "des", "Freun\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "ART", "NN", "ADV", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Sanft aufl\u00f6sen, und sanft sink in die Asche der Schmerz!", "tokens": ["Sanft", "auf\u00b7l\u00f6\u00b7sen", ",", "und", "sanft", "sink", "in", "die", "A\u00b7sche", "der", "Schmerz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Wieder in reinlicher Urne, zun\u00e4chst der bev\u00f6lkerten Wohnung,", "tokens": ["Wie\u00b7der", "in", "rein\u00b7li\u00b7cher", "Ur\u00b7ne", ",", "zu\u00b7n\u00e4chst", "der", "be\u00b7v\u00f6l\u00b7ker\u00b7ten", "Woh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Ruhe der k\u00f6stliche Rest aller Geliebten um uns!", "tokens": ["Ru\u00b7he", "der", "k\u00f6st\u00b7li\u00b7che", "Rest", "al\u00b7ler", "Ge\u00b7lieb\u00b7ten", "um", "uns", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}