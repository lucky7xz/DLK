{"textgrid.poem.38516": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Wasserfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wiegend gleitet der Kahn \u00fcber der leisen Fluth", "tokens": ["Wie\u00b7gend", "glei\u00b7tet", "der", "Kahn", "\u00fc\u00b7ber", "der", "lei\u00b7sen", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Sanft err\u00f6thendes Blau, schwebt im Najadentanz", "tokens": ["Sanft", "er\u00b7r\u00f6\u00b7then\u00b7des", "Blau", ",", "schwebt", "im", "Na\u00b7ja\u00b7den\u00b7tanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Winzerh\u00fctten vor\u00fcber,", "tokens": ["Win\u00b7zer\u00b7h\u00fct\u00b7ten", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und vergoldeten Erlenreihn!", "tokens": ["Und", "ver\u00b7gol\u00b7de\u00b7ten", "Er\u00b7len\u00b7reihn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Freude l\u00e4chelt des Tags herrlicher Niedergang;", "tokens": ["Freu\u00b7de", "l\u00e4\u00b7chelt", "des", "Tags", "herr\u00b7li\u00b7cher", "Nie\u00b7der\u00b7gang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Freude girret im Forst, fl\u00f6tet im Bl\u00fcthenstrauch;", "tokens": ["Freu\u00b7de", "gir\u00b7ret", "im", "Forst", ",", "fl\u00f6\u00b7tet", "im", "Bl\u00fc\u00b7then\u00b7strauch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Freude jauchzen die H\u00fcgel;", "tokens": ["Freu\u00b7de", "jauch\u00b7zen", "die", "H\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Freude jubelt im Wiederhall!", "tokens": ["Freu\u00b7de", "ju\u00b7belt", "im", "Wie\u00b7der\u00b7hall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Fl\u00fcgle rascher den Kahn, nervichter J\u00fcnglingsarm!", "tokens": ["Fl\u00fcg\u00b7le", "ra\u00b7scher", "den", "Kahn", ",", "ner\u00b7vich\u00b7ter", "J\u00fcng\u00b7lings\u00b7arm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Seht! von Lauben umgr\u00fcnt, unter der Linde Schirm,", "tokens": ["Seht", "!", "von", "Lau\u00b7ben", "um\u00b7gr\u00fcnt", ",", "un\u00b7ter", "der", "Lin\u00b7de", "Schirm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "APPR", "NN", "VVPP", "$,", "APPR", "ART", "NE", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Winkt, mit wehendem Kranze,", "tokens": ["Winkt", ",", "mit", "we\u00b7hen\u00b7dem", "Kran\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schon das l\u00e4ndliche Sorgenfrei!", "tokens": ["Schon", "das", "l\u00e4nd\u00b7li\u00b7che", "Sor\u00b7gen\u00b7frei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Herrsch' als K\u00f6nigin da, freundliche Gegenwart!", "tokens": ["Herr\u00b7sch'", "als", "K\u00f6\u00b7ni\u00b7gin", "da", ",", "freund\u00b7li\u00b7che", "Ge\u00b7gen\u00b7wart", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dir ert\u00f6n' unser Lied, str\u00f6m' unser Opfertrank!", "tokens": ["Dir", "er\u00b7t\u00f6n'", "un\u00b7ser", "Lied", ",", "str\u00f6m'", "un\u00b7ser", "Op\u00b7fer\u00b7trank", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Halt' uns liebend umschlungen,", "tokens": ["Halt'", "uns", "lie\u00b7bend", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bis Aurore den Zauber l\u00f6st!", "tokens": ["Bis", "Au\u00b7ro\u00b7re", "den", "Zau\u00b7ber", "l\u00f6st", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}