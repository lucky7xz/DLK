{"textgrid.poem.59517": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Deutsches Kriegslied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frischauf! Es ruft das Vaterland", "tokens": ["Frisc\u00b7hauf", "!", "Es", "ruft", "das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00e4nner in die Schlacht.", "tokens": ["Die", "M\u00e4n\u00b7ner", "in", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frischauf! Zu d\u00e4mpfen Trug und Schand'", "tokens": ["Frisc\u00b7hauf", "!", "Zu", "d\u00e4mp\u00b7fen", "Trug", "und", "Schand'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PTKZU", "VVINF", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heran mit Macht! mit Macht!", "tokens": ["He\u00b7ran", "mit", "Macht", "!", "mit", "Macht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$.", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Heran! und braucht den M\u00e4nnerleib,", "tokens": ["He\u00b7ran", "!", "und", "braucht", "den", "M\u00e4n\u00b7ner\u00b7leib", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wozu ihn Gott gebaut,", "tokens": ["Wo\u00b7zu", "ihn", "Gott", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Schirm der Jungfrau und dem Weib,", "tokens": ["Zum", "Schirm", "der", "Jung\u00b7frau", "und", "dem", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem S\u00e4ugling und der Braut!", "tokens": ["Dem", "S\u00e4ug\u00b7ling", "und", "der", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn ein Tyrann mit L\u00fcgenwort", "tokens": ["Denn", "ein", "Ty\u00b7rann", "mit", "L\u00fc\u00b7gen\u00b7wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Strick und Henkerschwert", "tokens": ["Und", "Strick", "und", "Hen\u00b7ker\u00b7schwert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00dcbt in dem Vaterlande Mord", "tokens": ["\u00dcbt", "in", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "Mord"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und sch\u00e4ndet Thron und Herd", "tokens": ["Und", "sch\u00e4n\u00b7det", "Thron", "und", "Herd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und will, so weit die Sonne scheint,", "tokens": ["Und", "will", ",", "so", "weit", "die", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der einz'ge K\u00f6nig sein:", "tokens": ["Der", "einz'\u00b7ge", "K\u00f6\u00b7nig", "sein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Menschenfeind, ein Freiheitsfeind,", "tokens": ["Ein", "Men\u00b7schen\u00b7feind", ",", "ein", "Frei\u00b7heits\u00b7feind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Spricht er: Die Welt ist mein.", "tokens": ["Spricht", "er", ":", "Die", "Welt", "ist", "mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Verh\u00fct' es Gott und Hermanns Blut!", "tokens": ["Ver\u00b7h\u00fct'", "es", "Gott", "und", "Her\u00b7manns", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie werde solches wahr!", "tokens": ["Nie", "wer\u00b7de", "sol\u00b7ches", "wahr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erwache, alter deutscher Mut,", "tokens": ["Er\u00b7wa\u00b7che", ",", "al\u00b7ter", "deut\u00b7scher", "Mut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Recht und Licht gebar!", "tokens": ["Der", "Recht", "und", "Licht", "ge\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Erwache! Sonder Rast und Ruh'", "tokens": ["Er\u00b7wa\u00b7che", "!", "Son\u00b7der", "Rast", "und", "Ruh'"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlag jeden, der dir droht,", "tokens": ["Schlag", "je\u00b7den", ",", "der", "dir", "droht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ruf ihm deutsche Losung zu:", "tokens": ["Und", "ruf", "ihm", "deut\u00b7sche", "Lo\u00b7sung", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieg gelt' es oder Tod!", "tokens": ["Sieg", "gelt'", "es", "o\u00b7der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hinein! Denn sch\u00f6ner ist kein Tod", "tokens": ["Hin\u00b7ein", "!", "Denn", "sch\u00f6\u00b7ner", "ist", "kein", "Tod"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KON", "ADJD", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Tod f\u00fcrs Vaterland,", "tokens": ["Als", "Tod", "f\u00fcrs", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ewig bleibt des Sklaven Not,", "tokens": ["Doch", "e\u00b7wig", "bleibt", "des", "Skla\u00b7ven", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unsterblich seine Schand'.", "tokens": ["U\u00b7nsterb\u00b7lich", "sei\u00b7ne", "Schand'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hinein! Kanonendonnerklang", "tokens": ["Hin\u00b7ein", "!", "Ka\u00b7no\u00b7nen\u00b7don\u00b7ner\u00b7klang"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hallt zu dem blut'gen Kampf,", "tokens": ["Hallt", "zu", "dem", "blut'\u00b7gen", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Die Fahnen wehn die Reihn entlang,", "tokens": ["Die", "Fah\u00b7nen", "wehn", "die", "Reihn", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Rosse wirbeln Dampf.", "tokens": ["Die", "Ros\u00b7se", "wir\u00b7beln", "Dampf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Hinein! Hinein! Wo Eisen klirrt,", "tokens": ["Hin\u00b7ein", "!", "Hin\u00b7ein", "!", "Wo", "Ei\u00b7sen", "klirrt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "$.", "PWAV", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wo Tod in Kugeln fleucht!", "tokens": ["Wo", "Tod", "in", "Ku\u00b7geln", "fleucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Mann, der heut begraben wird,", "tokens": ["Dem", "Mann", ",", "der", "heut", "be\u00b7gra\u00b7ben", "wird", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem sei die Erde leicht!", "tokens": ["Dem", "sei", "die", "Er\u00b7de", "leicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}