{"textgrid.poem.65631": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Mainottin", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe sieben S\u00f6hne aus meiner Brust ges\u00e4ugt,", "tokens": ["Ich", "ha\u00b7be", "sie\u00b7ben", "S\u00f6h\u00b7ne", "aus", "mei\u00b7ner", "Brust", "ge\u00b7s\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich habe sieben S\u00f6hnen das heil'ge Schwert gereicht,", "tokens": ["Ich", "ha\u00b7be", "sie\u00b7ben", "S\u00f6h\u00b7nen", "das", "heil'\u00b7ge", "Schwert", "ge\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das Schwert f\u00fcr unsern Glauben, f\u00fcr Freiheit, Ehr' und Recht \u2013", "tokens": ["Das", "Schwert", "f\u00fcr", "un\u00b7sern", "Glau\u00b7ben", ",", "f\u00fcr", "Frei\u00b7heit", ",", "Ehr'", "und", "Recht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Heil mir, von meinen S\u00f6hnen ist Keiner mehr ein Knecht!", "tokens": ["Heil", "mir", ",", "von", "mei\u00b7nen", "S\u00f6h\u00b7nen", "ist", "Kei\u00b7ner", "mehr", "ein", "Knecht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PPOSAT", "NN", "VAFIN", "PIS", "ADV", "ART", "NN", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sie sind zur Schlacht gezogen mit freudig wildem Muth \u2013", "tokens": ["Sie", "sind", "zur", "Schlacht", "ge\u00b7zo\u00b7gen", "mit", "freu\u00b7dig", "wil\u00b7dem", "Muth", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Heil mir, in ihren Adern flie\u00dft noch spartanisch Blut!", "tokens": ["Heil", "mir", ",", "in", "ih\u00b7ren", "A\u00b7dern", "flie\u00dft", "noch", "spar\u00b7ta\u00b7nisch", "Blut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Und als sie von mir schieden, das Herz ward mir nicht schwer,", "tokens": ["Und", "als", "sie", "von", "mir", "schie\u00b7den", ",", "das", "Herz", "ward", "mir", "nicht", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Ich sprach: Frei kehrt ihr wieder, frei oder nimmermehr!", "tokens": ["Ich", "sprach", ":", "Frei", "kehrt", "ihr", "wie\u00b7der", ",", "frei", "o\u00b7der", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VVFIN", "PPER", "ADV", "$,", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Ihr M\u00fctter der Mainotten, kommt, la\u00dft uns suchen gehn,", "tokens": ["Ihr", "M\u00fct\u00b7ter", "der", "Mai\u00b7not\u00b7ten", ",", "kommt", ",", "la\u00dft", "uns", "su\u00b7chen", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "VVFIN", "$,", "VVIMP", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Ob nicht von Sparta's Tr\u00fcmmern wir eine Spur ersp\u00e4hn;", "tokens": ["Ob", "nicht", "von", "Spar\u00b7ta's", "Tr\u00fcm\u00b7mern", "wir", "ei\u00b7ne", "Spur", "er\u00b7sp\u00e4hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "NE", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Da woll'n wir Steine sammeln, f\u00fcr unsre Hand gerecht,", "tokens": ["Da", "woll'n", "wir", "Stei\u00b7ne", "sam\u00b7meln", ",", "f\u00fcr", "uns\u00b7re", "Hand", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Mit hartem Gru\u00df zu gr\u00fc\u00dfen den ersten feigen Knecht,", "tokens": ["Mit", "har\u00b7tem", "Gru\u00df", "zu", "gr\u00fc\u00b7\u00dfen", "den", "ers\u00b7ten", "fei\u00b7gen", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Der ohne Blut und Wunde besiegt nach Hause kehrt,", "tokens": ["Der", "oh\u00b7ne", "Blut", "und", "Wun\u00b7de", "be\u00b7siegt", "nach", "Hau\u00b7se", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Und keinen Kranz gewonnen f\u00fcr seiner Mutter Herd!", "tokens": ["Und", "kei\u00b7nen", "Kranz", "ge\u00b7won\u00b7nen", "f\u00fcr", "sei\u00b7ner", "Mut\u00b7ter", "Herd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}