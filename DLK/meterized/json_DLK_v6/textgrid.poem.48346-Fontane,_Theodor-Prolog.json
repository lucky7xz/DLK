{"textgrid.poem.48346": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Prolog", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zweihundert Jahre, da\u00df wir hier zu Land", "tokens": ["Zwei\u00b7hun\u00b7dert", "Jah\u00b7re", ",", "da\u00df", "wir", "hier", "zu", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Obdach fanden, Freistatt f\u00fcr den Glauben", "tokens": ["Ein", "Ob\u00b7dach", "fan\u00b7den", ",", "Freis\u00b7tatt", "f\u00fcr", "den", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Zuflucht vor Bedr\u00e4ngnis der Gewissen.", "tokens": ["Und", "Zu\u00b7flucht", "vor", "Be\u00b7dr\u00e4ng\u00b7nis", "der", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein hochgemuter F\u00fcrst, so frei wie fromm,", "tokens": ["Ein", "hoch\u00b7ge\u00b7mu\u00b7ter", "F\u00fcrst", ",", "so", "frei", "wie", "fromm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Empfing uns hier, und wie der F\u00fcrst des Landes", "tokens": ["Emp\u00b7fing", "uns", "hier", ",", "und", "wie", "der", "F\u00fcrst", "des", "Lan\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Empfing uns auch sein Volk. Kein Neid ward wach,", "tokens": ["Emp\u00b7fing", "uns", "auch", "sein", "Volk", ".", "Kein", "Neid", "ward", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nicht Eifersucht \u2013 man \u00f6ffnete das Tor uns", "tokens": ["Nicht", "Ei\u00b7fer\u00b7sucht", "\u2013", "man", "\u00f6ff\u00b7ne\u00b7te", "das", "Tor", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$(", "PIS", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und hie\u00df als Glaubensbr\u00fcder uns willkommen.", "tokens": ["Und", "hie\u00df", "als", "Glau\u00b7bens\u00b7br\u00fc\u00b7der", "uns", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So ward die Freistatt bald zur Heimatsst\u00e4tte,", "tokens": ["So", "ward", "die", "Freis\u00b7tatt", "bald", "zur", "Hei\u00b7mats\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Zur St\u00e4tte neuer Lieb', und was seitdem", "tokens": ["Zur", "St\u00e4t\u00b7te", "neu\u00b7er", "Lieb'", ",", "und", "was", "seit\u00b7dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,", "KON", "PWS", "PPOSAT"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Durch Gottes Ratschlu\u00df dieses Land erfahren,", "tokens": ["Durch", "Got\u00b7tes", "Rat\u00b7schlu\u00df", "die\u00b7ses", "Land", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wir lebten's mit, sein Leid war unser Leid,", "tokens": ["Wir", "leb\u00b7ten's", "mit", ",", "sein", "Leid", "war", "un\u00b7ser", "Leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und was es freute, war auch unsre Freude.", "tokens": ["Und", "was", "es", "freu\u00b7te", ",", "war", "auch", "uns\u00b7re", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wohl pflegten wir das Eigne, der Gemeinde", "tokens": ["Wohl", "pfleg\u00b7ten", "wir", "das", "Eig\u00b7ne", ",", "der", "Ge\u00b7mein\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Gedeihn und Wachstum blieb uns Herzenssache,", "tokens": ["Ge\u00b7deihn", "und", "Wachs\u00b7tum", "blieb", "uns", "Her\u00b7zens\u00b7sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Doch nie verga\u00dfen wir der Pflicht und Sorge,", "tokens": ["Doch", "nie", "ver\u00b7ga\u00b7\u00dfen", "wir", "der", "Pflicht", "und", "Sor\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Da\u00df, was nur ", "tokens": ["Da\u00df", ",", "was", "nur"], "token_info": ["word", "punct", "word", "word"], "pos": ["KOUS", "$,", "PRELS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Mit flei\u00df'ger Hand, in allem wohl erfahren,", "tokens": ["Mit", "flei\u00df'\u00b7ger", "Hand", ",", "in", "al\u00b7lem", "wohl", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Was \u00e4lterer Kultur und w\u00e4rm'rer Sonne", "tokens": ["Was", "\u00e4l\u00b7te\u00b7rer", "Kul\u00b7tur", "und", "w\u00e4r\u00b7m'\u00b7rer", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Daheim entspro\u00df und einem reich'ren Lande \u2013", "tokens": ["Da\u00b7heim", "ent\u00b7spro\u00df", "und", "ei\u00b7nem", "reich'\u00b7ren", "Lan\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "So wirkten wir.", "tokens": ["So", "wirk\u00b7ten", "wir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Doch unser Tun zu r\u00fchmen,", "tokens": ["Doch", "un\u00b7ser", "Tun", "zu", "r\u00fch\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Es ist nicht ", "tokens": ["Es", "ist", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "Heut ziemt's uns nur zu huld'gen und zu danken.", "tokens": ["Heut", "ziemt's", "uns", "nur", "zu", "huld'\u00b7gen", "und", "zu", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und dieser Dank, was lieh' ihm gr\u00f6\u00df're Kraft", "tokens": ["Und", "die\u00b7ser", "Dank", ",", "was", "lieh'", "ihm", "gr\u00f6\u00df'\u00b7re", "Kraft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "PWS", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Inbrunst als ein R\u00fcckblick auf das Leid,", "tokens": ["Und", "In\u00b7brunst", "als", "ein", "R\u00fcck\u00b7blick", "auf", "das", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das einst aus unsrer Heimat uns vertrieben?", "tokens": ["Das", "einst", "aus", "uns\u00b7rer", "Hei\u00b7mat", "uns", "ver\u00b7trie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erklinge denn, Musik, und f\u00fchr' herauf,", "tokens": ["Er\u00b7klin\u00b7ge", "denn", ",", "Mu\u00b7sik", ",", "und", "f\u00fchr'", "her\u00b7auf", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Widerspiel zu dieser Stunde Gl\u00fcck,", "tokens": ["Im", "Wi\u00b7der\u00b7spiel", "zu", "die\u00b7ser", "Stun\u00b7de", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Uns Bilder aus der Zeit der ", "tokens": ["Uns", "Bil\u00b7der", "aus", "der", "Zeit", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}