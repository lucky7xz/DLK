{"textgrid.poem.63652": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Jugend, scheidest du?", "tokens": ["Sch\u00f6\u00b7ne", "Ju\u00b7gend", ",", "schei\u00b7dest", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl! du bliebst mir lange treu.", "tokens": ["Wohl", "!", "du", "bliebst", "mir", "lan\u00b7ge", "treu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich dir im Arm geruht,", "tokens": ["Weil", "ich", "dir", "im", "Arm", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schien die Welt mir lieb und gut,", "tokens": ["Schien", "die", "Welt", "mir", "lieb", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kampf und Ruh", "tokens": ["Kampf", "und", "Ruh"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Immer freudig, immer neu.", "tokens": ["Im\u00b7mer", "freu\u00b7dig", ",", "im\u00b7mer", "neu", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht entwichst du \u00fcber Nacht,", "tokens": ["Nicht", "ent\u00b7wichst", "du", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie uns Dirnengunst verl\u00e4\u00dft,", "tokens": ["Wie", "uns", "Dir\u00b7nen\u00b7gunst", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heischtest z\u00f6gernd nur zur\u00fcck", "tokens": ["Heischtest", "z\u00f6\u00b7gernd", "nur", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gab' um Gabe, Gl\u00fcck um Gl\u00fcck,", "tokens": ["Gab'", "um", "Ga\u00b7be", ",", "Gl\u00fcck", "um", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit Macht", "tokens": ["Und", "mit", "Macht"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Hielt ich noch die Fliehnde fest.", "tokens": ["Hielt", "ich", "noch", "die", "Fliehn\u00b7de", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ein feines Lieb sich kr\u00e4nkt,", "tokens": ["Wie", "ein", "fei\u00b7nes", "Lieb", "sich", "kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das vom Liebsten scheiden soll:", "tokens": ["Das", "vom", "Liebs\u00b7ten", "schei\u00b7den", "soll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer noch ein letzter Ku\u00df,", "tokens": ["Im\u00b7mer", "noch", "ein", "letz\u00b7ter", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch ein Seufzer, noch ein Gru\u00df \u2013", "tokens": ["Noch", "ein", "Seuf\u00b7zer", ",", "noch", "ein", "Gru\u00df", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fern noch schwenkt", "tokens": ["Fern", "noch", "schwenkt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Sie ihr T\u00fcchlein tr\u00e4nenvoll.", "tokens": ["Sie", "ihr", "T\u00fcch\u00b7lein", "tr\u00e4\u00b7nen\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, und nun dem Blick entflohn,", "tokens": ["Ach", ",", "und", "nun", "dem", "Blick", "ent\u00b7flohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trifft mich noch der Stimme Klang.", "tokens": ["Trifft", "mich", "noch", "der", "Stim\u00b7me", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweig! O locke nicht von fern!", "tokens": ["Schweig", "!", "O", "lo\u00b7cke", "nicht", "von", "fern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NE", "VVFIN", "PTKNEG", "APPR", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh, im Blau der Abendstern", "tokens": ["Sieh", ",", "im", "Blau", "der", "A\u00b7bends\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Schimmert schon \u2013", "tokens": ["Schim\u00b7mert", "schon", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Um den Schlaf bringt dein Gesang!", "tokens": ["Um", "den", "Schlaf", "bringt", "dein", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}