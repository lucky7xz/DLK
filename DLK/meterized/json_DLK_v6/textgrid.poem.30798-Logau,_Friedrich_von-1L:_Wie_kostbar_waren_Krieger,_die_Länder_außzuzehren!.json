{"textgrid.poem.30798": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie kostbar waren Krieger, die L\u00e4nder au\u00dfzuzehren!", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kostbar waren Krieger, die L\u00e4nder au\u00dfzuzehren!", "tokens": ["Wie", "kost\u00b7bar", "wa\u00b7ren", "Krie\u00b7ger", ",", "die", "L\u00e4n\u00b7der", "au\u00df\u00b7zu\u00b7zeh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie kostbar ist Gesinde, die L\u00e4nder zu ern\u00e4hren!", "tokens": ["Wie", "kost\u00b7bar", "ist", "Ge\u00b7sin\u00b7de", ",", "die", "L\u00e4n\u00b7der", "zu", "er\u00b7n\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was ist die gantze Wirthschafft? Ein kostbares Beschweren.", "tokens": ["Was", "ist", "die", "gant\u00b7ze", "Wirth\u00b7schafft", "?", "Ein", "kost\u00b7ba\u00b7res", "Be\u00b7schwe\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}