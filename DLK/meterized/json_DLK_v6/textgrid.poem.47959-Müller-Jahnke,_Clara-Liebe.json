{"textgrid.poem.47959": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In kindlicher Seele", "tokens": ["In", "kind\u00b7li\u00b7cher", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "erd\u00e4mmert die Liebe,", "tokens": ["er\u00b7d\u00e4m\u00b7mert", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "wie Gr\u00fcnes der Erde", "tokens": ["wie", "Gr\u00fc\u00b7nes", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "im Fr\u00fchling entkeimt.", "tokens": ["im", "Fr\u00fch\u00b7ling", "ent\u00b7keimt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Im Herzen der Jungfrau", "tokens": ["Im", "Her\u00b7zen", "der", "Jung\u00b7frau"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "da knospet die Liebe,", "tokens": ["da", "knos\u00b7pet", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "von k\u00fcnftiger Herrlichkeit", "tokens": ["von", "k\u00fcnf\u00b7ti\u00b7ger", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "sinnend sie tr\u00e4umt.", "tokens": ["sin\u00b7nend", "sie", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Bis da\u00df sie im Herzen", "tokens": ["Bis", "da\u00df", "sie", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "des Weibes entfaltet", "tokens": ["des", "Wei\u00b7bes", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "zu \u00fcppigster Bl\u00fcte", "tokens": ["zu", "\u00fcp\u00b7pigs\u00b7ter", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "berauschend erprangt.", "tokens": ["be\u00b7rau\u00b7schend", "er\u00b7prangt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Im Herzen der Mutter", "tokens": ["Im", "Her\u00b7zen", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "zur edelsten Reife,", "tokens": ["zur", "e\u00b7dels\u00b7ten", "Rei\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "zur Krone des Alls,", "tokens": ["zur", "Kro\u00b7ne", "des", "Alls", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "zur Vollendung gelangt.", "tokens": ["zur", "Vol\u00b7len\u00b7dung", "ge\u00b7langt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}