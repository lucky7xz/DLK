{"dta.poem.5323": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sondern wenn mans recht gebrauchet,", "tokens": ["Son\u00b7dern", "wenn", "mans", "recht", "ge\u00b7brau\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Geist, zu GOtt gekehrt,", "tokens": ["Und", "der", "Geist", ",", "zu", "Gott", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhn in unsrer Lust verehrt,", "tokens": ["Jhn", "in", "uns\u00b7rer", "Lust", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Danckt, da\u00df er sie uns uns beschert,", "tokens": ["Danckt", ",", "da\u00df", "er", "sie", "uns", "uns", "be\u00b7schert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPER", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Frohe Seufzer von sich hauchet;", "tokens": ["Fro\u00b7he", "Seuf\u00b7zer", "von", "sich", "hau\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sp\u00fchret man, durch jede Blume,", "tokens": ["Sp\u00fch\u00b7ret", "man", ",", "durch", "je\u00b7de", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df selbst in uns, GOTT zum Ruhme,", "tokens": ["Da\u00df", "selbst", "in", "uns", ",", "GoTT", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "$,", "NE", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Recht ein geistig Rauchwerck rauchet.", "tokens": ["Recht", "ein", "geis\u00b7tig", "Rauch\u00b7werck", "rau\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Eine solche Harmonie", "tokens": ["Ei\u00b7ne", "sol\u00b7che", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Holder D\u00fcnste qvillt aus ihnen,", "tokens": ["Hol\u00b7der", "D\u00fcns\u00b7te", "qvillt", "aus", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mit so s\u00fcssem Reitz, herf\u00fcr,", "tokens": ["Mit", "so", "s\u00fcs\u00b7sem", "Reitz", ",", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Solch\u2019 ein\u2019 Anmuth f\u00fcllet sie,", "tokens": ["Solch'", "ein'", "An\u00b7muth", "f\u00fcl\u00b7let", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df man sich zum Sch\u00f6pfer lencket,", "tokens": ["Da\u00df", "man", "sich", "zum", "Sch\u00f6p\u00b7fer", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und, zum Danck getrieben, dencket:", "tokens": ["Und", ",", "zum", "Danck", "ge\u00b7trie\u00b7ben", ",", "den\u00b7cket", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "APPRART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Wie ist doch der GOtt so gro\u00df,", "tokens": ["Wie", "ist", "doch", "der", "Gott", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.16": {"text": "Welcher in der Erden Scho\u00df", "tokens": ["Wel\u00b7cher", "in", "der", "Er\u00b7den", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Solche Wunder-Kraft gesencket,", "tokens": ["Sol\u00b7che", "Wun\u00b7der\u00b7Kraft", "ge\u00b7sen\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und, durch seine G\u00fcte blos,", "tokens": ["Und", ",", "durch", "sei\u00b7ne", "G\u00fc\u00b7te", "blos", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Mir so s\u00fcsse Wollust schencket.", "tokens": ["Mir", "so", "s\u00fcs\u00b7se", "Wol\u00b7lust", "schen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}