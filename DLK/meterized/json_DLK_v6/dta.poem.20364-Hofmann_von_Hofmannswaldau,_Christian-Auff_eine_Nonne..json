{"dta.poem.20364": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auff eine Nonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man nahm mir meinen schmuck/ und lie\u00df mir fleisch\nund blut/", "tokens": ["Man", "nahm", "mir", "mei\u00b7nen", "schmuck", "/", "und", "lie\u00df", "mir", "fleisch", "und", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVFIN", "ADJD", "$(", "KON", "VVFIN", "PPER", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man schnitt die haare weg/ und lie\u00df mir meine glut.", "tokens": ["Man", "schnitt", "die", "haa\u00b7re", "weg", "/", "und", "lie\u00df", "mir", "mei\u00b7ne", "glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$(", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im beten hat mir stets der glaube sehr behaget/", "tokens": ["Im", "be\u00b7ten", "hat", "mir", "stets", "der", "glau\u00b7be", "sehr", "be\u00b7ha\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADV", "ART", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil er von aufferstehn des fleisches etwas saget.", "tokens": ["Weil", "er", "von", "auf\u00b7fer\u00b7stehn", "des", "flei\u00b7sches", "et\u00b7was", "sa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "APPR", "ART", "ADJA", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}