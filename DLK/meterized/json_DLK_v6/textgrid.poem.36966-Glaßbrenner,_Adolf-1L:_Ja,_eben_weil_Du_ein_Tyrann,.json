{"textgrid.poem.36966": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, eben weil Du ein Tyrann,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, eben weil Du ein Tyrann,", "tokens": ["Ja", ",", "e\u00b7ben", "weil", "Du", "ein", "Ty\u00b7rann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du alter Adel: Brauch,", "tokens": ["Du", "al\u00b7ter", "A\u00b7del", ":", "Brauch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum achtet dich kein freier Mann,", "tokens": ["Drum", "ach\u00b7tet", "dich", "kein", "frei\u00b7er", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum hassen wir dich auch!", "tokens": ["Drum", "has\u00b7sen", "wir", "dich", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum ist aus deiner Despotie", "tokens": ["Drum", "ist", "aus", "dei\u00b7ner", "Des\u00b7po\u00b7tie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kraft und Sch\u00f6nheit verbannt;", "tokens": ["Kraft", "und", "Sch\u00f6n\u00b7heit", "ver\u00b7bannt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Drum hat noch nimmer ein Genie", "tokens": ["Drum", "hat", "noch", "nim\u00b7mer", "ein", "Ge\u00b7nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bestrahlt dein traurig Land!", "tokens": ["Be\u00b7strahlt", "dein", "trau\u00b7rig", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drum ist das H\u00f6chste uns der Geist,", "tokens": ["Drum", "ist", "das", "H\u00f6chs\u00b7te", "uns", "der", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "ADJA", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Freiheit und die Kunst,", "tokens": ["Die", "Frei\u00b7heit", "und", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum Alles, was dein Hof anpreist,", "tokens": ["Drum", "Al\u00b7les", ",", "was", "dein", "Hof", "an\u00b7preist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur eitel Trug und Dunst!", "tokens": ["Nur", "ei\u00b7tel", "Trug", "und", "Dunst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geborner Gr\u00f6\u00dfe lachen wir", "tokens": ["Ge\u00b7bor\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "la\u00b7chen", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum h\u00f6hnisch in's Gesicht;", "tokens": ["Drum", "h\u00f6h\u00b7nisch", "in's", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum beugt der Edle sich vor dir", "tokens": ["Drum", "beugt", "der", "Ed\u00b7le", "sich", "vor", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und deinen Vettern nicht!", "tokens": ["Und", "dei\u00b7nen", "Vet\u00b7tern", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, eben weil Du ein Tyrann,", "tokens": ["Ja", ",", "e\u00b7ben", "weil", "Du", "ein", "Ty\u00b7rann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du alter Adel: Brauch,", "tokens": ["Du", "al\u00b7ter", "A\u00b7del", ":", "Brauch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum achtet dich kein freier Mann,", "tokens": ["Drum", "ach\u00b7tet", "dich", "kein", "frei\u00b7er", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum hassen wir dich auch!", "tokens": ["Drum", "has\u00b7sen", "wir", "dich", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum sind uns Sterne, Orden, Gold", "tokens": ["Drum", "sind", "uns", "Ster\u00b7ne", ",", "Or\u00b7den", ",", "Gold"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PAV", "VAFIN", "PPER", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Beweise nie des Werth's;", "tokens": ["Be\u00b7wei\u00b7se", "nie", "des", "Wert\u00b7h's", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch ist das arme Volk uns hold,", "tokens": ["Doch", "ist", "das", "ar\u00b7me", "Volk", "uns", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schl\u00e4gt stolzer unser Herz!", "tokens": ["Schl\u00e4gt", "stol\u00b7zer", "un\u00b7ser", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum schaun am Strick' nicht, im Ornat", "tokens": ["Drum", "schaun", "am", "Strick'", "nicht", ",", "im", "Or\u00b7nat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "APPRART", "NN", "PTKNEG", "$,", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die gr\u00f6\u00dften Schurken wir;", "tokens": ["Die", "gr\u00f6\u00df\u00b7ten", "Schur\u00b7ken", "wir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum schimpfen wir, wie's Luther that,", "tokens": ["Drum", "schimp\u00b7fen", "wir", ",", "wie's", "Lu\u00b7ther", "that", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was sich beschimpft in dir!", "tokens": ["Was", "sich", "be\u00b7schimpft", "in", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum lachen wir, wenn \u00fcber uns", "tokens": ["Drum", "la\u00b7chen", "wir", ",", "wenn", "\u00fc\u00b7ber", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Orden Wehe! schreit:", "tokens": ["Dein", "Or\u00b7den", "We\u00b7he", "!", "schreit", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir fragen nicht nach Hinz und Kunz,", "tokens": ["Wir", "fra\u00b7gen", "nicht", "nach", "Hinz", "und", "Kunz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn uns ein Gott gebeut!", "tokens": ["Wenn", "uns", "ein", "Gott", "ge\u00b7beut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ja, eben weil du ein Tyrann,", "tokens": ["Ja", ",", "e\u00b7ben", "weil", "du", "ein", "Ty\u00b7rann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du alter Adel: Brauch,", "tokens": ["Du", "al\u00b7ter", "A\u00b7del", ":", "Brauch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum achtet Dich kein freier Mann,", "tokens": ["Drum", "ach\u00b7tet", "Dich", "kein", "frei\u00b7er", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum hassen wir dich auch!", "tokens": ["Drum", "has\u00b7sen", "wir", "dich", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gelobt sei Gott, schon wankt dein Thron!", "tokens": ["Ge\u00b7lobt", "sei", "Gott", ",", "schon", "wankt", "dein", "Thron", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich sch\u00fctzt kein Adel mehr!", "tokens": ["Dich", "sch\u00fctzt", "kein", "A\u00b7del", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit Dolchen gehn die Geister schon", "tokens": ["Mit", "Dol\u00b7chen", "gehn", "die", "Geis\u00b7ter", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN",