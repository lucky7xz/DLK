{"textgrid.poem.62164": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Zum 70j\u00e4hrigen Geburtstage eines Onkels", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was auch die Menschen trennt, die Geister scheidet,", "tokens": ["Was", "auch", "die", "Men\u00b7schen", "trennt", ",", "die", "Geis\u00b7ter", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eins gibt's, was alle Welt verehrt,", "tokens": ["Eins", "gibt's", ",", "was", "al\u00b7le", "Welt", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Tugend mit dem Strahlenkranze,", "tokens": ["Die", "Tu\u00b7gend", "mit", "dem", "Strah\u00b7len\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die uns sich selbst vergessen lehrt.", "tokens": ["Die", "uns", "sich", "selbst", "ver\u00b7ges\u00b7sen", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer so wie Du in ihr gelebt, gewandelt,", "tokens": ["Wer", "so", "wie", "Du", "in", "ihr", "ge\u00b7lebt", ",", "ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "KOKOM", "PPER", "APPR", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie liebend keinen Augenblick verlie\u00df,", "tokens": ["Sie", "lie\u00b7bend", "kei\u00b7nen", "Au\u00b7gen\u00b7blick", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit aller Kraft und Lust nur gut gehandelt,", "tokens": ["Mit", "al\u00b7ler", "Kraft", "und", "Lust", "nur", "gut", "ge\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu jeder Zeit man gerne pries!", "tokens": ["Zu", "je\u00b7der", "Zeit", "man", "ger\u00b7ne", "pries", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob viel gepr\u00fcft, gek\u00e4mpft, ob viel gelitten,", "tokens": ["Ob", "viel", "ge\u00b7pr\u00fcft", ",", "ge\u00b7k\u00e4mpft", ",", "ob", "viel", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "$,", "VVPP", "$,", "KOUS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewu\u00dftsein hei\u00dft das inn're Gl\u00fcck,", "tokens": ["Be\u00b7wu\u00df\u00b7tsein", "hei\u00dft", "das", "inn'\u00b7re", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Allerbesten ward nicht mehr beschieden \u2013", "tokens": ["Den", "Al\u00b7ler\u00b7bes\u00b7ten", "ward", "nicht", "mehr", "be\u00b7schie\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein h\u00f6h'res strahlt vom Thron zur\u00fcck. \u2013", "tokens": ["Kein", "h\u00f6h'\u00b7res", "strahlt", "vom", "Thron", "zu\u00b7r\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}