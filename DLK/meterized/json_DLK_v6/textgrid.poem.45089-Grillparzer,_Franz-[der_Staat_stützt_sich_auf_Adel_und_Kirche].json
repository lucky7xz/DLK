{"textgrid.poem.45089": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[der Staat st\u00fctzt sich auf Adel und Kirche]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Staat st\u00fctzt sich auf Adel und Kirche,", "tokens": ["Der", "Staat", "st\u00fctzt", "sich", "auf", "A\u00b7del", "und", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die beide sich wieder nur st\u00fctzen auf ihn.", "tokens": ["Die", "bei\u00b7de", "sich", "wie\u00b7der", "nur", "st\u00fct\u00b7zen", "auf", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PRF", "ADV", "ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das gleicht dem Versuch des Baron M\u00fcnchhausen,", "tokens": ["Das", "gleicht", "dem", "Ver\u00b7such", "des", "Ba\u00b7ron", "M\u00fcnch\u00b7hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "NE", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sich am eigenen Zopf aus dem Sumpfe zu ziehn.", "tokens": ["Sich", "am", "ei\u00b7ge\u00b7nen", "Zopf", "aus", "dem", "Sump\u00b7fe", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}}}}