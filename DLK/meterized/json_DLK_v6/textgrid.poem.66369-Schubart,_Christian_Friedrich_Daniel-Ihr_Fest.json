{"textgrid.poem.66369": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Ihr Fest", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Regina, wenn deine Zephyrh\u00e4nde,", "tokens": ["O", "Re\u00b7gi\u00b7na", ",", "wenn", "dei\u00b7ne", "Ze\u00b7phyr\u00b7h\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Engelfl\u00fcgel den Rosenbusch,", "tokens": ["Wie", "En\u00b7gel\u00b7fl\u00fc\u00b7gel", "den", "Ro\u00b7sen\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "F\u00e4cheln die Tasten", "tokens": ["F\u00e4\u00b7cheln", "die", "Tas\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Deines goldnen Saitenspiels;", "tokens": ["Dei\u00b7nes", "gold\u00b7nen", "Sai\u00b7ten\u00b7spiels", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn dann ", "tokens": ["Wenn", "dann"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Neben dir schwebt, und mit dem Schlage", "tokens": ["Ne\u00b7ben", "dir", "schwebt", ",", "und", "mit", "dem", "Schla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Der Fl\u00fcgel, deines Saitenspiels", "tokens": ["Der", "Fl\u00fc\u00b7gel", ",", "dei\u00b7nes", "Sai\u00b7ten\u00b7spiels"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gedanken beseelt; deine T\u00f6ne weckt und auftrinkt;", "tokens": ["Ge\u00b7dan\u00b7ken", "be\u00b7seelt", ";", "dei\u00b7ne", "T\u00f6\u00b7ne", "weckt", "und", "auf\u00b7trinkt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+-+---", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "So steh' des Gefangnen Bild vor deiner Seele,", "tokens": ["So", "steh'", "des", "Ge\u00b7fang\u00b7nen", "Bild", "vor", "dei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wie er \u00e4chzt im Kerkergekl\u00fcft,", "tokens": ["Wie", "er", "\u00e4chzt", "im", "Ker\u00b7ker\u00b7ge\u00b7kl\u00fcft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Und am Tage deines Werdens", "tokens": ["Und", "am", "Ta\u00b7ge", "dei\u00b7nes", "Wer\u00b7dens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "F\u00fcr dich betet, da\u00df die Stirn' ihm gl\u00fcht,", "tokens": ["F\u00fcr", "dich", "be\u00b7tet", ",", "da\u00df", "die", "Stirn'", "ihm", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Da\u00df Funken der Liebe dem Aug' entspr\u00fchn", "tokens": ["Da\u00df", "Fun\u00b7ken", "der", "Lie\u00b7be", "dem", "Aug'", "ent\u00b7spr\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Und der Andacht Z\u00e4hr' im Staube perlt.", "tokens": ["Und", "der", "An\u00b7dacht", "Z\u00e4hr'", "im", "Stau\u00b7be", "perlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "La\u00df sie leben! so seufzt er, der Einsame!", "tokens": ["La\u00df", "sie", "le\u00b7ben", "!", "so", "seufzt", "er", ",", "der", "Ein\u00b7sa\u00b7me", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "La\u00df sie leben, Erbarmer! Guter!", "tokens": ["La\u00df", "sie", "le\u00b7ben", ",", "Er\u00b7bar\u00b7mer", "!", "Gu\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "NN", "$.", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Gib ihr der Freuden viel!", "tokens": ["Gib", "ihr", "der", "Freu\u00b7den", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur wenige Tropfen la\u00df sie schl\u00fcrfen", "tokens": ["Nur", "we\u00b7ni\u00b7ge", "Trop\u00b7fen", "la\u00df", "sie", "schl\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aus dem Becher der Leiden!", "tokens": ["Aus", "dem", "Be\u00b7cher", "der", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ach! nur wenige Tropfen,", "tokens": ["Ach", "!", "nur", "we\u00b7ni\u00b7ge", "Trop\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Von deiner Gnade durchs\u00fc\u00dft!", "tokens": ["Von", "dei\u00b7ner", "Gna\u00b7de", "durch\u00b7s\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und sp\u00e4t, o sp\u00e4t erst, du Schaffer und T\u00f6dter!", "tokens": ["Und", "sp\u00e4t", ",", "o", "sp\u00e4t", "erst", ",", "du", "Schaf\u00b7fer", "und", "T\u00f6d\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "FM", "ADJD", "ADV", "$,", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Gib ihr den Ku\u00df der innigsten Liebe,", "tokens": ["Gib", "ihr", "den", "Ku\u00df", "der", "in\u00b7nigs\u00b7ten", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Da\u00df ihre Seele \u2013 dein Meisterst\u00fcck \u2013", "tokens": ["Da\u00df", "ih\u00b7re", "See\u00b7le", "\u2013", "dein", "Meis\u00b7ter\u00b7st\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Im Entz\u00fccken vom Leibe sich trennt!", "tokens": ["Im", "Ent\u00b7z\u00fc\u00b7cken", "vom", "Lei\u00b7be", "sich", "trennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}