{"textgrid.poem.57321": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer nicht f\u00fcrchtet, nicht hoft, nur der ist gl\u00fccklich! Also denkt er: Der Weis' erwartet ruhig,", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nicht f\u00fcrchtet, nicht hoft, nur der ist gl\u00fccklich! Also denkt er: Der Weis' erwartet ruhig,", "tokens": ["Wer", "nicht", "f\u00fcrch\u00b7tet", ",", "nicht", "hoft", ",", "nur", "der", "ist", "gl\u00fcck\u00b7lich", "!", "Al\u00b7so", "denkt", "er", ":", "Der", "Weis'", "er\u00b7war\u00b7tet", "ru\u00b7hig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "$,", "PTKNEG", "VVFIN", "$,", "ADV", "ART", "VAFIN", "ADJD", "$.", "ADV", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "--+--+-+-+-+-+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Was ihm senden die Vorsicht", "tokens": ["Was", "ihm", "sen\u00b7den", "die", "Vor\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Werde, Freud' oder Schmerz.", "tokens": ["Wer\u00b7de", ",", "Freud'", "o\u00b7der", "Schmerz", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Du, dem's hier sich noch w\u00f6lkt, du w\u00e4hnst die Zukunft", "tokens": ["Du", ",", "dem's", "hier", "sich", "noch", "w\u00f6lkt", ",", "du", "w\u00e4hnst", "die", "Zu\u00b7kunft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "PRF", "ADV", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auszusp\u00e4hen. Du Thor, wirst du denn niemals", "tokens": ["Aus\u00b7zu\u00b7sp\u00e4\u00b7hen", ".", "Du", "Thor", ",", "wirst", "du", "denn", "nie\u00b7mals"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Vom ganz anderen Ausgang,", "tokens": ["Vom", "ganz", "an\u00b7de\u00b7ren", "Aus\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dir zum Heile, gewarnt?", "tokens": ["Dir", "zum", "Hei\u00b7le", ",", "ge\u00b7warnt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Lernst du niemals, dass du, ach durch die Hofnung", "tokens": ["Lernst", "du", "nie\u00b7mals", ",", "dass", "du", ",", "ach", "durch", "die", "Hof\u00b7nung"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "$,", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Auch, dich qu\u00e4lest? Denn sie, wenn sie nun scheidet,", "tokens": ["Auch", ",", "dich", "qu\u00e4\u00b7lest", "?", "Denn", "sie", ",", "wenn", "sie", "nun", "schei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "$.", "KON", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Reicht, im gr\u00f6sseren Kelche,", "tokens": ["Reicht", ",", "im", "gr\u00f6s\u00b7se\u00b7ren", "Kel\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Herbes Trunkes viel mehr.", "tokens": ["Her\u00b7bes", "Trun\u00b7kes", "viel", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und verscheuchest du nicht, was jetzo da ist,", "tokens": ["Und", "ver\u00b7scheu\u00b7chest", "du", "nicht", ",", "was", "jet\u00b7zo", "da", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "ADV", "ADV", "VAFIN", "$,"], "meter": "--+-+++-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Durch des k\u00fcnftigen Traum? und lebst ein Leben,", "tokens": ["Durch", "des", "k\u00fcnf\u00b7ti\u00b7gen", "Traum", "?", "und", "lebst", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Welches, leer des Genusses,", "tokens": ["Wel\u00b7ches", ",", "leer", "des", "Ge\u00b7nus\u00b7ses", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Heut nicht, Morgen nicht hat?", "tokens": ["Heut", "nicht", ",", "Mor\u00b7gen", "nicht", "hat", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "NN", "PTKNEG", "VAFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Sey, Erwartung, gegr\u00fcsst, des Weisen St\u00e4rke,", "tokens": ["Sey", ",", "Er\u00b7war\u00b7tung", ",", "ge\u00b7gr\u00fcs\u00b7st", ",", "des", "Wei\u00b7sen", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "VVPP", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und Zufriedenheit du mit dem, was Gott schickt!", "tokens": ["Und", "Zu\u00b7frie\u00b7den\u00b7heit", "du", "mit", "dem", ",", "was", "Gott", "schickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "APPR", "ART", "$,", "PRELS", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Leitet ferner; ihr f\u00fchrtet", "tokens": ["Lei\u00b7tet", "fer\u00b7ner", ";", "ihr", "f\u00fchr\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sch\u00f6nen, einsamen Pfad", "tokens": ["Sch\u00f6\u00b7nen", ",", "ein\u00b7sa\u00b7men", "Pfad"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Hin am Meere, wo, nach verschwundner Heitre,", "tokens": ["Hin", "am", "Mee\u00b7re", ",", "wo", ",", "nach", "ver\u00b7schwund\u00b7ner", "Heit\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PWAV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "St\u00fcrme brausen, verweht der Nothschrey jammert,", "tokens": ["St\u00fcr\u00b7me", "brau\u00b7sen", ",", "ver\u00b7weht", "der", "Noth\u00b7schrey", "jam\u00b7mert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bis die Lasten der Lotse", "tokens": ["Bis", "die", "Las\u00b7ten", "der", "Lot\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Z\u00e4hlt, die Leichen nicht mit!", "tokens": ["Z\u00e4hlt", ",", "die", "Lei\u00b7chen", "nicht", "mit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Wo, nach leiserem Spiel der sanften Welle,", "tokens": ["Wo", ",", "nach", "lei\u00b7se\u00b7rem", "Spiel", "der", "sanf\u00b7ten", "Wel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wogen branden, dass dumpf das Felsgestad kracht,", "tokens": ["Wo\u00b7gen", "bran\u00b7den", ",", "dass", "dumpf", "das", "Fels\u00b7ge\u00b7stad", "kracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und der schwellende Todte", "tokens": ["Und", "der", "schwel\u00b7len\u00b7de", "Tod\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Str\u00f6mt zum weissen Gebein!", "tokens": ["Str\u00f6mt", "zum", "weis\u00b7sen", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}