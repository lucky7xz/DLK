{"textgrid.poem.66964": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch daran m\u00f6gt ihr den Meister erkennen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch daran m\u00f6gt ihr den Meister erkennen,", "tokens": ["Auch", "da\u00b7ran", "m\u00f6gt", "ihr", "den", "Meis\u00b7ter", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In Kunst und Leben sattelfest:", "tokens": ["In", "Kunst", "und", "Le\u00b7ben", "sat\u00b7tel\u00b7fest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er mu\u00df mit Empfindungen w\u00fcrfeln k\u00f6nnen,", "tokens": ["Er", "mu\u00df", "mit", "Emp\u00b7fin\u00b7dun\u00b7gen", "w\u00fcr\u00b7feln", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Statt da\u00df er selber sich w\u00fcrfeln l\u00e4\u00dft.", "tokens": ["Statt", "da\u00df", "er", "sel\u00b7ber", "sich", "w\u00fcr\u00b7feln", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "PRF", "VVINF", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Aber um das geschickt zu verstehen,", "tokens": ["A\u00b7ber", "um", "das", "ge\u00b7schickt", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit Schikanen so wahr wie fein,", "tokens": ["Mit", "Schi\u00b7ka\u00b7nen", "so", "wahr", "wie", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Mu\u00df er zu s\u00e4mtlichen Zahlenh\u00f6hen", "tokens": ["Mu\u00df", "er", "zu", "s\u00e4mt\u00b7li\u00b7chen", "Zah\u00b7len\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Vorher selber gew\u00fcrfelt sein.", "tokens": ["Vor\u00b7her", "sel\u00b7ber", "ge\u00b7w\u00fcr\u00b7felt", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}