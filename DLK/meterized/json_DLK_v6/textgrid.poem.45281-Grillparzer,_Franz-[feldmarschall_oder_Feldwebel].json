{"textgrid.poem.45281": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[feldmarschall oder Feldwebel]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feldmarschall oder Feldwebel,", "tokens": ["Feld\u00b7mar\u00b7schall", "o\u00b7der", "Feld\u00b7we\u00b7bel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sie achten doch nichts, als den S\u00e4bel.", "tokens": ["Sie", "ach\u00b7ten", "doch", "nichts", ",", "als", "den", "S\u00e4\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Poesie mag sich ihnen empfehlen,", "tokens": ["Die", "Poe\u00b7sie", "mag", "sich", "ih\u00b7nen", "emp\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als eine Art Branntwein f\u00fcr die Seelen.", "tokens": ["Als", "ei\u00b7ne", "Art", "Brannt\u00b7wein", "f\u00fcr", "die", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}