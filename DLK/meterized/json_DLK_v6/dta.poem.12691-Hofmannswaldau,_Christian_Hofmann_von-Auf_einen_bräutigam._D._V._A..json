{"dta.poem.12691": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen br\u00e4utigam.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Archisen trug sein sohn Aeneas aus den flammen:", "tokens": ["Ar\u00b7chi\u00b7sen", "trug", "sein", "sohn", "A\u00b7e\u00b7neas", "aus", "den", "flam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "NE", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seht aber, wie auch hier die liebe wunder thut:", "tokens": ["Seht", "a\u00b7ber", ",", "wie", "auch", "hier", "die", "lie\u00b7be", "wun\u00b7der", "thut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der br\u00e4utgam holt die braut zu Rochlitz aus der gluth,", "tokens": ["Der", "br\u00e4ut\u00b7gam", "holt", "die", "braut", "zu", "Roch\u00b7litz", "aus", "der", "gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "VVFIN", "APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und eh ein jahr vergeht, sind ihrer drey beysammen.", "tokens": ["Und", "eh", "ein", "jahr", "ver\u00b7geht", ",", "sind", "ih\u00b7rer", "drey", "bey\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "VAFIN", "PPOSAT", "CARD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}