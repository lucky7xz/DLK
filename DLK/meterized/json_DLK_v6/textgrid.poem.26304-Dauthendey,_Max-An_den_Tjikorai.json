{"textgrid.poem.26304": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "An den Tjikorai", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Berg, der hin zum \u00c4ther zieht,", "tokens": ["Du", "Berg", ",", "der", "hin", "zum", "\u00c4\u00b7ther", "zieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Gipfel \u00fcber die Zeiten sieht,", "tokens": ["Des", "Gip\u00b7fel", "\u00fc\u00b7ber", "die", "Zei\u00b7ten", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du Ewiger, der nicht altern kann,", "tokens": ["Du", "E\u00b7wi\u00b7ger", ",", "der", "nicht", "al\u00b7tern", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "PRELS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Die Jahre reichen nicht an dich heran.", "tokens": ["Die", "Jah\u00b7re", "rei\u00b7chen", "nicht", "an", "dich", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und die Jahrhunderte du kaum f\u00fchlst,", "tokens": ["Und", "die", "Jahr\u00b7hun\u00b7der\u00b7te", "du", "kaum", "f\u00fchlst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Wenn du die Stirn im Weltraum k\u00fchlst.", "tokens": ["Wenn", "du", "die", "Stirn", "im", "Welt\u00b7raum", "k\u00fchlst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du lebtest, als der erste Mann", "tokens": ["Du", "leb\u00b7test", ",", "als", "der", "ers\u00b7te", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das erste Frauenherz sich gewann.", "tokens": ["Das", "ers\u00b7te", "Frau\u00b7en\u00b7herz", "sich", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Du lebst noch, wenn einst das letzte Paar", "tokens": ["Du", "lebst", "noch", ",", "wenn", "einst", "das", "letz\u00b7te", "Paar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Hinstirbt im letzten Menschenjahr.", "tokens": ["Hins\u00b7tirbt", "im", "letz\u00b7ten", "Men\u00b7schen\u00b7jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie wichtig sind mir doch meine Sorgen.", "tokens": ["Wie", "wich\u00b7tig", "sind", "mir", "doch", "mei\u00b7ne", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Wie wichtig das Gestern, Heute und Morgen.", "tokens": ["Wie", "wich\u00b7tig", "das", "Ge\u00b7stern", ",", "Heu\u00b7te", "und", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "ADV", "KON", "NN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Du lehrst weit \u00fcber die Tage zu schauen,", "tokens": ["Du", "lehrst", "weit", "\u00fc\u00b7ber", "die", "Ta\u00b7ge", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Du lehrst, dem Ewigen zu vertrauen.", "tokens": ["Du", "lehrst", ",", "dem", "E\u00b7wi\u00b7gen", "zu", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}