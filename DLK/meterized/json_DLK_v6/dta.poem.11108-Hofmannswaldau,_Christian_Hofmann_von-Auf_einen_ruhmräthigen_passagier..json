{"dta.poem.11108": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen ruhmr\u00e4thigen passagier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.85", "et:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Madrit und Lisabon hat obulus gesehn,", "tokens": ["Mad\u00b7rit", "und", "Li\u00b7sa\u00b7bon", "hat", "o\u00b7bu\u00b7lus", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er wei\u00df, was in Pari\u00df und Londen ist geschehn,", "tokens": ["Er", "wei\u00df", ",", "was", "in", "Pa\u00b7ri\u00df", "und", "Lon\u00b7den", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPR", "NE", "KON", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er zehlt am finger her, was Rom und Florentz weiset;", "tokens": ["Er", "zehlt", "am", "fin\u00b7ger", "her", ",", "was", "Rom", "und", "Flo\u00b7rentz", "wei\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "PTKVZ", "$,", "PRELS", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur dieses l\u00e4ugnet er, da\u00df er durch Worms gereiset.", "tokens": ["Nur", "die\u00b7ses", "l\u00e4ug\u00b7net", "er", ",", "da\u00df", "er", "durch", "Worms", "ge\u00b7rei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}