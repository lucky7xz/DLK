{"textgrid.poem.43729": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Uhu", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum fliegt doch der Uhu in finsterer Nacht?", "tokens": ["Wa\u00b7rum", "fliegt", "doch", "der", "U\u00b7hu", "in", "fins\u00b7te\u00b7rer", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Ich m\u00f6chte wohl wissen, was dann er noch macht?", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wohl", "wis\u00b7sen", ",", "was", "dann", "er", "noch", "macht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "PRELS", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er k\u00f6nnte wie andere Leute ja ruhn,", "tokens": ["Er", "k\u00f6nn\u00b7te", "wie", "an\u00b7de\u00b7re", "Leu\u00b7te", "ja", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOKOM", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er f\u00e4nde bei Tage genug auch zu thun.", "tokens": ["Er", "f\u00e4n\u00b7de", "bei", "Ta\u00b7ge", "ge\u00b7nug", "auch", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbwie ein Dieb mu\u00df ich leben in finsterer Nacht,", "tokens": ["\u00bb", "wie", "ein", "Dieb", "mu\u00df", "ich", "le\u00b7ben", "in", "fins\u00b7te\u00b7rer", "Nacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "ART", "NN", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Dann geh' ich mit Frau und mit Kind auf die Jagd.", "tokens": ["Dann", "geh'", "ich", "mit", "Frau", "und", "mit", "Kind", "auf", "die", "Jagd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Des Tages erlaubt es die Sonne ja nicht,", "tokens": ["Des", "Ta\u00b7ges", "er\u00b7laubt", "es", "die", "Son\u00b7ne", "ja", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Drum scheuen der Dieb und der Uhu ihr Licht.\u00ab", "tokens": ["Drum", "scheu\u00b7en", "der", "Dieb", "und", "der", "U\u00b7hu", "ihr", "Licht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}