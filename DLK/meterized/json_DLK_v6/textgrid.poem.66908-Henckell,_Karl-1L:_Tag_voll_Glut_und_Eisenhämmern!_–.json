{"textgrid.poem.66908": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tag voll Glut und Eisenh\u00e4mmern! \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tag voll Glut und Eisenh\u00e4mmern! \u2013", "tokens": ["Tag", "voll", "Glut", "und", "Ei\u00b7sen\u00b7h\u00e4m\u00b7mern", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Stunde Schlag verhallt,", "tokens": ["Wie", "der", "Stun\u00b7de", "Schlag", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trupp auf Trupp dem Tor entwallt", "tokens": ["Trupp", "auf", "Trupp", "dem", "Tor", "ent\u00b7wallt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In das graue Winterd\u00e4mmern,", "tokens": ["In", "das", "grau\u00b7e", "Win\u00b7ter\u00b7d\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drob sich Schneegew\u00f6lke bleiern ballt.", "tokens": ["Drob", "sich", "Schnee\u00b7ge\u00b7w\u00f6l\u00b7ke", "blei\u00b7ern", "ballt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Endlos w\u00e4lzt die schwarze Schlange,", "tokens": ["End\u00b7los", "w\u00e4lzt", "die", "schwar\u00b7ze", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schieben tausend schwere Schuh,", "tokens": ["Schie\u00b7ben", "tau\u00b7send", "schwe\u00b7re", "Schuh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leiber voll Begehr nach Ruh,", "tokens": ["Lei\u00b7ber", "voll", "Be\u00b7gehr", "nach", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In eint\u00f6nig gleichem Gange", "tokens": ["In", "ein\u00b7t\u00f6\u00b7nig", "glei\u00b7chem", "Gan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schattenhaft dem steinernen Meer sich zu.", "tokens": ["Schat\u00b7ten\u00b7haft", "dem", "stei\u00b7ner\u00b7nen", "Meer", "sich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Langer Zug in schmalen Rotten,", "tokens": ["Lan\u00b7ger", "Zug", "in", "schma\u00b7len", "Rot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis die letzte fern versinkt,", "tokens": ["Bis", "die", "letz\u00b7te", "fern", "ver\u00b7sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Bogenlampe blinkt", "tokens": ["Wo", "die", "Bo\u00b7gen\u00b7lam\u00b7pe", "blinkt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Dunst der Menschenmotten", "tokens": ["Aus", "dem", "Dunst", "der", "Men\u00b7schen\u00b7mot\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und als bleicher Stern der Gro\u00dfstadt winkt.", "tokens": ["Und", "als", "blei\u00b7cher", "Stern", "der", "Gro\u00df\u00b7stadt", "winkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Manchen Schatten, die dort schwinden,", "tokens": ["Man\u00b7chen", "Schat\u00b7ten", ",", "die", "dort", "schwin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winkt ein Licht noch durch die Nacht,", "tokens": ["Winkt", "ein", "Licht", "noch", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief im Lebensgrund entfacht,", "tokens": ["Tief", "im", "Le\u00b7bens\u00b7grund", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsichtbar den ewig Blinden:", "tokens": ["Un\u00b7sicht\u00b7bar", "den", "e\u00b7wig", "Blin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Licht, das Arbeit frei und heilig macht.", "tokens": ["Licht", ",", "das", "Ar\u00b7beit", "frei", "und", "hei\u00b7lig", "macht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}