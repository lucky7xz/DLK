{"dta.poem.18419": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Printz Louys von Baden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der himmel will dir nichts als siege zuerkennen/", "tokens": ["Der", "him\u00b7mel", "will", "dir", "nichts", "als", "sie\u00b7ge", "zu\u00b7er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PIS", "KOKOM", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So offt der T\u00fcrcken macht die l\u00e4nder \u00fcberschwemmt;", "tokens": ["So", "offt", "der", "T\u00fcr\u00b7cken", "macht", "die", "l\u00e4n\u00b7der", "\u00fc\u00b7bersc\u00b7hwemmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist mit allem recht ein Josua zu nennen/", "tokens": ["Du", "bist", "mit", "al\u00b7lem", "recht", "ein", "Jo\u00b7sua", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "ADV", "ART", "NE", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der zwar die sonne nicht/ iedoch den monden hemmt.", "tokens": ["Der", "zwar", "die", "son\u00b7ne", "nicht", "/", "ie\u00b7doch", "den", "mon\u00b7den", "hemmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKNEG", "$(", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}