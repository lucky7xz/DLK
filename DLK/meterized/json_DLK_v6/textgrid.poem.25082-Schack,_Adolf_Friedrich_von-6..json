{"textgrid.poem.25082": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder schreit' ich l\u00e4ngs des Stromes,", "tokens": ["Wie\u00b7der", "schreit'", "ich", "l\u00e4ngs", "des", "Stro\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo uns, wenn mit ihr ich ging,", "tokens": ["Wo", "uns", ",", "wenn", "mit", "ihr", "ich", "ging", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "KOUS", "APPR", "PPOSAT", "PPER", "VVFIN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Trunken an des Himmelsdomes", "tokens": ["Trun\u00b7ken", "an", "des", "Him\u00b7mels\u00b7do\u00b7mes"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abendglanz das Auge hing.", "tokens": ["A\u00b7bend\u00b7glanz", "das", "Au\u00b7ge", "hing", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da bei Glocken-Sp\u00e4tgel\u00e4ute,", "tokens": ["Da", "bei", "Glo\u00b7cken\u00b7Sp\u00e4t\u00b7ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das in milden Melodien", "tokens": ["Das", "in", "mil\u00b7den", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch das Laub scholl, \u00fcberstreute", "tokens": ["Durch", "das", "Laub", "scholl", ",", "\u00fc\u00b7bers\u00b7treu\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns mit Bl\u00fcten der Jasmin;", "tokens": ["Uns", "mit", "Bl\u00fc\u00b7ten", "der", "Jas\u00b7min", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Abendnebel rauchten", "tokens": ["Und", "die", "A\u00b7bend\u00b7ne\u00b7bel", "rauch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.2": {"text": "Golden aus der Felsenkluft;", "tokens": ["Gol\u00b7den", "aus", "der", "Fel\u00b7sen\u00b7kluft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwischen unsre K\u00fcsse hauchten", "tokens": ["Zwi\u00b7schen", "uns\u00b7re", "K\u00fcs\u00b7se", "hauch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wilde Rosen ihren Duft.", "tokens": ["Wil\u00b7de", "Ro\u00b7sen", "ih\u00b7ren", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Stumm ist nun der Klang der Glocken,", "tokens": ["Stumm", "ist", "nun", "der", "Klang", "der", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4ngst der Blumen Duft verweht,", "tokens": ["L\u00e4ngst", "der", "Blu\u00b7men", "Duft", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Stromes Wellen stocken,", "tokens": ["Und", "des", "Stro\u00b7mes", "Wel\u00b7len", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo mein Fu\u00df vor\u00fcbergeht;", "tokens": ["Wo", "mein", "Fu\u00df", "vor\u00b7\u00fc\u00b7ber\u00b7geht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf zum dunkel-abendroten", "tokens": ["Auf", "zum", "dun\u00b7kel\u00b7a\u00b7bend\u00b7ro\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmel blick' ich trauerbang:", "tokens": ["Him\u00b7mel", "blick'", "ich", "trau\u00b7er\u00b7bang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Schatten einer Toten", "tokens": ["Denn", "der", "Schat\u00b7ten", "ei\u00b7ner", "To\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht mit mir das Thal entlang.", "tokens": ["Geht", "mit", "mir", "das", "Thal", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}