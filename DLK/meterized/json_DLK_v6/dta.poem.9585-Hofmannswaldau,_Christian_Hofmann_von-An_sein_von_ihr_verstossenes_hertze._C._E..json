{"dta.poem.9585": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An sein von ihr verstossenes hertze.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje nun/ du armes hertz? wo denckstu ietzt hinaus?", "tokens": ["Wie", "nun", "/", "du", "ar\u00b7mes", "hertz", "?", "wo", "dencks\u00b7tu", "ietzt", "hin\u00b7aus", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$(", "PPER", "ADJA", "NN", "$.", "PWAV", "PDS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo wirstu doch hinfort/ sag\u2019 mir/ wo wirstu bleiben/", "tokens": ["Wo", "wirs\u00b7tu", "doch", "hin\u00b7fort", "/", "sag'", "mir", "/", "wo", "wirs\u00b7tu", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "$(", "VVFIN", "PPER", "$(", "PWAV", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil Solime dich will aus ihrer brust vertreiben/", "tokens": ["Weil", "So\u00b7li\u00b7me", "dich", "will", "aus", "ih\u00b7rer", "brust", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und du auff ewig solt verla\u00dfen dieses hau\u00df?", "tokens": ["Und", "du", "auff", "e\u00b7wig", "solt", "ver\u00b7la\u00b7\u00dfen", "die\u00b7ses", "hau\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJD", "VMFIN", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O mehr als bitterer! o mehr als herber strau\u00df!", "tokens": ["O", "mehr", "als", "bit\u00b7te\u00b7rer", "!", "o", "mehr", "als", "her\u00b7ber", "strau\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "KOKOM", "ADJD", "$.", "FM", "PIAT", "KOKOM", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was hastu denn gethan? nichts; doch wer wird dirs gl\u00e4ubenk", "tokens": ["Was", "has\u00b7tu", "denn", "ge\u00b7than", "?", "nichts", ";", "doch", "wer", "wird", "dirs", "gl\u00e4u\u00b7benk"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "VVPP", "$.", "PIS", "$.", "KON", "PWS", "VAFIN", "PIS", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Was aber s\u00e4umstu viel? was suchstu dich zu str\u00e4uben?", "tokens": ["Was", "a\u00b7ber", "s\u00e4ums\u00b7tu", "viel", "?", "was", "suchs\u00b7tu", "dich", "zu", "str\u00e4u\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "$.", "PWS", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Umsonst/ du armes hertz! nur fort mit dir! heraus!", "tokens": ["Um\u00b7sonst", "/", "du", "ar\u00b7mes", "hertz", "!", "nur", "fort", "mit", "dir", "!", "he\u00b7raus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$(", "PPER", "ADJA", "NN", "$.", "ADV", "PTKVZ", "APPR", "PPER", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wohin/ verworffnes hertz/ wohin wirstu dich f\u00fcgen?", "tokens": ["Wo\u00b7hin", "/", "ver\u00b7worff\u00b7nes", "hertz", "/", "wo\u00b7hin", "wirs\u00b7tu", "dich", "f\u00fc\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "ADJA", "NN", "$(", "PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Chloris ist zu stoltz/ da findstu kein vergn\u00fcgen:", "tokens": ["Die", "Chlo\u00b7ris", "ist", "zu", "stoltz", "/", "da", "finds\u00b7tu", "kein", "ver\u00b7gn\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PTKA", "ADJD", "$(", "ADV", "VVFIN", "PIAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Arminde ist schon todt: Lisett ist gar zu scheu:", "tokens": ["Ar\u00b7min\u00b7de", "ist", "schon", "todt", ":", "Li\u00b7sett", "ist", "gar", "zu", "scheu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$.", "NE", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Libore weit entfernt/ und Philis ungetreu.", "tokens": ["Li\u00b7bo\u00b7re", "weit", "ent\u00b7fernt", "/", "und", "Phi\u00b7lis", "un\u00b7ge\u00b7treu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVPP", "$(", "KON", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bleib! bleib! bey Solimen; sie aber heist dich eilen:", "tokens": ["Bleib", "!", "bleib", "!", "bey", "So\u00b7li\u00b7men", ";", "sie", "a\u00b7ber", "heist", "dich", "ei\u00b7len", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$.", "APPR", "NN", "$.", "PPER", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Fort dann! ach! kanstu nicht ein wenig noch verweilen?", "tokens": ["Fort", "dann", "!", "ach", "!", "kans\u00b7tu", "nicht", "ein", "we\u00b7nig", "noch", "ver\u00b7wei\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "XY", "$.", "VMFIN", "PTKNEG", "ART", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}