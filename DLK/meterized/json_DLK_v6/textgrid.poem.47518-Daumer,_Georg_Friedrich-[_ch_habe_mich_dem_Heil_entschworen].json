{"textgrid.poem.47518": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ ch habe mich dem Heil entschworen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mich dem Heil entschworen,", "tokens": ["Ich", "ha\u00b7be", "mich", "dem", "Heil", "ent\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da wurd' ich in das Heil geboren.", "tokens": ["Da", "wurd'", "ich", "in", "das", "Heil", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht bei den Weisen fand ich Weisheit,", "tokens": ["Nicht", "bei", "den", "Wei\u00b7sen", "fand", "ich", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich fand sie aber bei den Thoren;", "tokens": ["Ich", "fand", "sie", "a\u00b7ber", "bei", "den", "Tho\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich fand sie nicht in B\u00fcchers\u00e4len,", "tokens": ["Ich", "fand", "sie", "nicht", "in", "B\u00fc\u00b7cher\u00b7s\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich fand sie unter Blumenfloren;", "tokens": ["Ich", "fand", "sie", "un\u00b7ter", "Blu\u00b7men\u00b7flo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich habe mich erst selbst gefunden,", "tokens": ["Ich", "ha\u00b7be", "mich", "erst", "selbst", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich mich ganz in ", "tokens": ["Da", "ich", "mich", "ganz", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}