{"textgrid.poem.57191": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[wer vom Ziel nicht wei\u00df]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer vom Ziel nicht wei\u00df,", "tokens": ["Wer", "vom", "Ziel", "nicht", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "kann den Weg nicht haben,", "tokens": ["kann", "den", "Weg", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "wird im selben Kreis", "tokens": ["wird", "im", "sel\u00b7ben", "Kreis"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "all sein Leben traben;", "tokens": ["all", "sein", "Le\u00b7ben", "tra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "kommt am Ende hin,", "tokens": ["kommt", "am", "En\u00b7de", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "wo er herger\u00fcckt,", "tokens": ["wo", "er", "her\u00b7ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "hat der Menge Sinn", "tokens": ["hat", "der", "Men\u00b7ge", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "nur noch mehr zerst\u00fcckt.", "tokens": ["nur", "noch", "mehr", "zer\u00b7st\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wer vom Ziel nichts kennt,", "tokens": ["Wer", "vom", "Ziel", "nichts", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "kann's doch heut erfahren;", "tokens": ["kann's", "doch", "heut", "er\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "wenn es ihn nur brennt", "tokens": ["wenn", "es", "ihn", "nur", "brennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "nach dem G\u00f6ttlich-Wahren;", "tokens": ["nach", "dem", "G\u00f6tt\u00b7lich\u00b7Wah\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "wenn in Eitelkeit", "tokens": ["wenn", "in", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "er nicht ganz versunken", "tokens": ["er", "nicht", "ganz", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "und vom Wein der Zeit", "tokens": ["und", "vom", "Wein", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "nicht bis oben trunken.", "tokens": ["nicht", "bis", "o\u00b7ben", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Denn zu fragen ist", "tokens": ["Denn", "zu", "fra\u00b7gen", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "nach den stillen Dingen,", "tokens": ["nach", "den", "stil\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "und zu wagen ist,", "tokens": ["und", "zu", "wa\u00b7gen", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "will man Licht erringen;", "tokens": ["will", "man", "Licht", "er\u00b7rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "wer nicht suchen kann,", "tokens": ["wer", "nicht", "su\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "wie nur je ein Freier,", "tokens": ["wie", "nur", "je", "ein", "Frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "bleibt im Trugesbann", "tokens": ["bleibt", "im", "Tru\u00b7ges\u00b7bann"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "siebenfacher Schleier.", "tokens": ["sie\u00b7ben\u00b7fa\u00b7cher", "Schlei\u00b7er", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}