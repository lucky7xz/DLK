{"textgrid.poem.36837": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Letztes Lied aus Capri", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Capri werde mir stets der Edelsteine", "tokens": ["Cap\u00b7ri", "wer\u00b7de", "mir", "stets", "der", "E\u00b7del\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wundervollster genannt, den Vater Ocean", "tokens": ["Wun\u00b7der\u00b7volls\u00b7ter", "ge\u00b7nannt", ",", "den", "Va\u00b7ter", "O\u00b7cean"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "ART", "NN", "NE"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mit der Wogen Azur umf\u00e4ngt; kein Eiland", "tokens": ["Mit", "der", "Wo\u00b7gen", "A\u00b7zur", "um\u00b7f\u00e4ngt", ";", "kein", "Ei\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "$.", "PIAT", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sei ihm gleich, ob's mit Weinlaub Bacchus kr\u00e4nze,", "tokens": ["Sei", "ihm", "gleich", ",", "ob's", "mit", "Wein\u00b7laub", "Bac\u00b7chus", "kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "NN", "NE", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ob's in furchtbarem Fels der Vorwelt Schreckni\u00df,", "tokens": ["Ob's", "in", "furcht\u00b7ba\u00b7rem", "Fels", "der", "Vor\u00b7welt", "Schrec\u00b7kni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Den Gedanken der Einsamkeit und deine", "tokens": ["Den", "Ge\u00b7dan\u00b7ken", "der", "Ein\u00b7sam\u00b7keit", "und", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "PPOSAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Werkstatt, Mutter Natur, im Schoo\u00df verberge,", "tokens": ["Werk\u00b7statt", ",", "Mut\u00b7ter", "Na\u00b7tur", ",", "im", "Schoo\u00df", "ver\u00b7ber\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Bl\u00fcthenweckender Hauch des Westes oder", "tokens": ["Bl\u00fc\u00b7then\u00b7we\u00b7cken\u00b7der", "Hauch", "des", "Wes\u00b7tes", "o\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "KON"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Sturm das Haupt ihm umweht, ob's Sitt' und Unschuld", "tokens": ["Sturm", "das", "Haupt", "ihm", "um\u00b7weht", ",", "ob's", "Sitt'", "und", "Un\u00b7schuld"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PPER", "VVFIN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Stillen Fischern, ein Greuel der Geschichte,", "tokens": ["Stil\u00b7len", "Fi\u00b7schern", ",", "ein", "Greu\u00b7el", "der", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+---+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "K\u00fcnft'gen Zeiten zum Graun geweiht: mein Eiland", "tokens": ["K\u00fcnft'\u00b7gen", "Zei\u00b7ten", "zum", "Graun", "ge\u00b7weiht", ":", "mein", "Ei\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVPP", "$.", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Bist du.", "tokens": ["Bist", "du", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "M\u00f6ge kein Fr\u00fchling mir verbl\u00fchen,", "tokens": ["M\u00f6\u00b7ge", "kein", "Fr\u00fch\u00b7ling", "mir", "ver\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Wo dein himmlischer Strand den Gast nicht aufnimmt,", "tokens": ["Wo", "dein", "himm\u00b7li\u00b7scher", "Strand", "den", "Gast", "nicht", "auf\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Nicht den ", "tokens": ["Nicht", "den"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Bin ich dir; mich erkennet Haus und Garten,", "tokens": ["Bin", "ich", "dir", ";", "mich", "er\u00b7ken\u00b7net", "Haus", "und", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Palm' und Feige, mich Fels und Fischerh\u00fctte,", "tokens": ["Palm'", "und", "Fei\u00b7ge", ",", "mich", "Fels", "und", "Fi\u00b7scher\u00b7h\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Mich der Mensch, die Natur; die falsche Meerfluth", "tokens": ["Mich", "der", "Mensch", ",", "die", "Na\u00b7tur", ";", "die", "fal\u00b7sche", "Meer\u00b7fluth"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Ist's allein, die den Kehrenden nicht kennet.", "tokens": ["Ist's", "al\u00b7lein", ",", "die", "den", "Keh\u00b7ren\u00b7den", "nicht", "ken\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "So auch, was ich geliebt, gethan hienieden,", "tokens": ["So", "auch", ",", "was", "ich", "ge\u00b7liebt", ",", "ge\u00b7than", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWS", "PPER", "VVPP", "$,", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Bleibt mir treu ins best\u00e4nd'ge Herz gewurzelt;", "tokens": ["Bleibt", "mir", "treu", "ins", "be\u00b7st\u00e4n\u00b7d'\u00b7ge", "Herz", "ge\u00b7wur\u00b7zelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Mag das wechselnde Schicksal jede Stunde", "tokens": ["Mag", "das", "wech\u00b7seln\u00b7de", "Schick\u00b7sal", "je\u00b7de", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PIAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Die verg\u00e4ngliche Well' im Sturm und Brandung", "tokens": ["Die", "ver\u00b7g\u00e4ng\u00b7li\u00b7che", "Well'", "im", "Sturm", "und", "Bran\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "KON", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Rauschend treiben zum Fels; beharrlich steht er.", "tokens": ["Rau\u00b7schend", "trei\u00b7ben", "zum", "Fels", ";", "be\u00b7harr\u00b7lich", "steht", "er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "APPRART", "NN", "$.", "ADJD", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wo am sch\u00f6nsten erscheinst du mir, o Eiland?", "tokens": ["Wo", "am", "sch\u00f6ns\u00b7ten", "er\u00b7scheinst", "du", "mir", ",", "o", "Ei\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "VVFIN", "PPER", "PPER", "$,", "FM", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ist's, wo Reben, des Geistes voll, den Abhang,", "tokens": ["Ist's", ",", "wo", "Re\u00b7ben", ",", "des", "Geis\u00b7tes", "voll", ",", "den", "Ab\u00b7hang", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NN", "$,", "ART", "NN", "ADJD", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sch\u00f6pferinnen verweg'ner Kraft, mir aufbl\u00fchn,", "tokens": ["Sch\u00f6p\u00b7fe\u00b7rin\u00b7nen", "ver\u00b7weg'\u00b7ner", "Kraft", ",", "mir", "auf\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und die holdere Sonne sie durchgl\u00fchet,", "tokens": ["Und", "die", "hol\u00b7de\u00b7re", "Son\u00b7ne", "sie", "durch\u00b7gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie ein besseres Herz die Liebe? Morgens", "tokens": ["Wie", "ein", "bes\u00b7se\u00b7res", "Herz", "die", "Lie\u00b7be", "?", "Mor\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gern aus Garten und Weinberg seh ich tr\u00e4umend", "tokens": ["Gern", "aus", "Gar\u00b7ten", "und", "Wein\u00b7berg", "seh", "ich", "tr\u00e4u\u00b7mend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "VVPP"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Schimmern Golf und Vesuv und Kap und Inseln,", "tokens": ["Schim\u00b7mern", "Golf", "und", "Ve\u00b7suv", "und", "Kap", "und", "In\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NE", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Ueber Ischia weg, wo weit im Norden,", "tokens": ["Ue\u00b7ber", "I\u00b7schia", "weg", ",", "wo", "weit", "im", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKVZ", "$,", "PWAV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Fast dem Himmel verm\u00e4hlt, der Circe zaubrisch", "tokens": ["Fast", "dem", "Him\u00b7mel", "ver\u00b7m\u00e4hlt", ",", "der", "Cir\u00b7ce", "zaub\u00b7risch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVPP", "$,", "ART", "NN", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Vorgebirge mich lange t\u00e4uscht, ob's Nebel", "tokens": ["Vor\u00b7ge\u00b7bir\u00b7ge", "mich", "lan\u00b7ge", "t\u00e4uscht", ",", "ob's", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "ADV", "VVFIN", "$,", "KOUS", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Oder Wirkliches ist. Dem Schmerz erscheinet", "tokens": ["O\u00b7der", "Wirk\u00b7li\u00b7ches", "ist", ".", "Dem", "Schmerz", "er\u00b7schei\u00b7net"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "$.", "ART", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "So vergangenes Gl\u00fcck. Vergieb der Sehnsucht", "tokens": ["So", "ver\u00b7gan\u00b7ge\u00b7nes", "Gl\u00fcck", ".", "Ver\u00b7gieb", "der", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$.", "NN", "ART", "NN"], "meter": "--+--+-+---", "measure": "anapaest.di.plus"}, "line.13": {"text": "Dieses Herzens, ich denke Roms.", "tokens": ["Die\u00b7ses", "Her\u00b7zens", ",", "ich", "den\u00b7ke", "Roms", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPER", "VVFIN", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Doch ewig,", "tokens": ["Doch", "e\u00b7wig", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Strand der Einsamkeit, auserw\u00e4hlet seist du,", "tokens": ["Strand", "der", "Ein\u00b7sam\u00b7keit", ",", "au\u00b7ser\u00b7w\u00e4h\u00b7let", "seist", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVPP", "VAFIN", "PPER", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Wo schreckhaft in des S\u00fcdmeers wilde Brandung", "tokens": ["Wo", "schreck\u00b7haft", "in", "des", "S\u00fcd\u00b7meers", "wil\u00b7de", "Bran\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Niedergraut des Solaro Fels, dem Vogel", "tokens": ["Nie\u00b7der\u00b7graut", "des", "So\u00b7la\u00b7ro", "Fels", ",", "dem", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NE", "NE", "$,", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Kahle, wolkenumrauschte Wohnung. Menschen", "tokens": ["Kah\u00b7le", ",", "wol\u00b7ke\u00b7num\u00b7rauschte", "Woh\u00b7nung", ".", "Men\u00b7schen"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Trifft mein Auge hier nicht, dem Oceane", "tokens": ["Trifft", "mein", "Au\u00b7ge", "hier", "nicht", ",", "dem", "O\u00b7cea\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "$,", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Preis gegeben, erscheint die Welt, in Tr\u00fcmmer", "tokens": ["Preis", "ge\u00b7ge\u00b7ben", ",", "er\u00b7scheint", "die", "Welt", ",", "in", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$,", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.21": {"text": "Liegt zersplittert der Fels, doch nur am Fu\u00dfe,", "tokens": ["Liegt", "zer\u00b7split\u00b7tert", "der", "Fels", ",", "doch", "nur", "am", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Dem j\u00e4h starrend erhebt sein stolzes Haupt sich", "tokens": ["Dem", "j\u00e4h", "star\u00b7rend", "er\u00b7hebt", "sein", "stol\u00b7zes", "Haupt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "VVFIN", "PPOSAT", "ADJA", "NN", "PRF"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Und den Scheitel bekr\u00f6nt die k\u00fchne Burg ihm,", "tokens": ["Und", "den", "Schei\u00b7tel", "be\u00b7kr\u00f6nt", "die", "k\u00fch\u00b7ne", "Burg", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Unten aber umtost, vom Schaum des Meeres", "tokens": ["Un\u00b7ten", "a\u00b7ber", "um\u00b7tost", ",", "vom", "Schaum", "des", "Mee\u00b7res"], 