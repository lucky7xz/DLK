{"textgrid.poem.55813": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den ganzen Tag hindurch bin ich besonnen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den ganzen Tag hindurch bin ich besonnen", "tokens": ["Den", "gan\u00b7zen", "Tag", "hin\u00b7durch", "bin", "ich", "be\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wei\u00df es so genau als irgend einer,", "tokens": ["Und", "wei\u00df", "es", "so", "ge\u00b7nau", "als", "ir\u00b7gend", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ADV", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dass du gestorben bist und dass man Dich", "tokens": ["Dass", "du", "ge\u00b7stor\u00b7ben", "bist", "und", "dass", "man", "Dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "KON", "KOUS", "PIS", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weit drau\u00dfen auf den gro\u00dfen, grellbesonnten,", "tokens": ["Weit", "drau\u00b7\u00dfen", "auf", "den", "gro\u00b7\u00dfen", ",", "grell\u00b7be\u00b7sonn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kasernenkahlen Friedhof eingegraben,", "tokens": ["Ka\u00b7ser\u00b7nen\u00b7kah\u00b7len", "Fried\u00b7hof", "ein\u00b7ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Wei\u00df, dass ich selber eine braune Scholle", "tokens": ["Wei\u00df", ",", "dass", "ich", "sel\u00b7ber", "ei\u00b7ne", "brau\u00b7ne", "Schol\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und einen Veilchenkranz Dir nachgeworfen.", "tokens": ["Und", "ei\u00b7nen", "Veil\u00b7chen\u00b7kranz", "Dir", "nach\u00b7ge\u00b7wor\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zur D\u00e4mmerzeit jedoch, da werd' ich wankend,", "tokens": ["Zur", "D\u00e4m\u00b7mer\u00b7zeit", "je\u00b7doch", ",", "da", "werd'", "ich", "wan\u00b7kend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und insgeheim erwacht in mir der Zweifel,", "tokens": ["Und", "ins\u00b7ge\u00b7heim", "er\u00b7wacht", "in", "mir", "der", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Obs wirklich wahr ist oder blo\u00df getr\u00e4umt,", "tokens": ["Obs", "wirk\u00b7lich", "wahr", "ist", "o\u00b7der", "blo\u00df", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "VAFIN", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ob das nicht irgend eine Fremde war,", "tokens": ["Ob", "das", "nicht", "ir\u00b7gend", "ei\u00b7ne", "Frem\u00b7de", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PTKNEG", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu deren Leichenzug ein blinder Zufall", "tokens": ["Zu", "de\u00b7ren", "Lei\u00b7chen\u00b7zug", "ein", "blin\u00b7der", "Zu\u00b7fall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An jenem Fr\u00fchlingsmorgen mich gef\u00fchrt.", "tokens": ["An", "je\u00b7nem", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen", "mich", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ob ich in jener dumpfig-k\u00fchlen Kammer", "tokens": ["Ob", "ich", "in", "je\u00b7ner", "dump\u00b7fig\u00b7k\u00fch\u00b7len", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit ihren wei\u00dfget\u00fcnchten, nackten W\u00e4nden", "tokens": ["Mit", "ih\u00b7ren", "wei\u00df\u00b7ge\u00b7t\u00fcnch\u00b7ten", ",", "nack\u00b7ten", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nicht eine m\u00fcdgespielte Schl\u00e4ferin nur", "tokens": ["Nicht", "ei\u00b7ne", "m\u00fcd\u00b7ge\u00b7spiel\u00b7te", "Schl\u00e4\u00b7fe\u00b7rin", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Gek\u00fcsst und keine Todte.", "tokens": ["Ge\u00b7k\u00fcsst", "und", "kei\u00b7ne", "Tod\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Sah sie doch", "tokens": ["Sah", "sie", "doch"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "So gar nicht todt aus! Nein, so rosig frisch,", "tokens": ["So", "gar", "nicht", "todt", "aus", "!", "Nein", ",", "so", "ro\u00b7sig", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADJD", "PTKVZ", "$.", "PTKANT", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So ganz wie alle Tage, gar nicht schrecklich.", "tokens": ["So", "ganz", "wie", "al\u00b7le", "Ta\u00b7ge", ",", "gar", "nicht", "schreck\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "PIAT", "NN", "$,", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und sehnsuchtsvoll durchirrt mein Aug' die Stra\u00dfen", "tokens": ["Und", "sehn\u00b7suchts\u00b7voll", "durch\u00b7irrt", "mein", "Aug'", "die", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und h\u00e4lt an jeder Ecke wartend still,", "tokens": ["Und", "h\u00e4lt", "an", "je\u00b7der", "E\u00b7cke", "war\u00b7tend", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ob nicht die zarte, reizende Figur", "tokens": ["Ob", "nicht", "die", "zar\u00b7te", ",", "rei\u00b7zen\u00b7de", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Mit ihrem lieben Amorettenkopf", "tokens": ["Mit", "ih\u00b7rem", "lie\u00b7ben", "A\u00b7mo\u00b7ret\u00b7ten\u00b7kopf"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und ihren Z\u00f6pfen, schwer und dunkelbraun,", "tokens": ["Und", "ih\u00b7ren", "Z\u00f6p\u00b7fen", ",", "schwer", "und", "dun\u00b7kel\u00b7braun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Auf einmal aus dem Menschentrubel auftaucht", "tokens": ["Auf", "ein\u00b7mal", "aus", "dem", "Men\u00b7schen\u00b7tru\u00b7bel", "auf\u00b7taucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und mir entgegenruft, erregt und lachend,", "tokens": ["Und", "mir", "ent\u00b7ge\u00b7gen\u00b7ruft", ",", "er\u00b7regt", "und", "la\u00b7chend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Um staunende Passanten unbek\u00fcmmert,", "tokens": ["Um", "stau\u00b7nen\u00b7de", "Pas\u00b7san\u00b7ten", "un\u00b7be\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Da bin ich schon, mein lieber Bub'. Gr\u00fc\u00df' Gott!", "tokens": ["Da", "bin", "ich", "schon", ",", "mein", "lie\u00b7ber", "Bub'", ".", "Gr\u00fc\u00df'", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.24": {"text": "Das ist am Abend, aber ach! am Tag'", "tokens": ["Das", "ist", "am", "A\u00b7bend", ",", "a\u00b7ber", "ach", "!", "am", "Tag'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "$,", "ADV", "ADV", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Da tr\u00e4um' ich nicht, da wei\u00df ich alles, alles ...", "tokens": ["Da", "tr\u00e4um'", "ich", "nicht", ",", "da", "wei\u00df", "ich", "al\u00b7les", ",", "al\u00b7les", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PIS", "$,", "PIS", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}