{"textgrid.poem.49977": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "9. Auf Dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als einst Susann entbl\u00f6sst im k\u00fchlen Brunnen stand,", "tokens": ["Als", "einst", "Su\u00b7sann", "ent\u00b7bl\u00f6sst", "im", "k\u00fch\u00b7len", "Brun\u00b7nen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "VVFIN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die zwey geile Greis' an ihrer Seite fand,", "tokens": ["Und", "die", "zwey", "gei\u00b7le", "Greis'", "an", "ih\u00b7rer", "Sei\u00b7te", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "CARD", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So schaute man, wie hier in einerley Gefahr", "tokens": ["So", "schau\u00b7te", "man", ",", "wie", "hier", "in", "ei\u00b7ner\u00b7ley", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}