{"textgrid.poem.31575": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bisher hat mir dein Geist die Gortheits-Sonn gezeiget.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bisher hat mir dein Geist die Gortheits-Sonn gezeiget.", "tokens": ["Bis\u00b7her", "hat", "mir", "dein", "Geist", "die", "Gort\u00b7heits\u00b7Sonn", "ge\u00b7zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ich hab'in seinem Liecht ihr Wunderliecht entdeckt.", "tokens": ["ich", "hab'\u00b7in", "sei\u00b7nem", "Liecht", "ihr", "Wun\u00b7der\u00b7liecht", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "doch hat er mir das Ziel des Forschung-Flugs gesteckt:", "tokens": ["doch", "hat", "er", "mir", "das", "Ziel", "des", "For\u00b7schung\u00b7Flugs", "ge\u00b7steckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "weil mehr Etk\u00e4ntnu\u00df aus der Seelen nicht gezweiget.", "tokens": ["weil", "mehr", "Et\u00b7k\u00e4nt\u00b7nu\u00df", "aus", "der", "See\u00b7len", "nicht", "ge\u00b7zwei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Mein Geist sich in die Tieff des Gnaden-Meeres neiget /", "tokens": ["Mein", "Geist", "sich", "in", "die", "Tieff", "des", "Gna\u00b7den\u00b7Mee\u00b7res", "nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "do man selbselbsten dich in deiner G\u00fcte schm\u00e4ckt.", "tokens": ["do", "man", "selb\u00b7selbs\u00b7ten", "dich", "in", "dei\u00b7ner", "G\u00fc\u00b7te", "schm\u00e4ckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich habe dich in mir zu suchen auch erkeckt /", "tokens": ["Ich", "ha\u00b7be", "dich", "in", "mir", "zu", "su\u00b7chen", "auch", "er\u00b7keckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPER", "PTKZU", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "weil du in deiner Gnad / sie in mir sich ereiget.", "tokens": ["weil", "du", "in", "dei\u00b7ner", "Gnad", "/", "sie", "in", "mir", "sich", "er\u00b7ei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$(", "PPER", "APPR", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vergib mir ich bekenn's! di\u00df Wort scheint sehr vermessen:", "tokens": ["Ver\u00b7gib", "mir", "ich", "be\u00b7kenn's", "!", "di\u00df", "Wort", "scheint", "sehr", "ver\u00b7mes\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PTKVZ", "$.", "PDS", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "doch ist es wahr / da\u00df ich nicht ohne deine Gnad /", "tokens": ["doch", "ist", "es", "wahr", "/", "da\u00df", "ich", "nicht", "oh\u00b7ne", "dei\u00b7ne", "Gnad", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$(", "KOUS", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "ein F\u00fcnklein k\u00f6nt verstehn / wann sie mich nicht besessen.", "tokens": ["ein", "F\u00fcn\u00b7klein", "k\u00f6nt", "ver\u00b7stehn", "/", "wann", "sie", "mich", "nicht", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$(", "PWAV", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es ist die Dankbarkeit der Wei\u00dfheits-Gaben Pfad.", "tokens": ["Es", "ist", "die", "Dank\u00b7bar\u00b7keit", "der", "Wei\u00df\u00b7heits\u00b7Ga\u00b7ben", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Gott! sey hoch gepreist vor solch' Erk\u00e4ntnus Blicke /", "tokens": ["Mein", "Gott", "!", "sey", "hoch", "ge\u00b7preist", "vor", "solch'", "Er\u00b7k\u00e4nt\u00b7nus", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VAFIN", "ADJD", "VVPP", "APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und bald im Himmel mich mit vollem Schein erquicke.", "tokens": ["und", "bald", "im", "Him\u00b7mel", "mich", "mit", "vol\u00b7lem", "Schein", "er\u00b7qui\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}