{"textgrid.poem.59625": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Feldmarschall", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was blasen die Trompeten? Husaren, heraus!", "tokens": ["Was", "bla\u00b7sen", "die", "Trom\u00b7pe\u00b7ten", "?", "Hu\u00b7sa\u00b7ren", ",", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es reitet der Feldmarschall im fliegenden Saus,", "tokens": ["Es", "rei\u00b7tet", "der", "Feld\u00b7mar\u00b7schall", "im", "flie\u00b7gen\u00b7den", "Saus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er reitet so freudig sein mutiges Pferd,", "tokens": ["Er", "rei\u00b7tet", "so", "freu\u00b7dig", "sein", "mu\u00b7ti\u00b7ges", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er schwinget so schneidig sein blitzendes Schwert.", "tokens": ["Er", "schwin\u00b7get", "so", "schnei\u00b7dig", "sein", "blit\u00b7zen\u00b7des", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "O schauet, wie ihm leuchten die Augen so klar!", "tokens": ["O", "schau\u00b7et", ",", "wie", "ihm", "leuch\u00b7ten", "die", "Au\u00b7gen", "so", "klar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O schauet, wie ihm wallet sein schneewei\u00dfes Haar!", "tokens": ["O", "schau\u00b7et", ",", "wie", "ihm", "wal\u00b7let", "sein", "schnee\u00b7wei\u00b7\u00dfes", "Haar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So frisch bl\u00fcht sein Alter wie greisender Wein,", "tokens": ["So", "frisch", "bl\u00fcht", "sein", "Al\u00b7ter", "wie", "grei\u00b7sen\u00b7der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Drum kann er Verwalter des Schlachtfeldes sein.", "tokens": ["Drum", "kann", "er", "Ver\u00b7wal\u00b7ter", "des", "Schlacht\u00b7fel\u00b7des", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Der Mann ist er gewesen, als alles versank,", "tokens": ["Der", "Mann", "ist", "er", "ge\u00b7we\u00b7sen", ",", "als", "al\u00b7les", "ver\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VAPP", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der mutig auf gen Himmel den Degen noch schwang,", "tokens": ["Der", "mu\u00b7tig", "auf", "gen", "Him\u00b7mel", "den", "De\u00b7gen", "noch", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "APPR", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da schwur er beim Eisen gar zornig und hart,", "tokens": ["Da", "schwur", "er", "beim", "Ei\u00b7sen", "gar", "zor\u00b7nig", "und", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den Welschen zu weisen die deutscheste Art.", "tokens": ["Den", "Wel\u00b7schen", "zu", "wei\u00b7sen", "die", "deut\u00b7sches\u00b7te", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Den Schwur hat er gehalten. Als Kriegsruf erklang,", "tokens": ["Den", "Schwur", "hat", "er", "ge\u00b7hal\u00b7ten", ".", "Als", "Kriegs\u00b7ruf", "er\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$.", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hei! wie der wei\u00dfe J\u00fcngling in 'n Sattel sich schwang,", "tokens": ["Hei", "!", "wie", "der", "wei\u00b7\u00dfe", "J\u00fcng\u00b7ling", "in", "'n", "Sat\u00b7tel", "sich", "schwang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Da ist er's gewesen, der Kehraus gemacht,", "tokens": ["Da", "ist", "er's", "ge\u00b7we\u00b7sen", ",", "der", "Ke\u00b7hraus", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VAPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit eisernen Besen das Land rein gemacht.", "tokens": ["Mit", "ei\u00b7ser\u00b7nen", "Be\u00b7sen", "das", "Land", "rein", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Bei L\u00fctzen auf der Aue er hielt solchen Strau\u00df,", "tokens": ["Bei", "L\u00fct\u00b7zen", "auf", "der", "Au\u00b7e", "er", "hielt", "sol\u00b7chen", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df vielen tausend Welschen der Atem ging aus,", "tokens": ["Da\u00df", "vie\u00b7len", "tau\u00b7send", "Wel\u00b7schen", "der", "A\u00b7tem", "ging", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "CARD", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df Tausende liefen dort hasigen Lauf,", "tokens": ["Da\u00df", "Tau\u00b7sen\u00b7de", "lie\u00b7fen", "dort", "ha\u00b7si\u00b7gen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zehntausend entschliefen, die nimmer wachen auf.", "tokens": ["Zehn\u00b7tau\u00b7send", "ent\u00b7schlie\u00b7fen", ",", "die", "nim\u00b7mer", "wa\u00b7chen", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PRELS", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Am Wasser der Katzbach er's auch hat bew\u00e4hrt,", "tokens": ["Am", "Was\u00b7ser", "der", "Katz\u00b7bach", "er's", "auch", "hat", "be\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NE", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da hat er den Franzosen das Schwimmen gelehrt:", "tokens": ["Da", "hat", "er", "den", "Fran\u00b7zo\u00b7sen", "das", "Schwim\u00b7men", "ge\u00b7lehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Fahrt wohl, ihr Franzosen, zur Ostsee hinab!", "tokens": ["Fahrt", "wohl", ",", "ihr", "Fran\u00b7zo\u00b7sen", ",", "zur", "Ost\u00b7see", "hin\u00b7ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und nehmt, Ohnehosen, den Walfisch zum Grab.", "tokens": ["Und", "nehmt", ",", "Oh\u00b7ne\u00b7ho\u00b7sen", ",", "den", "Wal\u00b7fisch", "zum", "Grab", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Bei Wartburg an der Elbe wie fuhr er hindurch!", "tokens": ["Bei", "Wart\u00b7burg", "an", "der", "El\u00b7be", "wie", "fuhr", "er", "hin\u00b7durch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NE", "KOKOM", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da schirmte die Franzosen nicht Schanze noch Burg,", "tokens": ["Da", "schirm\u00b7te", "die", "Fran\u00b7zo\u00b7sen", "nicht", "Schan\u00b7ze", "noch", "Burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da mu\u00dften sie springen wie Hasen \u00fcbers Feld,", "tokens": ["Da", "mu\u00df\u00b7ten", "sie", "sprin\u00b7gen", "wie", "Ha\u00b7sen", "\u00fc\u00b7bers", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "KOKOM", "NE", "APPRART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hinterdrein lie\u00df erklingen sein Hussa! der Held.", "tokens": ["Hin\u00b7ter\u00b7drein", "lie\u00df", "er\u00b7klin\u00b7gen", "sein", "Hus\u00b7sa", "!", "der", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "VAINF", "NE", "$.", "ART", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Bei Leipzig auf dem Plane, o herrliche Schlacht!", "tokens": ["Bei", "Leip\u00b7zig", "auf", "dem", "Pla\u00b7ne", ",", "o", "herr\u00b7li\u00b7che", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da brach er den Franzosen das Gl\u00fcck und die Macht,", "tokens": ["Da", "brach", "er", "den", "Fran\u00b7zo\u00b7sen", "das", "Gl\u00fcck", "und", "die", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da lagen sie sicher nach blutigem Fall,", "tokens": ["Da", "la\u00b7gen", "sie", "si\u00b7cher", "nach", "blu\u00b7ti\u00b7gem", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da ward der Herr Bl\u00fccher ein Feldmarschall.", "tokens": ["Da", "ward", "der", "Herr", "Bl\u00fc\u00b7cher", "ein", "Feld\u00b7mar\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NE", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Drum blaset, ihr Trompeten! Husaren, heraus!", "tokens": ["Drum", "bla\u00b7set", ",", "ihr", "Trom\u00b7pe\u00b7ten", "!", "Hu\u00b7sa\u00b7ren", ",", "he\u00b7raus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PPOSAT", "NN", "$.", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du reite, Herr Feldmarschall, wie Winde im Saus!", "tokens": ["Du", "rei\u00b7te", ",", "Herr", "Feld\u00b7mar\u00b7schall", ",", "wie", "Win\u00b7de", "im", "Saus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "NN", "$,", "PWAV", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Siege entgegen, zum Rhein, \u00fcbern Rhein!", "tokens": ["Dem", "Sie\u00b7ge", "ent\u00b7ge\u00b7gen", ",", "zum", "Rhein", ",", "\u00fc\u00b7bern", "Rhein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPRART", "NE", "$,", "VVFIN", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Du tapferer Degen, in Frankreich hinein!", "tokens": ["Du", "tap\u00b7fe\u00b7rer", "De\u00b7gen", ",", "in", "Fran\u00b7kreich", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "NE", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}