{"textgrid.poem.38240": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Bienenlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Liedlein will ich singen,", "tokens": ["Ein", "Lied\u00b7lein", "will", "ich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Honigv\u00f6gelein,", "tokens": ["Vom", "Ho\u00b7nig\u00b7v\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die hin und her sich schwingen,", "tokens": ["Die", "hin", "und", "her", "sich", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "KON", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo bunte Blumen seyn.", "tokens": ["Wo", "bun\u00b7te", "Blu\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das V\u00f6lklein in dem Gr\u00fcnen,", "tokens": ["Das", "V\u00f6l\u00b7klein", "in", "dem", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es schmauset auf der Weid,", "tokens": ["Es", "schmau\u00b7set", "auf", "der", "Weid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich singe von den Bienen,", "tokens": ["Ich", "sin\u00b7ge", "von", "den", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf dieser freien Haid.", "tokens": ["Auf", "die\u00b7ser", "frei\u00b7en", "Haid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Winter h\u00e4lt gefangen", "tokens": ["Der", "Win\u00b7ter", "h\u00e4lt", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das zarte Jungfernvolk,", "tokens": ["Das", "zar\u00b7te", "Jung\u00b7fern\u00b7volk", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis da\u00df der Schnee vergangen,", "tokens": ["Bis", "da\u00df", "der", "Schnee", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frost, Schauer, Nebelwolk.", "tokens": ["Frost", ",", "Schau\u00b7er", ",", "Ne\u00b7bel\u00b7wolk", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wann die Weste stimmen,", "tokens": ["Und", "wann", "die", "Wes\u00b7te", "stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach linder Lenzen Art,", "tokens": ["Nach", "lin\u00b7der", "Len\u00b7zen", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So machen sich die Immen", "tokens": ["So", "ma\u00b7chen", "sich", "die", "Im\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf ihre Blumenfahrt.", "tokens": ["Auf", "ih\u00b7re", "Blu\u00b7men\u00b7fahrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie ziehen mit der Trummel,", "tokens": ["Sie", "zie\u00b7hen", "mit", "der", "Trum\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Stachel weist das Schwerdt;", "tokens": ["Der", "Sta\u00b7chel", "weist", "das", "Schwerdt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Brummel und Gehummel", "tokens": ["Ihr", "Brum\u00b7mel", "und", "Ge\u00b7hum\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat niemand noch gef\u00e4hrdt.", "tokens": ["Hat", "nie\u00b7mand", "noch", "ge\u00b7f\u00e4hrdt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie nehmen sonder Morden", "tokens": ["Sie", "neh\u00b7men", "son\u00b7der", "Mor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den zarten Blumenraub,", "tokens": ["Den", "zar\u00b7ten", "Blu\u00b7men\u00b7raub", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ihre Beut ist worden", "tokens": ["Und", "ih\u00b7re", "Beut", "ist", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Baum und Bl\u00fcthen Laub.", "tokens": ["Der", "Baum", "und", "Bl\u00fc\u00b7then", "Laub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie sie die Wachsburg bauen,", "tokens": ["Wie", "sie", "die", "Wachs\u00b7burg", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus g\u00fcldnem Pergament,", "tokens": ["Aus", "g\u00fcld\u00b7nem", "Per\u00b7ga\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann niemand nicht beschauen,", "tokens": ["Kann", "nie\u00b7mand", "nicht", "be\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja keines K\u00fcnstlers H\u00e4nd", "tokens": ["Ja", "kei\u00b7nes", "K\u00fcnst\u00b7lers", "H\u00e4nd"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PIAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hat man so sehr bewundert,", "tokens": ["Hat", "man", "so", "sehr", "be\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Zimmerchen so gleich,", "tokens": ["Die", "Zim\u00b7mer\u00b7chen", "so", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sechseckigt ist gesondert", "tokens": ["Sechs\u00b7ec\u00b7kigt", "ist", "ge\u00b7son\u00b7dert"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Honigk\u00f6nigreich.", "tokens": ["Das", "Ho\u00b7nig\u00b7k\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Man sieht sie friedlich leben", "tokens": ["Man", "sieht", "sie", "fried\u00b7lich", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ohn Eigennutz und Streit,", "tokens": ["Ohn", "Ei\u00b7gen\u00b7nutz", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In steter M\u00fche weben,", "tokens": ["In", "ste\u00b7ter", "M\u00fc\u00b7he", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Lenz und Winterszeit;", "tokens": ["Zu", "Lenz", "und", "Win\u00b7ter\u00b7szeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie pflegen einzutragen", "tokens": ["Sie", "pfle\u00b7gen", "ein\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Blumen Saft und Thau,", "tokens": ["Der", "Blu\u00b7men", "Saft", "und", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und f\u00fchren mit Behagen", "tokens": ["Und", "f\u00fch\u00b7ren", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gesammt den Zuckerbau.", "tokens": ["Ge\u00b7sammt", "den", "Zu\u00b7cker\u00b7bau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}