{"textgrid.poem.55744": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dunklen Seelentiefen", "tokens": ["Aus", "dunk\u00b7len", "See\u00b7len\u00b7tie\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steigt's herzersch\u00fctternd empor,", "tokens": ["Steigt's", "her\u00b7zer\u00b7sch\u00fct\u00b7ternd", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Es quellen qualengepeitschte,", "tokens": ["Es", "quel\u00b7len", "qua\u00b7len\u00b7ge\u00b7peitschte", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wildschmerzliche Tr\u00e4nen hervor.", "tokens": ["Wild\u00b7schmerz\u00b7li\u00b7che", "Tr\u00e4\u00b7nen", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "++--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Wie hab' ich einst empfunden", "tokens": ["Wie", "hab'", "ich", "einst", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So lauter, keusch und tief,", "tokens": ["So", "lau\u00b7ter", ",", "keusch", "und", "tief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eh' Dein ber\u00fcckender Anblick", "tokens": ["Eh'", "Dein", "be\u00b7r\u00fc\u00b7cken\u00b7der", "An\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die schlafende Wollust rief.", "tokens": ["Die", "schla\u00b7fen\u00b7de", "Wol\u00b7lust", "rief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}