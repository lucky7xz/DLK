{"textgrid.poem.40832": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Pompeji's B\u00fcrger, du, mit dessen Aschen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pompeji's B\u00fcrger, du, mit dessen Aschen", "tokens": ["Pom\u00b7pe\u00b7ji's", "B\u00fcr\u00b7ger", ",", "du", ",", "mit", "des\u00b7sen", "A\u00b7schen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht gerad vorbei die Winde spielen,", "tokens": ["Viel\u00b7leicht", "ge\u00b7rad", "vor\u00b7bei", "die", "Win\u00b7de", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die vor mir, t\u00e4ndelnd, Reb' und Rose haschen", "tokens": ["Die", "vor", "mir", ",", "t\u00e4n\u00b7delnd", ",", "Reb'", "und", "Ro\u00b7se", "ha\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "$,", "VVPP", "$,", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in des Mittags Sonnenlocken w\u00fchlen!", "tokens": ["Und", "in", "des", "Mit\u00b7tags", "Son\u00b7nen\u00b7lo\u00b7cken", "w\u00fch\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein ist das Haus, das ich, dein Gast, begr\u00fc\u00dfe,", "tokens": ["Dein", "ist", "das", "Haus", ",", "das", "ich", ",", "dein", "Gast", ",", "be\u00b7gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sich versp\u00e4tet um zweitausend Jahre!", "tokens": ["Der", "sich", "ver\u00b7sp\u00e4\u00b7tet", "um", "zweit\u00b7au\u00b7send", "Jah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du bist ein Mann, mit dem sich's leben lie\u00dfe,", "tokens": ["Du", "bist", "ein", "Mann", ",", "mit", "dem", "sich's", "le\u00b7ben", "lie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und freundlich hei\u00dft willkommen mich dein Lare.", "tokens": ["Und", "freund\u00b7lich", "hei\u00dft", "will\u00b7kom\u00b7men", "mich", "dein", "La\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Nachstammelt dir's der Mosaik seit Jahren;", "tokens": ["Nach\u00b7stam\u00b7melt", "dir's", "der", "Mo\u00b7saik", "seit", "Jah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es gilt auch mir, wie einst den Nachbarn allen,", "tokens": ["Es", "gilt", "auch", "mir", ",", "wie", "einst", "den", "Nach\u00b7barn", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "$,", "PWAV", "ADV", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die jetzt mit dir dahin im Winde fahren.", "tokens": ["Die", "jetzt", "mit", "dir", "da\u00b7hin", "im", "Win\u00b7de", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "PAV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du wirst nicht z\u00fcrnen des Besuchs, des sp\u00e4ten,", "tokens": ["Du", "wirst", "nicht", "z\u00fcr\u00b7nen", "des", "Be\u00b7suchs", ",", "des", "sp\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVINF", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Inde\u00df auch ich's dem Hausherrn nicht verarge,", "tokens": ["In\u00b7de\u00df", "auch", "ich's", "dem", "Haus\u00b7herrn", "nicht", "ver\u00b7ar\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er statt Purpurkissen, Goldtapeten", "tokens": ["Da\u00df", "er", "statt", "Pur\u00b7pur\u00b7kis\u00b7sen", ",", "Gold\u00b7ta\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Sitz mir bietet nur die\u00df Moos, das karge.", "tokens": ["Zum", "Sitz", "mir", "bie\u00b7tet", "nur", "die\u00df", "Moos", ",", "das", "kar\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "ADV", "PDS", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wohl werden deine Laren sich vertragen", "tokens": ["Wohl", "wer\u00b7den", "dei\u00b7ne", "La\u00b7ren", "sich", "ver\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit meinen Hauskobolden g\u00fctlich k\u00f6nnen!", "tokens": ["Mit", "mei\u00b7nen", "Haus\u00b7ko\u00b7bol\u00b7den", "g\u00fct\u00b7lich", "k\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sie sich auch mit Kohlenbr\u00e4nden schlagen,", "tokens": ["Wenn", "sie", "sich", "auch", "mit", "Koh\u00b7len\u00b7br\u00e4n\u00b7den", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sie nur uns die Sch\u00fcsseln nicht verbrennen!", "tokens": ["Da\u00df", "sie", "nur", "uns", "die", "Sch\u00fcs\u00b7seln", "nicht", "ver\u00b7bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sind Deck' und Goldgeb\u00e4lk' auch l\u00e4ngst in Tr\u00fcmmern,", "tokens": ["Sind", "Deck'", "und", "Gold\u00b7ge\u00b7b\u00e4l\u00b7k'", "auch", "l\u00e4ngst", "in", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Deckt blauer Himmel uns auch nur statt ihnen,", "tokens": ["Deckt", "blau\u00b7er", "Him\u00b7mel", "uns", "auch", "nur", "statt", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPER", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin ein milder Gast und seh' ihn schimmern", "tokens": ["Ich", "bin", "ein", "mil\u00b7der", "Gast", "und", "seh'", "ihn", "schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als deine seidnen, blauen Festgardinen.", "tokens": ["Als", "dei\u00b7ne", "seid\u00b7nen", ",", "blau\u00b7en", "Fest\u00b7gar\u00b7di\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und sengt die Sonn' auch brennend meinen Scheitel,", "tokens": ["Und", "sengt", "die", "Sonn'", "auch", "bren\u00b7nend", "mei\u00b7nen", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie sei des Schweigens Rose, will ich schw\u00f6ren,", "tokens": ["Sie", "sei", "des", "Schwei\u00b7gens", "Ro\u00b7se", ",", "will", "ich", "schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gen deren Pracht selbst P\u00e4stums Rosen eitel,", "tokens": ["Gen", "de\u00b7ren", "Pracht", "selbst", "P\u00e4s\u00b7tums", "Ro\u00b7sen", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRELAT", "NN", "ADV", "NE", "NE", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und die du aufgesteckt dem Gast zu Ehren.", "tokens": ["Und", "die", "du", "auf\u00b7ge\u00b7steckt", "dem", "Gast", "zu", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Des Epheus Schnur, drauf die Cicade schaukelt,", "tokens": ["Des", "E\u00b7pheus", "Schnur", ",", "drauf", "die", "Ci\u00b7ca\u00b7de", "schau\u00b7kelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist \u00fcber'm Haupt als Seil uns aufgehangen,", "tokens": ["Ist", "\u00fc\u00b7ber'm", "Haupt", "als", "Seil", "uns", "auf\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "KOKOM", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drauf uns dein Gaukler seine Spr\u00fcnge gaukelt.", "tokens": ["Drauf", "uns", "dein", "Gauk\u00b7ler", "sei\u00b7ne", "Spr\u00fcn\u00b7ge", "gau\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir brauchen seines Sturzes nicht zu bangen!", "tokens": ["Wir", "brau\u00b7chen", "sei\u00b7nes", "Stur\u00b7zes", "nicht", "zu", "ban\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Hier ist auch Amor! Seine Siege blieben", "tokens": ["Hier", "ist", "auch", "A\u00b7mor", "!", "Sei\u00b7ne", "Sie\u00b7ge", "blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NE", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verewigt an der Wand von Farbendichtern!", "tokens": ["Ve\u00b7re\u00b7wigt", "an", "der", "Wand", "von", "Far\u00b7ben\u00b7dich\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Zwar etwas derb und keck! Doch scheint's, im Lieben", "tokens": ["Zwar", "et\u00b7was", "derb", "und", "keck", "!", "Doch", "scheint's", ",", "im", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "$.", "KON", "VVFIN", "$,", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ists besser allzukeck, als allzusch\u00fcchtern!", "tokens": ["Ists", "bes\u00b7ser", "all\u00b7zu\u00b7keck", ",", "als", "all\u00b7zu\u00b7sch\u00fcch\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "KOUS", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Bacchustroph\u00e4en, Amphor'n in den Hallen", "tokens": ["Bac\u00b7chus\u00b7trop\u00b7h\u00e4\u00b7en", ",", "Am\u00b7phor'n", "in", "den", "Hal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "APPR", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Zerstreut, wie trunkene Bacchanten, liegen;", "tokens": ["Zer\u00b7streut", ",", "wie", "trun\u00b7ke\u00b7ne", "Bac\u00b7chan\u00b7ten", ",", "lie\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist auch mit Asch' ihr Mund verstopft, doch lallen", "tokens": ["Ist", "auch", "mit", "Asch'", "ihr", "Mund", "ver\u00b7stopft", ",", "doch", "lal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NE", "PPOSAT", "NN", "VVPP", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie noch von ihres Gottes lust'gen Z\u00fcgen!", "tokens": ["Sie", "noch", "von", "ih\u00b7res", "Got\u00b7tes", "lust'\u00b7gen", "Z\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Gru\u00df, Musen, euch! Dort die Papyrusrolle,", "tokens": ["Gru\u00df", ",", "Mu\u00b7sen", ",", "euch", "!", "Dort", "die", "Pa\u00b7py\u00b7rus\u00b7rol\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Verkohlt und morsch, wahrt noch im Eingeweide,", "tokens": ["Ver\u00b7kohlt", "und", "morsch", ",", "wahrt", "noch", "im", "Ein\u00b7ge\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleich wie der Muschel Schrein, der perlenvolle,", "tokens": ["Gleich", "wie", "der", "Mu\u00b7schel", "Schrein", ",", "der", "per\u00b7len\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohl manche Perl aus eurem Festgeschmeide.", "tokens": ["Wohl", "man\u00b7che", "Perl", "aus", "eu\u00b7rem", "Fest\u00b7ge\u00b7schmei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "La\u00df uns zu deines Gartens Bl\u00fcthenfesten!", "tokens": ["La\u00df", "uns", "zu", "dei\u00b7nes", "Gar\u00b7tens", "Bl\u00fc\u00b7then\u00b7fes\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach, seine Mauern, die verwaisten, gleichen", "tokens": ["Ach", ",", "sei\u00b7ne", "Mau\u00b7ern", ",", "die", "ver\u00b7wais\u00b7ten", ",", "glei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Aschenkrug mit den verbrannten Resten", "tokens": ["Dem", "A\u00b7schen\u00b7krug", "mit", "den", "ver\u00b7brann\u00b7ten", "Res\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Lenzes, der als J\u00fcngling mu\u00dft' erbleichen!", "tokens": ["Des", "Len\u00b7zes", ",", "der", "als", "J\u00fcng\u00b7ling", "mu\u00dft'", "er\u00b7blei\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "KOUS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Doch sieh dort neu Viol' und Rose nickend", "tokens": ["Doch", "sieh", "dort", "neu", "Vi\u00b7ol'", "und", "Ro\u00b7se", "ni\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "ADJD", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Reben gr\u00fcnend, Palmen und Platanen!", "tokens": ["Und", "Re\u00b7ben", "gr\u00fc\u00b7nend", ",", "Pal\u00b7men", "und", "Pla\u00b7ta\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie sprie\u00dfen drau\u00dfen, still her\u00fcber blickend,", "tokens": ["Sie", "sprie\u00b7\u00dfen", "drau\u00b7\u00dfen", ",", "still", "her\u00b7\u00fc\u00b7ber", "bli\u00b7ckend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie wir jetzt auf die Gr\u00e4ber unsrer Ahnen!", "tokens": ["Wie", "wir", "jetzt", "auf", "die", "Gr\u00e4\u00b7ber", "uns\u00b7rer", "Ah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Und sieh, hier kommen ja noch andre G\u00e4ste!", "tokens": ["Und", "sieh", ",", "hier", "kom\u00b7men", "ja", "noch", "and\u00b7re", "G\u00e4s\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bequem macht sich, wie ich in deinem Zimmer", "tokens": ["Be\u00b7quem", "macht", "sich", ",", "wie", "ich", "in", "dei\u00b7nem", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In ihrer Schwester tausendj\u00e4hr'gem Neste", "tokens": ["In", "ih\u00b7rer", "Schwes\u00b7ter", "tau\u00b7send\u00b7j\u00e4hr'\u00b7gem", "Nes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schwalb', umschwebend deines Simses Tr\u00fcmmer!", "tokens": ["Die", "Schwalb'", ",", "um\u00b7schwe\u00b7bend", "dei\u00b7nes", "Sim\u00b7ses", "Tr\u00fcm\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Den Rosenfriedhof hier umschwebt ein dreister", "tokens": ["Den", "Ro\u00b7sen\u00b7fried\u00b7hof", "hier", "um\u00b7schwebt", "ein", "dreis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Goldfalter, wie ein Geist, der sich verirrte!", "tokens": ["Gold\u00b7fal\u00b7ter", ",", "wie", "ein", "Geist", ",", "der", "sich", "ver\u00b7irr\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ums\u00e4useln ihn des Gartens Blumengeister?", "tokens": ["Um\u00b7s\u00e4u\u00b7seln", "ihn", "des", "Gar\u00b7tens", "Blu\u00b7men\u00b7geis\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denkt er des Ahns, des Flug sie einst umschwirrte?", "tokens": ["Denkt", "er", "des", "Ahns", ",", "des", "Flug", "sie", "einst", "um\u00b7schwirr\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Ich aber wei\u00df, des Daseins Ring, der helle,", "tokens": ["Ich", "a\u00b7ber", "wei\u00df", ",", "des", "Da\u00b7seins", "Ring", ",", "der", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er ist in Einem ungeheuren Bogen", "tokens": ["Er", "ist", "in", "Ei\u00b7nem", "un\u00b7ge\u00b7heu\u00b7ren", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Stern und Baum, durch Rosen, Sonnenb\u00e4lle,", "tokens": ["Durch", "Stern", "und", "Baum", ",", "durch", "Ro\u00b7sen", ",", "Son\u00b7nen\u00b7b\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Menschenherz und Engelsbrust gezogen!", "tokens": ["Durch", "Men\u00b7schen\u00b7herz", "und", "En\u00b7gels\u00b7brust", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Des Daseins Lied, von Allen angeklungen,", "tokens": ["Des", "Da\u00b7seins", "Lied", ",", "von", "Al\u00b7len", "an\u00b7ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aussprechen kann f\u00fcr sich allein es Keiner!", "tokens": ["Aus\u00b7spre\u00b7chen", "kann", "f\u00fcr", "sich", "al\u00b7lein", "es", "Kei\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PRF", "ADV", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was meine Lippen ganz nicht ausgesungen,", "tokens": ["Was", "mei\u00b7ne", "Lip\u00b7pen", "ganz", "nicht", "aus\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erg\u00e4nzen Rose, Stern und Baum statt meiner!", "tokens": ["Er\u00b7g\u00e4n\u00b7zen", "Ro\u00b7se", ",", "Stern", "und", "Baum", "statt", "mei\u00b7ner", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Und nur ein Theil von mir wird eingegruftet,", "tokens": ["Und", "nur", "ein", "Theil", "von", "mir", "wird", "ein\u00b7ge\u00b7gruf\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Theil von mir wird fort sein Dasein leben;", "tokens": ["Ein", "Theil", "von", "mir", "wird", "fort", "sein", "Da\u00b7sein", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "PTKVZ", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Theil von mir ist's, was in Rosen duftet,", "tokens": ["Ein", "Theil", "von", "mir", "ist's", ",", "was", "in", "Ro\u00b7sen", "duf\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In Sonnen flammt und gr\u00fcnt in Palm' und Reben!", "tokens": ["In", "Son\u00b7nen", "flammt", "und", "gr\u00fcnt", "in", "Palm'", "und", "Re\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.19": {"line.1": {"text": "Ein Theil von mir ist's ja, das von dem H\u00fcgel", "tokens": ["Ein", "Theil", "von", "mir", "ist's", "ja", ",", "das", "von", "dem", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "ADV", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Quell durchst\u00fcrmt der Erde ew'ge Fluren,", "tokens": ["Als", "Quell", "durchs\u00b7t\u00fcrmt", "der", "Er\u00b7de", "ew'\u00b7ge", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Schmetterling noch schl\u00e4gt die farb'gen Fl\u00fcgel,", "tokens": ["Als", "Schmet\u00b7ter\u00b7ling", "noch", "schl\u00e4gt", "die", "fa\u00b7rb'\u00b7gen", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als Schwalbe noch verfolgt des Fr\u00fchlings Spuren!", "tokens": ["Als", "Schwal\u00b7be", "noch", "ver\u00b7folgt", "des", "Fr\u00fch\u00b7lings", "Spu\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.20": {"line.1": {"text": "So soll mein ", "tokens": ["So", "soll", "mein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wenn \u00fcber ihren Reben, Quellen, Rosen", "tokens": ["Wenn", "\u00fc\u00b7ber", "ih\u00b7ren", "Re\u00b7ben", ",", "Quel\u00b7len", ",", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Jubelfluge, auf des Windes Schwingen", "tokens": ["Im", "Ju\u00b7bel\u00b7flu\u00b7ge", ",", "auf", "des", "Win\u00b7des", "Schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor\u00fcber meine Aschenreste tosen!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "mei\u00b7ne", "A\u00b7schen\u00b7res\u00b7te", "to\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}