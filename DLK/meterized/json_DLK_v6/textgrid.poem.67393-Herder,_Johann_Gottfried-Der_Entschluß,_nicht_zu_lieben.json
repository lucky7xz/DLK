{"textgrid.poem.67393": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Entschlu\u00df, nicht zu lieben", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich liebte sie; sie konnte mich vergessen,", "tokens": ["\u00bb", "ich", "lieb\u00b7te", "sie", ";", "sie", "konn\u00b7te", "mich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$.", "PPER", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ich, beinahe selbst verga\u00df ich sie.", "tokens": ["Und", "ich", ",", "bei\u00b7na\u00b7he", "selbst", "ver\u00b7ga\u00df", "ich", "sie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Soll ich aufs Neue jetzt die Laufbahn messen?", "tokens": ["Soll", "ich", "aufs", "Neu\u00b7e", "jetzt", "die", "Lauf\u00b7bahn", "mes\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nein, Amor, nein! Ich lieb', ich liebe nie.\u00ab", "tokens": ["Nein", ",", "A\u00b7mor", ",", "nein", "!", "Ich", "lieb'", ",", "ich", "lie\u00b7be", "nie", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "PTKANT", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbwie?\u00ab sprach der Gott. \u00bbUnd wenn sie wiederkehrte,", "tokens": ["\u00bb", "wie", "?", "\u00ab", "sprach", "der", "Gott", ".", "\u00bb", "Und", "wenn", "sie", "wie\u00b7der\u00b7kehr\u00b7te", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "$(", "VVFIN", "ART", "NN", "$.", "$(", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz Grazie und Reiz und Harmonie,", "tokens": ["Ganz", "Gra\u00b7zie", "und", "Reiz", "und", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und ihren Trug und Deinen Dir erkl\u00e4rte?\u00ab", "tokens": ["Und", "ih\u00b7ren", "Trug", "und", "Dei\u00b7nen", "Dir", "er\u00b7kl\u00e4r\u00b7te", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbnein, Amor, nein! Ich lieb', ich liebe nie.\u00ab", "tokens": ["\u00bb", "nein", ",", "A\u00b7mor", ",", "nein", "!", "Ich", "lieb'", ",", "ich", "lie\u00b7be", "nie", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "NE", "$,", "PTKANT", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Themire kam. Wie soll ich sie mir nennen?", "tokens": ["The\u00b7mi\u00b7re", "kam", ".", "Wie", "soll", "ich", "sie", "mir", "nen\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PWAV", "VMFIN", "PPER", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dione, Pallas, Suada, Sympathie?", "tokens": ["Dio\u00b7ne", ",", "Pal\u00b7las", ",", "Sua\u00b7da", ",", "Sym\u00b7pa\u00b7thie", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich h\u00f6rte sie; mein Herz fing an, zu brennen.", "tokens": ["Ich", "h\u00f6r\u00b7te", "sie", ";", "mein", "Herz", "fing", "an", ",", "zu", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbunselig Herz, ich lieb', ich liebe nie.\u00ab", "tokens": ["\u00bb", "un\u00b7se\u00b7lig", "Herz", ",", "ich", "lieb'", ",", "ich", "lie\u00b7be", "nie", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "\u00bbso darfst Du sie doch ehren!\u00ab sprach die Liebe.", "tokens": ["\u00bb", "so", "darfst", "Du", "sie", "doch", "eh\u00b7ren", "!", "\u00ab", "sprach", "die", "Lie\u00b7be", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPER", "ADV", "VVINF", "$.", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbkomm unter meinen Schutz; Du liebest nie.\u00ab", "tokens": ["\u00bb", "komm", "un\u00b7ter", "mei\u00b7nen", "Schutz", ";", "Du", "lie\u00b7best", "nie", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich folgete dem schlauen Herzensdiebe,", "tokens": ["Ich", "fol\u00b7ge\u00b7te", "dem", "schlau\u00b7en", "Her\u00b7zens\u00b7die\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ach! ich lieb', auf ewig lieb' ich sie.", "tokens": ["Und", "ach", "!", "ich", "lieb'", ",", "auf", "e\u00b7wig", "lieb'", "ich", "sie", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "$,", "APPR", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}