{"textgrid.poem.43949": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du ehmahls liebster Ort der treuen Leonore,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du ehmahls liebster Ort der treuen Leonore,", "tokens": ["Du", "eh\u00b7mahls", "liebs\u00b7ter", "Ort", "der", "treu\u00b7en", "Le\u00b7o\u00b7no\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie z\u00e4rtlich r\u00fchrt mich nicht der Anblick deiner Thore,", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "r\u00fchrt", "mich", "nicht", "der", "An\u00b7blick", "dei\u00b7ner", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wodurch ich damahls oft an ihrer Hand spaziert!", "tokens": ["Wo\u00b7durch", "ich", "da\u00b7mahls", "oft", "an", "ih\u00b7rer", "Hand", "spa\u00b7ziert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dort merck ich schon den Raum, worauf wir uns versprochen,", "tokens": ["Dort", "merck", "ich", "schon", "den", "Raum", ",", "wo\u00b7rauf", "wir", "uns", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dort blickt der Altan vor, auf dem wir sechzig Wochen", "tokens": ["Dort", "blickt", "der", "Al\u00b7tan", "vor", ",", "auf", "dem", "wir", "sech\u00b7zig", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die W\u00e4chter hinters Licht gef\u00fchrt.", "tokens": ["Die", "W\u00e4ch\u00b7ter", "hin\u00b7ters", "Licht", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Seyd tausendmahl gegr\u00fc\u00dft, ihr Felder, Str\u00e4uch und B\u00e4ume;", "tokens": ["Seyd", "tau\u00b7send\u00b7mahl", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "Fel\u00b7der", ",", "Str\u00e4uch", "und", "B\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "VVPP", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr kennt wohl diesen noch, von dem ihr so viel Reime,", "tokens": ["Ihr", "kennt", "wohl", "die\u00b7sen", "noch", ",", "von", "dem", "ihr", "so", "viel", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDAT", "ADV", "$,", "APPR", "PRELS", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So manches Lied geh\u00f6rt, so manchen Ku\u00df gesehn;", "tokens": ["So", "man\u00b7ches", "Lied", "ge\u00b7h\u00f6rt", ",", "so", "man\u00b7chen", "Ku\u00df", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Besinnt euch auf die Lust der heitern Sommern\u00e4chte!", "tokens": ["Be\u00b7sinnt", "euch", "auf", "die", "Lust", "der", "hei\u00b7tern", "Som\u00b7mer\u00b7n\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was meint ihr, wenn mein Wuntsch nur eine wiederbr\u00e4chte?", "tokens": ["Was", "meint", "ihr", ",", "wenn", "mein", "Wunt\u00b7sch", "nur", "ei\u00b7ne", "wie\u00b7der\u00b7br\u00e4ch\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ART", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Das wird wohl nimmermehr geschehn.", "tokens": ["Das", "wird", "wohl", "nim\u00b7mer\u00b7mehr", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo find ich aber nun mein Allerliebstes wieder?", "tokens": ["Wo", "find", "ich", "a\u00b7ber", "nun", "mein", "Al\u00b7ler\u00b7liebs\u00b7tes", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verr\u00e4th mir gar kein Gra\u00df das Lager ihrer Glieder?", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "mir", "gar", "kein", "Gra\u00df", "das", "La\u00b7ger", "ih\u00b7rer", "Glie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich sp\u00fcre keinen Schritt, die Sommerstub ist leer.", "tokens": ["Ich", "sp\u00fc\u00b7re", "kei\u00b7nen", "Schritt", ",", "die", "Som\u00b7mer\u00b7stub", "ist", "leer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie traurig scheinstu mir, du nicht mehr sch\u00f6ner Garthen!", "tokens": ["Wie", "trau\u00b7rig", "scheins\u00b7tu", "mir", ",", "du", "nicht", "mehr", "sch\u00f6\u00b7ner", "Gar\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "$,", "PPER", "PTKNEG", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du hast ja zween gehabt, was soll ich einsam warthen?", "tokens": ["Du", "hast", "ja", "zween", "ge\u00b7habt", ",", "was", "soll", "ich", "ein\u00b7sam", "wart\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "VAPP", "$,", "PWS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach, stell auch beyde wieder her!", "tokens": ["Ach", ",", "stell", "auch", "bey\u00b7de", "wie\u00b7der", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "ADV", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du schickst mich in die Stadt; die tref ich desto schlimmer:", "tokens": ["Du", "schickst", "mich", "in", "die", "Stadt", ";", "die", "tref", "ich", "des\u00b7to", "schlim\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "ART", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Wirth, das Volck ist neu, ein Gast entweiht das Zimmer,", "tokens": ["Der", "Wirth", ",", "das", "Volck", "ist", "neu", ",", "ein", "Gast", "ent\u00b7weiht", "das", "Zim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Worein sonst nichts als wir und unsre Liebe kam.", "tokens": ["Wo\u00b7rein", "sonst", "nichts", "als", "wir", "und", "uns\u00b7re", "Lie\u00b7be", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "KOKOM", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Gott, wie \u00e4ndert sich so viel in wenig Jahren!", "tokens": ["Mein", "Gott", ",", "wie", "\u00e4n\u00b7dert", "sich", "so", "viel", "in", "we\u00b7nig", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PRF", "ADV", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was wird nicht noch geschehn? O sollt ich dies erfahren!", "tokens": ["Was", "wird", "nicht", "noch", "ge\u00b7schehn", "?", "O", "sollt", "ich", "dies", "er\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "ADV", "VVINF", "$.", "NE", "VMFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie war mir, da\u00df ich Abschied nahm!", "tokens": ["Wie", "war", "mir", ",", "da\u00df", "ich", "Ab\u00b7schied", "nahm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich geh den Tempel aus, ich suche durch die Ga\u00dfen,", "tokens": ["Ich", "geh", "den", "Tem\u00b7pel", "aus", ",", "ich", "su\u00b7che", "durch", "die", "Ga\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich such auch, wo sie sich wohl niemahls finden la\u00dfen,", "tokens": ["Ich", "such", "auch", ",", "wo", "sie", "sich", "wohl", "nie\u00b7mahls", "fin\u00b7den", "la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PRF", "ADV", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich ruf ihr um den Wall, der Wall hat schlecht Geh\u00f6r.", "tokens": ["Ich", "ruf", "ihr", "um", "den", "Wall", ",", "der", "Wall", "hat", "schlecht", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NE", "$,", "ART", "NE", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Steig, Schweidniz, steig und sey ein Phoenix in den Flammen,", "tokens": ["Steig", ",", "Schweid\u00b7niz", ",", "steig", "und", "sey", "ein", "Phoe\u00b7nix", "in", "den", "Flam\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "KON", "VAFIN", "ART", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bau Marmor, Erz und Gold und Schlo\u00df und Thurm zusammen,", "tokens": ["Bau", "Mar\u00b7mor", ",", "Erz", "und", "Gold", "und", "Schlo\u00df", "und", "Thurm", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mir bistu doch nicht Schweidniz mehr.", "tokens": ["Mir", "bis\u00b7tu", "doch", "nicht", "Schweid\u00b7niz", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKNEG", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}