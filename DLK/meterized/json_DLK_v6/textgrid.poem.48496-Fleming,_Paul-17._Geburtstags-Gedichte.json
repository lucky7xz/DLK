{"textgrid.poem.48496": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "17. Geburtstags-Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gl\u00fcckhaft war ich doch zu jener Zeit zu sch\u00e4tzen,", "tokens": ["Wie", "gl\u00fcck\u00b7haft", "war", "ich", "doch", "zu", "je\u00b7ner", "Zeit", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da ich in Gegenwart sie kunte binden an", "tokens": ["da", "ich", "in", "Ge\u00b7gen\u00b7wart", "sie", "kun\u00b7te", "bin\u00b7den", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VMFIN", "VVINF", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und mich auf diesen Tag in ihrer Huld ergetzen,", "tokens": ["und", "mich", "auf", "die\u00b7sen", "Tag", "in", "ih\u00b7rer", "Huld", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da mir durchs ganze Jahr kein liebrer kommen kan!", "tokens": ["da", "mir", "durchs", "gan\u00b7ze", "Jahr", "kein", "lieb\u00b7rer", "kom\u00b7men", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PIAT", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "War gleich der Blumen Zier durch blassen Frost erstorben", "tokens": ["War", "gleich", "der", "Blu\u00b7men", "Zier", "durch", "blas\u00b7sen", "Frost", "ers\u00b7tor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und keine Farbe mehr zu sehen durch die Welt,", "tokens": ["und", "kei\u00b7ne", "Far\u00b7be", "mehr", "zu", "se\u00b7hen", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "so klagte dennoch sie hierinnen nichts verdorben.", "tokens": ["so", "klag\u00b7te", "den\u00b7noch", "sie", "hie\u00b7rin\u00b7nen", "nichts", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "VVFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihr war mein fester Sinn ein weites Blumen-Feld.", "tokens": ["Ihr", "war", "mein", "fes\u00b7ter", "Sinn", "ein", "wei\u00b7tes", "Blu\u00b7men\u00b7Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich bunde sie mit mir. So durft' ich auch nicht sagen,", "tokens": ["Ich", "bun\u00b7de", "sie", "mit", "mir", ".", "So", "durft'", "ich", "auch", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$.", "ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "da\u00df ihr mein s\u00fc\u00dfer Brief nicht k\u00e4me recht zu Hand:", "tokens": ["da\u00df", "ihr", "mein", "s\u00fc\u00b7\u00dfer", "Brief", "nicht", "k\u00e4\u00b7me", "recht", "zu", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "mein Herze war die Post. Das reiste stets verborgen", "tokens": ["mein", "Her\u00b7ze", "war", "die", "Post", ".", "Das", "reis\u00b7te", "stets", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und brachte sich ihr selbst, sein Bote, Brief und Band.", "tokens": ["und", "brach\u00b7te", "sich", "ihr", "selbst", ",", "sein", "Bo\u00b7te", ",", "Brief", "und", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So bunden wir uns stets und l\u00f6sten uns stets wieder.", "tokens": ["So", "bun\u00b7den", "wir", "uns", "stets", "und", "l\u00f6s\u00b7ten", "uns", "stets", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das liebe lange Jahr war ", "tokens": ["Das", "lie\u00b7be", "lan\u00b7ge", "Jahr", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "der mit der Sonnen selbst ging t\u00e4glich auf und nieder", "tokens": ["der", "mit", "der", "Son\u00b7nen", "selbst", "ging", "t\u00e4g\u00b7lich", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "und uns nie schreiten lie\u00df aus seiner g\u00fcldnen Spur.", "tokens": ["und", "uns", "nie", "schrei\u00b7ten", "lie\u00df", "aus", "sei\u00b7ner", "g\u00fcld\u00b7nen", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was soll ich hier nun tun? Was soll ich doch beginnen?", "tokens": ["Was", "soll", "ich", "hier", "nun", "tun", "?", "Was", "soll", "ich", "doch", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ach! da\u00df ich kommen bin in die betr\u00fcbte Zeit!", "tokens": ["Ach", "!", "da\u00df", "ich", "kom\u00b7men", "bin", "in", "die", "be\u00b7tr\u00fcb\u00b7te", "Zeit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "VVINF", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Seid ihr denn ratsarm ganz, ihr abgekrankten Sinnen?", "tokens": ["Seid", "ihr", "denn", "rat\u00b7sarm", "ganz", ",", "ihr", "ab\u00b7ge\u00b7krank\u00b7ten", "Sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ADJD", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bind' oder bind' ich nicht? Es macht mir beides Leid.", "tokens": ["Bind'", "o\u00b7der", "bind'", "ich", "nicht", "?", "Es", "macht", "mir", "bei\u00b7des", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df ich doch solte nur kaum durch drei halbe Zeilen", "tokens": ["Da\u00df", "ich", "doch", "sol\u00b7te", "nur", "kaum", "durch", "drei", "hal\u00b7be", "Zei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "ADV", "ADV", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "mein Tun ihr schreiben zu! Doch es ist m\u00fcglich nicht.", "tokens": ["mein", "Tun", "ihr", "schrei\u00b7ben", "zu", "!", "Doch", "es", "ist", "m\u00fcg\u00b7lich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "PTKVZ", "$.", "KON", "PPER", "VAFIN", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich bin von ihr getrennt auf mehr als tausent Meilen.", "tokens": ["Ich", "bin", "von", "ihr", "ge\u00b7trennt", "auf", "mehr", "als", "tau\u00b7sent", "Mei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "APPR", "PIAT", "KOKOM", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "La\u00df ich es ganz denn nach, wo bleibet meine Pflicht?", "tokens": ["La\u00df", "ich", "es", "ganz", "denn", "nach", ",", "wo", "blei\u00b7bet", "mei\u00b7ne", "Pflicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dort ist Gefahr, hier Angst. Doch, da\u00df nicht auch die Reue", "tokens": ["Dort", "ist", "Ge\u00b7fahr", ",", "hier", "Angst", ".", "Doch", ",", "da\u00df", "nicht", "auch", "die", "Reu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "NN", "$.", "KON", "$,", "KOUS", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "zu diesem Kummer st\u00f6\u00dft, so la\u00df ichs ungewagt.", "tokens": ["zu", "die\u00b7sem", "Kum\u00b7mer", "st\u00f6\u00dft", ",", "so", "la\u00df", "ichs", "un\u00b7ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich habe Zuversicht in ihre starke Treue.", "tokens": ["Ich", "ha\u00b7be", "Zu\u00b7ver\u00b7sicht", "in", "ih\u00b7re", "star\u00b7ke", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das spricht mich bei ihr los, was mich bei ihr verklagt.", "tokens": ["Das", "spricht", "mich", "bei", "ihr", "los", ",", "was", "mich", "bei", "ihr", "ver\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$,", "PRELS", "PRF", "APPR", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Licht, wenn du dermaleins in meinen sch\u00f6nen B\u00fcchern", "tokens": ["Licht", ",", "wenn", "du", "der\u00b7ma\u00b7leins", "in", "mei\u00b7nen", "sch\u00f6\u00b7nen", "B\u00fc\u00b7chern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.30": {"text": "auch dieses lesen wirst, so wirst du erstlich sehn,", "tokens": ["auch", "die\u00b7ses", "le\u00b7sen", "wirst", ",", "so", "wirst", "du", "erst\u00b7lich", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "VVINF", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "wie hoch du meines Sinns hast k\u00f6nnen dich versichern", "tokens": ["wie", "hoch", "du", "mei\u00b7nes", "Sinns", "hast", "k\u00f6n\u00b7nen", "dich", "ver\u00b7si\u00b7chern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "PPOSAT", "NN", "VAFIN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Was dich und mich itzt kr\u00e4nkt, das mu\u00df aus Not geschehn.", "tokens": ["Was", "dich", "und", "mich", "itzt", "kr\u00e4nkt", ",", "das", "mu\u00df", "aus", "Not", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KON", "PPER", "ADV", "VVFIN", "$,", "PDS", "VMFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}