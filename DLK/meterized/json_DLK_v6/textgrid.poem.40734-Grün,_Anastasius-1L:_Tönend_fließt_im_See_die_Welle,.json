{"textgrid.poem.40734": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: T\u00f6nend flie\u00dft im See die Welle,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00f6nend flie\u00dft im See die Welle,", "tokens": ["T\u00f6\u00b7nend", "flie\u00dft", "im", "See", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4hne schaukeln in den Kieden,", "tokens": ["K\u00e4h\u00b7ne", "schau\u00b7keln", "in", "den", "Kie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Insel die Kapelle", "tokens": ["Auf", "der", "In\u00b7sel", "die", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Blinkt aus gr\u00fcnem Waldesfrieden.", "tokens": ["Blinkt", "aus", "gr\u00fc\u00b7nem", "Wal\u00b7des\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihre Glockenrufe gleiten", "tokens": ["Ih\u00b7re", "Glo\u00b7cken\u00b7ru\u00b7fe", "glei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zitternd \u00fcber Wellenkreise,", "tokens": ["Zit\u00b7ternd", "\u00fc\u00b7ber", "Wel\u00b7len\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ringen t\u00f6nend in die Weiten,", "tokens": ["Rin\u00b7gen", "t\u00f6\u00b7nend", "in", "die", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterben dann verhallend leise,", "tokens": ["Ster\u00b7ben", "dann", "ver\u00b7hal\u00b7lend", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df die Schwalben, die da fliegen,", "tokens": ["Da\u00df", "die", "Schwal\u00b7ben", ",", "die", "da", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In Musik die Schwingen baden,", "tokens": ["In", "Mu\u00b7sik", "die", "Schwin\u00b7gen", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Musik sich lieblich wiegen", "tokens": ["In", "Mu\u00b7sik", "sich", "lieb\u00b7lich", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PRF", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schifflein auf den Wellenpfaden.", "tokens": ["Schif\u00b7flein", "auf", "den", "Wel\u00b7len\u00b7pfa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bald wie Sehnsucht, bald wie Klagen", "tokens": ["Bald", "wie", "Sehn\u00b7sucht", ",", "bald", "wie", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "NN", "$,", "ADV", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt der Glockenton gezogen,", "tokens": ["Kommt", "der", "Glo\u00b7cken\u00b7ton", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt ein sch\u00fcchtern stockend Fragen,", "tokens": ["Jetzt", "ein", "sch\u00fcch\u00b7tern", "sto\u00b7ckend", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt der Hoffnung voll'res Wogen.", "tokens": ["Jetzt", "der", "Hoff\u00b7nung", "voll'\u00b7res", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wundersames, eignes Klingen,", "tokens": ["Wun\u00b7der\u00b7sa\u00b7mes", ",", "eig\u00b7nes", "Klin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ob F\u00fchlen im Metalle!", "tokens": ["Als", "ob", "F\u00fch\u00b7len", "im", "Me\u00b7tal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um zu Herzen so zu dringen,", "tokens": ["Um", "zu", "Her\u00b7zen", "so", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pocht ein Herz wohl in dem Schalle.", "tokens": ["Pocht", "ein", "Herz", "wohl", "in", "dem", "Schal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nicht des Gl\u00f6ckners H\u00e4nde f\u00fchren", "tokens": ["Nicht", "des", "Gl\u00f6ck\u00b7ners", "H\u00e4n\u00b7de", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Taktgerecht die Glockenstr\u00e4nge;", "tokens": ["Takt\u00b7ge\u00b7recht", "die", "Glo\u00b7ckens\u00b7tr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4ubig an das Seil zu r\u00fchren,", "tokens": ["Gl\u00e4u\u00b7big", "an", "das", "Seil", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00e4ngt sich hier die Pilgermenge.", "tokens": ["Dr\u00e4ngt", "sich", "hier", "die", "Pil\u00b7ger\u00b7men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn die Sage k\u00fcndet's Allen:", "tokens": ["Denn", "die", "Sa\u00b7ge", "k\u00fcn\u00b7det's", "Al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wem verg\u00f6nnt, die\u00df Seil zu schwingen,", "tokens": ["Wem", "ver\u00b7g\u00f6nnt", ",", "die\u00df", "Seil", "zu", "schwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PDS", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was er bei der Glocke Hallen", "tokens": ["Was", "er", "bei", "der", "Glo\u00b7cke", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcnschen mag, es soll gelingen!", "tokens": ["W\u00fcn\u00b7schen", "mag", ",", "es", "soll", "ge\u00b7lin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ruhlos t\u00f6nt das Gl\u00f6cklein immer,", "tokens": ["Ruh\u00b7los", "t\u00f6nt", "das", "Gl\u00f6c\u00b7klein", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "T\u00f6nt zu allen Tageszeiten;", "tokens": ["T\u00f6nt", "zu", "al\u00b7len", "Ta\u00b7ges\u00b7zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die W\u00fcnsche schlummern nimmer,", "tokens": ["Denn", "die", "W\u00fcn\u00b7sche", "schlum\u00b7mern", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pilgern ruhlos in die Weiten.", "tokens": ["Pil\u00b7gern", "ruh\u00b7los", "in", "die", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ob die Kl\u00e4nge voller schwellen,", "tokens": ["Ob", "die", "Kl\u00e4n\u00b7ge", "vol\u00b7ler", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob im Wind sie leis vergehen,", "tokens": ["Ob", "im", "Wind", "sie", "leis", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer \u00fcber diesen Wellen", "tokens": ["Im\u00b7mer", "\u00fc\u00b7ber", "die\u00b7sen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt des Geistes m\u00e4chtig Wehen.", "tokens": ["Schwebt", "des", "Geis\u00b7tes", "m\u00e4ch\u00b7tig", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und du f\u00fchlst, vom Hauch getroffen,", "tokens": ["Und", "du", "f\u00fchlst", ",", "vom", "Hauch", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die eigne Brust die Fluthen", "tokens": ["Durch", "die", "eig\u00b7ne", "Brust", "die", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "All der Andern Leid und Hoffen,", "tokens": ["All", "der", "An\u00b7dern", "Leid", "und", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fremde Schauer, fremde Gluthen;", "tokens": ["Frem\u00b7de", "Schau\u00b7er", ",", "frem\u00b7de", "Glu\u00b7then", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "F\u00fchlst, was Herzen kann bedr\u00e4ngen,", "tokens": ["F\u00fchlst", ",", "was", "Her\u00b7zen", "kann", "be\u00b7dr\u00e4n\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sie sporne, was sie qu\u00e4le;", "tokens": ["Was", "sie", "spor\u00b7ne", ",", "was", "sie", "qu\u00e4\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es t\u00f6nt in jenen Kl\u00e4ngen", "tokens": ["Denn", "es", "t\u00f6nt", "in", "je\u00b7nen", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch das All die Menschenseele.", "tokens": ["Durch", "das", "All", "die", "Men\u00b7schen\u00b7see\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}