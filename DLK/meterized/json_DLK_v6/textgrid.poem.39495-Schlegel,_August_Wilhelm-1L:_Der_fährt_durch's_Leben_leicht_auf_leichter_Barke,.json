{"textgrid.poem.39495": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der f\u00e4hrt durch's Leben leicht auf leichter Barke,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der f\u00e4hrt durch's Leben leicht auf leichter Barke,", "tokens": ["Der", "f\u00e4hrt", "durch's", "Le\u00b7ben", "leicht", "auf", "leich\u00b7ter", "Bar\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der l\u00e4\u00dft die Wimpel bunt und stattlich fliegen;", "tokens": ["Der", "l\u00e4\u00dft", "die", "Wim\u00b7pel", "bunt", "und", "statt\u00b7lich", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der will bis in den Mond erobernd siegen,", "tokens": ["Der", "will", "bis", "in", "den", "Mond", "er\u00b7o\u00b7bernd", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "ART", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der sorgt, wie er sein klein Gebiet vermarke;", "tokens": ["Der", "sorgt", ",", "wie", "er", "sein", "klein", "Ge\u00b7biet", "ver\u00b7mar\u00b7ke", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PPER", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der pflegt sich \u00fcppig mit des Landes Marke,", "tokens": ["Der", "pflegt", "sich", "\u00fcp\u00b7pig", "mit", "des", "Lan\u00b7des", "Mar\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der mu\u00df im Wetter nackt und hungrig liegen:", "tokens": ["Der", "mu\u00df", "im", "Wet\u00b7ter", "nackt", "und", "hung\u00b7rig", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch alle gleich, gewiegt in gleichen Wiegen", "tokens": ["Doch", "al\u00b7le", "gleich", ",", "ge\u00b7wiegt", "in", "glei\u00b7chen", "Wie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "$,", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der gro\u00dfen Mutter, Schwache so wie Starke.", "tokens": ["Der", "gro\u00b7\u00dfen", "Mut\u00b7ter", ",", "Schwa\u00b7che", "so", "wie", "Star\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "ADV", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und kaum gew\u00fcrdigt werden eines Blickes,", "tokens": ["Und", "kaum", "ge\u00b7w\u00fcr\u00b7digt", "wer\u00b7den", "ei\u00b7nes", "Bli\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die da gewesen; und die sind, verge\u00dfen", "tokens": ["Die", "da", "ge\u00b7we\u00b7sen", ";", "und", "die", "sind", ",", "ver\u00b7ge\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "VAPP", "$.", "KON", "PDS", "VAFIN", "$,", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr Wandeln \u00fcber hohlen Katakomben.", "tokens": ["Ihr", "Wan\u00b7deln", "\u00fc\u00b7ber", "hoh\u00b7len", "Ka\u00b7ta\u00b7kom\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es rollt die Erde wie das Rad des Gl\u00fcckes,", "tokens": ["Es", "rollt", "die", "Er\u00b7de", "wie", "das", "Rad", "des", "Gl\u00fc\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihr die Zeit, nie ruhend, ungeme\u00dfen,", "tokens": ["Mit", "ihr", "die", "Zeit", ",", "nie", "ru\u00b7hend", ",", "un\u00b7ge\u00b7me\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "ADV", "VVPP", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und st\u00fcndlich w\u00fcrgt der Tod sich Hekatomben.", "tokens": ["Und", "st\u00fcnd\u00b7lich", "w\u00fcrgt", "der", "Tod", "sich", "He\u00b7ka\u00b7tom\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PRF", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}