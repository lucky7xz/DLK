{"textgrid.poem.62006": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[das Paradies verschwand]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Paradies verschwand,", "tokens": ["Das", "Pa\u00b7ra\u00b7dies", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Erde vor mir stand", "tokens": ["Die", "Er\u00b7de", "vor", "mir", "stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ganz schwarz und anzubau'n:", "tokens": ["Ganz", "schwarz", "und", "an\u00b7zu\u00b7bau'n", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVIZU", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mich fa\u00dft ein tiefes Grau'n.", "tokens": ["Mich", "fa\u00dft", "ein", "tie\u00b7fes", "Grau'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch fa\u00dft ich mich geschwind,", "tokens": ["Doch", "fa\u00dft", "ich", "mich", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und rasch wie Seltne sind,", "tokens": ["Und", "rasch", "wie", "Selt\u00b7ne", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NE", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ward ich ein Ackersmann", "tokens": ["Ward", "ich", "ein", "A\u00b7ckers\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und fing die Arbeit an.", "tokens": ["Und", "fing", "die", "Ar\u00b7beit", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bald ward die Erde gr\u00fcn", "tokens": ["Bald", "ward", "die", "Er\u00b7de", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und tausend Bl\u00fcten bl\u00fch'n,", "tokens": ["Und", "tau\u00b7send", "Bl\u00fc\u00b7ten", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Paradies \u2013 von Neu \u2013", "tokens": ["Das", "Pa\u00b7ra\u00b7dies", "\u2013", "von", "Neu", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erstand, bei meiner Treu!", "tokens": ["Er\u00b7stand", ",", "bei", "mei\u00b7ner", "Treu", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}