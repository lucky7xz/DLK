{"textgrid.poem.35101": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbmehr Licht!\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmehr Licht, mehr Licht!\u00ab Die Finsterni\u00df", "tokens": ["\u00bb", "mehr", "Licht", ",", "mehr", "Licht", "!", "\u00ab", "Die", "Fins\u00b7ter\u00b7ni\u00df"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "PIAT", "NN", "$,", "PIAT", "NN", "$.", "$(", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft mich nur zagend vorw\u00e4rts gehn;", "tokens": ["L\u00e4\u00dft", "mich", "nur", "za\u00b7gend", "vor\u00b7w\u00e4rts", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schreite langsam, ungewi\u00df", "tokens": ["Ich", "schrei\u00b7te", "lang\u00b7sam", ",", "un\u00b7ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bleib oft \u00e4ngstlich tastend stehn.", "tokens": ["Und", "bleib", "oft", "\u00e4ngst\u00b7lich", "tas\u00b7tend", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbmehr Licht, mehr Licht!\u00ab Zwar leuchtet mir", "tokens": ["\u00bb", "mehr", "Licht", ",", "mehr", "Licht", "!", "\u00ab", "Zwar", "leuch\u00b7tet", "mir"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "PIAT", "NN", "$,", "PIAT", "NN", "$.", "$(", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Weisheit dieser klugen Welt,", "tokens": ["Die", "Weis\u00b7heit", "die\u00b7ser", "klu\u00b7gen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch so, da\u00df sie den Weg zu dir", "tokens": ["Doch", "so", ",", "da\u00df", "sie", "den", "Weg", "zu", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdunkelt, aber nicht erhellt.", "tokens": ["Ver\u00b7dun\u00b7kelt", ",", "a\u00b7ber", "nicht", "er\u00b7hellt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbmehr Licht, mehr Licht!\u00ab Am Glauben nur,", "tokens": ["\u00bb", "mehr", "Licht", ",", "mehr", "Licht", "!", "\u00ab", "Am", "Glau\u00b7ben", "nur", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$,", "PIAT", "NN", "$.", "$(", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An ihm allein, allein gebrichts;", "tokens": ["An", "ihm", "al\u00b7lein", ",", "al\u00b7lein", "ge\u00b7brichts", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn scheut die irdische Natur", "tokens": ["Ihn", "scheut", "die", "ir\u00b7di\u00b7sche", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit ihm dich, den Quell des Lichts.", "tokens": ["Und", "mit", "ihm", "dich", ",", "den", "Quell", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}