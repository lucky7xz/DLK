{"textgrid.poem.59506": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Des Knaben Segen", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben den Knaben ins Gras gelegt.", "tokens": ["Wir", "ha\u00b7ben", "den", "Kna\u00b7ben", "ins", "Gras", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie der Schelm sich lustig bewegt!", "tokens": ["Wie", "der", "Schelm", "sich", "lus\u00b7tig", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie er strebet mit H\u00e4nden und F\u00fc\u00dfen!", "tokens": ["Wie", "er", "stre\u00b7bet", "mit", "H\u00e4n\u00b7den", "und", "F\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Will mit Gewalt hinein in den s\u00fc\u00dfen", "tokens": ["Will", "mit", "Ge\u00b7walt", "hin\u00b7ein", "in", "den", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "ADV", "APPR", "ART", "ADJA"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Taumel, der um ihn summt und schwirrt!", "tokens": ["Tau\u00b7mel", ",", "der", "um", "ihn", "summt", "und", "schwirrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wie ihm das Auge lebendig wird!", "tokens": ["Wie", "ihm", "das", "Au\u00b7ge", "le\u00b7ben\u00b7dig", "wird", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "L\u00e4\u00dft es in der Entz\u00fcckung schweifen", "tokens": ["L\u00e4\u00dft", "es", "in", "der", "Ent\u00b7z\u00fc\u00b7ckung", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "In des Lichts unerme\u00dflichem Blau,", "tokens": ["In", "des", "Lichts", "un\u00b7er\u00b7me\u00df\u00b7li\u00b7chem", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.9": {"text": "M\u00f6chte alles so gern genau", "tokens": ["M\u00f6ch\u00b7te", "al\u00b7les", "so", "gern", "ge\u00b7nau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "ADJD"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Mit den Fingern und Augen greifen,", "tokens": ["Mit", "den", "Fin\u00b7gern", "und", "Au\u00b7gen", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "M\u00f6chte in das fr\u00f6hliche Leben", "tokens": ["M\u00f6ch\u00b7te", "in", "das", "fr\u00f6h\u00b7li\u00b7che", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Hinein mit Schwalben und Bienen schweben,", "tokens": ["Hin\u00b7ein", "mit", "Schwal\u00b7ben", "und", "Bie\u00b7nen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "M\u00f6chte sich st\u00fcrzen nimmersatt", "tokens": ["M\u00f6ch\u00b7te", "sich", "st\u00fcr\u00b7zen", "nim\u00b7mer\u00b7satt"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "VVFIN", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "In der Welten unendliches Bad!", "tokens": ["In", "der", "Wel\u00b7ten", "un\u00b7end\u00b7li\u00b7ches", "Bad", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NE", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Kleiner Unschuldiger, halte still!", "tokens": ["Klei\u00b7ner", "Un\u00b7schul\u00b7di\u00b7ger", ",", "hal\u00b7te", "still", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Dein Geschlecht kann nicht, was es will.", "tokens": ["Dein", "Ge\u00b7schlecht", "kann", "nicht", ",", "was", "es", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ach! wie schimmert dir, s\u00fc\u00dfer Knabe,", "tokens": ["Ach", "!", "wie", "schim\u00b7mert", "dir", ",", "s\u00fc\u00b7\u00dfer", "Kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In dem Blick die gef\u00e4hrliche Gabe,", "tokens": ["In", "dem", "Blick", "die", "ge\u00b7f\u00e4hr\u00b7li\u00b7che", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Alles zu fassen mit inniger Lust,", "tokens": ["Al\u00b7les", "zu", "fas\u00b7sen", "mit", "in\u00b7ni\u00b7ger", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Alles zu ziehen in die Brust!", "tokens": ["Al\u00b7les", "zu", "zie\u00b7hen", "in", "die", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Wirst den unendlichen Durst nicht stillen,", "tokens": ["Wirst", "den", "un\u00b7end\u00b7li\u00b7chen", "Durst", "nicht", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Wirst die unendliche Brust nicht f\u00fcllen.", "tokens": ["Wirst", "die", "un\u00b7end\u00b7li\u00b7che", "Brust", "nicht", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Spiele denn die fr\u00f6hliche Zeit,", "tokens": ["Spie\u00b7le", "denn", "die", "fr\u00f6h\u00b7li\u00b7che", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ehe der Lenz mit den Blumen verschneit,", "tokens": ["E\u00b7he", "der", "Lenz", "mit", "den", "Blu\u00b7men", "ver\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ehe die s\u00fc\u00dfe Nachtigall schweigt", "tokens": ["E\u00b7he", "die", "s\u00fc\u00b7\u00dfe", "Nach\u00b7ti\u00b7gall", "schweigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und der Sommer mit Wettern zeucht.", "tokens": ["Und", "der", "Som\u00b7mer", "mit", "Wet\u00b7tern", "zeucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "O wie wird's dann dem Busen enge!", "tokens": ["O", "wie", "wird's", "dann", "dem", "Bu\u00b7sen", "en\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "ADV", "ART", "NN", "ADJA", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Wie ist auf dem Wege so hei\u00df das Gedr\u00e4nge!", "tokens": ["Wie", "ist", "auf", "dem", "We\u00b7ge", "so", "hei\u00df", "das", "Ge\u00b7dr\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ein stetes Fluchen und Sto\u00dfen und Treiben \u2013", "tokens": ["Ein", "ste\u00b7tes", "Flu\u00b7chen", "und", "Sto\u00b7\u00dfen", "und", "Trei\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Kannst nicht fliehen und kannst nicht bleiben,", "tokens": ["Kannst", "nicht", "flie\u00b7hen", "und", "kannst", "nicht", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVINF", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Darfst nicht lieben und sollst nicht hassen \u2013", "tokens": ["Darfst", "nicht", "lie\u00b7ben", "und", "sollst", "nicht", "has\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "VVINF", "KON", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Wo soll das ge\u00e4ngstete Herz sich lassen?", "tokens": ["Wo", "soll", "das", "ge\u00b7\u00e4ngs\u00b7te\u00b7te", "Herz", "sich", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Flehend suchet das Aug' umher,", "tokens": ["Fle\u00b7hend", "su\u00b7chet", "das", "Aug'", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Wie der Wehrlose nach dem Speer,", "tokens": ["Wie", "der", "Wehr\u00b7lo\u00b7se", "nach", "dem", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Sehnst dich hinaus aus dem wilden Get\u00fcmmel", "tokens": ["Sehnst", "dich", "hin\u00b7aus", "aus", "dem", "wil\u00b7den", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APZR", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.14": {"text": "Unter der Kindheit freundlichen Himmel", "tokens": ["Un\u00b7ter", "der", "Kind\u00b7heit", "freund\u00b7li\u00b7chen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Zu dem Steckenpferde, zum Ball,", "tokens": ["Zu", "dem", "Ste\u00b7cken\u00b7pfer\u00b7de", ",", "zum", "Ball", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "W\u00fcnschest, da\u00df in der stillen Erde,", "tokens": ["W\u00fcn\u00b7schest", ",", "da\u00df", "in", "der", "stil\u00b7len", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Ferne von Sonne und Vogelschall,", "tokens": ["Fer\u00b7ne", "von", "Son\u00b7ne", "und", "Vo\u00b7gel\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.18": {"text": "Dir die Ruhe gegraben werde.", "tokens": ["Dir", "die", "Ru\u00b7he", "ge\u00b7gra\u00b7ben", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir haben den Knaben gesetzt auf die B\u00fchne,", "tokens": ["Wir", "ha\u00b7ben", "den", "Kna\u00b7ben", "ge\u00b7setzt", "auf", "die", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Worauf er k\u00fcnftig spielen soll.", "tokens": ["Wo\u00b7rauf", "er", "k\u00fcnf\u00b7tig", "spie\u00b7len", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es gehe dem Unschuldigen wohl!", "tokens": ["Es", "ge\u00b7he", "dem", "Un\u00b7schul\u00b7di\u00b7gen", "wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir vertrauen ihn dir, Erde du gr\u00fcne,", "tokens": ["Wir", "ver\u00b7trau\u00b7en", "ihn", "dir", ",", "Er\u00b7de", "du", "gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Dir, leuchtender Himmel, liebevoll.", "tokens": ["Dir", ",", "leuch\u00b7ten\u00b7der", "Him\u00b7mel", ",", "lie\u00b7be\u00b7voll", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wollet ihm kindlich das Herz bewahren", "tokens": ["Wol\u00b7let", "ihm", "kind\u00b7lich", "das", "Herz", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "ART", "NN", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "In der Verschuldung b\u00f6sen Jahren!", "tokens": ["In", "der", "Ver\u00b7schul\u00b7dung", "b\u00f6\u00b7sen", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wollet ihn machen liebereich!", "tokens": ["Wol\u00b7let", "ihn", "ma\u00b7chen", "lie\u00b7be\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "So bleibt das Herz ihm fromm und weich.", "tokens": ["So", "bleibt", "das", "Herz", "ihm", "fromm", "und", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gro\u00dfes Schicksal, das machtig waltet", "tokens": ["Gro\u00b7\u00dfes", "Schick\u00b7sal", ",", "das", "mach\u00b7tig", "wal\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Und das Leben verborgen gestaltet,", "tokens": ["Und", "das", "Le\u00b7ben", "ver\u00b7bor\u00b7gen", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Nimm die l\u00e4chelnde Unschuld hin!", "tokens": ["Nimm", "die", "l\u00e4\u00b7cheln\u00b7de", "Un\u00b7schuld", "hin", "!"]