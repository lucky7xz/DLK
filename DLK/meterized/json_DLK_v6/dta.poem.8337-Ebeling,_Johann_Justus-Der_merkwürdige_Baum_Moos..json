{"dta.poem.8337": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n merkw\u00fcrdige Baum Moos.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Moos der an den B\u00e4umen w\u00e4chst\nder Sammt womit sie sind umwunden,", "tokens": ["Den", "Moos", "der", "an", "den", "B\u00e4u\u00b7men", "w\u00e4chst", "der", "Sammt", "wo\u00b7mit", "sie", "sind", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Hab ich von mannigfaltger Art, an", "tokens": ["Hab", "ich", "von", "man\u00b7nig\u00b7falt\u00b7ger", "Art", ",", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN", "$,", "APPR"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und wie im Reiche der Natur nichts", "tokens": ["Und", "wie", "im", "Rei\u00b7che", "der", "Na\u00b7tur", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NE", "ART", "NN", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wollen wir auch dieses Moos zu unsers Sch\u00f6p-", "tokens": ["So", "wol\u00b7len", "wir", "auch", "die\u00b7ses", "Moos", "zu", "un\u00b7sers", "Sch\u00f6p"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PDAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der \u00e4usre Anblik r\u00fchrt uns nicht, es sieht von", "tokens": ["Der", "\u00e4us\u00b7re", "An\u00b7blik", "r\u00fchrt", "uns", "nicht", ",", "es", "sieht", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es scheint uns wie ein dikker Zwirn, der Lokken", "tokens": ["Es", "scheint", "uns", "wie", "ein", "dik\u00b7ker", "Zwirn", ",", "der", "Lok\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Allein durch ein Vergr\u00f6\u00dfrungs-Glas kan uns des", "tokens": ["Al\u00b7lein", "durch", "ein", "Ver\u00b7gr\u00f6\u00df\u00b7rungs\u00b7Glas", "kan", "uns", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wir sehen in den Moos ein Sammt, von wun-", "tokens": ["Wir", "se\u00b7hen", "in", "den", "Moos", "ein", "Sammt", ",", "von", "wun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Er w\u00e4chset aus den Baum hervor, bedekket dessen", "tokens": ["Er", "w\u00e4ch\u00b7set", "aus", "den", "Baum", "her\u00b7vor", ",", "be\u00b7dek\u00b7ket", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PDS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist unterschiedlich anzusehn, die eine Art gleicht ei-", "tokens": ["Ist", "un\u00b7ter\u00b7schied\u00b7lich", "an\u00b7zu\u00b7sehn", ",", "die", "ei\u00b7ne", "Art", "gleicht", "ei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVIZU", "$,", "PRELS", "ART", "NN", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Die andre w\u00e4chset, wie ein Filz, die dritte glei-", "tokens": ["Die", "and\u00b7re", "w\u00e4ch\u00b7set", ",", "wie", "ein", "Filz", ",", "die", "drit\u00b7te", "glei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als wenn der Baum mit d\u00fcnner Haut, als einer", "tokens": ["Als", "wenn", "der", "Baum", "mit", "d\u00fcn\u00b7ner", "Haut", ",", "als", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "$,", "KOUS", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Hier w\u00e4chset er als wie ein Filz, da wird man an-", "tokens": ["Hier", "w\u00e4ch\u00b7set", "er", "als", "wie", "ein", "Filz", ",", "da", "wird", "man", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "KOKOM", "ART", "NN", "$,", "ADV", "VAFIN", "PIS", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der wie ein Fenchel ausgekeimmt, mit einem art-", "tokens": ["Der", "wie", "ein", "Fen\u00b7chel", "aus\u00b7ge\u00b7keimmt", ",", "mit", "ei\u00b7nem", "ar\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "NN", "VVPP", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Farben sind auch mannigfalt, braungr\u00fcnlich,", "tokens": ["Die", "Far\u00b7ben", "sind", "auch", "man\u00b7nig\u00b7falt", ",", "braun\u00b7gr\u00fcn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Gr\u00fcn weislich-Asch-grau und was mehr, zu un-", "tokens": ["Gr\u00fcn", "weis\u00b7lich\u00b7Aschgrau", "und", "was", "mehr", ",", "zu", "un"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "PWS", "ADV", "$,", "APPR", "TRUNC"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Vor Farben noch am Moos zu sehn. F\u00fcrnemlich", "tokens": ["Vor", "Far\u00b7ben", "noch", "am", "Moos", "zu", "sehn", ".", "F\u00fcr\u00b7nem\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$.", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Den Moos der gleichsam Kohlenschwarz in denen", "tokens": ["Den", "Moos", "der", "gleich\u00b7sam", "Koh\u00b7len\u00b7schwarz", "in", "de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "APPR", "PRELS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der wenn man auch von ferne steht, uns in die", "tokens": ["Der", "wenn", "man", "auch", "von", "fer\u00b7ne", "steht", ",", "uns", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "KOUS", "PIS", "ADV", "APPR", "ADV", "VVFIN", "$,", "PPER", "APPR", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Als w\u00e4r der Baum mit schwarzer Farb von zarten", "tokens": ["Als", "w\u00e4r", "der", "Baum", "mit", "schwar\u00b7zer", "Farb", "von", "zar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Will man auf ihren Ursprung sehn; so w\u00e4chset ein-", "tokens": ["Will", "man", "auf", "ih\u00b7ren", "Ur\u00b7sprung", "sehn", ";", "so", "w\u00e4ch\u00b7set", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$.", "ADV", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "In einen d\u00fcrr und steinigten, das andre in den", "tokens": ["In", "ei\u00b7nen", "d\u00fcrr", "und", "stei\u00b7nig\u00b7ten", ",", "das", "and\u00b7re", "in", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "KON", "VVFIN", "$,", "PRELS", "PIS", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Allwo die fette Feuchtigkeit, als wie ein Oel im", "tokens": ["All\u00b7wo", "die", "fet\u00b7te", "Feuch\u00b7tig\u00b7keit", ",", "als", "wie", "ein", "O\u00b7el", "im"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "APPRART"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "Da aus den gr\u00f6bsten Erden-Saft hernach der Moos", "tokens": ["Da", "aus", "den", "gr\u00f6bs\u00b7ten", "Er\u00b7den\u00b7Saft", "her\u00b7nach", "der", "Moos"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ob zwar derselbe sch\u00e4dlich ist den B\u00e4umen die in", "tokens": ["Ob", "zwar", "der\u00b7sel\u00b7be", "sch\u00e4d\u00b7lich", "ist", "den", "B\u00e4u\u00b7men", "die", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PDAT", "ADJD", "VAFIN", "ART", "NN", "ART", "APPR"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.26": {"text": "Weil wir so viele hie und da, die durch den Moos", "tokens": ["Weil", "wir", "so", "vie\u00b7le", "hie", "und", "da", ",", "die", "durch", "den", "Moos"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIS", "ADV", "KON", "ADV", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So ist er doch nicht minder n\u00fcz, dieweil der Vor-", "tokens": ["So", "ist", "er", "doch", "nicht", "min\u00b7der", "n\u00fcz", ",", "die\u00b7weil", "der", "Vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dadurch auch manche Pflanz und Baum vor ihren", "tokens": ["Da\u00b7durch", "auch", "man\u00b7che", "Pflanz", "und", "Baum", "vor", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "PIAT", "NN", "KON", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Der Moos bedekt den Stamm beim Frost, vorm", "tokens": ["Der", "Moos", "be\u00b7dekt", "den", "Stamm", "beim", "Frost", ",", "vorm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Besch\u00fczt ihn vor der kalten Luft, wie man aus der", "tokens": ["Be\u00b7sch\u00fczt", "ihn", "vor", "der", "kal\u00b7ten", "Luft", ",", "wie", "man", "aus", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PIS", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Er w\u00e4chst am meisten an der Seit die zeiget nach", "tokens": ["Er", "w\u00e4chst", "am", "meis\u00b7ten", "an", "der", "Seit", "die", "zei\u00b7get", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "PIS", "APPR", "ART", "NN", "ART", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und ist dadurch dem Wandersmann, wenn er ver-", "tokens": ["Und", "ist", "da\u00b7durch", "dem", "Wan\u00b7ders\u00b7mann", ",", "wenn", "er", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PAV", "ART", "NN", "$,", "KOUS", "PPER", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.33": {"text": "Er dient zum Compas in dem Wald, weil er meist", "tokens": ["Er", "dient", "zum", "Com\u00b7pas", "in", "dem", "Wald", ",", "weil", "er", "meist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.34": {"text": "Am meisten an der Seite w\u00e4chst, wo sich der Baum", "tokens": ["Am", "meis\u00b7ten", "an", "der", "Sei\u00b7te", "w\u00e4chst", ",", "wo", "sich", "der", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "PIS", "APPR", "ART", "NN", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wer dieses Zeichen nur bemerkt der kan an eines", "tokens": ["Wer", "die\u00b7ses", "Zei\u00b7chen", "nur", "be\u00b7merkt", "der", "kan", "an", "ei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "ADV", "VVFIN", "ART", "VMFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "In einem unbekandten Wald, gar leicht die rech-", "tokens": ["In", "ei\u00b7nem", "un\u00b7be\u00b7kand\u00b7ten", "Wald", ",", "gar", "leicht", "die", "rech"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Moos h\u00e4lt manchen Schaden ab, er mindert", "tokens": ["Der", "Moos", "h\u00e4lt", "man\u00b7chen", "Scha\u00b7den", "ab", ",", "er", "min\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Die einen nahen Untergang den B\u00e4umen in den", "tokens": ["Die", "ei\u00b7nen", "na\u00b7hen", "Un\u00b7ter\u00b7gang", "den", "B\u00e4u\u00b7men", "in", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Er h\u00e4lt den Bis der Schweine auf, da\u00df sie den", "tokens": ["Er", "h\u00e4lt", "den", "Bis", "der", "Schwei\u00b7ne", "auf", ",", "da\u00df", "sie", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPR", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Woran sie in der wilden Wuth die scharfen Z\u00e4hne", "tokens": ["Wo\u00b7ran", "sie", "in", "der", "wil\u00b7den", "Wuth", "die", "schar\u00b7fen", "Z\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Er st\u00e4rkt die jungen Pflanzen noch; wie manche", "tokens": ["Er", "st\u00e4rkt", "die", "jun\u00b7gen", "Pflan\u00b7zen", "noch", ";", "wie", "man\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$.", "PWAV", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Stammt nicht aus den gewachsnen Moos zum rei-", "tokens": ["Stammt", "nicht", "aus", "den", "ge\u00b7wachs\u00b7nen", "Moos", "zum", "rei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Wie nuzbar ist er \u00f6fters nicht bei dem Gebrauch", "tokens": ["Wie", "nuz\u00b7bar", "ist", "er", "\u00f6f\u00b7ters", "nicht", "bei", "dem", "Ge\u00b7brauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER