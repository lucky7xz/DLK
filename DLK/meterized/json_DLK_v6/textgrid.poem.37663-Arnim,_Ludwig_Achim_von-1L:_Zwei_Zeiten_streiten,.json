{"textgrid.poem.37663": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Zeiten streiten,", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Zeiten streiten,", "tokens": ["Zwei", "Zei\u00b7ten", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und die veralten", "tokens": ["Und", "die", "ver\u00b7al\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sich nicht mehr halten,", "tokens": ["Sich", "nicht", "mehr", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die alten Zeiten", "tokens": ["Die", "al\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Einst waren sch\u00f6ne,", "tokens": ["Einst", "wa\u00b7ren", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Auch Fr\u00fchlingss\u00f6hne!", "tokens": ["Auch", "Fr\u00fch\u00b7lings\u00b7s\u00f6h\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Fr\u00fchlingss\u00f6hne", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7s\u00f6h\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vom kalten Wetter", "tokens": ["Vom", "kal\u00b7ten", "Wet\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sind d\u00fcrre Bl\u00e4tter,", "tokens": ["Sind", "d\u00fcr\u00b7re", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Doch von Gew\u00f6hnen", "tokens": ["Doch", "von", "Ge\u00b7w\u00f6h\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Noch fest am Stamme,", "tokens": ["Noch", "fest", "am", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Es h\u00e4lt zusammen.", "tokens": ["Es", "h\u00e4lt", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Fr\u00fchlingsst\u00fcrme", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7st\u00fcr\u00b7me"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Durchziehn die L\u00fcfte,", "tokens": ["Durch\u00b7ziehn", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und selbst die Kl\u00fcfte", "tokens": ["Und", "selbst", "die", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Belebt Gew\u00fcrme,", "tokens": ["Be\u00b7lebt", "Ge\u00b7w\u00fcr\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Es kommen Schw\u00e4rme", "tokens": ["Es", "kom\u00b7men", "Schw\u00e4r\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mit wildem L\u00e4rmen.", "tokens": ["Mit", "wil\u00b7dem", "L\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wozu der Tadel,", "tokens": ["Wo\u00b7zu", "der", "Ta\u00b7del", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ihr wart einst j\u00fcnger", "tokens": ["Ihr", "wart", "einst", "j\u00fcn\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und nicht geringer,", "tokens": ["Und", "nicht", "ge\u00b7rin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ihr Herrn von Adel,", "tokens": ["Ihr", "Herrn", "von", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Doch nur mit Fechten", "tokens": ["Doch", "nur", "mit", "Fech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "K\u00f6nnt ihr jetzt rechten.", "tokens": ["K\u00f6nnt", "ihr", "jetzt", "rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Der \u00e4ltste Herzog,", "tokens": ["Der", "\u00e4lts\u00b7te", "Her\u00b7zog", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der soll sie f\u00fchren,", "tokens": ["Der", "soll", "sie", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Man kann es sp\u00fcren,", "tokens": ["Man", "kann", "es", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie lang' sie scherzen,", "tokens": ["Wie", "lang'", "sie", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da ist kein Wachen,", "tokens": ["Da", "ist", "kein", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Kein Ronden machen.", "tokens": ["Kein", "Ron\u00b7den", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Sie sind umgangen,", "tokens": ["Sie", "sind", "um\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Noch eh geschlagen;", "tokens": ["Noch", "eh", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Noch ist kein Zagen,", "tokens": ["Noch", "ist", "kein", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Noch ist kein Bangen,", "tokens": ["Noch", "ist", "kein", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der Feind mit Listen", "tokens": ["Der", "Feind", "mit", "Lis\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Will sich schon r\u00fcsten.", "tokens": ["Will", "sich", "schon", "r\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Der graue Nebel", "tokens": ["Der", "grau\u00b7e", "Ne\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der alten Zeiten,", "tokens": ["Der", "al\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der will sich breiten,", "tokens": ["Der", "will", "sich", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "ADJA", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Durchhaun vom S\u00e4bel", "tokens": ["Durch\u00b7haun", "vom", "S\u00e4\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mu\u00df er sich fl\u00fcchten,", "tokens": ["Mu\u00df", "er", "sich", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie stehn im Lichten.", "tokens": ["Sie", "stehn", "im", "Lich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Sie sind sich nahe,", "tokens": ["Sie", "sind", "sich", "na\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sich zu erreichen,", "tokens": ["Sich", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Will keiner weichen", "tokens": ["Will", "kei\u00b7ner", "wei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PIS", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So weit ich sahe;", "tokens": ["So", "weit", "ich", "sa\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "So wird am Tage", "tokens": ["So", "wird", "am", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Welt zerschlagen.", "tokens": ["Die", "Welt", "zer\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Der Adel stehet", "tokens": ["Der", "A\u00b7del", "ste\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mit seinem Blute,", "tokens": ["Mit", "sei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mit hohem Hute,", "tokens": ["Mit", "ho\u00b7hem", "Hu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Da\u00df ihr ihn sehet,", "tokens": ["Da\u00df", "ihr", "ihn", "se\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Sie m\u00fcssen's f\u00fchlen,", "tokens": ["Sie", "m\u00fcs\u00b7sen's", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Feinde zielen.", "tokens": ["Die", "Fein\u00b7de", "zie\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Noch stehn die Glieder,", "tokens": ["Noch", "stehn", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der Herzog gr\u00fc\u00dfet,", "tokens": ["Der", "Her\u00b7zog", "gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und kl\u00e4glich schie\u00dfet", "tokens": ["Und", "kl\u00e4g\u00b7lich", "schie\u00b7\u00dfet"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Feind ihn nieder.", "tokens": ["Der", "Feind", "ihn", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der Herzog sinket,", "tokens": ["Der", "Her\u00b7zog", "sin\u00b7ket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Kein F\u00fchrer winket.", "tokens": ["Kein", "F\u00fch\u00b7rer", "win\u00b7ket", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Wie Opfertiere", "tokens": ["Wie", "Op\u00b7fer\u00b7tie\u00b7re"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Gehn die Soldaten,", "tokens": ["Gehn", "die", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sind sie verrathen,", "tokens": ["Sind", "sie", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Da\u00df keiner f\u00fchre;", "tokens": ["Da\u00df", "kei\u00b7ner", "f\u00fch\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da ist kein Fragen,", "tokens": ["Da", "ist", "kein", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sie sind geschlagen.", "tokens": ["Sie", "sind", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Es sehn mit Reuen,", "tokens": ["Es", "sehn", "mit", "Reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was nun geschehen,", "tokens": ["Was", "nun", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was ungeschehen", "tokens": ["Was", "un\u00b7ge\u00b7sche\u00b7hen"], "token_info": ["word", "word"], "pos": ["PWS", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sehr viele scheuen.", "tokens": ["Sehr", "vie\u00b7le", "scheu\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wer kann es sagen,", "tokens": ["Wer", "kann", "es", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ohn' Scheu zu tragen.", "tokens": ["Ohn'", "Scheu", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Die Helden bilden", "tokens": ["Die", "Hel\u00b7den", "bil\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nicht V\u00e4ter, G\u00fcter,", "tokens": ["Nicht", "V\u00e4\u00b7ter", ",", "G\u00fc\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nur die Gem\u00fcther,", "tokens": ["Nur", "die", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+--+-", "measu