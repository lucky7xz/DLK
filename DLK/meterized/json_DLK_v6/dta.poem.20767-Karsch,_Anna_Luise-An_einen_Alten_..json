{"dta.poem.20767": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An einen Alten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und h\u00e4ttest du gleich einen Bart               ", "tokens": ["Und", "h\u00e4t\u00b7test", "du", "gleich", "ei\u00b7nen", "Bart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Nestor, als er in der Schlacht", "tokens": ["Wie", "Nes\u00b7tor", ",", "als", "er", "in", "der", "Schlacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beim Diomed zum Kutscher ward,", "tokens": ["Beim", "Dio\u00b7med", "zum", "Kut\u00b7scher", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So w\u00fcrdest du doch angelacht", "tokens": ["So", "w\u00fcr\u00b7dest", "du", "doch", "an\u00b7ge\u00b7lacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Sapho, welche stets an J\u00fcngling, Mann und", "tokens": ["Von", "Sa\u00b7pho", ",", "wel\u00b7che", "stets", "an", "J\u00fcng\u00b7ling", ",", "Mann", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "PRELS", "ADV", "APPR", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Greis", "tokens": ["Greis"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Des Herzens Reitz zu sch\u00e4tzen wei\u00df.", "tokens": ["Des", "Her\u00b7zens", "Reitz", "zu", "sch\u00e4t\u00b7zen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}