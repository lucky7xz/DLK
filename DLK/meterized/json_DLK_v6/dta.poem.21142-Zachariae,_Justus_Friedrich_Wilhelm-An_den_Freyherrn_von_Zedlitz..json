{"dta.poem.21142": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den Freyherrn von Zedlitz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Zedlitz, wie gl\u00fccklich bist du im Umgang der\nlehrenden Todten!", "tokens": ["Mein", "Zed\u00b7litz", ",", "wie", "gl\u00fcck\u00b7lich", "bist", "du", "im", "Um\u00b7gang", "der", "leh\u00b7ren\u00b7den", "Tod\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Noth des Dummkopfs kennest du nicht,", "tokens": ["Die", "Noth", "des", "Dumm\u00b7kopfs", "ken\u00b7nest", "du", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Wenn ihn in seinem Pallast die Langeweile verfolget;", "tokens": ["Wenn", "ihn", "in", "sei\u00b7nem", "Pal\u00b7last", "die", "Lan\u00b7ge\u00b7wei\u00b7le", "ver\u00b7fol\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn sie zu hirnlosen Sch\u00f6nen ihn scheucht;", "tokens": ["Wenn", "sie", "zu", "hirn\u00b7lo\u00b7sen", "Sch\u00f6\u00b7nen", "ihn", "scheucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn er im wilden Basset die leeren N\u00e4chte durchwa-", "tokens": ["Wenn", "er", "im", "wil\u00b7den", "Bas\u00b7set", "die", "lee\u00b7ren", "N\u00e4ch\u00b7te", "durch\u00b7wa"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und in dem traurigen Lomberspiel g\u00e4hnt;", "tokens": ["Und", "in", "dem", "trau\u00b7ri\u00b7gen", "Lom\u00b7ber\u00b7spiel", "g\u00e4hnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn seinem ekelnden Sinn so wenig sein Pferd, als", "tokens": ["Wenn", "sei\u00b7nem", "e\u00b7keln\u00b7den", "Sinn", "so", "we\u00b7nig", "sein", "Pferd", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "PIS", "PPOSAT", "NN", "$,", "KOUS"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Noch Maskerade zum Zeitvertreib wird;", "tokens": ["Noch", "Mas\u00b7ke\u00b7ra\u00b7de", "zum", "Zeit\u00b7ver\u00b7treib", "wird", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn er das Leere nun f\u00fchlt, mit dem das Schicksal", "tokens": ["Wenn", "er", "das", "Lee\u00b7re", "nun", "f\u00fchlt", ",", "mit", "dem", "das", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,", "APPR", "ART", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das ihm zwar Ahnen und Reichth\u00fcmer gab;", "tokens": ["Das", "ihm", "zwar", "Ah\u00b7nen", "und", "Reicht\u00b7h\u00fc\u00b7mer", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch welches dagegen ihn auch der hohen Gaben berau-", "tokens": ["Doch", "wel\u00b7ches", "da\u00b7ge\u00b7gen", "ihn", "auch", "der", "ho\u00b7hen", "Ga\u00b7ben", "be\u00b7rau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PAV", "PPER", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+--+--+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die man nicht immer von Ahnen ererbt;", "tokens": ["Die", "man", "nicht", "im\u00b7mer", "von", "Ah\u00b7nen", "er\u00b7erbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann, Zedlitz, findet man dich im Krais der b\u00e4rtigen", "tokens": ["Dann", ",", "Zed\u00b7litz", ",", "fin\u00b7det", "man", "dich", "im", "Krais", "der", "b\u00e4r\u00b7ti\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NE", "$,", "VVFIN", "PIS", "PRF", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und bey den Helden des bl\u00fchenden Roms.", "tokens": ["Und", "bey", "den", "Hel\u00b7den", "des", "bl\u00fc\u00b7hen\u00b7den", "Roms", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Du wagest k\u00fchn auch alsdann dich zu tiefsinnigen Brit-", "tokens": ["Du", "wa\u00b7gest", "k\u00fchn", "auch", "als\u00b7dann", "dich", "zu", "tief\u00b7sin\u00b7ni\u00b7gen", "Brit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ADV", "PPER", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und zu der gallischen Dichter Gesang.", "tokens": ["Und", "zu", "der", "gal\u00b7li\u00b7schen", "Dich\u00b7ter", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wo bleibt alsdann dir der Tag, wo bleibt der Abend", "tokens": ["Wo", "bleibt", "als\u00b7dann", "dir", "der", "Tag", ",", "wo", "bleibt", "der", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "PPER", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Rauscht er mit eilendem Fittig nicht fort?", "tokens": ["Rauscht", "er", "mit", "ei\u00b7len\u00b7dem", "Fit\u00b7tig", "nicht", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und hat der Morgen nicht oft dich bey der vertrauli-", "tokens": ["Und", "hat", "der", "Mor\u00b7gen", "nicht", "oft", "dich", "bey", "der", "ver\u00b7trau\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "PRF", "APPR", "ART", "TRUNC"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Auf Miltons Ges\u00e4nge horchend gesehn?", "tokens": ["Auf", "Mil\u00b7tons", "Ge\u00b7s\u00e4n\u00b7ge", "hor\u00b7chend", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Welch ein entz\u00fcckender Trost ist die Gesellschaft der Mu-", "tokens": ["Welch", "ein", "ent\u00b7z\u00fc\u00b7cken\u00b7der", "Trost", "ist", "die", "Ge\u00b7sell\u00b7schaft", "der", "Mu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie folgen selber im Ungl\u00fcck uns nach.", "tokens": ["Sie", "fol\u00b7gen", "sel\u00b7ber", "im", "Un\u00b7gl\u00fcck", "uns", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie lassen uns niemals allein; und sind sowohl in der", "tokens": ["Sie", "las\u00b7sen", "uns", "nie\u00b7mals", "al\u00b7lein", ";", "und", "sind", "so\u00b7wohl", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$.", "KON", "VAFIN", "KON", "APPR", "ART"], "meter": "-+--+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als in bev\u00f6lkerten St\u00e4dten bey uns.", "tokens": ["Als", "in", "be\u00b7v\u00f6l\u00b7ker\u00b7ten", "St\u00e4d\u00b7ten", "bey", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}