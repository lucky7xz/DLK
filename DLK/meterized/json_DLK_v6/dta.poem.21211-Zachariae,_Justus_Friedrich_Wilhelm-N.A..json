{"dta.poem.21211": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seyd immer Wanderer auf Erden,", "tokens": ["Seyd", "im\u00b7mer", "Wan\u00b7de\u00b7rer", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und opfert nicht blo\u00df rednerischen Dank", "tokens": ["Und", "op\u00b7fert", "nicht", "blo\u00df", "red\u00b7ne\u00b7ri\u00b7schen", "Dank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem, der f\u00fcr euch den Kelch des Todes trank;", "tokens": ["Dem", ",", "der", "f\u00fcr", "euch", "den", "Kelch", "des", "To\u00b7des", "trank", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erf\u00fcllt gehorsam sein Gebot,", "tokens": ["Er\u00b7f\u00fcllt", "ge\u00b7hor\u00b7sam", "sein", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und preiset seinen Tod", "tokens": ["Und", "prei\u00b7set", "sei\u00b7nen", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch tugendhaftes Leben.", "tokens": ["Durch", "tu\u00b7gend\u00b7haf\u00b7tes