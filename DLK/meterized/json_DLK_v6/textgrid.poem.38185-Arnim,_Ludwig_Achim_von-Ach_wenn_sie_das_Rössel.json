{"textgrid.poem.38185": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ach wenn sie das R\u00f6ssel", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlimm Leut sind Studenten, man sagts \u00fcberall,", "tokens": ["Schlimm", "Leut", "sind", "Stu\u00b7den\u00b7ten", ",", "man", "sagts", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "NN", "$,", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Obwohl sie schon kommen im Jahr nur einmal,", "tokens": ["Ob\u00b7wohl", "sie", "schon", "kom\u00b7men", "im", "Jahr", "nur", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "ADV", "ADV", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So machens ins Dorf so viel Unruh und Mist,", "tokens": ["So", "ma\u00b7chens", "ins", "Dorf", "so", "viel", "Un\u00b7ruh", "und", "Mist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da\u00df uns die erste Woche schon weh dabey ist.", "tokens": ["Da\u00df", "uns", "die", "ers\u00b7te", "Wo\u00b7che", "schon", "weh", "da\u00b7bey", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "PAV", "VAFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir m\u00fcssen nur sorgen auf Mariengeburt,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "nur", "sor\u00b7gen", "auf", "Ma\u00b7ri\u00b7en\u00b7ge\u00b7burt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es w\u00fcnscht auch ein jeder, da\u00df Galli bald wird,", "tokens": ["Es", "w\u00fcnscht", "auch", "ein", "je\u00b7der", ",", "da\u00df", "Gal\u00b7li", "bald", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PIS", "$,", "KOUS", "NE", "ADV", "VAFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da kommens mit Degen und B\u00fcchsen daher,", "tokens": ["Da", "kom\u00b7mens", "mit", "De\u00b7gen", "und", "B\u00fcch\u00b7sen", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und machen im Dorfe ein j\u00e4mmerlich Gescher.", "tokens": ["Und", "ma\u00b7chen", "im", "Dor\u00b7fe", "ein", "j\u00e4m\u00b7mer\u00b7lich", "Ge\u00b7scher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nichts ist vor ihnen sicher, kein Henne, kein Taube,", "tokens": ["Nichts", "ist", "vor", "ih\u00b7nen", "si\u00b7cher", ",", "kein", "Hen\u00b7ne", ",", "kein", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "ADJD", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als w\u00e4rens erschaffen zum Pl\u00fcndern und Raube,", "tokens": ["Als", "w\u00e4\u00b7rens", "er\u00b7schaf\u00b7fen", "zum", "Pl\u00fcn\u00b7dern", "und", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVPP", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Darf ihnen kein Gans auf die Wiesen naus trauen,", "tokens": ["Darf", "ih\u00b7nen", "kein", "Gans", "auf", "die", "Wie\u00b7sen", "naus", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "APPR", "ART", "NN", "APZR", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Studenten thun ihr gleich den Kragen weghauen.", "tokens": ["Stu\u00b7den\u00b7ten", "thun", "ihr", "gleich", "den", "Kra\u00b7gen", "weg\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Sind G\u00e4rten mit Brettern und Riegeln umz\u00e4unt,", "tokens": ["Sind", "G\u00e4r\u00b7ten", "mit", "Bret\u00b7tern", "und", "Rie\u00b7geln", "um\u00b7z\u00e4unt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So thun sies zerbrechen, da\u00df die Sonne durchscheint,", "tokens": ["So", "thun", "sies", "zer\u00b7bre\u00b7chen", ",", "da\u00df", "die", "Son\u00b7ne", "durch\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie steigen um die Aepfel, zerreissen die B\u00e4um,", "tokens": ["Sie", "stei\u00b7gen", "um", "die", "A\u00b7e\u00b7pfel", ",", "zer\u00b7reis\u00b7sen", "die", "B\u00e4um", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "W\u00e4r zufrieden, tr\u00fcg jeder nur ein Tasche voll heim.", "tokens": ["W\u00e4r", "zu\u00b7frie\u00b7den", ",", "tr\u00fcg", "je\u00b7der", "nur", "ein", "Ta\u00b7sche", "voll", "heim", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VVFIN", "PIS", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Mit Feuer und Pulver sinds gar sehr gef\u00e4hr,", "tokens": ["Mit", "Feu\u00b7er", "und", "Pul\u00b7ver", "sinds", "gar", "sehr", "ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df oft eim sein H\u00e4usel verbrennet gar w\u00e4r,", "tokens": ["Da\u00df", "oft", "eim", "sein", "H\u00e4u\u00b7sel", "ver\u00b7bren\u00b7net", "gar", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "ADV", "VAFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Lassen pulverne Fr\u00f6schle einem hupfen aufs Dach,", "tokens": ["Las\u00b7sen", "pul\u00b7ver\u00b7ne", "Fr\u00f6schle", "ei\u00b7nem", "hup\u00b7fen", "aufs", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Wenns brennet, so fragens kein Teufel danach.", "tokens": ["Wenns", "bren\u00b7net", ",", "so", "fra\u00b7gens", "kein", "Teu\u00b7fel", "da\u00b7nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ADV", "ADV", "PIAT", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Hat einer beym H\u00e4usel ein wachbaren Hund,", "tokens": ["Hat", "ei\u00b7ner", "beym", "H\u00e4u\u00b7sel", "ein", "wach\u00b7ba\u00b7ren", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der sonst von der Kette nicht abkommen kunt,", "tokens": ["Der", "sonst", "von", "der", "Ket\u00b7te", "nicht", "ab\u00b7kom\u00b7men", "kunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie lassen ihn laufen, es w\u00e4r ja nicht noth,", "tokens": ["Sie", "las\u00b7sen", "ihn", "lau\u00b7fen", ",", "es", "w\u00e4r", "ja", "nicht", "noth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u00bbpotz Hagel da schie\u00dft's ja!\u00ab der Pudel ist todt.", "tokens": ["\u00bb", "potz", "Ha\u00b7gel", "da", "schie\u00dft's", "ja", "!", "\u00ab", "der", "Pu\u00b7del", "ist", "todt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "ADV", "VVFIN", "ADV", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Studenten im Wirthshaus, sinds aus der Weis frisch,", "tokens": ["Stu\u00b7den\u00b7ten", "im", "Wirths\u00b7haus", ",", "sinds", "aus", "der", "Weis", "frisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+--++", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie brauchen allein ein gro\u00dfm\u00e4chtigen Tisch,", "tokens": ["Sie", "brau\u00b7chen", "al\u00b7lein", "ein", "gro\u00df\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sie saufen und schreien als g'h\u00f6rt das Haus ihn'n,", "tokens": ["Sie", "sau\u00b7fen", "und", "schrei\u00b7en", "als", "g'\u00b7h\u00f6rt", "das", "Haus", "ihn'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KOKOM", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und saufen und schreien sich blitzblau und gr\u00fcn.", "tokens": ["Und", "sau\u00b7fen", "und", "schrei\u00b7en", "sich", "blitz\u00b7blau", "und", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Bald redens lapodeinisch, ich kanns nicht verstehn,", "tokens": ["Bald", "re\u00b7dens", "la\u00b7po\u00b7dei\u00b7nisch", ",", "ich", "kanns", "nicht", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ists leicht zu rathen, auf uns mu\u00df es gehn,", "tokens": ["Doch", "ists", "leicht", "zu", "ra\u00b7then", ",", "auf", "uns", "mu\u00df", "es", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,", "APPR", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Bald tanzens und springens und hupfens am Fleck,", "tokens": ["Bald", "tan\u00b7zens", "und", "sprin\u00b7gens", "und", "hup\u00b7fens", "am", "Fleck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "VVFIN", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und nehmen den Knechten den Tanzboden weg.", "tokens": ["Und", "neh\u00b7men", "den", "Knech\u00b7ten", "den", "Tanz\u00b7bo\u00b7den", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Und schmeissen die Knecht sie auch alle heraus,", "tokens": ["Und", "schmeis\u00b7sen", "die", "Knecht", "sie", "auch", "al\u00b7le", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So laufens wie die M\u00e4us auf die Strassen hinaus,", "tokens": ["So", "lau\u00b7fens", "wie", "die", "M\u00e4us", "auf", "die", "Stras\u00b7sen", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und machen ein Haufen und grausam Gefecht,", "tokens": ["Und", "ma\u00b7chen", "ein", "Hau\u00b7fen", "und", "grau\u00b7sam", "Ge\u00b7fecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und hauen und stechen und schreien erst recht.", "tokens": ["Und", "hau\u00b7en", "und", "ste\u00b7chen", "und", "schrei\u00b7en", "erst", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVINF", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Ziehn naus auf die Felder und geben kein Fried,", "tokens": ["Ziehn", "naus", "auf", "die", "Fel\u00b7der", "und", "ge\u00b7ben", "kein", "Fried", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ist grad wie ein Wetter, so spielens damit,", "tokens": ["Ist", "grad", "wie", "ein", "Wet\u00b7ter", ",", "so", "spie\u00b7lens", "da\u00b7mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "ART", "NN", "$,", "ADV", "ADV", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da tretens die Aecker, verstehn nicht was 's ist,", "tokens": ["Da", "tre\u00b7tens", "die", "A\u00b7e\u00b7cker", ",", "ver\u00b7stehn", "nicht", "was", "'s", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "PTKNEG", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn einer schwarz Brod um sein Handarbeit fri\u00dft.", "tokens": ["Wenn", "ei\u00b7ner", "schwarz", "Brod", "um", "sein", "Hand\u00b7ar\u00b7beit", "fri\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Sind Ro\u00df auf der Weide, und rasten ein Weil,", "tokens": ["Sind", "Ro\u00df", "auf", "der", "Wei\u00b7de", ",", "und", "ras\u00b7ten", "ein", "Weil", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So nehmens Studenten, es ist gar ein Gr\u00e4ul,", "tokens": ["So", "neh\u00b7mens", "Stu\u00b7den\u00b7ten", ",", "es", "ist", "gar", "ein", "Gr\u00e4ul", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und hauens in die Seiten mit allbeiden F\u00fcssen:", 