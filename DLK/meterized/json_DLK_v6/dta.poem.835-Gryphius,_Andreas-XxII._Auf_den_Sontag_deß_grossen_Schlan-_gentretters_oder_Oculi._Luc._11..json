{"dta.poem.835": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxII.  Auf den Sontag de\u00df grossen Schlan-  \n gentretters/ oder  Oculi.  Luc. 11.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der du dich von dem Thron der Ewigkeit begeben ", "tokens": ["Der", "du", "dich", "von", "dem", "Thron", "der", "E\u00b7wig\u00b7keit", "be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Raube-Schlo\u00df der Welt/ die mehr denn feste", "tokens": ["Ins", "Rau\u00b7be\u00b7Schlo\u00df", "der", "Welt", "/", "die", "mehr", "denn", "fes\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$(", "ART", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Macht ", "tokens": ["Macht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Mit der der H\u00f6llen F\u00fcrst/ der Printz der schwartzen Nacht", "tokens": ["Mit", "der", "der", "H\u00f6l\u00b7len", "F\u00fcrst", "/", "der", "Printz", "der", "schwart\u00b7zen", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "NN", "$(", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein R\u00fcsthau\u00df hat verschrenckt/ auff ewig auff zuheben:", "tokens": ["Sein", "R\u00fcst\u00b7hau\u00df", "hat", "ver\u00b7schrenckt", "/", "auff", "e\u00b7wig", "auff", "zu\u00b7he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$(", "APPR", "ADJD", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach schaw\u2019/ in was f\u00fcr Furcht/ in was f\u00fcr angst wir ", "tokens": ["Ach", "scha\u00b7w'", "/", "in", "was", "f\u00fcr", "Furcht", "/", "in", "was", "f\u00fcr", "angst", "wir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$(", "APPR", "PRELS", "APPR", "NN", "$(", "APPR", "PRELS", "APPR", "VVFIN", "PPER"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In dem der starcke Feind schier augenblicklich tracht", "tokens": ["In", "dem", "der", "star\u00b7cke", "Feind", "schier", "au\u00b7gen\u00b7blick\u00b7lich", "tracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie er durch grim\u0303 vnd List/ durch wollust/ Pein v\u00f1 Pracht/", "tokens": ["Wie", "er", "durch", "grim\u0303", "vnd", "List", "/", "durch", "wol\u00b7lust", "/", "Pein", "v\u00f1", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NE", "KON", "NN", "$(", "APPR", "NN", "$(", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was du dir selbst erwehlt/ jhm mache recht vnd eben.", "tokens": ["Was", "du", "dir", "selbst", "er\u00b7wehlt", "/", "jhm", "ma\u00b7che", "recht", "vnd", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVPP", "$(", "PPER", "VVFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Treib aus du starcker Heldt/ treib den Verderber aus/", "tokens": ["Treib", "aus", "du", "star\u00b7cker", "Heldt", "/", "treib", "den", "Ver\u00b7der\u00b7ber", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJA", "NN", "$(", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mordet vnd verstrew\u2019t. Zeuch in mein Seelen Hau\u00df", "tokens": ["Der", "mor\u00b7det", "vnd", "ver\u00b7stre\u00b7w'", "t.", "Zeuch", "in", "mein", "See\u00b7len", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "NE", "NE", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vnd la\u00df mich eins mit dir durch lieb vnd glauben bleiben/", "tokens": ["Vnd", "la\u00df", "mich", "eins", "mit", "dir", "durch", "lieb", "vnd", "glau\u00b7ben", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PIS", "APPR", "PPER", "APPR", "ADJD", "KON", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gib da\u00df ich deine Wortt vnd gnadenreiche Lehr/", "tokens": ["Gib", "da\u00df", "ich", "dei\u00b7ne", "Wortt", "vnd", "gna\u00b7den\u00b7rei\u00b7che", "Lehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "PPOSAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die einig-seelig macht/ mit ernster Andacht h\u00f6r/", "tokens": ["Die", "ei\u00b7nig\u00b7see\u00b7lig", "macht", "/", "mit", "erns\u00b7ter", "An\u00b7dacht", "h\u00f6r", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$(", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd was ich h\u00f6re/ m\u00f6g in dieses Hertz einschreiben.", "tokens": ["Vnd", "was", "ich", "h\u00f6\u00b7re", "/", "m\u00f6g", "in", "die\u00b7ses", "Hertz", "ein\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$(", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}