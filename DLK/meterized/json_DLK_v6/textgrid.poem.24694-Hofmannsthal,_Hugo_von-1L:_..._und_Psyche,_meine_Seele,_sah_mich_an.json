{"textgrid.poem.24694": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L:  ... und Psyche, meine Seele, sah mich an", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": " ... und Psyche, meine Seele, sah mich an", "tokens": ["...", "und", "Psy\u00b7che", ",", "mei\u00b7ne", "See\u00b7le", ",", "sah", "mich", "an"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "KON", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von unterdr\u00fccktem Weinen bla\u00df und bebend", "tokens": ["Von", "un\u00b7ter\u00b7dr\u00fcck\u00b7tem", "Wei\u00b7nen", "bla\u00df", "und", "be\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sagte leise: \u00bbHerr, ich m\u00f6chte sterben,", "tokens": ["Und", "sag\u00b7te", "lei\u00b7se", ":", "\u00bb", "Herr", ",", "ich", "m\u00f6ch\u00b7te", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "$(", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin zum Sterben m\u00fcde und mich friert.\u00ab", "tokens": ["Ich", "bin", "zum", "Ster\u00b7ben", "m\u00fc\u00b7de", "und", "mich", "friert", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADJD", "KON", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Psyche, Psyche, meine kleine Seele,", "tokens": ["O", "Psy\u00b7che", ",", "Psy\u00b7che", ",", "mei\u00b7ne", "klei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sei still, ich will dir einen Trank bereiten,", "tokens": ["Sei", "still", ",", "ich", "will", "dir", "ei\u00b7nen", "Trank", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der warmes Leben str\u00f6mt durch alle Glieder.", "tokens": ["Der", "war\u00b7mes", "Le\u00b7ben", "str\u00f6mt", "durch", "al\u00b7le", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit gutem warmem Wein will ich dich tr\u00e4nken,", "tokens": ["Mit", "gu\u00b7tem", "war\u00b7mem", "Wein", "will", "ich", "dich", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit gl\u00fchendem spr\u00fchendem Saft des lebendigen", "tokens": ["Mit", "gl\u00fc\u00b7hen\u00b7dem", "spr\u00fc\u00b7hen\u00b7dem", "Saft", "des", "le\u00b7ben\u00b7di\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "ART", "ADJA"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Funkelnden, dunkelnden, rauschend unb\u00e4ndigen,", "tokens": ["Fun\u00b7keln\u00b7den", ",", "dun\u00b7keln\u00b7den", ",", "rau\u00b7schend", "un\u00b7b\u00e4n\u00b7di\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJD", "ADJD", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Quellenden, schwellenden, lachenden Lebens,", "tokens": ["Quel\u00b7len\u00b7den", ",", "schwel\u00b7len\u00b7den", ",", "la\u00b7chen\u00b7den", "Le\u00b7bens", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Mit Farben und Garben des trunkenen Bebens:", "tokens": ["Mit", "Far\u00b7ben", "und", "Gar\u00b7ben", "des", "trun\u00b7ke\u00b7nen", "Be\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Mit sehnender Seele von weinenden Liedern,", "tokens": ["Mit", "seh\u00b7nen\u00b7der", "See\u00b7le", "von", "wei\u00b7nen\u00b7den", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Mit Ballspiel und Grazie von tanzenden Gliedern,", "tokens": ["Mit", "Ball\u00b7spiel", "und", "Gra\u00b7zie", "von", "tan\u00b7zen\u00b7den", "Glie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Mit jauchzender Sch\u00f6nheit von sonnigem Wehen", "tokens": ["Mit", "jauch\u00b7zen\u00b7der", "Sch\u00f6n\u00b7heit", "von", "son\u00b7ni\u00b7gem", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "Hellrollender St\u00fcrme auf schwarzgr\u00fcnen Seen,", "tokens": ["Hell\u00b7rol\u00b7len\u00b7der", "St\u00fcr\u00b7me", "auf", "schwarz\u00b7gr\u00fc\u00b7nen", "Seen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Mit G\u00e4rten, wo Rosen und Efeu verwildern,", "tokens": ["Mit", "G\u00e4r\u00b7ten", ",", "wo", "Ro\u00b7sen", "und", "E\u00b7feu", "ver\u00b7wil\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Mit blassen Frauen und leuchtenden Bildern,", "tokens": ["Mit", "blas\u00b7sen", "Frau\u00b7en", "und", "leuch\u00b7ten\u00b7den", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Mit fremden L\u00e4ndern, mit violetten", "tokens": ["Mit", "frem\u00b7den", "L\u00e4n\u00b7dern", ",", "mit", "vi\u00b7o\u00b7let\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Gelbleuchtenden Wolken und Rosenbetten,", "tokens": ["Gelb\u00b7leuch\u00b7ten\u00b7den", "Wol\u00b7ken", "und", "Ro\u00b7sen\u00b7bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Mit hei\u00dfen Rubinen, gr\u00fcngoldenen Ringen", "tokens": ["Mit", "hei\u00b7\u00dfen", "Ru\u00b7bi\u00b7nen", ",", "gr\u00fcn\u00b7gol\u00b7de\u00b7nen", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Und allen prunkenden duftenden Dingen.", "tokens": ["Und", "al\u00b7len", "prun\u00b7ken\u00b7den", "duf\u00b7ten\u00b7den", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und Psyche, meine Seele, sah mich an", "tokens": ["Und", "Psy\u00b7che", ",", "mei\u00b7ne", "See\u00b7le", ",", "sah", "mich", "an"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sagte traurig: \u00bbAlle diese Dinge", "tokens": ["Und", "sag\u00b7te", "trau\u00b7rig", ":", "\u00bb", "Al\u00b7le", "die\u00b7se", "Din\u00b7ge"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$.", "$(", "PIS", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind schal und tr\u00fcb und tot. Das Leben hat", "tokens": ["Sind", "schal", "und", "tr\u00fcb", "und", "tot", ".", "Das", "Le\u00b7ben", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht Glanz und Duft. Ich bin es m\u00fcde, Herr.\u00ab", "tokens": ["Nicht", "Glanz", "und", "Duft", ".", "Ich", "bin", "es", "m\u00fc\u00b7de", ",", "Herr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich sagte: Noch wei\u00df ich wohl eine Welt,", "tokens": ["Ich", "sag\u00b7te", ":", "Noch", "wei\u00df", "ich", "wohl", "ei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn dir die lebendige nicht gef\u00e4llt.", "tokens": ["Wenn", "dir", "die", "le\u00b7ben\u00b7di\u00b7ge", "nicht", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit wunderbar nie vernommenen Worten", "tokens": ["Mit", "wun\u00b7der\u00b7bar", "nie", "ver\u00b7nom\u00b7me\u00b7nen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADV", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Rei\u00df ich dir auf der Tr\u00e4ume Pforten:", "tokens": ["Rei\u00df", "ich", "dir", "auf", "der", "Tr\u00e4u\u00b7me", "Pfor\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Mit goldengl\u00fchenden, s\u00fc\u00dfen lauen", "tokens": ["Mit", "gol\u00b7den\u00b7gl\u00fc\u00b7hen\u00b7den", ",", "s\u00fc\u00b7\u00dfen", "lau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADV", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie duftendes Tanzen von lachenden Frauen,", "tokens": ["Wie", "duf\u00b7ten\u00b7des", "Tan\u00b7zen", "von", "la\u00b7chen\u00b7den", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Mit monddurchsickerten n\u00e4chtig webenden", "tokens": ["Mit", "mond\u00b7durch\u00b7si\u00b7cker\u00b7ten", "n\u00e4ch\u00b7tig", "we\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVINF"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wie fiebernde Blumenkelche bebenden,", "tokens": ["Wie", "fie\u00b7bern\u00b7de", "Blu\u00b7men\u00b7kel\u00b7che", "be\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Mit gr\u00fcnen, rieselnden, k\u00fchlen, feuchten", "tokens": ["Mit", "gr\u00fc\u00b7nen", ",", "rie\u00b7seln\u00b7den", ",", "k\u00fch\u00b7len", ",", "feuch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ADJA", "$,", "VVFIN", "$,", "VVINF", "$,", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Wie rieselndes gr\u00fcnes Meeresleuchten,", "tokens": ["Wie", "rie\u00b7seln\u00b7des", "gr\u00fc\u00b7nes", "Mee\u00b7res\u00b7leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Mit trunken tanzenden, dunklen, schw\u00fclen", 