{"textgrid.poem.60324": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Urwald", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Land voll tr\u00e4umerischem Trug,", "tokens": ["Es", "ist", "ein", "Land", "voll", "tr\u00e4u\u00b7me\u00b7ri\u00b7schem", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf das die Freiheit im Vor\u00fcberflug", "tokens": ["Auf", "das", "die", "Frei\u00b7heit", "im", "Vor\u00b7\u00fc\u00b7berf\u00b7lug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bezaubernd ihren Schatten fallen l\u00e4\u00dft,", "tokens": ["Be\u00b7zau\u00b7bernd", "ih\u00b7ren", "Schat\u00b7ten", "fal\u00b7len", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und das ihn h\u00e4lt in tausend Bildern fest;", "tokens": ["Und", "das", "ihn", "h\u00e4lt", "in", "tau\u00b7send", "Bil\u00b7dern", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVFIN", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wohin das Ungl\u00fcck fl\u00fcchtet ferneher", "tokens": ["Wo\u00b7hin", "das", "Un\u00b7gl\u00fcck", "fl\u00fcch\u00b7tet", "fer\u00b7ne\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und das Verbrechen zittert \u00fcbers Meer;", "tokens": ["Und", "das", "Ver\u00b7bre\u00b7chen", "zit\u00b7tert", "\u00fc\u00b7bers", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Land, bei dessen lockendem Verhei\u00dfen", "tokens": ["Das", "Land", ",", "bei", "des\u00b7sen", "lo\u00b7cken\u00b7dem", "Ver\u00b7hei\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Hoffnung oft vom Sterbelager sprang", "tokens": ["Die", "Hoff\u00b7nung", "oft", "vom", "Ster\u00b7be\u00b7la\u00b7ger", "sprang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und ihr Panier durch alle St\u00fcrme schwang,", "tokens": ["Und", "ihr", "Pa\u00b7nier", "durch", "al\u00b7le", "St\u00fcr\u00b7me", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Um es am fremden Strande zu zerrei\u00dfen", "tokens": ["Um", "es", "am", "frem\u00b7den", "Stran\u00b7de", "zu", "zer\u00b7rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "APPRART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und dort den zwiefach bittern Tod zu haben;", "tokens": ["Und", "dort", "den", "zwie\u00b7fach", "bit\u00b7tern", "Tod", "zu", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "APPR", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die Heimat h\u00e4tte weicher sie begraben! \u2013", "tokens": ["Die", "Hei\u00b7mat", "h\u00e4t\u00b7te", "wei\u00b7cher", "sie", "be\u00b7gra\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "In jenem Lande bin ich einst geritten", "tokens": ["In", "je\u00b7nem", "Lan\u00b7de", "bin", "ich", "einst", "ge\u00b7rit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Den Weg, der einen finstern Wald durchschnitten;", "tokens": ["Den", "Weg", ",", "der", "ei\u00b7nen", "fins\u00b7tern", "Wald", "durch\u00b7schnit\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die Sonne war geneigt im Untergang,", "tokens": ["Die", "Son\u00b7ne", "war", "ge\u00b7neigt", "im", "Un\u00b7ter\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Nur leise strich der Wind, kein Vogel sang.", "tokens": ["Nur", "lei\u00b7se", "strich", "der", "Wind", ",", "kein", "Vo\u00b7gel", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ART", "NN", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Da stieg ich ab, mein Ro\u00df am Quell zu tr\u00e4nken,", "tokens": ["Da", "stieg", "ich", "ab", ",", "mein", "Ro\u00df", "am", "Quell", "zu", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Mich in den Blick der Wildnis zu versenken.", "tokens": ["Mich", "in", "den", "Blick", "der", "Wild\u00b7nis", "zu", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Vermildernd schien das helle Abendrot", "tokens": ["Ver\u00b7mil\u00b7dernd", "schien", "das", "hel\u00b7le", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Auf dieses Urwalds grauenvolle St\u00e4tte,", "tokens": ["Auf", "die\u00b7ses", "Ur\u00b7walds", "grau\u00b7en\u00b7vol\u00b7le", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wo ungest\u00f6rt das Leben mit dem Tod", "tokens": ["Wo", "un\u00b7ge\u00b7st\u00f6rt", "das", "Le\u00b7ben", "mit", "dem", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Jahrtausendlang gek\u00e4mpft die ernste Wette.", "tokens": ["Jahr\u00b7tau\u00b7send\u00b7lang", "ge\u00b7k\u00e4mpft", "die", "erns\u00b7te", "Wet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Umsonst das Leben hier zu gr\u00fcnen sucht,", "tokens": ["Um\u00b7sonst", "das", "Le\u00b7ben", "hier", "zu", "gr\u00fc\u00b7nen", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Erdr\u00fccket von des Todes \u00dcberwucht,", "tokens": ["Er\u00b7dr\u00fc\u00b7cket", "von", "des", "To\u00b7des", "\u00dc\u00b7ber\u00b7wucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Denn endlich hat der Tod, der starke Zwinger,", "tokens": ["Denn", "end\u00b7lich", "hat", "der", "Tod", ",", "der", "star\u00b7ke", "Zwin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die Faust geballt, das Leben eingeschlossen,", "tokens": ["Die", "Faust", "ge\u00b7ballt", ",", "das", "Le\u00b7ben", "ein\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Es sucht umsonst, hier, dort hervorzusprossen", "tokens": ["Es", "sucht", "um\u00b7sonst", ",", "hier", ",", "dort", "her\u00b7vor\u00b7zu\u00b7spros\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Durch Moderst\u00e4mme, d\u00fcrre Todesfinger.", "tokens": ["Durch", "Mo\u00b7der\u00b7st\u00e4m\u00b7me", ",", "d\u00fcr\u00b7re", "To\u00b7des\u00b7fin\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Wohin, o Tod, wirst du das Pflanzenleben", "tokens": ["Wo\u00b7hin", ",", "o", "Tod", ",", "wirst", "du", "das", "Pflan\u00b7zen\u00b7le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "FM", "NN", "$,", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "In deiner starken Faust und meines heben?", "tokens": ["In", "dei\u00b7ner", "star\u00b7ken", "Faust", "und", "mei\u00b7nes", "he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Wirst du sie \u00f6ffnen? wird sie ewig schlie\u00dfen?", "tokens": ["Wirst", "du", "sie", "\u00f6ff\u00b7nen", "?", "wird", "sie", "e\u00b7wig", "schlie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVINF", "$.", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "So frug ich bange zweifelnd und empfand", "tokens": ["So", "frug", "ich", "ban\u00b7ge", "zwei\u00b7felnd", "und", "emp\u00b7fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Im Wind das F\u00e4cheln schon der Todeshand", "tokens": ["Im", "Wind", "das", "F\u00e4\u00b7cheln", "schon", "der", "To\u00b7des\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Und f\u00fchlt es k\u00fchler schon im Herzen flie\u00dfen.", "tokens": ["Und", "f\u00fchlt", "es", "k\u00fch\u00b7ler", "schon", "im", "Her\u00b7zen", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und lange lag ich auf des Waldes Grund,", "tokens": ["Und", "lan\u00b7ge", "lag", "ich", "auf", "des", "Wal\u00b7des", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Das Haupt gedr\u00fcckt ins alte, tiefe Laub,", "tokens": ["Das", "Haupt", "ge\u00b7dr\u00fcckt", "ins", "al\u00b7te", ",", "tie\u00b7fe", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Und starrte, trauriger Gedanken Raub,", "tokens": ["Und", "starr\u00b7te", ",", "trau\u00b7ri\u00b7ger", "Ge\u00b7dan\u00b7ken", "Raub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Dem Weltgeheimnis in den finstern Schlund.", "tokens": ["Dem", "Welt\u00b7ge\u00b7heim\u00b7nis", "in", "den", "fins\u00b7tern", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Wo sind die Bl\u00fcten, die den Wald umschlangen,", "tokens": ["Wo", "sind", "die", "Bl\u00fc\u00b7ten", ",", "die", "den", "Wald", "um\u00b7schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Wo sind die V\u00f6gel, die hier lustig sangen?", "tokens": ["Wo", "sind", "die", "V\u00f6\u00b7gel", ",", "die", "hier", "lus\u00b7tig", "san\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Nun ist der Wald verlassen und verdorrt,", "tokens": ["Nun", "ist", "der", "Wald", "ver\u00b7las\u00b7sen", "und", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "L\u00e4ngst sind die Bl\u00fcten und die V\u00f6gel fort.", "tokens": ["L\u00e4ngst", "sind", "die", "Bl\u00fc\u00b7ten", "und", "die", "V\u00f6\u00b7gel", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "So sind vielleicht gar bald auch mir verbl\u00fcht", "tokens": ["So", "sind", "viel\u00b7leicht", "gar", "bald", "auch", "mir", "ver\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADV", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Die sch\u00f6nen Ahndungsblumen im Gem\u00fct;", "tokens": ["Die", "sch\u00f6\u00b7nen", "Ahn\u00b7dungs\u00b7blu\u00b7men", "im", "Ge\u00b7m\u00fct", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Und ist der Wuchs des Lebens mir verdorrt,", "tokens": ["Und", "ist", "der", "Wuchs", "des", "Le\u00b7bens", "mir", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Sind auch die V\u00f6gel, meine Lieder, fort;", "tokens": ["Sind", "auch", "die", "V\u00f6\u00b7gel", ",", "mei\u00b7ne", "Lie\u00b7der", ",", "fort", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Dann bin ich still und tot, wie dieser Baum,", "tokens": ["Dann", "bin", "ich", "still", "und", "tot", ",", "wie", "die\u00b7ser", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "PWAV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Der Seele Fr\u00fchling war, wie seiner \u2013 Traum.", "tokens": ["Der", "See\u00b7le", "Fr\u00fch\u00b7ling", "war", ",", "wie", "sei\u00b7ner", "\u2013", "Traum", "."], "token_info": ["word", "wor