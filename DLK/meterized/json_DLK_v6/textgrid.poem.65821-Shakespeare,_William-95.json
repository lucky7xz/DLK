{"textgrid.poem.65821": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "95", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieb und hold die Schmach gekleidet ist,", "tokens": ["Wie", "lieb", "und", "hold", "die", "Schmach", "ge\u00b7klei\u00b7det", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die, wie ein Wurm in duft'gen Hyazinthen,", "tokens": ["Die", ",", "wie", "ein", "Wurm", "in", "duft'\u00b7gen", "Hy\u00b7a\u00b7zin\u00b7then", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Knospensch\u00f6nheit deines Namens fri\u00dft!", "tokens": ["Die", "Knos\u00b7pen\u00b7sch\u00f6n\u00b7heit", "dei\u00b7nes", "Na\u00b7mens", "fri\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O, welchen Reiz umgibst du deinen S\u00fcnden!", "tokens": ["O", ",", "wel\u00b7chen", "Reiz", "um\u00b7gibst", "du", "dei\u00b7nen", "S\u00fcn\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Zunge, die von deinem Wandel spricht,", "tokens": ["Die", "Zun\u00b7ge", ",", "die", "von", "dei\u00b7nem", "Wan\u00b7del", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Leichtfertig deutend dein Vergn\u00fcgen tadelt;", "tokens": ["Leicht\u00b7fer\u00b7tig", "deu\u00b7tend", "dein", "Ver\u00b7gn\u00fc\u00b7gen", "ta\u00b7delt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Ihr Leumund selbst ist ohne Beifall nicht,", "tokens": ["Ihr", "Leu\u00b7mund", "selbst", "ist", "oh\u00b7ne", "Bei\u00b7fall", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "APPR", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Weil sie ", "tokens": ["Weil", "sie"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "O, welch ein Wohnhaus fiel den Fehlern zu,", "tokens": ["O", ",", "welch", "ein", "Wohn\u00b7haus", "fiel", "den", "Feh\u00b7lern", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die ihren Aufenthalt in dir sich w\u00e4hlten!", "tokens": ["Die", "ih\u00b7ren", "Auf\u00b7ent\u00b7halt", "in", "dir", "sich", "w\u00e4hl\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da schleiert Anmut jeden Makel zu:", "tokens": ["Da", "schlei\u00b7ert", "An\u00b7mut", "je\u00b7den", "Ma\u00b7kel", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Was Augen sehn, es mu\u00df f\u00fcr lieblich gelten.", "tokens": ["Was", "Au\u00b7gen", "sehn", ",", "es", "mu\u00df", "f\u00fcr", "lieb\u00b7lich", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "$,", "PPER", "VMFIN", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Dies weite Vorrecht f\u00fcrchte, teures Herz!", "tokens": ["Dies", "wei\u00b7te", "Vor\u00b7recht", "f\u00fcrch\u00b7te", ",", "teu\u00b7res", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Denn Mi\u00dfbrauch stumpft der sch\u00e4rfsten Messer Erz.", "tokens": ["Denn", "Mi\u00df\u00b7brauch", "stumpft", "der", "sch\u00e4rfs\u00b7ten", "Mes\u00b7ser", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}