{"textgrid.poem.55780": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn sich Deine starren Z\u00fcge", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich Deine starren Z\u00fcge", "tokens": ["Wenn", "sich", "Dei\u00b7ne", "star\u00b7ren", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zerren in der Seele Qual,", "tokens": ["Zer\u00b7ren", "in", "der", "See\u00b7le", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn Dir Deines Daseins L\u00fcge", "tokens": ["Wenn", "Dir", "Dei\u00b7nes", "Da\u00b7seins", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Aufersteht mit einem Mal,", "tokens": ["Auf\u00b7er\u00b7steht", "mit", "ei\u00b7nem", "Mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn sich Deine Glieder b\u00e4umen,", "tokens": ["Wenn", "sich", "Dei\u00b7ne", "Glie\u00b7der", "b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trotzig, in verbiss'ner Wut,", "tokens": ["Trot\u00b7zig", ",", "in", "ver\u00b7biss'\u00b7ner", "Wut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aufgeschreckt aus Wonnetr\u00e4umen,", "tokens": ["Auf\u00b7ge\u00b7schreckt", "aus", "Won\u00b7net\u00b7r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Knirscht und tobt der Nerven Brut,", "tokens": ["Knirscht", "und", "tobt", "der", "Ner\u00b7ven", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die Thr\u00e4nenschleier flie\u00dfen", "tokens": ["Wenn", "die", "Thr\u00e4\u00b7nen\u00b7schlei\u00b7er", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Deine dunkle Haut,", "tokens": ["\u00dc\u00b7ber", "Dei\u00b7ne", "dunk\u00b7le", "Haut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die wilden Worte schie\u00dfen", "tokens": ["Und", "die", "wil\u00b7den", "Wor\u00b7te", "schie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gellend, wie der Zinken Laut:", "tokens": ["Gel\u00b7lend", ",", "wie", "der", "Zin\u00b7ken", "Laut", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann durchrieseln Wonneschauer", "tokens": ["Dann", "durc\u00b7hrie\u00b7seln", "Won\u00b7ne\u00b7schau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Meinen Leib; \u2013 ein Dankgebet", "tokens": ["Mei\u00b7nen", "Leib", ";", "\u2013", "ein", "Dank\u00b7ge\u00b7bet"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "$(", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwillt zu Dir \u2013 aus deren Trauer", "tokens": ["Schwillt", "zu", "Dir", "\u2013", "aus", "de\u00b7ren", "Trau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "$(", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines Gl\u00fcckes Odem weht.", "tokens": ["Mei\u00b7nes", "Gl\u00fc\u00b7ckes", "O\u00b7dem", "weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Klafft und blutet, d\u00fcrre Wunden:", "tokens": ["Klafft", "und", "blu\u00b7tet", ",", "d\u00fcr\u00b7re", "Wun\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wonnen, die nur ich empfand", "tokens": ["Won\u00b7nen", ",", "die", "nur", "ich", "emp\u00b7fand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leben wieder \u2013 f\u00fcr Secunden", "tokens": ["Le\u00b7ben", "wie\u00b7der", "\u2013", "f\u00fcr", "Se\u00b7cun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "$(", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Seele auferstand.", "tokens": ["Mei\u00b7ne", "See\u00b7le", "auf\u00b7er\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}