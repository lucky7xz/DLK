{"textgrid.poem.24596": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Liebes Versicherungen bey den meisten Sch\u00f6nen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Iris/ die ", "tokens": ["Da\u00df", "I\u00b7ris", "/", "die"], "token_info": ["word", "word", "punct", "word"], "pos": ["KOUS", "NE", "$(", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.2": {"text": "Wenn man sie liebt/ und Liebens-w\u00fcrdig hei\u00dft/", "tokens": ["Wenn", "man", "sie", "liebt", "/", "und", "Lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "hei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$(", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das glaub ich sonder Zweiffel.", "tokens": ["Das", "glaub", "ich", "son\u00b7der", "Zweif\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch da\u00df die Lust bey ihr nicht heimlich ungemein/", "tokens": ["Doch", "da\u00df", "die", "Lust", "bey", "ihr", "nicht", "heim\u00b7lich", "un\u00b7ge\u00b7mein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPER", "PTKNEG", "ADJD", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So h\u00f6chst verdrie\u00dflich stets/ ja Lebens-lang zu seyn/", "tokens": ["So", "h\u00f6chst", "ver\u00b7drie\u00df\u00b7lich", "stets", "/", "ja", "Le\u00b7bens\u00b7lang", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "$(", "ADV", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das glaub ein dummer Teuffel.", "tokens": ["Das", "glaub", "ein", "dum\u00b7mer", "Teuf\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}