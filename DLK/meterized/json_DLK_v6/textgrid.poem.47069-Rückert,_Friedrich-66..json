{"textgrid.poem.47069": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie der Kiebitz, der unb\u00e4nd'ge Schreier,", "tokens": ["Gleich\u00b7wie", "der", "Kie\u00b7bitz", ",", "der", "un\u00b7b\u00e4n\u00b7d'\u00b7ge", "Schrei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um zu verh\u00fcten, da\u00df nicht seine L\u00e4ger", "tokens": ["Um", "zu", "ver\u00b7h\u00fc\u00b7ten", ",", "da\u00df", "nicht", "sei\u00b7ne", "L\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PTKZU", "VVINF", "$,", "KOUS", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch seine Schuld an den verschmitzten J\u00e4ger", "tokens": ["Durch", "sei\u00b7ne", "Schuld", "an", "den", "ver\u00b7schmitz\u00b7ten", "J\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Verraten werden, oder an den Geier,", "tokens": ["Ver\u00b7ra\u00b7ten", "wer\u00b7den", ",", "o\u00b7der", "an", "den", "Gei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von weitem, scheu um den geliebten Weiher,", "tokens": ["Von", "wei\u00b7tem", ",", "scheu", "um", "den", "ge\u00b7lieb\u00b7ten", "Wei\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo er sein Nest hat, streifend, zieht in schr\u00e4ger", "tokens": ["Wo", "er", "sein", "Nest", "hat", ",", "strei\u00b7fend", ",", "zieht", "in", "schr\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VAFIN", "$,", "VVPP", "$,", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umkreisung seinen Flug, bis ins Geheg' er", "tokens": ["Um\u00b7krei\u00b7sung", "sei\u00b7nen", "Flug", ",", "bis", "ins", "Ge\u00b7heg'", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "APPRART", "NN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich senkt auf seine Jungen oder Eier;", "tokens": ["Sich", "senkt", "auf", "sei\u00b7ne", "Jun\u00b7gen", "o\u00b7der", "Ei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So kreist, im Zauberwirbel hingezogen,", "tokens": ["So", "kreist", ",", "im", "Zau\u00b7ber\u00b7wir\u00b7bel", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Geist mit sehnsuchtm\u00fcden Fl\u00fcgelspitzen", "tokens": ["Mein", "Geist", "mit", "sehn\u00b7sucht\u00b7m\u00fc\u00b7den", "Fl\u00fc\u00b7gel\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ums Haus der Lieb' an stillen Wasserborden,", "tokens": ["Ums", "Haus", "der", "Lieb'", "an", "stil\u00b7len", "Was\u00b7ser\u00b7bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Stets n\u00e4her r\u00fcckend in stets engern Bogen,", "tokens": ["Stets", "n\u00e4\u00b7her", "r\u00fc\u00b7ckend", "in", "stets", "en\u00b7gern", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis unaufhaltsam er sich st\u00fcrzt gleich Blitzen", "tokens": ["Bis", "un\u00b7auf\u00b7halt\u00b7sam", "er", "sich", "st\u00fcrzt", "gleich", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "PPER", "PRF", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hei\u00df auf das Nest, das kaum erst kalt geworden.", "tokens": ["Hei\u00df", "auf", "das", "Nest", ",", "das", "kaum", "erst", "kalt", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}