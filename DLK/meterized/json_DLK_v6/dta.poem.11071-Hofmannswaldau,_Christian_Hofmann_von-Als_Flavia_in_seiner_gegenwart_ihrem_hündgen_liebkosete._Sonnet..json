{"dta.poem.11071": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als Flavia in seiner gegenwart ihrem  \n h\u00fcndgen liebkosete.  \n  Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hatte Flavia ihr h\u00fcndgen auf dem schos,", "tokens": ["Es", "hat\u00b7te", "Fla\u00b7via", "ihr", "h\u00fcnd\u00b7gen", "auf", "dem", "schos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "PPER", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie stopffet\u2019 ihm das maul mit lauter mandelkernen,", "tokens": ["Sie", "stopf\u00b7fet'", "ihm", "das", "maul", "mit", "lau\u00b7ter", "man\u00b7del\u00b7ker\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es fiel manch s\u00fcsser blick aus ihren holden sternen,", "tokens": ["Es", "fiel", "manch", "s\u00fcs\u00b7ser", "blick", "aus", "ih\u00b7ren", "hol\u00b7den", "ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den dieses lumpenthier, doch ohn verdienst, genos.", "tokens": ["Den", "die\u00b7ses", "lum\u00b7pen\u00b7thier", ",", "doch", "ohn", "ver\u00b7dienst", ",", "ge\u00b7nos", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PDAT", "NN", "$,", "ADV", "APPR", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie stellet ihm den schnee der reinen br\u00fcste blos,", "tokens": ["Sie", "stel\u00b7let", "ihm", "den", "schnee", "der", "rei\u00b7nen", "br\u00fcs\u00b7te", "blos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wolte nicht den mund von seinem ku\u00df entfernen,", "tokens": ["Und", "wol\u00b7te", "nicht", "den", "mund", "von", "sei\u00b7nem", "ku\u00df", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich muste den verdru\u00df dabey verbeissen lernen,", "tokens": ["Ich", "mus\u00b7te", "den", "ver\u00b7dru\u00df", "da\u00b7bey", "ver\u00b7beis\u00b7sen", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PAV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So starck mir auch die gall in mund und hertze flos.", "tokens": ["So", "starck", "mir", "auch", "die", "gall", "in", "mund", "und", "hert\u00b7ze", "flos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Indessen konnt\u2019 ich mich der w\u00f6rter nicht erwehren:", "tokens": ["In\u00b7des\u00b7sen", "konnt'", "ich", "mich", "der", "w\u00f6r\u00b7ter", "nicht", "er\u00b7weh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewie gl\u00fccklich w\u00fcrde sich doch meine zeit verzehren,", "tokens": ["\u201e", "wie", "gl\u00fcck\u00b7lich", "w\u00fcr\u00b7de", "sich", "doch", "mei\u00b7ne", "zeit", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "VAFIN", "PRF", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ew\u00e4r\u2019 ich, ach Flavia! dein so geliebter hund!", "tokens": ["\u201e", "w\u00e4r'", "ich", ",", "ach", "Fla\u00b7via", "!", "dein", "so", "ge\u00b7lieb\u00b7ter", "hund", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "$,", "XY", "NE", "$.", "PPOSAT", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "\u201edoch weil der himmel mich zum menschen auserkohren,", "tokens": ["\u201e", "doch", "weil", "der", "him\u00b7mel", "mich", "zum", "men\u00b7schen", "au\u00b7ser\u00b7koh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "ART", "NN", "PPER", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eso bin ich durch den grimm der Flavia verlohren,", "tokens": ["\u201e", "so", "bin", "ich", "durch", "den", "grimm", "der", "Fla\u00b7via", "ver\u00b7loh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "APPR", "ART", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "\u201edenn meine flamme lescht nur ihr gek\u00fcster mund.", "tokens": ["\u201e", "denn", "mei\u00b7ne", "flam\u00b7me", "lescht", "nur", "ihr", "ge\u00b7k\u00fcs\u00b7ter", "mund", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}