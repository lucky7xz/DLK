{"textgrid.poem.43248": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warum sie lieben? ... Gleicht sie denn nicht Allen?", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum sie lieben? ... Gleicht sie denn nicht Allen?", "tokens": ["Wa\u00b7rum", "sie", "lie\u00b7ben", "?", "...", "Gleicht", "sie", "denn", "nicht", "Al\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$.", "$(", "NN", "PPER", "ADV", "PTKNEG", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4r' sie auch besser, was gew\u00e4nn' ich dann?", "tokens": ["W\u00e4r'", "sie", "auch", "bes\u00b7ser", ",", "was", "ge\u00b7w\u00e4nn'", "ich", "dann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Gl\u00fcck, das meine Hand erfassen kann,", "tokens": ["Ein", "Gl\u00fcck", ",", "das", "mei\u00b7ne", "Hand", "er\u00b7fas\u00b7sen", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In meiner Hand zerbrechen kann, zerfallen.", "tokens": ["In", "mei\u00b7ner", "Hand", "zer\u00b7bre\u00b7chen", "kann", ",", "zer\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist vorbei ... doch wenn ich suchend dr\u00fccke", "tokens": ["Das", "ist", "vor\u00b7bei", "...", "doch", "wenn", "ich", "su\u00b7chend", "dr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "$(", "ADV", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die F\u00e4nge meines Geistes in ihr Hirn,", "tokens": ["Die", "F\u00e4n\u00b7ge", "mei\u00b7nes", "Geis\u00b7tes", "in", "ihr", "Hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "D\u00fcnkt mich, da\u00df hinter dieser hohen Stirn", "tokens": ["D\u00fcnkt", "mich", ",", "da\u00df", "hin\u00b7ter", "die\u00b7ser", "ho\u00b7hen", "Stirn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Etwas liegt, das einst gefehlt dem Gl\u00fccke.", "tokens": ["Ein", "Et\u00b7was", "liegt", ",", "das", "einst", "ge\u00b7fehlt", "dem", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich gr\u00fcble, denke, weil voll Uebermuth", "tokens": ["Ich", "gr\u00fcb\u00b7le", ",", "den\u00b7ke", ",", "weil", "voll", "Ue\u00b7ber\u00b7muth"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie mich in einer tollen Stunde rief.", "tokens": ["Sie", "mich", "in", "ei\u00b7ner", "tol\u00b7len", "Stun\u00b7de", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil sie nun selber sich vergarnt so tief,", "tokens": ["Weil", "sie", "nun", "sel\u00b7ber", "sich", "ver\u00b7garnt", "so", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PRF", "VVPP", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und zu mir spricht in schamvoll-scheuer Gluth?", "tokens": ["Und", "zu", "mir", "spricht", "in", "scham\u00b7voll\u00b7scheu\u00b7er", "Gluth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Weil sie verwirrt und ungeschickt mir schreibt,", "tokens": ["Weil", "sie", "ver\u00b7wirrt", "und", "un\u00b7ge\u00b7schickt", "mir", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und laut zu lachen sich vergeblich m\u00fcht,", "tokens": ["Und", "laut", "zu", "la\u00b7chen", "sich", "ver\u00b7geb\u00b7lich", "m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und weil ihr feines Angesicht verbl\u00fcht?", "tokens": ["Und", "weil", "ihr", "fei\u00b7nes", "An\u00b7ge\u00b7sicht", "ver\u00b7bl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ist nicht gut ... Ist das vorbei, was bleibt?", "tokens": ["Das", "ist", "nicht", "gut", "...", "Ist", "das", "vor\u00b7bei", ",", "was", "bleibt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "$(", "VAFIN", "PDS", "PTKVZ", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was bleibt am Weib Ertr\u00e4gliches uns noch,", "tokens": ["Was", "bleibt", "am", "Weib", "Er\u00b7tr\u00e4g\u00b7li\u00b7ches", "uns", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "ADJA", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn er verschwand, der sanfte Sch\u00f6nheitsglanz,", "tokens": ["Wenn", "er", "ver\u00b7schwand", ",", "der", "sanf\u00b7te", "Sch\u00f6n\u00b7heits\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcber allen Herzensfirlefanz", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7len", "Her\u00b7zens\u00b7fir\u00b7le\u00b7fanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der erste graue Herbstesnebel kroch.", "tokens": ["Der", "ers\u00b7te", "grau\u00b7e", "Herbs\u00b7tes\u00b7ne\u00b7bel", "kroch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}