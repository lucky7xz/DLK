{"textgrid.poem.64186": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Karl IX. nach der Bartholom\u00e4usnacht", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig Karl war leichenfahl:", "tokens": ["Der", "K\u00f6\u00b7nig", "Karl", "war", "lei\u00b7chen\u00b7fahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wankte durch den leeren Saal.", "tokens": ["Er", "wank\u00b7te", "durch", "den", "lee\u00b7ren", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbwie lang doch eine Novembernacht,", "tokens": ["\u00bb", "wie", "lang", "doch", "ei\u00b7ne", "No\u00b7vem\u00b7ber\u00b7nacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn man sie einsam still durchwacht!", "tokens": ["Wenn", "man", "sie", "ein\u00b7sam", "still", "durch\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie flog die gestrige vorbei", "tokens": ["Wie", "flog", "die", "ge\u00b7stri\u00b7ge", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Mit Schie\u00dfen und br\u00fcllendem Mordgeschrei! \u2013", "tokens": ["Mit", "Schie\u00b7\u00dfen", "und", "br\u00fcl\u00b7len\u00b7dem", "Mord\u00b7ge\u00b7schrei", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Ich kann nicht Menschen um mich haben:", "tokens": ["Ich", "kann", "nicht", "Men\u00b7schen", "um", "mich", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie riechen nach Blut wie Leichenraben. \u2013", "tokens": ["Sie", "rie\u00b7chen", "nach", "Blut", "wie", "Lei\u00b7chen\u00b7ra\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KOKOM", "NN", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Bei dem ersten Rapport, \u2013 wie dem schwarzen Tavannes", "tokens": ["Bei", "dem", "ers\u00b7ten", "Rap\u00b7port", ",", "\u2013", "wie", "dem", "schwar\u00b7zen", "Ta\u00b7van\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "$(", "KOKOM", "ART", "ADJA", "NN"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schon das Blut so rot aus dem Barte rann!", "tokens": ["Schon", "das", "Blut", "so", "rot", "aus", "dem", "Bar\u00b7te", "rann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und zu neuem Jagen lief er fort,", "tokens": ["Und", "zu", "neu\u00b7em", "Ja\u00b7gen", "lief", "er", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Seine gellende Losung: \u203aTod und Mord!\u2039", "tokens": ["Sei\u00b7ne", "gel\u00b7len\u00b7de", "Lo\u00b7sung", ":", "\u203a", "Tod", "und", "Mord", "!", "\u2039"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$(", "NN", "KON", "NN", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Und des jungen Guise zerkratztes Gesicht!", "tokens": ["Und", "des", "jun\u00b7gen", "Gui\u00b7se", "zer\u00b7kratz\u00b7tes", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er lachte: \u203aDas half der Ketzerin nicht!", "tokens": ["Er", "lach\u00b7te", ":", "\u203a", "Das", "half", "der", "Ket\u00b7ze\u00b7rin", "nicht", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PDS", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich hab' sie gezwungen und dann erschossen!\u2039", "tokens": ["Ich", "hab'", "sie", "ge\u00b7zwun\u00b7gen", "und", "dann", "er\u00b7schos\u00b7sen", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "KON", "ADV", "VVPP", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da\u00df er mir's erz\u00e4hlte, das hat mich verdrossen:", "tokens": ["Da\u00df", "er", "mir's", "er\u00b7z\u00e4hl\u00b7te", ",", "das", "hat", "mich", "ver\u00b7dros\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und wie in die Seine sprangen zwei Schwestern \u2013 \u2013:", "tokens": ["Und", "wie", "in", "die", "Sei\u00b7ne", "spran\u00b7gen", "zwei", "Schwes\u00b7tern", "\u2013", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PWAV", "APPR", "ART", "NE", "VVFIN", "CARD", "NN", "$(", "$(", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich kann sie nicht sehn, die Genossen von gestern.", "tokens": ["Ich", "kann", "sie", "nicht", "sehn", ",", "die", "Ge\u00b7nos\u00b7sen", "von", "ge\u00b7stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Wenn nur die Sekunde vor\u00fcber w\u00e4r',", "tokens": ["Wenn", "nur", "die", "Se\u00b7kun\u00b7de", "vor\u00b7\u00fc\u00b7ber", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da die Glocke des Louvre, dumpf und schwer,", "tokens": ["Da", "die", "Glo\u00b7cke", "des", "Louv\u00b7re", ",", "dumpf", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das Zeichen gab, wie wir's ausgemacht:", "tokens": ["Das", "Zei\u00b7chen", "gab", ",", "wie", "wir's", "aus\u00b7ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PIS", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das war ein Viertel vor Mitternacht:", "tokens": ["Das", "war", "ein", "Vier\u00b7tel", "vor", "Mit\u00b7ter\u00b7nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie rasch gleich drauf das Pistol gekracht! \u2013", "tokens": ["Wie", "rasch", "gleich", "drauf", "das", "Pis\u00b7tol", "ge\u00b7kracht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PAV", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "O Mutter, ich w\u00e4lz' es auf dein Gewissen!", "tokens": ["O", "Mut\u00b7ter", ",", "ich", "w\u00e4lz'", "es", "auf", "dein", "Ge\u00b7wis\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du hast an der z\u00f6gernden Hand mir gerissen!", "tokens": ["Du", "hast", "an", "der", "z\u00f6\u00b7gern\u00b7den", "Hand", "mir", "ge\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich wollte nicht dran! \u2013 Es ward mir bang: \u2013", "tokens": ["Ich", "woll\u00b7te", "nicht", "dran", "!", "\u2013", "Es", "ward", "mir", "bang", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PTKVZ", "$.", "$(", "PPER", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du schobst in die Faust mir den Glockenstrang", "tokens": ["Du", "schobst", "in", "die", "Faust", "mir", "den", "Glo\u00b7cken\u00b7strang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und zerrtest mich pl\u00f6tzlich ... \u2013", "tokens": ["Und", "zerr\u00b7test", "mich", "pl\u00f6tz\u00b7lich", "...", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$(", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Horch! Welch' ein Klang! \u2013", "tokens": ["Horch", "!", "Welch'", "ein", "Klang", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "PIAT", "ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Hui weh! Da schl\u00e4gt es Dreiviertel! \u2013 Weh! \u2013", "tokens": ["Hui", "weh", "!", "Da", "schl\u00e4gt", "es", "Drei\u00b7vier\u00b7tel", "!", "\u2013", "Weh", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["FM", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "NN", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Rings blutige Schatten, wohin ich seh'!", "tokens": ["Rings", "blu\u00b7ti\u00b7ge", "Schat\u00b7ten", ",", "wo\u00b7hin", "ich", "seh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Luft! Luft! Ich ersticke! Rings wirbeln Gespenster!", "tokens": ["Luft", "!", "Luft", "!", "Ich", "er\u00b7sti\u00b7cke", "!", "Rings", "wir\u00b7beln", "Ge\u00b7spens\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "VVFIN", "$.", "NN", "VVFIN", "NN", "$."], "meter": "++--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Rasch auf mit dem Laden! \u2013 Weh, das ist das Fenster:", "tokens": ["Rasch", "auf", "mit", "dem", "La\u00b7den", "!", "\u2013", "Weh", ",", "das", "ist", "das", "Fens\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "ART", "NN", "$.", "$(", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Hier scho\u00df ich heraus! Angoul\u00eame lud! \u2013", "tokens": ["Hier", "scho\u00df", "ich", "he\u00b7raus", "!", "An\u00b7goul\u00ea\u00b7me", "lud", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Was wirbelt herein wie Nebelflut?", "tokens": ["Was", "wir\u00b7belt", "her\u00b7ein", "wie", "Ne\u00b7bel\u00b7flut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKVZ", "KOKOM", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus dem Nebel schwillt eine wei\u00dfe Gestalt \u2013:", "tokens": ["Aus", "dem", "Ne\u00b7bel", "schwillt", "ei\u00b7ne", "wei\u00b7\u00dfe", "Ge\u00b7stalt", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$(", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ach, ich kenne dies Haupt mit dem klaffenden Spalt,", "tokens": ["Ach", ",", "ich", "ken\u00b7ne", "dies", "Haupt", "mit", "dem", "klaf\u00b7fen\u00b7den", "Spalt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PDS", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mit den rieselnden Wunden ohne Zahl \u2013", "tokens": ["Mit", "den", "rie\u00b7seln\u00b7den", "Wun\u00b7den", "oh\u00b7ne", "Zahl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mit dem silbernen Haar! \u2013 Ich nicht, Admiral!", "tokens": ["Mit", "dem", "sil\u00b7ber\u00b7nen", "Haar", "!", "\u2013", "Ich", "nicht", ",", "Ad\u00b7mi\u00b7ral", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$(", "PPER", "PTKNEG", "$,", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Der Guise war's und Paul Medici, \u2013", "tokens": ["Der", "Gui\u00b7se", "wa\u00b7r's", "und", "Paul", "Me\u00b7di\u00b7ci", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "KON", "NE", "NE", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ich war nicht darunter, Coligny!", "tokens": ["Ich", "war", "nicht", "da\u00b7run\u00b7ter", ",", "Co\u00b7lig\u00b7ny", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PAV", "$,", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Er greift mich! Zu Hilfe! Wachen, herbei!\u00ab \u2013 \u2013", "tokens": ["Er", "greift", "mich", "!", "Zu", "Hil\u00b7fe", "!", "Wa\u00b7chen", ",", "her\u00b7bei", "!", "\u00ab", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "APPR", "NN", "$.", "NN", "$,", "PTKVZ", "$.", "$(", "$(", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Durch das schweigende Louvre schrillt sein Schrei. \u2013 \u2013", "tokens": ["Durch", "das", "schwei\u00b7gen\u00b7de", "Louv\u00b7re", "schrillt", "sein", "Schrei", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Der K\u00f6nig hat nach dieser Nacht", "tokens": ["Der", "K\u00f6\u00b7nig", "hat", "nach", "die\u00b7ser", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht eine mehr allein verbracht:", "tokens": ["Nicht", "ei\u00b7ne", "mehr", "al\u00b7lein", "ver\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zumauern lie\u00df er das Erkerfen