{"textgrid.poem.42295": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Prolog zu Sprickmanns Eulalia", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Darf, Edle, die ihr hier versammelt seid,", "tokens": ["Darf", ",", "Ed\u00b7le", ",", "die", "ihr", "hier", "ver\u00b7sam\u00b7melt", "seid", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ADJA", "$,", "PRELS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Darf auch des Schauspiels Muse den Kristall,", "tokens": ["Darf", "auch", "des", "Schau\u00b7spiels", "Mu\u00b7se", "den", "Kris\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "+--+-+--++", "measure": "iambic.penta.invert"}, "line.3": {"text": "Worin sie alles, was vom Anbeginn", "tokens": ["Wo\u00b7rin", "sie", "al\u00b7les", ",", "was", "vom", "An\u00b7be\u00b7ginn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIS", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Erde unter Sonn, und Mond geschah,", "tokens": ["Der", "Er\u00b7de", "un\u00b7ter", "Sonn", ",", "und", "Mond", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Lebendig darstellt, darf die Muse wohl", "tokens": ["Le\u00b7ben\u00b7dig", "dar\u00b7stellt", ",", "darf", "die", "Mu\u00b7se", "wohl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "VMFIN", "ART", "NN", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Den Zauberspiegel, d\u00fcstrer Scenen voll,", "tokens": ["Den", "Zau\u00b7ber\u00b7spie\u00b7gel", ",", "d\u00fcst\u00b7rer", "Sce\u00b7nen", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Euch vor das Antlitz halten, da\u00df vor Schreck", "tokens": ["Euch", "vor", "das", "Ant\u00b7litz", "hal\u00b7ten", ",", "da\u00df", "vor", "Schreck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Knie' euch wanken, da\u00df von bitterm Schmerz", "tokens": ["Die", "Knie'", "euch", "wan\u00b7ken", ",", "da\u00df", "von", "bit\u00b7term", "Schmerz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Busen schwellen und von Thr\u00e4nen Euch", "tokens": ["Die", "Bu\u00b7sen", "schwel\u00b7len", "und", "von", "Thr\u00e4\u00b7nen", "Euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "APPR", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Augen \u00fcbergehn? \u2013 Erg\u00f6tztet ihr", "tokens": ["Die", "Au\u00b7gen", "\u00fc\u00b7ber\u00b7gehn", "?", "\u2013", "Er\u00b7g\u00f6tz\u00b7tet", "ihr"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "$(", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nicht lieber euch am l\u00e4cherlichen Tand", "tokens": ["Nicht", "lie\u00b7ber", "euch", "am", "l\u00e4\u00b7cher\u00b7li\u00b7chen", "Tand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Thorheit? Oder an dem heitern Gl\u00fcck,", "tokens": ["Der", "Thor\u00b7heit", "?", "O\u00b7der", "an", "dem", "hei\u00b7tern", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Womit am Schlu\u00df des drolligen Romans", "tokens": ["Wo\u00b7mit", "am", "Schlu\u00df", "des", "drol\u00b7li\u00b7gen", "Ro\u00b7mans"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die Lieb' ein leicht genecktes Paar belohnt? \u2013", "tokens": ["Die", "Lieb'", "ein", "leicht", "ge\u00b7neck\u00b7tes", "Paar", "be\u00b7lohnt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vielleicht! \u2013 Vielleicht behagt' es euch auch wohl,", "tokens": ["Viel\u00b7leicht", "!", "\u2013", "Viel\u00b7leicht", "be\u00b7hagt'", "es", "euch", "auch", "wohl", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein sch\u00f6nes, keusches, liebetreues Weib,", "tokens": ["Ein", "sch\u00f6\u00b7nes", ",", "keu\u00b7sches", ",", "lie\u00b7be\u00b7treu\u00b7es", "Weib", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umlagert von der schn\u00f6den Wollust Brut,", "tokens": ["Um\u00b7la\u00b7gert", "von", "der", "schn\u00f6\u00b7den", "Wol\u00b7lust", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In einen sauern Kampf verstrickt zu sehn.", "tokens": ["In", "ei\u00b7nen", "sau\u00b7ern", "Kampf", "ver\u00b7strickt", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr n\u00e4hmet teil an ihrer Angst und Not;", "tokens": ["Ihr", "n\u00e4h\u00b7met", "teil", "an", "ih\u00b7rer", "Angst", "und", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihr zittertet und weintet bald mit ihr;", "tokens": ["Ihr", "zit\u00b7ter\u00b7tet", "und", "wein\u00b7tet", "bald", "mit", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bald z\u00f6get ihr, mit rascherm Odemzug,", "tokens": ["Bald", "z\u00f6\u00b7get", "ihr", ",", "mit", "ra\u00b7scherm", "O\u00b7dem\u00b7zug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Mut zu \u00fcberwinden mit ihr ein.", "tokens": ["Den", "Mut", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", "mit", "ihr", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch m\u00fc\u00dft' auch dann am Ende Heil und Sieg", "tokens": ["Doch", "m\u00fc\u00dft'", "auch", "dann", "am", "En\u00b7de", "Heil", "und", "Sieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ADV", "APPRART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Brut zerschmettern, und den Kranz,", "tokens": ["Die", "Brut", "zer\u00b7schmet\u00b7tern", ",", "und", "den", "Kranz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Den sch\u00f6nen Kranz um ihre Scheitel ziehn,", "tokens": ["Den", "sch\u00f6\u00b7nen", "Kranz", "um", "ih\u00b7re", "Schei\u00b7tel", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Woran ihr Recht bew\u00e4hrte Tugend hat;", "tokens": ["Wo\u00b7ran", "ihr", "Recht", "be\u00b7w\u00e4hr\u00b7te", "Tu\u00b7gend", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Doch m\u00fc\u00dft' auch dann des Friedens sanfte Ruh", "tokens": ["Doch", "m\u00fc\u00dft'", "auch", "dann", "des", "Frie\u00b7dens", "sanf\u00b7te", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die Wunden heilen, die der Kampf ihr schlug;", "tokens": ["Die", "Wun\u00b7den", "hei\u00b7len", ",", "die", "der", "Kampf", "ihr", "schlug", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und nicht das arme, keusche, treue Weib", "tokens": ["Und", "nicht", "das", "ar\u00b7me", ",", "keu\u00b7sche", ",", "treu\u00b7e", "Weib"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ihr Heil, \u2013 o Gott, ihr eines letztes Heil! \u2013", "tokens": ["Ihr", "Heil", ",", "\u2013", "o", "Gott", ",", "ihr", "ei\u00b7nes", "letz\u00b7tes", "Heil", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "$(", "FM", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Gezwungen sein zu suchen \u2013 in der Gruft! \u2013", "tokens": ["Ge\u00b7zwun\u00b7gen", "sein", "zu", "su\u00b7chen", "\u2013", "in", "der", "Gruft", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAINF", "PTKZU", "VVINF", "$(", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl ist ein edles herrliches Gef\u00fchl,", "tokens": ["Wohl", "ist", "ein", "ed\u00b7les", "herr\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das solche W\u00fcnsch' in euern Herzen zeugt.", "tokens": ["Das", "sol\u00b7che", "W\u00fcn\u00b7sch'", "in", "eu\u00b7ern", "Her\u00b7zen", "zeugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Allein auf Erden k\u00e4mpft nicht immerdar", "tokens": ["Al\u00b7lein", "auf", "Er\u00b7den", "k\u00e4mpft", "nicht", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Tugend, wie der Edle w\u00fcnscht. Ach! oft", "tokens": ["Die", "Tu\u00b7gend", ",", "wie", "der", "Ed\u00b7le", "w\u00fcnscht", ".", "Ach", "!", "oft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "VVFIN", "$.", "ITJ", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist nichts Geringers, als das Leben selbst,", "tokens": ["Ist", "nichts", "Ge\u00b7rin\u00b7gers", ",", "als", "das", "Le\u00b7ben", "selbst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NN", "$,", "KOUS", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das L\u00f6segeld f\u00fcr den erhabnen Sieg.", "tokens": ["Das", "L\u00f6\u00b7se\u00b7geld", "f\u00fcr", "den", "er\u00b7hab\u00b7nen", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Lorbeerzweig, nach dem sie blutend rang,", "tokens": ["Der", "Lor\u00b7beer\u00b7zweig", ",", "nach", "dem", "sie", "blu\u00b7tend", "rang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Flicht sich zur Totenkron' auf ihren Sarg. \u2013", "tokens": ["Flicht", "sich", "zur", "To\u00b7ten\u00b7kron'", "auf", "ih\u00b7ren", "Sarg", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch dann auch mag's euch frommen, diesen Kampf,", "tokens": ["Doch", "dann", "auch", "mag's", "euch", "from\u00b7men", ",", "die\u00b7sen", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NE", "PPER", "ADJA", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den blutigen, den Todeskampf zu sehn;", "tokens": ["Den", "blu\u00b7ti\u00b7gen", ",", "den", "To\u00b7des\u00b7kampf", "zu", "sehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu sehen, wie von allen Seiten her", "tokens": ["Zu", "se\u00b7hen", ",", "wie", "von", "al\u00b7len", "Sei\u00b7ten", "her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "APPR", "PIAT", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die B\u00fcberei mit Netzen sie umstellt;", "tokens": ["Die", "B\u00fc\u00b7be\u00b7rei", "mit", "Net\u00b7zen", "sie", "um\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu sehn, wie nirgends eine Freistatt ihr,", "tokens": ["Zu", "sehn", ",", "wie", "nir\u00b7gends", "ei\u00b7ne", "Freis\u00b7tatt", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ADV", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als unter ihr das Grab nur, offen steht;", "tokens": ["Als", "un\u00b7ter", "ihr", "das", "Grab", "nur", ",", "of\u00b7fen", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "ADV", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und ach! zu sehn, wie sie hinunterst\u00fcrzt", "tokens": ["Und", "ach", "!", "zu", "sehn", ",", "wie", "sie", "hin\u00b7un\u00b7ter\u00b7st\u00fcrzt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$.", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und ihre Himmelsperle mit sich nimmt. \u2013", "tokens": ["Und", "ih\u00b7re", "Him\u00b7mel\u00b7sper\u00b7le", "mit", "sich", "nimmt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure":