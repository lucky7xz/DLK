{"textgrid.poem.60400": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An den Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch immer, Fr\u00fchling, bist du nicht", "tokens": ["Noch", "im\u00b7mer", ",", "Fr\u00fch\u00b7ling", ",", "bist", "du", "nicht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "VAFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gekommen in mein Tal,", "tokens": ["Ge\u00b7kom\u00b7men", "in", "mein", "Tal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ich dein liebes Angesicht", "tokens": ["Wo", "ich", "dein", "lie\u00b7bes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begr\u00fc\u00dft das letztemal.", "tokens": ["Be\u00b7gr\u00fc\u00dft", "das", "letz\u00b7te\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch stehn die B\u00e4ume d\u00fcrr und bar", "tokens": ["Noch", "stehn", "die", "B\u00e4u\u00b7me", "d\u00fcrr", "und", "bar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um deinen Weg herum", "tokens": ["Um", "dei\u00b7nen", "Weg", "he\u00b7rum"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "APZR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und strecken, eine Bettlerschar,", "tokens": ["Und", "stre\u00b7cken", ",", "ei\u00b7ne", "Bett\u00b7ler\u00b7schar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dir die Arme stumm.", "tokens": ["Nach", "dir", "die", "Ar\u00b7me", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Fr\u00fchblumen w\u00e4hnten dich schon hier,", "tokens": ["Fr\u00fch\u00b7blu\u00b7men", "w\u00e4hn\u00b7ten", "dich", "schon", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frost bringt sie um ihr Gl\u00fcck,", "tokens": ["Frost", "bringt", "sie", "um", "ihr", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sehnten sich heraus nach dir", "tokens": ["Sie", "sehn\u00b7ten", "sich", "he\u00b7raus", "nach", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00f6nnen nicht zur\u00fcck.", "tokens": ["Und", "k\u00f6n\u00b7nen", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Schwalbe fliegt best\u00fcrzt umher", "tokens": ["Die", "Schwal\u00b7be", "fliegt", "be\u00b7st\u00fcrzt", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ruft nach dir voll Gram,", "tokens": ["Und", "ruft", "nach", "dir", "voll", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bereut schon, da\u00df sie \u00fcbers Meer", "tokens": ["Be\u00b7reut", "schon", ",", "da\u00df", "sie", "\u00fc\u00b7bers", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu fr\u00fch her\u00fcberkam.", "tokens": ["Zu", "fr\u00fch", "her\u00b7\u00fc\u00b7ber\u00b7kam", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}