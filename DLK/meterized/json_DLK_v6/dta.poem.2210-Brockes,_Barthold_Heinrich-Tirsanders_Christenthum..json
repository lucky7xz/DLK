{"dta.poem.2210": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Tirsanders Christenthum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer will, da\u00df alles, wie er will und w\u00fcnscht, soll", "tokens": ["Wer", "will", ",", "da\u00df", "al\u00b7les", ",", "wie", "er", "will", "und", "w\u00fcnscht", ",", "soll"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VMFIN", "$,", "KOUS", "PIS", "$,", "PWAV", "PPER", "VMFIN", "KON", "VVFIN", "$,", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf der Welt geschehen,", "tokens": ["auf", "der", "Welt", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wer will, da\u00df es ihm \u00fcberall, wie ers verlanget, soll", "tokens": ["Wer", "will", ",", "da\u00df", "es", "ihm", "\u00fc\u00b7be\u00b7rall", ",", "wie", "ers", "ver\u00b7lan\u00b7get", ",", "soll"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VMFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$,", "VMFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "ergehen,", "tokens": ["er\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Der will, indem er dieses will, zugleich der Erden Ze-", "tokens": ["Der", "will", ",", "in\u00b7dem", "er", "die\u00b7ses", "will", ",", "zu\u00b7gleich", "der", "Er\u00b7den", "Ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "$,", "KOUS", "PPER", "PDS", "VMFIN", "$,", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "pter f\u00fchren,", "tokens": ["pter", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Es scheint, er wolle nicht, da\u00df Gott regieren soll, er", "tokens": ["Es", "scheint", ",", "er", "wol\u00b7le", "nicht", ",", "da\u00df", "Gott", "re\u00b7gie\u00b7ren", "soll", ",", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "$,", "KOUS", "NN", "VVINF", "VMFIN", "$,", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "will regieren:", "tokens": ["will", "re\u00b7gie\u00b7ren", ":"], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Er denket nicht auf Gottes Ordnung, noch den Zusam-", "tokens": ["Er", "den\u00b7ket", "nicht", "auf", "Got\u00b7tes", "Ord\u00b7nung", ",", "noch", "den", "Zu\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "NN", "$,", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "menhang der Dinge,", "tokens": ["men\u00b7hang", "der", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Er denkt auf sich, und w\u00fcnscht, das alles um ihn, als", "tokens": ["Er", "denkt", "auf", "sich", ",", "und", "w\u00fcnscht", ",", "das", "al\u00b7les", "um", "ihn", ",", "als"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "$,", "KON", "VVFIN", "$,", "PRELS", "PIS", "APPR", "PPER", "$,", "KOUS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "um ein Centrum, gienge.", "tokens": ["um", "ein", "Cen\u00b7trum", ",", "gien\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}