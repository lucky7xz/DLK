{"textgrid.poem.48866": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "91. An ihren Garten", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich denke noch an sie, die tausent lieben Stunden", "tokens": ["Ich", "den\u00b7ke", "noch", "an", "sie", ",", "die", "tau\u00b7sent", "lie\u00b7ben", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "PRELS", "CARD", "ADJA", "NN"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "und tausent noch darzu, die ich in deiner Scho\u00df,", "tokens": ["und", "tau\u00b7sent", "noch", "dar\u00b7zu", ",", "die", "ich", "in", "dei\u00b7ner", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "du wolgelegner Platz, mehr nutzbar als zu gro\u00df,", "tokens": ["du", "wol\u00b7ge\u00b7leg\u00b7ner", "Platz", ",", "mehr", "nutz\u00b7bar", "als", "zu", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "ADJD", "KOKOM", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "an keinerlei Frucht arm, zu jener Zeit empfunden,", "tokens": ["an", "kei\u00b7ner\u00b7lei", "Frucht", "arm", ",", "zu", "je\u00b7ner", "Zeit", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "wenn, wie ", "tokens": ["wenn", ",", "wie"], "token_info": ["word", "punct", "word"], "pos": ["KOUS", "$,", "PWAV"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "so ich sie wiederum in die zwei Arme schlo\u00df.", "tokens": ["so", "ich", "sie", "wie\u00b7de\u00b7rum", "in", "die", "zwei", "Ar\u00b7me", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "ADV", "APPR", "ART", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie selig war ich da, wie aller Menschheit los,", "tokens": ["Wie", "se\u00b7lig", "war", "ich", "da", ",", "wie", "al\u00b7ler", "Menschheit", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$,", "PWAV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wie ofte hat uns doch Priapus so gefunden!", "tokens": ["wie", "of\u00b7te", "hat", "uns", "doch", "Pria\u00b7pus", "so", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Es steht ein Maulbeerbaum bald bei dem Vorderteiche,", "tokens": ["Es", "steht", "ein", "Maul\u00b7be\u00b7er\u00b7baum", "bald", "bei", "dem", "Vor\u00b7der\u00b7tei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "dem w\u00fcndsch ich, da\u00df sein Haar ihm nimmermehr verbleiche,", "tokens": ["dem", "w\u00fcndsch", "ich", ",", "da\u00df", "sein", "Haar", "ihm", "nim\u00b7mer\u00b7mehr", "ver\u00b7blei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "entgehe nie sein Saft. Denn die vermeinte Nacht,", "tokens": ["ent\u00b7ge\u00b7he", "nie", "sein", "Saft", ".", "Denn", "die", "ver\u00b7mein\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$.", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "die er dem Stamme gibt mit seinen dicken Bl\u00e4ttern,", "tokens": ["die", "er", "dem", "Stam\u00b7me", "gibt", "mit", "sei\u00b7nen", "di\u00b7cken", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die wei\u00df es, wie sie mich zuerst hat angelacht.", "tokens": ["die", "wei\u00df", "es", ",", "wie", "sie", "mich", "zu\u00b7erst", "hat", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr diese Gunst sei du befohlen allen G\u00f6ttern!", "tokens": ["F\u00fcr", "die\u00b7se", "Gunst", "sei", "du", "be\u00b7foh\u00b7len", "al\u00b7len", "G\u00f6t\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}