{"dta.poem.7120": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In uns erhebt sich aus dem Magen", "tokens": ["In", "uns", "er\u00b7hebt", "sich", "aus", "dem", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein feuchter Dufft umnebelt das Gehirn,", "tokens": ["Ein", "feuch\u00b7ter", "Dufft", "um\u00b7ne\u00b7belt", "das", "Ge\u00b7hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bet\u00e4ubt die rege Krafft, erf\u00fcllet unsre Stirn,", "tokens": ["Be\u00b7t\u00e4ubt", "die", "re\u00b7ge", "Krafft", ",", "er\u00b7f\u00fcl\u00b7let", "uns\u00b7re", "Stirn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sinckt allgemach gantz unvermerckt hernieder,", "tokens": ["Sinckt", "all\u00b7ge\u00b7mach", "gantz", "un\u00b7ver\u00b7merckt", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dr\u00fcckt mit sanffter Last der Augen schlaffe Lieder,", "tokens": ["Und", "dr\u00fcckt", "mit", "sanff\u00b7ter", "Last", "der", "Au\u00b7gen", "schlaf\u00b7fe", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Leibes Fenster-Schl\u00e4ge, zu.", "tokens": ["Des", "Lei\u00b7bes", "Fens\u00b7ter\u00b7Schl\u00e4\u00b7ge", ",", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann schleicht der Schlaf herein,", "tokens": ["Dann", "schleicht", "der", "Schlaf", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und bringet nicht das Aug\u2019 und Ohr allein;", "tokens": ["Und", "brin\u00b7get", "nicht", "das", "Aug'", "und", "Ohr", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den gantzen Leib, zur Ruh\u2019.", "tokens": ["Den", "gant\u00b7zen", "Leib", ",", "zur", "Ruh'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Es schlafen Hand und Fu\u00df, es schl\u00e4fet Arm und Bein,", "tokens": ["Es", "schla\u00b7fen", "Hand", "und", "Fu\u00df", ",", "es", "schl\u00e4\u00b7fet", "Arm", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es schl\u00e4ft der gantze Mensch, jedoch das Hertze ruht", "tokens": ["Es", "schl\u00e4ft", "der", "gant\u00b7ze", "Mensch", ",", "je\u00b7doch", "das", "Hert\u00b7ze", "ruht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "PDS", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und schl\u00e4ft zu keiner Zeit: es treibet stets das Blut,", "tokens": ["Und", "schl\u00e4ft", "zu", "kei\u00b7ner", "Zeit", ":", "es", "trei\u00b7bet", "stets", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jedoch so hefftig nicht: es gehen dennoch richtig", "tokens": ["Je\u00b7doch", "so", "heff\u00b7tig", "nicht", ":", "es", "ge\u00b7hen", "den\u00b7noch", "rich\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die innern R\u00e4derchen. Dies Werckzeug bleibet t\u00fcchtig", "tokens": ["Die", "in\u00b7nern", "R\u00e4\u00b7der\u00b7chen", ".", "Dies", "Wer\u00b7ck\u00b7zeug", "blei\u00b7bet", "t\u00fcch\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PDS", "NN", "VVFIN", "ADJD"], "meter": "-+-+--++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Zu allen Handlungen, wie es vorhero war,", "tokens": ["Zu", "al\u00b7len", "Hand\u00b7lun\u00b7gen", ",", "wie", "es", "vor\u00b7he\u00b7ro", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-++-+-+--+", "measure": "iambic.hexa.chol"}, "line.16": {"text": "So gar,", "tokens": ["So", "gar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Da\u00df, um die Nahrungs-S\u00e4ffte", "tokens": ["Da\u00df", ",", "um", "die", "Nah\u00b7rungs\u00b7S\u00e4ff\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUI", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Wol abzusondern, zu vereinen,", "tokens": ["Wol", "ab\u00b7zu\u00b7son\u00b7dern", ",", "zu", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Auch zum Verdauen, ihre Kr\u00e4ffte", "tokens": ["Auch", "zum", "Ver\u00b7dau\u00b7en", ",", "ih\u00b7re", "Kr\u00e4ff\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Sich dann noch zu vermehren scheinen.", "tokens": ["Sich", "dann", "noch", "zu", "ver\u00b7meh\u00b7ren", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}