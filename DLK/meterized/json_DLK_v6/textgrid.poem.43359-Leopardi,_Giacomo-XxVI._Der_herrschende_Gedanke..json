asure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Welch eine Welt, welch neue", "tokens": ["Welch", "ei\u00b7ne", "Welt", ",", "welch", "neu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "PWAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Unendlichkeit, o welch ein Paradies", "tokens": ["Un\u00b7end\u00b7lich\u00b7keit", ",", "o", "welch", "ein", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "PWAT", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erschlie\u00dft mir oft dein allgewalt'ger Zauber", "tokens": ["Er\u00b7schlie\u00dft", "mir", "oft", "dein", "all\u00b7ge\u00b7walt'\u00b7ger", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In hohem Flug! Mir d\u00e4ucht", "tokens": ["In", "ho\u00b7hem", "Flug", "!", "Mir", "d\u00e4ucht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu wandeln unter einer neuen Sonne,", "tokens": ["Zu", "wan\u00b7deln", "un\u00b7ter", "ei\u00b7ner", "neu\u00b7en", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo all mein irdisch F\u00fchlen,", "tokens": ["Wo", "all", "mein", "ir\u00b7disch", "F\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und was ich Wahrheit nannte, von mir weicht.", "tokens": ["Und", "was", "ich", "Wahr\u00b7heit", "nann\u00b7te", ",", "von", "mir", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So m\u00fcssen G\u00f6tter tr\u00e4umen,", "tokens": ["So", "m\u00fcs\u00b7sen", "G\u00f6t\u00b7ter", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sag' ich mir dann. Ach, bist du doch f\u00fcrwahr,", "tokens": ["Sag'", "ich", "mir", "dann", ".", "Ach", ",", "bist", "du", "doch", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "$.", "ITJ", "$,", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Holder Gedank', ein Traum, der oft uns mild", "tokens": ["Hol\u00b7der", "Ge\u00b7dank'", ",", "ein", "Traum", ",", "der", "oft", "uns", "mild"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Versch\u00f6nt der Wahrheit Bild,", "tokens": ["Ver\u00b7sch\u00f6nt", "der", "Wahr\u00b7heit", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ein offenbarer Wahn; und doch vor allen", "tokens": ["Ein", "of\u00b7fen\u00b7ba\u00b7rer", "Wahn", ";", "und", "doch", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "ADV", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Holdsel'gen Wahngebilden", "tokens": ["Hold\u00b7sel'\u00b7gen", "Wahn\u00b7ge\u00b7bil\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Bist g\u00f6ttlich du, von solcher Lebensmacht,", "tokens": ["Bist", "g\u00f6tt\u00b7lich", "du", ",", "von", "sol\u00b7cher", "Le\u00b7bens\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Da\u00df du bestehst, wenn alle Masken fallen,", "tokens": ["Da\u00df", "du", "be\u00b7stehst", ",", "wenn", "al\u00b7le", "Mas\u00b7ken", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Oft wesenhaft erscheinest", "tokens": ["Oft", "we\u00b7sen\u00b7haft", "er\u00b7schei\u00b7nest"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und erst entschwindest in des Todes Nacht.", "tokens": ["Und", "erst", "ent\u00b7schwin\u00b7dest", "in", "des", "To\u00b7des", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Gewi\u00df, du mein Gedanke, der du einzig", "tokens": ["Ge\u00b7wi\u00df", ",", "du", "mein", "Ge\u00b7dan\u00b7ke", ",", "der", "du", "ein\u00b7zig"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PT