{"textgrid.poem.43359": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XxVI. Der herrschende Gedanke.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du holdester von allen", "tokens": ["Du", "hol\u00b7des\u00b7ter", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "PIAT"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Gewaltherrn, der mein Herz lenkt nach Gefallen,", "tokens": ["Ge\u00b7walt\u00b7herrn", ",", "der", "mein", "Herz", "lenkt", "nach", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Furchtbar Geschenk des Himmels,", "tokens": ["Furcht\u00b7bar", "Ge\u00b7schenk", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und doch mir ewig theuer,", "tokens": ["Und", "doch", "mir", "e\u00b7wig", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein treuster Freund im Leide,", "tokens": ["Mein", "treus\u00b7ter", "Freund", "im", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gedanke, dran ich f\u00fcr und f\u00fcr mich weide:", "tokens": ["Ge\u00b7dan\u00b7ke", ",", "dran", "ich", "f\u00fcr", "und", "f\u00fcr", "mich", "wei\u00b7de", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "PPER", "APPR", "KON", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer spricht von deines Wesens", "tokens": ["Wer", "spricht", "von", "dei\u00b7nes", "We\u00b7sens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geheimni\u00df nicht? Wer ward nicht schon bezwungen", "tokens": ["Ge\u00b7heim\u00b7ni\u00df", "nicht", "?", "Wer", "ward", "nicht", "schon", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$.", "PWS", "VAFIN", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von deiner Macht? Doch immer,", "tokens": ["Von", "dei\u00b7ner", "Macht", "?", "Doch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So oft von Menschenzungen", "tokens": ["So", "oft", "von", "Men\u00b7schen\u00b7zun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erklingt des eignen F\u00fchlens Lust und Qual,", "tokens": ["Er\u00b7klingt", "des", "eig\u00b7nen", "F\u00fch\u00b7lens", "Lust", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Scheint neu das Wort, als kl\u00e4ng's zum ersten Mal.", "tokens": ["Scheint", "neu", "das", "Wort", ",", "als", "kl\u00e4ng's", "zum", "ers\u00b7ten", "Mal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "KOUS", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie ist doch meine Seele", "tokens": ["Wie", "ist", "doch", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vereinsamt seit den Tagen,", "tokens": ["Ver\u00b7ein\u00b7samt", "seit", "den", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo du darin die Wohnung aufgeschlagen!", "tokens": ["Wo", "du", "da\u00b7rin", "die", "Woh\u00b7nung", "auf\u00b7ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Blitzesschnelle f\u00fchlt' ich mir im Nu", "tokens": ["Mit", "Blit\u00b7zes\u00b7schnel\u00b7le", "f\u00fchlt'", "ich", "mir", "im", "Nu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Entschwinden die Gedanken,", "tokens": ["Ent\u00b7schwin\u00b7den", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die andern allzumal. In \u00f6dem Felde", "tokens": ["Die", "an\u00b7dern", "all\u00b7zu\u00b7mal", ".", "In", "\u00f6\u00b7dem", "Fel\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Thurm, so ragtest du", "tokens": ["Ein", "Thurm", ",", "so", "rag\u00b7test", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Gigantisch einsam in des Busens Schranken.", "tokens": ["Gi\u00b7gan\u00b7tisch", "ein\u00b7sam", "in", "des", "Bu\u00b7sens", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was galt hinfort mir, au\u00dfer dir allein,", "tokens": ["Was", "galt", "hin\u00b7fort", "mir", ",", "au\u00b7\u00dfer", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPER", "$,", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dies ganze arme Leben,", "tokens": ["Dies", "gan\u00b7ze", "ar\u00b7me", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was aller irdische Tand in meinen Augen?", "tokens": ["Was", "al\u00b7ler", "ir\u00b7di\u00b7sche", "Tand", "in", "mei\u00b7nen", "Au\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Welch schales Zeitvergeuden", "tokens": ["Welch", "scha\u00b7les", "Zeit\u00b7ver\u00b7geu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schien all dies Thun und Treiben!", "tokens": ["Schien", "all", "dies", "Thun", "und", "Trei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PDS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach, nur um eitle Lust ein eitles M\u00fchen,", "tokens": ["Ach", ",", "nur", "um", "eit\u00b7le", "Lust", "ein", "eit\u00b7les", "M\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verglichen mit den Freuden,", "tokens": ["Ver\u00b7gli\u00b7chen", "mit", "den", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den himmlischen, die mir durch dich erbl\u00fchen.", "tokens": ["Den", "himm\u00b7li\u00b7schen", ",", "die", "mir", "durch", "dich", "er\u00b7bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie von des Apennin", "tokens": ["Wie", "von", "des", "A\u00b7pen\u00b7nin"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NE"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Unwirthlich nackten W\u00e4nden", "tokens": ["Un\u00b7wirth\u00b7lich", "nack\u00b7ten", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zur gr\u00fcnen Flur, die fern her\u00fcberlacht,", "tokens": ["Zur", "gr\u00fc\u00b7nen", "Flur", ",", "die", "fern", "her\u00b7\u00fc\u00b7berl\u00b7acht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Sehnsucht sich des Wandrers Blicke wenden,", "tokens": ["In", "Sehn\u00b7sucht", "sich", "des", "Wand\u00b7rers", "Bli\u00b7cke", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So von dem unfruchtbaren", "tokens": ["So", "von", "dem", "un\u00b7frucht\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und rauhen Weltverkehr \u2013 wie streb' ich gerne,", "tokens": ["Und", "rau\u00b7hen", "Welt\u00b7ver\u00b7kehr", "\u2013", "wie", "streb'", "ich", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "PWAV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als in ein Paradies, zu dir zur\u00fccke,", "tokens": ["Als", "in", "ein", "Pa\u00b7ra\u00b7dies", ",", "zu", "dir", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df deine N\u00e4he jeden Sinn erquicke!", "tokens": ["Da\u00df", "dei\u00b7ne", "N\u00e4\u00b7he", "je\u00b7den", "Sinn", "er\u00b7qui\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich kann es kaum verstehen,", "tokens": ["Ich", "kann", "es", "kaum", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ich so lang dies Leben, diese Welt", "tokens": ["Wie", "ich", "so", "lang", "dies", "Le\u00b7ben", ",", "die\u00b7se", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "PDS", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Voll Unverstand und Plagen", "tokens": ["Voll", "Un\u00b7ver\u00b7stand", "und", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hab' ohne dich ertragen;", "tokens": ["Hab'", "oh\u00b7ne", "dich", "er\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Begreifen kann ich's kaum,", "tokens": ["Be\u00b7grei\u00b7fen", "kann", "ich's", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie sich an andern Freuden,", "tokens": ["Wie", "sich", "an", "an\u00b7dern", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als du gew\u00e4hrst, sich Andre m\u00f6gen weiden.", "tokens": ["Als", "du", "ge\u00b7w\u00e4hrst", ",", "sich", "And\u00b7re", "m\u00f6\u00b7gen", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PRF", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nie bis zu jener Zeit,", "tokens": ["Nie", "bis", "zu", "je\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo ich zuerst, was Leben hei\u00dft, erfahren,", "tokens": ["Wo", "ich", "zu\u00b7erst", ",", "was", "Le\u00b7ben", "hei\u00dft", ",", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$,", "PWS", "NN", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat Todesfurcht die Seele mir bewegt.", "tokens": ["Hat", "To\u00b7des\u00b7furcht", "die", "See\u00b7le", "mir", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Heut d\u00fcnkt mich nur ein Spiel,", "tokens": ["Heut", "d\u00fcnkt", "mich", "nur", "ein", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was Thoren Angst erregt,", "tokens": ["Was", "Tho\u00b7ren", "Angst", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ob sie es preisen auch mit Heuchelmunde:", "tokens": ["Ob", "sie", "es", "prei\u00b7sen", "auch", "mit", "Heu\u00b7chel\u00b7mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das Mu\u00df der letzten Stunde,", "tokens": ["Das", "Mu\u00df", "der", "letz\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und zeigt Gefahr sich, kann ich ohne Grauen", "tokens": ["Und", "zeigt", "Ge\u00b7fahr", "sich", ",", "kann", "ich", "oh\u00b7ne", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "PRF", "$,", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mit L\u00e4cheln in ihr dr\u00e4uend Antlitz schauen.", "tokens": ["Mit", "L\u00e4\u00b7cheln", "in", "ihr", "dr\u00e4u\u00b7end", "Ant\u00b7litz", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Verachtet hab' ich immer", "tokens": ["Ver\u00b7ach\u00b7tet", "hab'", "ich", "im\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die feigen, ungro\u00dfm\u00fcth'gen,", "tokens": ["Die", "fei\u00b7gen", ",", "un\u00b7gro\u00df\u00b7m\u00fcth'\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Verworfnen Seelen; jetzt emp\u00f6rt sofort", "tokens": ["Ver\u00b7worf\u00b7nen", "See\u00b7len", ";", "jetzt", "em\u00b7p\u00f6rt", "so\u00b7fort"], "token_info": [