{"textgrid.poem.36868": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Der Weg des Frommen", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Gottes Wege geht, nur der hat gro\u00dfen Frieden,", "tokens": ["Wer", "Got\u00b7tes", "We\u00b7ge", "geht", ",", "nur", "der", "hat", "gro\u00b7\u00dfen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "VVFIN", "$,", "ADV", "ART", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er widersteht der b\u00f6sen Lust;", "tokens": ["Er", "wi\u00b7der\u00b7steht", "der", "b\u00f6\u00b7sen", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er k\u00e4mpft, und ist des Lohns, den Gott dem Kampf beschieden,", "tokens": ["Er", "k\u00e4mpft", ",", "und", "ist", "des", "Lohns", ",", "den", "Gott", "dem", "Kampf", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist seiner Tugend sich bewu\u00dft.", "tokens": ["Ist", "sei\u00b7ner", "Tu\u00b7gend", "sich", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er merkt auf seinen Gang, geht ihn mit heilgem Mute,", "tokens": ["Er", "merkt", "auf", "sei\u00b7nen", "Gang", ",", "geht", "ihn", "mit", "heil\u00b7gem", "Mu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4chst an Erkenntnis und an Kraft,", "tokens": ["W\u00e4chst", "an", "Er\u00b7kennt\u00b7nis", "und", "an", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird aus der Schwachheit stark, und liebt und schmeckt das Gute,", "tokens": ["Wird", "aus", "der", "Schwach\u00b7heit", "stark", ",", "und", "liebt", "und", "schmeckt", "das", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "$,", "KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Gott in seiner Seele schafft.", "tokens": ["Das", "Gott", "in", "sei\u00b7ner", "See\u00b7le", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihn hat er allezeit vor Augen und im Herzen,", "tokens": ["Ihn", "hat", "er", "al\u00b7le\u00b7zeit", "vor", "Au\u00b7gen", "und", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Pr\u00fcft t\u00e4glich sich vor seinem Thron,", "tokens": ["Pr\u00fcft", "t\u00e4g\u00b7lich", "sich", "vor", "sei\u00b7nem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bereut der Fehler Zahl, und tilgt der S\u00fcnden Schmerzen", "tokens": ["Be\u00b7reut", "der", "Feh\u00b7ler", "Zahl", ",", "und", "tilgt", "der", "S\u00fcn\u00b7den", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "$,", "KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Jesum Christum, seinen Sohn.", "tokens": ["Durch", "Je\u00b7sum", "Chris\u00b7tum", ",", "sei\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Getreu in seinem Stand, genie\u00dft er Gottes Gaben,", "tokens": ["Ge\u00b7treu", "in", "sei\u00b7nem", "Stand", ",", "ge\u00b7nie\u00dft", "er", "Got\u00b7tes", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wehrt seiner Seele Geiz und Neid,", "tokens": ["Wehrt", "sei\u00b7ner", "See\u00b7le", "Geiz", "und", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist, wenn andre gleich viel Weins und Kornes haben,", "tokens": ["Und", "ist", ",", "wenn", "and\u00b7re", "gleich", "viel", "Weins", "und", "Kor\u00b7nes", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PIS", "ADV", "PIAT", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Gott bei wenigem erfreut.", "tokens": ["In", "Gott", "bei", "we\u00b7ni\u00b7gem", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schenkt seine Hand ihm viel: so wird er vielen n\u00fctzen,", "tokens": ["Schenkt", "sei\u00b7ne", "Hand", "ihm", "viel", ":", "so", "wird", "er", "vie\u00b7len", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$.", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, wie sein Gott, gutt\u00e4tig sein;", "tokens": ["Und", ",", "wie", "sein", "Gott", ",", "gut\u00b7t\u00e4\u00b7tig", "sein", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPOSAT", "NN", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Freundes Gl\u00fcck erh\u00f6hn, verla\u00dfne Tugend sch\u00fctzen,", "tokens": ["Des", "Freun\u00b7des", "Gl\u00fcck", "er\u00b7h\u00f6hn", ",", "ver\u00b7la\u00df\u00b7ne", "Tu\u00b7gend", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und selbst den Feind in Not erfreun.", "tokens": ["Und", "selbst", "den", "Feind", "in", "Not", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihm ist es leichte Last, die Pflichten auszu\u00fcben,", "tokens": ["Ihm", "ist", "es", "leich\u00b7te", "Last", ",", "die", "Pflich\u00b7ten", "aus\u00b7zu\u00b7\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die er dem N\u00e4chsten schuldig ist;", "tokens": ["Die", "er", "dem", "N\u00e4chs\u00b7ten", "schul\u00b7dig", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe gegen Gott hei\u00dft ihn die Menschen lieben;", "tokens": ["Die", "Lie\u00b7be", "ge\u00b7gen", "Gott", "hei\u00dft", "ihn", "die", "Men\u00b7schen", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und durch die Liebe siegt der Christ.", "tokens": ["Und", "durch", "die", "Lie\u00b7be", "siegt", "der", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er kr\u00e4nket nie dein Gl\u00fcck, sch\u00fctzt deinen Ruhm, dein Leben;", "tokens": ["Er", "kr\u00e4n\u00b7ket", "nie", "dein", "Gl\u00fcck", ",", "sch\u00fctzt", "dei\u00b7nen", "Ruhm", ",", "dein", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn er ehrt Gottes Bild in dir.", "tokens": ["Denn", "er", "ehrt", "Got\u00b7tes", "Bild", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er tr\u00e4gt dich mit Geduld, ist willig zum Vergeben;", "tokens": ["Er", "tr\u00e4gt", "dich", "mit", "Ge\u00b7duld", ",", "ist", "wil\u00b7lig", "zum", "Ver\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn Gott, denkt er, vergibt auch mir.", "tokens": ["Denn", "Gott", ",", "denkt", "er", ",", "ver\u00b7gibt", "auch", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "PPER", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Sein Beispiel sucht dein Herz im Guten zu best\u00e4rken,", "tokens": ["Sein", "Bei\u00b7spiel", "sucht", "dein", "Herz", "im", "Gu\u00b7ten", "zu", "be\u00b7st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er nimmt an deiner Tugend teil;", "tokens": ["Er", "nimmt", "an", "dei\u00b7ner", "Tu\u00b7gend", "teil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn alle sind von Gott gezeugt zu guten Werken,", "tokens": ["Denn", "al\u00b7le", "sind", "von", "Gott", "ge\u00b7zeugt", "zu", "gu\u00b7ten", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und haben ", "tokens": ["Und", "ha\u00b7ben"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.9": {"line.1": {"text": "Dies Heil der Ewigkeit, das hier der Fromme schmecket,", "tokens": ["Dies", "Heil", "der", "E\u00b7wig\u00b7keit", ",", "das", "hier", "der", "From\u00b7me", "schme\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erh\u00f6ht sein Gl\u00fcck, stillt seinen Schmerz,", "tokens": ["Er\u00b7h\u00f6ht", "sein", "Gl\u00fcck", ",", "stillt", "sei\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gibt ihm Geduld und Mut. Kein Tod, der ihn erschrecket!", "tokens": ["Gibt", "ihm", "Ge\u00b7duld", "und", "Mut", ".", "Kein", "Tod", ",", "der", "ihn", "er\u00b7schre\u00b7cket", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$.", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Tode noch freut sich sein Herz.", "tokens": ["Im", "To\u00b7de", "noch", "freut", "sich", "sein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}