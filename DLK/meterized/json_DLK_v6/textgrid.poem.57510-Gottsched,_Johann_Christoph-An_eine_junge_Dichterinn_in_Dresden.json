{"textgrid.poem.57510": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "An eine junge Dichterinn in Dresden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Poetinn, was ich j\u00fcngst von deiner Hand gelesen,", "tokens": ["Poe\u00b7tinn", ",", "was", "ich", "j\u00fcngst", "von", "dei\u00b7ner", "Hand", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Hat mich von Anfang zwar mit vieler Lust ergetzt:", "tokens": ["Hat", "mich", "von", "An\u00b7fang", "zwar", "mit", "vie\u00b7ler", "Lust", "er\u00b7getzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch schade! da\u00df sie nicht auch dauerhaft gewesen,", "tokens": ["Doch", "scha\u00b7de", "!", "da\u00df", "sie", "nicht", "auch", "dau\u00b7er\u00b7haft", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil mich der Schlu\u00df gelehrt, was du dir vorgesetzt.", "tokens": ["Weil", "mich", "der", "Schlu\u00df", "ge\u00b7lehrt", ",", "was", "du", "dir", "vor\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du willst, so drohest du, den Musenberg verlassen,", "tokens": ["Du", "willst", ",", "so", "dro\u00b7hest", "du", ",", "den", "Mu\u00b7sen\u00b7berg", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den doch dein Auge schon, so nahe vor sich sieht;", "tokens": ["Den", "doch", "dein", "Au\u00b7ge", "schon", ",", "so", "na\u00b7he", "vor", "sich", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "ADV", "$,", "ADV", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und das gelehrte Chor der klugen Schwestern hassen,", "tokens": ["Und", "das", "ge\u00b7lehr\u00b7te", "Chor", "der", "klu\u00b7gen", "Schwes\u00b7tern", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob deine Neigung dich gleich sie zu lieben zieht.", "tokens": ["Ob", "dei\u00b7ne", "Nei\u00b7gung", "dich", "gleich", "sie", "zu", "lie\u00b7ben", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ists m\u00f6glich? darf ich dir in diesem St\u00fccke glauben?", "tokens": ["Ists", "m\u00f6g\u00b7lich", "?", "darf", "ich", "dir", "in", "die\u00b7sem", "St\u00fc\u00b7cke", "glau\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "VMFIN", "PPER", "PPER", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und schreibt dein Kiel nicht mehr, als dein Gem\u00fcth gedenkt?", "tokens": ["Und", "schreibt", "dein", "Kiel", "nicht", "mehr", ",", "als", "dein", "Ge\u00b7m\u00fcth", "ge\u00b7denkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So sprich, was kann dir wohl die Lust zum Dichten rauben?", "tokens": ["So", "sprich", ",", "was", "kann", "dir", "wohl", "die", "Lust", "zum", "Dich\u00b7ten", "rau\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und was hat dich so schnell auf diesen Schlu\u00df gelenkt?", "tokens": ["Und", "was", "hat", "dich", "so", "schnell", "auf", "die\u00b7sen", "Schlu\u00df", "ge\u00b7lenkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hat Ph\u00f6bus dich erz\u00fcrnt? Hat eine von den Sch\u00f6nen,", "tokens": ["Hat", "Ph\u00f6\u00b7bus", "dich", "er\u00b7z\u00fcrnt", "?", "Hat", "ei\u00b7ne", "von", "den", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PPER", "VVFIN", "$.", "VAFIN", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die seine Schwestern sind, dich h\u00f6hnisch angesehn?", "tokens": ["Die", "sei\u00b7ne", "Schwes\u00b7tern", "sind", ",", "dich", "h\u00f6h\u00b7nisch", "an\u00b7ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "O nein, das thun sie nicht; ja selbst von ihren S\u00f6hnen", "tokens": ["O", "nein", ",", "das", "thun", "sie", "nicht", ";", "ja", "selbst", "von", "ih\u00b7ren", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKANT", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ist deiner Poesie bisher kein Leid geschehn.", "tokens": ["Ist", "dei\u00b7ner", "Poe\u00b7sie", "bis\u00b7her", "kein", "Leid", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Du sprichst hier: Dieses ists, was ich hinf\u00fchro scheue:", "tokens": ["Du", "sprichst", "hier", ":", "Die\u00b7ses", "ists", ",", "was", "ich", "hin\u00b7f\u00fch\u00b7ro", "scheu\u00b7e", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PDS", "VAFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Man schont euch Dichter nicht, und sticht euch oft den Staar.", "tokens": ["Man", "schont", "euch", "Dich\u00b7ter", "nicht", ",", "und", "sticht", "euch", "oft", "den", "Staar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "PTKNEG", "$,", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Geschieht nun auch an mir, was ich mir prophezeihe,", "tokens": ["Ge\u00b7schieht", "nun", "auch", "an", "mir", ",", "was", "ich", "mir", "pro\u00b7phe\u00b7zei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPER", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So wird auch dermaleins mein Vorwitz offenbar.", "tokens": ["So", "wird", "auch", "der\u00b7ma\u00b7leins", "mein", "Vor\u00b7witz", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch, sch\u00f6ne Dichterinn! die Furcht ist ungegr\u00fcndet,", "tokens": ["Doch", ",", "sch\u00f6\u00b7ne", "Dich\u00b7te\u00b7rinn", "!", "die", "Furcht", "ist", "un\u00b7ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die weiter nichts besorgt, als diese Tadelsucht:", "tokens": ["Die", "wei\u00b7ter", "nichts", "be\u00b7sorgt", ",", "als", "die\u00b7se", "Ta\u00b7del\u00b7sucht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VVPP", "$,", "KOUS", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und wenn dein scharfer Sinn nichts mehr zu f\u00fcrchten findet,", "tokens": ["Und", "wenn", "dein", "schar\u00b7fer", "Sinn", "nichts", "mehr", "zu", "f\u00fcrch\u00b7ten", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "PIS", "PIS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So nimmst du vom Parna\u00df noch viel zu fr\u00fch die Flucht.", "tokens": ["So", "nimmst", "du", "vom", "Par\u00b7na\u00df", "noch", "viel", "zu", "fr\u00fch", "die", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NE", "ADV", "ADV", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Erw\u00e4ge nur einmal mit ungest\u00f6rten Sinnen,", "tokens": ["Er\u00b7w\u00e4\u00b7ge", "nur", "ein\u00b7mal", "mit", "un\u00b7ge\u00b7st\u00f6r\u00b7ten", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-++-++-+-+-", "measure": "unknown.measure.septa"}, "line.26": {"text": "Da\u00df M\u00e4nner unter sich ganz ungezwungen sind.", "tokens": ["Da\u00df", "M\u00e4n\u00b7ner", "un\u00b7ter", "sich", "ganz", "un\u00b7ge\u00b7zwun\u00b7gen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PRF", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Man schenkt einander nichts, und l\u00e4\u00dft sich nie gewinnen,", "tokens": ["Man", "schenkt", "ein\u00b7an\u00b7der", "nichts", ",", "und", "l\u00e4\u00dft", "sich", "nie", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PIS", "$,", "KON", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So lange Momus selbst noch einen Tadel findt.", "tokens": ["So", "lan\u00b7ge", "Mo\u00b7mus", "selbst", "noch", "ei\u00b7nen", "Ta\u00b7del", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So, wie mans andern macht, so machens andre wieder,", "tokens": ["So", ",", "wie", "mans", "an\u00b7dern", "macht", ",", "so", "ma\u00b7chens", "and\u00b7re", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIS", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das Echo schallt zur\u00fcck, wie man zum Walde schreyt.", "tokens": ["Das", "E\u00b7cho", "schallt", "zu\u00b7r\u00fcck", ",", "wie", "man", "zum", "Wal\u00b7de", "schreyt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PWAV", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein aufgebrachter Kiel legt alle Sanftmuth nieder,", "tokens": ["Ein", "auf\u00b7ge\u00b7brach\u00b7ter", "Kiel", "legt", "al\u00b7le", "Sanft\u00b7muth", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und weis aus Rachgier oft von keiner H\u00f6flichkeit.", "tokens": ["Und", "weis", "aus", "Rach\u00b7gier", "oft", "von", "kei\u00b7ner", "H\u00f6f\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "APPR", "NN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ganz anders geht es zu, wo sch\u00f6ne Kinder singen:", "tokens": ["Ganz", "an\u00b7ders", "geht", "es", "zu", ",", "wo", "sch\u00f6\u00b7ne", "Kin\u00b7der", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wer wollte da so scharf auf ihre Lieder seyn?", "tokens": ["Wer", "woll\u00b7te", "da", "so", "scharf", "auf", "ih\u00b7re", "Lie\u00b7der", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wer lie\u00dfe sich so gar den Unverstand bezwingen,", "tokens": ["Wer", "lie\u00b7\u00dfe", "sich", "so", "gar", "den", "Un\u00b7ver\u00b7stand", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der zarten Laute gleich den Untergang zu dr\u00e4un?", "tokens": ["Der", "zar\u00b7ten", "Lau\u00b7te", "gleich", "den", "Un\u00b7ter\u00b7gang", "zu", "dr\u00e4un", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "F\u00fcrwahr! das hie\u00dfe sich der Grobheit ganz ergeben,", "tokens": ["F\u00fcr\u00b7wahr", "!", "das", "hie\u00b7\u00dfe", "sich", "der", "Grob\u00b7heit", "ganz", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "VVFIN", "PRF", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das w\u00e4re zweifelsfrey der gr\u00f6bste Baurenstreich!", "tokens": ["Das", "w\u00e4\u00b7re", "zwei\u00b7fels\u00b7frey", "der", "gr\u00f6bs\u00b7te", "Bau\u00b7ren\u00b7streich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "CARD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wo bliebe wohl dabey die Art geschickt zu leben?", "tokens": ["Wo", "blie\u00b7be", "wohl", "da\u00b7bey", "die", "Art", "ge\u00b7schickt", "zu", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PAV", "ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Was w\u00e4r an Aberwitz dergleichen Sp\u00f6ttern gleich?", "tokens": ["Was", "w\u00e4r", "an", "A\u00b7ber\u00b7witz", "derg\