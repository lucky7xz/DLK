{"dta.poem.5446": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abermahlige Thau-Betrachtungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn das entstandne Morgenroth die Schatten Westen-", "tokens": ["Wenn", "das", "ent\u00b7stand\u00b7ne", "Mor\u00b7gen\u00b7roth", "die", "Schat\u00b7ten", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "w\u00e4rts verdrenget,", "tokens": ["w\u00e4rts", "ver\u00b7dren\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Und das bethaute, feuchte Feld den ersten Sonnen-Strahl", "tokens": ["Und", "das", "be\u00b7thau\u00b7te", ",", "feuch\u00b7te", "Feld", "den", "ers\u00b7ten", "Son\u00b7nen\u00b7Strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "$,", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "empf\u00e4nget,", "tokens": ["emp\u00b7f\u00e4n\u00b7get", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Der \u00fcber die begrasten Wiesen, wie eine g\u00fcldne Fluth, sich", "tokens": ["Der", "\u00fc\u00b7ber", "die", "be\u00b7gras\u00b7ten", "Wie\u00b7sen", ",", "wie", "ei\u00b7ne", "g\u00fcld\u00b7ne", "Fluth", ",", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "PRF"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "legt;", "tokens": ["legt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Wird Millionen reinen Tropfen ein himmlisch Gl\u00e4utzen", "tokens": ["Wird", "Mil\u00b7lion\u00b7en", "rei\u00b7nen", "Trop\u00b7fen", "ein", "himm\u00b7lisch", "Gl\u00e4ut\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADJA", "NN", "ART", "ADJD", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "eingepr\u00e4gt.", "tokens": ["ein\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Zu Anfang sieht man hohe Kr\u00e4uter, und langen Grases", "tokens": ["Zu", "An\u00b7fang", "sieht", "man", "ho\u00b7he", "Kr\u00e4u\u00b7ter", ",", "und", "lan\u00b7gen", "Gra\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "schwancke Spitzen,", "tokens": ["schwan\u00b7cke", "Spit\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Durch die zuerst empfundne Glut, nur eintzeln hin und", "tokens": ["Durch", "die", "zu\u00b7erst", "emp\u00b7fund\u00b7ne", "Glut", ",", "nur", "eint\u00b7zeln", "hin", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADV", "ADJA", "NN", "$,", "ADV", "ADJD", "PTKVZ", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "wieder blitzen,", "tokens": ["wie\u00b7der", "blit\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Bi\u00df allgemach ein tausend-f\u00e4rbig-und Diamanten-gleicher", "tokens": ["Bi\u00df", "all\u00b7ge\u00b7mach", "ein", "tau\u00b7sen\u00b7df\u00e4r\u00b7big\u00b7\u00b7und", "Di\u00b7a\u00b7man\u00b7ten\u00b7glei\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Schein", "tokens": ["Schein"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Des gantzen Feldes Fl\u00e4chen decket: das Funckeln ist jetzt", "tokens": ["Des", "gant\u00b7zen", "Fel\u00b7des", "Fl\u00e4\u00b7chen", "de\u00b7cket", ":", "das", "Fun\u00b7ckeln", "ist", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "allgemein.", "tokens": ["all\u00b7ge\u00b7mein", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Man siehet alles, was man sieht, in einem bunten Glantze", "tokens": ["Man", "sie\u00b7het", "al\u00b7les", ",", "was", "man", "sieht", ",", "in", "ei\u00b7nem", "bun\u00b7ten", "Glant\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "glimmen;", "tokens": ["glim\u00b7men", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Es scheint der halb entz\u00fcckte Blick zu gleich zu gl\u00fchen und", "tokens": ["Es", "scheint", "der", "halb", "ent\u00b7z\u00fcck\u00b7te", "Blick", "zu", "gleich", "zu", "gl\u00fc\u00b7hen", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "APPR", "ADV", "PTKZU", "VVINF", "KON"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "zu schwimmen", "tokens": ["zu", "schwim\u00b7men"], "token_info": ["word", "word"], "pos": ["PTKZU", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "In bunt gef\u00e4rbtem Feur und Wasser, von welchem die", "tokens": ["In", "bunt", "ge\u00b7f\u00e4rb\u00b7tem", "Feur", "und", "Was\u00b7ser", ",", "von", "wel\u00b7chem", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "KON", "NN", "$,", "APPR", "PRELS", "ART"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "vereinte Pracht,", "tokens": ["ver\u00b7ein\u00b7te", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Durchs Aug\u2019 und Hirn, in unsre Seele den angenehmsten", "tokens": ["Durchs", "Aug'", "und", "Hirn", ",", "in", "uns\u00b7re", "See\u00b7le", "den", "an\u00b7ge\u00b7nehms\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Eindruck macht.", "tokens": ["Ein\u00b7druck", "macht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Den sonst kein Vorwurf wircken kann. Man wundre sich", "tokens": ["Den", "sonst", "kein", "Vor\u00b7wurf", "wir\u00b7cken", "kann", ".", "Man", "wund\u00b7re", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$.", "PIS", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "hier\u00fcber nicht,", "tokens": ["hier\u00b7\u00fc\u00b7ber", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Da\u00df, da der Sonnen-Strahl im Thau sich recht als wie", "tokens": ["Da\u00df", ",", "da", "der", "Son\u00b7nen\u00b7Strahl", "im", "Thau", "sich", "recht", "als", "wie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "PRF", "ADV", "KOUS", "KOKOM"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "ein Demant bricht;", "tokens": ["ein", "De\u00b7mant", "bricht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}