{"textgrid.poem.62147": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die lieblichen S\u00e4nger des Feldes", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die lieblichen S\u00e4nger des Feldes", "tokens": ["Die", "lieb\u00b7li\u00b7chen", "S\u00e4n\u00b7ger", "des", "Fel\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ach, nackt und zum Fra\u00dfe bereit,", "tokens": ["Ach", ",", "nackt", "und", "zum", "Fra\u00b7\u00dfe", "be\u00b7reit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "KON", "APPRART", "NN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ihr werdet doch Lerchen nicht essen?", "tokens": ["Ihr", "wer\u00b7det", "doch", "Ler\u00b7chen", "nicht", "es\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mein Gott, ihr w\u00e4r't nicht gescheit!", "tokens": ["Mein", "Gott", ",", "ihr", "w\u00e4r't", "nicht", "ge\u00b7scheit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die Lerche, die wahre Poetin,", "tokens": ["Die", "Ler\u00b7che", ",", "die", "wah\u00b7re", "Poe\u00b7tin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zum Himmel sich schwingend hinauf,", "tokens": ["Zum", "Him\u00b7mel", "sich", "schwin\u00b7gend", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ihr Nestlein ach sorglos am Boden,", "tokens": ["Ihr", "Nes\u00b7tlein", "ach", "sorg\u00b7los", "am", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die Senner, sie treten darauf.", "tokens": ["Die", "Sen\u00b7ner", ",", "sie", "tre\u00b7ten", "da\u00b7rauf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Allein der Bauer vom Lande,", "tokens": ["Al\u00b7lein", "der", "Bau\u00b7er", "vom", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Er hat ein nat\u00fcrliches Herz, \u2013", "tokens": ["Er", "hat", "ein", "na\u00b7t\u00fcr\u00b7li\u00b7ches", "Herz", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mit Schonung schwingt er die Sense,", "tokens": ["Mit", "Scho\u00b7nung", "schwingt", "er", "die", "Sen\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Sense von Stahl und Erz.", "tokens": ["Die", "Sen\u00b7se", "von", "Stahl", "und", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "In Leipzig aber da schlachten", "tokens": ["In", "Leip\u00b7zig", "a\u00b7ber", "da", "schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADV", "ADV", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die singenden Kehlchen sie,", "tokens": ["Die", "sin\u00b7gen\u00b7den", "Kehl\u00b7chen", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u2013 Ach, nackt und zart zum Erbarmen \u2013", "tokens": ["\u2013", "Ach", ",", "nackt", "und", "zart", "zum", "Er\u00b7bar\u00b7men", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADJD", "KON", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Schlachten der Poesie!", "tokens": ["Ein", "Schlach\u00b7ten", "der", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}