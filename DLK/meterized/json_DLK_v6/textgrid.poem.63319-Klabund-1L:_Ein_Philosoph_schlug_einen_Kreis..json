{"textgrid.poem.63319": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Philosoph schlug einen Kreis.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Philosoph schlug einen Kreis.", "tokens": ["Ein", "Phi\u00b7lo\u00b7soph", "schlug", "ei\u00b7nen", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wei\u00df,", "tokens": ["Wer", "wei\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Was er damit bedachte.", "tokens": ["Was", "er", "da\u00b7mit", "be\u00b7dach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und siehe da \u2013 wie hingeschnellt", "tokens": ["Und", "sie\u00b7he", "da", "\u2013", "wie", "hin\u00b7ge\u00b7schnellt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "$(", "PWAV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat sich ein zweiter zugesellt.", "tokens": ["Hat", "sich", "ein", "zwei\u00b7ter", "zu\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da war es eine Achte.", "tokens": ["Da", "war", "es", "ei\u00b7ne", "Ach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So gehts den Philosophen meist,", "tokens": ["So", "gehts", "den", "Phi\u00b7lo\u00b7so\u00b7phen", "meist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie zwei nackte Nullen dreist", "tokens": ["Da\u00df", "sie", "zwei", "nack\u00b7te", "Nul\u00b7len", "dreist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "CARD", "ADJA", "NN", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu einer Acht erheben.", "tokens": ["Zu", "ei\u00b7ner", "Acht", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch sehn sie das Exempel ein?", "tokens": ["Doch", "sehn", "sie", "das", "Ex\u00b7em\u00b7pel", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Nein!", "tokens": ["Nein", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wo bliebe sonst ihr Leben?", "tokens": ["Wo", "blie\u00b7be", "sonst", "ihr", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}