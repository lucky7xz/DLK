{"textgrid.poem.33282": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Widerspr\u00fcche der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tyrannin, die so viele Sklaven", "tokens": ["Die", "Ty\u00b7ran\u00b7nin", ",", "die", "so", "vie\u00b7le", "Skla\u00b7ven"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN"], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Z\u00e4hlt als Menschen auf der Erde sind,", "tokens": ["Z\u00e4hlt", "als", "Men\u00b7schen", "auf", "der", "Er\u00b7de", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit ihren sieggewohnten Waffen", "tokens": ["Und", "mit", "ih\u00b7ren", "sieg\u00b7ge\u00b7wohn\u00b7ten", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Alles zwingt, ist doch der Freiheit Kind.", "tokens": ["Al\u00b7les", "zwingt", ",", "ist", "doch", "der", "Frei\u00b7heit", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sie, an deren schwerem Siegeswagen", "tokens": ["Sie", ",", "an", "de\u00b7ren", "schwe\u00b7rem", "Sie\u00b7ges\u00b7wa\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir nie anders als gebunden geh'n,", "tokens": ["Wir", "nie", "an\u00b7ders", "als", "ge\u00b7bun\u00b7den", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der nur Zwang und Sklavendienst behagen,", "tokens": ["Der", "nur", "Zwang", "und", "Skla\u00b7ven\u00b7dienst", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kann doch ohne Freiheit nicht besteh'n.", "tokens": ["Kann", "doch", "oh\u00b7ne", "Frei\u00b7heit", "nicht", "be\u00b7steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VMFIN", "ADV", "APPR", "NN", "PTKNEG", "VVFIN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sie, die mit dem Blick die Freiheit t\u00f6dtet,", "tokens": ["Sie", ",", "die", "mit", "dem", "Blick", "die", "Frei\u00b7heit", "t\u00f6d\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Stirbt doch selbst vom kleinsten Hauch der Pflicht,", "tokens": ["Stirbt", "doch", "selbst", "vom", "kleins\u00b7ten", "Hauch", "der", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sie, die uns so fest zusammenkettet,", "tokens": ["Sie", ",", "die", "uns", "so", "fest", "zu\u00b7sam\u00b7men\u00b7ket\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Duldet die geringste Fessel nicht.", "tokens": ["Dul\u00b7det", "die", "ge\u00b7rings\u00b7te", "Fes\u00b7sel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sie, die Widerstand nicht \u00fcberwindet,", "tokens": ["Sie", ",", "die", "Wi\u00b7der\u00b7stand", "nicht", "\u00fc\u00b7berw\u00b7in\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die selbst Elternfluch nicht \u00fcbermannt,", "tokens": ["Die", "selbst", "El\u00b7tern\u00b7fluch", "nicht", "\u00fc\u00b7berm\u00b7annt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Flieht vor jedem Schein des Zwangs, und schwindet", "tokens": ["Flieht", "vor", "je\u00b7dem", "Schein", "des", "Zwangs", ",", "und", "schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unter'm Segen einer Priesterhand.", "tokens": ["Un\u00b7ter'm", "Se\u00b7gen", "ei\u00b7ner", "Pries\u00b7ter\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Sie, die frei im ew'gen Lenze bl\u00fchet,", "tokens": ["Sie", ",", "die", "frei", "im", "ew'\u00b7gen", "Len\u00b7ze", "bl\u00fc\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welket \u00fcber Nacht im Ehhett' ab;", "tokens": ["Wel\u00b7ket", "\u00fc\u00b7ber", "Nacht", "im", "Eh\u00b7hett'", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sie, die nach Genusse lechzt und gl\u00fchet,", "tokens": ["Sie", ",", "die", "nach", "Ge\u00b7nus\u00b7se", "lechzt", "und", "gl\u00fc\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Findet im Genusse selbst ihr Grab.", "tokens": ["Fin\u00b7det", "im", "Ge\u00b7nus\u00b7se", "selbst", "ihr", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "D'rum wozu soll sich der Mensch entschliessen?", "tokens": ["D'\u00b7rum", "wo\u00b7zu", "soll", "sich", "der", "Mensch", "ent\u00b7schlies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWAV", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Soll er ewig fruchtlos Sklave seyn?", "tokens": ["Soll", "er", "e\u00b7wig", "frucht\u00b7los", "Skla\u00b7ve", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADJD", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Soll er lieben ohne zu geniessen?", "tokens": ["Soll", "er", "lie\u00b7ben", "oh\u00b7ne", "zu", "ge\u00b7nies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KOUI", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Oder soll er ohne Liebe frey'n?", "tokens": ["O\u00b7der", "soll", "er", "oh\u00b7ne", "Lie\u00b7be", "frey'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}