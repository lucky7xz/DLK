{"textgrid.poem.35570": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein sanftes Glockenspiel t\u00f6nt in Elis' Brust", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein sanftes Glockenspiel t\u00f6nt in Elis' Brust", "tokens": ["Ein", "sanf\u00b7tes", "Glo\u00b7cken\u00b7spiel", "t\u00f6nt", "in", "E\u00b7lis'", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am Abend,", "tokens": ["Am", "A\u00b7bend", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da sein Haupt ins schwarze Kissen sinkt.", "tokens": ["Da", "sein", "Haupt", "ins", "schwar\u00b7ze", "Kis\u00b7sen", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ein blaues Wild", "tokens": ["Ein", "blau\u00b7es", "Wild"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Blutet leise im Dornengestr\u00fcpp.", "tokens": ["Blu\u00b7tet", "lei\u00b7se", "im", "Dor\u00b7nen\u00b7ge\u00b7str\u00fcpp", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein brauner Baum steht abgeschieden da;", "tokens": ["Ein", "brau\u00b7ner", "Baum", "steht", "ab\u00b7ge\u00b7schie\u00b7den", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seine blauen Fr\u00fcchte fielen von ihm.", "tokens": ["Sei\u00b7ne", "blau\u00b7en", "Fr\u00fcch\u00b7te", "fie\u00b7len", "von", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Zeichen und Sterne", "tokens": ["Zei\u00b7chen", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Versinken leise im Abendweiher.", "tokens": ["Ver\u00b7sin\u00b7ken", "lei\u00b7se", "im", "Ab\u00b7end\u00b7wei\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Hinter dem H\u00fcgel ist es Winter geworden.", "tokens": ["Hin\u00b7ter", "dem", "H\u00fc\u00b7gel", "ist", "es", "Win\u00b7ter", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "NN", "VAPP", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Blaue Tauben", "tokens": ["Blau\u00b7e", "Tau\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Trinken nachts den eisigen Schwei\u00df,", "tokens": ["Trin\u00b7ken", "nachts", "den", "ei\u00b7si\u00b7gen", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der von Elis' kristallener Stirne rinnt.", "tokens": ["Der", "von", "E\u00b7lis'", "kris\u00b7tal\u00b7le\u00b7ner", "Stir\u00b7ne", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Immer t\u00f6nt", "tokens": ["Im\u00b7mer", "t\u00f6nt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "An schwarzen Mauern Gottes einsamer Wind.", "tokens": ["An", "schwar\u00b7zen", "Mau\u00b7ern", "Got\u00b7tes", "ein\u00b7sa\u00b7mer", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}