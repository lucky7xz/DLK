{"textgrid.poem.55699": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit ehernem Griffel grub ich", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit ehernem Griffel grub ich", "tokens": ["Mit", "e\u00b7her\u00b7nem", "Grif\u00b7fel", "grub", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tief in Dein weiches Herz", "tokens": ["Tief", "in", "Dein", "wei\u00b7ches", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr immer mein Bild.", "tokens": ["F\u00fcr", "im\u00b7mer", "mein", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und wenn auch Vorsicht,", "tokens": ["Und", "wenn", "auch", "Vor\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Klugwaltende, sichergehende", "tokens": ["Klug\u00b7wal\u00b7ten\u00b7de", ",", "si\u00b7cher\u00b7ge\u00b7hen\u00b7de"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dich beh\u00fctet", "tokens": ["Dich", "be\u00b7h\u00fc\u00b7tet"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Vor des Dichters", "tokens": ["Vor", "des", "Dich\u00b7ters"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Flammenzehrender Liebe,", "tokens": ["Flam\u00b7men\u00b7zeh\u00b7ren\u00b7der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Niemals wirst Du mich vergessen.", "tokens": ["Nie\u00b7mals", "wirst", "Du", "mich", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Deines Mundes erste K\u00fcsse", "tokens": ["Dei\u00b7nes", "Mun\u00b7des", "ers\u00b7te", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Lernte Dein herrlicher Leib", "tokens": ["Lern\u00b7te", "Dein", "herr\u00b7li\u00b7cher", "Leib"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Ahnungsvolle,", "tokens": ["Ah\u00b7nungs\u00b7vol\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Bebende Liebe.", "tokens": ["Be\u00b7ben\u00b7de", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "All die s\u00fc\u00dfen ersten Schw\u00fcre", "tokens": ["All", "die", "s\u00fc\u00b7\u00dfen", "ers\u00b7ten", "Schw\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Deines jungen,", "tokens": ["Dei\u00b7nes", "jun\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Verlangenden Herzens", "tokens": ["Ver\u00b7lan\u00b7gen\u00b7den", "Her\u00b7zens"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Galten mir.", "tokens": ["Gal\u00b7ten", "mir", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "In Deiner Seele leben", "tokens": ["In", "Dei\u00b7ner", "See\u00b7le", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ewig jung", "tokens": ["E\u00b7wig", "jung"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Und ewig reizvoll", "tokens": ["Und", "e\u00b7wig", "reiz\u00b7voll"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Ungenossene und unermessene", "tokens": ["Un\u00b7ge\u00b7nos\u00b7se\u00b7ne", "und", "un\u00b7er\u00b7mes\u00b7se\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "KON", "ADJA"], "meter": "+-+---+-+--", "measure": "unknown.measure.tetra"}, "line.23": {"text": "Scheugeahnte Wonnen,", "tokens": ["Scheu\u00b7geahn\u00b7te", "Won\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Unzertrennbar gebunden", "tokens": ["Un\u00b7zer\u00b7trenn\u00b7bar", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.25": {"text": "An ", "tokens": ["An"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Verfluchen wirst Du mich vielleicht,", "tokens": ["Ver\u00b7flu\u00b7chen", "wirst", "Du", "mich", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Vergessen aber ", "tokens": ["Ver\u00b7ges\u00b7sen", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}}}}}