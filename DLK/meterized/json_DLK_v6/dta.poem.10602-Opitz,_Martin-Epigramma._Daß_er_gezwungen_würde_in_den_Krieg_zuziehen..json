{"dta.poem.10602": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma.  \n  Da\u00df er gezwungen w\u00fcrde in den Krieg zuziehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr G\u00f6tter/ soll mich dann des schn\u00f6den Gl\u00fcckes Neidt", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "/", "soll", "mich", "dann", "des", "schn\u00f6\u00b7den", "Gl\u00fc\u00b7ckes", "Neidt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht lassen? Mu\u00df ich mich begeben in den streit?", "tokens": ["Nicht", "las\u00b7sen", "?", "Mu\u00df", "ich", "mich", "be\u00b7ge\u00b7ben", "in", "den", "streit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$.", "VMFIN", "PPER", "PRF", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach last mich/ last mich hier/ der Krieg ist nicht vonn\u00f6ten/", "tokens": ["Ach", "last", "mich", "/", "last", "mich", "hier", "/", "der", "Krieg", "ist", "nicht", "von\u00b7n\u00f6\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "ADV", "$(", "ART", "NN", "VAFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Last mich bey meinem Lieb/ sie kan mich besser t\u00f6dten.", "tokens": ["Last", "mich", "bey", "mei\u00b7nem", "Lieb", "/", "sie", "kan", "mich", "bes\u00b7ser", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}