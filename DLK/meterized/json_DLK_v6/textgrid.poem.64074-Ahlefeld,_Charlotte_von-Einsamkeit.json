{"textgrid.poem.64074": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist wahre Einsamkeit?", "tokens": ["Was", "ist", "wah\u00b7re", "Ein\u00b7sam\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind wir einsam, wenn das Leben", "tokens": ["Sind", "wir", "ein\u00b7sam", ",", "wenn", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings von Stille ist umgeben?", "tokens": ["Rings", "von", "Stil\u00b7le", "ist", "um\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die rege Fantasie", "tokens": ["Wenn", "die", "re\u00b7ge", "Fan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Uns in schaffender Magie", "tokens": ["Uns", "in", "schaf\u00b7fen\u00b7der", "Ma\u00b7gie"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Neu beseelt mit s\u00fcssem Streben", "tokens": ["Neu", "be\u00b7seelt", "mit", "s\u00fcs\u00b7sem", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bilder der Vergangenheit? \u2013", "tokens": ["Bil\u00b7der", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist das wahre Einsamkeit?", "tokens": ["Ist", "das", "wah\u00b7re", "Ein\u00b7sam\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Oder wenn in stillen Gr\u00fcnden,", "tokens": ["O\u00b7der", "wenn", "in", "stil\u00b7len", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Waldes heil'ger Nacht,", "tokens": ["In", "des", "Wal\u00b7des", "heil'\u00b7ger", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnenglanz in reiner Pracht", "tokens": ["Son\u00b7nen\u00b7glanz", "in", "rei\u00b7ner", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die leis' bewegten Wipfel,", "tokens": ["Durch", "die", "leis'", "be\u00b7weg\u00b7ten", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die glanzums\u00e4umten Gipfel", "tokens": ["Durch", "die", "glan\u00b7zum\u00b7s\u00e4um\u00b7ten", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur verstohlen blickend, lacht,", "tokens": ["Nur", "ver\u00b7stoh\u00b7len", "bli\u00b7ckend", ",", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in den verworrnen Zweigen", "tokens": ["Und", "in", "den", "ver\u00b7worr\u00b7nen", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Selbst die kleinen S\u00e4nger schweigen?", "tokens": ["Selbst", "die", "klei\u00b7nen", "S\u00e4n\u00b7ger", "schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder wenn in dunklen Mauern,", "tokens": ["O\u00b7der", "wenn", "in", "dunk\u00b7len", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Kerkers engen Raum,", "tokens": ["In", "des", "Ker\u00b7kers", "en\u00b7gen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Gefangene sich kaum", "tokens": ["Der", "Ge\u00b7fan\u00b7ge\u00b7ne", "sich", "kaum"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf in seinen Ketten regen,", "tokens": ["Darf", "in", "sei\u00b7nen", "Ket\u00b7ten", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sein Herz mit raschen Schl\u00e4gen", "tokens": ["Wenn", "sein", "Herz", "mit", "ra\u00b7schen", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "N\u00e4hrt der Hoffnung G\u00f6ttertraum,", "tokens": ["N\u00e4hrt", "der", "Hoff\u00b7nung", "G\u00f6t\u00b7ter\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und getheilt in Freud' und Trauern,", "tokens": ["Und", "ge\u00b7theilt", "in", "Freud'", "und", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ahndungen ihn tief durchschauern? \u2013", "tokens": ["Ahn\u00b7dun\u00b7gen", "ihn", "tief", "durch\u00b7schau\u00b7ern", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADJD", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Nein, nur das ist Einsamkeit,", "tokens": ["Nein", ",", "nur", "das", "ist", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PDS", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sich Wesen um uns drangen,", "tokens": ["Wenn", "sich", "We\u00b7sen", "um", "uns", "dran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denen nicht in zarten Kl\u00e4ngen", "tokens": ["De\u00b7nen", "nicht", "in", "zar\u00b7ten", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich vernehmbar macht das Herz,", "tokens": ["Sich", "ver\u00b7nehm\u00b7bar", "macht", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oft voll Wonne, oft voll Schmerz \u2013", "tokens": ["Oft", "voll", "Won\u00b7ne", ",", "oft", "voll", "Schmerz", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,", "ADV", "ADJD", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die uns das Gem\u00fcth verengen", "tokens": ["Die", "uns", "das", "Ge\u00b7m\u00fcth", "ver\u00b7en\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Durch der ", "tokens": ["Durch", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Das ist wahre Einsamkeit!", "tokens": ["Das", "ist", "wah\u00b7re", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}