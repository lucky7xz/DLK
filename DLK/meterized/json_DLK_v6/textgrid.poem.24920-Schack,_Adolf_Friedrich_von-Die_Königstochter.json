{"textgrid.poem.24920": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die K\u00f6nigstochter", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas br\u00fctest und tr\u00e4umst du, junger Fant?", "tokens": ["\u00bb", "was", "br\u00fc\u00b7test", "und", "tr\u00e4umst", "du", ",", "jun\u00b7ger", "Fant", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "KON", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heut gilt es kein Lied zur Zither;", "tokens": ["Heut", "gilt", "es", "kein", "Lied", "zur", "Zi\u00b7ther", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Links weichen die Unsern; dort halte stand,", "tokens": ["Links", "wei\u00b7chen", "die", "Un\u00b7sern", ";", "dort", "hal\u00b7te", "stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und selbst dich schlag' ich zum Ritter!\u00ab", "tokens": ["Und", "selbst", "dich", "schlag'", "ich", "zum", "Rit\u00b7ter", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Err\u00f6tend st\u00fcrmte der Edelknecht", "tokens": ["Er\u00b7r\u00f6\u00b7tend", "st\u00fcrm\u00b7te", "der", "E\u00b7del\u00b7knecht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beim Wort des K\u00f6nigs von dannen;", "tokens": ["Beim", "Wort", "des", "K\u00f6\u00b7nigs", "von", "dan\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In die dichtesten Reihn der Feinde hinein", "tokens": ["In", "die", "dich\u00b7tes\u00b7ten", "Reihn", "der", "Fein\u00b7de", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "APZR"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Von neuem ri\u00df er die Mannen.", "tokens": ["Von", "neu\u00b7em", "ri\u00df", "er", "die", "Man\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Hoch flammte sein Schwert; zu Boden sank", "tokens": ["Hoch", "flamm\u00b7te", "sein", "Schwert", ";", "zu", "Bo\u00b7den", "sank"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$.", "APPR", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Feind bei jedem der Streiche;", "tokens": ["Ein", "Feind", "bei", "je\u00b7dem", "der", "Strei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Beim sinkenden Tag am Boden lag", "tokens": ["Beim", "sin\u00b7ken\u00b7den", "Tag", "am", "Bo\u00b7den", "lag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der J\u00fcngling selber als Leiche.", "tokens": ["Der", "J\u00fcng\u00b7ling", "sel\u00b7ber", "als", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOUS", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gewonnen der Sieg! Zur Hauptstadt kehrt", "tokens": ["Ge\u00b7won\u00b7nen", "der", "Sieg", "!", "Zur", "Haupt\u00b7stadt", "kehrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der K\u00f6nig mit seinen Vasallen;", "tokens": ["Der", "K\u00f6\u00b7nig", "mit", "sei\u00b7nen", "Va\u00b7sal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+----", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Doch traurige M\u00e4r bei der Wiederkehr", "tokens": ["Doch", "trau\u00b7ri\u00b7ge", "M\u00e4r", "bei", "der", "Wie\u00b7der\u00b7kehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Liest er in den Blicken von allen.", "tokens": ["Liest", "er", "in", "den", "Bli\u00b7cken", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PIAT", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Er findet die Tochter totenbleich", "tokens": ["Er", "fin\u00b7det", "die", "Toch\u00b7ter", "to\u00b7ten\u00b7bleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aufs Purpurkissen gebettet.", "tokens": ["Aufs", "Pur\u00b7pur\u00b7kis\u00b7sen", "ge\u00b7bet\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbauf, sendet Boten! Mein halbes Reich", "tokens": ["\u00bb", "auf", ",", "sen\u00b7det", "Bo\u00b7ten", "!", "Mein", "hal\u00b7bes", "Reich"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "VVFIN", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Dem, der vom Tode sie rettet!\u00ab", "tokens": ["Dem", ",", "der", "vom", "To\u00b7de", "sie", "ret\u00b7tet", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Nicht einer, so viele der Aerzte sind,", "tokens": ["Nicht", "ei\u00b7ner", ",", "so", "vie\u00b7le", "der", "A\u00b7erz\u00b7te", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "$,", "ADV", "PIS", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wei\u00df, was ihr fehle, zu sagen.", "tokens": ["Wei\u00df", ",", "was", "ihr", "feh\u00b7le", ",", "zu", "sa\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Bang forscht der Vater: \u00bbWas ist dir, Kind?\u00ab", "tokens": ["Bang", "forscht", "der", "Va\u00b7ter", ":", "\u00bb", "Was", "ist", "dir", ",", "Kind", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "$(", "PWS", "VAFIN", "PPER", "$,", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Stumm bleibt sie bei allen Fragen.", "tokens": ["Stumm", "bleibt", "sie", "bei", "al\u00b7len", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbund zehrt am Herzen dir Liebesqual,", "tokens": ["\u00bb", "und", "zehrt", "am", "Her\u00b7zen", "dir", "Lie\u00b7bes\u00b7qual", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "APPRART", "NN", "PPER", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O Tochter, h\u00f6r mich geloben:", "tokens": ["O", "Toch\u00b7ter", ",", "h\u00f6r", "mich", "ge\u00b7lo\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wen immer du w\u00e4hlst, er sei dein Gemahl!\u00ab", "tokens": ["Wen", "im\u00b7mer", "du", "w\u00e4hlst", ",", "er", "sei", "dein", "Ge\u00b7mahl", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie schaut, wie jammernd, nach oben.", "tokens": ["Sie", "schaut", ",", "wie", "jam\u00b7mernd", ",", "nach", "o\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "$,", "APPR", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Der K\u00f6nig wacht an der Lagerstatt,", "tokens": ["Der", "K\u00f6\u00b7nig", "wacht", "an", "der", "La\u00b7ger\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis bla\u00df aufd\u00e4mmert der Morgen;", "tokens": ["Bis", "bla\u00df", "auf\u00b7d\u00e4m\u00b7mert", "der", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da hebt mit der Rechten die Kranke ein Blatt,", "tokens": ["Da", "hebt", "mit", "der", "Rech\u00b7ten", "die", "Kran\u00b7ke", "ein", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das sie auf dem Busen verborgen,", "tokens": ["Das", "sie", "auf", "dem", "Bu\u00b7sen", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Und k\u00fc\u00dft es lange und seufzt so tief,", "tokens": ["Und", "k\u00fc\u00dft", "es", "lan\u00b7ge", "und", "seufzt", "so", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als f\u00fchlte das Herz sie zerspringen:", "tokens": ["Als", "f\u00fchl\u00b7te", "das", "Herz", "sie", "zer\u00b7sprin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbleb wohl! Das ist sein letzter Brief;", "tokens": ["\u00bb", "leb", "wohl", "!", "Das", "ist", "sein", "letz\u00b7ter", "Brief", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$.", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will ihm die Antwort bringen.\u00ab", "tokens": ["Ich", "will", "ihm", "die", "Ant\u00b7wort", "brin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}