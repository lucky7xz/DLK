{"textgrid.poem.35609": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sommer", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Abend schweigt die Klage", "tokens": ["Am", "A\u00b7bend", "schweigt", "die", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Kuckucks im Wald.", "tokens": ["Des", "Ku\u00b7ckucks", "im", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Tiefer neigt sich das Korn,", "tokens": ["Tie\u00b7fer", "neigt", "sich", "das", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der rote Mohn.", "tokens": ["Der", "ro\u00b7te", "Mohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schwarzes Gewitter droht", "tokens": ["Schwar\u00b7zes", "Ge\u00b7wit\u00b7ter", "droht"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "\u00dcber dem H\u00fcgel.", "tokens": ["\u00dc\u00b7ber", "dem", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das alte Lied der Grille", "tokens": ["Das", "al\u00b7te", "Lied", "der", "Gril\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erstirbt im Feld.", "tokens": ["Er\u00b7stirbt", "im", "Feld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nimmer regt sich das Laub", "tokens": ["Nim\u00b7mer", "regt", "sich", "das", "Laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der Kastanie.", "tokens": ["Der", "Kas\u00b7ta\u00b7nie", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf der Wendeltreppe", "tokens": ["Auf", "der", "Wen\u00b7del\u00b7trep\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Rauscht dein Kleid.", "tokens": ["Rauscht", "dein", "Kleid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Stille leuchtet die Kerze", "tokens": ["Stil\u00b7le", "leuch\u00b7tet", "die", "Ker\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Im dunklen Zimmer;", "tokens": ["Im", "dunk\u00b7len", "Zim\u00b7mer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Eine silberne Hand", "tokens": ["Ei\u00b7ne", "sil\u00b7ber\u00b7ne", "Hand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "L\u00f6schte sie aus;", "tokens": ["L\u00f6schte", "sie", "aus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Windstille, sternlose Nacht.", "tokens": ["Wind\u00b7stil\u00b7le", ",", "stern\u00b7lo\u00b7se", "Nacht", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "++-+--+", "measure": "iambic.tetra.chol"}}}}}