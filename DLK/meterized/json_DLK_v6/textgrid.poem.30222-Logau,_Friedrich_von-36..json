{"textgrid.poem.30222": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter so viel tausent Menschen schuff GOtt schwerlich derer zwey,", "tokens": ["Un\u00b7ter", "so", "viel", "tau\u00b7sent", "Men\u00b7schen", "schuff", "Gott", "schwer\u00b7lich", "de\u00b7rer", "zwey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "CARD", "NN", "APPR", "NN", "ADJD", "PDS", "CARD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Drunter einer wie der ander durch und durch gar gleiche sey;", "tokens": ["Drun\u00b7ter", "ei\u00b7ner", "wie", "der", "an\u00b7der", "durch", "und", "durch", "gar", "glei\u00b7che", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "KOKOM", "ART", "ADJD", "APPR", "KON", "APPR", "ADV", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nur die Mode wil es haben, das die Leute gar in ein", "tokens": ["Nur", "die", "Mo\u00b7de", "wil", "es", "ha\u00b7ben", ",", "das", "die", "Leu\u00b7te", "gar", "in", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "VAFIN", "$,", "PRELS", "ART", "NN", "ADV", "APPR", "ART"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sich solln kleiden und geberden oder gar nicht Menschen seyn.", "tokens": ["Sich", "solln", "klei\u00b7den", "und", "ge\u00b7ber\u00b7den", "o\u00b7der", "gar", "nicht", "Men\u00b7schen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VMFIN", "VVINF", "KON", "VVPP", "KON", "ADV", "PTKNEG", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}