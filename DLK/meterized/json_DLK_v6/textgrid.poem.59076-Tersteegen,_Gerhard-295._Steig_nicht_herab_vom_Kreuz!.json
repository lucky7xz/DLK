{"textgrid.poem.59076": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "295. Steig nicht herab vom Kreuz!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt dir Kreuz, Druck und Angst zu Haus,", "tokens": ["Kommt", "dir", "Kreuz", ",", "Druck", "und", "Angst", "zu", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Such doch nicht Luft, tracht nicht hinaus,", "tokens": ["Such", "doch", "nicht", "Luft", ",", "tracht", "nicht", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "NN", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "La\u00df dich ganz Gott ohn' Woll'n und W\u00e4hlen,", "tokens": ["La\u00df", "dich", "ganz", "Gott", "ohn'", "Woll'n", "und", "W\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sinkst du sanft aus allem Qu\u00e4len!", "tokens": ["So", "sinkst", "du", "sanft", "aus", "al\u00b7lem", "Qu\u00e4\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}