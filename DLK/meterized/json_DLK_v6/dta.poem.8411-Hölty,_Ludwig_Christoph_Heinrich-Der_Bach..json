{"dta.poem.8411": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Bach.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Deine Lispel, o Bach; tanze der Horchenden", "tokens": ["Dei\u00b7ne", "Lis\u00b7pel", ",", "o", "Bach", ";", "tan\u00b7ze", "der", "Hor\u00b7chen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "NE", "$.", "VVFIN", "ART", "NN"], "meter": "+-+--+---+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Silberblinkend vor\u00fcber;", "tokens": ["Sil\u00b7ber\u00b7blin\u00b7kend", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Gr\u00fcnt, ihr Erlen des Ufers, ihr!", "tokens": ["Gr\u00fcnt", ",", "ihr", "Er\u00b7len", "des", "U\u00b7fers", ",", "ihr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "ART", "NN", "$,", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Dein Gemurmel, das leis' \u00fcber die Kiesel h\u00fcpft,", "tokens": ["Dein", "Ge\u00b7mur\u00b7mel", ",", "das", "leis'", "\u00fc\u00b7ber", "die", "Kie\u00b7sel", "h\u00fcpft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Euer zitterndes Laub, duftende Freundinnen,", "tokens": ["Eu\u00b7er", "zit\u00b7tern\u00b7des", "Laub", ",", "duf\u00b7ten\u00b7de", "Freun\u00b7din\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Giesst ein lindes Erbeben", "tokens": ["Giesst", "ein", "lin\u00b7des", "Er\u00b7be\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Durch die Saiten der Seele mir.", "tokens": ["Durch", "die", "Sai\u00b7ten", "der", "See\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Hier, auf schwellendem Moos, horch' ich der Nachtigall,", "tokens": ["Hier", ",", "auf", "schwel\u00b7len\u00b7dem", "Moos", ",", "horch'", "ich", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die hier liebender klagt, horch' ich dem Schilfger\u00e4usch,", "tokens": ["Die", "hier", "lie\u00b7ben\u00b7der", "klagt", ",", "horch'", "ich", "dem", "Schilf\u00b7ge\u00b7r\u00e4usch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$,", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und dem Pl\u00e4tschern des Aales,", "tokens": ["Und", "dem", "Pl\u00e4t\u00b7schern", "des", "Aa\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der im Schatten der Erle schwebt.", "tokens": ["Der", "im", "Schat\u00b7ten", "der", "Er\u00b7le", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Und ein magischer Hain s\u00e4uselt um mich empor,", "tokens": ["Und", "ein", "ma\u00b7gi\u00b7scher", "Hain", "s\u00e4u\u00b7selt", "um", "mich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eine H\u00fctte darin winkt mir, mit Wein umrankt,", "tokens": ["Ei\u00b7ne", "H\u00fct\u00b7te", "da\u00b7rin", "winkt", "mir", ",", "mit", "Wein", "um\u00b7rankt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "PPER", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und ein freundliches M\u00e4dchen", "tokens": ["Und", "ein", "freund\u00b7li\u00b7ches", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "H\u00fcpft durch Blumen, und l\u00e4chelt mir.", "tokens": ["H\u00fcpft", "durch", "Blu\u00b7men", ",", "und", "l\u00e4\u00b7chelt", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Von des sinkenden Tags Golde ger\u00f6thet, s\u00e4umt", "tokens": ["Von", "des", "sin\u00b7ken\u00b7den", "Tags", "Gol\u00b7de", "ge\u00b7r\u00f6\u00b7thet", ",", "s\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVPP", "$,", "VVFIN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hinter Rosen sie her, eilet, und k\u00fcsst mich sanft;", "tokens": ["Hin\u00b7ter", "Ro\u00b7sen", "sie", "her", ",", "ei\u00b7let", ",", "und", "k\u00fcsst", "mich", "sanft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKVZ", "$,", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Fleucht, und l\u00e4chelt, und birgt sich", "tokens": ["Fleucht", ",", "und", "l\u00e4\u00b7chelt", ",", "und", "birgt", "sich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "PRF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wieder hinter den Bl\u00fctenbusch.", "tokens": ["Wie\u00b7der", "hin\u00b7ter", "den", "Bl\u00fc\u00b7ten\u00b7busch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Weil'! ich fliege dir nach! Warum entflohest du?", "tokens": ["Weil'", "!", "ich", "flie\u00b7ge", "dir", "nach", "!", "Wa\u00b7rum", "ent\u00b7flo\u00b7hest", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Pl\u00f6zlich lispelt der Strauch; Himmel! sie bebt hervor,", "tokens": ["Pl\u00f6z\u00b7lich", "lis\u00b7pelt", "der", "Strauch", ";", "Him\u00b7mel", "!", "sie", "bebt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Und es sch\u00fcttelt der Strauch ihr", "tokens": ["Und", "es", "sch\u00fct\u00b7telt", "der", "Strauch", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPOSAT"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Einen Regen von Bl\u00fcten nach.", "tokens": ["Ei\u00b7nen", "Re\u00b7gen", "von", "Bl\u00fc\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}