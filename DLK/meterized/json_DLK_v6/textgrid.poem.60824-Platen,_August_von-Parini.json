{"textgrid.poem.60824": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Parini", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6chst ehrw\u00fcrdig und gro\u00df zeigt Dante des alten Italiens", "tokens": ["H\u00f6chst", "ehr\u00b7w\u00fcr\u00b7dig", "und", "gro\u00df", "zeigt", "Dan\u00b7te", "des", "al\u00b7ten", "I\u00b7ta\u00b7li\u00b7ens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bild, und das mittlere zeigt lieblich und sch\u00f6n Ariost;", "tokens": ["Bild", ",", "und", "das", "mitt\u00b7le\u00b7re", "zeigt", "lieb\u00b7lich", "und", "sch\u00f6n", "A\u00b7riost", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "ADJA", "VVFIN", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Aber du maltest das neue, Parini! Wie sehr es gesunken,", "tokens": ["A\u00b7ber", "du", "mal\u00b7test", "das", "neu\u00b7e", ",", "Pa\u00b7ri\u00b7ni", "!", "Wie", "sehr", "es", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "$,", "NE", "$.", "PWAV", "ADV", "PPER", "VVPP", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Zeigt dein spielender, dein feiner und bei\u00dfender Spott.", "tokens": ["Zeigt", "dein", "spie\u00b7len\u00b7der", ",", "dein", "fei\u00b7ner", "und", "bei\u00b7\u00dfen\u00b7der", "Spott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "KON", "PIAT", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dient es zum Vorwurf dir, da\u00df dein Jahrhundert so klein war?", "tokens": ["Dient", "es", "zum", "Vor\u00b7wurf", "dir", ",", "da\u00df", "dein", "Jahr\u00b7hun\u00b7dert", "so", "klein", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "+--+---+-+--+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Eher zum Lobe! Du warst wirklicher Dichter der Zeit.", "tokens": ["E\u00b7her", "zum", "Lo\u00b7be", "!", "Du", "warst", "wirk\u00b7li\u00b7cher", "Dich\u00b7ter", "der", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "PPER", "VAFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}