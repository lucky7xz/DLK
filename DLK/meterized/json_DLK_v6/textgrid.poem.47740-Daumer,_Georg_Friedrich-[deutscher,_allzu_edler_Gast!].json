{"textgrid.poem.47740": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[deutscher, allzu edler Gast!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutscher, allzu edler Gast!", "tokens": ["Deut\u00b7scher", ",", "all\u00b7zu", "ed\u00b7ler", "Gast", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was in unsre H\u00fctte lenkst du?", "tokens": ["Was", "in", "uns\u00b7re", "H\u00fct\u00b7te", "lenkst", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nicht in unsrer Mitte senkst du", "tokens": ["Nicht", "in", "uns\u00b7rer", "Mit\u00b7te", "senkst", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein Gem\u00fcth in linde Rast.", "tokens": ["Dein", "Ge\u00b7m\u00fcth", "in", "lin\u00b7de", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht im Hofe magst du bleiben,", "tokens": ["Nicht", "im", "Ho\u00b7fe", "magst", "du", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Regen ist und Wind da drau\u00dfen;", "tokens": ["Re\u00b7gen", "ist", "und", "Wind", "da", "drau\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "'s qualmt im Innern Rauchgew\u00f6lke,", "tokens": ["'s", "qualmt", "im", "In\u00b7nern", "Rauch\u00b7ge\u00b7w\u00f6l\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht im Innern magst du hausen.", "tokens": ["Nicht", "im", "In\u00b7nern", "magst", "du", "hau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wei\u00dft du was? Zur H\u00f6lle fahre,", "tokens": ["Wei\u00dft", "du", "was", "?", "Zur", "H\u00f6l\u00b7le", "fah\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$.", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo man helle Flammen sch\u00fcrt,", "tokens": ["Wo", "man", "hel\u00b7le", "Flam\u00b7men", "sch\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Dort von allem Rauche bist du,", "tokens": ["Dort", "von", "al\u00b7lem", "Rau\u00b7che", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Allem Regen ungenirt.", "tokens": ["Al\u00b7lem", "Re\u00b7gen", "un\u00b7ge\u00b7nirt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}