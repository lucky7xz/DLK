, "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Duft der Rosen und Narzissen ...", "tokens": ["Vom", "Duft", "der", "Ro\u00b7sen", "und", "Nar\u00b7zis\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Denken war ein Morgenstrahl,", "tokens": ["Mein", "Den\u00b7ken", "war", "ein", "Mor\u00b7gen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entbrochen schwarzen Finsternissen \u2013", "tokens": ["Ent\u00b7bro\u00b7chen", "schwar\u00b7zen", "Fins\u00b7ter\u00b7nis\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich lebte! O mein M\u00fctterlein \u2013", "tokens": ["Ich", "leb\u00b7te", "!", "O", "mein", "M\u00fct\u00b7ter\u00b7lein", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ri\u00df, umspr\u00fcht von Freudenfunken,", "tokens": ["Und", "ri\u00df", ",", "um\u00b7spr\u00fcht", "von", "Freu\u00b7den\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sph\u00e4ren an mein Bruderherz,", "tokens": ["Die", "Sph\u00e4\u00b7ren", "an", "mein", "Bru\u00b7der\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN