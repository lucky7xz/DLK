"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die aber pre\u00dft mich fest und l\u00e4\u00dft", "tokens": ["Die", "a\u00b7ber", "pre\u00dft", "mich", "fest", "und", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich nicht aus ihren erznen Krallen \u2013", "tokens": ["Mich", "nicht", "aus", "ih\u00b7ren", "erz\u00b7nen", "Kral\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von Blut und Kot und Schwei\u00df gen\u00e4\u00dft,", "tokens": ["Von", "Blut", "und", "Kot", "und", "Schwei\u00df", "ge\u00b7n\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schleif' ich durchs Leben, fluchverfallen ... \u2013", "tokens": ["Schleif'", "ich", "durchs", "Le\u00b7ben", ",", "fluch\u00b7ver\u00b7fal\u00b7len", "...", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$,", "VVINF", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ja, Mutter, stirb! Und bist du tot,", "tokens": ["Ja", ",", "Mut\u00b7ter", ",", "stirb", "!", "Und", "bist", "du", "tot", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "VVIMP", "$.", "KON", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann wollen wir, ein seltsam P\u00e4rchen,", "tokens": ["Dann", "wol\u00b7len", "wir", ",", "ein", "selt\u00b7sam", "P\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Abend- bis zum Morgenrot", "tokens": ["Vom", "A\u00b7ben\u00b7d", "bis", "zum", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "TRUNC", "KON", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eins plaudern von dem tollen M\u00e4rchen,", "tokens": ["Eins", "plau\u00b7dern", "von", "dem", "tol\u00b7len", "M\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem mich das Schicksal auserw\u00e4hlt,", "tokens": ["Dem", "mich", "das", "Schick\u00b7sal", "au\u00b7ser\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich brav \u2013 recht brav drin auszuleben \u2013", "tokens": ["Mich", "brav", "\u2013", "recht", "brav", "drin", "aus\u00b7zu\u00b7le\u00b7ben", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$(", "ADJD", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und hab' ich's dir dann auserz\u00e4hlt,", "tokens": ["Und", "hab'", "ich's", "dir", "dann", "aus\u00b7er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hast du auch schweigend mir vergeben ...", "tokens": ["Hast", "du", "auch", "schwei\u00b7gend", "mir", "ver\u00b7ge\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Dann reck' ich hoch mein Haupt empor \u2013", "tokens": ["Dann", "reck'", "ich", "hoch", "mein", "Haupt", "em\u00b7por", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bei des Tages ersten Gr\u00fc\u00dfen", "tokens": ["Und", "bei", "des", "Ta\u00b7ges", "ers\u00b7ten", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schmei\u00df' ich den eklen Erdenstaub", "tokens": ["Schmei\u00df'", "ich", "den", "ek\u00b7len", "Er\u00b7dens\u00b7taub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von meinen wanderm\u00fcden F\u00fc\u00dfen ...", "tokens": ["Von", "mei\u00b7nen", "wan\u00b7der\u00b7m\u00fc\u00b7den", "F\u00fc\u00b7\u00dfen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es fliegt der Filz ins feuchte Gras,", "tokens": ["Es", "fliegt", "der", "Filz", "ins", "feuch\u00b7te", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich r\u00fcste mich zum letzten Traume \u2013", "tokens": ["Ich", "r\u00fcs\u00b7te", "mich", "zum", "letz\u00b7ten", "Trau\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zerbreche meinen Knotenstock", "tokens": ["Zer\u00b7bre\u00b7che", "mei\u00b7nen", "Kno\u00b7ten\u00b7stock"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und h\u00e4ng' mich auf am n\u00e4chsten Baume ... \u2013", "tokens": ["Und", "h\u00e4ng'", "mich", "auf", "am", "n\u00e4chs\u00b7ten", "Bau\u00b7me", "...", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPRART", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}