{"textgrid.poem.35531": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der verlorene Sohn", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein M\u00fctterlein, zu dieser Stund',", "tokens": ["Mein", "M\u00fct\u00b7ter\u00b7lein", ",", "zu", "die\u00b7ser", "Stund'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dieser Stund' in tiefer Nacht", "tokens": ["Zu", "die\u00b7ser", "Stund'", "in", "tie\u00b7fer", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bist du aus leisem, kurzem Schlaf", "tokens": ["Bist", "du", "aus", "lei\u00b7sem", ",", "kur\u00b7zem", "Schlaf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl j\u00e4hlings, j\u00e4hlings aufgewacht!", "tokens": ["Wohl", "j\u00e4h\u00b7lings", ",", "j\u00e4h\u00b7lings", "auf\u00b7ge\u00b7wacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du f\u00e4hrst empor und starrst und horchst;", "tokens": ["Du", "f\u00e4hrst", "em\u00b7por", "und", "starrst", "und", "horchst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und eine bange Ahnung schwirrt", "tokens": ["Und", "ei\u00b7ne", "ban\u00b7ge", "Ah\u00b7nung", "schwirrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dir durch die angstumschn\u00fcrte Brust:", "tokens": ["Dir", "durch", "die", "angs\u00b7tum\u00b7schn\u00fcr\u00b7te", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ruhelos dein Kind noch irrt ...", "tokens": ["Da\u00df", "ru\u00b7he\u00b7los", "dein", "Kind", "noch", "irrt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch irrt auf fernem, fremden Pfad,", "tokens": ["Noch", "irrt", "auf", "fer\u00b7nem", ",", "frem\u00b7den", "Pfad", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch irrt in sp\u00e4ter, schwarzer Nacht \u2013", "tokens": ["Noch", "irrt", "in", "sp\u00e4\u00b7ter", ",", "schwar\u00b7zer", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJD", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du aber wei\u00dft nicht seine Spur,", "tokens": ["Du", "a\u00b7ber", "wei\u00dft", "nicht", "sei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00dft nicht, was es so ruhlos macht ...", "tokens": ["Wei\u00dft", "nicht", ",", "was", "es", "so", "ruh\u00b7los", "macht", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wei\u00dft nur, da\u00df es aus dieser Not", "tokens": ["Wei\u00dft", "nur", ",", "da\u00df", "es", "aus", "die\u00b7ser", "Not"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Mutterliebe einzig risse,", "tokens": ["Die", "Mut\u00b7ter\u00b7lie\u00b7be", "ein\u00b7zig", "ris\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und m\u00f6chtest wohl es suchen gehn", "tokens": ["Und", "m\u00f6ch\u00b7test", "wohl", "es", "su\u00b7chen", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch schwarze, schwarze Finsternisse ...", "tokens": ["Durch", "schwar\u00b7ze", ",", "schwar\u00b7ze", "Fins\u00b7ter\u00b7nis\u00b7se", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein M\u00fctterlein, dein armes Kind,", "tokens": ["Mein", "M\u00fct\u00b7ter\u00b7lein", ",", "dein", "ar\u00b7mes", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sucht dich nicht in seinen Aengsten,", "tokens": ["Es", "sucht", "dich", "nicht", "in", "sei\u00b7nen", "A\u00b7engs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es taumelt durch die Nebelnacht,", "tokens": ["Es", "tau\u00b7melt", "durch", "die", "Ne\u00b7bel\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geschleift von seines D\u00e4mons Hengsten.", "tokens": ["Ge\u00b7schleift", "von", "sei\u00b7nes", "D\u00e4\u00b7mons", "Hengs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hei! Wie es brennt in seiner Brust!", "tokens": ["Hei", "!", "Wie", "es", "brennt", "in", "sei\u00b7ner", "Brust", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wie schn\u00fcrt's die Kehle ihm zusammen!", "tokens": ["Wie", "schn\u00fcrt's", "die", "Keh\u00b7le", "ihm", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O Mutter, deine milde Hand", "tokens": ["O", "Mut\u00b7ter", ",", "dei\u00b7ne", "mil\u00b7de", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Beschwor mir nicht die Wahnsinnsflammen.", "tokens": ["Be\u00b7schwor", "mir", "nicht", "die", "Wahn\u00b7sinns\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein M\u00fctterlein, la\u00df ab, la\u00df ab!", "tokens": ["Mein", "M\u00fct\u00b7ter\u00b7lein", ",", "la\u00df", "ab", ",", "la\u00df", "ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das du in Schmerzen einst geboren,", "tokens": ["Das", "du", "in", "Schmer\u00b7zen", "einst", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Kind, du hast es einmal doch", "tokens": ["Dein", "Kind", ",", "du", "hast", "es", "ein\u00b7mal", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An diesem Tage \u2013 ach, verloren!", "tokens": ["An", "die\u00b7sem", "Ta\u00b7ge", "\u2013", "ach", ",", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$(", "XY", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es fragt nichts mehr nach deiner Lust \u2013", "tokens": ["Es", "fragt", "nichts", "mehr", "nach", "dei\u00b7ner", "Lust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es fragt nichts mehr nach deinem Kummer,", "tokens": ["Es", "fragt", "nichts", "mehr", "nach", "dei\u00b7nem", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "An seiner Leidenschaft Brust", "tokens": ["An", "sei\u00b7ner", "Lei\u00b7den\u00b7schaft", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Erw\u00fcrgt es deiner N\u00e4chte Schlummer ...", "tokens": ["Er\u00b7w\u00fcrgt", "es", "dei\u00b7ner", "N\u00e4ch\u00b7te", "Schlum\u00b7mer", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein M\u00fctterlein, wenn's dich verzehrt,", "tokens": ["Mein", "M\u00fct\u00b7ter\u00b7lein", ",", "wenn's", "dich", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du dein Kind hast lassen m\u00fcssen,", "tokens": ["Da\u00df", "du", "dein", "Kind", "hast", "las\u00b7sen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann ruh dich auf der Bahre aus", "tokens": ["Dann", "ruh", "dich", "auf", "der", "Bah\u00b7re", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von deines Lebens K\u00fcmmernissen ...", "tokens": ["Von", "dei\u00b7nes", "Le\u00b7bens", "K\u00fcm\u00b7mer\u00b7nis\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann schlie\u00df die m\u00fcden Augen zu,", "tokens": ["Dann", "schlie\u00df", "die", "m\u00fc\u00b7den", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die oft um mich in Tr\u00e4nen lagen \u2013", "tokens": ["Die", "oft", "um", "mich", "in", "Tr\u00e4\u00b7nen", "la\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann la\u00df zur allerletzten Ruh'", "tokens": ["Dann", "la\u00df", "zur", "al\u00b7ler\u00b7letz\u00b7ten", "Ruh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dich heimlich auf den Kirchhof tragen ...", "tokens": ["Dich", "heim\u00b7lich", "auf", "den", "Kirch\u00b7hof", "tra\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vielleicht bin ich des Wanderns m\u00fcd,", "tokens": ["Viel\u00b7leicht", "bin", "ich", "des", "Wan\u00b7derns", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ist die Unrast all' verlodert \u2013", "tokens": ["Und", "ist", "die", "Un\u00b7rast", "all'", "ver\u00b7lo\u00b7dert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht, da\u00df dann mein Schicksal mich", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "dann", "mein", "Schick\u00b7sal", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADV", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort rasten l\u00e4\u00dft, wo du vermodert ...", "tokens": ["Dort", "ras\u00b7ten", "l\u00e4\u00dft", ",", "wo", "du", "ver\u00b7mo\u00b7dert", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann sind wir beide ganz allein,", "tokens": ["Dann", "sind", "wir", "bei\u00b7de", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und unsre Liebe darf nicht s\u00e4umen \u2013 \u2013", "tokens": ["Und", "uns\u00b7re", "Lie\u00b7be", "darf", "nicht", "s\u00e4u\u00b7men", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann will ich meines Lebens Traum", "tokens": ["Dann", "will", "ich", "mei\u00b7nes", "Le\u00b7bens", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit dir noch einmal still durchtr\u00e4umen.", "tokens": ["Mit", "dir", "noch", "ein\u00b7mal", "still", "durch\u00b7tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann will ich alles dir gestehn \u2013", "tokens": ["Dann", "will", "ich", "al\u00b7les", "dir", "ge\u00b7stehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Schuld auf Schuld sich lud, dir sagen \u2013", "tokens": ["Wie", "Schuld", "auf", "Schuld", "sich", "lud", ",", "dir", "sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "PRF", "VVFIN", "$,", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann will ich mit dir heimw\u00e4rts gehn", "tokens": ["Dann", "will", "ich", "mit", "dir", "heim\u00b7w\u00e4rts", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meines Lebens ersten Tagen ...", "tokens": ["Zu", "mei\u00b7nes", "Le\u00b7bens", "ers\u00b7ten", "Ta\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein totes M\u00fctterlein, dann gibt", "tokens": ["Mein", "to\u00b7tes", "M\u00fct\u00b7ter\u00b7lein", ",", "dann", "gibt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es nichts, was dir verborgen bliebe \u2013", "tokens": ["Es", "nichts", ",", "was", "dir", "ver\u00b7bor\u00b7gen", "blie\u00b7be", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PWS", "PPER", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann wei\u00dft du, wie ich dich geliebt", "tokens": ["Dann", "wei\u00dft", "du", ",", "wie", "ich", "dich", "ge\u00b7liebt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und doch verraten deine Liebe!", "tokens": ["Und", "doch", "ver\u00b7ra\u00b7ten", "dei\u00b7ne", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dann wei\u00dft du, wie es pl\u00f6tzlich mich", "tokens": ["Dann", "wei\u00dft", "du", ",", "wie", "es", "pl\u00f6tz\u00b7lich", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit hei\u00dfem Atem angepfiffen \u2013", "tokens": ["Mit", "hei\u00b7\u00dfem", "A\u00b7tem", "an\u00b7ge\u00b7pfif\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie es in meine Seele schlug,", "tokens": ["Wie", "es", "in", "mei\u00b7ne", "See\u00b7le", "schlug", ","], "token_info": ["word", "word", 