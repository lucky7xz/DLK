{"textgrid.poem.63972": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Eine Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein priesterlich Wort gab uns eigene Rechte,", "tokens": ["Kein", "pries\u00b7ter\u00b7lich", "Wort", "gab", "uns", "ei\u00b7ge\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Keiner Orgel Brust sprach kr\u00e4ftigen Segen,", "tokens": ["Kei\u00b7ner", "Or\u00b7gel", "Brust", "sprach", "kr\u00e4f\u00b7ti\u00b7gen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "In weichem Golde heilig nicht regten", "tokens": ["In", "wei\u00b7chem", "Gol\u00b7de", "hei\u00b7lig", "nicht", "reg\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKNEG", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich Kerzen, \u2013 uns schwiegen die weihenden M\u00e4chte.", "tokens": ["Sich", "Ker\u00b7zen", ",", "\u2013", "uns", "schwie\u00b7gen", "die", "wei\u00b7hen\u00b7den", "M\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "$,", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wir haben uns selber zusammen gefunden", "tokens": ["Wir", "ha\u00b7ben", "uns", "sel\u00b7ber", "zu\u00b7sam\u00b7men", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "In einsamen, k\u00fchn ergl\u00fchenden Stunden,", "tokens": ["In", "ein\u00b7sa\u00b7men", ",", "k\u00fchn", "er\u00b7gl\u00fc\u00b7hen\u00b7den", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Du mein Lieb von einst, du mein Lieb f\u00fcr immer.", "tokens": ["Du", "mein", "Lieb", "von", "einst", ",", "du", "mein", "Lieb", "f\u00fcr", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "ADV", "$,", "PPER", "PPOSAT", "NN", "APPR", "ADV", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.2": {"line.1": {"text": "Kein Schleier hat \u00fcber der Feier gehangen,", "tokens": ["Kein", "Schlei\u00b7er", "hat", "\u00fc\u00b7ber", "der", "Fei\u00b7er", "ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Myrthe nicht knapp umgr\u00fcnt deine Flechten,", "tokens": ["Die", "Myr\u00b7the", "nicht", "knapp", "um\u00b7gr\u00fcnt", "dei\u00b7ne", "Flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kein Gebetbuch geruht in deiner Rechten,", "tokens": ["Kein", "Ge\u00b7bet\u00b7buch", "ge\u00b7ruht", "in", "dei\u00b7ner", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In sch\u00e4miger Glut nicht standen die Wangen.", "tokens": ["In", "sch\u00e4\u00b7mi\u00b7ger", "Glut", "nicht", "stan\u00b7den", "die", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Keine Rose im eigenen Tau wie ein M\u00e4rchen,", "tokens": ["Kei\u00b7ne", "Ro\u00b7se", "im", "ei\u00b7ge\u00b7nen", "Tau", "wie", "ein", "M\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "ADJA", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Kein Papa im Schwips, keine Mama im Z\u00e4hrchen,", "tokens": ["Kein", "Pa\u00b7pa", "im", "Schwips", ",", "kei\u00b7ne", "Ma\u00b7ma", "im", "Z\u00e4hr\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "--+-+---+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Nur wir, du mein Lieb, du mein Lieb f\u00fcr immer.", "tokens": ["Nur", "wir", ",", "du", "mein", "Lieb", ",", "du", "mein", "Lieb", "f\u00fcr", "im\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "APPR", "ADV", "$."], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Kein Vergi\u00dfmeinnichtbach sind jene Zeiten,", "tokens": ["Kein", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht\u00b7bach", "sind", "je\u00b7ne", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Kein Hirtenlied bei bl\u00f6der Fl\u00f6te,", "tokens": ["Kein", "Hir\u00b7ten\u00b7lied", "bei", "bl\u00f6\u00b7der", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, bebend und blutend stand alles in R\u00f6te,", "tokens": ["Nein", ",", "be\u00b7bend", "und", "blu\u00b7tend", "stand", "al\u00b7les", "in", "R\u00f6\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVPP", "KON", "ADJD", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "----+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Uns ri\u00df zueinander verzweifeltes Streiten,", "tokens": ["Uns", "ri\u00df", "zu\u00b7ei\u00b7nan\u00b7der", "ver\u00b7zwei\u00b7fel\u00b7tes", "Strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-----+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und Hader und Qualen, kampfdurchblutete,", "tokens": ["Und", "Ha\u00b7der", "und", "Qua\u00b7len", ",", "kampf\u00b7durch\u00b7blu\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und Liebe, von innigen Lippen durchglutete,", "tokens": ["Und", "Lie\u00b7be", ",", "von", "in\u00b7ni\u00b7gen", "Lip\u00b7pen", "durch\u00b7glu\u00b7te\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.7": {"text": "Du mein Lieb von einst, du mein Lieb f\u00fcr immer.", "tokens": ["Du", "mein", "Lieb", "von", "einst", ",", "du", "mein", "Lieb", "f\u00fcr", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "ADV", "$,", "PPER", "PPOSAT", "NN", "APPR", "ADV", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.4": {"line.1": {"text": "Und k\u00e4me ein Engel im wei\u00dfen Gewande,", "tokens": ["Und", "k\u00e4\u00b7me", "ein", "En\u00b7gel", "im", "wei\u00b7\u00dfen", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und k\u00e4me der wei\u00dfe Engel gegangen,", "tokens": ["Und", "k\u00e4\u00b7me", "der", "wei\u00b7\u00dfe", "En\u00b7gel", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Unschuldige R\u00f6te auf tr\u00e4umenden Wangen,", "tokens": ["Un\u00b7schul\u00b7di\u00b7ge", "R\u00f6\u00b7te", "auf", "tr\u00e4u\u00b7men\u00b7den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und f\u00e4nde uns beide stehen im Leide", "tokens": ["Und", "f\u00e4n\u00b7de", "uns", "bei\u00b7de", "ste\u00b7hen", "im", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIS", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und st\u00e4nd' und spr\u00e4che: \u00bbIch nehme die Seele", "tokens": ["Und", "st\u00e4nd'", "und", "spr\u00e4\u00b7che", ":", "\u00bb", "Ich", "neh\u00b7me", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von euch, was vergangen, die brennende Fehle \u2013", "tokens": ["Von", "euch", ",", "was", "ver\u00b7gan\u00b7gen", ",", "die", "bren\u00b7nen\u00b7de", "Feh\u00b7le", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "VVPP", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Dir und dem Lieb von einst und von immer!\u00ab", "tokens": ["Dir", "und", "dem", "Lieb", "von", "einst", "und", "von", "im\u00b7mer", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "KON", "ART", "NN", "APPR", "ADV", "KON", "APPR", "ADV", "$.", "$("], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.5": {"line.1": {"text": "\u00bbhalt, Engel, halt ein! Die Hand von dem Leben,", "tokens": ["\u00bb", "halt", ",", "En\u00b7gel", ",", "halt", "ein", "!", "Die", "Hand", "von", "dem", "Le\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NE", "$,", "VVFIN", "PTKVZ", "$.", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das uns in hei\u00dfer Leidenschaft gl\u00fchte,", "tokens": ["Das", "uns", "in", "hei\u00b7\u00dfer", "Lei\u00b7den\u00b7schaft", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Scharlach im pr\u00e4chtig lohen Gem\u00fcte,", "tokens": ["Ein", "Schar\u00b7lach", "im", "pr\u00e4ch\u00b7tig", "lo\u00b7hen", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit schroffem Zorn und innigem Beben.", "tokens": ["Mit", "schrof\u00b7fem", "Zorn", "und", "in\u00b7ni\u00b7gem", "Be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Ins graue Heute sehen die Tage", "tokens": ["Ins", "grau\u00b7e", "Heu\u00b7te", "se\u00b7hen", "die", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie eine Nordlicht blutende Sage,", "tokens": ["Wie", "ei\u00b7ne", "Nord\u00b7licht", "blu\u00b7ten\u00b7de", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Du mein Lieb von einst, du mein Lieb von immer!\u00ab", "tokens": ["Du", "mein", "Lieb", "von", "einst", ",", "du", "mein", "Lieb", "von", "im\u00b7mer", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "ADV", "$,", "PPER", "PPOSAT", "NN", "APPR", "ADV", "$.", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}}}}