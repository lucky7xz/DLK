{"dta.poem.7408": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Gebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Brause, Gott, mit Sturmesodem durch die f\u00fcrchterliche Stille,", "tokens": ["Brau\u00b7se", ",", "Gott", ",", "mit", "Stur\u00b7me\u00b7so\u00b7dem", "durch", "die", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Stil\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Gib ein Trauerspiel der Freiheit f\u00fcr der Sklaverei Idylle;", "tokens": ["Gib", "ein", "Trau\u00b7er\u00b7spiel", "der", "Frei\u00b7heit", "f\u00fcr", "der", "Skla\u00b7ve\u00b7rei", "I\u00b7dyl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "La\u00df das Herz doch wieder schlagen in der Brust der kalten Welt,", "tokens": ["La\u00df", "das", "Herz", "doch", "wie\u00b7der", "schla\u00b7gen", "in", "der", "Brust", "der", "kal\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und erweck' ihr einen R\u00e4cher, und erweck' ihr einen Held!", "tokens": ["Und", "er\u00b7weck'", "ihr", "ei\u00b7nen", "R\u00e4\u00b7cher", ",", "und", "er\u00b7weck'", "ihr", "ei\u00b7nen", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Wenn sie in der eignen Heimat frei zu leben uns nicht g\u00f6nnen,", "tokens": ["Wenn", "sie", "in", "der", "eig\u00b7nen", "Hei\u00b7mat", "frei", "zu", "le\u00b7ben", "uns", "nicht", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schaff' uns eine gr\u00fcne Insel, wo wir frei noch ", "tokens": ["Schaff'", "uns", "ei\u00b7ne", "gr\u00fc\u00b7ne", "In\u00b7sel", ",", "wo", "wir", "frei", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADJD", "ADV"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sterben k\u00f6nnen froh und freudig in der frischen freien Luft", "tokens": ["Ster\u00b7ben", "k\u00f6n\u00b7nen", "froh", "und", "freu\u00b7dig", "in", "der", "fri\u00b7schen", "frei\u00b7en", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADJD", "KON", "ADJD", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und uns selbst die Rosen tr\u00e4ufeln aus den Wunden auf die Gruft!", "tokens": ["Und", "uns", "selbst", "die", "Ro\u00b7sen", "tr\u00e4u\u00b7feln", "aus", "den", "Wun\u00b7den", "auf", "die", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Aus dem Nachtmalkelch der Freiheit la\u00df uns wieder einmal schl\u00fcrfen,", "tokens": ["Aus", "dem", "Nacht\u00b7mal\u00b7kelch", "der", "Frei\u00b7heit", "la\u00df", "uns", "wie\u00b7der", "ein\u00b7mal", "schl\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVIMP", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Baue wieder einen Altar, drauf wir uns dir opfern d\u00fcrfen,", "tokens": ["Bau\u00b7e", "wie\u00b7der", "ei\u00b7nen", "Al\u00b7tar", ",", "drauf", "wir", "uns", "dir", "op\u00b7fern", "d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PAV", "PPER", "PPER", "PPER", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Breite vor uns einen Wahlplatz, einen Platz der V\u00f6lkerwahl,", "tokens": ["Brei\u00b7te", "vor", "uns", "ei\u00b7nen", "Wahl\u00b7platz", ",", "ei\u00b7nen", "Platz", "der", "V\u00f6l\u00b7ker\u00b7wahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aus dem Kerker, aus der Scheide sehnt sich wieder unser Stahl!", "tokens": ["Aus", "dem", "Ker\u00b7ker", ",", "aus", "der", "Schei\u00b7de", "sehnt", "sich", "wie\u00b7der", "un\u00b7ser", "Stahl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Ach! um jenes Sturms Verheissung hat der Frieden uns betrogen,", "tokens": ["Ach", "!", "um", "je\u00b7nes", "Sturms", "Ver\u00b7heis\u00b7sung", "hat", "der", "Frie\u00b7den", "uns", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "PDAT", "NN", "NN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und das goldne Schiff der Hoffnung, das als Wiege in die Wogen", "tokens": ["Und", "das", "gold\u00b7ne", "Schiff", "der", "Hoff\u00b7nung", ",", "das", "als", "Wie\u00b7ge", "in", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "KOUS", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Unter Klang und Sang gesteuert und so reiche Sch\u00e4tze barg,", "tokens": ["Un\u00b7ter", "Klang", "und", "Sang", "ge\u00b7steu\u00b7ert", "und", "so", "rei\u00b7che", "Sch\u00e4t\u00b7ze", "barg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "KON", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ruht gescheitert, schwarzbewimpelt in dem Hafen jetzt, ein Sarg.", "tokens": ["Ruht", "ge\u00b7schei\u00b7tert", ",", "schwarz\u00b7be\u00b7wim\u00b7pelt", "in", "dem", "Ha\u00b7fen", "jetzt", ",", "ein", "Sarg", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Will mein Volk nun ewig klagend dieses morsche Wrack umstehen?", "tokens": ["Will", "mein", "Volk", "nun", "e\u00b7wig", "kla\u00b7gend", "die\u00b7ses", "mor\u00b7sche", "Wrack", "um\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Soll in thatenlosen Seufzern seine beste Kraft verwehen?", "tokens": ["Soll", "in", "tha\u00b7ten\u00b7lo\u00b7sen", "Seuf\u00b7zern", "sei\u00b7ne", "bes\u00b7te", "Kraft", "ver\u00b7we\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Donnert nie durch seinen Himmel der Entscheidung scharfer Ton?", "tokens": ["Don\u00b7nert", "nie", "durch", "sei\u00b7nen", "Him\u00b7mel", "der", "Ent\u00b7schei\u00b7dung", "schar\u00b7fer", "Ton", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wahrlich ein Despote zaudert nicht so lang am Rubikon!", "tokens": ["Wahr\u00b7lich", "ein", "Des\u00b7po\u00b7te", "zau\u00b7dert", "nicht", "so", "lang", "am", "Ru\u00b7bi\u00b7kon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Glaubet ihr, der Frieden werd' euch f\u00fcr des Hauses Freude b\u00fcrgen?", "tokens": ["Glau\u00b7bet", "ihr", ",", "der", "Frie\u00b7den", "werd'", "euch", "f\u00fcr", "des", "Hau\u00b7ses", "Freu\u00b7de", "b\u00fcr\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nur vernichten kann der Krieg uns, solch ein Frieden wird uns w\u00fcrgen!", "tokens": ["Nur", "ver\u00b7nich\u00b7ten", "kann", "der", "Krieg", "uns", ",", "solch", "ein", "Frie\u00b7den", "wird", "uns", "w\u00fcr\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "ART", "NN", "PPER", "$,", "PIAT", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "In dem wilden Kampfgew\u00fchle mag es wohl ihr werden hei\u00df,", "tokens": ["In", "dem", "wil\u00b7den", "Kampf\u00b7ge\u00b7w\u00fch\u00b7le", "mag", "es", "wohl", "ihr", "wer\u00b7den", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aber ", "tokens": ["A\u00b7ber"], "token_info": ["word"], "pos": ["KON"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "So ihr nicht begie\u00dft die Pflanze, wird sie allgemach verk\u00fcmmern,", "tokens": ["So", "ihr", "nicht", "be\u00b7gie\u00dft", "die", "Pflan\u00b7ze", ",", "wird", "sie", "all\u00b7ge\u00b7mach", "ver\u00b7k\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$,", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "So ihr nicht gebraucht den Degen, wird ihn schnell der Rost zertr\u00fcmmern;", "tokens": ["So", "ihr", "nicht", "ge\u00b7braucht", "den", "De\u00b7gen", ",", "wird", "ihn", "schnell", "der", "Rost", "zer\u00b7tr\u00fcm\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "VVPP", "ART", "NN", "$,", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Eine Ader sich zu \u00f6ffnen f\u00fcr die Freiheit, w\u00e4re gut,", "tokens": ["Ei\u00b7ne", "A\u00b7der", "sich", "zu", "\u00f6ff\u00b7nen", "f\u00fcr", "die", "Frei\u00b7heit", ",", "w\u00e4\u00b7re", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sonsten zweifeln die Tyrannen an der V\u00f6lker reinem Blut.", "tokens": ["Sons\u00b7ten", "zwei\u00b7feln", "die", "Ty\u00b7ran\u00b7nen", "an", "der", "V\u00f6l\u00b7ker", "rei\u00b7nem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Aber wollen mich die M\u00e4nner nicht verstehn, die schwerverirrten,", "tokens": ["A\u00b7ber", "wol\u00b7len", "mich", "die", "M\u00e4n\u00b7ner", "nicht", "ver\u00b7stehn", ",", "die", "schwer\u00b7ve\u00b7rirr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "NN", "PTKNEG", "VVINF", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "O so h\u00f6ret ", "tokens": ["O", "so", "h\u00f6\u00b7ret"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Traget Ihr ein Schwert in Myrten; denn mich d\u00fcnket, Frau und Frei,", "tokens": ["Tra\u00b7get", "Ihr", "ein", "Schwert", "in", "Myr\u00b7ten", ";", "denn", "mich", "d\u00fcn\u00b7ket", ",", "Frau", "und", "Frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$.", "KON", "PPER", "VVFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nicht so fremd einander klingen diese Worte, diese Zwei!", "tokens": ["Nicht", "so", "fremd", "ein\u00b7an\u00b7der", "klin\u00b7gen", "die\u00b7se", "Wor\u00b7te", ",", "die\u00b7se", "Zwei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "ADV", "VVFIN", "PDAT", "NN", "$,", "PDAT", "CARD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}