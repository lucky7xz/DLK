{"textgrid.poem.66865": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie das flirrt und schwirrt und schmeichelt,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie das flirrt und schwirrt und schmeichelt,", "tokens": ["Wie", "das", "flirrt", "und", "schwirrt", "und", "schmei\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockend girrt, gef\u00e4llig streichelt!", "tokens": ["Lo\u00b7ckend", "girrt", ",", "ge\u00b7f\u00e4l\u00b7lig", "strei\u00b7chelt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbfr\u00e4ulein sprudeln von Geschmack.", "tokens": ["\u00bb", "fr\u00e4u\u00b7lein", "spru\u00b7deln", "von", "Ge\u00b7schmack", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwar, wem gnadenvoll wie Ihnen", "tokens": ["Zwar", ",", "wem", "gna\u00b7den\u00b7voll", "wie", "Ih\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "ADJD", "KOKOM", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00f6ttin Sch\u00f6nheit selbst erschienen \u2013\u00ab", "tokens": ["G\u00f6t\u00b7tin", "Sch\u00f6n\u00b7heit", "selbst", "er\u00b7schie\u00b7nen", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "ADV", "VVINF", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbdanke, Herr von Chapeau Claque!\u00ab", "tokens": ["\u00bb", "dan\u00b7ke", ",", "Herr", "von", "Cha\u00b7pe\u00b7au", "Cla\u00b7que", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "APPR", "NE", "NE", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbdiese Palmen, wahrhaft s\u00fcdlich!", "tokens": ["\u00bb", "die\u00b7se", "Pal\u00b7men", ",", "wahr\u00b7haft", "s\u00fcd\u00b7lich", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schneidige Nischen! Urgem\u00fctlich!", "tokens": ["Schnei\u00b7di\u00b7ge", "Ni\u00b7schen", "!", "Ur\u00b7ge\u00b7m\u00fct\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ganz pomp\u00f6ser Gartensaal!", "tokens": ["Ganz", "pom\u00b7p\u00f6\u00b7ser", "Gar\u00b7ten\u00b7saal", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sauerherings Soireen,", "tokens": ["Sau\u00b7er\u00b7he\u00b7rings", "Soi\u00b7re\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fr\u00e4ulein m\u00f6gen selbst gestehen,", "tokens": ["Fr\u00e4u\u00b7lein", "m\u00f6\u00b7gen", "selbst", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind entschieden ideal.\u00ab", "tokens": ["Sind", "ent\u00b7schie\u00b7den", "i\u00b7deal", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Vorgebeugt auf samtnem Sockel,", "tokens": ["Vor\u00b7ge\u00b7beugt", "auf", "samt\u00b7nem", "So\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr Assessor von Monockel", "tokens": ["Herr", "As\u00b7ses\u00b7sor", "von", "Mo\u00b7no\u00b7ckel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Wie ein Gockel nickt und kr\u00e4ht.", "tokens": ["Wie", "ein", "Go\u00b7ckel", "nickt", "und", "kr\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine sch\u00f6nbeschleiften Halbschuh,", "tokens": ["Sei\u00b7ne", "sch\u00f6n\u00b7be\u00b7schleif\u00b7ten", "Halb\u00b7schuh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Seine idealen Kalbschuh", "tokens": ["Sei\u00b7ne", "i\u00b7dea\u00b7len", "Kalb\u00b7schuh"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Sind von Siegesrausch gebl\u00e4ht.", "tokens": ["Sind", "von", "Sie\u00b7ges\u00b7rausch", "ge\u00b7bl\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbsagen Sie mir doch, Herr Doktor,", "tokens": ["\u00bb", "sa\u00b7gen", "Sie", "mir", "doch", ",", "Herr", "Dok\u00b7tor", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPER", "ADV", "$,", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dichten Sie? So'n sch\u00f6ngelockter", "tokens": ["Dich\u00b7ten", "Sie", "?", "So'n", "sch\u00f6n\u00b7ge\u00b7lock\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$.", "NN", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Dichter machte mir mal Spa\u00df.", "tokens": ["Dich\u00b7ter", "mach\u00b7te", "mir", "mal", "Spa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz wie Schiller auf den Bildern,", "tokens": ["Ganz", "wie", "Schil\u00b7ler", "auf", "den", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O das w\u00e4re zum Verwildern! ...", "tokens": ["O", "das", "w\u00e4\u00b7re", "zum", "Ver\u00b7wil\u00b7dern", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PDS", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00f6gen Sie kein Ananas?\u00ab", "tokens": ["M\u00f6\u00b7gen", "Sie", "kein", "A\u00b7na\u00b7nas", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Und die geniale Glatze", "tokens": ["Und", "die", "ge\u00b7ni\u00b7a\u00b7le", "Glat\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Faltet die feudale Fratze,", "tokens": ["Fal\u00b7tet", "die", "feu\u00b7da\u00b7le", "Frat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "W\u00e4hrend sie voll Wehmut spricht:", "tokens": ["W\u00e4h\u00b7rend", "sie", "voll", "Weh\u00b7mut", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbgn\u00e4diges Fr\u00e4ulein, Verse machen", "tokens": ["\u00bb", "gn\u00e4\u00b7di\u00b7ges", "Fr\u00e4u\u00b7lein", ",", "Ver\u00b7se", "ma\u00b7chen"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJA", "NN", "$,", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ginge schon mit Ach und Krachen,", "tokens": ["Gin\u00b7ge", "schon", "mit", "Ach", "und", "Kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber Locken \u2013 das geht nicht!\u00ab", "tokens": ["A\u00b7ber", "Lo\u00b7cken", "\u2013", "das", "geht", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "$(", "PDS", "VVFIN", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}