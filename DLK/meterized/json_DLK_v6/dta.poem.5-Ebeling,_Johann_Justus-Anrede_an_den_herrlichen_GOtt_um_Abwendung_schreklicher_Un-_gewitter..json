{"dta.poem.5": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Anrede an den herrlichen GOtt  \n um Abwendung schreklicher Un-  \n gewitter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herrlich bist du, grosser GOtt! in\ndem Himmel und auf Erden,", "tokens": ["Herr\u00b7lich", "bist", "du", ",", "gros\u00b7ser", "Gott", "!", "in", "dem", "Him\u00b7mel", "und", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADJA", "NN", "$.", "APPR", "ART", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Dort bei denen Seraphinen sieht", "tokens": ["Dort", "bei", "de\u00b7nen", "Se\u00b7ra\u00b7phi\u00b7nen", "sieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PRELS", "NE", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und durch deine Kreatur must du", "tokens": ["Und", "durch", "dei\u00b7ne", "Kre\u00b7a\u00b7tur", "must", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VMFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Weil der Himmel, Sonn, Gestirne, Erd und al-", "tokens": ["Weil", "der", "Him\u00b7mel", ",", "Sonn", ",", "Ge\u00b7stir\u00b7ne", ",", "Erd", "und", "al"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Was wir mit dem Augen sehn, was wir mit dem", "tokens": ["Was", "wir", "mit", "dem", "Au\u00b7gen", "sehn", ",", "was", "wir", "mit", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVINF", "$,", "PRELS", "PPER", "APPR", "ART"], "meter": "-+--+-+----", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mu\u00df dich Sch\u00f6pfer der Natur! herrlich uns ins", "tokens": ["Mu\u00df", "dich", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", "!", "herr\u00b7lich", "uns", "ins"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "ART", "NN", "$.", "ADJD", "PPER", "APPRART"], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "Deine Wetter, strenger GOtt! die der Luft-\nkreis in sich schliesset", "tokens": ["Dei\u00b7ne", "Wet\u00b7ter", ",", "stren\u00b7ger", "Gott", "!", "die", "der", "Luft", "kreis", "in", "sich", "schlies\u00b7set"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "ART", "ART", "TRUNC", "NN", "APPR", "PRF", "VVFIN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Als das R\u00fcsthaus deines Grimmes, kommen uns", "tokens": ["Als", "das", "R\u00fcst\u00b7haus", "dei\u00b7nes", "Grim\u00b7mes", ",", "kom\u00b7men", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Wenn dein Arm das Hagel-Korn, auf die gr\u00fc-", "tokens": ["Wenn", "dein", "Arm", "das", "Ha\u00b7gel\u00b7Korn", ",", "auf", "die", "gr\u00fc"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wenn der Bliz das Auge blendet und der Donner", "tokens": ["Wenn", "der", "Bliz", "das", "Au\u00b7ge", "blen\u00b7det", "und", "der", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Mit der Schreckens-Stimme br\u00fcllt: So mu\u00df bei", "tokens": ["Mit", "der", "Schre\u00b7ckens\u00b7Stim\u00b7me", "br\u00fcllt", ":", "So", "mu\u00df", "bei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "ADV", "VMFIN", "APPR"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Auch ein Felsenhartes Herz, ", "tokens": ["Auch", "ein", "Fel\u00b7sen\u00b7har\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schone uns mit diesem Thon, denn wir ken-\nnen schon die Stimme,", "tokens": ["Scho\u00b7ne", "uns", "mit", "die\u00b7sem", "Thon", ",", "denn", "wir", "ken", "nen", "schon", "die", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,", "KON", "PPER", "TRUNC", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Die mit drohenden Get\u00f6\u00dfen uns vor Angst zu Bo-", "tokens": ["Die", "mit", "dro\u00b7hen\u00b7den", "Ge\u00b7t\u00f6\u00b7\u00dfen", "uns", "vor", "Angst", "zu", "Bo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "APPR", "NN", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Wir erbeben vor dem Zorn, und vergehn vor dei-", "tokens": ["Wir", "er\u00b7be\u00b7ben", "vor", "dem", "Zorn", ",", "und", "ver\u00b7gehn", "vor", "dei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVINF", "APPR", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Davor selbst der Himmel fliehet, und sich Erd und", "tokens": ["Da\u00b7vor", "selbst", "der", "Him\u00b7mel", "flie\u00b7het", ",", "und", "sich", "Erd", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ART", "NN", "VVFIN", "$,", "KON", "PRF", "NN", "KON"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Strekke deinen Allmachts-Arm, aus mit g\u00fctigen", "tokens": ["Strek\u00b7ke", "dei\u00b7nen", "All\u00b7machts\u00b7Arm", ",", "aus", "mit", "g\u00fc\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPR", "APPR", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und la\u00df deine Gnadenstimme h\u00f6ren den beklomnen", "tokens": ["Und", "la\u00df", "dei\u00b7ne", "Gna\u00b7den\u00b7stim\u00b7me", "h\u00f6\u00b7ren", "den", "be\u00b7klom\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Gib uns in den feuchten Thau auf dem Felde\nals in Spiegeln,", "tokens": ["Gib", "uns", "in", "den", "feuch\u00b7ten", "Thau", "auf", "dem", "Fel\u00b7de", "als", "in", "Spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Deine G\u00fcte anzusehen; gib uns warmen Sonnen-", "tokens": ["Dei\u00b7ne", "G\u00fc\u00b7te", "an\u00b7zu\u00b7se\u00b7hen", ";", "gib", "uns", "war\u00b7men", "Son\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVIZU", "$.", "VVIMP", "PPER", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "La\u00df die Luft die Hagel droht, diesen Sommer fest", "tokens": ["La\u00df", "die", "Luft", "die", "Ha\u00b7gel", "droht", ",", "die\u00b7sen", "Som\u00b7mer", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ART", "NN", "VVFIN", "$,", "PDAT", "NN", "PTKVZ"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Zeig uns deiner Allmacht Wunder, und la\u00df alles", "tokens": ["Zeig", "uns", "dei\u00b7ner", "All\u00b7macht", "Wun\u00b7der", ",", "und", "la\u00df", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "PPOSAT", "NN", "NN", "$,", "KON", "VVFIN", "PIS"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Damit Aue Wald und Feld sammt den K\u00f6rnerrei-", "tokens": ["Da\u00b7mit", "Au\u00b7e", "Wald", "und", "Feld", "sammt", "den", "K\u00f6r\u00b7ner\u00b7rei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADJA", "NN", "KON", "NN", "APPR", "ART", "TRUNC"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dir stat unser Klagelied bringen lauter Freuden-", "tokens": ["Dir", "stat", "un\u00b7ser", "Kla\u00b7ge\u00b7lied", "brin\u00b7gen", "lau\u00b7ter", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVFIN", "PIAT", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}}}}