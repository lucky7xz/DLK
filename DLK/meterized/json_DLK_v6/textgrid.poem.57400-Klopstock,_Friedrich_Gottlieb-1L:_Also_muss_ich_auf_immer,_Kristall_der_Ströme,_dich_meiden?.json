{"textgrid.poem.57400": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Also muss ich auf immer, Kristall der Str\u00f6me, dich meiden?", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Also muss ich auf immer, Kristall der Str\u00f6me, dich meiden?", "tokens": ["Al\u00b7so", "muss", "ich", "auf", "im\u00b7mer", ",", "Kris\u00b7tall", "der", "Str\u00f6\u00b7me", ",", "dich", "mei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADV", "$,", "NN", "ART", "NN", "$,", "PRF", "VVINF", "$."], "meter": "+-+-+--++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Darf nie wieder am Fuss schwingen die Fl\u00fcgel des Stahls?", "tokens": ["Darf", "nie", "wie\u00b7der", "am", "Fuss", "schwin\u00b7gen", "die", "Fl\u00fc\u00b7gel", "des", "Stahls", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wasserkothurn, du warest der Heilenden einer; ich h\u00e4tte,", "tokens": ["Was\u00b7ser\u00b7ko\u00b7thurn", ",", "du", "wa\u00b7rest", "der", "Hei\u00b7len\u00b7den", "ei\u00b7ner", ";", "ich", "h\u00e4t\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "$.", "PPER", "VAFIN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Unbeseelet von dir, weniger Sonnen gesehn!", "tokens": ["Un\u00b7be\u00b7see\u00b7let", "von", "dir", ",", "we\u00b7ni\u00b7ger", "Son\u00b7nen", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "PIAT", "NN", "VVPP", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Manche Rose hat mich erquickt; sie verwelkten! und du liegst,", "tokens": ["Man\u00b7che", "Ro\u00b7se", "hat", "mich", "er\u00b7quickt", ";", "sie", "ver\u00b7welk\u00b7ten", "!", "und", "du", "liegst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Auch des Schimmers beraubt, liegest verrostet nun da!", "tokens": ["Auch", "des", "Schim\u00b7mers", "be\u00b7raubt", ",", "lie\u00b7gest", "ver\u00b7ros\u00b7tet", "nun", "da", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,", "VVFIN", "VVFIN", "ADV", "ADV", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Welche Tage gabest du mir! wie begannen sie, wenn sich", "tokens": ["Wel\u00b7che", "Ta\u00b7ge", "ga\u00b7best", "du", "mir", "!", "wie", "be\u00b7gan\u00b7nen", "sie", ",", "wenn", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "VVFIN", "PPER", "PPER", "$.", "PWAV", "VVFIN", "PPER", "$,", "KOUS", "PRF"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "In der Fr\u00fche Glanz f\u00e4rbte noch bleibender Reif;", "tokens": ["In", "der", "Fr\u00fc\u00b7he", "Glanz", "f\u00e4rb\u00b7te", "noch", "blei\u00b7ben\u00b7der", "Reif", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "--+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Welche N\u00e4chte, wenn nun der Mond mit der Heitre des Himmels,", "tokens": ["Wel\u00b7che", "N\u00e4ch\u00b7te", ",", "wenn", "nun", "der", "Mond", "mit", "der", "Heit\u00b7re", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Um der Sch\u00f6nheit Preis, siegend stritt, und besiegt.", "tokens": ["Um", "der", "Sch\u00f6n\u00b7heit", "Preis", ",", "sie\u00b7gend", "stritt", ",", "und", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,", "ADJD", "VVFIN", "$,", "KON", "VVPP", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Dann war leichter der Schwung, und die Stellung unk\u00fcnstlicher, froher", "tokens": ["Dann", "war", "leich\u00b7ter", "der", "Schwung", ",", "und", "die", "Stel\u00b7lung", "un\u00b7k\u00fcnst\u00b7li\u00b7cher", ",", "fro\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "$,", "KON", "ART", "NN", "ADJD", "$,", "ADJA"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Dann der rufenden Laut, blinkete heller der Wein,", "tokens": ["Dann", "der", "ru\u00b7fen\u00b7den", "Laut", ",", "blin\u00b7ke\u00b7te", "hel\u00b7ler", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Und wie war der Schlaf, der endlich erm\u00fcdeten eisern,", "tokens": ["Und", "wie", "war", "der", "Schlaf", ",", "der", "end\u00b7lich", "er\u00b7m\u00fc\u00b7de\u00b7ten", "ei\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Wie unerwecklich! Wer schlief jemals am Baume wie wir?", "tokens": ["Wie", "un\u00b7er\u00b7weck\u00b7lich", "!", "Wer", "schlief", "je\u00b7mals", "am", "Bau\u00b7me", "wie", "wir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$.", "PWS", "VVFIN", "ADV", "APPRART", "NN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Aber es kam mit gebotnem Gepolter der Knecht; und wir sahen", "tokens": ["A\u00b7ber", "es", "kam", "mit", "ge\u00b7bot\u00b7nem", "Ge\u00b7pol\u00b7ter", "der", "Knecht", ";", "und", "wir", "sa\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Wieder den farbigen Reif, wieder den Schimmer der Nacht.", "tokens": ["Wie\u00b7der", "den", "far\u00b7bi\u00b7gen", "Reif", ",", "wie\u00b7der", "den", "Schim\u00b7mer", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Der du so oft mit der labenden Glut der gef\u00fchlten Gesundheit", "tokens": ["Der", "du", "so", "oft", "mit", "der", "la\u00b7ben\u00b7den", "Glut", "der", "ge\u00b7f\u00fchl\u00b7ten", "Ge\u00b7sund\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Mich durchstr\u00f6metest, Quell l\u00e4ngeres Lebens mir warst,", "tokens": ["Mich", "durch\u00b7str\u00f6\u00b7me\u00b7test", ",", "Quell", "l\u00e4n\u00b7ge\u00b7res", "Le\u00b7bens", "mir", "warst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "ADJA", "NN", "PPER", "VAFIN", "$,"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Wenn ich vor\u00fcberglitt an hellbebl\u00fcheten Ulmen;", "tokens": ["Wenn", "ich", "vor\u00b7\u00fc\u00b7berg\u00b7litt", "an", "hell\u00b7be\u00b7bl\u00fc\u00b7he\u00b7ten", "Ul\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "(schnee war die Blume;) der Bahn warnende Stimme vernahm,", "tokens": ["(", "schnee", "war", "die", "Blu\u00b7me", ";)", "der", "Bahn", "war\u00b7nen\u00b7de", "Stim\u00b7me", "ver\u00b7nahm", ","], "token_info": ["punct", "word", "word", "word", "word", "emoticon", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "VAFIN", "ART", "NN", "$(", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.21": {"text": "Mit nachhorchendem Ohr; auch wohl hinschwebt' an der Ostsee,", "tokens": ["Mit", "nach\u00b7hor\u00b7chen\u00b7dem", "Ohr", ";", "auch", "wohl", "hin\u00b7schwebt'", "an", "der", "Ost\u00b7see", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Zwischen der Sonne, die sank, und dem Monde, der stieg;", "tokens": ["Zwi\u00b7schen", "der", "Son\u00b7ne", ",", "die", "sank", ",", "und", "dem", "Mon\u00b7de", ",", "der", "stieg", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "KON", "ART", "NE", "$,", "PRELS", "VVFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.23": {"text": "Oder wenn, den die Flocken zu tausenden in sich verh\u00fcllten,", "tokens": ["O\u00b7der", "wenn", ",", "den", "die", "Flo\u00b7cken", "zu", "tau\u00b7sen\u00b7den", "in", "sich", "ver\u00b7h\u00fcll\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PRELS", "ART", "NN", "PTKZU", "VVINF", "APPR", "PRF", "VVFIN", "$,"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.24": {"text": "Und den schwindelte, Sturm auf das Gestade mich warf:", "tokens": ["Und", "den", "schwin\u00b7del\u00b7te", ",", "Sturm", "auf", "das", "Ge\u00b7sta\u00b7de", "mich", "warf", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "$,", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Ach einst wurdest du mir, Kothurn, zum tragischen! f\u00fchrtest", "tokens": ["Ach", "einst", "wur\u00b7dest", "du", "mir", ",", "Ko\u00b7thurn", ",", "zum", "tra\u00b7gi\u00b7schen", "!", "f\u00fchr\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "PPER", "$,", "NE", "$,", "APPRART", "VVINF", "$.", "VVFIN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Mich auf j\u00fcngeres Eis, welches dem eilenden brach.", "tokens": ["Mich", "auf", "j\u00fcn\u00b7ge\u00b7res", "Eis", ",", "wel\u00b7ches", "dem", "ei\u00b7len\u00b7den", "brach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "VVFIN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Bleich stand da der Gef\u00e4hrt; mein Schutzgeist gab mir Entschluss ein;", "tokens": ["Bleich", "stand", "da", "der", "Ge\u00b7f\u00e4hrt", ";", "mein", "Schutz\u00b7geist", "gab", "mir", "Ent\u00b7schluss", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Jener bebte nicht mehr; und die Errettung gelang.", "tokens": ["Je\u00b7ner", "beb\u00b7te", "nicht", "mehr", ";", "und", "die", "Er\u00b7ret\u00b7tung", "ge\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "PTKNEG", "ADV", "$.", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Als sie noch schwankend schien, da r\u00fchrte mich innig des Himmels", "tokens": ["Als", "sie", "noch", "schwan\u00b7kend", "schien", ",", "da", "r\u00fchr\u00b7te", "mich", "in\u00b7nig", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.30": {"text": "Lichtere Bl\u00e4ue, vielleicht bald nun die letzte f\u00fcr mich!", "tokens": ["Lich\u00b7te\u00b7re", "Bl\u00e4u\u00b7e", ",", "viel\u00b7leicht", "bald", "nun", "die", "letz\u00b7te", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADV", "ADV", "ART", "ADJA", "APPR", "PPER", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.31": {"text": "Dank dir noch Einmal, Beindorf, dass du mich rettetest! Dir kam", "tokens": ["Dank", "dir", "noch", "Ein\u00b7mal", ",", "Be\u00b7in\u00b7dorf", ",", "dass", "du", "mich", "ret\u00b7te\u00b7test", "!", "Dir", "kam"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ADV", "$,", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.32": {"text": "Lang schon die letzte; mir macht sie die Erde noch sch\u00f6n.", "tokens": ["Lang", "schon", "die", "letz\u00b7te", ";", "mir", "macht", "sie", "die", "Er\u00b7de", "noch", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}