{"textgrid.poem.34831": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh, Deutschland, meine ferne Liebe,", "tokens": ["Oh", ",", "Deutschland", ",", "mei\u00b7ne", "fer\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gedenk ich deiner, wein ich fast!", "tokens": ["Ge\u00b7denk", "ich", "dei\u00b7ner", ",", "wein", "ich", "fast", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "$,", "PWAV", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das muntre Frankreich scheint mir tr\u00fcbe,", "tokens": ["Das", "mun\u00b7tre", "Fran\u00b7kreich", "scheint", "mir", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das leichte Volk wird mir zur Last.", "tokens": ["Das", "leich\u00b7te", "Volk", "wird", "mir", "zur", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur der Verstand, so kalt und trocken,", "tokens": ["Nur", "der", "Ver\u00b7stand", ",", "so", "kalt", "und", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herrscht in dem witzigen Paris \u2013", "tokens": ["Herrscht", "in", "dem", "wit\u00b7zi\u00b7gen", "Pa\u00b7ris", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NE", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Oh, Narrheitsgl\u00f6cklein, Glaubensglocken,", "tokens": ["Oh", ",", "Nar\u00b7rheits\u00b7gl\u00f6c\u00b7klein", ",", "Glau\u00b7bens\u00b7glo\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie klingelt ihr daheim so s\u00fc\u00df!", "tokens": ["Wie", "klin\u00b7gelt", "ihr", "da\u00b7heim", "so", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6fliche M\u00e4nner! Doch verdrossen", "tokens": ["H\u00f6f\u00b7li\u00b7che", "M\u00e4n\u00b7ner", "!", "Doch", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Geb ich den art'gen Gru\u00df zur\u00fcck. \u2013", "tokens": ["Geb", "ich", "den", "art'\u00b7gen", "Gru\u00df", "zu\u00b7r\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Grobheit, die ich einst genossen", "tokens": ["Die", "Grob\u00b7heit", ",", "die", "ich", "einst", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Vaterland, das war mein Gl\u00fcck!", "tokens": ["Im", "Va\u00b7ter\u00b7land", ",", "das", "war", "mein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "L\u00e4chelnde Weiber! Plappern immer,", "tokens": ["L\u00e4\u00b7cheln\u00b7de", "Wei\u00b7ber", "!", "Plap\u00b7pern", "im\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "ADV", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie M\u00fchlenr\u00e4der stets bewegt!", "tokens": ["Wie", "M\u00fch\u00b7len\u00b7r\u00e4\u00b7der", "stets", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lob ich Deuschlands Frauenzimmer,", "tokens": ["Da", "lob", "ich", "Deu\u00b7schlands", "Frau\u00b7en\u00b7zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das schweigend sich zu Bette legt.", "tokens": ["Das", "schwei\u00b7gend", "sich", "zu", "Bet\u00b7te", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und alles dreht sich hier im Kreise,", "tokens": ["Und", "al\u00b7les", "dreht", "sich", "hier", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Ungest\u00fcm, wie 'n toller Traum!", "tokens": ["Mit", "Un\u00b7ge\u00b7st\u00fcm", ",", "wie", "'n", "tol\u00b7ler", "Traum", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bei uns bleibt alles h\u00fcbsch im Gleise,", "tokens": ["Bei", "uns", "bleibt", "al\u00b7les", "h\u00fcbsch", "im", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie angenagelt, r\u00fchrt sich kaum.", "tokens": ["Wie", "an\u00b7ge\u00b7na\u00b7gelt", ",", "r\u00fchrt", "sich", "kaum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mir ist, als h\u00f6rt' ich fern erklingen", "tokens": ["Mir", "ist", ",", "als", "h\u00f6rt'", "ich", "fern", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nachtw\u00e4chterh\u00f6rner, sanft und traut;", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ter\u00b7h\u00f6r\u00b7ner", ",", "sanft", "und", "traut", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nachtw\u00e4chterlieder h\u00f6r ich singen,", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ter\u00b7lie\u00b7der", "h\u00f6r", "ich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dazwischen Nachtigallenlaut.", "tokens": ["Da\u00b7zwi\u00b7schen", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7laut", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dem Dichter war so wohl daheime,", "tokens": ["Dem", "Dich\u00b7ter", "war", "so", "wohl", "da\u00b7hei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Schildas teurem Eichenhain!", "tokens": ["In", "Schil\u00b7das", "teu\u00b7rem", "Ei\u00b7chen\u00b7hain", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort wob ich meine zarten Reime", "tokens": ["Dort", "wob", "ich", "mei\u00b7ne", "zar\u00b7ten", "Rei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Veilchenduft und Mondenschein.", "tokens": ["Aus", "Veil\u00b7chen\u00b7duft", "und", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}