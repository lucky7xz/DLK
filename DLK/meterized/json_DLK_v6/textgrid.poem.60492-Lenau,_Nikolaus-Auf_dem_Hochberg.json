{"textgrid.poem.60492": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Hochberg", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Gletscher gl\u00fchen in dem goldnen Lichte", "tokens": ["Die", "Glet\u00b7scher", "gl\u00fc\u00b7hen", "in", "dem", "gold\u00b7nen", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und r\u00f6tlich gl\u00e4nzt die Felsenwand,", "tokens": ["Und", "r\u00f6t\u00b7lich", "gl\u00e4nzt", "die", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um diese Gipfel wehen Traumgesichte,", "tokens": ["Um", "die\u00b7se", "Gip\u00b7fel", "we\u00b7hen", "Traum\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus fr\u00fchen Tagen mir bekannt.", "tokens": ["Aus", "fr\u00fc\u00b7hen", "Ta\u00b7gen", "mir", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Purpurmeer seh ich den Nachen treiben:", "tokens": ["Im", "Pur\u00b7pur\u00b7meer", "seh", "ich", "den", "Na\u00b7chen", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne spiegelt sich im weiten See.", "tokens": ["Die", "Son\u00b7ne", "spie\u00b7gelt", "sich", "im", "wei\u00b7ten", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am fernen Kloster z\u00e4hl ich alle Scheiben,", "tokens": ["Am", "fer\u00b7nen", "Klos\u00b7ter", "z\u00e4hl", "ich", "al\u00b7le", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Herzen wird mirs wohl und weh.", "tokens": ["Im", "Her\u00b7zen", "wird", "mirs", "wohl", "und", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NE", "ADV", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es locken T\u00e4ler hinter Felsentoren,", "tokens": ["Es", "lo\u00b7cken", "T\u00e4\u00b7ler", "hin\u00b7ter", "Fel\u00b7sen\u00b7to\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Sehnen fa\u00dft mich im Gem\u00fct,", "tokens": ["Ein", "Seh\u00b7nen", "fa\u00dft", "mich", "im", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach Gl\u00fcck, besessen \u2013 nie \u2013 und nie verloren,", "tokens": ["Nach", "Gl\u00fcck", ",", "be\u00b7ses\u00b7sen", "\u2013", "nie", "\u2013", "und", "nie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVPP", "$(", "ADV", "$(", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verwelkt und niemals doch erbl\u00fcht!", "tokens": ["Ver\u00b7welkt", "und", "nie\u00b7mals", "doch", "er\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Blick la\u00df in die blaue Ferne tauchen", "tokens": ["Den", "Blick", "la\u00df", "in", "die", "blau\u00b7e", "Fer\u00b7ne", "tau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVIMP", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": " \u2013 Dort ist es nicht, nur Trug und Pein!", "tokens": ["\u2013", "Dort", "ist", "es", "nicht", ",", "nur", "Trug", "und", "Pein", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PTKNEG", "$,", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da unten, wo die stillen H\u00fctten rauchen,", "tokens": ["Da", "un\u00b7ten", ",", "wo", "die", "stil\u00b7len", "H\u00fct\u00b7ten", "rau\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da mu\u00df es oder nirgend sein!", "tokens": ["Da", "mu\u00df", "es", "o\u00b7der", "nir\u00b7gend", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf Alpenh\u00f6he mit dir, Seelenschwester,", "tokens": ["Auf", "Al\u00b7pen\u00b7h\u00f6\u00b7he", "mit", "dir", ",", "See\u00b7len\u00b7schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Abendschein ich schweigend stand,", "tokens": ["Im", "A\u00b7bend\u00b7schein", "ich", "schwei\u00b7gend", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht reden k\u00f6nnt ich, dr\u00fcckte fest und fester", "tokens": ["Nicht", "re\u00b7den", "k\u00f6nnt", "ich", ",", "dr\u00fcck\u00b7te", "fest", "und", "fes\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "$,", "VVFIN", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur deine liebe, treue Hand.", "tokens": ["Nur", "dei\u00b7ne", "lie\u00b7be", ",", "treu\u00b7e", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Glocken riefen zum Gebet die M\u00fcden,", "tokens": ["Die", "Glo\u00b7cken", "rie\u00b7fen", "zum", "Ge\u00b7bet", "die", "M\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und aller Zauber der Natur", "tokens": ["Und", "al\u00b7ler", "Zau\u00b7ber", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kam \u00fcber uns mit seinem tiefen Frieden,", "tokens": ["Kam", "\u00fc\u00b7ber", "uns", "mit", "sei\u00b7nem", "tie\u00b7fen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch blieb auch eine Wehmutspur.", "tokens": ["Doch", "blieb", "auch", "ei\u00b7ne", "Weh\u00b7muts\u00b7pur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wann stehen wir wie jetzt so eng verbunden", "tokens": ["Wann", "ste\u00b7hen", "wir", "wie", "jetzt", "so", "eng", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "KOKOM", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl wieder in dem Abendstrahl,", "tokens": ["Wohl", "wie\u00b7der", "in", "dem", "A\u00b7bend\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann bringen sp\u00e4te Jahre solcher Stunden", "tokens": ["Wann", "brin\u00b7gen", "sp\u00e4\u00b7te", "Jah\u00b7re", "sol\u00b7cher", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADJA", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verein im gr\u00fcnen Alpental?", "tokens": ["Ver\u00b7ein", "im", "gr\u00fc\u00b7nen", "Al\u00b7pen\u00b7tal", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Bald wird der Abschied mir die Brust zerschneiden,", "tokens": ["Bald", "wird", "der", "Ab\u00b7schied", "mir", "die", "Brust", "zer\u00b7schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Vaterland, vom Vaterhaus.", "tokens": ["Vom", "Va\u00b7ter\u00b7land", ",", "vom", "Va\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Getrennt von dir, mu\u00df Herzensfrost ich leiden,", "tokens": ["Ge\u00b7trennt", "von", "dir", ",", "mu\u00df", "Her\u00b7zens\u00b7frost", "ich", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "$,", "VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Fremde treibt es mich hinaus.", "tokens": ["Zur", "Frem\u00b7de", "treibt", "es", "mich", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "APZR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du bist mir mehr als meine Heimatschwelle,", "tokens": ["Du", "bist", "mir", "mehr", "als", "mei\u00b7ne", "Hei\u00b7mat\u00b7schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Herz ist mir ein Heilgenschrein,", "tokens": ["Dein", "Herz", "ist", "mir", "ein", "Heil\u00b7gen\u00b7schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir wie dem m\u00fcden Pilgrim die Kapelle;", "tokens": ["Mir", "wie", "dem", "m\u00fc\u00b7den", "Pil\u00b7grim", "die", "Ka\u00b7pel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich legte Wonn und Schmerz hinein!", "tokens": ["Ich", "leg\u00b7te", "Wonn", "und", "Schmerz", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wir werden oft uns, einst gewi\u00df ach! trennen,", "tokens": ["Wir", "wer\u00b7den", "oft", "uns", ",", "einst", "ge\u00b7wi\u00df", "ach", "!", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "$,", "ADV", "ADV", "ADV", "$.", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vereint doch sein in Lieb, Gebet.", "tokens": ["Ver\u00b7eint", "doch", "sein", "in", "Lieb", ",", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "APPR", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir werden wieder sehen uns, erkennen:", "tokens": ["Wir", "wer\u00b7den", "wie\u00b7der", "se\u00b7hen", "uns", ",", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "PPER", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Trost ist dies, der fest besteht.", "tokens": ["Ein", "Trost", "ist", "dies", ",", "der", "fest", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}