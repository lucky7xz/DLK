{"textgrid.poem.58804": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "23. Eine Krone", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anfechtung, Leiden, Spott und Hohn", "tokens": ["An\u00b7fech\u00b7tung", ",", "Lei\u00b7den", ",", "Spott", "und", "Hohn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind hier der Christen Ehrenkron';", "tokens": ["Sind", "hier", "der", "Chris\u00b7ten", "Eh\u00b7ren\u00b7kron'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort werden sie als K\u00f6n'ge leben,", "tokens": ["Dort", "wer\u00b7den", "sie", "als", "K\u00f6n'\u00b7ge", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Gottes Herrlichkeit umgeben.", "tokens": ["Mit", "Got\u00b7tes", "Herr\u00b7lich\u00b7keit", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}