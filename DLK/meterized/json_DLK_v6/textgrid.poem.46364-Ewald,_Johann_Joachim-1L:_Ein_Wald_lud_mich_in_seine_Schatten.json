{"textgrid.poem.46364": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Wald lud mich in seine Schatten", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wald lud mich in seine Schatten", "tokens": ["Ein", "Wald", "lud", "mich", "in", "sei\u00b7ne", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich irrte drin in krummen G\u00e4ngen", "tokens": ["Ich", "irr\u00b7te", "drin", "in", "krum\u00b7men", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und pl\u00f6tzlich sah' ich Daphnen kommen.", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "sah'", "ich", "Daph\u00b7nen", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schl\u00fcpft in eine hohle Eiche", "tokens": ["Ich", "schl\u00fcpft", "in", "ei\u00b7ne", "hoh\u00b7le", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und h\u00f6rte aus der hohlen Eiche", "tokens": ["Und", "h\u00f6r\u00b7te", "aus", "der", "hoh\u00b7len", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was Daphne mit sich selber redte;", "tokens": ["Was", "Daph\u00b7ne", "mit", "sich", "sel\u00b7ber", "red\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie sprach: ach k\u00f6nnt ich Mittel finden", "tokens": ["Sie", "sprach", ":", "ach", "k\u00f6nnt", "ich", "Mit\u00b7tel", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Mund des Thirsis zu verschlie\u00dfen,", "tokens": ["Den", "Mund", "des", "Thir\u00b7sis", "zu", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So offt er mich um Liebe flehet!", "tokens": ["So", "offt", "er", "mich", "um", "Lie\u00b7be", "fle\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Schnell rief ich in dem Schoo\u00df der Eiche", "tokens": ["Schnell", "rief", "ich", "in", "dem", "Schoo\u00df", "der", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Nicht anders als Orakel sprechen:", "tokens": ["Nicht", "an\u00b7ders", "als", "O\u00b7ra\u00b7kel", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dein Ku\u00df wird Thirsis Mund verschlie\u00dfen!", "tokens": ["Dein", "Ku\u00df", "wird", "Thir\u00b7sis", "Mund", "ver\u00b7schlie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}