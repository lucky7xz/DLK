{"dta.poem.16474": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "135  Bey Gott ist nur sein Sohn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch werd\u2019 au\u00df Gott gebohrn: bey seiner GOttheit", "tokens": ["Mensch", "werd'", "au\u00df", "Gott", "ge\u00b7bohrn", ":", "bey", "sei\u00b7ner", "Got\u00b7theit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "NN", "VVPP", "$.", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Thron/", "tokens": ["Thron", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Steht niemand anders als der eingebohrne Sohn.", "tokens": ["Steht", "nie\u00b7mand", "an\u00b7ders", "als", "der", "ein\u00b7ge\u00b7bohr\u00b7ne", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}