{"dta.poem.23502": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Vlysses vnd Si-  \n rene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kom her/ du wehrter Griech kom her/", "tokens": ["Kom", "her", "/", "du", "wehr\u00b7ter", "Griech", "kom", "her", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$(", "PPER", "ADJA", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Woltest du nicht alhie anfahren?", "tokens": ["Wol\u00b7test", "du", "nicht", "al\u00b7hie", "an\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der Wind vn\u0303 das M\u00f6hr toben sehr/", "tokens": ["Der", "Wind", "v\u00f1", "das", "M\u00f6hr", "to\u00b7ben", "sehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Vnd alhie kan dich nichts befahren:", "tokens": ["Vnd", "al\u00b7hie", "kan", "dich", "nichts", "be\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PRF", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Alhie sehen vnd h\u00f6ren Wir", "tokens": ["Al\u00b7hie", "se\u00b7hen", "vnd", "h\u00f6\u00b7ren", "Wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der f\u00fcr-schiffenden m\u00fch vnd klagen;", "tokens": ["Der", "f\u00fcr\u00b7schif\u00b7fen\u00b7den", "m\u00fch", "vnd", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "KON", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Alhie kanst du fr\u00f6lich mit mir", "tokens": ["Al\u00b7hie", "kanst", "du", "fr\u00f6\u00b7lich", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "PPER"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Alle sorg au\u00df dem hertzen jagen.", "tokens": ["Al\u00b7le", "sorg", "au\u00df", "dem", "hert\u00b7zen", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}