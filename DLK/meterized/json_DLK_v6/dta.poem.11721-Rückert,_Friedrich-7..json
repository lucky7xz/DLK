{"dta.poem.11721": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Ganzen Theile sind als Theile nicht vorhanden,", "tokens": ["Des", "Gan\u00b7zen", "Thei\u00b7le", "sind", "als", "Thei\u00b7le", "nicht", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "KOUS", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deswegen, weil sie ja zum Ganzen sich verbanden.", "tokens": ["Des\u00b7we\u00b7gen", ",", "weil", "sie", "ja", "zum", "Gan\u00b7zen", "sich", "ver\u00b7ban\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Grenzpf\u00e4hle steckest du, um ein Gebiet zu messen;", "tokens": ["Grenz\u00b7pf\u00e4h\u00b7le", "ste\u00b7ckest", "du", ",", "um", "ein", "Ge\u00b7biet", "zu", "mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch da\u00df du sie nur steckst, das sollst du nicht vergessen.", "tokens": ["Doch", "da\u00df", "du", "sie", "nur", "steckst", ",", "das", "sollst", "du", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der grade Gegensatz setzt grad die Wahrheit schief,", "tokens": ["Der", "gra\u00b7de", "Ge\u00b7gen\u00b7satz", "setzt", "grad", "die", "Wahr\u00b7heit", "schief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil stets in Wahrheit eins ins andre sich verlief.", "tokens": ["Weil", "stets", "in", "Wahr\u00b7heit", "eins", "ins", "and\u00b7re", "sich", "ver\u00b7lief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "PIS", "APPRART", "PIS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}