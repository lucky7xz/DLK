{"textgrid.poem.65371": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Feuerstein", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kreid' an Jasmunds K\u00fcste", "tokens": ["Die", "Kreid'", "an", "Jas\u00b7munds", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist nicht so weich und wei\u00df,", "tokens": ["Ist", "nicht", "so", "weich", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie deine Haut, o M\u00e4dchen,", "tokens": ["Wie", "dei\u00b7ne", "Haut", ",", "o", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du aller M\u00e4dchen Preis.", "tokens": ["Du", "al\u00b7ler", "M\u00e4d\u00b7chen", "Preis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und deine Wangen gl\u00fchen,", "tokens": ["Und", "dei\u00b7ne", "Wan\u00b7gen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie wenn der Morgenschein", "tokens": ["Wie", "wenn", "der", "Mor\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit seinen rothen Strahlen", "tokens": ["Mit", "sei\u00b7nen", "ro\u00b7then", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bemahlt den bleichen Stein.", "tokens": ["Be\u00b7mahlt", "den", "blei\u00b7chen", "Stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es lag an Jasmunds K\u00fcste", "tokens": ["Es", "lag", "an", "Jas\u00b7munds", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein sch\u00f6nes Kreidest\u00fcck.", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Krei\u00b7de\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich nahm's in meinen Nachen", "tokens": ["Ich", "nahm's", "in", "mei\u00b7nen", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ruderte zur\u00fcck.", "tokens": ["Und", "ru\u00b7der\u00b7te", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Und als ich kam nach Hause", "tokens": ["Und", "als", "ich", "kam", "nach", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sah die Ladung an,", "tokens": ["Und", "sah", "die", "La\u00b7dung", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da dacht' ich dein, o M\u00e4dchen,", "tokens": ["Da", "dacht'", "ich", "dein", ",", "o", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und war ein froher Mann.", "tokens": ["Und", "war", "ein", "fro\u00b7her", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich wollt's bei Seite legen,", "tokens": ["Ich", "wollt's", "bei", "Sei\u00b7te", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da brach's in meiner Hand.", "tokens": ["Da", "brach's", "in", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ei Gott beh\u00fct', o M\u00e4dchen!", "tokens": ["Ei", "Gott", "be\u00b7h\u00fct'", ",", "o", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4lt so die Liebe Stand?", "tokens": ["H\u00e4lt", "so", "die", "Lie\u00b7be", "Stand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Und in der wei\u00dfen Schale", "tokens": ["Und", "in", "der", "wei\u00b7\u00dfen", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da lag ein Feuerstein,", "tokens": ["Da", "lag", "ein", "Feu\u00b7ers\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein scharfer, harter, schwarzer \u2013", "tokens": ["Ein", "schar\u00b7fer", ",", "har\u00b7ter", ",", "schwar\u00b7zer", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJA", "$,", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das soll kein Herz doch sein?", "tokens": ["Das", "soll", "kein", "Herz", "doch", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die b\u00f6sen Zungen sagen", "tokens": ["Die", "b\u00f6\u00b7sen", "Zun\u00b7gen", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir vieles B\u00f6se nach,", "tokens": ["Dir", "vie\u00b7les", "B\u00f6\u00b7se", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum frag' ich keine Seele,", "tokens": ["Drum", "frag'", "ich", "kei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was das bedeuten mag.", "tokens": ["Was", "das", "be\u00b7deu\u00b7ten", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PWS", "ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Und sperr' ich b\u00f6sen Zungen", "tokens": ["Und", "sperr'", "ich", "b\u00f6\u00b7sen", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Ohren und das Haus,", "tokens": ["Die", "Oh\u00b7ren", "und", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will ich den Stein auch werfen", "tokens": ["Will", "ich", "den", "Stein", "auch", "wer\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Fenster gleich hinaus.", "tokens": ["Zum", "Fens\u00b7ter", "gleich", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}