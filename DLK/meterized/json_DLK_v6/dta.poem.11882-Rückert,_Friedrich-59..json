{"dta.poem.11882": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Esel, den mit Salz sie \u00fcberladen hatten,", "tokens": ["Der", "E\u00b7sel", ",", "den", "mit", "Salz", "sie", "\u00fc\u00b7berl\u00b7a\u00b7den", "hat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Im Flusse legt' er sich, und das kam ihm zu Statten.", "tokens": ["Im", "Flus\u00b7se", "legt'", "er", "sich", ",", "und", "das", "kam", "ihm", "zu", "Stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "$,", "KON", "PDS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Er widerholt es dann, da kam es ihm zu Schaden,", "tokens": ["Er", "wi\u00b7der\u00b7holt", "es", "dann", ",", "da", "kam", "es", "ihm", "zu", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er nun, statt mit Salz, mit Wolle war beladen.", "tokens": ["Weil", "er", "nun", ",", "statt", "mit", "Salz", ",", "mit", "Wol\u00b7le", "war", "be\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUI", "APPR", "NN", "$,", "APPR", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Salz im Wasser schmolz, die Woll' im Wasser schwoll;", "tokens": ["Das", "Salz", "im", "Was\u00b7ser", "schmolz", ",", "die", "Woll'", "im", "Was\u00b7ser", "schwoll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "$,", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort gieng er leicht davon und schwerer hier mit Groll.", "tokens": ["Dort", "gieng", "er", "leicht", "da\u00b7von", "und", "schwe\u00b7rer", "hier", "mit", "Groll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PAV", "KON", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er grollte dem, der ihm gegeben diesen Rath,", "tokens": ["Er", "groll\u00b7te", "dem", ",", "der", "ihm", "ge\u00b7ge\u00b7ben", "die\u00b7sen", "Rath", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVPP", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da doch der Esel sich es zuzuschreiben hat.", "tokens": ["Da", "doch", "der", "E\u00b7sel", "sich", "es", "zu\u00b7zu\u00b7schrei\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "PPER", "VVIZU", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur einmal gilt ein Rath, nur einmal eine List;", "tokens": ["Nur", "ein\u00b7mal", "gilt", "ein", "Rath", ",", "nur", "ein\u00b7mal", "ei\u00b7ne", "List", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gelernt vom Esel hat dies, wer da weiser ist.", "tokens": ["Ge\u00b7lernt", "vom", "E\u00b7sel", "hat", "dies", ",", "wer", "da", "wei\u00b7ser", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "VAFIN", "PDS", "$,", "PWS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}