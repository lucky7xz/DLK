{"dta.poem.596": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er tr\u00f6stet sich selbst in seinem Ungl\u00fccke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Hertz/ da\u00df sich mit Sorgen plagt/", "tokens": ["Ein", "Hertz", "/", "da\u00df", "sich", "mit", "Sor\u00b7gen", "plagt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verzehrt sich selbst vor Zeiten/", "tokens": ["Ver\u00b7zehrt", "sich", "selbst", "vor", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer t\u00e4glich \u00fcber Ungl\u00fcck klagt/", "tokens": ["Wer", "t\u00e4g\u00b7lich", "\u00fc\u00b7ber", "Un\u00b7gl\u00fcck", "klagt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird sich das Grab bereiten:", "tokens": ["Wird", "sich", "das", "Grab", "be\u00b7rei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es kan doch nichts best\u00e4ndig seyn/", "tokens": ["Es", "kan", "doch", "nichts", "be\u00b7st\u00e4n\u00b7dig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf Regen folget Sonnenschein.", "tokens": ["Auf", "Re\u00b7gen", "fol\u00b7get", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Zeiten sind ver\u00e4nderlich/", "tokens": ["Die", "Zei\u00b7ten", "sind", "ver\u00b7\u00e4n\u00b7der\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wechseln Tag und Stunden/", "tokens": ["Es", "wech\u00b7seln", "Tag", "und", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es hat nach Blitz und Donner sich", "tokens": ["Es", "hat", "nach", "Blitz", "und", "Don\u00b7ner", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stille eingefunden/", "tokens": ["Die", "Stil\u00b7le", "ein\u00b7ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Nacht kan nicht so finster seyn/", "tokens": ["Die", "Nacht", "kan", "nicht", "so", "fins\u00b7ter", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Tag bricht wiederum herein.", "tokens": ["Der", "Tag", "bricht", "wie\u00b7de\u00b7rum", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00e4lt gleich das angenehme Licht", "tokens": ["H\u00e4lt", "gleich", "das", "an\u00b7ge\u00b7neh\u00b7me", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Gl\u00fcckes sich verborgen/", "tokens": ["Des", "Gl\u00fc\u00b7ckes", "sich", "ver\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So glaub ich/ gl\u00e4ntzt es heute nicht/", "tokens": ["So", "glaub", "ich", "/", "gl\u00e4ntzt", "es", "heu\u00b7te", "nicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es doch scheinet morgen/", "tokens": ["Da\u00df", "es", "doch", "schei\u00b7net", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es kommt bey mir auch wol der Tag", "tokens": ["Es", "kommt", "bey", "mir", "auch", "wol", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich mich gl\u00fccklich nennen mag.", "tokens": ["Da\u00df", "ich", "mich", "gl\u00fcck\u00b7lich", "nen\u00b7nen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Schiff da\u00df in der Wassers-Fluth", "tokens": ["Ein", "Schiff", "da\u00df", "in", "der", "Was\u00b7ser\u00b7sFluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit \u2019Wind und Wellen streitet/", "tokens": ["Mit", "'wind", "und", "Wel\u00b7len", "strei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verlieret nicht so fort den Muth/", "tokens": ["Ver\u00b7lie\u00b7ret", "nicht", "so", "fort", "den", "Muth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PTKVZ", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil es die Hoffnung leitet/", "tokens": ["Weil", "es", "die", "Hoff\u00b7nung", "lei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es werde bald den Sturm entgehn/", "tokens": ["Es", "wer\u00b7de", "bald", "den", "Sturm", "ent\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sich im sichern Hafen sehn.", "tokens": ["Und", "sich", "im", "si\u00b7chern", "Ha\u00b7fen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Liegt gleich mein Schiff vor Ancker noch/", "tokens": ["Liegt", "gleich", "mein", "Schiff", "vor", "An\u00b7cker", "noch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat ", "tokens": ["Und", "hat"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "So ist bey mir die Hoffnung doch/", "tokens": ["So", "ist", "bey", "mir", "die", "Hoff\u00b7nung", "doch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich noch endlich finde/", "tokens": ["Da\u00df", "ich", "noch", "end\u00b7lich", "fin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Haven der mir ist beschert", "tokens": ["Den", "Ha\u00b7ven", "der", "mir", "ist", "be\u00b7schert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und den mein W\u00fcnschen l\u00e4ngst begehrt.", "tokens": ["Und", "den", "mein", "W\u00fcn\u00b7schen", "l\u00e4ngst", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer wei\u00df/ wo noch mein Gl\u00fccke soll", "tokens": ["Wer", "wei\u00df", "/", "wo", "noch", "mein", "Gl\u00fc\u00b7cke", "soll"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "PWAV", "ADV", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In voller Bl\u00fchte stehen/", "tokens": ["In", "vol\u00b7ler", "Bl\u00fch\u00b7te", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und scheinet es heute noch so toll/", "tokens": ["Und", "schei\u00b7net", "es", "heu\u00b7te", "noch", "so", "toll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kans morgen doch geschehen/", "tokens": ["Kans", "mor\u00b7gen", "doch", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich durch einen guten Weg", "tokens": ["Da\u00df", "ich", "durch", "ei\u00b7nen", "gu\u00b7ten", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erlange den gew\u00fcnschten Zweck.", "tokens": ["Er\u00b7lan\u00b7ge", "den", "ge\u00b7w\u00fcnschten", "Zweck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Das Gl\u00fccke ist den R\u00e4dern gleich/", "tokens": ["Das", "Gl\u00fc\u00b7cke", "ist", "den", "R\u00e4\u00b7dern", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Unterste kommt oben/", "tokens": ["Das", "Un\u00b7ters\u00b7te", "kommt", "o\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der heute arm ist morgen reich/", "tokens": ["Der", "heu\u00b7te", "arm", "ist", "mor\u00b7gen", "reich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00f6rt sich herrlich loben/", "tokens": ["Und", "h\u00f6rt", "sich", "herr\u00b7lich", "lo\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie mancher ist im Augenblick", "tokens": ["Wie", "man\u00b7cher", "ist", "im", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gelangt zu einen grossen Gl\u00fcck.", "tokens": ["Ge\u00b7langt", "zu", "ei\u00b7nen", "gros\u00b7sen", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Lacht gleich das Gl\u00fccke ander an/", "tokens": ["Lacht", "gleich", "das", "Gl\u00fc\u00b7cke", "an\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will es ihnen g\u00f6nnen/", "tokens": ["Ich", "will", "es", "ih\u00b7nen", "g\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil auch der Tag erscheinen kan/", "tokens": ["Weil", "auch", "der", "Tag", "er\u00b7schei\u00b7nen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Daran ich werd\u2019 erkennen/", "tokens": ["Da\u00b7ran", "ich", "werd'", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df Hoffnung doch den Prei\u00df beh\u00e4lt", "tokens": ["Da\u00df", "Hoff\u00b7nung", "doch", "den", "Prei\u00df", "be\u00b7h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey allen Ungl\u00fcck in der Welt.", "tokens": ["Bey", "al\u00b7len", "Un\u00b7gl\u00fcck", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Drum mein Gem\u00fcth ermuntre dich/", "tokens": ["Drum", "mein", "Ge\u00b7m\u00fcth", "er\u00b7munt\u00b7re", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00df die Sorgen fahren/", "tokens": ["Und", "la\u00df", "die", "Sor\u00b7gen", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es geht das Gl\u00fccke wunderlich/", "tokens": ["Es", "geht", "das", "Gl\u00fc\u00b7cke", "wun\u00b7der\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man hat es nicht beym Haaren/", "tokens": ["Man", "hat", "es", "nicht", "beym", "Haa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df sorgen/ wer da sorgen will/", "tokens": ["La\u00df", "sor\u00b7gen", "/", "wer", "da", "sor\u00b7gen", "will", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$(", "PWS", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich hoff\u2019 und warte in der Still.", "tokens": ["Ich", "hoff'", "und", "war\u00b7te", "in", "der", "Still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}