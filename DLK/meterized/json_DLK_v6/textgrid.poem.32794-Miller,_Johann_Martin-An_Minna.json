{"textgrid.poem.32794": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An Minna", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blicke schlossen unsern Bund,", "tokens": ["Bli\u00b7cke", "schlos\u00b7sen", "un\u00b7sern", "Bund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Engel waren Zeugen;", "tokens": ["En\u00b7gel", "wa\u00b7ren", "Zeu\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber Liebe hie\u00df den Mund", "tokens": ["A\u00b7ber", "Lie\u00b7be", "hie\u00df", "den", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ehrerbietig schweigen!", "tokens": ["Ehr\u00b7er\u00b7bie\u00b7tig", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Minna, nimmer hast du mir", "tokens": ["Min\u00b7na", ",", "nim\u00b7mer", "hast", "du", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieb' und Treu' geschworen,", "tokens": ["Lieb'", "und", "Treu'", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und nun leb' ich fern von dir,", "tokens": ["Und", "nun", "leb'", "ich", "fern", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin in Gram verloren!", "tokens": ["Bin", "in", "Gram", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Br\u00e4chest du der Herzen Bund,", "tokens": ["Br\u00e4\u00b7chest", "du", "der", "Her\u00b7zen", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach wer w\u00fcrde zeugen?", "tokens": ["Ach", "wer", "w\u00fcr\u00b7de", "zeu\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VAFIN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Liebe, Liebe hie\u00df den Mund", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", "hie\u00df", "den", "Mund"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ehrerbietig schweigen!", "tokens": ["Ehr\u00b7er\u00b7bie\u00b7tig", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Engel, Engel saht ihr nicht", "tokens": ["En\u00b7gel", ",", "En\u00b7gel", "saht", "ihr", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "VVFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Thr\u00e4nen sprechen?", "tokens": ["Uns\u00b7re", "Thr\u00e4\u00b7nen", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "O erinnert sie der Pflicht,", "tokens": ["O", "e\u00b7rin\u00b7nert", "sie", "der", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht den Bund zu brechen!", "tokens": ["Nicht", "den", "Bund", "zu", "bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}