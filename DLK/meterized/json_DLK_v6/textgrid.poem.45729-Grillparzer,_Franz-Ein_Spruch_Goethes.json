{"textgrid.poem.45729": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Ein Spruch Goethes", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas man in der Jugend w\u00fcnscht, hat man im Alter genug\u00ab,", "tokens": ["\u00bb", "was", "man", "in", "der", "Ju\u00b7gend", "w\u00fcnscht", ",", "hat", "man", "im", "Al\u00b7ter", "ge\u00b7nug", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "PIS", "APPR", "ART", "NN", "VVFIN", "$,", "VAFIN", "PIS", "APPRART", "NN", "ADV", "$(", "$,"], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "So sagen die Reichbegabten mit Fug,", "tokens": ["So", "sa\u00b7gen", "die", "Reich\u00b7be\u00b7gab\u00b7ten", "mit", "Fug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir aber mindern Pfundes Verwalter,", "tokens": ["Wir", "a\u00b7ber", "min\u00b7dern", "Pfun\u00b7des", "Ver\u00b7wal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was wir jung hatten, w\u00fcnschen wir im Alter.", "tokens": ["Was", "wir", "jung", "hat\u00b7ten", ",", "w\u00fcn\u00b7schen", "wir", "im", "Al\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}