{"textgrid.poem.61839": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie oft wird in politischen Fragen", "tokens": ["Wie", "oft", "wird", "in", "po\u00b7li\u00b7ti\u00b7schen", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dein Herz die Antwort dir versagen!", "tokens": ["Dein", "Herz", "die", "Ant\u00b7wort", "dir", "ver\u00b7sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Recht ist meistens zweifelhaft;", "tokens": ["Das", "Recht", "ist", "meis\u00b7tens", "zwei\u00b7fel\u00b7haft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da h\u00e4ltst du's denn mit Mut und Kraft.", "tokens": ["Da", "h\u00e4ltst", "du's", "denn", "mit", "Mut", "und", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}