{"textgrid.poem.29029": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo Poeten durch entz\u00fccken", "tokens": ["Wo", "Po\u00b7et\u00b7en", "durch", "ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich zu guten Reimen schicken,", "tokens": ["Sich", "zu", "gu\u00b7ten", "Rei\u00b7men", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat es allenthalben Hasen,", "tokens": ["Hat", "es", "al\u00b7len\u00b7thal\u00b7ben", "Ha\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat es Leute, die da rasen;", "tokens": ["Hat", "es", "Leu\u00b7te", ",", "die", "da", "ra\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat auch demnach keine N\u00f6then", "tokens": ["Hat", "auch", "dem\u00b7nach", "kei\u00b7ne", "N\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PAV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An den Reimen und Poeten.", "tokens": ["An", "den", "Rei\u00b7men", "und", "Po\u00b7et\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}