{"textgrid.poem.23896": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Krieg und Frieden", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drau\u00dfen, drau\u00dfen,", "tokens": ["Drau\u00b7\u00dfen", ",", "drau\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Drau\u00dfen auf der Stra\u00dfe", "tokens": ["Drau\u00b7\u00dfen", "auf", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist der Krieg.", "tokens": ["Ist", "der", "Krieg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Raufend liegen wild sich", "tokens": ["Rau\u00b7fend", "lie\u00b7gen", "wild", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "PRF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "In frisierten Haaren", "tokens": ["In", "fri\u00b7sier\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wohlerzogene Gentlemen-", "tokens": ["Woh\u00b7ler\u00b7zo\u00b7ge\u00b7ne", "Gent\u00b7le\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJA", "TRUNC"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "R\u00e4uber und -Korsaren,", "tokens": ["R\u00e4u\u00b7ber", "und", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Glatte Amazonen", "tokens": ["Glat\u00b7te", "A\u00b7maz\u00b7o\u00b7nen"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Mit geschminkten Backen", "tokens": ["Mit", "ge\u00b7schmink\u00b7ten", "Ba\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Knieen augenfunkelnd", "tokens": ["Kni\u00b7e\u00b7en", "au\u00b7gen\u00b7fun\u00b7kelnd"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Auf besiegten Nacken,", "tokens": ["Auf", "be\u00b7sieg\u00b7ten", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Und der Beutebeutel ist der Sieg.", "tokens": ["Und", "der", "Beu\u00b7te\u00b7beu\u00b7tel", "ist", "der", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Innen, innen,", "tokens": ["In\u00b7nen", ",", "in\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Innen in der Stube,", "tokens": ["In\u00b7nen", "in", "der", "Stu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da ist's still,", "tokens": ["Da", "ist's", "still", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Zwischen meinen W\u00e4nden,", "tokens": ["Zwi\u00b7schen", "mei\u00b7nen", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Meinen bunten vieren,", "tokens": ["Mei\u00b7nen", "bun\u00b7ten", "vie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Geh ich mit dem Gl\u00fccke", "tokens": ["Geh", "ich", "mit", "dem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Arm in Arm spazieren,", "tokens": ["Arm", "in", "Arm", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Nehme auch zuweilen", "tokens": ["Neh\u00b7me", "auch", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Auf den Schoo\u00df die Dame,", "tokens": ["Auf", "den", "Schoo\u00df", "die", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "W\u00e4hrend ich in alten", "tokens": ["W\u00e4h\u00b7rend", "ich", "in", "al\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Raschelstr\u00e4u\u00dfen krame;", "tokens": ["Ra\u00b7schel\u00b7str\u00e4u\u00b7\u00dfen", "kra\u00b7me", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Komm herein zu mir, wer Frieden will!", "tokens": ["Komm", "her\u00b7ein", "zu", "mir", ",", "wer", "Frie\u00b7den", "will", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPER", "$,", "PWS", "NN", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}