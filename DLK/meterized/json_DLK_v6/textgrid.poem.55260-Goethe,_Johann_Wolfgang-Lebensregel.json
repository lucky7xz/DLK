{"textgrid.poem.55260": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Lebensregel", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du dir ein h\u00fcbsch Leben zimmern,", "tokens": ["Willst", "du", "dir", "ein", "h\u00fcbsch", "Le\u00b7ben", "zim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00dft dich ums Vergangne nicht bek\u00fcmmern;", "tokens": ["Mu\u00dft", "dich", "ums", "Ver\u00b7gang\u00b7ne", "nicht", "be\u00b7k\u00fcm\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das Wenigste mu\u00df dich verdrie\u00dfen;", "tokens": ["Das", "We\u00b7nigs\u00b7te", "mu\u00df", "dich", "ver\u00b7drie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mu\u00dft stets die Gegenwart genie\u00dfen,", "tokens": ["Mu\u00dft", "stets", "die", "Ge\u00b7gen\u00b7wart", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Besonders keinen Menschen hassen", "tokens": ["Be\u00b7son\u00b7ders", "kei\u00b7nen", "Men\u00b7schen", "has\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Zukunft Gott \u00fcberlassen.", "tokens": ["Und", "die", "Zu\u00b7kunft", "Gott", "\u00fc\u00b7ber\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}