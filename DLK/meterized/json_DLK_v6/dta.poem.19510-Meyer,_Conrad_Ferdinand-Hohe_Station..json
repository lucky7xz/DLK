{"dta.poem.19510": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Hohe Station.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch an der Windung des Passes bewohn' ich ein niedriges", "tokens": ["Hoch", "an", "der", "Win\u00b7dung", "des", "Pas\u00b7ses", "be\u00b7wohn'", "ich", "ein", "nied\u00b7ri\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+--+---+-+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Heut ist vor\u00fcber die Post, heut bin ich oben allein.", "tokens": ["Heut", "ist", "vor\u00b7\u00fc\u00b7ber", "die", "Post", ",", "heut", "bin", "ich", "o\u00b7ben", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Lehnend am Fenster belausch' ich die Stille des d\u00e4mmernden", "tokens": ["Leh\u00b7nend", "am", "Fens\u00b7ter", "be\u00b7lausch'", "ich", "die", "Stil\u00b7le", "des", "d\u00e4m\u00b7mern\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA"], "meter": "+--+--+--+-+++-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Rings kein Laut! Nur der Specht h\u00e4mmert im harzigen Tann.", "tokens": ["Rings", "kein", "Laut", "!", "Nur", "der", "Specht", "h\u00e4m\u00b7mert", "im", "har\u00b7zi\u00b7gen", "Tann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$.", "ADV", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Leicht aus dem Wald in den Wald h\u00fcpft \u00fcber die Matte das", "tokens": ["Leicht", "aus", "dem", "Wald", "in", "den", "Wald", "h\u00fcpft", "\u00fc\u00b7ber", "die", "Mat\u00b7te", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Spielend auf offenem Plan; denn es ist Herr im Bezirk.", "tokens": ["Spie\u00b7lend", "auf", "of\u00b7fe\u00b7nem", "Plan", ";", "denn", "es", "ist", "Herr", "im", "Be\u00b7zirk", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$.", "KON", "PPER", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Jammer! Was h\u00f6r' ich? Ein schrilles Gesurre: \u201eGemordet ist", "tokens": ["Jam\u00b7mer", "!", "Was", "h\u00f6r'", "ich", "?", "Ein", "schril\u00b7les", "Ge\u00b7sur\u00b7re", ":", "\u201e", "Ge\u00b7mor\u00b7det", "ist"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "$.", "PWS", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "$.", "$(", "VVPP", "VAFIN"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.8": {"text": "\u201ebismarck z\u00fcrnt im Gezelt!\u201c \u201eV\u00e4terlich segnet der Papst!\u201c", "tokens": ["\u201e", "bis\u00b7marck", "z\u00fcrnt", "im", "Ge\u00b7zelt", "!", "\u201c", "\u201e", "V\u00e4\u00b7ter\u00b7lich", "seg\u00b7net", "der", "Papst", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "APPRART", "NN", "$.", "$(", "$(", "ADJD", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-++-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Schwirrt in der Luft ein Ger\u00fccht? Was gewahr' ich? Ein", "tokens": ["Schwirrt", "in", "der", "Luft", "ein", "Ge\u00b7r\u00fccht", "?", "Was", "ge\u00b7wahr'", "ich", "?", "Ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "$.", "ART"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Unter dem Fenstergesims bebt der electrische Draht,", "tokens": ["Un\u00b7ter", "dem", "Fens\u00b7ter\u00b7ge\u00b7sims", "bebt", "der", "e\u00b7lect\u00b7ri\u00b7sche", "Draht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.11": {"text": "Der, wie die Schl\u00e4ge des Pulses beseelend den K\u00f6rper der", "tokens": ["Der", ",", "wie", "die", "Schl\u00e4\u00b7ge", "des", "Pul\u00b7ses", "be\u00b7see\u00b7lend", "den", "K\u00f6r\u00b7per", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVPP", "ART", "NN", "ART"], "meter": "-+-+--+--+--+--", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Durch das entlegenste Thal tr\u00e4gt die Geberde der Zeit.", "tokens": ["Durch", "das", "ent\u00b7le\u00b7gens\u00b7te", "Thal", "tr\u00e4gt", "die", "Ge\u00b7ber\u00b7de", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+---+", "measure": "trochaic.hexa.relaxed"}}}}}