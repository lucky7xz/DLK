{"textgrid.poem.63041": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am gr\u00fcnen Rand des Tajo gieng", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am gr\u00fcnen Rand des Tajo gieng", "tokens": ["Am", "gr\u00fc\u00b7nen", "Rand", "des", "Ta\u00b7jo", "gieng"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Scheerer auf die Jagd der Canthariden.", "tokens": ["Ein", "Schee\u00b7rer", "auf", "die", "Jagd", "der", "Can\u00b7tha\u00b7ri\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbtyrann! rief eine, die er fieng,", "tokens": ["\u00bb", "ty\u00b7rann", "!", "rief", "ei\u00b7ne", ",", "die", "er", "fi\u00b7eng", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Was n\u00fctzet dich mein Tod? la\u00df mich in Frieden!\u00ab", "tokens": ["Was", "n\u00fct\u00b7zet", "dich", "mein", "Tod", "?", "la\u00df", "mich", "in", "Frie\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "VVIMP", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sehr viel! sprach Podalirius:", "tokens": ["Sehr", "viel", "!", "sprach", "Po\u00b7da\u00b7li\u00b7rius", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn wisse, da\u00df ich, zart pulverisieret", "tokens": ["Denn", "wis\u00b7se", ",", "da\u00df", "ich", ",", "zart", "pul\u00b7ve\u00b7ri\u00b7sie\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Dich einem M\u00f6nch, der greulich phantasieret,", "tokens": ["Dich", "ei\u00b7nem", "M\u00f6nch", ",", "der", "greu\u00b7lich", "phan\u00b7ta\u00b7sie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Auf seinen Schedel streuen mu\u00df,", "tokens": ["Auf", "sei\u00b7nen", "Sche\u00b7del", "streu\u00b7en", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und, eh der Abend k\u00f6mmt, wird sein Verstand erwachen.", "tokens": ["Und", ",", "eh", "der", "A\u00b7bend", "k\u00f6mmt", ",", "wird", "sein", "Ver\u00b7stand", "er\u00b7wa\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O weh euch, armen Thierchen, flieht!", "tokens": ["O", "weh", "euch", ",", "ar\u00b7men", "Thier\u00b7chen", ",", "flieht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Rief eine Schnake: flieht vor diesem Drachen!", "tokens": ["Rief", "ei\u00b7ne", "Schna\u00b7ke", ":", "flieht", "vor", "die\u00b7sem", "Dra\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Denn, wenn durch euch die Heilkunst sich bem\u00fcht,", "tokens": ["Denn", ",", "wenn", "durch", "euch", "die", "Heil\u00b7kunst", "sich", "be\u00b7m\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "APPR", "PPER", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Den M\u00f6nchen zur Vernunft zu helfen,", "tokens": ["Den", "M\u00f6n\u00b7chen", "zur", "Ver\u00b7nunft", "zu", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "So reizet ihr den Zorn der Inquisition,", "tokens": ["So", "rei\u00b7zet", "ihr", "den", "Zorn", "der", "In\u00b7qui\u00b7si\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+---", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und es ergeht euch wie den W\u00f6lfen", "tokens": ["Und", "es", "er\u00b7geht", "euch", "wie", "den", "W\u00f6l\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Im m\u00f6rderischen Albion.", "tokens": ["Im", "m\u00f6r\u00b7de\u00b7ri\u00b7schen", "Al\u00b7bi\u00b7on", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}