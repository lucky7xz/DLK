{"textgrid.poem.57796": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ging einmal ein Wind,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging einmal ein Wind,", "tokens": ["Es", "ging", "ein\u00b7mal", "ein", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ei, ging einmal ein Wind;", "tokens": ["Ei", ",", "ging", "ein\u00b7mal", "ein", "Wind", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er ging wohl \u00fcber Stock und Stein,", "tokens": ["Er", "ging", "wohl", "\u00fc\u00b7ber", "Stock", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fand ein blaues Bl\u00fcmelein,", "tokens": ["Und", "fand", "ein", "blau\u00b7es", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das bracht er mir geschwind.", "tokens": ["Das", "bracht", "er", "mir", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und das hei\u00dft Ehrenpreis,", "tokens": ["Und", "das", "hei\u00dft", "Eh\u00b7ren\u00b7preis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ei, das hei\u00dft Ehrenpreis;", "tokens": ["Ei", ",", "das", "hei\u00dft", "Eh\u00b7ren\u00b7preis", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es bl\u00fcht nicht f\u00fcr die Ewigkeit,", "tokens": ["Es", "bl\u00fcht", "nicht", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es bl\u00fcht blo\u00df eine kurze Zeit,", "tokens": ["Es", "bl\u00fcht", "blo\u00df", "ei\u00b7ne", "kur\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann ist es welk und wei\u00df.", "tokens": ["Dann", "ist", "es", "welk", "und", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es hei\u00dft auch M\u00e4nnertreu,", "tokens": ["Es", "hei\u00dft", "auch", "M\u00e4n\u00b7ner\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ei, hei\u00dft auch M\u00e4nnertreu;", "tokens": ["Ei", ",", "hei\u00dft", "auch", "M\u00e4n\u00b7ner\u00b7treu", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Schatz, der mich so viel gek\u00fc\u00dft,", "tokens": ["Mein", "Schatz", ",", "der", "mich", "so", "viel", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df nicht, wo er blieben ist,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wo", "er", "blie\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Lieben ist vorbei.", "tokens": ["Das", "Lie\u00b7ben", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}