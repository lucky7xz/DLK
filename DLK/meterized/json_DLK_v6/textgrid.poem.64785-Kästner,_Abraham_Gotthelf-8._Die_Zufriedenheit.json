{"textgrid.poem.64785": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "8. Die Zufriedenheit", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zufriedenheit, du Quell von allem Gl\u00fccke,", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", ",", "du", "Quell", "von", "al\u00b7lem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Jeder sucht, und die doch Jedem fehlt,", "tokens": ["Die", "Je\u00b7der", "sucht", ",", "und", "die", "doch", "Je\u00b7dem", "fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "KON", "ART", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entdecke dich, sprich, wo vor unserm Blicke", "tokens": ["Ent\u00b7de\u00b7cke", "dich", ",", "sprich", ",", "wo", "vor", "un\u00b7serm", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich fern von uns ein selig Land verhehlt?", "tokens": ["Dich", "fern", "von", "uns", "ein", "se\u00b7lig", "Land", "ver\u00b7hehlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPER", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bot dich vielleicht auf unschiffbaren Meeren", "tokens": ["Bot", "dich", "viel\u00b7leicht", "auf", "un\u00b7schiff\u00b7ba\u00b7ren", "Mee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Vermuthlich nicht, weil ihm zur\u00fcckzukehren,", "tokens": ["Ver\u00b7muth\u00b7lich", "nicht", ",", "weil", "ihm", "zu\u00b7r\u00fcck\u00b7zu\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "KOUS", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mehr als das Reich von seiner Insel war.", "tokens": ["Mehr", "als", "das", "Reich", "von", "sei\u00b7ner", "In\u00b7sel", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nein, deine Lust erf\u00fcllt auch unsre Seelen,", "tokens": ["Nein", ",", "dei\u00b7ne", "Lust", "er\u00b7f\u00fcllt", "auch", "uns\u00b7re", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Doch wirst du nur den M\u00e4chtigen zu Theil:", "tokens": ["Doch", "wirst", "du", "nur", "den", "M\u00e4ch\u00b7ti\u00b7gen", "zu", "Theil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ja du erscheinst, wenn F\u00fcrsten dir befehlen,", "tokens": ["Ja", "du", "er\u00b7scheinst", ",", "wenn", "F\u00fcrs\u00b7ten", "dir", "be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Du bist um Gold ja f\u00fcr den Reichen feil.", "tokens": ["Du", "bist", "um", "Gold", "ja", "f\u00fcr", "den", "Rei\u00b7chen", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ach, w\u00fcrdest du nicht meine Brust begl\u00fccken,", "tokens": ["Ach", ",", "w\u00fcr\u00b7dest", "du", "nicht", "mei\u00b7ne", "Brust", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Bes\u00e4\u00df' ich auch, gleich Andern, Ehr und Geld?", "tokens": ["Be\u00b7s\u00e4\u00df'", "ich", "auch", ",", "gleich", "An\u00b7dern", ",", "Ehr", "und", "Geld", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ADV", "ADJA", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Was fehlet Dem, vor dem sich Sklaven b\u00fccken?", "tokens": ["Was", "feh\u00b7let", "Dem", ",", "vor", "dem", "sich", "Skla\u00b7ven", "b\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "$,", "APPR", "PRELS", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Den, frey von M\u00fch, der Schuldner Flei\u00df erh\u00e4lt?", "tokens": ["Den", ",", "frey", "von", "M\u00fch", ",", "der", "Schuld\u00b7ner", "Flei\u00df", "er\u00b7h\u00e4lt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "APPR", "NE", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gequ\u00e4lt von Neid und von vergebnem Kummer,", "tokens": ["Ge\u00b7qu\u00e4lt", "von", "Neid", "und", "von", "ver\u00b7geb\u00b7nem", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstrich mir so die H\u00e4lfte von der Nacht;", "tokens": ["Ver\u00b7strich", "mir", "so", "die", "H\u00e4lf\u00b7te", "von", "der", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der m\u00fcde Leib verfiel in einen Schlummer", "tokens": ["Der", "m\u00fc\u00b7de", "Leib", "ver\u00b7fiel", "in", "ei\u00b7nen", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von jener Art, dabey die Seele wacht.", "tokens": ["Von", "je\u00b7ner", "Art", ",", "da\u00b7bey", "die", "See\u00b7le", "wacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Schatten weicht; umgl\u00e4nzt mit reinem Lichte,", "tokens": ["Der", "Schat\u00b7ten", "weicht", ";", "um\u00b7gl\u00e4nzt", "mit", "rei\u00b7nem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Seh' ich vor mir ein himmlisch Bildni\u00df stehn,", "tokens": ["Seh'", "ich", "vor", "mir", "ein", "himm\u00b7lisch", "Bild\u00b7ni\u00df", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein holder Ernst erheitert ihr Gesichte,", "tokens": ["Ein", "hol\u00b7der", "Ernst", "er\u00b7hei\u00b7tert", "ihr", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie Alter klug, und wie die Jugend sch\u00f6n.", "tokens": ["Wie", "Al\u00b7ter", "klug", ",", "und", "wie", "die", "Ju\u00b7gend", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$,", "KON", "PWAV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie? Lie\u00df sie sich mit sanfter Strenge h\u00f6ren,", "tokens": ["Wie", "?", "Lie\u00df", "sie", "sich", "mit", "sanf\u00b7ter", "Stren\u00b7ge", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verwegner Mensch! klagst du die Vorsicht an?", "tokens": ["Ver\u00b7weg\u00b7ner", "Mensch", "!", "klagst", "du", "die", "Vor\u00b7sicht", "an", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wi\u00df', ihren Schlu\u00df vern\u00fcnftig zu verehren,", "tokens": ["Wi\u00df'", ",", "ih\u00b7ren", "Schlu\u00df", "ver\u00b7n\u00fcnf\u00b7tig", "zu", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "ADJD", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur dieses ist, was mich dir geben kann.", "tokens": ["Nur", "die\u00b7ses", "ist", ",", "was", "mich", "dir", "ge\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "$,", "PWS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bey ihm zu seyn, kann mir kein F\u00fcrst gebieten,", "tokens": ["Bey", "ihm", "zu", "seyn", ",", "kann", "mir", "kein", "F\u00fcrst", "ge\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VAINF", "$,", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Wenn Ehrsucht ihn, wie er die V\u00f6lker qu\u00e4lt.", "tokens": ["Wenn", "Ehr\u00b7sucht", "ihn", ",", "wie", "er", "die", "V\u00f6l\u00b7ker", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Man kauft mich nicht, um einen Schatz zu h\u00fcten,", "tokens": ["Man", "kauft", "mich", "nicht", ",", "um", "ei\u00b7nen", "Schatz", "zu", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den blasser Geiz mit Millionen z\u00e4hlt.", "tokens": ["Den", "blas\u00b7ser", "Geiz", "mit", "Mil\u00b7lion\u00b7en", "z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Aus zweyen eins hast du dir zu erw\u00e4hlen,", "tokens": ["Aus", "zwe\u00b7yen", "eins", "hast", "du", "dir", "zu", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIS", "VAFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den \u00e4u\u00dfern Glanz, die inn're Seelenruh:", "tokens": ["Den", "\u00e4u\u00b7\u00dfern", "Glanz", ",", "die", "inn'\u00b7re", "See\u00b7len\u00b7ruh", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Such' jenen erst, so werd' ich ewig fehlen,", "tokens": ["Such'", "je\u00b7nen", "erst", ",", "so", "werd'", "ich", "e\u00b7wig", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ziehst du mich vor, kommt er vielleicht dazu;", "tokens": ["Ziehst", "du", "mich", "vor", ",", "kommt", "er", "viel\u00b7leicht", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Doch sey gefa\u00dft, ohn' ihn begl\u00fcckt zu leben,", "tokens": ["Doch", "sey", "ge\u00b7fa\u00dft", ",", "ohn'", "ihn", "be\u00b7gl\u00fcckt", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,", "KOUI", "PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sonst wird er selbst dein wahres Elend seyn,", "tokens": ["Sonst", "wird", "er", "selbst", "dein", "wah\u00b7res", "E\u00b7lend", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Das, was dir n\u00fctzt, will dir die Allmacht geben;", "tokens": ["Das", ",", "was", "dir", "n\u00fctzt", ",", "will", "dir", "die", "All\u00b7macht", "ge\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "In diesen Satz schr\u00e4nk Wunsch und Hoffen ein.", "tokens": ["In", "die\u00b7sen", "Satz", "schr\u00e4nk", "Wunsch", "und", "Hof\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ein gr\u00f6\u00dfer Wohl, als Rang und Gold gew\u00e4hren,", "tokens": ["Ein", "gr\u00f6\u00b7\u00dfer", "Wohl", ",", "als", "Rang", "und", "Gold", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Hat dir die Huld der Vorsicht zugedacht;", "tokens": ["Hat", "dir", "die", "Huld", "der", "Vor\u00b7sicht", "zu\u00b7ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ein redlich Herz, gen\u00fcgsam in Begehren,", "tokens": ["Ein", "red\u00b7lich", "Herz", ",", "ge\u00b7n\u00fcg\u00b7sam", "in", "Be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und einen Geist, den Denken gl\u00fccklich macht.", "tokens": ["Und", "ei\u00b7nen", "Geist", ",", "den", "Den\u00b7ken", "gl\u00fcck\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}