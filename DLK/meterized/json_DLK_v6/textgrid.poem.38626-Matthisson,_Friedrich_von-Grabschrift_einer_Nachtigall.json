{"textgrid.poem.38626": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift einer Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still im Lorbeergeb\u00fcsch ruht Philomelens", "tokens": ["Still", "im", "Lor\u00b7beer\u00b7ge\u00b7b\u00fcsch", "ruht", "Phi\u00b7lo\u00b7me\u00b7lens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "NE"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Leichter Schleier. Die Liebesg\u00f6tter klagten", "tokens": ["Leich\u00b7ter", "Schlei\u00b7er", ".", "Die", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "klag\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Als ihr z\u00e4rtlicher Maigesang verstummte.", "tokens": ["Als", "ihr", "z\u00e4rt\u00b7li\u00b7cher", "Mai\u00b7ge\u00b7sang", "ver\u00b7stumm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Aber selig und frei entflog ihr Schatten", "tokens": ["A\u00b7ber", "se\u00b7lig", "und", "frei", "ent\u00b7flog", "ihr", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Zum elysischen Hain; dort neben Sapphos", "tokens": ["Zum", "e\u00b7ly\u00b7si\u00b7schen", "Hain", ";", "dort", "ne\u00b7ben", "Sap\u00b7phos"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "ADV", "APPR", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und Anakreons Amaranthenlaube", "tokens": ["Und", "A\u00b7nak\u00b7re\u00b7ons", "A\u00b7ma\u00b7ran\u00b7then\u00b7lau\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wohnt in ewiger Jugend nun die holde", "tokens": ["Wohnt", "in", "e\u00b7wi\u00b7ger", "Ju\u00b7gend", "nun", "die", "hol\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADV", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Fr\u00fchlingss\u00e4ngerin. Wirf ein Lorbeerbl\u00e4ttchen", "tokens": ["Fr\u00fch\u00b7lings\u00b7s\u00e4n\u00b7ge\u00b7rin", ".", "Wirf", "ein", "Lor\u00b7beer\u00b7bl\u00e4tt\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Auf ihr Grabmal, o Wandrer! ihren Manen", "tokens": ["Auf", "ihr", "Grab\u00b7mal", ",", "o", "Wand\u00b7rer", "!", "ih\u00b7ren", "Ma\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Opfr' ein liebendes Weib die erste Rose.", "tokens": ["Opf\u00b7r'", "ein", "lie\u00b7ben\u00b7des", "Weib", "die", "ers\u00b7te", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}