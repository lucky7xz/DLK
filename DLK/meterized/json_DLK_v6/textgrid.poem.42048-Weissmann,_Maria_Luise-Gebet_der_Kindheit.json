{"textgrid.poem.42048": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Gebet der Kindheit", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein schwarzer Hund bei Nacht, Herr Jesus,", "tokens": ["Es", "ist", "ein", "schwar\u00b7zer", "Hund", "bei", "Nacht", ",", "Herr", "Je\u00b7sus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der auf seinen Krallen die h\u00f6lzerne Stiege abw\u00e4rtsgeht.", "tokens": ["Der", "auf", "sei\u00b7nen", "Kral\u00b7len", "die", "h\u00f6l\u00b7zer\u00b7ne", "Stie\u00b7ge", "ab\u00b7w\u00e4rts\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Es ist ein wei\u00dfer Schatten manchesmal, Herr Jesus,", "tokens": ["Es", "ist", "ein", "wei\u00b7\u00dfer", "Schat\u00b7ten", "man\u00b7ches\u00b7mal", ",", "Herr", "Je\u00b7sus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Der fr\u00fch am Wegrand an der Weide steht.", "tokens": ["Der", "fr\u00fch", "am", "Weg\u00b7rand", "an", "der", "Wei\u00b7de", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Heiland, die Blume, die ich dir ges\u00e4t,", "tokens": ["Hei\u00b7land", ",", "die", "Blu\u00b7me", ",", "die", "ich", "dir", "ge\u00b7s\u00e4t", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Ragt immer h\u00f6her auf zu deinem Schein.", "tokens": ["Ragt", "im\u00b7mer", "h\u00f6\u00b7her", "auf", "zu", "dei\u00b7nem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du ziehst sie gro\u00df zu dir. Ich will ein Hirte sein,", "tokens": ["Du", "ziehst", "sie", "gro\u00df", "zu", "dir", ".", "Ich", "will", "ein", "Hir\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$.", "PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dann bin ich mit den L\u00e4mmern dir zusammen.", "tokens": ["Dann", "bin", "ich", "mit", "den", "L\u00e4m\u00b7mern", "dir", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die K\u00fcken sind ausgekrochen, aber dem einen fehlt ein Bein.", "tokens": ["Die", "K\u00fc\u00b7ken", "sind", "aus\u00b7ge\u00b7kro\u00b7chen", ",", "a\u00b7ber", "dem", "ei\u00b7nen", "fehlt", "ein", "Bein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ADV", "ART", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Die Mutter sagte vor deiner Kerze: Alle Flammen", "tokens": ["Die", "Mut\u00b7ter", "sag\u00b7te", "vor", "dei\u00b7ner", "Ker\u00b7ze", ":", "Al\u00b7le", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PIAT", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "M\u00fcnden zu deiner gro\u00dfen Sonne ein.", "tokens": ["M\u00fcn\u00b7den", "zu", "dei\u00b7ner", "gro\u00b7\u00dfen", "Son\u00b7ne", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}