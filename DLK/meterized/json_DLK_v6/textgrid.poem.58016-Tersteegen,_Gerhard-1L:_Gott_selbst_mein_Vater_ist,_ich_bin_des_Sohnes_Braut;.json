{"textgrid.poem.58016": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott selbst mein Vater ist, ich bin des Sohnes Braut;", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott selbst mein Vater ist, ich bin des Sohnes Braut;", "tokens": ["Gott", "selbst", "mein", "Va\u00b7ter", "ist", ",", "ich", "bin", "des", "Soh\u00b7nes", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "VAFIN", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Geist, das Pfand und Band, wodurch ich ihm vertraut.", "tokens": ["Sein", "Geist", ",", "das", "Pfand", "und", "Band", ",", "wo\u00b7durch", "ich", "ihm", "ver\u00b7traut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "KON", "NN", "$,", "PWAV", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott hat mir mehr geschenkt als allen Seraphinen,", "tokens": ["Gott", "hat", "mir", "mehr", "ge\u00b7schenkt", "als", "al\u00b7len", "Se\u00b7ra\u00b7phi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Engel stetig mich begleiten und bedienen,", "tokens": ["Die", "En\u00b7gel", "ste\u00b7tig", "mich", "be\u00b7glei\u00b7ten", "und", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich habe, was ich will, die ganze Welt ist mein,", "tokens": ["Ich", "ha\u00b7be", ",", "was", "ich", "will", ",", "die", "gan\u00b7ze", "Welt", "ist", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die H\u00f6lle f\u00fcrchtet mich, ich f\u00fcrchte Gott allein,", "tokens": ["Die", "H\u00f6l\u00b7le", "f\u00fcrch\u00b7tet", "mich", ",", "ich", "f\u00fcrch\u00b7te", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Im Himmel wandle ich als eine K\u00f6nigin:", "tokens": ["Im", "Him\u00b7mel", "wand\u00b7le", "ich", "als", "ei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sag, armes Weltkind, ob ich nicht 'was Gro\u00dfes bin!", "tokens": ["Sag", ",", "ar\u00b7mes", "Welt\u00b7kind", ",", "ob", "ich", "nicht", "'was", "Gro\u00b7\u00dfes", "bin", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}