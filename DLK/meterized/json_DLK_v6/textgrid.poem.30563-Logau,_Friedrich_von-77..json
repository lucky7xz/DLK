{"textgrid.poem.30563": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrstin, der Euch denckt zu preisen unter denen, die Euch kennen,", "tokens": ["F\u00fcrs\u00b7tin", ",", "der", "Euch", "denckt", "zu", "prei\u00b7sen", "un\u00b7ter", "de\u00b7nen", ",", "die", "Euch", "ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "PTKZU", "VVFIN", "APPR", "PRELS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Mu\u00df die Schuld nothwendig haben, da\u00df er nicht kunt alles nennen;", "tokens": ["Mu\u00df", "die", "Schuld", "noth\u00b7wen\u00b7dig", "ha\u00b7ben", ",", "da\u00df", "er", "nicht", "kunt", "al\u00b7les", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "VAINF", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Wer Euch wil bey denen loben, die vorher nichts von Euch wissen,", "tokens": ["Wer", "Euch", "wil", "bey", "de\u00b7nen", "lo\u00b7ben", ",", "die", "vor\u00b7her", "nichts", "von", "Euch", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "APPR", "PDS", "VVINF", "$,", "PRELS", "ADV", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "(derer wenig ich vermuthe), wird 