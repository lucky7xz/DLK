{"dta.poem.11726": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft nur Alles nicht verlangen gleich von allen,", "tokens": ["Du", "mu\u00dft", "nur", "Al\u00b7les", "nicht", "ver\u00b7lan\u00b7gen", "gleich", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "PTKNEG", "VVFIN", "ADV", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird in seiner Art dir alles wohlgefallen.", "tokens": ["So", "wird", "in", "sei\u00b7ner", "Art", "dir", "al\u00b7les", "wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn eine duftig riecht, die andre farbig gl\u00e4nzt,", "tokens": ["Wenn", "ei\u00b7ne", "duf\u00b7tig", "riecht", ",", "die", "and\u00b7re", "far\u00b7big", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "VVFIN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist von der einen sch\u00f6n die andre Blum' erg\u00e4nzt.", "tokens": ["Ist", "von", "der", "ei\u00b7nen", "sch\u00f6n", "die", "and\u00b7re", "Blum'", "er\u00b7g\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ART", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und ist die eine gar geruch- und farbenreich,", "tokens": ["Und", "ist", "die", "ei\u00b7ne", "gar", "ge\u00b7ruch", "und", "far\u00b7ben\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ART", "ADV", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlange nicht, sie sei auch s\u00fc\u00dfe Frucht zugleich.", "tokens": ["Ver\u00b7lan\u00b7ge", "nicht", ",", "sie", "sei", "auch", "s\u00fc\u00b7\u00dfe", "Frucht", "zu\u00b7gleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die sch\u00f6nste Blum' ist, in den Mund genommen, bitter;", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Blum'", "ist", ",", "in", "den", "Mund", "ge\u00b7nom\u00b7men", ",", "bit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn heimlich ist ein Gift in jedem Sinnenflitter.", "tokens": ["Denn", "heim\u00b7lich", "ist", "ein", "Gift", "in", "je\u00b7dem", "Sin\u00b7nen\u00b7flit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}