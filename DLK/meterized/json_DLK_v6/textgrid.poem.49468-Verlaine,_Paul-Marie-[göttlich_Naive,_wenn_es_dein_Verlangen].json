{"textgrid.poem.49468": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[g\u00f6ttlich Naive, wenn es dein Verlangen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttlich Naive, wenn es dein Verlangen,", "tokens": ["G\u00f6tt\u00b7lich", "Nai\u00b7ve", ",", "wenn", "es", "dein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "So bin ich nur des einen noch bewusst,", "tokens": ["So", "bin", "ich", "nur", "des", "ei\u00b7nen", "noch", "be\u00b7wusst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit kund'ger Hand dich schmeichelnd zu umfangen", "tokens": ["Mit", "kun\u00b7d'\u00b7ger", "Hand", "dich", "schmei\u00b7chelnd", "zu", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In irren Gluten fieberhafter Lust,", "tokens": ["In", "ir\u00b7ren", "Glu\u00b7ten", "fie\u00b7ber\u00b7haf\u00b7ter", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "G\u00f6ttlich Naive, wenn es dein Verlangen.", "tokens": ["G\u00f6tt\u00b7lich", "Nai\u00b7ve", ",", "wenn", "es", "dein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Versinken lass im Rausch uns ohne Scham", "tokens": ["Ver\u00b7sin\u00b7ken", "lass", "im", "Rausch", "uns", "oh\u00b7ne", "Scham"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Hirsch und Hindin tief in Waldesreichen;", "tokens": ["Wie", "Hirsch", "und", "Hin\u00b7din", "tief", "in", "Wal\u00b7des\u00b7rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Keuschheit, mag sie gehn, woher sie kam,", "tokens": ["Die", "Keuschheit", ",", "mag", "sie", "gehn", ",", "wo\u00b7her", "sie", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und nichts soll unserm dreisten Feuer gleichen,", "tokens": ["Und", "nichts", "soll", "un\u00b7serm", "dreis\u00b7ten", "Feu\u00b7er", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPOSAT", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Versinken lass im Rausch uns ohne Scham.", "tokens": ["Ver\u00b7sin\u00b7ken", "lass", "im", "Rausch", "uns", "oh\u00b7ne", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vor allem lass uns nichts von B\u00fcchern sagen,", "tokens": ["Vor", "al\u00b7lem", "lass", "uns", "nichts", "von", "B\u00fc\u00b7chern", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Teufel Leser, Dichter und Verlag,", "tokens": ["Zum", "Teu\u00b7fel", "Le\u00b7ser", ",", "Dich\u00b7ter", "und", "Ver\u00b7lag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir folgen der Natur in jungen Tagen,", "tokens": ["Wir", "fol\u00b7gen", "der", "Na\u00b7tur", "in", "jun\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die selig nichts von Fesseln wissen mag.", "tokens": ["Die", "se\u00b7lig", "nichts", "von", "Fes\u00b7seln", "wis\u00b7sen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJD", "PIS", "APPR", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und o, lass ja nichts uns von B\u00fcchern sagen.", "tokens": ["Und", "o", ",", "lass", "ja", "nichts", "uns", "von", "B\u00fc\u00b7chern", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "$,", "VVFIN", "ADV", "PIS", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Geniessen, schlafen, Liebste, meinst du nicht?", "tokens": ["Ge\u00b7nies\u00b7sen", ",", "schla\u00b7fen", ",", "Liebs\u00b7te", ",", "meinst", "du", "nicht", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Soll unsre h\u00f6chste Seligkeit uns geben,", "tokens": ["Soll", "uns\u00b7re", "h\u00f6chs\u00b7te", "Se\u00b7lig\u00b7keit", "uns", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur das allein sei unsre h\u00f6chste Pflicht,", "tokens": ["Nur", "das", "al\u00b7lein", "sei", "uns\u00b7re", "h\u00f6chs\u00b7te", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Gewissen uns und alles Licht im Leben.", "tokens": ["Ge\u00b7wis\u00b7sen", "uns", "und", "al\u00b7les", "Licht", "im", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Geniessen, schlafen, Liebste, meinst du nicht?", "tokens": ["Ge\u00b7nies\u00b7sen", ",", "schla\u00b7fen", ",", "Liebs\u00b7te", ",", "meinst", "du", "nicht", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}