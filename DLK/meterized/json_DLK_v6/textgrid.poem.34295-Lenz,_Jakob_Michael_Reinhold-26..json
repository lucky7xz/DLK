{"textgrid.poem.34295": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich dich besingen, Phyllis? \u2013 Nein!", "tokens": ["Ich", "dich", "be\u00b7sin\u00b7gen", ",", "Phyl\u00b7lis", "?", "\u2013", "Nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "PRF", "VVINF", "$,", "NE", "$.", "$(", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich f\u00fchle dich zu sehr, um jetzt nicht stumm zu sein.", "tokens": ["Ich", "f\u00fch\u00b7le", "dich", "zu", "sehr", ",", "um", "jetzt", "nicht", "stumm", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "$,", "KOUI", "ADV", "PTKNEG", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}