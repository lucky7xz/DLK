{"textgrid.poem.38820": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh die zarten Bl\u00fcthen keimen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh die zarten Bl\u00fcthen keimen", "tokens": ["Sieh", "die", "zar\u00b7ten", "Bl\u00fc\u00b7then", "kei\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie aus sich selbst erwachen,", "tokens": ["Wie", "sie", "aus", "sich", "selbst", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie Kinder aus den Tr\u00e4umen", "tokens": ["Und", "wie", "Kin\u00b7der", "aus", "den", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir entgegen lieblich lachen.", "tokens": ["Dir", "ent\u00b7ge\u00b7gen", "lieb\u00b7lich", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihre Farbe ist im Spielen", "tokens": ["Ih\u00b7re", "Far\u00b7be", "ist", "im", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zugekehrt der goldnen Sonne,", "tokens": ["Zu\u00b7ge\u00b7kehrt", "der", "gold\u00b7nen", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren heissen Ku\u00df zu f\u00fchlen,", "tokens": ["De\u00b7ren", "heis\u00b7sen", "Ku\u00df", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist ihre h\u00f6chste Wonne:", "tokens": ["Das", "ist", "ih\u00b7re", "h\u00f6chs\u00b7te", "Won\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "An den K\u00fcssen zu verschmachten,", "tokens": ["An", "den", "K\u00fcs\u00b7sen", "zu", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu vergehn in Lieb' und Wehmuth;", "tokens": ["Zu", "ver\u00b7gehn", "in", "Lieb'", "und", "Weh\u00b7muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Also stehn die eben lachten", "tokens": ["Al\u00b7so", "stehn", "die", "e\u00b7ben", "lach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald verwelkt in stiller Demuth.", "tokens": ["Bald", "ver\u00b7welkt", "in", "stil\u00b7ler", "De\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Das ist ihre h\u00f6chste Freude,", "tokens": ["Das", "ist", "ih\u00b7re", "h\u00f6chs\u00b7te", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Geliebten sich verzehren,", "tokens": ["Im", "Ge\u00b7lieb\u00b7ten", "sich", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich im Tode zu verkl\u00e4ren,", "tokens": ["Sich", "im", "To\u00b7de", "zu", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu vergehn in s\u00fcssem Leide.", "tokens": ["Zu", "ver\u00b7gehn", "in", "s\u00fcs\u00b7sem", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann ergie\u00dfen sie die D\u00fcfte,", "tokens": ["Dann", "er\u00b7gie\u00b7\u00dfen", "sie", "die", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Geister, mit Entz\u00fccken,", "tokens": ["Ih\u00b7re", "Geis\u00b7ter", ",", "mit", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es berauschen sich die L\u00fcfte", "tokens": ["Es", "be\u00b7rau\u00b7schen", "sich", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im balsamischen Erquicken.", "tokens": ["Im", "bal\u00b7sa\u00b7mi\u00b7schen", "Er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Liebe kommt zum Menschenherzen,", "tokens": ["Lie\u00b7be", "kommt", "zum", "Men\u00b7schen\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Regt die goldnen Saitenspiele,", "tokens": ["Regt", "die", "gold\u00b7nen", "Sai\u00b7ten\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Seele spricht: ich f\u00fchle", "tokens": ["Und", "die", "See\u00b7le", "spricht", ":", "ich", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was das Sch\u00f6nste sei, wonach ich ziele,", "tokens": ["Was", "das", "Sch\u00f6ns\u00b7te", "sei", ",", "wo\u00b7nach", "ich", "zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wehmuth, Sehnsucht und der Liebe Schmerzen.", "tokens": ["Weh\u00b7muth", ",", "Sehn\u00b7sucht", "und", "der", "Lie\u00b7be", "Schmer\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}