{"textgrid.poem.59262": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die ernsthafte Fastnacht", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl vor Wittenberg auf den Schanzen", "tokens": ["Wohl", "vor", "Wit\u00b7ten\u00b7berg", "auf", "den", "Schan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sind der edlen Werber viel,", "tokens": ["Sind", "der", "ed\u00b7len", "Wer\u00b7ber", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen da zur Fastnacht tanzen", "tokens": ["Wol\u00b7len", "da", "zur", "Fast\u00b7nacht", "tan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein gar seltsam Ritterspiel.", "tokens": ["Ein", "gar", "selt\u00b7sam", "Rit\u00b7ter\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Stadt vom Felsen droben", "tokens": ["Und", "die", "Stadt", "vom", "Fel\u00b7sen", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiegelt sich im Sonnenschein,", "tokens": ["Spie\u00b7gelt", "sich", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Jungfr\u00e4ulein erhoben \u2013", "tokens": ["Wie", "ein", "Jung\u00b7fr\u00e4u\u00b7lein", "er\u00b7ho\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder will ihr Br\u00e4ut'gam sein.", "tokens": ["Je\u00b7der", "will", "ihr", "Br\u00e4ut'\u00b7gam", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "NE", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "J\u00e4ger! la\u00dft die H\u00f6rner klingen", "tokens": ["J\u00e4\u00b7ger", "!", "la\u00dft", "die", "H\u00f6r\u00b7ner", "klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Morgen kalt und blank!", "tokens": ["Durch", "den", "Mor\u00b7gen", "kalt", "und", "blank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl, sie l\u00e4\u00dft sich noch bezwingen,", "tokens": ["Wohl", ",", "sie", "l\u00e4\u00dft", "sich", "noch", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt sie alten deutschen Klang.", "tokens": ["H\u00f6rt", "sie", "al\u00b7ten", "deut\u00b7schen", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drauf sie einen Reiter schnelle", "tokens": ["Drauf", "sie", "ei\u00b7nen", "Rei\u00b7ter", "schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Senden, der so fr\u00f6hlich schaut,", "tokens": ["Sen\u00b7den", ",", "der", "so", "fr\u00f6h\u00b7lich", "schaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der bl\u00e4st seinen Gru\u00df so helle,", "tokens": ["Der", "bl\u00e4st", "sei\u00b7nen", "Gru\u00df", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADV", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wirbt da um die stolze Braut.", "tokens": ["Wirbt", "da", "um", "die", "stol\u00b7ze", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbsieh, wir werben lang verstohlen", "tokens": ["\u00bb", "sieh", ",", "wir", "wer\u00b7ben", "lang", "ver\u00b7stoh\u00b7len"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "$,", "PPER", "VAFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon um dich in Not und Tod,", "tokens": ["Schon", "um", "dich", "in", "Not", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm! sonst wollen wir dich holen,", "tokens": ["Komm", "!", "sonst", "wol\u00b7len", "wir", "dich", "ho\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann der Mond scheint blutig rot!\u00ab", "tokens": ["Wann", "der", "Mond", "scheint", "blu\u00b7tig", "rot", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADJD", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Bleich schon fallen Abendlichter \u2013", "tokens": ["Bleich", "schon", "fal\u00b7len", "A\u00b7bend\u00b7lich\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Reiter bl\u00e4st nur zu,", "tokens": ["Und", "der", "Rei\u00b7ter", "bl\u00e4st", "nur", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nacht schon webt sich dicht und dichter \u2013", "tokens": ["Nacht", "schon", "webt", "sich", "dicht", "und", "dich\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch das Tor bleibt immer zu.", "tokens": ["Doch", "das", "Tor", "bleibt", "im\u00b7mer", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun so spielt denn, Musikanten,", "tokens": ["Nun", "so", "spielt", "denn", ",", "Mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blast zum Tanz aus frischer Brust!", "tokens": ["Blast", "zum", "Tanz", "aus", "fri\u00b7scher", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herz und Sinne mir entbrannten,", "tokens": ["Herz", "und", "Sin\u00b7ne", "mir", "ent\u00b7brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O du sch\u00f6ne, wilde Lust!", "tokens": ["O", "du", "sch\u00f6\u00b7ne", ",", "wil\u00b7de", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wer hat je so 'n Saal gesehen?", "tokens": ["Wer", "hat", "je", "so", "'n", "Saal", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Strom und W\u00e4lder spielen auf,", "tokens": ["Strom", "und", "W\u00e4l\u00b7der", "spie\u00b7len", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterne auf und nieder gehen,", "tokens": ["Ster\u00b7ne", "auf", "und", "nie\u00b7der", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stecken hoch die Lampen auf.", "tokens": ["Ste\u00b7cken", "hoch", "die", "Lam\u00b7pen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ja der Herr leucht't selbst zum Tanze,", "tokens": ["Ja", "der", "Herr", "leucht't", "selbst", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frisch denn, Kameraden mein!", "tokens": ["Frisch", "denn", ",", "Ka\u00b7me\u00b7ra\u00b7den", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "$,", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Funkelnd sch\u00f6n im Mondesglanze", "tokens": ["Fun\u00b7kelnd", "sch\u00f6n", "im", "Mon\u00b7des\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Strenges Lieb, mu\u00dft unser sein! \u2013", "tokens": ["Stren\u00b7ges", "Lieb", ",", "mu\u00dft", "un\u00b7ser", "sein", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPOSAT", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und es kam der Morgen heiter,", "tokens": ["Und", "es", "kam", "der", "Mor\u00b7gen", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mancher T\u00e4nzer lag da tot,", "tokens": ["Man\u00b7cher", "T\u00e4n\u00b7zer", "lag", "da", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Von dem Wall ins Morgenrot.", "tokens": ["Von", "dem", "Wall", "ins", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Schlesier wohl zu Ruhm und Preise", "tokens": ["Schle\u00b7sier", "wohl", "zu", "Ruhm", "und", "Prei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben sich dies Lieb gewonnen,", "tokens": ["Ha\u00b7ben", "sich", "dies", "Lieb", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PDS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Schlesier diese Weise", "tokens": ["Und", "ein", "Schle\u00b7sier", "die\u00b7se", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Recht aus Herzenslust ersonnen.", "tokens": ["Recht", "aus", "Her\u00b7zens\u00b7lust", "er\u00b7son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}