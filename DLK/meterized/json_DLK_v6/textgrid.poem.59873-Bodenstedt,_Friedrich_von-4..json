{"textgrid.poem.59873": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hielt mich auf der Stra\u00dfe an", "tokens": ["Sie", "hielt", "mich", "auf", "der", "Stra\u00b7\u00dfe", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fragte: \u00bbKannst du schreiben?\u00ab \u2013 \u00bbJa!\u00ab \u2013", "tokens": ["Und", "frag\u00b7te", ":", "\u00bb", "Kannst", "du", "schrei\u00b7ben", "?", "\u00ab", "\u2013", "\u00bb", "Ja", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "VMFIN", "PPER", "VVINF", "$.", "$(", "$(", "$(", "PTKANT", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbso schreib mir einen Talisman!\u00ab", "tokens": ["\u00bb", "so", "schreib", "mir", "ei\u00b7nen", "Ta\u00b7lis\u00b7man", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 \u00bbWird der dein Weh vertreiben?\u00ab \u2013\u00bbJa!\u00ab", "tokens": ["\u2013", "\u00bb", "Wird", "der", "dein", "Weh", "ver\u00b7trei\u00b7ben", "?", "\u00ab", "\u2013", "\u00bb", "Ja", "!", "\u00ab"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct"], "pos": ["$(", "$(", "VAFIN", "ART", "PPOSAT", "NN", "VVINF", "$.", "$(", "$(", "$(", "PTKANT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich griff sofort zum Kalemdan.", "tokens": ["Ich", "griff", "so\u00b7fort", "zum", "Ka\u00b7lem\u00b7dan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbkomm\u00ab \u2013 sprach sie \u2013 \u00bbtreten wir ins Haus,", "tokens": ["\u00bb", "komm", "\u00ab", "\u2013", "sprach", "sie", "\u2013", "\u00bb", "tre\u00b7ten", "wir", "ins", "Haus", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$(", "$(", "VVFIN", "PPER", "$(", "$(", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort schreibst du mir den Talisman!\u00ab", "tokens": ["Dort", "schreibst", "du", "mir", "den", "Ta\u00b7lis\u00b7man", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 \u00bbUnd darf dann bei dir bleiben?\u00ab \u2013 \u00bbJa!\u00ab", "tokens": ["\u2013", "\u00bb", "Und", "darf", "dann", "bei", "dir", "blei\u00b7ben", "?", "\u00ab", "\u2013", "\u00bb", "Ja", "!", "\u00ab"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct"], "pos": ["$(", "$(", "KON", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$.", "$(", "$(", "$(", "PTKANT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit ihr ins Haus trat ich alsdann ...", "tokens": ["Mit", "ihr", "ins", "Haus", "trat", "ich", "als\u00b7dann", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mirza-Schaffy, es w\u00e4hrte lang!", "tokens": ["Mir\u00b7za\u00b7Schaffy", ",", "es", "w\u00e4hr\u00b7te", "lang", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch: schriebst du ihr den Talisman?", "tokens": ["Doch", ":", "schriebst", "du", "ihr", "den", "Ta\u00b7lis\u00b7man", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und half dein langes Bleiben? \u2013 Ja! \u2013", "tokens": ["Und", "half", "dein", "lan\u00b7ges", "Blei\u00b7ben", "?", "\u2013", "Ja", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$(", "PTKANT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}