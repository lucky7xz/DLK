{"textgrid.poem.62075": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von den h\u00f6chsten Bergen", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von den h\u00f6chsten Bergen", "tokens": ["Von", "den", "h\u00f6chs\u00b7ten", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Kommst Du so weit her!", "tokens": ["Kommst", "Du", "so", "weit", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wei\u00dfe, sammtne Blume", "tokens": ["Wei\u00b7\u00dfe", ",", "sammt\u00b7ne", "Blu\u00b7me"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Interessierst mich sehr.", "tokens": ["In\u00b7ter\u00b7es\u00b7sierst", "mich", "sehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hast gar viel gesehen,", "tokens": ["Hast", "gar", "viel", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fels und Berg und Tal,", "tokens": ["Fels", "und", "Berg", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "All' die gr\u00fcnen Seen,", "tokens": ["All'", "die", "gr\u00fc\u00b7nen", "Seen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wunder ohne Zahl.", "tokens": ["Wun\u00b7der", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und des Eises Grotte,", "tokens": ["Und", "des", "Ei\u00b7ses", "Grot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und des Gletschers Wand,", "tokens": ["Und", "des", "Glet\u00b7schers", "Wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Rauschende Luzzine,", "tokens": ["Rau\u00b7schen\u00b7de", "Luz\u00b7zi\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+--++-", "measure": "dactylic.init"}, "line.4": {"text": "Schwarz und wei\u00df genannt.", "tokens": ["Schwarz", "und", "wei\u00df", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und den Savoyarden,", "tokens": ["Und", "den", "Sa\u00b7vo\u00b7yar\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Streckend aus die Hand,", "tokens": ["Stre\u00b7ckend", "aus", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seine dunklen Blicke,", "tokens": ["Sei\u00b7ne", "dunk\u00b7len", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Flehend, festgebannt.", "tokens": ["Fle\u00b7hend", ",", "fest\u00b7ge\u00b7bannt", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Viel hast Du gesehen,", "tokens": ["Viel", "hast", "Du", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Fels und Berg und Tal,", "tokens": ["Fels", "und", "Berg", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Eis und Schnee und Seen,", "tokens": ["Eis", "und", "Schnee", "und", "Seen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wunder ohne Zahl.", "tokens": ["Wun\u00b7der", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Deine Heimat, Bl\u00fcmlein,", "tokens": ["Dei\u00b7ne", "Hei\u00b7mat", ",", "Bl\u00fcm\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Edelwei\u00df genannt,", "tokens": ["E\u00b7del\u00b7wei\u00df", "ge\u00b7nannt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ist ein kleines Eden,", "tokens": ["Ist", "ein", "klei\u00b7nes", "E\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sch\u00f6n das Schweizerland.", "tokens": ["Sch\u00f6n", "das", "Schwei\u00b7zer\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}