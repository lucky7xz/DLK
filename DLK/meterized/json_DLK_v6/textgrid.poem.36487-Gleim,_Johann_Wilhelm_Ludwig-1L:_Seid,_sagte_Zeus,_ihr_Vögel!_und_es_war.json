{"textgrid.poem.36487": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seid, sagte Zeus, ihr V\u00f6gel! und es war", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid, sagte Zeus, ihr V\u00f6gel! und es war", "tokens": ["Seid", ",", "sag\u00b7te", "Zeus", ",", "ihr", "V\u00f6\u00b7gel", "!", "und", "es", "war"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "$,", "VVFIN", "NE", "$,", "PPOSAT", "NN", "$.", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ganze V\u00f6gelschar,", "tokens": ["Die", "gan\u00b7ze", "V\u00f6\u00b7gel\u00b7schar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf Feldern, und in B\u00fcschen!", "tokens": ["Auf", "Fel\u00b7dern", ",", "und", "in", "B\u00fc\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Kuckuck rufte Stundenlang,", "tokens": ["Der", "Ku\u00b7ckuck", "ruf\u00b7te", "Stun\u00b7den\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wachtel schlug, die Lerche sang,", "tokens": ["Die", "Wach\u00b7tel", "schlug", ",", "die", "Ler\u00b7che", "sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sperling zwitscherte dazwischen.", "tokens": ["Der", "Sper\u00b7ling", "zwit\u00b7scher\u00b7te", "da\u00b7zwi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch alle schwiegen auf einmal;", "tokens": ["Doch", "al\u00b7le", "schwie\u00b7gen", "auf", "ein\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und pl\u00f6tzlich lie\u00df aus einem Thal", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "lie\u00df", "aus", "ei\u00b7nem", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nachtigall ihr Lied erschallen,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "ihr", "Lied", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Vater Zeus zum Wohlgefallen!", "tokens": ["Dem", "Va\u00b7ter", "Zeus", "zum", "Wohl\u00b7ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sing, sagte Vater Zeus, du Kleine! noch einmal!", "tokens": ["Sing", ",", "sag\u00b7te", "Va\u00b7ter", "Zeus", ",", "du", "Klei\u00b7ne", "!", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NN", "NE", "$,", "PPER", "ADJA", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}