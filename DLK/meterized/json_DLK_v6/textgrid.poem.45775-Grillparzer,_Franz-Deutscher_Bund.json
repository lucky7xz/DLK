{"textgrid.poem.45775": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Deutscher Bund", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der deutsche Bund war nicht schlecht von Haus,", "tokens": ["Der", "deut\u00b7sche", "Bund", "war", "nicht", "schlecht", "von", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gab auch Schutz in jeder F\u00e4hrlichkeit,", "tokens": ["Gab", "auch", "Schutz", "in", "je\u00b7der", "F\u00e4hr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nur setzt' er etwas Altmodisches voraus:", "tokens": ["Nur", "setzt'", "er", "et\u00b7was", "Alt\u00b7mo\u00b7di\u00b7sches", "vo\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Treue und die Ehrlichkeit.", "tokens": ["Die", "Treu\u00b7e", "und", "die", "Ehr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}