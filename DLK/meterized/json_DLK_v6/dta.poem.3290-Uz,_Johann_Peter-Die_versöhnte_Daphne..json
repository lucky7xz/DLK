{"dta.poem.3290": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die vers\u00f6hnte Daphne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Schatten einer alten Eiche", "tokens": ["Jm", "Schat\u00b7ten", "ei\u00b7ner", "al\u00b7ten", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sa\u00df Daphne, da die Sonne wich;", "tokens": ["Sa\u00df", "Daph\u00b7ne", ",", "da", "die", "Son\u00b7ne", "wich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als in dem einsamen Gestr\u00e4uche", "tokens": ["Als", "in", "dem", "ein\u00b7sa\u00b7men", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Myrtill sich ihr zur Seite schlich.", "tokens": ["Myr\u00b7till", "sich", "ihr", "zur", "Sei\u00b7te", "schlich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er will den Liljenhals umfassen,", "tokens": ["Er", "will", "den", "Lil\u00b7jen\u00b7hals", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der seinen K\u00fcssen sich entzieht.", "tokens": ["Der", "sei\u00b7nen", "K\u00fcs\u00b7sen", "sich", "ent\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts, leider! wird ihm zugelassen:", "tokens": ["Nichts", ",", "lei\u00b7der", "!", "wird", "ihm", "zu\u00b7ge\u00b7las\u00b7sen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "$.", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie rafft sich zornig auf und flieht.", "tokens": ["Sie", "rafft", "sich", "zor\u00b7nig", "auf", "und", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was wird von Sch\u00f6nen uns versaget,", "tokens": ["Was", "wird", "von", "Sch\u00f6\u00b7nen", "uns", "ver\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das k\u00fchne Schalkheit nicht erpresst", "tokens": ["Das", "k\u00fch\u00b7ne", "Schalk\u00b7heit", "nicht", "er\u00b7presst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da Daphne flieht und fliehend klaget,", "tokens": ["Da", "Daph\u00b7ne", "flieht", "und", "flie\u00b7hend", "kla\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4lt ihr Myrtill sie schmeichlend fest.", "tokens": ["H\u00e4lt", "ihr", "Myr\u00b7till", "sie", "schmeich\u00b7lend", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Myrtill erzwingt von Daphnen K\u00fcsse,", "tokens": ["Myr\u00b7till", "er\u00b7zwingt", "von", "Daph\u00b7nen", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihre Hand nur schwach bek\u00e4mpft:", "tokens": ["Die", "ih\u00b7re", "Hand", "nur", "schwach", "be\u00b7k\u00e4mpft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn, ach! ein Ku\u00df ist viel zu s\u00fcsse!", "tokens": ["Denn", ",", "ach", "!", "ein", "Ku\u00df", "ist", "viel", "zu", "s\u00fcs\u00b7se", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Ku\u00df hat manchen Zwist ged\u00e4mpft.", "tokens": ["Ein", "Ku\u00df", "hat", "man\u00b7chen", "Zwist", "ge\u00b7d\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie schl\u00e4gt die Augen schamroth nieder:", "tokens": ["Sie", "schl\u00e4gt", "die", "Au\u00b7gen", "scham\u00b7roth", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das bl\u00f6de M\u00e4dchen thut sich Zwang", "tokens": ["Das", "bl\u00f6\u00b7de", "M\u00e4d\u00b7chen", "thut", "sich", "Zwang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eifert auf gewisse Lieder,", "tokens": ["Und", "ei\u00b7fert", "auf", "ge\u00b7wis\u00b7se", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die j\u00fcngst Myrtill der Chloe sang.", "tokens": ["Die", "j\u00fcngst", "Myr\u00b7till", "der", "Chloe", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Doch, f\u00e4hrt sie fort, um dir zu zeigen,", "tokens": ["Doch", ",", "f\u00e4hrt", "sie", "fort", ",", "um", "dir", "zu", "zei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich mit dir nicht z\u00fcrnen will;", "tokens": ["Da\u00df", "ich", "mit", "dir", "nicht", "z\u00fcr\u00b7nen", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will zu neuem Frevel schweigen;", "tokens": ["Ich", "will", "zu", "neu\u00b7em", "Fre\u00b7vel", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00fc\u00df immer noch einmal, Myrtill!", "tokens": ["K\u00fc\u00df", "im\u00b7mer", "noch", "ein\u00b7mal", ",", "Myr\u00b7till", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "$,", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}