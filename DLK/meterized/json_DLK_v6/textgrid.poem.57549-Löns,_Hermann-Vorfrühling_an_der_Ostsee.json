{"textgrid.poem.57549": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Vorfr\u00fchling an der Ostsee", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hellgr\u00fcne Felder,", "tokens": ["Hell\u00b7gr\u00fc\u00b7ne", "Fel\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Braungr\u00fcne W\u00e4lder,", "tokens": ["Braun\u00b7gr\u00fc\u00b7ne", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.3": {"text": "Jubelnder, jauchzender Singdrosselsang,", "tokens": ["Ju\u00b7beln\u00b7der", ",", "jauch\u00b7zen\u00b7der", "Sing\u00b7dros\u00b7sel\u00b7sang", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Erlstr\u00e4ucher bl\u00fchen,", "tokens": ["Erl\u00b7str\u00e4u\u00b7cher", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Strandl\u00e4ufer ziehen", "tokens": ["Strand\u00b7l\u00e4u\u00b7fer", "zie\u00b7hen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Lockend und trillernd die D\u00fcnung entlang.", "tokens": ["Lo\u00b7ckend", "und", "tril\u00b7lernd", "die", "D\u00fc\u00b7nung", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit wunderbaren", "tokens": ["Mit", "wun\u00b7der\u00b7ba\u00b7ren"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Brandroten Haaren", "tokens": ["Brand\u00b7ro\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Prangend ein K\u00f6pfchen im Arme mir lehnt,", "tokens": ["Pran\u00b7gend", "ein", "K\u00f6pf\u00b7chen", "im", "Ar\u00b7me", "mir", "lehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Schneewei\u00dfe Stirne,", "tokens": ["Schnee\u00b7wei\u00b7\u00dfe", "Stir\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Blutjunge Dirne,", "tokens": ["Blut\u00b7jun\u00b7ge", "Dir\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.6": {"text": "Sehns\u00fcchtig schwer sich der Busen ihr dehnt.", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "schwer", "sich", "der", "Bu\u00b7sen", "ihr", "dehnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PRF", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Fern schimmert R\u00fcgen.", "tokens": ["Fern", "schim\u00b7mert", "R\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In m\u00e4chtigen Fl\u00fcgen", "tokens": ["In", "m\u00e4ch\u00b7ti\u00b7gen", "Fl\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Klingeln die Wildenten \u00fcber uns her,", "tokens": ["Klin\u00b7geln", "die", "Wil\u00b7den\u00b7ten", "\u00fc\u00b7ber", "uns", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Schneewei\u00dfe M\u00f6vchen", "tokens": ["Schnee\u00b7wei\u00b7\u00dfe", "M\u00f6v\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.5": {"text": "Und Wolkensch\u00e4fchen,", "tokens": ["Und", "Wol\u00b7ken\u00b7sch\u00e4f\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Hellblauer Himmel und tiefblaues Meer.", "tokens": ["Hell\u00b7blau\u00b7er", "Him\u00b7mel", "und", "tief\u00b7blau\u00b7es", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Fr\u00f6stelt dich, Kleine?", "tokens": ["Fr\u00f6s\u00b7telt", "dich", ",", "Klei\u00b7ne", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Am Rande vom Haine", "tokens": ["Am", "Ran\u00b7de", "vom", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wei\u00df ich ein Wirtshaus, von Pappeln umdr\u00e4ngt,", "tokens": ["Wei\u00df", "ich", "ein", "Wirts\u00b7haus", ",", "von", "Pap\u00b7peln", "um\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit schelmischem Blicke", "tokens": ["Mit", "schel\u00b7mi\u00b7schem", "Bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die kornblonde, dicke", "tokens": ["Die", "korn\u00b7blon\u00b7de", ",", "di\u00b7cke"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Fangschiffers Witwe dort Gl\u00fchwein versch\u00e4nkt.", "tokens": ["Fang\u00b7schif\u00b7fers", "Wit\u00b7we", "dort", "Gl\u00fch\u00b7wein", "ver\u00b7sch\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Schatz, einen Halben!", "tokens": ["Schatz", ",", "ei\u00b7nen", "Hal\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nicht wie die Schwalben,", "tokens": ["Nicht", "wie", "die", "Schwal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "T\u00fcchtig geschluckt, da\u00df die Backe dir gl\u00fcht!", "tokens": ["T\u00fcch\u00b7tig", "ge\u00b7schluckt", ",", "da\u00df", "die", "Ba\u00b7cke", "dir", "gl\u00fcht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Auf deinen Wangen", "tokens": ["Auf", "dei\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Soll Abendrot prangen.", "tokens": ["Soll", "A\u00b7ben\u00b7drot", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Reichliche Ku\u00dfernte draus mir erbl\u00fcht!", "tokens": ["Reich\u00b7li\u00b7che", "Ku\u00b7\u00dfern\u00b7te", "draus", "mir", "er\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "PPER", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Pferdegetrappel.", "tokens": ["Pfer\u00b7de\u00b7ge\u00b7trap\u00b7pel", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unter der Pappel", "tokens": ["Un\u00b7ter", "der", "Pap\u00b7pel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "H\u00e4lt schon Johann mit dem Rappengespann.", "tokens": ["H\u00e4lt", "schon", "Jo\u00b7hann", "mit", "dem", "Rap\u00b7pen\u00b7ge\u00b7spann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schatz, in den Wagen!", "tokens": ["Schatz", ",", "in", "den", "Wa\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Hannes, nun jagen!", "tokens": ["Han\u00b7nes", ",", "nun", "ja\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Aber beim M\u00fchlgarten, da halte an.", "tokens": ["A\u00b7ber", "beim", "M\u00fchl\u00b7gar\u00b7ten", ",", "da", "hal\u00b7te", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+--++--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "In Pelzverstecke,", "tokens": ["In", "Pelz\u00b7ver\u00b7ste\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Kopftuch und Decke", "tokens": ["Kopf\u00b7tuch", "und", "De\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wickle das fuchsrote P\u00fcppchen ich ein.", "tokens": ["Wick\u00b7le", "das", "fuchs\u00b7ro\u00b7te", "P\u00fcpp\u00b7chen", "ich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Vogelbeerb\u00e4ume", "tokens": ["Vo\u00b7gel\u00b7beer\u00b7b\u00e4u\u00b7me"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Fliegen wie Tr\u00e4ume,", "tokens": ["Flie\u00b7gen", "wie", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Klippklapp der Hufe auf spitzem Gestein.", "tokens": ["Klipp\u00b7klapp", "der", "Hu\u00b7fe", "auf", "spit\u00b7zem", "Ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Vor uns ein Schimmern,", "tokens": ["Vor", "uns", "ein", "Schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Funkeln und Flimmern,", "tokens": ["Fun\u00b7keln", "und", "Flim\u00b7mern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "R\u00e4dergerassel und ferne Musik,", "tokens": ["R\u00e4\u00b7der\u00b7ge\u00b7ras\u00b7sel", "und", "fer\u00b7ne", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Leuchtende Fenster,", "tokens": ["Leuch\u00b7ten\u00b7de", "Fens\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Rudtanzgespenster,", "tokens": ["Rud\u00b7tanz\u00b7ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Brummba\u00dfgerummel und Fiedelgequiek.", "tokens": ["Brumm\u00b7ba\u00df\u00b7ge\u00b7rum\u00b7mel", "und", "Fie\u00b7del\u00b7ge\u00b7qui\u00b7ek", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Halt! Schatz, nun schnelle,", "tokens": ["Halt", "!", "Schatz", ",", "nun", "schnel\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Horch, die Kapelle", "tokens": ["Horch", ",", "die", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Spielt unsern Leibtanz, die Kreuzpolka, schon,", "tokens": ["Spielt", "un\u00b7sern", "Leib\u00b7tanz", ",", "die", "Kreuz\u00b7pol\u00b7ka", ",", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "ADV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wehende R\u00f6cke,", "tokens": ["We\u00b7hen\u00b7de", "R\u00f6\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dr\u00f6hnende Decke,", "tokens": ["Dr\u00f6h\u00b7nen\u00b7de", "De\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Trappelnder Tritte takthaltender Ton.", "tokens": ["Trap\u00b7peln\u00b7der", "Trit\u00b7te", "tak\u00b7thal\u00b7ten\u00b7der", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Wirbelndes Fliegen,", "tokens": ["Wir\u00b7beln\u00b7des", "Flie\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Leiberumschmiegen,", "tokens": ["Lei\u00b7be\u00b7rum\u00b7schmie\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Noch einmal rund und die Polka ist aus.", "tokens": ["Noch", "ein\u00b7mal", "rund", "und", "die", "Pol\u00b7ka", "ist", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schnell in den Wagen!", "tokens": ["Schnell", "in", "den", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Hanns, wieder jagen!", "tokens": ["Hanns", ",", "wie\u00b7der", "ja\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Herzliebes Schatzing, gleich sind wir zu Haus.", "tokens": ["Herz\u00b7lie\u00b7bes", "Schat\u00b7zing", ",", "gleich", "sind", "wir", "zu", "Haus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}