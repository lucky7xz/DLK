{"textgrid.poem.41696": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sahn der Sterne licht Gefunkel,", "tokens": ["Wir", "sahn", "der", "Ster\u00b7ne", "licht", "Ge\u00b7fun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sahen W\u00fcstensand und Wellen ungez\u00e4hlt;", "tokens": ["Wir", "sa\u00b7hen", "W\u00fcs\u00b7ten\u00b7sand", "und", "Wel\u00b7len", "un\u00b7ge\u00b7z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Trotz manchem Ungl\u00fccksschlag, trotz Sturm und Wetterdunkel", "tokens": ["Trotz", "man\u00b7chem", "Un\u00b7gl\u00fccks\u00b7schlag", ",", "trotz", "Sturm", "und", "Wet\u00b7ter\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat Langeweile uns ganz so wie hier gequ\u00e4lt.", "tokens": ["Hat", "Lan\u00b7ge\u00b7wei\u00b7le", "uns", "ganz", "so", "wie", "hier", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "ADV", "ADV", "KOKOM", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Sonne Glorie auf den veilchenfarbnen Meeren,", "tokens": ["Der", "Son\u00b7ne", "Glo\u00b7rie", "auf", "den", "veil\u00b7chen\u00b7farb\u00b7nen", "Mee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der St\u00e4dte Glorie, wann die Sonne leuchtend sinkt,", "tokens": ["Der", "St\u00e4d\u00b7te", "Glo\u00b7rie", ",", "wann", "die", "Son\u00b7ne", "leuch\u00b7tend", "sinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entz\u00fcndeten in uns ein ruhelos Begehren", "tokens": ["Ent\u00b7z\u00fcn\u00b7de\u00b7ten", "in", "uns", "ein", "ru\u00b7he\u00b7los", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach eines Himmels Glanz, der fremd verlockend blinkt.", "tokens": ["Nach", "ei\u00b7nes", "Him\u00b7mels", "Glanz", ",", "der", "fremd", "ver\u00b7lo\u00b7ckend", "blinkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die reichsten St\u00e4dte und die prangendsten Gefilde", "tokens": ["Die", "reichs\u00b7ten", "St\u00e4d\u00b7te", "und", "die", "pran\u00b7gends\u00b7ten", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Enthielten nimmermehr den mystisch-seltnen Reiz,", "tokens": ["Ent\u00b7hiel\u00b7ten", "nim\u00b7mer\u00b7mehr", "den", "mys\u00b7tischselt\u00b7nen", "Reiz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie ihn aus Wolken formt des Zufalls fremd Gebilde.", "tokens": ["Wie", "ihn", "aus", "Wol\u00b7ken", "formt", "des", "Zu\u00b7falls", "fremd", "Ge\u00b7bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Stets hauchte Sehnsucht uns die Schauer bangen Leids.", "tokens": ["Stets", "hauch\u00b7te", "Sehn\u00b7sucht", "uns", "die", "Schau\u00b7er", "ban\u00b7gen", "Leids", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "\u2013 In dem Genusse wei\u00df die Sehnsucht Kraft zu finden.", "tokens": ["\u2013", "In", "dem", "Ge\u00b7nus\u00b7se", "wei\u00df", "die", "Sehn\u00b7sucht", "Kraft", "zu", "fin\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Sehnsucht, alter Baum, der von der Lust sich n\u00e4hrt,", "tokens": ["O", "Sehn\u00b7sucht", ",", "al\u00b7ter", "Baum", ",", "der", "von", "der", "Lust", "sich", "n\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indessen du ergreist und h\u00e4rtest deine Rinden,", "tokens": ["In\u00b7des\u00b7sen", "du", "er\u00b7greist", "und", "h\u00e4r\u00b7test", "dei\u00b7ne", "Rin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sieh, wie dein schlank Gezweig zur Sonn' emporbegehrt.", "tokens": ["Sieh", ",", "wie", "dein", "schlank", "Ge\u00b7zweig", "zur", "Sonn'", "em\u00b7por\u00b7be\u00b7gehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPOSAT", "ADJD", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Strebst ewig, gro\u00dfer Baum, du m\u00e4chtiger nach oben", "tokens": ["Strebst", "e\u00b7wig", ",", "gro\u00b7\u00dfer", "Baum", ",", "du", "m\u00e4ch\u00b7ti\u00b7ger", "nach", "o\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJA", "NN", "$,", "PPER", "ADJA", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als die Zypresse? \u2013 Doch wir haben sorglich, wi\u00dft,", "tokens": ["Als", "die", "Zyp\u00b7res\u00b7se", "?", "\u2013", "Doch", "wir", "ha\u00b7ben", "sorg\u00b7lich", ",", "wi\u00dft", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$.", "$(", "KON", "PPER", "VAFIN", "ADJD", "$,", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "F\u00fcr eures Sammelns Gier euch Skizzen aufgehoben,", "tokens": ["F\u00fcr", "eu\u00b7res", "Sam\u00b7melns", "Gier", "euch", "Skiz\u00b7zen", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Br\u00fcder, die ihr preist, was aus der Ferne ist.", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "die", "ihr", "preist", ",", "was", "aus", "der", "Fer\u00b7ne", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wir gr\u00fc\u00dften G\u00f6tzen, die mit Riesenr\u00fcsseln dr\u00e4uten,", "tokens": ["Wir", "gr\u00fc\u00df\u00b7ten", "G\u00f6t\u00b7zen", ",", "die", "mit", "Rie\u00b7sen\u00b7r\u00fcs\u00b7seln", "dr\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Throne, die gebaut aus lichtem Edelstein;", "tokens": ["Und", "Thro\u00b7ne", ",", "die", "ge\u00b7baut", "aus", "lich\u00b7tem", "E\u00b7del\u00b7stein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Prunkpal\u00e4ste Glanz, an dem wir uns erfreuten,", "tokens": ["Der", "Prunk\u00b7pa\u00b7l\u00e4s\u00b7te", "Glanz", ",", "an", "dem", "wir", "uns", "er\u00b7freu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "M\u00f6cht euren Handelsherrn Traum und Verderben sein.", "tokens": ["M\u00f6cht", "eu\u00b7ren", "Han\u00b7dels\u00b7herrn", "Traum", "und", "Ver\u00b7der\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Gewande, die das Aug entz\u00fccken und berauschen,", "tokens": ["Ge\u00b7wan\u00b7de", ",", "die", "das", "Aug", "ent\u00b7z\u00fc\u00b7cken", "und", "be\u00b7rau\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fraun, die sich f\u00e4rben Z\u00e4hn und N\u00e4gel, schauten wir", "tokens": ["Fraun", ",", "die", "sich", "f\u00e4r\u00b7ben", "Z\u00e4hn", "und", "N\u00e4\u00b7gel", ",", "schau\u00b7ten", "wir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PRELS", "PRF", "VVFIN", "NN", "KON", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weise Zauberer, auf welche Schlangen lauschen.", "tokens": ["Und", "wei\u00b7se", "Zau\u00b7be\u00b7rer", ",", "auf", "wel\u00b7che", "Schlan\u00b7gen", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "APPR", "PWAT", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}