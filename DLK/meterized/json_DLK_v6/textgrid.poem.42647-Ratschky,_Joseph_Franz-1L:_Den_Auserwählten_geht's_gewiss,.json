{"textgrid.poem.42647": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Auserw\u00e4hlten geht's gewiss,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Auserw\u00e4hlten geht's gewiss,", "tokens": ["Den", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "geht's", "ge\u00b7wiss", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Obwohl in diesem Punkt die Bibel", "tokens": ["Ob\u00b7wohl", "in", "die\u00b7sem", "Punkt", "die", "Bi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ziemlich uns im Dunkel liess,", "tokens": ["So", "ziem\u00b7lich", "uns", "im", "Dun\u00b7kel", "liess", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrem Paradies nicht \u00fcbel,", "tokens": ["In", "ih\u00b7rem", "Pa\u00b7ra\u00b7dies", "nicht", "\u00fc\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wahrlich nicht so wohl als mir", "tokens": ["Doch", "wahr\u00b7lich", "nicht", "so", "wohl", "als", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "ADV", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In diesem herrlichen Revier.", "tokens": ["In", "die\u00b7sem", "herr\u00b7li\u00b7chen", "Re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Befreyt von trocknen Amtsgesch\u00e4ften,", "tokens": ["Be\u00b7freyt", "von", "trock\u00b7nen", "Amts\u00b7ge\u00b7sch\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Muth, Gef\u00fchl und Geist entkr\u00e4ften,", "tokens": ["Die", "Muth", ",", "Ge\u00b7f\u00fchl", "und", "Geist", "ent\u00b7kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Lieg' ich hier ruhig hingestreckt", "tokens": ["Lieg'", "ich", "hier", "ru\u00b7hig", "hin\u00b7ge\u00b7streckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Im Schatten einer dunkeln Fichte,", "tokens": ["Im", "Schat\u00b7ten", "ei\u00b7ner", "dun\u00b7keln", "Fich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die mich mit ihren Zweigen deckt,", "tokens": ["Die", "mich", "mit", "ih\u00b7ren", "Zwei\u00b7gen", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und denk' an dich, o Freund! und dichte.", "tokens": ["Und", "denk'", "an", "dich", ",", "o", "Freund", "!", "und", "dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "FM", "NN", "$.", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O welche Luft! rings um mich hin", "tokens": ["O", "wel\u00b7che", "Luft", "!", "rings", "um", "mich", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PWAT", "NN", "$.", "ADV", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im finstern Thal, auf Felsensteinen", "tokens": ["Im", "fins\u00b7tern", "Thal", ",", "auf", "Fel\u00b7sen\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Prangt die Natur im frischen Gr\u00fcn", "tokens": ["Prangt", "die", "Na\u00b7tur", "im", "fri\u00b7schen", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von unermessnen Tannenhainen,", "tokens": ["Von", "un\u00b7er\u00b7mess\u00b7nen", "Tan\u00b7nen\u00b7hai\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein schmaler Bach, der \u00fcber Sand", "tokens": ["Ein", "schma\u00b7ler", "Bach", ",", "der", "\u00fc\u00b7ber", "Sand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Kiesel glitscht, und sanft den Rand", "tokens": ["Und", "Kie\u00b7sel", "glitscht", ",", "und", "sanft", "den", "Rand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$,", "KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des bunten Ufers k\u00fcsst, verschwistert", "tokens": ["Des", "bun\u00b7ten", "U\u00b7fers", "k\u00fcsst", ",", "ver\u00b7schwis\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sein s\u00fcsses Pl\u00e4tschern mit dem Laut", "tokens": ["Sein", "s\u00fcs\u00b7ses", "Pl\u00e4t\u00b7schern", "mit", "dem", "Laut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Des Morgenwindes, der vertraut", "tokens": ["Des", "Mor\u00b7gen\u00b7win\u00b7des", ",", "der", "ver\u00b7traut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Bl\u00e4tter meines Buchs durchfl\u00fcstert.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "mei\u00b7nes", "Buchs", "durch\u00b7fl\u00fcs\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von Moos und Dorngestr\u00e4uch verh\u00fcllt,", "tokens": ["Von", "Moos", "und", "Dorn\u00b7ge\u00b7str\u00e4uch", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Steht dort auf jener Felsenspitze,", "tokens": ["Steht", "dort", "auf", "je\u00b7ner", "Fel\u00b7sen\u00b7spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mit scheuen Eulen angef\u00fcllt,", "tokens": ["Mit", "scheu\u00b7en", "Eu\u00b7len", "an\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Rest von einem Rittersitze,", "tokens": ["Der", "Rest", "von", "ei\u00b7nem", "Rit\u00b7ter\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Den einst zu unsrer Ahnen Zeit,", "tokens": ["Den", "einst", "zu", "uns\u00b7rer", "Ah\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wie mich ein Landmann hier belehrte.", "tokens": ["Wie", "mich", "ein", "Land\u00b7mann", "hier", "be\u00b7lehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Muselm\u00e4nner Grausamkeit", "tokens": ["Der", "Mu\u00b7sel\u00b7m\u00e4n\u00b7ner", "Grau\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mit r\u00e4uberischer Hand verheerte.", "tokens": ["Mit", "r\u00e4u\u00b7be\u00b7ri\u00b7scher", "Hand", "ver\u00b7heer\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Diess Schloss giebt meiner Neubegier", "tokens": ["Diess", "Schloss", "giebt", "mei\u00b7ner", "Neu\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Oft Anlass zu gelehrten Fragen:", "tokens": ["Oft", "An\u00b7lass", "zu", "ge\u00b7lehr\u00b7ten", "Fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Wer f\u00fchrt' es auf? wer hauste hier?", "tokens": ["Wer", "f\u00fchrt'", "es", "auf", "?", "wer", "haus\u00b7te", "hier", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PWS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Doch niemand weiss mir das zu sagen.", "tokens": ["Doch", "nie\u00b7mand", "weiss", "mir", "das", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch dunkles Dickicht klettr' ich dann", "tokens": ["Durch", "dunk\u00b7les", "Di\u00b7ckicht", "klett\u00b7r'", "ich", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit meinem Stabe frisch und munter", "tokens": ["Mit", "mei\u00b7nem", "Sta\u00b7be", "frisch", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den h\u00f6chsten steilsten Berg hinan:", "tokens": ["Den", "h\u00f6chs\u00b7ten", "steils\u00b7ten", "Berg", "hi\u00b7nan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da seh' ich froh in's Thal hinunter,", "tokens": ["Da", "seh'", "ich", "froh", "in's", "Thal", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und staune mein St\u00fcck Arbeit an,", "tokens": ["Und", "stau\u00b7ne", "mein", "St\u00fcck", "Ar\u00b7beit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als h\u00e4tt' ich wer weiss was gethan.", "tokens": ["Als", "h\u00e4tt'", "ich", "wer", "weiss", "was", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PWS", "VVFIN", "PIS", "VVPP", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "So fliesset Tag f\u00fcr Tag, mein Lieber!", "tokens": ["So", "flies\u00b7set", "Tag", "f\u00fcr", "Tag", ",", "mein", "Lie\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In's Meer der grauen Zeit hin\u00fcber,", "tokens": ["In's", "Meer", "der", "grau\u00b7en", "Zeit", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und t\u00e4glich w\u00e4chst in mir der Hang", "tokens": ["Und", "t\u00e4g\u00b7lich", "w\u00e4chst", "in", "mir", "der", "Hang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dichterischem M\u00fcssiggang.", "tokens": ["Zu", "dich\u00b7te\u00b7ri\u00b7schem", "M\u00fcs\u00b7sig\u00b7gang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Oft, wenn mit wonnetrunknen Blicken", "tokens": ["Oft", ",", "wenn", "mit", "won\u00b7ne\u00b7trunk\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Aug' im fr\u00f6hlichsten Entz\u00fccken", "tokens": ["Mein", "Aug'", "im", "fr\u00f6h\u00b7lichs\u00b7ten", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die stille Gegend \u00fcbersieht,", "tokens": ["Die", "stil\u00b7le", "Ge\u00b7gend", "\u00fc\u00b7ber\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00fcnsch' ich im Ernst als Eremit", "tokens": ["W\u00fcn\u00b7sch'", "ich", "im", "Ernst", "als", "E\u00b7re\u00b7mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KOUS", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Mir eine Zelle hier zu bauen:", "tokens": ["Mir", "ei\u00b7ne", "Zel\u00b7le", "hier", "zu", "bau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Doch eitle W\u00fcnsche! Das Geschick", "tokens": ["Doch", "eit\u00b7le", "W\u00fcn\u00b7sche", "!", "Das", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Fasst bald mich an mit ehrnen Klauen,", "tokens": ["Fasst", "bald", "mich", "an", "mit", "ehr\u00b7nen", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und schleppt mich nach der Stadt zur\u00fcck.", "tokens": ["Und", "schleppt", "mich", "nach", "der", "Stadt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann lebet wohl, ihr D\u00e4mmerungen", "tokens": ["Dann", "le\u00b7bet", "wohl", ",", "ihr", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des k\u00fchlen Walds! Um Lohn gedungen,", "tokens": ["Des", "k\u00fch\u00b7len", "Walds", "!", "Um", "Lohn", "ge\u00b7dun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KOUI", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kriech' ich an meine Ruderbank,", "tokens": ["Kriech'", "ich", "an", "mei\u00b7ne", "Ru\u00b7der\u00b7bank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOS