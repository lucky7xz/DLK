{"dta.poem.7019": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein ausb\u00fcndig-sch\u00f6ner, in der  \n Bl\u00fchte stehender, Castanien-Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um von dem Baume nun noch mehrers zu erzehlen,", "tokens": ["Um", "von", "dem", "Bau\u00b7me", "nun", "noch", "meh\u00b7rers", "zu", "er\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ADV", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf dem man solchen Schmuck in solcher Meng erblicket;", "tokens": ["Auf", "dem", "man", "sol\u00b7chen", "Schmuck", "in", "sol\u00b7cher", "Meng", "er\u00b7bli\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "PIAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ist kein Mensch, der seine Pracht,", "tokens": ["So", "ist", "kein", "Mensch", ",", "der", "sei\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Anmuth seiner gr\u00fcnen Nacht,", "tokens": ["Die", "An\u00b7muth", "sei\u00b7ner", "gr\u00fc\u00b7nen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zumahl in schw\u00fcler Zeit, in hellen Sonnen-Strahlen", "tokens": ["Zu\u00b7mahl", "in", "schw\u00fc\u00b7ler", "Zeit", ",", "in", "hel\u00b7len", "Son\u00b7nen\u00b7Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nat\u00fcrlich bilden kan, beschreiben oder mahlen.", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "bil\u00b7den", "kan", ",", "be\u00b7schrei\u00b7ben", "o\u00b7der", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$,", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn man die Augen mit Bedacht", "tokens": ["Wenn", "man", "die", "Au\u00b7gen", "mit", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf seine \u00e4usre Sch\u00f6nheit lencket,", "tokens": ["Auf", "sei\u00b7ne", "\u00e4us\u00b7re", "Sch\u00f6n\u00b7heit", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auch in die gr\u00fcne Tieffe sencket;", "tokens": ["Auch", "in", "die", "gr\u00fc\u00b7ne", "Tief\u00b7fe", "sen\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vergn\u00fcget sich das Hertz, und r\u00fchmt des Sch\u00f6pfers Macht.", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "sich", "das", "Hertz", ",", "und", "r\u00fchmt", "des", "Sch\u00f6p\u00b7fers", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Indem, dies holde Gr\u00fcn dem, der es wol bemerckt,", "tokens": ["In\u00b7dem", ",", "dies", "hol\u00b7de", "Gr\u00fcn", "dem", ",", "der", "es", "wol", "be\u00b7merckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PDS", "ADJA", "NN", "ART", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nicht nur sein leibliches sein Seelen-Aug\u2019 auch st\u00e4rckt.", "tokens": ["Nicht", "nur", "sein", "leib\u00b7li\u00b7ches", "sein", "See\u00b7len\u00b7Aug'", "auch", "st\u00e4rckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "ADJA", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}