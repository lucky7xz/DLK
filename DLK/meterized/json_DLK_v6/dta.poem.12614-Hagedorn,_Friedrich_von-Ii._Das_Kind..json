{"dta.poem.12614": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n  Das Kind.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als mich die Mama", "tokens": ["Als", "mich", "die", "Ma\u00b7ma"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "H\u00e4nnschen k\u00fcssen sah,", "tokens": ["H\u00e4nn\u00b7schen", "k\u00fcs\u00b7sen", "sah", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Strafte sie mich ab.", "tokens": ["Straf\u00b7te", "sie", "mich", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Doch sie lachte ja,", "tokens": ["Doch", "sie", "lach\u00b7te", "ja", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Als ihr der Papa", "tokens": ["Als", "ihr", "der", "Pa\u00b7pa"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Heut ein M\u00e4ulchen gab.", "tokens": ["Heut", "ein", "M\u00e4ul\u00b7chen", "gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Warum lehrt sie mich:", "tokens": ["Wa\u00b7rum", "lehrt", "sie", "mich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "M\u00e4dchen! machs wie ich?", "tokens": ["M\u00e4d\u00b7chen", "!", "machs", "wie", "ich", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "KOKOM", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sieh was andre sind.", "tokens": ["Sieh", "was", "and\u00b7re", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "PIS", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nun ich solches thu,", "tokens": ["Nun", "ich", "sol\u00b7ches", "thu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schm\u00e4hlt sie noch dazu:", "tokens": ["Schm\u00e4hlt", "sie", "noch", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ach ich armes Kind!", "tokens": ["Ach", "ich", "ar\u00b7mes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Schwestern! sagt mirs sein:", "tokens": ["Schwes\u00b7tern", "!", "sagt", "mirs", "sein", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "NE", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ist mir, weil ich klein,", "tokens": ["Ist", "mir", ",", "weil", "ich", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Noch kein Ku\u00df verg\u00f6nnt?", "tokens": ["Noch", "kein", "Ku\u00df", "ver\u00b7g\u00f6nnt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Seht! ich wachse schon,", "tokens": ["Seht", "!", "ich", "wach\u00b7se", "schon", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Seit des Nachbars Sohn", "tokens": ["Seit", "des", "Nach\u00b7bars", "Sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Mich sein Sch\u00e4tzchen nennt.", "tokens": ["Mich", "sein", "Sch\u00e4tz\u00b7chen", "nennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}