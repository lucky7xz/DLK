{"textgrid.poem.55147": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Nachtgedanken", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch bedaur' ich, ungl\u00fccksel'ge Sterne,", "tokens": ["Euch", "be\u00b7daur'", "ich", ",", "un\u00b7gl\u00fcck\u00b7sel'\u00b7ge", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die ihr sch\u00f6n seid und so herrlich scheinet,", "tokens": ["Die", "ihr", "sch\u00f6n", "seid", "und", "so", "herr\u00b7lich", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VAFIN", "KON", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dem bedr\u00e4ngten Schiffer gerne leuchtet,", "tokens": ["Dem", "be\u00b7dr\u00e4ng\u00b7ten", "Schif\u00b7fer", "ger\u00b7ne", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unbelohnt von G\u00f6ttern und von Menschen:", "tokens": ["Un\u00b7be\u00b7lohnt", "von", "G\u00f6t\u00b7tern", "und", "von", "Men\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Denn ihr liebt nicht, kanntet nie die Liebe!", "tokens": ["Denn", "ihr", "liebt", "nicht", ",", "kann\u00b7tet", "nie", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Unaufhaltsam f\u00fchren ew'ge Stunden", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "f\u00fch\u00b7ren", "ew'\u00b7ge", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eure Reihen durch den weiten Himmel.", "tokens": ["Eu\u00b7re", "Rei\u00b7hen", "durch", "den", "wei\u00b7ten", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Welche Reise habt ihr schon vollendet,", "tokens": ["Wel\u00b7che", "Rei\u00b7se", "habt", "ihr", "schon", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seit ich, weilend in dem Arm der Liebsten,", "tokens": ["Seit", "ich", ",", "wei\u00b7lend", "in", "dem", "Arm", "der", "Liebs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Euer und der Mitternacht vergessen!", "tokens": ["Eu\u00b7er", "und", "der", "Mit\u00b7ter\u00b7nacht", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}