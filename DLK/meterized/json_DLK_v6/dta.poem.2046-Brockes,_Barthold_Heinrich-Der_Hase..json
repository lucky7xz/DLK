{"dta.poem.2046": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Hase.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieses Thier scheint nicht allein uns zum Nutzen nur", "tokens": ["Die\u00b7ses", "Thier", "scheint", "nicht", "al\u00b7lein", "uns", "zum", "Nut\u00b7zen", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PTKNEG", "ADV", "PPER", "APPRART", "NN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "gemacht,", "tokens": ["ge\u00b7macht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Sondern auch zur Lust des Menschen \u00fcberall hervorge-", "tokens": ["Son\u00b7dern", "auch", "zur", "Lust", "des", "Men\u00b7schen", "\u00fc\u00b7be\u00b7rall", "her\u00b7vor\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "bracht.", "tokens": ["bracht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Alle Glieder eines Hasen kommen weislich \u00fcberein", "tokens": ["Al\u00b7le", "Glie\u00b7der", "ei\u00b7nes", "Ha\u00b7sen", "kom\u00b7men", "weis\u00b7lich", "\u00fc\u00b7be\u00b7re\u00b7in"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.6": {"text": "Mit der furchtsamen Natur, die die\u00df Thier vor andern zeiget.", "tokens": ["Mit", "der", "furcht\u00b7sa\u00b7men", "Na\u00b7tur", ",", "die", "die\u00df", "Thier", "vor", "an\u00b7dern", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PDS", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Kopf und Ohren, sonderlich seine lange Hinterbein\u2019,", "tokens": ["Kopf", "und", "Oh\u00b7ren", ",", "son\u00b7der\u00b7lich", "sei\u00b7ne", "lan\u00b7ge", "Hin\u00b7ter\u00b7bein'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Welche man sonst Spr\u00fcnge nennt, die zum Absprung", "tokens": ["Wel\u00b7che", "man", "sonst", "Spr\u00fcn\u00b7ge", "nennt", ",", "die", "zum", "Ab\u00b7sprung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PIS", "ADV", "NN", "VVFIN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "recht gebeuget,", "tokens": ["recht", "ge\u00b7beu\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Dienen ihm zum Schutz im Laufen, und verl\u00e4ngern unsre", "tokens": ["Die\u00b7nen", "ihm", "zum", "Schutz", "im", "Lau\u00b7fen", ",", "und", "ver\u00b7l\u00e4n\u00b7gern", "uns\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPER", "APPRART", "NN", "APPRART", "NN", "$,", "KON", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Lust,", "tokens": ["Lust", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Wenn sein spielend Ohr die Noth, wenn ihn ein Get\u00f6se", "tokens": ["Wenn", "sein", "spie\u00b7lend", "Ohr", "die", "Noth", ",", "wenn", "ihn", "ein", "Ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJD", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "schreckt,", "tokens": ["schreckt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Um bey Zeiten sich zu retten, ihm von weitem schon entdeckt.", "tokens": ["Um", "bey", "Zei\u00b7ten", "sich", "zu", "ret\u00b7ten", ",", "ihm", "von", "wei\u00b7tem", "schon", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$,", "PPER", "APPR", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Seine List ist sonderbar, da\u00df von Sp\u00fcr- und andern", "tokens": ["Sei\u00b7ne", "List", "ist", "son\u00b7der\u00b7bar", ",", "da\u00df", "von", "Sp\u00fcr", "und", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KOUS", "APPR", "TRUNC", "KON", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Hunden,", "tokens": ["Hun\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Deren Nasen ihm fatal, seine Spur nicht sey gefunden;", "tokens": ["De\u00b7ren", "Na\u00b7sen", "ihm", "fa\u00b7tal", ",", "sei\u00b7ne", "Spur", "nicht", "sey", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Macht er hin und wieder Spr\u00fcnge, eh\u2019 er sich ins Lager", "tokens": ["Macht", "er", "hin", "und", "wie\u00b7der", "Spr\u00fcn\u00b7ge", ",", "eh'", "er", "sich", "ins", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "KON", "ADV", "NN", "$,", "KOUS", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "legt,", "tokens": ["legt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Wohin er, ohn diese Vorsicht, nie sich zu begeben pflegt.", "tokens": ["Wo\u00b7hin", "er", ",", "ohn", "die\u00b7se", "Vor\u00b7sicht", ",", "nie", "sich", "zu", "be\u00b7ge\u00b7ben", "pflegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "KOUI", "PDAT", "NN", "$,", "ADV", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.21": {"text": "Es ist nicht sein Fleisch allein uns zur Nahrung guter Art,", "tokens": ["Es", "ist", "nicht", "sein", "Fleisch", "al\u00b7lein", "uns", "zur", "Nah\u00b7rung", "gu\u00b7ter", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "PPER", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-++--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "Nied- und lieblich von Geschmack, angenehm, und m\u00fcrb", "tokens": ["Nie\u00b7d", "und", "lieb\u00b7lich", "von", "Ge\u00b7schmack", ",", "an\u00b7ge\u00b7nehm", ",", "und", "m\u00fcrb"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["TRUNC", "KON", "ADJD", "APPR", "NN", "$,", "ADJD", "$,", "KON", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "und zart;", "tokens": ["und", "zart", ";"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "Auch sein Balg, das Herz, die Lunge, seine Nieren, seine", "tokens": ["Auch", "sein", "Balg", ",", "das", "Herz", ",", "die", "Lun\u00b7ge", ",", "sei\u00b7ne", "Nie\u00b7ren", ",", "sei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.25": {"text": "Geilen", "tokens": ["Gei\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Sollen in den Arzeneyen manchem Kranken H\u00fclf\u2019 ertheilen.", "tokens": ["Sol\u00b7len", "in", "den", "Ar\u00b7ze\u00b7ne\u00b7yen", "man\u00b7chem", "Kran\u00b7ken", "H\u00fcl\u00b7f'", "er\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PIAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+--+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "Haare, Blut und Balg sind n\u00fctzlich. Stralt denn nicht", "tokens": ["Haa\u00b7re", ",", "Blut", "und", "Balg", "sind", "n\u00fctz\u00b7lich", ".", "Stralt", "denn", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN", "ADJD", "$.", "NN", "ADV", "PTKNEG"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.28": {"text": "aus diesem Thier,", "tokens": ["aus", "die\u00b7sem", "Thier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Nebst der Weisheit und der Allmacht, auch des Sch\u00f6pfers", "tokens": ["Nebst", "der", "Weis\u00b7heit", "und", "der", "All\u00b7macht", ",", "auch", "des", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-++--+-", "measure": "trochaic.hexa.relaxed"}, "line.30": {"text": "Lieb' herf\u00fcr?", "tokens": ["Lieb'", "her\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}