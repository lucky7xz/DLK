{"dta.poem.9406": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n Anap\u00e4stisch Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr W\u00e4lder und Felder/ jhr Th\u00e4ler und Steine/", "tokens": ["Ihr", "W\u00e4l\u00b7der", "und", "Fel\u00b7der", "/", "jhr", "Th\u00e4\u00b7ler", "und", "Stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$(", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ach sehet die Thr\u00e4nen/ den traurigen Sinn/", "tokens": ["Ach", "se\u00b7het", "die", "Thr\u00e4\u00b7nen", "/", "den", "trau\u00b7ri\u00b7gen", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie ein samich sitze/ wie traurig ich bin/", "tokens": ["Wie", "ein", "sa\u00b7mich", "sit\u00b7ze", "/", "wie", "trau\u00b7rig", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "VVFIN", "$(", "PWAV", "ADJD", "PPER", "VAFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "La\u00dft eure begr\u00fcnte Lust trauren jhr Haine/", "tokens": ["La\u00dft", "eu\u00b7re", "be\u00b7gr\u00fcn\u00b7te", "Lust", "trau\u00b7ren", "jhr", "Hai\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Weil keine mich tr\u00f6stet/ mu\u00df sitzen alleine/", "tokens": ["Weil", "kei\u00b7ne", "mich", "tr\u00f6s\u00b7tet", "/", "mu\u00df", "sit\u00b7zen", "al\u00b7lei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPER", "VVFIN", "$(", "VMFIN", "VVINF", "ADV", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mu\u00df klagen/ Leid-tragen und zagen forthin/", "tokens": ["Mu\u00df", "kla\u00b7gen", "/", "Lei\u00b7dtra\u00b7gen", "und", "za\u00b7gen", "for\u00b7thin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$(", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Betr\u00fcbn\u00fcs ist jtzo mein bester gewinn;", "tokens": ["Be\u00b7tr\u00fcb\u00b7n\u00fcs", "ist", "jt\u00b7zo", "mein", "bes\u00b7ter", "ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich \u00e4chtze vor Langmuth bin \u00e4hnlich dem scheine/", "tokens": ["Ich", "\u00e4cht\u00b7ze", "vor", "Lang\u00b7muth", "bin", "\u00e4hn\u00b7lich", "dem", "schei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VAFIN", "ADJD", "ART", "ADJA", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "und niemand erbarmet sich memer jtzund/", "tokens": ["und", "nie\u00b7mand", "er\u00b7bar\u00b7met", "sich", "me\u00b7mer", "jt\u00b7zund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "ADV", "ADV", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Wangen seyn tunckel/ der r\u00f6\u00dflichte Mund", "tokens": ["Die", "Wan\u00b7gen", "seyn", "tun\u00b7ckel", "/", "der", "r\u00f6\u00df\u00b7lich\u00b7te", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Verblasset vor Thr\u00e4nen/ der Odem verschwindet/", "tokens": ["Ver\u00b7blas\u00b7set", "vor", "Thr\u00e4\u00b7nen", "/", "der", "O\u00b7dem", "ver\u00b7schwin\u00b7det", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Ich ruffe mit kr\u00e4fften und seuftze mit macht/", "tokens": ["Ich", "ruf\u00b7fe", "mit", "kr\u00e4ff\u00b7ten", "und", "seuft\u00b7ze", "mit", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "KON", "VVFIN", "APPR", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wenn Hesperus schildert und Venus erwacht/", "tokens": ["Wenn", "Hes\u00b7pe\u00b7rus", "schil\u00b7dert", "und", "Ve\u00b7nus", "er\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "KON", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da niemand das klagen und seustzen empfindet.", "tokens": ["Da", "nie\u00b7mand", "das", "kla\u00b7gen", "und", "seust\u00b7zen", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "KON", "VVINF", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}