{"dta.poem.18872": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ist der Wein wieder erlaubt", "tokens": ["Nun", "ist", "der", "Wein", "wie\u00b7der", "er\u00b7laubt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der erst versagt war,", "tokens": ["Der", "erst", "ver\u00b7sagt", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mit vieler Arbeit", "tokens": ["Mit", "vie\u00b7ler", "Ar\u00b7beit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gewann ich mir die Erlaubniss.", "tokens": ["Ge\u00b7wann", "ich", "mir", "die", "Er\u00b7laub\u00b7niss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}