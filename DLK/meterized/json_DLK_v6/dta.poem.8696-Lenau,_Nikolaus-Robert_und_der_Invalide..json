{"dta.poem.8696": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Robert und der Invalide.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Dich tr\u00f6sten wollen mag ein bittrer Sp\u00f6tter!", "tokens": ["Dich", "tr\u00f6s\u00b7ten", "wol\u00b7len", "mag", "ein", "bit\u00b7trer", "Sp\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was einmal tief und wahrhaft dich gekr\u00e4nkt,", "tokens": ["Was", "ein\u00b7mal", "tief", "und", "wahr\u00b7haft", "dich", "ge\u00b7kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das bleibt auf ewig dir ins Mark gesenkt:", "tokens": ["Das", "bleibt", "auf", "e\u00b7wig", "dir", "ins", "Mark", "ge\u00b7senkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJD", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hier steht das Ungl\u00fcck h\u00f6her als die G\u00f6tter!", "tokens": ["Hier", "steht", "das", "Un\u00b7gl\u00fcck", "h\u00f6\u00b7her", "als", "die", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Himmel mag vor deinen Gram sich lagern,", "tokens": ["Der", "Him\u00b7mel", "mag", "vor", "dei\u00b7nen", "Gram", "sich", "la\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "All' seine G\u00f6tterkr\u00e4fte la\u00df ergl\u00fch'n,", "tokens": ["All'", "sei\u00b7ne", "G\u00f6t\u00b7ter\u00b7kr\u00e4f\u00b7te", "la\u00df", "er\u00b7gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVIMP", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df er die Seele dir von ihren Nagern", "tokens": ["Da\u00df", "er", "die", "See\u00b7le", "dir", "von", "ih\u00b7ren", "Na\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Rein schaffe und sie wieder mache bl\u00fch'n:", "tokens": ["Rein", "schaf\u00b7fe", "und", "sie", "wie\u00b7der", "ma\u00b7che", "bl\u00fch'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wird er den Seelenwurm hinausbeschw\u00f6ren,", "tokens": ["Wird", "er", "den", "See\u00b7len\u00b7wurm", "hin\u00b7aus\u00b7be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Will er nicht Seel' und Wurm zugleich zerst\u00f6ren?! \u2014", "tokens": ["Will", "er", "nicht", "Seel'", "und", "Wurm", "zu\u00b7gleich", "zer\u00b7st\u00f6\u00b7ren", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df einen treuen Freund an mir du hast,", "tokens": ["Da\u00df", "ei\u00b7nen", "treu\u00b7en", "Freund", "an", "mir", "du", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Bis sie mir einst im Dorfe dr\u00fcben l\u00e4uten,", "tokens": ["Bis", "sie", "mir", "einst", "im", "Dor\u00b7fe", "dr\u00fc\u00b7ben", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wenn sie mich tragen zur ersehnten Rast,", "tokens": ["Wenn", "sie", "mich", "tra\u00b7gen", "zur", "er\u00b7sehn\u00b7ten", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das ist wohl wahr, doch hier kann's nichts bedeuten. \u2014", "tokens": ["Das", "ist", "wohl", "wahr", ",", "doch", "hier", "kann's", "nichts", "be\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADV", "VMFIN", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die Sonn' ist unter; \u2014 wie die Nebel flattern,", "tokens": ["Die", "Sonn'", "ist", "un\u00b7ter", ";", "wie", "die", "Ne\u00b7bel", "flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "$.", "$(", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Vom Herbstwind aufgejagt, aus dunklem Moor! \u2014", "tokens": ["Vom", "Herbst\u00b7wind", "auf\u00b7ge\u00b7jagt", ",", "aus", "dunk\u00b7lem", "Moor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "So war der Abend, als mir Laura schwor! \u2014", "tokens": ["So", "war", "der", "A\u00b7bend", ",", "als", "mir", "Lau\u00b7ra", "schwor", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "H\u00f6rst du die Wildgans in den L\u00fcften schnattern?", "tokens": ["H\u00f6rst", "du", "die", "Wild\u00b7gans", "in", "den", "L\u00fcf\u00b7ten", "schnat\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Das k\u00fcndet Frost, mein Freund, und tr\u00fcbe Zeit! \u2014", "tokens": ["Das", "k\u00fcn\u00b7det", "Frost", ",", "mein", "Freund", ",", "und", "tr\u00fc\u00b7be", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Schon wieder gaukelt da die b\u00f6se Sippe", "tokens": ["Schon", "wie\u00b7der", "gau\u00b7kelt", "da", "die", "b\u00f6\u00b7se", "Sip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Von Nachtgestalten der Vergangenheit;", "tokens": ["Von", "Nacht\u00b7ge\u00b7stal\u00b7ten", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Nun mag ich flieh'n durch Gr\u00e4ser und Gestr\u00fcppe,", "tokens": ["Nun", "mag", "ich", "flieh'n", "durch", "Gr\u00e4\u00b7ser", "und", "Ge\u00b7str\u00fcp\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Sie folgt mir stets, sie spottet stets mir nach:", "tokens": ["Sie", "folgt", "mir", "stets", ",", "sie", "spot\u00b7tet", "stets", "mir", "nach", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "\u201edu Thor, mit deinem fabelhaften Sehnen!", "tokens": ["\u201e", "du", "Thor", ",", "mit", "dei\u00b7nem", "fa\u00b7bel\u00b7haf\u00b7ten", "Seh\u00b7nen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "\u201ehast du's noch nicht ers\u00e4uft in deinen Thr\u00e4nen?\u201c", "tokens": ["\u201e", "hast", "du's", "noch", "nicht", "er\u00b7s\u00e4uft", "in", "dei\u00b7nen", "Thr\u00e4\u00b7nen", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PIS", "ADV", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Und alle meine Wunden werden wach.", "tokens": ["Und", "al\u00b7le", "mei\u00b7ne", "Wun\u00b7den", "wer\u00b7den", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Wie Buben einen Narren durch die Stra\u00dfen", "tokens": ["Wie", "Bu\u00b7ben", "ei\u00b7nen", "Nar\u00b7ren", "durch", "die", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Nicht ungeneckt hingeh'n und tr\u00e4umen lassen,", "tokens": ["Nicht", "un\u00b7ge\u00b7neckt", "hin\u00b7geh'n", "und", "tr\u00e4u\u00b7men", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "KON", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "So folgt es h\u00f6hnend mir durch diese Heide,", "tokens": ["So", "folgt", "es", "h\u00f6h\u00b7nend", "mir", "durch", "die\u00b7se", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und l\u00e4\u00dft nicht rasten mich von meinem Leide.", "tokens": ["Und", "l\u00e4\u00dft", "nicht", "ras\u00b7ten", "mich", "von", "mei\u00b7nem", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}