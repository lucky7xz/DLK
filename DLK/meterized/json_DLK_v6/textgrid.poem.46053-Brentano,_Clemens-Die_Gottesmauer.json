{"textgrid.poem.46053": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Gottesmauer", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Draus vor Schleswig an der Pforte", "tokens": ["Draus", "vor", "Schles\u00b7wig", "an", "der", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohnen armer Leute viel.", "tokens": ["Woh\u00b7nen", "ar\u00b7mer", "Leu\u00b7te", "viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! des Feindes wilder Horde", "tokens": ["Ach", "!", "des", "Fein\u00b7des", "wil\u00b7der", "Hor\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden sie das erste Ziel.", "tokens": ["Wer\u00b7den", "sie", "das", "ers\u00b7te", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Waffenstillstand ist gek\u00fcndet;", "tokens": ["Waf\u00b7fen\u00b7still\u00b7stand", "ist", "ge\u00b7k\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00e4nen ziehen aus zur Nacht;", "tokens": ["D\u00e4\u00b7nen", "zie\u00b7hen", "aus", "zur", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Russen, Schweden sind verb\u00fcndet,", "tokens": ["Rus\u00b7sen", ",", "Schwe\u00b7den", "sind", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Brechen ein mit wilder Macht.", "tokens": ["Bre\u00b7chen", "ein", "mit", "wil\u00b7der", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Draus vor Schleswig, weit vor allen", "tokens": ["Draus", "vor", "Schles\u00b7wig", ",", "weit", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "APPR", "NE", "$,", "ADJD", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt ein H\u00fcttlein ausgesetzt.", "tokens": ["Liegt", "ein", "H\u00fctt\u00b7lein", "aus\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Draus vor Schleswig in der H\u00fctte", "tokens": ["Draus", "vor", "Schles\u00b7wig", "in", "der", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt ein frommes M\u00fctterlein:", "tokens": ["Singt", "ein", "from\u00b7mes", "M\u00fct\u00b7ter\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbherr, in deinen Scho\u00df ich sch\u00fctte", "tokens": ["\u00bb", "herr", ",", "in", "dei\u00b7nen", "Scho\u00df", "ich", "sch\u00fct\u00b7te"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle meine Sorg' und Pein!\u00ab", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Sor\u00b7g'", "und", "Pein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Doch ihr Enkel, ohn' Vertrauen,", "tokens": ["Doch", "ihr", "En\u00b7kel", ",", "ohn'", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwanzigj\u00e4hrig, neuster Zeit,", "tokens": ["Zwan\u00b7zig\u00b7j\u00e4h\u00b7rig", ",", "neus\u00b7ter", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat, den Br\u00e4utigam zu schauen,", "tokens": ["Hat", ",", "den", "Br\u00e4u\u00b7ti\u00b7gam", "zu", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine Lampe nicht bereit.", "tokens": ["Sei\u00b7ne", "Lam\u00b7pe", "nicht", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Draus vor Schleswig in der H\u00fctte", "tokens": ["Draus", "vor", "Schles\u00b7wig", "in", "der", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt das fromme M\u00fctterlein.", "tokens": ["Singt", "das", "from\u00b7me", "M\u00fct\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbeine Mauer um uns baue!\u00ab", "tokens": ["\u00bb", "ei\u00b7ne", "Mau\u00b7er", "um", "uns", "bau\u00b7e", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt das fromme M\u00fctterlein:", "tokens": ["Singt", "das", "from\u00b7me", "M\u00fct\u00b7ter\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbda\u00df dem Feinde vor uns graue,", "tokens": ["\u00bb", "da\u00df", "dem", "Fein\u00b7de", "vor", "uns", "grau\u00b7e", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm in deine Burg uns ein!\u00ab", "tokens": ["Nimm", "in", "dei\u00b7ne", "Burg", "uns", "ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbmutter\u00ab, spricht der Weltgesinnte,", "tokens": ["\u00bb", "mut\u00b7ter", "\u00ab", ",", "spricht", "der", "Welt\u00b7ge\u00b7sinn\u00b7te", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$(", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbeine Mauer uns ums Haus", "tokens": ["\u00bb", "ei\u00b7ne", "Mau\u00b7er", "uns", "ums", "Haus"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kriegt f\u00fcrwahr nicht so geschwinde", "tokens": ["Kriegt", "f\u00fcr\u00b7wahr", "nicht", "so", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Euer lieber Gott heraus!\u00ab", "tokens": ["Eu\u00b7er", "lie\u00b7ber", "Gott", "he\u00b7raus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADV", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbeine Mauer um uns baue!\u00ab", "tokens": ["\u00bb", "ei\u00b7ne", "Mau\u00b7er", "um", "uns", "bau\u00b7e", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt das fromme M\u00fctterlein.", "tokens": ["Singt", "das", "from\u00b7me", "M\u00fct\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbenkel, fest ist mein Vertrauen,", "tokens": ["\u00bb", "en\u00b7kel", ",", "fest", "ist", "mein", "Ver\u00b7trau\u00b7en", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn's dem lieben Gott gef\u00e4llt,", "tokens": ["Wenn's", "dem", "lie\u00b7ben", "Gott", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann Er uns die Mauer bauen,", "tokens": ["Kann", "Er", "uns", "die", "Mau\u00b7er", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was Er will, ist wohl bestellt.\u00ab", "tokens": ["Was", "Er", "will", ",", "ist", "wohl", "be\u00b7stellt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trommeln rumdidum rings prasseln;", "tokens": ["Trom\u00b7meln", "rum\u00b7di\u00b7dum", "rings", "pras\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Trompeten schmettern drein;", "tokens": ["Die", "Trom\u00b7pe\u00b7ten", "schmet\u00b7tern", "drein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Rosse wiehern, Wagen rasseln;", "tokens": ["Ros\u00b7se", "wie\u00b7hern", ",", "Wa\u00b7gen", "ras\u00b7seln", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, nun bricht der Feind herein!", "tokens": ["Ach", ",", "nun", "bricht", "der", "Feind", "her\u00b7ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbeine Mauer um uns baue!\u00ab", "tokens": ["\u00bb", "ei\u00b7ne", "Mau\u00b7er", "um", "uns", "bau\u00b7e", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt das fromme M\u00fctterlein.", "tokens": ["Singt", "das", "from\u00b7me", "M\u00fct\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Rings in alle H\u00fctten brechen", "tokens": ["Rings", "in", "al\u00b7le", "H\u00fct\u00b7ten", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwed' und Russe mit Geschrei,", "tokens": ["Schwed'", "und", "Rus\u00b7se", "mit", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fluchen, l\u00e4rmen, toben, zechen,", "tokens": ["Flu\u00b7chen", ",", "l\u00e4r\u00b7men", ",", "to\u00b7ben", ",", "ze\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch dies Haus gehn sie vorbei.", "tokens": ["Doch", "dies", "Haus", "gehn", "sie", "vor\u00b7bei", "."], "to