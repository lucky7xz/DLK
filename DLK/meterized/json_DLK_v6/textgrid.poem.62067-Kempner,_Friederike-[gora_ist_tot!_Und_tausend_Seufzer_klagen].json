{"textgrid.poem.62067": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[gora ist tot! Und tausend Seufzer klagen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gora ist tot! Und tausend Seufzer klagen,", "tokens": ["Go\u00b7ra", "ist", "tot", "!", "Und", "tau\u00b7send", "Seuf\u00b7zer", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$.", "KON", "CARD", "NN", "VVINF", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und tausend Tr\u00e4nen gr\u00fc\u00dft das Morgenrot,", "tokens": ["Und", "tau\u00b7send", "Tr\u00e4\u00b7nen", "gr\u00fc\u00dft", "das", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein treuer Sinn, ein Helfer in der Not,", "tokens": ["Ein", "treu\u00b7er", "Sinn", ",", "ein", "Hel\u00b7fer", "in", "der", "Not", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein gro\u00dfes Herz hat aufgeh\u00f6rt zu schlagen.", "tokens": ["Ein", "gro\u00b7\u00dfes", "Herz", "hat", "auf\u00b7ge\u00b7h\u00f6rt", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des G\u00f6nners Herz! La\u00dft uns den Jammer tragen,", "tokens": ["Des", "G\u00f6n\u00b7ners", "Herz", "!", "La\u00dft", "uns", "den", "Jam\u00b7mer", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit uns trauere eine ganze Welt,", "tokens": ["Und", "mit", "uns", "trau\u00b7e\u00b7re", "ei\u00b7ne", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Es schlug f\u00fcr sie, ihr Leid hat es geschwellt.", "tokens": ["Es", "schlug", "f\u00fcr", "sie", ",", "ihr", "Leid", "hat", "es", "ge\u00b7schwellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Das wack're Herz hat aufgeh\u00f6rt zu schlagen!", "tokens": ["Das", "wack'\u00b7re", "Herz", "hat", "auf\u00b7ge\u00b7h\u00f6rt", "zu", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Tr\u00e4nen trocknen und die Seufzer schweigen,", "tokens": ["Die", "Tr\u00e4\u00b7nen", "trock\u00b7nen", "und", "die", "Seuf\u00b7zer", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Bl\u00fcmlein an der stillen Gruft verbl\u00fcht,", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "an", "der", "stil\u00b7len", "Gruft", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Poesie singt ihm ein ernstes Lied,", "tokens": ["Die", "Poe\u00b7sie", "singt", "ihm", "ein", "erns\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gr\u00fc\u00dft ihn mit ihren ewig gr\u00fcnen Zweigen.", "tokens": ["Gr\u00fc\u00dft", "ihn", "mit", "ih\u00b7ren", "e\u00b7wig", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie singt: Des Mannes Taten bleiben eigen,", "tokens": ["Sie", "singt", ":", "Des", "Man\u00b7nes", "Ta\u00b7ten", "blei\u00b7ben", "ei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "NN", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewaltig ist der Geist, der von uns schied.", "tokens": ["Ge\u00b7wal\u00b7tig", "ist", "der", "Geist", ",", "der", "von", "uns", "schied", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Schatten stolz an uns vor\u00fcber zieht. \u2013 \u2013", "tokens": ["Sein", "Schat\u00b7ten", "stolz", "an", "uns", "vor\u00b7\u00fc\u00b7ber", "zieht", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "PPER", "ADV", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es lebt ein Gott, la\u00dft uns die H\u00e4upter neigen.", "tokens": ["Es", "lebt", "ein", "Gott", ",", "la\u00dft", "uns", "die", "H\u00e4up\u00b7ter", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVIMP", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie singt: Es knien an seinem \u00f6den Grabe", "tokens": ["Sie", "singt", ":", "Es", "kni\u00b7en", "an", "sei\u00b7nem", "\u00f6\u00b7den", "Gra\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie Lichtgestalten, hold und engelrein,", "tokens": ["Wie", "Licht\u00b7ge\u00b7stal\u00b7ten", ",", "hold", "und", "en\u00b7gel\u00b7rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die treue Gattin und die T\u00f6chter sein,", "tokens": ["Die", "treu\u00b7e", "Gat\u00b7tin", "und", "die", "T\u00f6ch\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und stolz bewahrt sein Schwert der zarte Knabe!", "tokens": ["Und", "stolz", "be\u00b7wahrt", "sein", "Schwert", "der", "zar\u00b7te", "Kna\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}