{"textgrid.poem.39038": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf steinigem Wege", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf steinigem Wege", "tokens": ["Auf", "stei\u00b7ni\u00b7gem", "We\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ist der Wagen gebrochen,", "tokens": ["Ist", "der", "Wa\u00b7gen", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und m\u00fcd' und ermattet", "tokens": ["Und", "m\u00fcd'", "und", "er\u00b7mat\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wandern wir den Fels hinauf", "tokens": ["Wan\u00b7dern", "wir", "den", "Fels", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In der gl\u00fchenden Hitze.", "tokens": ["In", "der", "gl\u00fc\u00b7hen\u00b7den", "Hit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Endlich \u2013 was brennt da droben \u00fcber dem dunkeln Wald? \u2013", "tokens": ["End\u00b7lich", "\u2013", "was", "brennt", "da", "dro\u00b7ben", "\u00fc\u00b7ber", "dem", "dun\u00b7keln", "Wald", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$(", "PWS", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Es ist der Wunderdom", "tokens": ["Es", "ist", "der", "Wun\u00b7der\u00b7dom"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mit Bild und goldner Mosaik geschm\u00fcckt.", "tokens": ["Mit", "Bild", "und", "gold\u00b7ner", "Mo\u00b7saik", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "So leuchtet ein Goldpokal freundlich", "tokens": ["So", "leuch\u00b7tet", "ein", "Gold\u00b7po\u00b7kal", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Dem d\u00fcrstenden Zecher entgegen,", "tokens": ["Dem", "d\u00fcrs\u00b7ten\u00b7den", "Ze\u00b7cher", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Und sein Auge trinkt schon vor der Zunge.", "tokens": ["Und", "sein", "Au\u00b7ge", "trinkt", "schon", "vor", "der", "Zun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nein, nicht dir allein", "tokens": ["Nein", ",", "nicht", "dir", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKNEG", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Frommer Johann von Fiesole,", "tokens": ["From\u00b7mer", "Jo\u00b7hann", "von", "Fie\u00b7so\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.3": {"text": "Oder dir, hoher Signorelli, wird hier gehuldigt.", "tokens": ["O\u00b7der", "dir", ",", "ho\u00b7her", "Sig\u00b7no\u00b7rel\u00b7li", ",", "wird", "hier", "ge\u00b7hul\u00b7digt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "NE", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Ei, wie mundet hier der leichte, liebliche Wein,", "tokens": ["Ei", ",", "wie", "mun\u00b7det", "hier", "der", "leich\u00b7te", ",", "lieb\u00b7li\u00b7che", "Wein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Den ich in Rom fast verschm\u00e4ht,", "tokens": ["Den", "ich", "in", "Rom", "fast", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NE", "ADV", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Mit dem Genu\u00df w\u00e4chst das Verst\u00e4ndni\u00df.", "tokens": ["Mit", "dem", "Ge\u00b7nu\u00df", "w\u00e4chst", "das", "Ver\u00b7st\u00e4nd\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So hat doch jedes Wort und jeder sch\u00f6ne Vers", "tokens": ["So", "hat", "doch", "je\u00b7des", "Wort", "und", "je\u00b7der", "sch\u00f6\u00b7ne", "Vers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Im Gedicht des Lebens", "tokens": ["Im", "Ge\u00b7dicht", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Die rechte Stelle, wo sie verstanden werden.", "tokens": ["Die", "rech\u00b7te", "Stel\u00b7le", ",", "wo", "sie", "ver\u00b7stan\u00b7den", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}