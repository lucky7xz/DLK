{"dta.poem.15938": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "143.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht tr\u00e4ge mu\u00dft du seyn dich zu vertheidigen,", "tokens": ["Nicht", "tr\u00e4\u00b7ge", "mu\u00dft", "du", "seyn", "dich", "zu", "ver\u00b7thei\u00b7di\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPER", "VAINF", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn dich ein T\u00f6lpel will, ein Wicht beleidigen.", "tokens": ["Wenn", "dich", "ein", "T\u00f6l\u00b7pel", "will", ",", "ein", "Wicht", "be\u00b7lei\u00b7di\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch mancher Angriff tritt nicht deiner Ehre nah;", "tokens": ["Doch", "man\u00b7cher", "An\u00b7griff", "tritt", "nicht", "dei\u00b7ner", "Eh\u00b7re", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df ihn nur unbemerkt, so ist er gar nicht da.", "tokens": ["La\u00df", "ihn", "nur", "un\u00b7be\u00b7merkt", ",", "so", "ist", "er", "gar", "nicht", "da", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "A