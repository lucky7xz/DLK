 "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat ihn auf's Pferd geschwungen.", "tokens": ["Hat", "ihn", "auf's", "Pferd", "ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Hinaus mit seiner Schar in's Thal", "tokens": ["Hin\u00b7aus", "mit", "sei\u00b7ner", "Schar", "in's", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jagt's ihn weit in die Ferne,", "tokens": ["Jagt's", "ihn", "weit", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Als f\u00fcrchtet' er den Blitzesstral", "tokens": ["Als", "f\u00fcrch\u00b7tet'", "er", "den", "Blit\u00b7zes\u00b7stral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihrem Augensterne. \u2013", "tokens": ["Aus", "ih\u00b7rem", "Au\u00b7gens\u00b7ter\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Glocken sind noch nicht verhallt,", "tokens": ["Die", "Glo\u00b7cken", "sind", "noch", "nicht", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da wandelt zu den Thoren",