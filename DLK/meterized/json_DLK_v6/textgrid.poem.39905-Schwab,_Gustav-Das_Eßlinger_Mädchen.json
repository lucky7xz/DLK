{"textgrid.poem.39905": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das E\u00dflinger M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit seinen w\u00fctigen Scharen", "tokens": ["Mit", "sei\u00b7nen", "w\u00fc\u00b7ti\u00b7gen", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gezogen kam durch's Neckarthal,", "tokens": ["Ge\u00b7zo\u00b7gen", "kam", "durch's", "Ne\u00b7ck\u00b7ar\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gen E\u00dflingen gefahren.", "tokens": ["Gen", "E\u00df\u00b7lin\u00b7gen", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und auf der Burg da sitzt er schon,", "tokens": ["Und", "auf", "der", "Burg", "da", "sitzt", "er", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man h\u00f6rt ihn lachend sprechen,", "tokens": ["Man", "h\u00f6rt", "ihn", "la\u00b7chend", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie er die Stadt zum Trotz und Hohn", "tokens": ["Wie", "er", "die", "Stadt", "zum", "Trotz", "und", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Am andern Tag will brechen.", "tokens": ["Am", "an\u00b7dern", "Tag", "will", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er tritt zu \u00e4u\u00dferst auf den Wall", "tokens": ["Er", "tritt", "zu", "\u00e4u\u00b7\u00dferst", "auf", "den", "Wall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Pulverdampf sich labend,", "tokens": ["Am", "Pul\u00b7ver\u00b7dampf", "sich", "la\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der wolkig zieht, mit seinem Schwall", "tokens": ["Der", "wol\u00b7kig", "zieht", ",", "mit", "sei\u00b7nem", "Schwall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ganze Stadt begrabend.", "tokens": ["Die", "gan\u00b7ze", "Stadt", "be\u00b7gra\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch wie den Qualm zertheilt der Wind,", "tokens": ["Doch", "wie", "den", "Qualm", "zer\u00b7theilt", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sieht er ein H\u00e4uslein stehen,", "tokens": ["Sieht", "er", "ein", "H\u00e4us\u00b7lein", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Daraus ein sch\u00f6nes B\u00fcrgerkind", "tokens": ["Da\u00b7raus", "ein", "sch\u00f6\u00b7nes", "B\u00fcr\u00b7ger\u00b7kind"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In halbem Nebel gehen.", "tokens": ["In", "hal\u00b7bem", "Ne\u00b7bel", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er ist in welscher Glut entbrannt:", "tokens": ["Er", "ist", "in", "wel\u00b7scher", "Glut", "ent\u00b7brannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PWAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdas M\u00e4gdlein will ich haben!", "tokens": ["\u00bb", "das", "M\u00e4gd\u00b7lein", "will", "ich", "ha\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es giebt in diesem Schwabenland", "tokens": ["Es", "giebt", "in", "die\u00b7sem", "Schwa\u00b7ben\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So viele sch\u00f6ne Gaben;", "tokens": ["So", "vie\u00b7le", "sch\u00f6\u00b7ne", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mir will der Wein in diesem Thal", "tokens": ["Mir", "will", "der", "Wein", "in", "die\u00b7sem", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schier wie der heim'sche munden,", "tokens": ["Schier", "wie", "der", "heim'\u00b7sche", "mun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Darum verlangt mein Herz zumal", "tokens": ["Da\u00b7rum", "ver\u00b7langt", "mein", "Herz", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach heim'schen Sch\u00e4ferstunden!\u00ab", "tokens": ["Nach", "heim'\u00b7schen", "Sch\u00e4\u00b7fer\u00b7stun\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Noch an demselben Abend steht", "tokens": ["Noch", "an", "dem\u00b7sel\u00b7ben", "A\u00b7bend", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Herold vor den Thoren,", "tokens": ["Ein", "He\u00b7rold", "vor", "den", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und an die Stadt sein Ruf ergeht:", "tokens": ["Und", "an", "die", "Stadt", "sein", "Ruf", "er\u00b7geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will sie nicht sein verloren,", "tokens": ["Will", "sie", "nicht", "sein", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VAINF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Soll sie alsbald die sch\u00f6ne Magd", "tokens": ["Soll", "sie", "als\u00b7bald", "die", "sch\u00f6\u00b7ne", "Magd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem argen Dr\u00e4nger senden,", "tokens": ["Dem", "ar\u00b7gen", "Dr\u00e4n\u00b7ger", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sonst raucht die Stadt, sobald es tagt,", "tokens": ["Sonst", "raucht", "die", "Stadt", ",", "so\u00b7bald", "es", "tagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von tausend Feuerbr\u00e4nden.", "tokens": ["Von", "tau\u00b7send", "Feu\u00b7er\u00b7br\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der frommen B\u00fcrger Antwort hat", "tokens": ["Der", "from\u00b7men", "B\u00fcr\u00b7ger", "Ant\u00b7wort", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In gutem Deutsch geklungen:", "tokens": ["In", "gu\u00b7tem", "Deutsch", "ge\u00b7klun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbvon einer freien Reichesstadt", "tokens": ["\u00bb", "von", "ei\u00b7ner", "frei\u00b7en", "Rei\u00b7ches\u00b7stadt"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird solches nicht bedungen;", "tokens": ["Wird", "sol\u00b7ches", "nicht", "be\u00b7dun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir gehen freudig in den Fall,", "tokens": ["Wir", "ge\u00b7hen", "freu\u00b7dig", "in", "den", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn keine Seel' verdorben,", "tokens": ["Wenn", "kei\u00b7ne", "Seel'", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und sterben unsre T\u00f6chter all,", "tokens": ["Und", "ster\u00b7ben", "uns\u00b7re", "T\u00f6ch\u00b7ter", "all", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So sind sie keusch gestorben!\u00ab", "tokens": ["So", "sind", "sie", "keusch", "ge\u00b7stor\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der andre Morgen d\u00e4mmert still,", "tokens": ["Der", "and\u00b7re", "Mor\u00b7gen", "d\u00e4m\u00b7mert", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Glocken alle schallen,", "tokens": ["Die", "Glo\u00b7cken", "al\u00b7le", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Stadt als Eine Seele will", "tokens": ["Die", "Stadt", "als", "Ei\u00b7ne", "See\u00b7le", "will"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gen Himmel betend wallen.", "tokens": ["Gen", "Him\u00b7mel", "be\u00b7tend", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da schm\u00fcckt sich bei der Glocke Klang", "tokens": ["Da", "schm\u00fcckt", "sich", "bei", "der", "Glo\u00b7cke", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Jungfrau auserkoren,", "tokens": ["Die", "Jung\u00b7frau", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zur Kirche wallt des Volkes Drang", "tokens": ["Zur", "Kir\u00b7che", "wallt", "des", "Vol\u00b7kes", "Drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie wandelt nach den Thoren.", "tokens": ["Sie", "wan\u00b7delt", "nach", "den", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auf geht die Pforte kaum ber\u00fchrt,", "tokens": ["Auf", "geht", "die", "Pfor\u00b7te", "kaum", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War's durch die Hand der W\u00e4chter?", "tokens": ["Wa\u00b7r's", "durch", "die", "Hand", "der", "W\u00e4ch\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "War's Gottes Arm, der helfend f\u00fchrt", "tokens": ["Wa\u00b7r's", "Got\u00b7tes", "Arm", ",", "der", "hel\u00b7fend", "f\u00fchrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die reinste seiner T\u00f6chter?", "tokens": ["Die", "reins\u00b7te", "sei\u00b7ner", "T\u00f6ch\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch Freund' und Feinde frei sie geht,", "tokens": ["Durch", "Freund'", "und", "Fein\u00b7de", "frei", "sie", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Magd mit stillem Tritte,", "tokens": ["Die", "Magd", "mit", "stil\u00b7lem", "Trit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hinauf bis wo die Fahne weht", "tokens": ["Hin\u00b7auf", "bis", "wo", "die", "Fah\u00b7ne", "weht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Melac's Lagerh\u00fctte.", "tokens": ["Von", "Me\u00b7lac's", "La\u00b7ger\u00b7h\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Gesprungen war er auf in Wut,", "tokens": ["Ge\u00b7sprun\u00b7gen", "war", "er", "auf", "in", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ihn ein Traum betrogen,", "tokens": ["Weil", "ihn", "ein", "Traum", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der ihm von hei\u00dfer K\u00fcsse Glut", "tokens": ["Der", "ihm", "von", "hei\u00b7\u00dfer", "K\u00fcs\u00b7se", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Betr\u00fcglich vorgelogen;", "tokens": ["Be\u00b7tr\u00fcg\u00b7lich", "vor\u00b7ge\u00b7lo\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er wirft sich in die Waffen stolz:", "tokens": ["Er", "wirft", "sich", "in", "die", "Waf\u00b7fen", "stolz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sollen's alle f\u00fchlen!", "tokens": ["Sie", "sol\u00b7len's", "al\u00b7le", "f\u00fch\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Am d\u00fcrren und am gr\u00fcnen Holz", "tokens": ["Am", "d\u00fcr\u00b7ren", "und", "am", "gr\u00fc\u00b7nen", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Will seine Brunst sich k\u00fchlen.", "tokens": ["Will", "sei\u00b7ne", "Brunst", "sich", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wie er will schreiten aus dem Saal,", "tokens": ["Wie", "er", "will", "schrei\u00b7ten", "aus", "dem", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht er die Th\u00fcre gehen,", "tokens": ["Sieht", "er", "die", "Th\u00fc\u00b7re", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mit dem ersten Sonnenstral", "tokens": ["Und", "mit", "dem", "ers\u00b7ten", "Son\u00b7nen\u00b7stral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Jungfrau vor sich stehen;", "tokens": ["Die", "Jung\u00b7frau", "vor", "sich", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit ihrem H\u00e4ublein spielt das Licht", "tokens": ["Mit", "ih\u00b7rem", "H\u00e4ub\u00b7lein", "spielt", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als einem Heil'genscheine,", "tokens": ["Als", "ei\u00b7nem", "Heil'\u00b7gen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aus ihrem blauen Auge bricht", "tokens": ["Aus", "ih\u00b7rem", "blau\u00b7en", "Au\u00b7ge", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des deutschen Sinnes Reine.", "tokens": ["Des", "deut\u00b7schen", "Sin\u00b7nes", "Rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nicht Angst, nicht andre Regung z\u00fcckt", "tokens": ["Nicht", "Angst", ",", "nicht", "and\u00b7re", "Re\u00b7gung", "z\u00fcckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ihre schlanken Glieder,", "tokens": ["Durch", "ih\u00b7re", "schlan\u00b7ken", "Gli