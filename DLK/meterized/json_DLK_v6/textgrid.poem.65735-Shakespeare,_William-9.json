{"textgrid.poem.65735": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "9", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du dein Leben ehelos vergeuden,", "tokens": ["Willst", "du", "dein", "Le\u00b7ben", "e\u00b7he\u00b7los", "ver\u00b7geu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Damit nicht eine Witwentr\u00e4ne f\u00e4llt?", "tokens": ["Da\u00b7mit", "nicht", "ei\u00b7ne", "Wit\u00b7went\u00b7r\u00e4\u00b7ne", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach! wenn du kinderlos dann m\u00fc\u00dftest scheiden,", "tokens": ["Ach", "!", "wenn", "du", "kin\u00b7der\u00b7los", "dann", "m\u00fc\u00df\u00b7test", "schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "ADJD", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bangt um dich das verla\u00df'ne Weib: die Welt.", "tokens": ["Bangt", "um", "dich", "das", "ver\u00b7la\u00df'\u00b7ne", "Weib", ":", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$.", "ART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Da\u00df sie von dir kein Ebenbild beh\u00e4lt,", "tokens": ["Da\u00df", "sie", "von", "dir", "kein", "E\u00b7ben\u00b7bild", "be\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn jede Erdenwitw' in ihren Kleinen", "tokens": ["Wenn", "je\u00b7de", "Er\u00b7den\u00b7wit\u00b7w'", "in", "ih\u00b7ren", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Des Gatten Gleichnis sich lebendig h\u00e4lt.", "tokens": ["Des", "Gat\u00b7ten", "Gleich\u00b7nis", "sich", "le\u00b7ben\u00b7dig", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Sieh, was ein W\u00fcstling in der Welt verschwendet,", "tokens": ["Sieh", ",", "was", "ein", "W\u00fcst\u00b7ling", "in", "der", "Welt", "ver\u00b7schwen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Vertauscht die St\u00e4tte nur, es bleibt im Brauch;", "tokens": ["Ver\u00b7tauscht", "die", "St\u00e4t\u00b7te", "nur", ",", "es", "bleibt", "im", "Brauch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Doch in der Welt verpra\u00dfte Sch\u00f6nheit endet:", "tokens": ["Doch", "in", "der", "Welt", "ver\u00b7pra\u00df\u00b7te", "Sch\u00f6n\u00b7heit", "en\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und sie zerst\u00f6rt verbrauchend Nichtgebrauch.", "tokens": ["Und", "sie", "zer\u00b7st\u00f6rt", "ver\u00b7brau\u00b7chend", "Nicht\u00b7ge\u00b7brauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Das Herz liebt andre nicht, das solche Schmach", "tokens": ["Das", "Herz", "liebt", "and\u00b7re", "nicht", ",", "das", "sol\u00b7che", "Schmach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Selbstmordend an sich selber \u00fcben mag.", "tokens": ["Selbst\u00b7mor\u00b7dend", "an", "sich", "sel\u00b7ber", "\u00fc\u00b7ben", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADJD", "APPR", "PRF", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}