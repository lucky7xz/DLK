{"textgrid.poem.55318": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Metamorphose der Pflanzen", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich verwirret, Geliebte, die tausendf\u00e4ltige Mischung", "tokens": ["Dich", "ver\u00b7wir\u00b7ret", ",", "Ge\u00b7lieb\u00b7te", ",", "die", "tau\u00b7send\u00b7f\u00e4l\u00b7ti\u00b7ge", "Misc\u00b7hung"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Dieses Blumengew\u00fchls \u00fcber dem Garten umher;", "tokens": ["Die\u00b7ses", "Blu\u00b7men\u00b7ge\u00b7w\u00fchls", "\u00fc\u00b7ber", "dem", "Gar\u00b7ten", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Viele Namen h\u00f6rest du an, und immer verdr\u00e4nget", "tokens": ["Vie\u00b7le", "Na\u00b7men", "h\u00f6\u00b7rest", "du", "an", ",", "und", "im\u00b7mer", "ver\u00b7dr\u00e4n\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADV", "VVFIN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit barbarischem Klang einer den andern im Ohr.", "tokens": ["Mit", "bar\u00b7ba\u00b7ri\u00b7schem", "Klang", "ei\u00b7ner", "den", "an\u00b7dern", "im", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Alle Gestalten sind \u00e4hnlich, und keine gleichet der andern;", "tokens": ["Al\u00b7le", "Ge\u00b7stal\u00b7ten", "sind", "\u00e4hn\u00b7lich", ",", "und", "kei\u00b7ne", "glei\u00b7chet", "der", "an\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PIAT", "VVFIN", "ART", "ADJA", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und so deutet das Chor auf ein geheimes Gesetz,", "tokens": ["Und", "so", "deu\u00b7tet", "das", "Chor", "auf", "ein", "ge\u00b7hei\u00b7mes", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Auf ein heiliges R\u00e4tsel. Oh, k\u00f6nnt ich dir, liebliche Freundin,", "tokens": ["Auf", "ein", "hei\u00b7li\u00b7ges", "R\u00e4t\u00b7sel", ".", "Oh", ",", "k\u00f6nnt", "ich", "dir", ",", "lieb\u00b7li\u00b7che", "Freun\u00b7din", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "ITJ", "$,", "VVFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "\u00dcberliefern sogleich gl\u00fccklich das l\u00f6sende Wort!", "tokens": ["\u00dc\u00b7berl\u00b7ie\u00b7fern", "sog\u00b7leich", "gl\u00fcck\u00b7lich", "das", "l\u00f6\u00b7sen\u00b7de", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Werdend betrachte sie nun, wie nach und nach sich die Pflanze,", "tokens": ["Wer\u00b7dend", "be\u00b7trach\u00b7te", "sie", "nun", ",", "wie", "nach", "und", "nach", "sich", "die", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "PWAV", "APPR", "KON", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Stufenweise gef\u00fchrt, bildet zu Bl\u00fcten und Frucht.", "tokens": ["Stu\u00b7fen\u00b7wei\u00b7se", "ge\u00b7f\u00fchrt", ",", "bil\u00b7det", "zu", "Bl\u00fc\u00b7ten", "und", "Frucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Aus dem Samen entwickelt sie sich, sobald ihn der Erde", "tokens": ["Aus", "dem", "Sa\u00b7men", "ent\u00b7wi\u00b7ckelt", "sie", "sich", ",", "so\u00b7bald", "ihn", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Stille befruchtender Scho\u00df hold in das Leben entl\u00e4\u00dft", "tokens": ["Stil\u00b7le", "be\u00b7fruch\u00b7ten\u00b7der", "Scho\u00df", "hold", "in", "das", "Le\u00b7ben", "ent\u00b7l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Und dem Reize des Lichts, des heiligen, ewig bewegten,", "tokens": ["Und", "dem", "Rei\u00b7ze", "des", "Lichts", ",", "des", "hei\u00b7li\u00b7gen", ",", "e\u00b7wig", "be\u00b7weg\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJD", "ADJA", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Gleich den z\u00e4rtesten Bau keimender Bl\u00e4tter empfiehlt.", "tokens": ["Gleich", "den", "z\u00e4r\u00b7tes\u00b7ten", "Bau", "kei\u00b7men\u00b7der", "Bl\u00e4t\u00b7ter", "emp\u00b7fiehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Einfach schlief in dem Samen die Kraft; ein beginnendes Vorbild", "tokens": ["Ein\u00b7fach", "schlief", "in", "dem", "Sa\u00b7men", "die", "Kraft", ";", "ein", "be\u00b7gin\u00b7nen\u00b7des", "Vor\u00b7bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "Lag, verschlossen in sich, unter die H\u00fclle gebeugt,", "tokens": ["Lag", ",", "ver\u00b7schlos\u00b7sen", "in", "sich", ",", "un\u00b7ter", "die", "H\u00fcl\u00b7le", "ge\u00b7beugt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "PRF", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Blatt und Wurzel und Keim, nur halb geformet und farblos;", "tokens": ["Blatt", "und", "Wur\u00b7zel", "und", "Keim", ",", "nur", "halb", "ge\u00b7for\u00b7met", "und", "farb\u00b7los", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,", "ADV", "ADJD", "VVPP", "KON", "ADJD", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Trocken erh\u00e4lt so der Kern ruhiges Leben bewahrt,", "tokens": ["Tro\u00b7cken", "er\u00b7h\u00e4lt", "so", "der", "Kern", "ru\u00b7hi\u00b7ges", "Le\u00b7ben", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Quillet strebend empor, sich milder Feuchte vertrauend,", "tokens": ["Quil\u00b7let", "stre\u00b7bend", "em\u00b7por", ",", "sich", "mil\u00b7der", "Feuch\u00b7te", "ver\u00b7trau\u00b7end", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "PRF", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Und erhebt sich sogleich aus der umgebenden Nacht.", "tokens": ["Und", "er\u00b7hebt", "sich", "sog\u00b7leich", "aus", "der", "um\u00b7ge\u00b7ben\u00b7den", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Aber einfach bleibt die Gestalt der ersten Erscheinung;", "tokens": ["A\u00b7ber", "ein\u00b7fach", "bleibt", "die", "Ge\u00b7stalt", "der", "ers\u00b7ten", "Er\u00b7schei\u00b7nung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Und so bezeichnet sich auch unter den Pflanzen das Kind.", "tokens": ["Und", "so", "be\u00b7zeich\u00b7net", "sich", "auch", "un\u00b7ter", "den", "Pflan\u00b7zen", "das", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Gleich darauf ein folgender Trieb, sich erhebend, erneuet,", "tokens": ["Gleich", "da\u00b7rauf", "ein", "fol\u00b7gen\u00b7der", "Trieb", ",", "sich", "er\u00b7he\u00b7bend", ",", "er\u00b7neu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PAV", "ART", "ADJA", "NN", "$,", "PRF", "VVPP", "$,", "VVFIN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Knoten auf Knoten get\u00fcrmt, immer das erste Gebild.", "tokens": ["Kno\u00b7ten", "auf", "Kno\u00b7ten", "ge\u00b7t\u00fcrmt", ",", "im\u00b7mer", "das", "ers\u00b7te", "Ge\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.25": {"text": "Zwar nicht immer das gleiche; denn mannigfaltig erzeugt sich,", "tokens": ["Zwar", "nicht", "im\u00b7mer", "das", "glei\u00b7che", ";", "denn", "man\u00b7nig\u00b7fal\u00b7tig", "er\u00b7zeugt", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ART", "ADJA", "$.", "KON", "ADJD", "VVFIN", "PRF", "$,"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Ausgebildet, du siehst's, immer das folgende Blatt,", "tokens": ["Aus\u00b7ge\u00b7bil\u00b7det", ",", "du", "siehst's", ",", "im\u00b7mer", "das", "fol\u00b7gen\u00b7de", "Blatt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "NE", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Ausgedehnter, gekerbter, getrennter in Spitzen und Teile,", "tokens": ["Aus\u00b7ge\u00b7dehn\u00b7ter", ",", "ge\u00b7kerb\u00b7ter", ",", "ge\u00b7trenn\u00b7ter", "in", "Spit\u00b7zen", "und", "Tei\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Die verwachsen vorher ruhten im untern Organ.", "tokens": ["Die", "ver\u00b7wach\u00b7sen", "vor\u00b7her", "ruh\u00b7ten", "im", "un\u00b7tern", "Or\u00b7gan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Und so erreicht es zuerst die h\u00f6chst bestimmte Vollendung,", "tokens": ["Und", "so", "er\u00b7reicht", "es", "zu\u00b7erst", "die", "h\u00f6chst", "be\u00b7stimm\u00b7te", "Vol\u00b7len\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+--", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Die bei manchem Geschlecht dich zum Erstaunen bewegt.", "tokens": ["Die", "bei", "man\u00b7chem", "Ge\u00b7schlecht", "dich", "zum", "Er\u00b7stau\u00b7nen", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Viel gerippt und gezackt, auf mastig strotzender Fl\u00e4che,", "tokens": ["Viel", "ge\u00b7rippt", "und", "ge\u00b7zackt", ",", "auf", "mas\u00b7tig", "strot\u00b7zen\u00b7der", "Fl\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "VVPP", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+---+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Scheinet die F\u00fclle des Triebs frei und unendlich zu sein.", "tokens": ["Schei\u00b7net", "die", "F\u00fcl\u00b7le", "des", "Triebs", "frei", "und", "un\u00b7end\u00b7lich", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+--+---+-+-+-+", "measure": "dactylic.di.plus"}, "line.33": {"text": "Doch hier h\u00e4lt die Natur, mit m\u00e4chtigen H\u00e4nden, die Bildung", "tokens": ["Doch", "hier", "h\u00e4lt", "die", "Na\u00b7tur", ",", "mit", "m\u00e4ch\u00b7ti\u00b7gen", "H\u00e4n\u00b7den", ",", "die", "Bil\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.34": {"text": "An und lenket sie sanft in das Vollkommnere hin.", "tokens": ["An", "und", "len\u00b7ket", "sie", "sanft", "in", "das", "Voll\u00b7komm\u00b7ne\u00b7re", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.35": {"text": "M\u00e4\u00dfiger leitet sie nun den Saft, verengt die Gef\u00e4\u00dfe,", "tokens": ["M\u00e4\u00b7\u00dfi\u00b7ger", "lei\u00b7tet", "sie", "nun", "den", "Saft", ",", "ver\u00b7engt", "die", "Ge\u00b7f\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Und gleich zeigt die Gestalt z\u00e4rtere Wirkungen an.", "tokens": ["Und", "gleich", "zeigt", "die", "Ge\u00b7stalt", "z\u00e4r\u00b7te\u00b7re", "Wir\u00b7kun\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.37": {"text": "Stille zieht sich der Trieb der strebenden R\u00e4nder zur\u00fccke,", "tokens": ["Stil\u00b7le", "zieht", "sich", "der", "Trieb", "der", "stre\u00b7ben\u00b7den", "R\u00e4n\u00b7der", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.38": {"text": "Und die Rippe des Stiels bildet sich v\u00f6lliger aus.", "tokens": ["Und", "die", "Rip\u00b7pe", "des", "Stiels", "bil\u00b7det", "sich", "v\u00f6l\u00b7li\u00b7ger", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.39": {"text": "Blattlos aber und schnell erhebt sich der z\u00e4rtere Stengel,", "tokens": ["Blatt\u00b7los", "a\u00b7ber", "und", "schnell", "er\u00b7hebt", "sich", "der", "z\u00e4r\u00b7te\u00b7re", "Sten\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "ADJD", "VVFIN"