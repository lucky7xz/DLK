{"textgrid.poem.54894": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ists m\u00f6glich/ wie man sagt/ da\u00df die geh\u00e4ufften Schmertzen/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ists m\u00f6glich/ wie man sagt/ da\u00df die geh\u00e4ufften Schmertzen/", "tokens": ["Ists", "m\u00f6g\u00b7lich", "/", "wie", "man", "sagt", "/", "da\u00df", "die", "ge\u00b7h\u00e4uff\u00b7ten", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "PWAV", "PIS", "VVFIN", "$(", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In die ich mich vertiefft/ noch iemand gehn zu Hertzen/", "tokens": ["In", "die", "ich", "mich", "ver\u00b7tiefft", "/", "noch", "ie\u00b7mand", "gehn", "zu", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVPP", "$(", "ADV", "PIS", "VVINF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ists m\u00f6glich/ da\u00df man noch mit dem Mitleiden tr\u00e4gt/", "tokens": ["Ists", "m\u00f6g\u00b7lich", "/", "da\u00df", "man", "noch", "mit", "dem", "Mit\u00b7lei\u00b7den", "tr\u00e4gt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KOUS", "PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf den der harte Blitz mit lichtem Feuer schl\u00e4gt/", "tokens": ["Auf", "den", "der", "har\u00b7te", "Blitz", "mit", "lich\u00b7tem", "Feu\u00b7er", "schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den zwar die grause Noth/ die Kirch und Haus verzehret/", "tokens": ["Den", "zwar", "die", "grau\u00b7se", "Noth", "/", "die", "Kirch", "und", "Haus", "ver\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und St\u00e4dte weggesengt/ und L\u00e4nder umgekehret/", "tokens": ["Und", "St\u00e4d\u00b7te", "weg\u00b7ge\u00b7sengt", "/", "und", "L\u00e4n\u00b7der", "um\u00b7ge\u00b7keh\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$(", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch mehr das tolle Gl\u00fcck mit aller Donner Macht/", "tokens": ["Doch", "mehr", "das", "tol\u00b7le", "Gl\u00fcck", "mit", "al\u00b7ler", "Don\u00b7ner", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und grimmer Winde Sturm und tr\u00fcber Wetter Nacht/", "tokens": ["Und", "grim\u00b7mer", "Win\u00b7de", "Sturm", "und", "tr\u00fc\u00b7ber", "Wet\u00b7ter", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schier ieden Tag zusetzt. Was kan wol einer nennen", "tokens": ["Schier", "ie\u00b7den", "Tag", "zu\u00b7setzt", ".", "Was", "kan", "wol", "ei\u00b7ner", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "VVPP", "$.", "PWS", "VMFIN", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aus aller Jammer Heer/ da\u00df ich nicht werde kennen/", "tokens": ["Aus", "al\u00b7ler", "Jam\u00b7mer", "Heer", "/", "da\u00df", "ich", "nicht", "wer\u00b7de", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$(", "KOUS", "PPER", "PTKNEG", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das mich nicht hat verletzt. Als noch die liebe Scho\u00df", "tokens": ["Das", "mich", "nicht", "hat", "ver\u00b7letzt", ".", "Als", "noch", "die", "lie\u00b7be", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "PTKNEG", "VAFIN", "VVPP", "$.", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Mutter mich ihr Pfand und letzte Lust beschlo\u00df/", "tokens": ["Der", "Mut\u00b7ter", "mich", "ihr", "Pfand", "und", "letz\u00b7te", "Lust", "be\u00b7schlo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hat stracks/ ich wei\u00df nicht was/ auf was noch nicht gebohren/", "tokens": ["Hat", "stracks", "/", "ich", "wei\u00df", "nicht", "was", "/", "auf", "was", "noch", "nicht", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$(", "PPER", "VVFIN", "PTKNEG", "PWS", "$(", "APPR", "PRELS", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "O unerh\u00f6rter Grimm! O Laster! sich verschworen/", "tokens": ["O", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "Grimm", "!", "O", "Las\u00b7ter", "!", "sich", "ver\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$.", "NE", "NE", "$.", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und als ich kaum den Tag di\u00df s\u00fcsse Licht erblickt/", "tokens": ["Und", "als", "ich", "kaum", "den", "Tag", "di\u00df", "s\u00fcs\u00b7se", "Licht", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "PDS", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Durch unerkannte List und fremde St\u00fcck entr\u00fcckt.", "tokens": ["Durch", "un\u00b7er\u00b7kann\u00b7te", "List", "und", "frem\u00b7de", "St\u00fcck", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "O h\u00e4tt ich doch die Welt/ als sie mich erst gegr\u00fcsset/", "tokens": ["O", "h\u00e4tt", "ich", "doch", "die", "Welt", "/", "als", "sie", "mich", "erst", "ge\u00b7gr\u00fcs\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Eh ich sie noch erkennt/ auffs letzt alsbald gek\u00fcsset/", "tokens": ["Eh", "ich", "sie", "noch", "er\u00b7kennt", "/", "auffs", "letzt", "als\u00b7bald", "ge\u00b7k\u00fcs\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$(", "APPRART", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So schlieff ich sonder Pein! eh mich das vierdte Jahr", "tokens": ["So", "schlieff", "ich", "son\u00b7der", "Pein", "!", "eh", "mich", "das", "vierd\u00b7te", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$.", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der vierdte Winter fand/ lag dieser auf der Bahr", "tokens": ["Der", "vierd\u00b7te", "Win\u00b7ter", "fand", "/", "lag", "die\u00b7ser", "auf", "der", "Bahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "VVFIN", "PDAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den ich mich schuldig bin/ und di\u00df mein m\u00fcdes Leben;", "tokens": ["Den", "ich", "mich", "schul\u00b7dig", "bin", "/", "und", "di\u00df", "mein", "m\u00fc\u00b7des", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PRF", "ADJD", "VAFIN", "$(", "KON", "PDS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er fiel durch Gifft/ das ihm ein falscher Freund gegeben/", "tokens": ["Er", "fiel", "durch", "Gifft", "/", "das", "ihm", "ein", "fal\u00b7scher", "Freund", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$(", "PRELS", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der offt vor seinem Muth und hohen Geist erblast.", "tokens": ["Der", "offt", "vor", "sei\u00b7nem", "Muth", "und", "ho\u00b7hen", "Geist", "er\u00b7blast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mir leyder viel zu fr\u00fch. Eh ich die rauhe Last", "tokens": ["Mir", "ley\u00b7der", "viel", "zu", "fr\u00fch", ".", "Eh", "ich", "die", "rau\u00b7he", "Last"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "PTKA", "ADJD", "$.", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und den Verlust empfand/ hat die so schwache Glieder", "tokens": ["Und", "den", "Ver\u00b7lust", "emp\u00b7fand", "/", "hat", "die", "so", "schwa\u00b7che", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$(", "VAFIN", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Des Febers Hitz entsteckt/ die Kranckheit warff mich nieder/", "tokens": ["Des", "Fe\u00b7bers", "Hitz", "ent\u00b7steckt", "/", "die", "Kran\u00b7ck\u00b7heit", "warff", "mich", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$(", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Der Todt schw\u00e4rmt \u00fcber mir; Doch weil ich ihn begehrt", "tokens": ["Der", "Todt", "schw\u00e4rmt", "\u00fc\u00b7ber", "mir", ";", "Doch", "weil", "ich", "ihn", "be\u00b7gehrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$.", "KON", "KOUS", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Hat mir der Menschen Feind den R\u00fccken zugekehrt/", "tokens": ["Hat", "mir", "der", "Men\u00b7schen", "Feind", "den", "R\u00fc\u00b7cken", "zu\u00b7ge\u00b7kehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und nahm die Seele weg im Mittel ihrer Tage", "tokens": ["Und", "nahm", "die", "See\u00b7le", "weg", "im", "Mit\u00b7tel", "ih\u00b7rer", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ja Fr\u00fchling ihrer Zeit/ um die ich kl\u00e4glich zage.", "tokens": ["Ja", "Fr\u00fch\u00b7ling", "ih\u00b7rer", "Zeit", "/", "um", "die", "ich", "kl\u00e4g\u00b7lich", "za\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "PPOSAT", "NN", "$(", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wiewohl sie/ weil sich noch in mir ein' Ader regt", "tokens": ["Wie\u00b7wohl", "sie", "/", "weil", "sich", "noch", "in", "mir", "ein'", "A\u00b7der", "regt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$(", "KOUS", "PRF", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und weil der warme Geist in beyden Br\u00fcste schl\u00e4gt", "tokens": ["Und", "weil", "der", "war\u00b7me", "Geist", "in", "bey\u00b7den", "Br\u00fcs\u00b7te", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mir wird im Hertzen stehn. Die die mich hat gebohren/", "tokens": ["Mir", "wird", "im", "Hert\u00b7zen", "stehn", ".", "Die", "die", "mich", "hat", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVINF", "$.", "ART", "PRELS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die lieber ihren Leib/ als mich ihr Kind verlohren.", "tokens": ["Die", "lie\u00b7ber", "ih\u00b7ren", "Leib", "/", "als", "mich", "ihr", "Kind", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was hat mich/ da sie weg/ was hat mich nicht verletzt/", "tokens": ["Was", "hat", "mich", "/", "da", "sie", "weg", "/", "was", "hat", "mich", "nicht", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$(", "KOUS", "PPER", "PTKVZ", "$(", "PWS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Welch Schmertzen/ welche Qual hat mir nicht zugesetzt?", "tokens": ["Welch", "Schmert\u00b7zen", "/", "wel\u00b7che", "Qual", "hat", "mir", "nicht", "zu\u00b7ge\u00b7setzt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PWAT", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wer hat der G\u00fcter Rest nicht diebisch mir entzogen/", "tokens": ["Wer", "hat", "der", "G\u00fc\u00b7ter", "Rest", "nicht", "die\u00b7bisch", "mir", "ent\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "PTKNEG", "ADJD", "PPER", "VVPP", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Und meinen Geist gekr\u00e4nckt/ und mich mit List betrogen?", "tokens": ["Und", "mei\u00b7nen", "Geist", "ge\u00b7kr\u00e4nckt", "/", "und", "mich", "mit", "List", "be\u00b7tro\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$(", "KON", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wen hab ich nicht/ der ie mein Elend recht beschaut", "tokens": ["Wen", "hab", "ich", "nicht", "/", "der", "ie", "mein", "E\u00b7lend", "recht", "be\u00b7schaut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "$(", "ART", "ADV", "PPOSAT", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mit h\u00f6chstem Seelen Weh der schwartzen Grufft vertraut?", "tokens": ["Mit", "h\u00f6chs\u00b7tem", "See\u00b7len", "Weh", "der", "schwart\u00b7zen", "Grufft", "ver\u00b7traut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich hab ", "tokens": ["Ich", "hab"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.42": {"text": "Und deine keusche Leich ", "tokens": ["Und", "dei\u00b7ne", "keu\u00b7sche", "Leich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Die ehmals mich erg\u00f6tzt um die ich ietzund wein.", "tokens": ["Die", "eh\u00b7mals", "mich", "er\u00b7g\u00f6tzt", "um", "die", "ich", "ie\u00b7tzund", "wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PPER", "VVPP", "APPR", "PRELS", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und den das gantze Land ans F\u00fcrsten Statt geehret/", "tokens": ["Und", "den", "das", "gant\u00b7ze", "Land", "ans", "F\u00fcrs\u00b7ten", "Statt", "ge\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "APPRART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und in der Armen Band den wert