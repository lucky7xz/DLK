"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "PWAV", "ADJD", "PDAT", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.50": {"text": "Doch kan ich sie noch nicht mit dieser Angst vergleichen/", "tokens": ["Doch", "kan", "ich", "sie", "noch", "nicht", "mit", "die\u00b7ser", "Angst", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Die ietzt mich \u00fcberf\u00e4llt. Ach h\u00e4tt ich deine Leichen/", "tokens": ["Die", "ietzt", "mich", "\u00fc\u00b7berf\u00b7\u00e4llt", ".", "Ach", "h\u00e4tt", "ich", "dei\u00b7ne", "Lei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$.", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Mein Bruder h\u00e4tt ich doch die Leiche noch gek\u00fcst/", "tokens": ["Mein", "Bru\u00b7der", "h\u00e4tt", "ich", "doch", "die", "Lei\u00b7che", "noch", "ge\u00b7k\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wenn ja der ", "tokens": ["Wenn", "ja", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.54": {"text": "Ein Wort/ ein kurtz Ade mir nicht verg\u00f6nnt zu h\u00f6ren;", "tokens": ["Ein", "Wort", "/", "ein", "kurtz", "A\u00b7de", "mir", "nicht", "ver\u00b7g\u00f6nnt", "zu", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJD", "NN", "PPER", "PTKNEG", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.55": {"text": "Ach mu\u00df mich dieser Blitz der scharffe Pfeil versehren!", "tokens": ["Ach", "mu\u00df", "mich", "die\u00b7ser", "Blitz", "der", "scharf\u00b7fe", "Pfeil", "ver\u00b7seh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "PDAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Weil ich so fern von dir ein unbekandtes Land", "tokens": ["Weil", "ich", "so", "fern", "von", "dir", "ein", "un\u00b7be\u00b7kand\u00b7tes", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Und weites Volck beschau. Ach zeuchst du deine Hand", "tokens": ["Und", "wei\u00b7tes", "Volck", "be\u00b7schau", ".", "Ach", "zeuchst", "du", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$.", "ITJ", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "So pl\u00f6tzlich von mir ab! nun ieder mich verlassen", "tokens": ["So", "pl\u00f6tz\u00b7lich", "von", "mir", "ab", "!", "nun", "ie\u00b7der", "mich", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$.", "ADV", "PIAT", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Und nichts als Ach und Angst und Schmertz und Weh umfassen", "tokens": ["Und", "nichts", "als", "Ach", "und", "Angst", "und", "Schmertz", "und", "Weh", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Und solche Noth die auch ein fremdes Hertz durchbricht/", "tokens": ["Und", "sol\u00b7che", "Noth", "die", "auch", "ein", "frem\u00b7des", "Hertz", "durch\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Wenn man ein wenig nur von meinem Elend spricht", "tokens": ["Wenn", "man", "ein", "we\u00b7nig", "nur", "von", "mei\u00b7nem", "E\u00b7lend", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Das hier kaum iemand wei\u00df. Was kan ich mehr begehren", "tokens": ["Das", "hier", "kaum", "ie\u00b7mand", "wei\u00df", ".", "Was", "kan", "ich", "mehr", "be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "PIS", "VVFIN", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Als da\u00df mein Nahm und Land und Stand und heisse Z\u00e4hren", "tokens": ["Als", "da\u00df", "mein", "Nahm", "und", "Land", "und", "Stand", "und", "heis\u00b7se", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Bleib allen unbekandt. Weil/ wenn ich di\u00df betracht/", "tokens": ["Bleib", "al\u00b7len", "un\u00b7be\u00b7kandt", ".", "Weil", "/", "wenn", "ich", "di\u00df", "be\u00b7tracht", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJD", "$.", "KOUS", "$(", "KOUS", "PPER", "PDS", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Mein Nahm und Land und Stand nur viel betr\u00fcbte macht.", "tokens": ["Mein", "Nahm", "und", "Land", "und", "Stand", "nur", "viel", "be\u00b7tr\u00fcb\u00b7te", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "KON", "NN", "ADV", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Fragt ", "tokens": ["Fragt"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.67": {"text": "Fragt nicht mehr wer ich sey/ wo richtig\u2013- was ihr saget", "tokens": ["Fragt", "nicht", "mehr", "wer", "ich", "sey", "/", "wo", "rich\u00b7tig", "\u2013", "was", "ihr", "sa\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "PWS", "PPER", "VAFIN", "$(", "PWAV", "ADJD", "$(", "$(", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Und euch der rauhe Sturm der mich noch ietzt anweht/", "tokens": ["Und", "euch", "der", "rau\u00b7he", "Sturm", "der", "mich", "noch", "ietzt", "an\u00b7weht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ART", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "So tieff zu Hertzen geht/ so w\u00fcnsch ich Sch\u00f6nste seht", "tokens": ["So", "tieff", "zu", "Hert\u00b7zen", "geht", "/", "so", "w\u00fcnsch", "ich", "Sch\u00f6ns\u00b7te", "seht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "$(", "ADV", "ADJD", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Euch in so hoher Ruh als grimmig meine Wunden/", "tokens": ["Euch", "in", "so", "ho\u00b7her", "Ruh", "als", "grim\u00b7mig", "mei\u00b7ne", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "ADJA", "NN", "KOKOM", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Und findet so viel Freud und angenehme Stunden", "tokens": ["Und", "fin\u00b7det", "so", "viel", "Freud", "und", "an\u00b7ge\u00b7neh\u00b7me", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Und unverf\u00e4lschte Lust als Jammer in der Welt", "tokens": ["Und", "un\u00b7ver\u00b7f\u00e4lschte", "Lust", "als", "Jam\u00b7mer", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.73": {"text": "Und Weh und Pein und Angst mich t\u00e4glich \u00fcberf\u00e4lt.", "tok