{"textgrid.poem.29661": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mode gieng spatziren und kam zu einem Alten;", "tokens": ["Die", "Mo\u00b7de", "gieng", "spat\u00b7zi\u00b7ren", "und", "kam", "zu", "ei\u00b7nem", "Al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da war ihr gar zu wider bei ihm sich auff zu halten.", "tokens": ["Da", "war", "ihr", "gar", "zu", "wi\u00b7der", "bei", "ihm", "sich", "auff", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKZU", "APPR", "APPR", "PPER", "PRF", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Alte, der di\u00df merckte, sprach: Liebe Freundin, dencke,", "tokens": ["Der", "Al\u00b7te", ",", "der", "di\u00df", "merck\u00b7te", ",", "sprach", ":", "Lie\u00b7be", "Freun\u00b7din", ",", "den\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDS", "VVFIN", "$,", "VVFIN", "$.", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Man legt dich nach sechs Monden gleichwol schon unter B\u00e4ncke.", "tokens": ["Man", "legt", "dich", "nach", "sechs", "Mon\u00b7den", "gleich\u00b7wol", "schon", "un\u00b7ter", "B\u00e4n\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "CARD", "NN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}