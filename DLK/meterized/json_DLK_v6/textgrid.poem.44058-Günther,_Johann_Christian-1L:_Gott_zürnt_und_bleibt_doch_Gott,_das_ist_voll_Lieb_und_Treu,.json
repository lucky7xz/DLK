{"textgrid.poem.44058": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott z\u00fcrnt und bleibt doch Gott, das ist voll Lieb und Treu,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott z\u00fcrnt und bleibt doch Gott, das ist voll Lieb und Treu,", "tokens": ["Gott", "z\u00fcrnt", "und", "bleibt", "doch", "Gott", ",", "das", "ist", "voll", "Lieb", "und", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVFIN", "ADV", "NN", "$,", "PDS", "VAFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}, "line.3": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}, "line.4": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}, "line.5": {"text": "Drum spreche doch kein Mensch: Nun bin ich hingebracht,", "tokens": ["Drum", "spre\u00b7che", "doch", "kein", "Mensch", ":", "Nun", "bin", "ich", "hin\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PIAT", "NN", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So nah und gro\u00df ihm auch die gr\u00f6ste Tr\u00fcbsahl scheinet.", "tokens": ["So", "nah", "und", "gro\u00df", "ihm", "auch", "die", "gr\u00f6s\u00b7te", "Tr\u00fcb\u00b7sahl", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Trost kommt oft daher, wohin man nie gedacht,", "tokens": ["Der", "Trost", "kommt", "oft", "da\u00b7her", ",", "wo\u00b7hin", "man", "nie", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PAV", "$,", "PWAV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und eher, als noch wohl der W\u00fcntsche Furcht vermeinet.", "tokens": ["Und", "e\u00b7her", ",", "als", "noch", "wohl", "der", "W\u00fcnt\u00b7sche", "Furcht", "ver\u00b7mei\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich sa\u00df von allem leer, was jeder Mensch gebraucht,", "tokens": ["Ich", "sa\u00df", "von", "al\u00b7lem", "leer", ",", "was", "je\u00b7der", "Mensch", "ge\u00b7braucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "ADJD", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Im Winckel, kranck, verhast und unter fremden Leuten,", "tokens": ["Im", "Win\u00b7ckel", ",", "kranck", ",", "ver\u00b7hast", "und", "un\u00b7ter", "frem\u00b7den", "Leu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "$,", "VVFIN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bey \u00c4rgern\u00fc\u00df und Gift, den grobe Bo\u00dfheit haucht,", "tokens": ["Bey", "\u00c4r\u00b7ger\u00b7n\u00fc\u00df", "und", "Gift", ",", "den", "gro\u00b7be", "Bo\u00df\u00b7heit", "haucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In allgemeinem Weh und jammervollen Zeiten.", "tokens": ["In", "all\u00b7ge\u00b7mei\u00b7nem", "Weh", "und", "jam\u00b7mer\u00b7vol\u00b7len", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sonst fast ich mir noch Muth, jezt aber h\u00e4tt ich bald", "tokens": ["Sonst", "fast", "ich", "mir", "noch", "Muth", ",", "jezt", "a\u00b7ber", "h\u00e4tt", "ich", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PPER", "ADV", "NN", "$,", "ADV", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Buch, Leben und Gedult auf einmahl hingeschmi\u00dfen,", "tokens": ["Buch", ",", "Le\u00b7ben", "und", "Ge\u00b7dult", "auf", "ein\u00b7mahl", "hin\u00b7ge\u00b7schmi\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da \u00fcber alle Noth kein treues Weinen galt", "tokens": ["Da", "\u00fc\u00b7ber", "al\u00b7le", "Noth", "kein", "treu\u00b7es", "Wei\u00b7nen", "galt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und jeder L\u00e4strer sprach: Schon recht, so mu\u00df er b\u00fc\u00dfen.", "tokens": ["Und", "je\u00b7der", "L\u00e4st\u00b7rer", "sprach", ":", "Schon", "recht", ",", "so", "mu\u00df", "er", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$.", "ADV", "ADJD", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich gieng gleichwohl in mich und war mir nichts bewust,", "tokens": ["Ich", "gieng", "gleich\u00b7wohl", "in", "mich", "und", "war", "mir", "nichts", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "KON", "VAFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als da\u00df ich so gefehlt, wie alle Menschen fallen.", "tokens": ["Als", "da\u00df", "ich", "so", "ge\u00b7fehlt", ",", "wie", "al\u00b7le", "Men\u00b7schen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVPP", "$,", "PWAV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da fiel nun vollends gar die Langmuth aus der Brust,", "tokens": ["Da", "fiel", "nun", "vol\u00b7lends", "gar", "die", "Lang\u00b7muth", "aus", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und dies mein redlich Blut fing eifrig an zu wallen.", "tokens": ["Und", "dies", "mein", "red\u00b7lich", "Blut", "fing", "eif\u00b7rig", "an", "zu", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PPOSAT", "ADJD", "NN", "VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich war schon im Begriff und wollte \u2013 \u2013 Was geschieht?", "tokens": ["Ich", "war", "schon", "im", "Be\u00b7griff", "und", "woll\u00b7te", "\u2013", "\u2013", "Was", "ge\u00b7schieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "KON", "VMFIN", "$(", "$(", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es mu\u00df zu meiner Ruh ein andrer Mensch erbla\u00dfen", "tokens": ["Es", "mu\u00df", "zu", "mei\u00b7ner", "Ruh", "ein", "an\u00b7drer", "Mensch", "er\u00b7bla\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und, da der schnelle Fall viel Volck zu Grabe zieht,", "tokens": ["Und", ",", "da", "der", "schnel\u00b7le", "Fall", "viel", "Volck", "zu", "Gra\u00b7be", "zieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN", "PIAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ein l\u00e4ngst verge\u00dfner Freund mich in dem Volck umfa\u00dfen.", "tokens": ["Ein", "l\u00e4ngst", "ver\u00b7ge\u00df\u00b7ner", "Freund", "mich", "in", "dem", "Volck", "um\u00b7fa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da lag der schwere Stein auf einmahl wegger\u00fcckt,", "tokens": ["Da", "lag", "der", "schwe\u00b7re", "Stein", "auf", "ein\u00b7mahl", "weg\u00b7ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich kam dadurch nunmehr zu Nothdurft und Vergn\u00fcgen;", "tokens": ["Ich", "kam", "da\u00b7durch", "nun\u00b7mehr", "zu", "Noth\u00b7durft", "und", "Ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es schien, als w\u00fcrde mir ein Engel zugeschickt:", "tokens": ["Es", "schien", ",", "als", "w\u00fcr\u00b7de", "mir", "ein", "En\u00b7gel", "zu\u00b7ge\u00b7schickt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So schnell und wunderlich verf\u00e4hrt das weise F\u00fcgen.", "tokens": ["So", "schnell", "und", "wun\u00b7der\u00b7lich", "ver\u00b7f\u00e4hrt", "das", "wei\u00b7se", "F\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Man meint, es h\u00e4tten l\u00e4ngst die Wunder aufgeh\u00f6rt;", "tokens": ["Man", "meint", ",", "es", "h\u00e4t\u00b7ten", "l\u00e4ngst", "die", "Wun\u00b7der", "auf\u00b7ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ihr Thoren b\u00f6ser Art, wie seichte steht der Glauben!", "tokens": ["Ihr", "Tho\u00b7ren", "b\u00f6\u00b7ser", "Art", ",", "wie", "seich\u00b7te", "steht", "der", "Glau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "PWAV", "VVFIN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Er murrt schon, wenn ihn nicht ein hohes Zeichen st\u00f6rt,", "tokens": ["Er", "murrt", "schon", ",", "wenn", "ihn", "nicht", "ein", "ho\u00b7hes", "Zei\u00b7chen", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und will der Warheit selbst gar oft die Warheit rauben.", "tokens": ["Und", "will", "der", "War\u00b7heit", "selbst", "gar", "oft", "die", "War\u00b7heit", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was soll der tolle Wuntsch, des rothen Meeres Grund,", "tokens": ["Was", "soll", "der", "tol\u00b7le", "Wunt\u00b7sch", ",", "des", "ro\u00b7then", "Mee\u00b7res", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Den Fels, den Moses schlug, Eli\u00e4 Brodt und Raben,", "tokens": ["Den", "Fels", ",", "den", "Mo\u00b7ses", "schlug", ",", "E\u00b7li\u00e4", "Brodt", "und", "Ra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Die Sonne, so der Hand des Helden stille stund,", "tokens": ["Die", "Son\u00b7ne", ",", "so", "der", "Hand", "des", "Hel\u00b7den", "stil\u00b7le", "stund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und bey des Heilands Gruft den Stein gesehn zu haben?", "tokens": ["Und", "bey", "des", "Hei\u00b7lands", "Gruft", "den", "Stein", "ge\u00b7sehn", "zu", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Was steigt ihr weit und hoch? Ihr habt es in der N\u00e4h.", "tokens": ["Was", "steigt", "ihr", "weit", "und", "hoch", "?", "Ihr", "habt", "es", "in", "der", "N\u00e4h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Durchgeht den Lebenslauf bald von den ersten Jahren:", "tokens": ["Durch\u00b7geht", "den", "Le\u00b7bens\u00b7lauf", "bald", "von", "den", "ers\u00b7ten", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Je mehr ihr Stunden zehlt, je mehr ich Wunder seh,", "tokens": ["Je", "mehr", "ihr", "Stun\u00b7den", "zehlt", ",", "je", "mehr", "ich", "Wun\u00b7der", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die ihr so gut als ich von Gottes Huld erfahren.", "tokens": ["Die", "ihr", "so", "gut", "als", "ich", "von", "Got\u00b7tes", "Huld", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wie sch\u00f6n und ordentlich beuth immer Lust und Pein,", "tokens": ["Wie", "sch\u00f6n", "und", "or\u00b7dent\u00b7lich", "beuth", "im\u00b7mer", "Lust", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Verdienst und Straf und Lohn einander nicht die H\u00e4nde!", "tokens": ["Ver\u00b7dienst", "und", "Straf", "und", "Lohn", "ein\u00b7an\u00b7der", "nicht", "die", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wie \u00f6fters schien es nicht schon v\u00f6llig aus zu seyn?", "tokens": ["Wie", "\u00f6f\u00b7ters", "schien", "es", "nicht", "schon", "v\u00f6l\u00b7lig", "aus", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "PTKVZ", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ich wende kaum die Hand, so folgt ein be\u00dfer Ende.", "tokens": ["Ich", "wen\u00b7de", "kaum", "die", "Hand", ",", "so", "folgt", "ein", "be\u00b7\u00dfer", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ach, gro\u00df- und weiser Gott, was vor ein L\u00f6segeld", "tokens": ["Ach", ",", "gro\u00df", "und", "wei\u00b7ser", "Gott", ",", "was", "vor", "ein", "L\u00f6\u00b7se\u00b7geld"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "TRUNC", "KON", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Soll vor so viele Treu mein schuldig Herz entbinden?", "tokens": ["Soll", "vor", "so", "vie\u00b7le", "Treu", "mein", "schul\u00b7dig", "Herz", "ent\u00b7bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADV", "PIAT", "NN", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Du bist es, der mich liebt und unter Ruthen h\u00e4lt,", "tokens": ["Du", "bist", "es", ",", "der", "mich", "liebt", "und", "un\u00b7ter", "Ru\u00b7then", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "KON", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Damit nicht Fleisch und Blut den Weg zur H\u00f6lle finden;", "tokens": ["Da\u00b7mit", "nicht", "Fleisch", "und", "Blut", "den", "Weg", "zur", "H\u00f6l\u00b7le", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "NN", "KON", "NN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Du hilfst mir allzeit aus, erweckst und stillst Gefahr,", "tokens": ["Du", "hilfst", "mir", "all\u00b7zeit", "aus", ",", "er\u00b7weckst", "und", "stillst", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "VVFIN", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Erhebst mich durch die Last, damit ich h\u00f6her blicke.", "tokens": ["Er\u00b7hebst", "mich", "durch", "die", "Last", ",", "da\u00b7mit", "ich", "h\u00f6\u00b7her", "bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Nun schmeck ich in der That, nun werd ich recht gewahr:", "tokens": ["Nun", "schmeck", "ich", "in", "der", "That", ",", "nun", "werd", "ich", "recht", "ge\u00b7wahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Ein Herz, dem Creuz gebricht, entbehrt die Lust vom Gl\u00fccke.", "tokens": ["Ein", "Herz", ",", "dem", "Creuz", "ge\u00b7bricht", ",", "ent\u00b7behrt", "die", "Lust", "vom", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Ja, wo ein Gegentheil dem andern Nachdruck giebt,", "tokens": ["Ja", ",", "wo", "ein", "Ge\u00b7gen\u00b7theil", "dem", "an\u00b7dern", "Nach\u00b7druck", "giebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "So mu\u00df ich dermahleins der Freude mehr genie\u00dfen", "tokens": ["So", "mu\u00df", "ich", "der\u00b7mah\u00b7leins", "der", "Freu\u00b7de", "mehr", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Als die, so in der Zeit sich gar um nichts betr\u00fcbt;", "tokens": ["Als", "die", ",", "so", "in", "der", "Zeit", "sich", "gar", "um", "nichts", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "APPR", "ART", "NN", "PRF", "ADV", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "So weis die Noth sogar den Himmel zu vers\u00fc\u00dfen.", "tokens": ["So", "weis", "die", "Noth", "so\u00b7gar", "den", "Him\u00b7mel", "zu", "ver\u00b7s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "ART", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Ach, las mir stets ein Theil von solcher Traurigkeit,", "tokens": ["Ach", ",", "las", "mir", "stets", "ein", "Theil", "von", "sol\u00b7cher", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Nur las mich nicht die Noth an Kunst und Wei\u00dfheit hindern;", "tokens": ["Nur", "las", "mich", "nicht", "die", "Noth", "an", "Kunst", "und", "Wei\u00df\u00b7heit", "hin\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Kurz, mach es, wie du wilst, du kennest Maas und Zeit", "tokens": ["Kurz", ",", "mach", "es", ",", "wie", "du", "wilst", ",", "du", "ken\u00b7nest", "Maas", "und", "Zeit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Und wirst mir wohl keinmahl ein wahres Heil vermindern.", "tokens": ["Und", "wirst", "mir", "wohl", "kein\u00b7mahl", "ein", "wah\u00b7res", "Heil", "ver\u00b7min\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}