{"textgrid.poem.41598": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer solche Last zu heben sinnt,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer solche Last zu heben sinnt,", "tokens": ["Wer", "sol\u00b7che", "Last", "zu", "he\u00b7ben", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Braucht, Sisyphus, deine St\u00e4rke", "tokens": ["Braucht", ",", "Si\u00b7syp\u00b7hus", ",", "dei\u00b7ne", "St\u00e4r\u00b7ke"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und hat er Herz auch zum Werke \u2013", "tokens": ["Und", "hat", "er", "Herz", "auch", "zum", "Wer\u00b7ke", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Kunst ist lang, die Zeit entrinnt.", "tokens": ["Die", "Kunst", "ist", "lang", ",", "die", "Zeit", "ent\u00b7rinnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fern von prangenden Sarkophagen", "tokens": ["Fern", "von", "pran\u00b7gen\u00b7den", "Sar\u00b7ko\u00b7pha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zieht zu einsamem Gr\u00e4berreich", "tokens": ["Zieht", "zu", "ein\u00b7sa\u00b7mem", "Gr\u00e4\u00b7ber\u00b7reich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Mein Herz, verh\u00fclltem Trommler gleich,", "tokens": ["Mein", "Herz", ",", "ver\u00b7h\u00fcll\u00b7tem", "Tromm\u00b7ler", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den letzten Grabmarsch zu schlagen.", "tokens": ["Den", "letz\u00b7ten", "Grab\u00b7marsch", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Manch Kleinod schl\u00e4ft im Grund versteckt,", "tokens": ["Manch", "Klei\u00b7nod", "schl\u00e4ft", "im", "Grund", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo niemals es ein Karst entdeckt,", "tokens": ["Wo", "nie\u00b7mals", "es", "ein", "Karst", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Nacht und Vergessen sich breiten;", "tokens": ["Wo", "Nacht", "und", "Ver\u00b7ges\u00b7sen", "sich", "brei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Manch eine Blume f\u00fcllt die Luft", "tokens": ["Manch", "ei\u00b7ne", "Blu\u00b7me", "f\u00fcllt", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umsonst mit s\u00fc\u00dfgeheimem Duft", "tokens": ["Um\u00b7sonst", "mit", "s\u00fc\u00df\u00b7ge\u00b7hei\u00b7mem", "Duft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In der Tiefe der Einsamkeiten.", "tokens": ["In", "der", "Tie\u00b7fe", "der", "Ein\u00b7sam\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}