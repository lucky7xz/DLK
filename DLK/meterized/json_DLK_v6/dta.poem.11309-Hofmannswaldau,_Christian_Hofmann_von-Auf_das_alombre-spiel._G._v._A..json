{"dta.poem.11309": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das alombre-spiel.  \n G. v. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alombre spielt mit uns der blasse schatten-mann:", "tokens": ["A\u00b7lomb\u00b7re", "spielt", "mit", "uns", "der", "blas\u00b7se", "schat\u00b7ten\u00b7mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er geht mit spaten ein, die uns das grab bereiten,", "tokens": ["Er", "geht", "mit", "spa\u00b7ten", "ein", ",", "die", "uns", "das", "grab", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "VVFIN", "PTKVZ", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "L\u00e4st manchen ", "tokens": ["L\u00e4st", "man\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADV", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Kein l\u00e4ugnen gilt, kein k\u00f6nig n\u00fctzt,", "tokens": ["Kein", "l\u00e4ug\u00b7nen", "gilt", ",", "kein", "k\u00f6\u00b7nig", "n\u00fctzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "$,", "PIAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein ", "tokens": ["Kein"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Riposto kommt in sarg, wir m\u00fcssen alle dran,", "tokens": ["Ri\u00b7pos\u00b7to", "kommt", "in", "sarg", ",", "wir", "m\u00fcs\u00b7sen", "al\u00b7le", "dran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJD", "$,", "PPER", "VMFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer ist, der ihn zuletzt am ", "tokens": ["Wer", "ist", ",", "der", "ihn", "zu\u00b7letzt", "am"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPER", "ADV", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}