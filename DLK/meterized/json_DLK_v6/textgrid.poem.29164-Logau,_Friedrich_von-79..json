{"textgrid.poem.29164": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter uns in denen Landen, wo die Leute nackend wandeln,", "tokens": ["Un\u00b7ter", "uns", "in", "de\u00b7nen", "Lan\u00b7den", ",", "wo", "die", "Leu\u00b7te", "na\u00b7ckend", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PRELS", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-++--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Meint man, da\u00df der Liebe Sachen sie nicht mehr als wir verhandeln;", "tokens": ["Meint", "man", ",", "da\u00df", "der", "Lie\u00b7be", "Sa\u00b7chen", "sie", "nicht", "mehr", "als", "wir", "ver\u00b7han\u00b7deln", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "ART", "NN", "NN", "PPER", "PTKNEG", "ADV", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Was man frey und t\u00e4glich schauet, pfleget minder zu bewegen;", "tokens": ["Was", "man", "frey", "und", "t\u00e4g\u00b7lich", "schau\u00b7et", ",", "pfle\u00b7get", "min\u00b7der", "zu", "be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "KON", "ADJD", "VVFIN", "$,", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Kleider decken offters M\u00e4ngel, die die Liebe nieder legen.", "tokens": ["Klei\u00b7der", "de\u00b7cken", "off\u00b7ters", "M\u00e4n\u00b7gel", ",", "die", "die", "Lie\u00b7be", "nie\u00b7der", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "$,", "PRELS", "ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}