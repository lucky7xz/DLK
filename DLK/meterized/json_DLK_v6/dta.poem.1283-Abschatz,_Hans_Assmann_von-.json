{"dta.poem.1283": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.85", "en:0.14"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Flora sa\u00df auff ihrem Throne/", "tokens": ["Flo\u00b7ra", "sa\u00df", "auff", "ih\u00b7rem", "Thro\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey ihr sah man die Napeen", "tokens": ["Bey", "ihr", "sah", "man", "die", "Na\u00b7peen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter tausend Blumen stehen;", "tokens": ["Un\u00b7ter", "tau\u00b7send", "Blu\u00b7men", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lo\u00df und Gl\u00fccke solten weisen/", "tokens": ["Lo\u00df", "und", "Gl\u00fc\u00b7cke", "sol\u00b7ten", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welch am meisten sey zu preisen:", "tokens": ["Welch", "am", "meis\u00b7ten", "sey", "zu", "prei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PTKA", "PIS", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was geschach? Die weisse Bohne", "tokens": ["Was", "ge\u00b7schach", "?", "Die", "weis\u00b7se", "Boh\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ward der Anemone.", "tokens": ["Ward", "der", "A\u00b7ne\u00b7mo\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Flora rieff mit hellem Thone:", "tokens": ["Flo\u00b7ra", "rieff", "mit", "hel\u00b7lem", "Tho\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manche sp\u00fcrt man \u00fcbel riechen/", "tokens": ["Man\u00b7che", "sp\u00fcrt", "man", "\u00fc\u00b7bel", "rie\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob sie sch\u00f6n: die Veyeln kriechen/", "tokens": ["Ob", "sie", "sch\u00f6n", ":", "die", "Vey\u00b7eln", "krie\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$.", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rosen stechen/ Liljen f\u00e4rben/", "tokens": ["Ro\u00b7sen", "ste\u00b7chen", "/", "Lil\u00b7jen", "f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$(", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nelcken welcken/ Tulpen sterben:", "tokens": ["Nel\u00b7cken", "wel\u00b7cken", "/", "Tul\u00b7pen", "ster\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$(", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drum f\u00fcr aller Blumen Crone", "tokens": ["Drum", "f\u00fcr", "al\u00b7ler", "Blu\u00b7men", "Cro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "PIAT", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gilt mir Anemone.", "tokens": ["Gilt", "mir", "A\u00b7ne\u00b7mo\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nord-Wind/ wenn du mir zu Hohne", "tokens": ["Nord\u00b7Wind", "/", "wenn", "du", "mir", "zu", "Hoh\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "PPER", "PPER", "APPR", "NN"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit verneutem Winter dr\u00e4uest/", "tokens": ["Mit", "ver\u00b7neu\u00b7tem", "Win\u00b7ter", "dr\u00e4u\u00b7est", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schnee und Hagel um dich streuest/", "tokens": ["Schnee", "und", "Ha\u00b7gel", "um", "dich", "streu\u00b7est", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich zwar gedultig lassen", "tokens": ["Will", "ich", "zwar", "ge\u00b7dul\u00b7tig", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Manche weiche Blum\u2019 erblassen/", "tokens": ["Man\u00b7che", "wei\u00b7che", "Blum'", "er\u00b7blas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber di\u00df beding\u2019 ich/ schone", "tokens": ["A\u00b7ber", "di\u00df", "be\u00b7ding'", "ich", "/", "scho\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$(", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meiner Anemone.", "tokens": ["Mei\u00b7ner", "A\u00b7ne\u00b7mo\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Venus mit dem zarten Sohne", "tokens": ["Ve\u00b7nus", "mit", "dem", "zar\u00b7ten", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Kam in Garten Blumen brechen/", "tokens": ["Kam", "in", "Gar\u00b7ten", "Blu\u00b7men", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rte diese Worte sprechen/", "tokens": ["H\u00f6r\u00b7te", "die\u00b7se", "Wor\u00b7te", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagte/ da\u00df Sylvanders Treue", "tokens": ["Sag\u00b7te", "/", "da\u00df", "Syl\u00b7van\u00b7ders", "Treu\u00b7e"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$(", "KOUS", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bald die sch\u00f6nste Blum erfreue?", "tokens": ["Bald", "die", "sch\u00f6ns\u00b7te", "Blum", "er\u00b7freu\u00b7e", "?"], "token_info": ["word", "word", "word", "wor