": ["Weil", "ich", "wer\u00b7de", "L\u00e4m\u00b7mer", "wey\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo ich treibe/ wo ich wohne/", "tokens": ["Wo", "ich", "trei\u00b7be", "/", "wo", "ich", "woh\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bl\u00fch\u2019 mir Anemone!", "tokens": ["Bl\u00fch'", "mir", "A\u00b7ne\u00b7mo\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}