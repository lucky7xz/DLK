{"textgrid.poem.36779": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Triumphbogen des Titus", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vieles erz\u00e4hlt die Geschichte von Titus' menschlicher G\u00fcte,", "tokens": ["Vie\u00b7les", "er\u00b7z\u00e4hlt", "die", "Ge\u00b7schich\u00b7te", "von", "Ti\u00b7tus'", "menschli\u00b7cher", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "Und das dankbare Rom hat ihm dies Denkmal geweiht.", "tokens": ["Und", "das", "dank\u00b7ba\u00b7re", "Rom", "hat", "ihm", "dies", "Denk\u00b7mal", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NE", "VAFIN", "PPER", "PDS", "NN", "VVPP", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Noch verehrt die liebende Welt den liebenden Heiden,", "tokens": ["Noch", "ver\u00b7ehrt", "die", "lie\u00b7ben\u00b7de", "Welt", "den", "lie\u00b7ben\u00b7den", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Hat vor der Liebe ja nun auch der Verstand sich geb\u00fcckt.", "tokens": ["Hat", "vor", "der", "Lie\u00b7be", "ja", "nun", "auch", "der", "Ver\u00b7stand", "sich", "ge\u00b7b\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ADV", "ADV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Kapuziner durchziehen das Thor mit frommem Gesange,", "tokens": ["Ka\u00b7pu\u00b7zi\u00b7ner", "durch\u00b7zie\u00b7hen", "das", "Thor", "mit", "from\u00b7mem", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Und statt dem Lorbeer bedeckt nun die Kapuze das Haupt.", "tokens": ["Und", "statt", "dem", "Lor\u00b7beer", "be\u00b7deckt", "nun", "die", "Ka\u00b7pu\u00b7ze", "das", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}