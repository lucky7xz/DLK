{"dta.poem.1153": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "39. Die Sch\u00f6ne Gro\u00df-Nase.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "In einer See voll Milch und Blutt der frischen Wangen ", "tokens": ["In", "ei\u00b7ner", "See", "voll", "Milch", "und", "Blutt", "der", "fri\u00b7schen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "KON", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist deiner Nase Thurm zum ", "tokens": ["Ist", "dei\u00b7ner", "Na\u00b7se", "Thurm", "zum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Damit der Hoffnung Schiff am Felsen nicht zerschellt/", "tokens": ["Da\u00b7mit", "der", "Hoff\u00b7nung", "Schiff", "am", "Fel\u00b7sen", "nicht", "zer\u00b7schellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "APPRART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gl\u00e4nzt ein gedoppelt Licht von oben ausgehangen.", "tokens": ["Gl\u00e4nzt", "ein", "ge\u00b7dop\u00b7pelt", "Licht", "von", "o\u00b7ben", "aus\u00b7ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "VVPP", "NN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Recht/ was dem Himmel schmeckt/ mu\u00df in die H\u00f6he prangen.", "tokens": ["Recht", "/", "was", "dem", "Him\u00b7mel", "schmeckt", "/", "mu\u00df", "in", "die", "H\u00f6\u00b7he", "pran\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "ART", "NN", "VVFIN", "$(", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Cupido/ der dein Aug als seinen Bogen h\u00e4lt/", "tokens": ["Cu\u00b7pi\u00b7do", "/", "der", "dein", "Aug", "als", "sei\u00b7nen", "Bo\u00b7gen", "h\u00e4lt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "PPOSAT", "NN", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Hat ihm so starcken Pfeil mit Fleisse zugesellt/", "tokens": ["Hat", "ihm", "so", "star\u00b7cken", "Pfeil", "mit", "Fleis\u00b7se", "zu\u00b7ge\u00b7sellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df er uns desto mehr ins Hertze k\u00f6nne langen.", "tokens": ["Da\u00df", "er", "uns", "des\u00b7to", "mehr", "ins", "Hert\u00b7ze", "k\u00f6n\u00b7ne", "lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "APPRART", "VVFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O Nase/ werth dem Stirn-Geb\u00fcrge beyzuwohnen/", "tokens": ["O", "Na\u00b7se", "/", "werth", "dem", "Stirn\u00b7Ge\u00b7b\u00fcr\u00b7ge", "bey\u00b7zu\u00b7woh\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ADJD", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du kanst in dem Gesicht/ das aller H\u00fcgel rein/", "tokens": ["Du", "kanst", "in", "dem", "Ge\u00b7sicht", "/", "das", "al\u00b7ler", "H\u00fc\u00b7gel", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "$(", "ART", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Klugheit Wetzstein und der Sch\u00f6nheit Br\u00fccke seyn.", "tokens": ["Der", "Klug\u00b7heit", "Wetz\u00b7stein", "und", "der", "Sch\u00f6n\u00b7heit", "Br\u00fc\u00b7cke", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wem deine stoltze Zier in Augen ist ein Dorn/", "tokens": ["Wem", "dei\u00b7ne", "stolt\u00b7ze", "Zier", "in", "Au\u00b7gen", "ist", "ein", "Dorn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der schm\u00e4h den Adler auch/ das grosse Nasenhorn/", "tokens": ["Der", "schm\u00e4h", "den", "Ad\u00b7ler", "auch", "/", "das", "gros\u00b7se", "Na\u00b7sen\u00b7horn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Naso/ den Nasic/ und alle Nasamonen.", "tokens": ["Den", "Na\u00b7so", "/", "den", "Na\u00b7sic", "/", "und", "al\u00b7le", "Na\u00b7sa\u00b7mo\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}