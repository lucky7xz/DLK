{"textgrid.poem.67759": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Mittelstra\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich, G\u00f6ttin wahrer Freuden,", "tokens": ["Dich", ",", "G\u00f6t\u00b7tin", "wah\u00b7rer", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bild der Vollkommenheit,", "tokens": ["Bild", "der", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das alle Thoren meiden,", "tokens": ["Das", "al\u00b7le", "Tho\u00b7ren", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Pomp und L\u00e4rm zerstreut,", "tokens": ["Die", "Pomp", "und", "L\u00e4rm", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dich sing' ich. Stimme selbst die Saiten,", "tokens": ["Dich", "sing'", "ich", ".", "Stim\u00b7me", "selbst", "die", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df meine Hand und Ton", "tokens": ["La\u00df", "mei\u00b7ne", "Hand", "und", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nicht ab- und aufw\u00e4rts gleiten!", "tokens": ["Nicht", "ab", "und", "auf\u00b7w\u00e4rts", "glei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "TRUNC", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}