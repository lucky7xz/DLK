{"textgrid.poem.50343": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "59. Auf Menalcas", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ob gleich manch Gem\u00e4hld hierinnen ihm behaget;", "tokens": ["Und", "ob", "gleich", "manch", "Ge\u00b7m\u00e4hld", "hie\u00b7rin\u00b7nen", "ihm", "be\u00b7ha\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIAT", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch sch\u00fcttelt er den Kopf, und saget:", "tokens": ["Doch", "sch\u00fct\u00b7telt", "er", "den", "Kopf", ",", "und", "sa\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dass ich ein Bild zwar ", "tokens": ["Dass", "ich", "ein", "Bild", "zwar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dass in der Einbildung ich manche Wartzen schaffe,", "tokens": ["Dass", "in", "der", "Ein\u00b7bil\u00b7dung", "ich", "man\u00b7che", "Wart\u00b7zen", "schaf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Und die auf frembde Wangen streich;", "tokens": ["Und", "die", "auf", "fremb\u00b7de", "Wan\u00b7gen", "streich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wollen, ich mahl' ihn selbst itzt ", "tokens": ["Wol\u00b7len", ",", "ich", "mahl'", "ihn", "selbst", "itzt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Hier stehts, Menalcas ", "tokens": ["Hier", "stehts", ",", "Me\u00b7nal\u00b7cas"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}