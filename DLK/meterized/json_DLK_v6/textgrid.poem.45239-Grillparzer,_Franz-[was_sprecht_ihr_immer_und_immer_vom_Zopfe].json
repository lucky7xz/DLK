{"textgrid.poem.45239": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[was sprecht ihr immer und immer vom Zopfe]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sprecht ihr immer und immer vom Zopfe,", "tokens": ["Was", "sprecht", "ihr", "im\u00b7mer", "und", "im\u00b7mer", "vom", "Zop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Meint ihr, ihr w\u00e4rt zopflos aus eigener Wahl?", "tokens": ["Meint", "ihr", ",", "ihr", "w\u00e4rt", "zopf\u00b7los", "aus", "ei\u00b7ge\u00b7ner", "Wahl", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Man braucht dazu wenigstens Haar auf dem Kopfe,", "tokens": ["Man", "braucht", "da\u00b7zu", "we\u00b7nigs\u00b7tens", "Haar", "auf", "dem", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PAV", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr aber, Erb\u00e4rmliche, ihr seid kahl.", "tokens": ["Ihr", "a\u00b7ber", ",", "Er\u00b7b\u00e4rm\u00b7li\u00b7che", ",", "ihr", "seid", "kahl", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}