{"textgrid.poem.24940": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Am Meere", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun nimm mich wieder an deine Brust,", "tokens": ["Nun", "nimm", "mich", "wie\u00b7der", "an", "dei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mein altes, geliebtes Meer!", "tokens": ["Mein", "al\u00b7tes", ",", "ge\u00b7lieb\u00b7tes", "Meer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch rollst du in Mut und Jugendluft,", "tokens": ["Noch", "rollst", "du", "in", "Mut", "und", "Ju\u00b7gen\u00b7dluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie da ich dich lie\u00df, einher.", "tokens": ["Wie", "da", "ich", "dich", "lie\u00df", ",", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "KOUS", "PPER", "PRF", "VVFIN", "$,", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Mir t\u00f6nt's aus der brandenden Wogen Schwall", "tokens": ["Mir", "t\u00f6nt's", "aus", "der", "bran\u00b7den\u00b7den", "Wo\u00b7gen", "Schwall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Entgegen wie Freundeslaut;", "tokens": ["Ent\u00b7ge\u00b7gen", "wie", "Freun\u00b7des\u00b7laut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als liebe Gespielen begr\u00fc\u00df' ich sie all,", "tokens": ["Als", "lie\u00b7be", "Ge\u00b7spie\u00b7len", "be\u00b7gr\u00fc\u00df'", "ich", "sie", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "PPER", "PPER", "PIAT", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die ich seit lang nicht geschaut.", "tokens": ["Die", "ich", "seit", "lang", "nicht", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich st\u00fcrze hinein in die sch\u00e4umende Flut;", "tokens": ["Ich", "st\u00fcr\u00b7ze", "hin\u00b7ein", "in", "die", "sch\u00e4u\u00b7men\u00b7de", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mir jubelt die Seele mit ihr:", "tokens": ["Mir", "ju\u00b7belt", "die", "See\u00b7le", "mit", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Den Knaben, der einst ihr am Busen geruht,", "tokens": ["Den", "Kna\u00b7ben", ",", "der", "einst", "ihr", "am", "Bu\u00b7sen", "ge\u00b7ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Erkennt sie freudig in mir.", "tokens": ["Er\u00b7kennt", "sie", "freu\u00b7dig", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und wie das Na\u00df, gegei\u00dfelt vom Nord,", "tokens": ["Und", "wie", "das", "Na\u00df", ",", "ge\u00b7gei\u00b7\u00dfelt", "vom", "Nord", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Brust und die Stirne mir k\u00fchlt,", "tokens": ["Die", "Brust", "und", "die", "Stir\u00b7ne", "mir", "k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "F\u00fchl' ich mir leise vom Herzen fort", "tokens": ["F\u00fchl'", "ich", "mir", "lei\u00b7se", "vom", "Her\u00b7zen", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "ADJD", "APPRART", "NN", "PTKVZ"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Den Rost des Lebens gesp\u00fclt.", "tokens": ["Den", "Rost", "des", "Le\u00b7bens", "ge\u00b7sp\u00fclt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Die Wangen umkost mir der wirbelnde Schaum,", "tokens": ["Die", "Wan\u00b7gen", "um\u00b7kost", "mir", "der", "wir\u00b7beln\u00b7de", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es lacht ihn hinweg mein Mund;", "tokens": ["Es", "lacht", "ihn", "hin\u00b7weg", "mein", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bald schaukelt die Welle mich hoch auf dem Saum,", "tokens": ["Bald", "schau\u00b7kelt", "die", "Wel\u00b7le", "mich", "hoch", "auf", "dem", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bald tauch' ich hinab in den Schlund.", "tokens": ["Bald", "tauch'", "ich", "hin\u00b7ab", "in", "den", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Hinaus! Ins Allunendliche hin!", "tokens": ["Hin\u00b7aus", "!", "Ins", "Al\u00b7lu\u00b7nend\u00b7li\u00b7che", "hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Das mi\u00dft' ich so manches Jahr.", "tokens": ["Das", "mi\u00dft'", "ich", "so", "man\u00b7ches", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ja, altes geliebtes Meer, noch bin", "tokens": ["Ja", ",", "al\u00b7tes", "ge\u00b7lieb\u00b7tes", "Meer", ",", "noch", "bin"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ADJA", "ADJA", "NN", "$,", "ADV", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich derselbe, der einst ich war.", "tokens": ["Ich", "der\u00b7sel\u00b7be", ",", "der", "einst", "ich", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "$,", "PRELS", "ADV", "PPER", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}