{"textgrid.poem.28676": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Mutter war der Hunger; seit sie mich au\u00df sich geboren,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Mutter war der Hunger; seit sie mich au\u00df sich geboren,", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "war", "der", "Hun\u00b7ger", ";", "seit", "sie", "mich", "au\u00df", "sich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$.", "KOUS", "PPER", "PRF", "APPR", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hat sie sich bey keinem Tage noch zur Zeit au\u00df mir verloren.", "tokens": ["Hat", "sie", "sich", "bey", "kei\u00b7nem", "Ta\u00b7ge", "noch", "zur", "Zeit", "au\u00df", "mir", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "ADV", "APPRART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Zwar zehn Monat trug sie mich, und zehn Jahre trag ich sie;", "tokens": ["Zwar", "zehn", "Mo\u00b7nat", "trug", "sie", "mich", ",", "und", "zehn", "Jah\u00b7re", "trag", "ich", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "PPER", "PPER", "$,", "KON", "CARD", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Keines hat f\u00fcr diese Last andrem noch gedancket ie.", "tokens": ["Kei\u00b7nes", "hat", "f\u00fcr", "die\u00b7se", "Last", "an\u00b7drem", "noch", "ge\u00b7dan\u00b7cket", "ie."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PIS", "VAFIN", "APPR", "PDAT", "NN", "PIS", "ADV", "VVFIN", "NE"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Ich war klein, da sie mich truge; sie ist m\u00e4chtig gro\u00df zu tragen;", "tokens": ["Ich", "war", "klein", ",", "da", "sie", "mich", "tru\u00b7ge", ";", "sie", "ist", "m\u00e4ch\u00b7tig", "gro\u00df", "zu", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Drum entstunden ihr gar kleine, mir gar grosse Kindes-Plagen.", "tokens": ["Drum", "ent\u00b7stun\u00b7den", "ihr", "gar", "klei\u00b7ne", ",", "mir", "gar", "gros\u00b7se", "Kin\u00b7des\u00b7Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJA", "$,", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Ich auch f\u00fchle fort und fort grosse Schmertzen, grosses Weh;", "tokens": ["Ich", "auch", "f\u00fch\u00b7le", "fort", "und", "fort", "gros\u00b7se", "Schmert\u00b7zen", ",", "gros\u00b7ses", "Weh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "KON", "PTKVZ", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Doch vermerck ich, da\u00df so bald sie von mir nicht weg noch geh.", "tokens": ["Doch", "ver\u00b7merck", "ich", ",", "da\u00df", "so", "bald", "sie", "von", "mir", "nicht", "weg", "noch", "geh."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVIMP", "PPER", "$,", "KOUS", "ADV", "ADV", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "ADV", "NN"], "meter": "+-+-+-+-+-++-", "measure": "unknown.measure.septa"}}}}}