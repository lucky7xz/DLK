{"textgrid.poem.43902": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Buch, das eure Feder kennt,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Buch, das eure Feder kennt,", "tokens": ["Mein", "Buch", ",", "das", "eu\u00b7re", "Fe\u00b7der", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Zimmer, das nun w\u00fcste stehet,", "tokens": ["Mein", "Zim\u00b7mer", ",", "das", "nun", "w\u00fcs\u00b7te", "ste\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herd, der jezund einsam brennt,", "tokens": ["Der", "Herd", ",", "der", "je\u00b7zund", "ein\u00b7sam", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stra\u00dfe, so nach Striegau gehet,", "tokens": ["Die", "Stra\u00b7\u00dfe", ",", "so", "nach", "Strie\u00b7gau", "ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Abend, so den Freund erstach,", "tokens": ["Der", "A\u00b7bend", ",", "so", "den", "Freund", "er\u00b7stach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df mir das Herze zehnmahl brach \u2013", "tokens": ["Da\u00df", "mir", "das", "Her\u00b7ze", "zehn\u00b7mahl", "brach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies alles, sag ich, sind vorwahr", "tokens": ["Dies", "al\u00b7les", ",", "sag", "ich", ",", "sind", "vor\u00b7wahr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PIS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Friedensst\u00f6rer meiner Sinnen.", "tokens": ["Die", "Frie\u00b7dens\u00b7st\u00f6\u00b7rer", "mei\u00b7ner", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach d\u00fcrft ich, sprech ich, noch ein Jahr", "tokens": ["Ach", "d\u00fcrft", "ich", ",", "sprech", "ich", ",", "noch", "ein", "Jahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von der verflo\u00dfnen Zeit gewinnen,", "tokens": ["Von", "der", "ver\u00b7flo\u00df\u00b7nen", "Zeit", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So h\u00f6re meines Lebens Lauf,", "tokens": ["So", "h\u00f6\u00b7re", "mei\u00b7nes", "Le\u00b7bens", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn es verflo\u00dfen, willig auf.", "tokens": ["Wenn", "es", "ver\u00b7flo\u00b7\u00dfen", ",", "wil\u00b7lig", "auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, Kinder, ach, ich kan nicht mehr", "tokens": ["Ach", ",", "Kin\u00b7der", ",", "ach", ",", "ich", "kan", "nicht", "mehr"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$,", "ITJ", "$,", "PPER", "VMFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Finger vor Betr\u00fcbn\u00fc\u00df r\u00fchren.", "tokens": ["Die", "Fin\u00b7ger", "vor", "Be\u00b7tr\u00fcb\u00b7n\u00fc\u00df", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gebt meiner Bitte noch Geh\u00f6r:", "tokens": ["Gebt", "mei\u00b7ner", "Bit\u00b7te", "noch", "Ge\u00b7h\u00f6r", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Last mich die alte R\u00fchrung sp\u00fcren,", "tokens": ["Last", "mich", "die", "al\u00b7te", "R\u00fch\u00b7rung", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ver\u00e4ndert nicht die reine Treu", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "nicht", "die", "rei\u00b7ne", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und steht mir bey dem H\u00f6chsten bey!", "tokens": ["Und", "steht", "mir", "bey", "dem", "H\u00f6chs\u00b7ten", "bey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}