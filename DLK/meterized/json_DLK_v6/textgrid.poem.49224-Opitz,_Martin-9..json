{"textgrid.poem.49224": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schwarze Nacht, die du die Welt umfangen", "tokens": ["Du", "schwar\u00b7ze", "Nacht", ",", "die", "du", "die", "Welt", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast \u00fcberall mit Furcht und Tunkelheit,", "tokens": ["Hast", "\u00fc\u00b7be\u00b7rall", "mit", "Furcht", "und", "Tun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sch\u00e4mst du dich nicht, wann meiner Liebsten Wangen", "tokens": ["Sch\u00e4mst", "du", "dich", "nicht", ",", "wann", "mei\u00b7ner", "Liebs\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich lassen sehn mit ihrer Zierlichkeit?", "tokens": ["Sich", "las\u00b7sen", "sehn", "mit", "ih\u00b7rer", "Zier\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr Sternen auch, d\u00f6rft ihr von oben schauen", "tokens": ["Ihr", "Ster\u00b7nen", "auch", ",", "d\u00f6rft", "ihr", "von", "o\u00b7ben", "schau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$,", "VVFIN", "PPER", "APPR", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und in der Luft so ganz stehn unverwendt,", "tokens": ["Und", "in", "der", "Luft", "so", "ganz", "stehn", "un\u00b7ver\u00b7wendt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wann ihr das Licht der sch\u00f6nesten Jungfrauen,", "tokens": ["Wann", "ihr", "das", "Licht", "der", "sch\u00f6\u00b7nes\u00b7ten", "Jung\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So bi\u00df zu euch gen Himmel reicht, erkennt?", "tokens": ["So", "bi\u00df", "zu", "euch", "gen", "Him\u00b7mel", "reicht", ",", "er\u00b7kennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie m\u00f6get ihr nicht also bald verbleichen,", "tokens": ["Wie", "m\u00f6\u00b7get", "ihr", "nicht", "al\u00b7so", "bald", "ver\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wann sich der Glanz le\u00dft sehn mit solcher Pracht?", "tokens": ["Wann", "sich", "der", "Glanz", "le\u00dft", "sehn", "mit", "sol\u00b7cher", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Aurora selbst, die pfleget ihr zu weichen,", "tokens": ["Au\u00b7ro\u00b7ra", "selbst", ",", "die", "pfle\u00b7get", "ihr", "zu", "wei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PRELS", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df sie f\u00fcr ihr auch schamroth wird gemacht.", "tokens": ["Da\u00df", "sie", "f\u00fcr", "ihr", "auch", "scham\u00b7roth", "wird", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}