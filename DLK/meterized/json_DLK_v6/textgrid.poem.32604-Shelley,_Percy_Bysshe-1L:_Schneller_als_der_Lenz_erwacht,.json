{"textgrid.poem.32604": {"metadata": {"author": {"name": "Shelley, Percy Bysshe", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schneller als der Lenz erwacht,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schneller als der Lenz erwacht,", "tokens": ["Schnel\u00b7ler", "als", "der", "Lenz", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schneller als der Jugend Pracht,", "tokens": ["Schnel\u00b7ler", "als", "der", "Ju\u00b7gend", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schneller als die sel'ge Nacht,", "tokens": ["Schnel\u00b7ler", "als", "die", "sel'\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kamst und flohst du mich.", "tokens": ["Kamst", "und", "flohst", "du", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wie im Herbst der Erde Schoo\u00df,", "tokens": ["Wie", "im", "Herbst", "der", "Er\u00b7de", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie die Nacht, die schlummerlos,", "tokens": ["Wie", "die", "Nacht", ",", "die", "schlum\u00b7mer\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie das Herz, der Freude blo\u00df,", "tokens": ["Wie", "das", "Herz", ",", "der", "Freu\u00b7de", "blo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bin verlassen ich.", "tokens": ["Bin", "ver\u00b7las\u00b7sen", "ich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die Schwalbe Lenz wird wieder nahn,", "tokens": ["Die", "Schwal\u00b7be", "Lenz", "wird", "wie\u00b7der", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Eule Nacht kommt auch heran,", "tokens": ["Die", "Eu\u00b7le", "Nacht", "kommt", "auch", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch der Jugend wilder Schwan", "tokens": ["Doch", "der", "Ju\u00b7gend", "wil\u00b7der", "Schwan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Floh mit dir, an Trug dir gleich.", "tokens": ["Floh", "mit", "dir", ",", "an", "Trug", "dir", "gleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "APPR", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bang ersehn' ich stets den Morgen,", "tokens": ["Bang", "er\u00b7sehn'", "ich", "stets", "den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Selbst der Schlaf zerrinnt in Sorgen,", "tokens": ["Selbst", "der", "Schlaf", "zer\u00b7rinnt", "in", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, vergebens m\u00f6cht' ich borgen", "tokens": ["Ach", ",", "ver\u00b7ge\u00b7bens", "m\u00f6cht'", "ich", "bor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sonnig Laub von jedem Zweig.", "tokens": ["Son\u00b7nig", "Laub", "von", "je\u00b7dem", "Zweig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liljen sei'n der Braut geweiht,", "tokens": ["Lil\u00b7jen", "sei'n", "der", "Braut", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rosen habt der Frau bereit,", "tokens": ["Ro\u00b7sen", "habt", "der", "Frau", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Veilchen f\u00fcr die todte Maid,", "tokens": ["Veil\u00b7chen", "f\u00fcr", "die", "tod\u00b7te", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Vergi\u00dfmeinnicht will ich.", "tokens": ["Und", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "will", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zollt sie ohne Thr\u00e4n' und Klage", "tokens": ["Zollt", "sie", "oh\u00b7ne", "Thr\u00e4n'", "und", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Lebens Sarkophage,", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "Sar\u00b7ko\u00b7pha\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in Furcht und Hoffnung schlage", "tokens": ["Und", "in", "Furcht", "und", "Hoff\u00b7nung", "schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Keines Freundes Herz f\u00fcr mich.", "tokens": ["Kei\u00b7nes", "Freun\u00b7des", "Herz", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}