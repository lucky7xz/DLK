{"textgrid.poem.62576": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Vor Drey St\u00fccke, so mein Sch\u00f6pfer mir so liebreich zugedacht", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Wird ihm auch mit allem Fleise, Ehre, Lob und Dank gebracht.", "tokens": ["Wird", "ihm", "auch", "mit", "al\u00b7lem", "Flei\u00b7se", ",", "Eh\u00b7re", ",", "Lob", "und", "Dank", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PIS", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df ich wei\u00df, was Geist und Sinn mir zum Heyl und besten denket,", "tokens": ["Da\u00df", "ich", "wei\u00df", ",", "was", "Geist", "und", "Sinn", "mir", "zum", "Heyl", "und", "bes\u00b7ten", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "NN", "KON", "NN", "PPER", "APPRART", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und ich meine Leibes-Glieder froh und n\u00fctzlich brauchen kan.", "tokens": ["Und", "ich", "mei\u00b7ne", "Lei\u00b7bes\u00b7Glie\u00b7der", "froh", "und", "n\u00fctz\u00b7lich", "brau\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Zu der wahren Kirche bracht, da\u00df ich kan die Biebel lesen,", "tokens": ["Zu", "der", "wah\u00b7ren", "Kir\u00b7che", "bracht", ",", "da\u00df", "ich", "kan", "die", "Bie\u00b7bel", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Denn dadurch kan ich von Schmerz, und von Angst und Noth genesen,", "tokens": ["Denn", "da\u00b7durch", "kan", "ich", "von", "Schmerz", ",", "und", "von", "Angst", "und", "Noth", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "APPR", "NN", "$,", "KON", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und mein Herz auch fr\u00f6lich mache, wenn mich Kreuz und Tr\u00fcbsal trit.", "tokens": ["Und", "mein", "Herz", "auch", "fr\u00f6\u00b7lich", "ma\u00b7che", ",", "wenn", "mich", "Kreuz", "und", "Tr\u00fcb\u00b7sal", "trit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Nach dem sanften Grabe thu/ und da\u00df meinem Lauf und Leben", "tokens": ["Nach", "dem", "sanf\u00b7ten", "Gra\u00b7be", "thu", "/", "und", "da\u00df", "mei\u00b7nem", "Lauf", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$(", "KON", "KOUS", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Ziel und Stunde, Zeit und Ma\u00df ist gesetzet und gegeben.", "tokens": ["Ziel", "und", "Stun\u00b7de", ",", "Zeit", "und", "Ma\u00df", "ist", "ge\u00b7set\u00b7zet", "und", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}