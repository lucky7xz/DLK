{"textgrid.poem.48017": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hab mich lange Zeit gestr\u00e4ubt", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab mich lange Zeit gestr\u00e4ubt", "tokens": ["Ich", "hab", "mich", "lan\u00b7ge", "Zeit", "ge\u00b7str\u00e4ubt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und wollt es nie und nie verstehen,", "tokens": ["und", "wollt", "es", "nie", "und", "nie", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "was ich im Leuchten deines Blicks,", "tokens": ["was", "ich", "im", "Leuch\u00b7ten", "dei\u00b7nes", "Blicks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Zucken deines Munds gesehen.", "tokens": ["im", "Zu\u00b7cken", "dei\u00b7nes", "Munds", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es war in tiefer Dunkelheit", "tokens": ["Es", "war", "in", "tie\u00b7fer", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein Irrlicht, welches mich geblendet;", "tokens": ["ein", "Irr\u00b7licht", ",", "wel\u00b7ches", "mich", "ge\u00b7blen\u00b7det", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und da\u00df der Pfad, auf den es lockt,", "tokens": ["und", "da\u00df", "der", "Pfad", ",", "auf", "den", "es", "lockt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in Nacht und Not und Grauen endet \u2013", "tokens": ["in", "Nacht", "und", "Not", "und", "Grau\u00b7en", "en\u00b7det", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df es l\u00e4ngst: \u2013 In bleicher Angst", "tokens": ["Ich", "wei\u00df", "es", "l\u00e4ngst", ":", "\u2013", "In", "blei\u00b7cher", "Angst"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "halt ich die Augen fest geschlossen,", "tokens": ["halt", "ich", "die", "Au\u00b7gen", "fest", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und dennoch f\u00fchl ich Fieberglut", "tokens": ["und", "den\u00b7noch", "f\u00fchl", "ich", "Fie\u00b7berg\u00b7lut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "durch alle Adern mir ergossen \u2013", "tokens": ["durch", "al\u00b7le", "A\u00b7dern", "mir", "er\u00b7gos\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und dennoch pocht bei deinem Blick", "tokens": ["Und", "den\u00b7noch", "pocht", "bei", "dei\u00b7nem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mein Herz in immer hei\u00dfern Schl\u00e4gen:", "tokens": ["mein", "Herz", "in", "im\u00b7mer", "hei\u00b7\u00dfern", "Schl\u00e4\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "magnetisch zieht das Zauberlicht", "tokens": ["mag\u00b7ne\u00b7tisch", "zieht", "das", "Zau\u00b7ber\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "den Tiefen mich der Schuld entgegen.", "tokens": ["den", "Tie\u00b7fen", "mich", "der", "Schuld", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}