{"textgrid.poem.39503": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht blo\u00df die Blume welkt, das Duftgewebe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht blo\u00df die Blume welkt, das Duftgewebe", "tokens": ["Nicht", "blo\u00df", "die", "Blu\u00b7me", "welkt", ",", "das", "Duft\u00b7ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Fr\u00fche rei\u00dft, entflieht des Lenzes Prangen;", "tokens": ["Der", "Fr\u00fc\u00b7he", "rei\u00dft", ",", "ent\u00b7flieht", "des", "Len\u00b7zes", "Pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht blo\u00df erbleichen junge Rosenwangen;", "tokens": ["Nicht", "blo\u00df", "er\u00b7blei\u00b7chen", "jun\u00b7ge", "Ro\u00b7sen\u00b7wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Geist auch droht's, da\u00df er sich \u00fcberlebe.", "tokens": ["Dem", "Geist", "auch", "droht's", ",", "da\u00df", "er", "sich", "\u00fc\u00b7berl\u00b7e\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NE", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie k\u00fchn er erst auf freien Fl\u00fcgeln schwebe,", "tokens": ["Wie", "k\u00fchn", "er", "erst", "auf", "frei\u00b7en", "Fl\u00fc\u00b7geln", "schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dumpf gn\u00fcgsam bleibt er bald am Boden hangen.", "tokens": ["Dumpf", "gn\u00fcg\u00b7sam", "bleibt", "er", "bald", "am", "Bo\u00b7den", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O wi\u00dft ihr, f\u00fcr sein gr\u00e4nzenlos Verlangen,", "tokens": ["O", "wi\u00dft", "ihr", ",", "f\u00fcr", "sein", "gr\u00e4n\u00b7zen\u00b7los", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weis' oder Dichter, keinen Trank der Hebe?", "tokens": ["Weis'", "o\u00b7der", "Dich\u00b7ter", ",", "kei\u00b7nen", "Trank", "der", "He\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "PIAT", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Nichts w\u00e4hn' er sein; Besitzthum ist ihm Schranke;", "tokens": ["Nichts", "w\u00e4hn'", "er", "sein", ";", "Be\u00b7sitz\u00b7thum", "ist", "ihm", "Schran\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VAINF", "$.", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruh' Tod; ein ew'ger Kampf der Freiheit Wesen.", "tokens": ["Ruh'", "Tod", ";", "ein", "ew'\u00b7ger", "Kampf", "der", "Frei\u00b7heit", "We\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es k\u00fcmmr' ihn nie, was hinter ihm versunken.", "tokens": ["Es", "k\u00fcmm\u00b7r'", "ihn", "nie", ",", "was", "hin\u00b7ter", "ihm", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PRELS", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Vernichtend, schaffend, wechsle der Gedanke.", "tokens": ["Ver\u00b7nich\u00b7tend", ",", "schaf\u00b7fend", ",", "wechs\u00b7le", "der", "Ge\u00b7dan\u00b7ke", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Reinste sei zum Flammengrab erlesen,", "tokens": ["Das", "Reins\u00b7te", "sei", "zum", "Flam\u00b7men\u00b7grab", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo ihn, verj\u00fcngend, treffe Gottes Funken.", "tokens": ["Wo", "ihn", ",", "ver\u00b7j\u00fcn\u00b7gend", ",", "tref\u00b7fe", "Got\u00b7tes", "Fun\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVPP", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}