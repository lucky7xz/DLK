{"dta.poem.6704": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "De la Vue.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jemehr wir den Gebrauch der Sinnen nun erwegen,", "tokens": ["Je\u00b7mehr", "wir", "den", "Ge\u00b7brauch", "der", "Sin\u00b7nen", "nun", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit desto gr\u00f6ssrer Klarheit", "tokens": ["Mit", "des\u00b7to", "gr\u00f6ss\u00b7rer", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zeigt der Verstand uns diese Wahrheit,", "tokens": ["Zeigt", "der", "Ver\u00b7stand", "uns", "die\u00b7se", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df die Empfindung blo\u00df dem Geist sey beyzulegen.", "tokens": ["Da\u00df", "die", "Emp\u00b7fin\u00b7dung", "blo\u00df", "dem", "Geist", "sey", "bey\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch besser, als die T\u00f6n\u2019, erweisen des Gesichts", "tokens": ["Noch", "bes\u00b7ser", ",", "als", "die", "T\u00f6n'", ",", "er\u00b7wei\u00b7sen", "des", "Ge\u00b7sichts"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verschiedne W\u00fcrckungen, sammt vieler Vorw\u00fcrff\u2019 Bildern,", "tokens": ["Ver\u00b7schied\u00b7ne", "W\u00fcr\u00b7ckun\u00b7gen", ",", "sammt", "vie\u00b7ler", "Vor\u00b7w\u00fcrff'", "Bil\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die wir uns selber schildern,", "tokens": ["Die", "wir", "uns", "sel\u00b7ber", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und machen es recht \u00fcberzeuglich klar.", "tokens": ["Und", "ma\u00b7chen", "es", "recht", "\u00fc\u00b7berz\u00b7eug\u00b7lich", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Rede, das Geh\u00f6r, hat einen Vortheil zwar,", "tokens": ["Die", "Re\u00b7de", ",", "das", "Ge\u00b7h\u00f6r", ",", "hat", "ei\u00b7nen", "Vor\u00b7theil", "zwar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es kan ein jedes Ding sich durch die Sprach erkl\u00e4ren,", "tokens": ["Es", "kan", "ein", "je\u00b7des", "Ding", "sich", "durch", "die", "Sprach", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "PIAT", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es zeigen sich dem Geist die Nutz-erf\u00fcllte Lehren.", "tokens": ["Es", "zei\u00b7gen", "sich", "dem", "Geist", "die", "Nutz\u00b7er\u00b7f\u00fcll\u00b7te", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Allein das grosse Rund dr\u00fcckt uns den Wunder-Schein", "tokens": ["Al\u00b7lein", "das", "gros\u00b7se", "Rund", "dr\u00fcckt", "uns", "den", "Wun\u00b7der\u00b7Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und sein so herrlich Bild, ohn alle Red-Kunst ein.", "tokens": ["Und", "sein", "so", "herr\u00b7lich", "Bild", ",", "ohn", "al\u00b7le", "Re\u00b7dKunst", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "ADJD", "NN", "$,", "KOUI", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So bald sich nur ein Aug\u2019 er\u00f6ffnen kan,", "tokens": ["So", "bald", "sich", "nur", "ein", "Aug'", "er\u00b7\u00f6ff\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Sieht man bereits den Glantz, den nichts verdunckelt, an.", "tokens": ["Sieht", "man", "be\u00b7reits", "den", "Glantz", ",", "den", "nichts", "ver\u00b7dun\u00b7ckelt", ",", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}