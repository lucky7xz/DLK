{"textgrid.poem.66967": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Orchis poetica", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist's, so war's, so bleibt's zu allen Zeiten:", "tokens": ["So", "ist's", ",", "so", "wa\u00b7r's", ",", "so", "bleibt's", "zu", "al\u00b7len", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ADV", "VAFIN", "$,", "ADV", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein echter Dichter mag den Schlamm durchschreiten,", "tokens": ["Ein", "ech\u00b7ter", "Dich\u00b7ter", "mag", "den", "Schlamm", "durch\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es wird, wohin sein Fu\u00df getreten eben,", "tokens": ["Es", "wird", ",", "wo\u00b7hin", "sein", "Fu\u00df", "ge\u00b7tre\u00b7ten", "e\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich eine Orchis aus dem Sumpf erheben.", "tokens": ["Sich", "ei\u00b7ne", "Or\u00b7chis", "aus", "dem", "Sumpf", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}