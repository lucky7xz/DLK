{"textgrid.poem.25495": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebe, die an Sch\u00f6nheit sich entz\u00fcndet,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe, die an Sch\u00f6nheit sich entz\u00fcndet,", "tokens": ["Lie\u00b7be", ",", "die", "an", "Sch\u00f6n\u00b7heit", "sich", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gleicht Konfect, der im Genusse schwindet,", "tokens": ["Gleicht", "Kon\u00b7fect", ",", "der", "im", "Ge\u00b7nus\u00b7se", "schwin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und zu bald nur Ueberdru\u00df erweckt;", "tokens": ["Und", "zu", "bald", "nur", "Ue\u00b7berd\u00b7ru\u00df", "er\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Freundschaft, die auf innerm Werth' sich gr\u00fcndet", "tokens": ["Freund\u00b7schaft", ",", "die", "auf", "in\u00b7nerm", "Wert\u00b7h'", "sich", "gr\u00fcn\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN", "PRF", "VVFIN"], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und zwei Herzen voll Gef\u00fchl verbindet,", "tokens": ["Und", "zwei", "Her\u00b7zen", "voll", "Ge\u00b7f\u00fchl", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ist gesundes Brod, das t\u00e4glich schmeckt.", "tokens": ["Ist", "ge\u00b7sun\u00b7des", "Brod", ",", "das", "t\u00e4g\u00b7lich", "schmeckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Jene Lieb' ist ein geschm\u00fcckter Nachen,", "tokens": ["Je\u00b7ne", "Lieb'", "ist", "ein", "ge\u00b7schm\u00fcck\u00b7ter", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Eine Lustfahrt auf der Spree zu machen,", "tokens": ["Ei\u00b7ne", "Lust\u00b7fahrt", "auf", "der", "Spree", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wenn im Lenz' sich kaum ein L\u00fcftchen regt;", "tokens": ["Wenn", "im", "Lenz'", "sich", "kaum", "ein", "L\u00fcft\u00b7chen", "regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Jene Freundschaft ist ein Schiff mit Masten,", "tokens": ["Je\u00b7ne", "Freund\u00b7schaft", "ist", "ein", "Schiff", "mit", "Mas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "H\u00e4lt die offne See, und tr\u00e4get Lasten,", "tokens": ["H\u00e4lt", "die", "off\u00b7ne", "See", ",", "und", "tr\u00e4\u00b7get", "Las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Wird in Donnerwettern zwar bewegt,", "tokens": ["Wird", "in", "Don\u00b7ner\u00b7wet\u00b7tern", "zwar", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Doch nicht sinken, und im Kampf' nicht fliehen,", "tokens": ["Doch", "nicht", "sin\u00b7ken", ",", "und", "im", "Kampf'", "nicht", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "$,", "KON", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Sondern muthig Feuer um sich spr\u00fchen,", "tokens": ["Son\u00b7dern", "mut\u00b7hig", "Feu\u00b7er", "um", "sich", "spr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Bis herab der Tod die Flagge schl\u00e4gt.", "tokens": ["Bis", "her\u00b7ab", "der", "Tod", "die", "Flag\u00b7ge", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Wage du mit meinem Schiff' die Reise,", "tokens": ["Wa\u00b7ge", "du", "mit", "mei\u00b7nem", "Schiff'", "die", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Denn es machten seinen Steuermann", "tokens": ["Denn", "es", "mach\u00b7ten", "sei\u00b7nen", "Steu\u00b7er\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Jahre, Schicksal und Erfahrung weise,", "tokens": ["Jah\u00b7re", ",", "Schick\u00b7sal", "und", "Er\u00b7fah\u00b7rung", "wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Da\u00df er sicherer dich f\u00fchren kann", "tokens": ["Da\u00df", "er", "si\u00b7che\u00b7rer", "dich", "f\u00fch\u00b7ren", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PRF", "VVINF", "VMFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Auf dem Meer' voll Klippen und voll B\u00e4nke,", "tokens": ["Auf", "dem", "Meer'", "voll", "Klip\u00b7pen", "und", "voll", "B\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Wenn auch selbst kein Stern am Himmel glimmt.", "tokens": ["Wenn", "auch", "selbst", "kein", "Stern", "am", "Him\u00b7mel", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Da\u00df ich gern f\u00fcr dich das Steuer lenke,", "tokens": ["Da\u00df", "ich", "gern", "f\u00fcr", "dich", "das", "Steu\u00b7er", "len\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Bis der Tod mir aus der Hand es nimmt,", "tokens": ["Bis", "der", "Tod", "mir", "aus", "der", "Hand", "es", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Und nach so viel St\u00fcrmen und Gefahren", "tokens": ["Und", "nach", "so", "viel", "St\u00fcr\u00b7men", "und", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "D\u00e4mmern wird Kozytus finstrer Strand:", "tokens": ["D\u00e4m\u00b7mern", "wird", "Ko\u00b7zy\u00b7tus", "finst\u00b7rer", "Strand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Dieses wei\u00dft du. Dort, nach wenig Jahren,", "tokens": ["Die\u00b7ses", "wei\u00dft", "du", ".", "Dort", ",", "nach", "we\u00b7nig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Tret' ich sicherlich zuerst ans Land;", "tokens": ["Tret'", "ich", "si\u00b7cher\u00b7lich", "zu\u00b7erst", "ans", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Lerne selbst denn fr\u00fch das Meer befahren,", "tokens": ["Ler\u00b7ne", "selbst", "denn", "fr\u00fch", "das", "Meer", "be\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Dieses Meer voll Raub und Unbestand.", "tokens": ["Die\u00b7ses", "Meer", "voll", "Raub", "und", "Un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}