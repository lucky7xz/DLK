{"textgrid.poem.55327": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Was es gilt", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bringst du die Natur heran,", "tokens": ["Bringst", "du", "die", "Na\u00b7tur", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie jeder nutzen kann;", "tokens": ["Da\u00df", "sie", "je\u00b7der", "nut\u00b7zen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Falsches hast du nicht ersonnen,", "tokens": ["Fal\u00b7sches", "hast", "du", "nicht", "er\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast der Menschen Gunst gewonnen.", "tokens": ["Hast", "der", "Men\u00b7schen", "Gunst", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00f6get ihr das Licht zerst\u00fcckeln,", "tokens": ["M\u00f6\u00b7get", "ihr", "das", "Licht", "zer\u00b7st\u00fc\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Farb um Farbe draus entwickeln", "tokens": ["Farb", "um", "Far\u00b7be", "draus", "ent\u00b7wi\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "PAV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder andre Schw\u00e4nke f\u00fchren,", "tokens": ["O\u00b7der", "and\u00b7re", "Schw\u00e4n\u00b7ke", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcgelchen polarisieren,", "tokens": ["K\u00fc\u00b7gel\u00b7chen", "po\u00b7la\u00b7ri\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df der H\u00f6rer ganz erschrocken", "tokens": ["Da\u00df", "der", "H\u00f6\u00b7rer", "ganz", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchlet Sinn und Sinne stocken:", "tokens": ["F\u00fch\u00b7let", "Sinn", "und", "Sin\u00b7ne", "sto\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein! Es soll euch nicht gelingen,", "tokens": ["Nein", "!", "Es", "soll", "euch", "nicht", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sollt uns nicht beiseite bringen;", "tokens": ["Sollt", "uns", "nicht", "bei\u00b7sei\u00b7te", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kr\u00e4ftig, wie wir's angefangen,", "tokens": ["Kr\u00e4f\u00b7tig", ",", "wie", "wir's", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wollen wir zum Ziel gelangen.", "tokens": ["Wol\u00b7len", "wir", "zum", "Ziel", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}