{"dta.poem.18784": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6n geschriebenen,", "tokens": ["Die", "sch\u00f6n", "ge\u00b7schrie\u00b7be\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Herrlich umg\u00fcldeten,", "tokens": ["Herr\u00b7lich", "um\u00b7g\u00fcl\u00b7de\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Bel\u00e4cheltest du", "tokens": ["Be\u00b7l\u00e4\u00b7chel\u00b7test", "du"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die anmasslichen Bl\u00e4tter,", "tokens": ["Die", "an\u00b7mass\u00b7li\u00b7chen", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Verziehst mein Prahlen", "tokens": ["Ver\u00b7ziehst", "mein", "Prah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Von deiner Lieb\u2019 und meinem", "tokens": ["Von", "dei\u00b7ner", "Lieb'", "und", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch dich gl\u00fccklichen Gelingen,", "tokens": ["Durch", "dich", "gl\u00fcck\u00b7li\u00b7chen", "Ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Verziehst anmuthigem Selbstlob.", "tokens": ["Ver\u00b7ziehst", "an\u00b7mut\u00b7hi\u00b7gem", "Selbst\u00b7lob", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Selbstlob! Nur dem Neide stinkt\u2019s,", "tokens": ["Selbst\u00b7lob", "!", "Nur", "dem", "Nei\u00b7de", "stinkt's", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohlgeruch Freunden", "tokens": ["Wohl\u00b7ge\u00b7ruch", "Freun\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und eignem Schmack!", "tokens": ["Und", "eig\u00b7nem", "Schmack", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Freude des Daseyns ist gross,", "tokens": ["Freu\u00b7de", "des", "Da\u00b7seyns", "ist", "gross", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00f6sser die Freud\u2019 am Daseyn.", "tokens": ["Gr\u00f6s\u00b7ser", "die", "Freud'", "am", "Da\u00b7seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wenn du Suleika", "tokens": ["Wenn", "du", "Sul\u00b7ei\u00b7ka"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mich \u00fcberschw\u00e4nglich begl\u00fcckst,", "tokens": ["Mich", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich", "be\u00b7gl\u00fcckst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Deine Leidenschaft mir zuwirfst", "tokens": ["Dei\u00b7ne", "Lei\u00b7den\u00b7schaft", "mir", "zu\u00b7wirfst"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Als w\u00e4r\u2019s ein Ball,", "tokens": ["Als", "w\u00e4r's", "ein", "Ball", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dass ich ihn fange,", "tokens": ["Dass", "ich", "ihn", "fan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Dir zur\u00fcckwerfe", "tokens": ["Dir", "zu\u00b7r\u00fcck\u00b7wer\u00b7fe"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Mein gewidmetes Ich;", "tokens": ["Mein", "ge\u00b7wid\u00b7me\u00b7tes", "Ich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Das ist ein Augenblick!", "tokens": ["Das", "ist", "ein", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und dann reisst mich von dir", "tokens": ["Und", "dann", "reisst", "mich", "von", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bald der Franke, bald der Armenier.", "tokens": ["Bald", "der", "Fran\u00b7ke", ",", "bald", "der", "Ar\u00b7me\u00b7nier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Aber Tage w\u00e4hrt\u2019s,", "tokens": ["A\u00b7ber", "Ta\u00b7ge", "w\u00e4hrt's", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Jahre dauert\u2019s, dass ich neu erschaffe", "tokens": ["Jah\u00b7re", "dau\u00b7ert's", ",", "dass", "ich", "neu", "er\u00b7schaf\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tausendf\u00e4ltig deiner Verschwendungen F\u00fclle", "tokens": ["Tau\u00b7send\u00b7f\u00e4l\u00b7tig", "dei\u00b7ner", "Ver\u00b7schwen\u00b7dun\u00b7gen", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Auftr\u00f6sle die bunte Schnur meines Gl\u00fccks,", "tokens": ["Auf\u00b7tr\u00f6s\u00b7le", "die", "bun\u00b7te", "Schnur", "mei\u00b7nes", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gekl\u00f6pplet tausendfadig", "tokens": ["Ge\u00b7kl\u00f6pp\u00b7let", "tau\u00b7send\u00b7fa\u00b7dig"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von dir, o Suleika.", "tokens": ["Von", "dir", ",", "o", "Sul\u00b7ei\u00b7ka", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hier nun dagegen", "tokens": ["Hier", "nun", "da\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "PAV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dichtrische Perlen,", "tokens": ["Dich\u00b7tri\u00b7sche", "Per\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die mir deiner Leidenschaft", "tokens": ["Die", "mir", "dei\u00b7ner", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gewaltige Brandung", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ge", "Bran\u00b7dung"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Warf an des Lebens", "tokens": ["Warf", "an", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ver\u00f6deten Strand aus.", "tokens": ["Ver\u00b7\u00f6\u00b7de\u00b7ten", "Strand", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mit spitzen Fingern", "tokens": ["Mit", "spit\u00b7zen", "Fin\u00b7gern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Zierlich gelesen,", "tokens": ["Zier\u00b7lich", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Durchreiht mit juwelenem", "tokens": ["Durch\u00b7reiht", "mit", "ju\u00b7we\u00b7le\u00b7nem"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Goldschmuck.", "tokens": ["Gold\u00b7schmuck", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Nimm sie an deinen Hals,", "tokens": ["Nimm", "sie", "an", "dei\u00b7nen", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "An deinen Busen!", "tokens": ["An", "dei\u00b7nen", "Bu\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Die Regentropfen Allahs,", "tokens": ["Die", "Re\u00b7gen\u00b7trop\u00b7fen", "Al\u00b7lahs", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Gereift in bescheidener Muschel.", "tokens": ["Ge\u00b7reift", "in", "be\u00b7schei\u00b7de\u00b7ner", "Mu\u00b7schel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}