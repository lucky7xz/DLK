{"dta.poem.21428": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mir schwanden die Sinne, und als ich aufschlug", "tokens": ["Mir", "schwan\u00b7den", "die", "Sin\u00b7ne", ",", "und", "als", "ich", "auf\u00b7schlug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "KOUS", "PPER", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Augen, sa\u00df ich an der Seite", "tokens": ["Die", "Au\u00b7gen", ",", "sa\u00df", "ich", "an", "der", "Sei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der G\u00f6ttin noch immer, es lehnte mein Haupt", "tokens": ["Der", "G\u00f6t\u00b7tin", "noch", "im\u00b7mer", ",", "es", "lehn\u00b7te", "mein", "Haupt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "An ihre Brust, die breite.", "tokens": ["An", "ih\u00b7re", "Brust", ",", "die", "brei\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es blitzte ihr Blick, es gl\u00fchte ihr Mund,", "tokens": ["Es", "blitz\u00b7te", "ihr", "Blick", ",", "es", "gl\u00fch\u00b7te", "ihr", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es zuckten die N\u00fcstern der Nase,", "tokens": ["Es", "zuck\u00b7ten", "die", "N\u00fcs\u00b7tern", "der", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bachantisch umschlang sie den Dichter und sang", "tokens": ["Ba\u00b7ch\u00b7an\u00b7tisch", "um\u00b7schlang", "sie", "den", "Dich\u00b7ter", "und", "sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Mit schauerlich wilder Extase:", "tokens": ["Mit", "schau\u00b7er\u00b7lich", "wil\u00b7der", "Ex\u00b7ta\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "\u201ebleib bei mir in Hamburg, ich liebe dich,", "tokens": ["\u201e", "bleib", "bei", "mir", "in", "Ham\u00b7burg", ",", "ich", "lie\u00b7be", "dich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPER", "APPR", "NE", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wir wollen trinken und essen", "tokens": ["Wir", "wol\u00b7len", "trin\u00b7ken", "und", "es\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den Wein und die Austern der Gegenwart,", "tokens": ["Den", "Wein", "und", "die", "Aus\u00b7tern", "der", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und die dunkle Zukunft vergessen.", "tokens": ["Und", "die", "dunk\u00b7le", "Zu\u00b7kunft", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201eden Deckel darauf! damit uns nicht", "tokens": ["\u201e", "den", "De\u00b7ckel", "da\u00b7rauf", "!", "da\u00b7mit", "uns", "nicht"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "PAV", "$.", "KOUS", "PPER", "PTKNEG"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Mi\u00dfduft die Freude vertr\u00fcbet \u2013", "tokens": ["Der", "Mi\u00df\u00b7duft", "die", "Freu\u00b7de", "ver\u00b7tr\u00fc\u00b7bet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich liebe dich, wie je ein Weib", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "wie", "je", "ein", "Weib"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einen deutschen Poeten geliebet!", "tokens": ["Ei\u00b7nen", "deut\u00b7schen", "Po\u00b7et\u00b7en", "ge\u00b7lie\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}