{"textgrid.poem.42008": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebte dich, und ach, ich mu\u00df entsagen;", "tokens": ["Ich", "lieb\u00b7te", "dich", ",", "und", "ach", ",", "ich", "mu\u00df", "ent\u00b7sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "XY", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht z\u00fcrn' ich dir, ich z\u00fcrne dem Geschick;", "tokens": ["Nicht", "z\u00fcrn'", "ich", "dir", ",", "ich", "z\u00fcr\u00b7ne", "dem", "Ge\u00b7schick", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVIMP", "PPER", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wirst du mich je um meine Thr\u00e4nen fragen,", "tokens": ["Wirst", "du", "mich", "je", "um", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So gieb nur selbst die Antwort dir zur\u00fcck.", "tokens": ["So", "gieb", "nur", "selbst", "die", "Ant\u00b7wort", "dir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich liebte dich, ich will es nicht verhehlen,", "tokens": ["Ich", "lieb\u00b7te", "dich", ",", "ich", "will", "es", "nicht", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War auch nur Schmerz der langen Sehnsucht Ziel;", "tokens": ["War", "auch", "nur", "Schmerz", "der", "lan\u00b7gen", "Sehn\u00b7sucht", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist Liebe nicht ein Antheil sch\u00f6n'rer Seelen,", "tokens": ["Ist", "Lie\u00b7be", "nicht", "ein", "An\u00b7theil", "sch\u00f6n'\u00b7rer", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lohnet sich Gef\u00fchl nicht durch Gef\u00fchl?", "tokens": ["Und", "loh\u00b7net", "sich", "Ge\u00b7f\u00fchl", "nicht", "durch", "Ge\u00b7f\u00fchl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich liebte dich mit jenem zarten Triebe,", "tokens": ["Ich", "lieb\u00b7te", "dich", "mit", "je\u00b7nem", "zar\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem nicht Genu\u00df, dem Liebe nur gen\u00fcgt;", "tokens": ["Dem", "nicht", "Ge\u00b7nu\u00df", ",", "dem", "Lie\u00b7be", "nur", "ge\u00b7n\u00fcgt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, du begannst und schlossest meine Liebe!", "tokens": ["Ach", ",", "du", "be\u00b7gannst", "und", "schlos\u00b7sest", "mei\u00b7ne", "Lie\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer mich besiegt hat ewig mich besiegt.", "tokens": ["Wer", "mich", "be\u00b7siegt", "hat", "e\u00b7wig", "mich", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich liebe dich und kann dich nie vergessen;", "tokens": ["Ich", "lie\u00b7be", "dich", "und", "kann", "dich", "nie", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch schweigen will ich mit verhaltnem Schmerz,", "tokens": ["Doch", "schwei\u00b7gen", "will", "ich", "mit", "ver\u00b7halt\u00b7nem", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will allen Gram in ", "tokens": ["Will", "al\u00b7len", "Gram", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In einen Seufzer mein zerdr\u00fccktes Herz.", "tokens": ["In", "ei\u00b7nen", "Seuf\u00b7zer", "mein", "zer\u00b7dr\u00fcck\u00b7tes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Jetzt mag das Schicksal jedes Gl\u00fcck mir rauben;", "tokens": ["Jetzt", "mag", "das", "Schick\u00b7sal", "je\u00b7des", "Gl\u00fcck", "mir", "rau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der giebt sein Alles, wer sein Bestes giebt;", "tokens": ["Der", "giebt", "sein", "Al\u00b7les", ",", "wer", "sein", "Bes\u00b7tes", "giebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAINF", "PIS", "$,", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O la\u00df mir nur den letzten s\u00fc\u00dfen Glauben,", "tokens": ["O", "la\u00df", "mir", "nur", "den", "letz\u00b7ten", "s\u00fc\u00b7\u00dfen", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df du mich nicht geha\u00dft, weil ich geliebt.", "tokens": ["Da\u00df", "du", "mich", "nicht", "ge\u00b7ha\u00dft", ",", "weil", "ich", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "La\u00df mir die Lust dich geistig zu verehren,", "tokens": ["La\u00df", "mir", "die", "Lust", "dich", "geis\u00b7tig", "zu", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPER", "ADJD", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im s\u00fc\u00dfen Traum dein Bildni\u00df zu umfahn,", "tokens": ["Im", "s\u00fc\u00b7\u00dfen", "Traum", "dein", "Bild\u00b7ni\u00df", "zu", "um\u00b7fahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df mir den Trost der stillen Wehmuthsz\u00e4hren,", "tokens": ["La\u00df", "mir", "den", "Trost", "der", "stil\u00b7len", "Weh\u00b7muths\u00b7z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Geistern\u00e4he wunders\u00fc\u00dfen Wahn.", "tokens": ["Der", "Geis\u00b7ter\u00b7n\u00e4\u00b7he", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfen", "Wahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ach, jede Lust wird doppelt mich entz\u00fccken,", "tokens": ["Ach", ",", "je\u00b7de", "Lust", "wird", "dop\u00b7pelt", "mich", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "VAFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn deine Lust zu f\u00fchlen w\u00e4hnt mein Herz;", "tokens": ["Denn", "dei\u00b7ne", "Lust", "zu", "f\u00fch\u00b7len", "w\u00e4hnt", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jeder Schmerz, er wird mich minder dr\u00fccken,", "tokens": ["Und", "je\u00b7der", "Schmerz", ",", "er", "wird", "mich", "min\u00b7der", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn tr\u00f6stend ruft's: Sie f\u00fchlet deinen Schmerz!", "tokens": ["Denn", "tr\u00f6s\u00b7tend", "ruft's", ":", "Sie", "f\u00fch\u00b7let", "dei\u00b7nen", "Schmerz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Als Ideal sollst du jetzt vor mir schweben,", "tokens": ["Als", "I\u00b7deal", "sollst", "du", "jetzt", "vor", "mir", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was ich gedacht, gef\u00fchlt, dir will ich's weihn,", "tokens": ["Was", "ich", "ge\u00b7dacht", ",", "ge\u00b7f\u00fchlt", ",", "dir", "will", "ich's", "weihn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du sollst die Gluth der Phantasie beleben,", "tokens": ["Du", "sollst", "die", "Gluth", "der", "Phan\u00b7ta\u00b7sie", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sollst mein Lied und meine Muse seyn.", "tokens": ["Du", "sollst", "mein", "Lied", "und", "mei\u00b7ne", "Mu\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Empor aus dieses Lebens D\u00e4mmerungen", "tokens": ["Em\u00b7por", "aus", "die\u00b7ses", "Le\u00b7bens", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll mich dein Bild zum reinern Licht erh\u00f6hn,", "tokens": ["Soll", "mich", "dein", "Bild", "zum", "rei\u00b7nern", "Licht", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ist mir je ein sch\u00f6nes Werk gelungen,", "tokens": ["Und", "ist", "mir", "je", "ein", "sch\u00f6\u00b7nes", "Werk", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es soll nur dir als ew'ges Denkmahl stehn.", "tokens": ["Es", "soll", "nur", "dir", "als", "ew'\u00b7ges", "Denk\u00b7mahl", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "In der Madonna seelenvollen Z\u00fcgen,", "tokens": ["In", "der", "Ma\u00b7don\u00b7na", "see\u00b7len\u00b7vol\u00b7len", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im zarten Bild der j\u00fcngsten Huldg\u00f6ttin,", "tokens": ["Im", "zar\u00b7ten", "Bild", "der", "j\u00fcng\u00b7sten", "Huld\u00b7g\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In jedem Reiz, dem sich die Herzen schmiegen,", "tokens": ["In", "je\u00b7dem", "Reiz", ",", "dem", "sich", "die", "Her\u00b7zen", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erblick' ich dich und sinke vor dir hin.", "tokens": ["Er\u00b7blick'", "ich", "dich", "und", "sin\u00b7ke", "vor", "dir", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Als Heilige wird dich mein Herz verehren,", "tokens": ["Als", "Hei\u00b7li\u00b7ge", "wird", "dich", "mein", "Herz", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sich zu nahn der Pilger nicht erk\u00fchnt,", "tokens": ["Der", "sich", "zu", "nahn", "der", "Pil\u00b7ger", "nicht", "er\u00b7k\u00fchnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der er von fern nur mit der Inbrunst Z\u00e4hren,", "tokens": ["Der", "er", "von", "fern", "nur", "mit", "der", "In\u00b7brunst", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJD", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur mit dem Opfer frommer Seufzer dient.", "tokens": ["Nur", "mit", "dem", "Op\u00b7fer", "from\u00b7mer", "Seuf\u00b7zer", "dient", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Gieb mir den Ku\u00df der schwesterlichen Treue;", "tokens": ["Gieb", "mir", "den", "Ku\u00df", "der", "schwes\u00b7ter\u00b7li\u00b7chen", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nur Freundschaft sey der Herzen neues Band.", "tokens": ["Nur", "Freund\u00b7schaft", "sey", "der", "Her\u00b7zen", "neu\u00b7es", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", 