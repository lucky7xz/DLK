{"textgrid.poem.37625": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Kanon auf einen Lichterputzer", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelobt sei, wer die Lichter putzt,", "tokens": ["Ge\u00b7lobt", "sei", ",", "wer", "die", "Lich\u00b7ter", "putzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist ein Mann, der allen nutzt,", "tokens": ["Er", "ist", "ein", "Mann", ",", "der", "al\u00b7len", "nutzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er brauchet gar nicht viel Verstand,", "tokens": ["Er", "brau\u00b7chet", "gar", "nicht", "viel", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur eine Lichtscheer in der Hand,", "tokens": ["Nur", "ei\u00b7ne", "Licht\u00b7scheer", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vertrauen und Gewandtheit viel,", "tokens": ["Ver\u00b7trau\u00b7en", "und", "Ge\u00b7wand\u00b7theit", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst wird er aller Sp\u00f6tter Ziel.", "tokens": ["Sonst", "wird", "er", "al\u00b7ler", "Sp\u00f6t\u00b7ter", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Licht, das er hat ausgeputzt,", "tokens": ["Ein", "Licht", ",", "das", "er", "hat", "aus\u00b7ge\u00b7putzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird ihm von allen aufgemutzt,", "tokens": ["Wird", "ihm", "von", "al\u00b7len", "auf\u00b7ge\u00b7mutzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tritt er wieder an das Licht,", "tokens": ["Und", "tritt", "er", "wie\u00b7der", "an", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schreien alle: Putze nicht,", "tokens": ["So", "schrei\u00b7en", "al\u00b7le", ":", "Put\u00b7ze", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$.", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein gutes Licht sich selber putzt", "tokens": ["Ein", "gu\u00b7tes", "Licht", "sich", "sel\u00b7ber", "putzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und deine Lichtscheer uns beschmutzt.", "tokens": ["Und", "dei\u00b7ne", "Licht\u00b7scheer", "uns", "be\u00b7schmutzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}