{"textgrid.poem.55651": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[was machst du an der Welt? Sie ist schon gemacht]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was machst du an der Welt? Sie ist schon gemacht,", "tokens": ["Was", "machst", "du", "an", "der", "Welt", "?", "Sie", "ist", "schon", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der Herr der Sch\u00f6pfung hat alles bedacht.", "tokens": ["Der", "Herr", "der", "Sch\u00f6p\u00b7fung", "hat", "al\u00b7les", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dein Los ist gefallen, verfolge die Weise,", "tokens": ["Dein", "Los", "ist", "ge\u00b7fal\u00b7len", ",", "ver\u00b7fol\u00b7ge", "die", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Weg ist begonnen, vollende die Reise:", "tokens": ["Der", "Weg", "ist", "be\u00b7gon\u00b7nen", ",", "voll\u00b7en\u00b7de", "die", "Rei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ADJA", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Denn Sorgen und Kummer ver\u00e4ndern es nicht,", "tokens": ["Denn", "Sor\u00b7gen", "und", "Kum\u00b7mer", "ver\u00b7\u00e4n\u00b7dern", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie schleudern dich ewig aus gleichem Gewicht.", "tokens": ["Sie", "schleu\u00b7dern", "dich", "e\u00b7wig", "aus", "glei\u00b7chem", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}