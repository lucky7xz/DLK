{"textgrid.poem.53400": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[gott z\u00fcrnt auff uns zusammen]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott z\u00fcrnt auff uns zusammen,", "tokens": ["Gott", "z\u00fcrnt", "auff", "uns", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer ist der bleiben kan", "tokens": ["Wer", "ist", "der", "blei\u00b7ben", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr seines Eiffers Flammen?", "tokens": ["F\u00fcr", "sei\u00b7nes", "Eif\u00b7fers", "Flam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er greifft die Cedern an,", "tokens": ["Er", "greifft", "die", "Ce\u00b7dern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Berge m\u00fcssen beben,", "tokens": ["Die", "Ber\u00b7ge", "m\u00fcs\u00b7sen", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das gantze Land erschrickt", "tokens": ["Das", "gant\u00b7ze", "Land", "er\u00b7schrickt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd hat in ihm kein Leben", "tokens": ["Vnd", "hat", "in", "ihm", "kein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nun er es angeblickt.", "tokens": ["Nun", "er", "es", "an\u00b7ge\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Weh uns und unsern S\u00fcnden,", "tokens": ["Weh", "uns", "und", "un\u00b7sern", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn diese sind allein", "tokens": ["Denn", "die\u00b7se", "sind", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ihn zur Rach' entz\u00fcnden!", "tokens": ["Die", "ihn", "zur", "Rach'", "ent\u00b7z\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer nimmt uns zu sich ein?", "tokens": ["Wer", "nimmt", "uns", "zu", "sich", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer l\u00e4sst uns Schutz gewinnen", "tokens": ["Wer", "l\u00e4sst", "uns", "Schutz", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bi\u00df sich das Wetter legt,", "tokens": ["Bi\u00df", "sich", "das", "Wet\u00b7ter", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er wieder Vater-Sinnen", "tokens": ["Er", "wie\u00b7der", "Va\u00b7ter\u00b7Sin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu seinen Kindern tr\u00e4gt?", "tokens": ["Zu", "sei\u00b7nen", "Kin\u00b7dern", "tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sein liebstes Volck die Frommen", "tokens": ["Sein", "liebs\u00b7tes", "Volck", "die", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind fern von dieser Noht,", "tokens": ["Sind", "fern", "von", "die\u00b7ser", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er heisst sie zu sich kommen", "tokens": ["Er", "heisst", "sie", "zu", "sich", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch einen selign Tod:", "tokens": ["Durch", "ei\u00b7nen", "se\u00b7lign", "Tod", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie gehn in ihre Kammer", "tokens": ["Sie", "gehn", "in", "ih\u00b7re", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In die gew\u00fcnschte Ruh,", "tokens": ["In", "die", "ge\u00b7w\u00fcnschte", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "Vnd schliessen f\u00fcr dem Jammer", "tokens": ["Vnd", "schlies\u00b7sen", "f\u00fcr", "dem", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Th\u00fcr behutsam zu.", "tokens": ["Die", "Th\u00fcr", "be\u00b7hut\u00b7sam", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir bleiben hie wir Armen", "tokens": ["Wir", "blei\u00b7ben", "hie", "wir", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In grosser Angst und Pein.", "tokens": ["In", "gros\u00b7ser", "Angst", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott wolle sich erbarmen", "tokens": ["Gott", "wol\u00b7le", "sich", "er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd unsre Zuflucht seyn:", "tokens": ["Vnd", "uns\u00b7re", "Zu\u00b7flucht", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Woll' unsre Feinde f\u00e4llen", "tokens": ["Woll'", "uns\u00b7re", "Fein\u00b7de", "f\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und trewlich bey uns stehn,", "tokens": ["Und", "trew\u00b7lich", "bey", "uns", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df wir in diesen Wellen", "tokens": ["Da\u00df", "wir", "in", "die\u00b7sen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht kl\u00e4glich untergehn.", "tokens": ["Nicht", "kl\u00e4g\u00b7lich", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Herr Jesu, hilff uns tragen", "tokens": ["Herr", "Je\u00b7su", ",", "hilff", "uns", "tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Zeiten schwere Last,", "tokens": ["Der", "Zei\u00b7ten", "schwe\u00b7re", "Last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit wir nicht verzagen,", "tokens": ["Da\u00b7mit", "wir", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sey unser Burg und Rast:", "tokens": ["Sey", "un\u00b7ser", "Burg", "und", "Rast", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lehr uns die S\u00fcnde meiden,", "tokens": ["Lehr", "uns", "die", "S\u00fcn\u00b7de", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wasch uns mit deinem Blut,", "tokens": ["Wasch", "uns", "mit", "dei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd nimm aus diesem Leiden", "tokens": ["Vnd", "nimm", "aus", "die\u00b7sem", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vns in die ewig' Hut.", "tokens": ["Vns", "in", "die", "e\u00b7wig'", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}