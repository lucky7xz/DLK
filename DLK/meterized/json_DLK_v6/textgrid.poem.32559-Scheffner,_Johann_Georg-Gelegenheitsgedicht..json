{"textgrid.poem.32559": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Gelegenheitsgedicht.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n war der Abend, Fr\u00fchlingsduft", "tokens": ["Sch\u00f6n", "war", "der", "A\u00b7bend", ",", "Fr\u00fch\u00b7lings\u00b7duft"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchbalsamte die heitre Luft,", "tokens": ["Durch\u00b7bal\u00b7sam\u00b7te", "die", "heit\u00b7re", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und jeder Stern war aufgegangen,", "tokens": ["Und", "je\u00b7der", "Stern", "war", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Cytherens himmlich Feuermeer", "tokens": ["Cy\u00b7the\u00b7rens", "himm\u00b7lich", "Feu\u00b7er\u00b7meer"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJD", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Scho\u00df zehnfach Stralen um sich her,", "tokens": ["Scho\u00df", "zehn\u00b7fach", "Stra\u00b7len", "um", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und Nachtigallen sangen.", "tokens": ["Und", "Nach\u00b7ti\u00b7gal\u00b7len", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch Venus Stern und Fr\u00fchlingsduft", "tokens": ["Doch", "Ve\u00b7nus", "Stern", "und", "Fr\u00fch\u00b7lings\u00b7duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Nachtigall und Abendluft", "tokens": ["Und", "Nach\u00b7ti\u00b7gall", "und", "A\u00b7ben\u00b7dluft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verga\u00df ich in der Sch\u00e4ferstunde.", "tokens": ["Ver\u00b7ga\u00df", "ich", "in", "der", "Sch\u00e4\u00b7fer\u00b7stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gipfel aller Lust erstieg", "tokens": ["Den", "Gip\u00b7fel", "al\u00b7ler", "Lust", "er\u00b7stieg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Geist, und Balsam go\u00df der Sieg", "tokens": ["Der", "Geist", ",", "und", "Bal\u00b7sam", "go\u00df", "der", "Sieg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sanft um den Rand der Pfirsichwunde.", "tokens": ["Sanft", "um", "den", "Rand", "der", "Pfir\u00b7sich\u00b7wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sprich M\u00e4dchen schlug im Busch und Thal,", "tokens": ["Sprich", "M\u00e4d\u00b7chen", "schlug", "im", "Busch", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "VVFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Je eine sch\u00f6nre Nachtigall?", "tokens": ["Je", "ei\u00b7ne", "sch\u00f6n\u00b7re", "Nach\u00b7ti\u00b7gall", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lie\u00df je ihr Lied dein Herz so wallen?", "tokens": ["Lie\u00df", "je", "ihr", "Lied", "dein", "Herz", "so", "wal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und als sie m\u00fcd vom Nachtgesang", "tokens": ["Und", "als", "sie", "m\u00fcd", "vom", "Nacht\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zahm auf den wei\u00dfen Busen sprang,", "tokens": ["Zahm", "auf", "den", "wei\u00b7\u00dfen", "Bu\u00b7sen", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat sie auch da Dir noch gefallen?", "tokens": ["Hat", "sie", "auch", "da", "Dir", "noch", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Wie auf dem jungen Zweig vergn\u00fcgt", "tokens": ["Wie", "auf", "dem", "jun\u00b7gen", "Zweig", "ver\u00b7gn\u00fcgt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Philomele einsam wiegt;", "tokens": ["Sich", "Phi\u00b7lo\u00b7me\u00b7le", "ein\u00b7sam", "wiegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wiegte sich auf Busenh\u00fcgeln,", "tokens": ["So", "wieg\u00b7te", "sich", "auf", "Bu\u00b7sen\u00b7h\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sah umher die Nachtigall,", "tokens": ["Und", "sah", "um\u00b7her", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um sich zum neuen Flug ins Thal", "tokens": ["Um", "sich", "zum", "neu\u00b7en", "Flug", "ins", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "APPRART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch jeden Herzschlag zu befl\u00fcgeln.", "tokens": ["Durch", "je\u00b7den", "Herz\u00b7schlag", "zu", "be\u00b7fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Druck mit der weichen Hand doch nur", "tokens": ["Druck", "mit", "der", "wei\u00b7chen", "Hand", "doch", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADV", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den Lieblingsspro\u00dfer deiner Flur", "tokens": ["Den", "Lieb\u00b7lings\u00b7spro\u00b7\u00dfer", "dei\u00b7ner", "Flur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An Dich, und spiel mit seinen Schwingen", "tokens": ["An", "Dich", ",", "und", "spiel", "mit", "sei\u00b7nen", "Schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist ein kleines dankbars Thier,", "tokens": ["Er", "ist", "ein", "klei\u00b7nes", "dank\u00b7bars", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wird f\u00fcr dies Geschmeichel Dir", "tokens": ["Und", "wird", "f\u00fcr", "dies", "Ge\u00b7schmei\u00b7chel", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PDS", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das sch\u00f6nste Ritornello singen.", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Ri\u00b7tor\u00b7nel\u00b7lo", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}