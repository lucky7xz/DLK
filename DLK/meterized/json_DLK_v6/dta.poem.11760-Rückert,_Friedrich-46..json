{"dta.poem.11760": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein altes Sprichwort sagt: Im Tr\u00fcben ist gut fischen.", "tokens": ["Ein", "al\u00b7tes", "Sprich\u00b7wort", "sagt", ":", "Im", "Tr\u00fc\u00b7ben", "ist", "gut", "fi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "APPRART", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein andres: gut ists auch im Tr\u00fcben zu entwischen.", "tokens": ["Ein", "and\u00b7res", ":", "gut", "ists", "auch", "im", "Tr\u00fc\u00b7ben", "zu", "ent\u00b7wi\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$.", "ADJD", "VAFIN", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dort ists der Fischer selbst der seinen T\u00fcmpfel tr\u00fcbt,", "tokens": ["Dort", "ists", "der", "Fi\u00b7scher", "selbst", "der", "sei\u00b7nen", "T\u00fcmp\u00b7fel", "tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und am beth\u00f6rten Fisch mit Gl\u00fcck sein Handwerk \u00fcbt.", "tokens": ["Und", "am", "be\u00b7th\u00f6r\u00b7ten", "Fisch", "mit", "Gl\u00fcck", "sein", "Hand\u00b7werk", "\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und also tr\u00fcbt die Flut um sich der Kraken auch,", "tokens": ["Und", "al\u00b7so", "tr\u00fcbt", "die", "Flut", "um", "sich", "der", "Kra\u00b7ken", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "PRF", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df blinde Heringsbrut sich dr\u00e4ng' in seinen Bauch.", "tokens": ["Da\u00df", "blin\u00b7de", "He\u00b7rings\u00b7brut", "sich", "dr\u00e4ng'", "in", "sei\u00b7nen", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch hier ein Fischlein ists, das keine andre Kraft", "tokens": ["Doch", "hier", "ein", "Fisc\u00b7hlein", "ists", ",", "das", "kei\u00b7ne", "and\u00b7re", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "$,", "PRELS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu seiner Nothwehr hat als seinen braunen Saft.", "tokens": ["Zu", "sei\u00b7ner", "Noth\u00b7wehr", "hat", "als", "sei\u00b7nen", "brau\u00b7nen", "Saft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der braune Saft, um den die Menschen selbst es fangen,", "tokens": ["Der", "brau\u00b7ne", "Saft", ",", "um", "den", "die", "Men\u00b7schen", "selbst", "es", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "ART", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Derselbe ists, durch den es ihnen ist entgangen.", "tokens": ["Der\u00b7sel\u00b7be", "ists", ",", "durch", "den", "es", "ih\u00b7nen", "ist", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPR", "PRELS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Spritz, arme Sepie, wehrloser Tintenfisch,", "tokens": ["Spritz", ",", "ar\u00b7me", "Se\u00b7pie", ",", "wehr\u00b7lo\u00b7ser", "Tin\u00b7ten\u00b7fisch", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Tinte nach dem Feind, und in der Tr\u00fcb' entwisch!", "tokens": ["Die", "Tin\u00b7te", "nach", "dem", "Feind", ",", "und", "in", "der", "Tr\u00fcb'", "ent\u00b7wisch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}