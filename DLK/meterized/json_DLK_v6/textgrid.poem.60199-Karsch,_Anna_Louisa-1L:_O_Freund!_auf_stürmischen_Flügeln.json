{"textgrid.poem.60199": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Freund! auf st\u00fcrmischen Fl\u00fcgeln", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Freund! auf st\u00fcrmischen Fl\u00fcgeln", "tokens": ["O", "Freund", "!", "auf", "st\u00fcr\u00b7mi\u00b7schen", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hochheulend \u00fcber den Dohm", "tokens": ["Hoch\u00b7heu\u00b7lend", "\u00fc\u00b7ber", "den", "Dohm"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bringt der unfreundliche Nordwind", "tokens": ["Bringt", "der", "un\u00b7freund\u00b7li\u00b7che", "Nord\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mir meinen festlichen Tag.", "tokens": ["Mir", "mei\u00b7nen", "fest\u00b7li\u00b7chen", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ich denk an st\u00fcrmende Sorgen;", "tokens": ["Ich", "denk", "an", "st\u00fcr\u00b7men\u00b7de", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor\u00fcber brauseten sie.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "brau\u00b7se\u00b7ten", "sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So denkt der landende Schiffer", "tokens": ["So", "denkt", "der", "lan\u00b7den\u00b7de", "Schif\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Hafen an den Orcan!", "tokens": ["Im", "Ha\u00b7fen", "an", "den", "Or\u00b7can", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mich fand der himmlischen einer", "tokens": ["Mich", "fand", "der", "himm\u00b7li\u00b7schen", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Am Tage meiner Geburt", "tokens": ["Am", "Ta\u00b7ge", "mei\u00b7ner", "Ge\u00b7burt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bedeckt mit H\u00fcllen der Armuth.", "tokens": ["Be\u00b7deckt", "mit", "H\u00fcl\u00b7len", "der", "Ar\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mitleidig sah er mich an,", "tokens": ["Mit\u00b7lei\u00b7dig", "sah", "er", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und sprach zum Vater der Menschen:", "tokens": ["Und", "sprach", "zum", "Va\u00b7ter", "der", "Men\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Herr \u00fcber Leben und Gl\u00fcck!", "tokens": ["Herr", "\u00fc\u00b7ber", "Le\u00b7ben", "und", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Gieb diese niedrig gebohrne", "tokens": ["Gieb", "die\u00b7se", "nied\u00b7rig", "ge\u00b7bohr\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PDAT", "ADJD", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "In meinen leitenden Schutz;", "tokens": ["In", "mei\u00b7nen", "lei\u00b7ten\u00b7den", "Schutz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Sie liegt im Schoosse des Kummers,", "tokens": ["Sie", "liegt", "im", "Schoos\u00b7se", "des", "Kum\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tief decket schm\u00e4hliger Staub", "tokens": ["Tief", "de\u00b7cket", "schm\u00e4h\u00b7li\u00b7ger", "Staub"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Ernstbefaltete Stirne", "tokens": ["Die", "Ernst\u00b7be\u00b7fal\u00b7te\u00b7te", "Stir\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von dir zum Denken gebaut!", "tokens": ["Von", "dir", "zum", "Den\u00b7ken", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Dein sey sie, sagte zum Engel", "tokens": ["Dein", "sey", "sie", ",", "sag\u00b7te", "zum", "En\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "PPER", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der alles schaffende Gott.", "tokens": ["Der", "al\u00b7les", "schaf\u00b7fen\u00b7de", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da ward mir eine der Musen", "tokens": ["Da", "ward", "mir", "ei\u00b7ne", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und diese Leyer gebracht,", "tokens": ["Und", "die\u00b7se", "Le\u00b7yer", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Auf der ich festliche Hymnen", "tokens": ["Auf", "der", "ich", "fest\u00b7li\u00b7che", "Hym\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Des Helden Friedrichs Lob,", "tokens": ["Des", "Hel\u00b7den", "Fried\u00b7richs", "Lob", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Tugend, heilige Freundschaft", "tokens": ["Die", "Tu\u00b7gend", ",", "hei\u00b7li\u00b7ge", "Freund\u00b7schaft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sanfte Liebe gespielt!", "tokens": ["Und", "sanf\u00b7te", "Lie\u00b7be", "ge\u00b7spielt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Du h\u00f6rest meine Ges\u00e4nge:", "tokens": ["Du", "h\u00f6\u00b7rest", "mei\u00b7ne", "Ge\u00b7s\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O Freund! ich singe noch heut", "tokens": ["O", "Freund", "!", "ich", "sin\u00b7ge", "noch", "heut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dem, der von Menschen Gehorsam,", "tokens": ["Dem", ",", "der", "von", "Men\u00b7schen", "Ge\u00b7hor\u00b7sam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "NN", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und Hecatomben nicht, heischt.", "tokens": ["Und", "He\u00b7ca\u00b7tom\u00b7ben", "nicht", ",", "heischt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "$,", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Ich komm' und trage dem Winter", "tokens": ["Ich", "komm'", "und", "tra\u00b7ge", "dem", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zum Trotz, auf klopfender Brust", "tokens": ["Zum", "Trotz", ",", "auf", "klop\u00b7fen\u00b7der", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Den Strau\u00df von gr\u00fcnenden Lorbeeren;", "tokens": ["Den", "Strau\u00df", "von", "gr\u00fc\u00b7nen\u00b7den", "Lor\u00b7bee\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zwo M\u00e4dchen wanden ihn mir!", "tokens": ["Zwo", "M\u00e4d\u00b7chen", "wan\u00b7den", "ihn", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Du aber rufe den Diener", "tokens": ["Du", "a\u00b7ber", "ru\u00b7fe", "den", "Die\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gesch\u00e4ftger sahe Horaz", "tokens": ["Ge\u00b7sch\u00e4ft\u00b7ger", "sa\u00b7he", "Ho\u00b7raz"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht den einschenkenden Knaben", "tokens": ["Nicht", "den", "ein\u00b7schen\u00b7ken\u00b7den", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mit Becherreichender Hand.", "tokens": ["Mit", "Be\u00b7cher\u00b7rei\u00b7chen\u00b7der", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Ruf ihn. Er bringe die Flasche", "tokens": ["Ruf", "ihn", ".", "Er", "brin\u00b7ge", "die", "Fla\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Voll von zehnj\u00e4hrigem Wein", "tokens": ["Voll", "von", "zehn\u00b7j\u00e4h\u00b7ri\u00b7gem", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Gereift im Lande, das Frieden", "tokens": ["Ge\u00b7reift", "im", "Lan\u00b7de", ",", "das", "Frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Fleht, von Brittanniens Thron.", "tokens": ["Fleht", ",", "von", "Brit\u00b7tan\u00b7ni\u00b7ens", "Thron", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Er kr\u00e4nzt den Becher mit Blumen", "tokens": ["Er", "kr\u00e4nzt", "den", "Be\u00b7cher", "mit", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geraubt der armen Natur.", "tokens": ["Ge\u00b7raubt", "der", "ar\u00b7men", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Genannt wird Tyrsis und Sulzer,", "tokens": ["Ge\u00b7nannt", "wird", "Tyr\u00b7sis", "und", "Sul\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wer dich kennet und liebt.", "tokens": ["Und", "wer", "dich", "ken\u00b7net", "und", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}