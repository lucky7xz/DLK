{"textgrid.poem.65199": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwarum verstummt das Lied, da\u00df hei\u00dfe Welle", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwarum verstummt das Lied, da\u00df hei\u00dfe Welle", "tokens": ["\u00bb", "wa\u00b7rum", "ver\u00b7stummt", "das", "Lied", ",", "da\u00df", "hei\u00b7\u00dfe", "Wel\u00b7le"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWAV", "VVFIN", "ART", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich st\u00fcrmisch einst aus deiner Brust ergo\u00df?", "tokens": ["Sich", "st\u00fcr\u00b7misch", "einst", "aus", "dei\u00b7ner", "Brust", "er\u00b7go\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Lied, das dunkel bald, bald wieder helle", "tokens": ["Das", "Lied", ",", "das", "dun\u00b7kel", "bald", ",", "bald", "wie\u00b7der", "hel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADV", "$,", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In fremde Herzen str\u00f6mend \u00fcberflo\u00df?", "tokens": ["In", "frem\u00b7de", "Her\u00b7zen", "str\u00f6\u00b7mend", "\u00fc\u00b7ber\u00b7flo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist's unerh\u00f6rten Mi\u00dfgeschickes Grelle,", "tokens": ["Ist's", "un\u00b7er\u00b7h\u00f6r\u00b7ten", "Mi\u00df\u00b7ge\u00b7schi\u00b7ckes", "Grel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die, dich versteinernd, dir die Lippe schlo\u00df?", "tokens": ["Die", ",", "dich", "ver\u00b7stei\u00b7nernd", ",", "dir", "die", "Lip\u00b7pe", "schlo\u00df", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PPER", "VVPP", "$,", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie? Oder ward dir solches Gl\u00fcck zu eigen,", "tokens": ["Wie", "?", "O\u00b7der", "ward", "dir", "sol\u00b7ches", "Gl\u00fcck", "zu", "ei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NE", "VAFIN", "PPER", "PIAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df es zu feiern nur mit sel'gem Schweigen?\u00ab", "tokens": ["Da\u00df", "es", "zu", "fei\u00b7ern", "nur", "mit", "sel'\u00b7gem", "Schwei\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist es nicht. Kein Ueberma\u00df der Wonnen,", "tokens": ["Das", "ist", "es", "nicht", ".", "Kein", "Ue\u00b7ber\u00b7ma\u00df", "der", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "$.", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein ird'scher Schmerz h\u00e4lt mich in seinem Bann,", "tokens": ["Kein", "ird'\u00b7scher", "Schmerz", "h\u00e4lt", "mich", "in", "sei\u00b7nem", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und fahre ich nicht fort wie ich begonnen,", "tokens": ["Und", "fah\u00b7re", "ich", "nicht", "fort", "wie", "ich", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "KOKOM", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So ist's nur, weil ein Traumbild mir zerrann,", "tokens": ["So", "ist's", "nur", ",", "weil", "ein", "Traum\u00b7bild", "mir", "zer\u00b7rann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein holder Wahn, ein s\u00fc\u00dfer M\u00e4rchenbronnen,", "tokens": ["Ein", "hol\u00b7der", "Wahn", ",", "ein", "s\u00fc\u00b7\u00dfer", "M\u00e4r\u00b7chen\u00b7bron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus dem ich immer neue Kraft gewann,", "tokens": ["Aus", "dem", "ich", "im\u00b7mer", "neu\u00b7e", "Kraft", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im Lebenskampf mich standhaft zu bew\u00e4hren,", "tokens": ["Im", "Le\u00b7bens\u00b7kampf", "mich", "stand\u00b7haft", "zu", "be\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Lebens Leid im Liede zu verkl\u00e4ren.", "tokens": ["Des", "Le\u00b7bens", "Leid", "im", "Lie\u00b7de", "zu", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was dieses Traumbild war! Es war der Glaube", "tokens": ["Was", "die\u00b7ses", "Traum\u00b7bild", "war", "!", "Es", "war", "der", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "VAFIN", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An eines Gottes treue Vaterhand,", "tokens": ["An", "ei\u00b7nes", "Got\u00b7tes", "treu\u00b7e", "Va\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An eine Heimat \u00fcber'm Erdenstaube,", "tokens": ["An", "ei\u00b7ne", "Hei\u00b7mat", "\u00fc\u00b7ber'm", "Er\u00b7dens\u00b7tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An meines Wesens ewigen Bestand!", "tokens": ["An", "mei\u00b7nes", "We\u00b7sens", "e\u00b7wi\u00b7gen", "Be\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Seit er dem grimmen Zweifel ward zum Raube,", "tokens": ["Seit", "er", "dem", "grim\u00b7men", "Zwei\u00b7fel", "ward", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist stumm mein Lied, mein Inn'res ausgebrannt,", "tokens": ["Ist", "stumm", "mein", "Lied", ",", "mein", "Inn'\u00b7res", "aus\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und l\u00e4hmend dringet mir auf allen Wegen", "tokens": ["Und", "l\u00e4h\u00b7mend", "drin\u00b7get", "mir", "auf", "al\u00b7len", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Moderhauch der Endlichkeit entgegen!", "tokens": ["Der", "Mo\u00b7der\u00b7hauch", "der", "End\u00b7lich\u00b7keit", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn blinde M\u00e4chte nur das Weltall lenken", "tokens": ["Wenn", "blin\u00b7de", "M\u00e4ch\u00b7te", "nur", "das", "Wel\u00b7tall", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die selber weiter nichts als Stoff und Kraft,", "tokens": ["Die", "sel\u00b7ber", "wei\u00b7ter", "nichts", "als", "Stoff", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIS", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann fluche ich dem F\u00fchlen und dem Denken,", "tokens": ["Dann", "flu\u00b7che", "ich", "dem", "F\u00fch\u00b7len", "und", "dem", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Folterknechten unsrer grausen Haft,", "tokens": ["Den", "Fol\u00b7ter\u00b7knech\u00b7ten", "uns\u00b7rer", "grau\u00b7sen", "Haft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann lob' ich jeden, der mit Taumeltr\u00e4nken", "tokens": ["Dann", "lob'", "ich", "je\u00b7den", ",", "der", "mit", "Tau\u00b7mel\u00b7tr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem marternden Bewu\u00dftsein sich entrafft,", "tokens": ["Dem", "mar\u00b7tern\u00b7den", "Be\u00b7wu\u00df\u00b7tsein", "sich", "en\u00b7trafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df er, samt seines Geistes reinsten Trieben,", "tokens": ["Da\u00df", "er", ",", "samt", "sei\u00b7nes", "Geis\u00b7tes", "reins\u00b7ten", "Trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ob heut', ob morgen spurlos mu\u00df zerstieben!", "tokens": ["Ob", "heut'", ",", "ob", "mor\u00b7gen", "spur\u00b7los", "mu\u00df", "zer\u00b7stie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "KOUS", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dem finstern R\u00e4tsel nimmer nachzusinnen,", "tokens": ["Dem", "fins\u00b7tern", "R\u00e4t\u00b7sel", "nim\u00b7mer", "nach\u00b7zu\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begl\u00fcckt, wem sich der einz'ge Ausweg bot!", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wem", "sich", "der", "einz'\u00b7ge", "Aus\u00b7weg", "bot", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir liegt er fern; auf Stunden nicht entrinnen", "tokens": ["Mir", "liegt", "er", "fern", ";", "auf", "Stun\u00b7den", "nicht", "ent\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kann ich dem Wehgef\u00fchl so bitt'rer Not.", "tokens": ["Kann", "ich", "dem", "Weh\u00b7ge\u00b7f\u00fchl", "so", "bitt'\u00b7rer", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Soll diesem Sein kein h\u00f6h'res sich entspinnen,", "tokens": ["Soll", "die\u00b7sem", "Sein", "kein", "h\u00f6h'\u00b7res", "sich", "ent\u00b7spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "PIAT", "ADJA", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So ist es mir nur ein geschminkter Tod,", "tokens": ["So", "ist", "es", "mir", "nur", "ein", "ge\u00b7schmink\u00b7ter", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Truggesicht mit glei\u00dfnerischen Z\u00fcgen,", "tokens": ["Ein", "Trug\u00b7ge\u00b7sicht", "mit", "glei\u00df\u00b7ne\u00b7ri\u00b7schen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die frechste, die verruchteste der L\u00fcgen! \u2013", "tokens": ["Die", "frechs\u00b7te", ",", "die", "ver\u00b7ruch\u00b7tes\u00b7te", "der", "L\u00fc\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dem starren Weltgesetz mu\u00df ich mich neigen,", "tokens": ["Dem", "star\u00b7ren", "Welt\u00b7ge\u00b7setz", "mu\u00df", "ich", "mich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das mich zum Staub erniedrigt, zum Atom!", "tokens": ["Das", "mich", "zum", "Staub", "er\u00b7nied\u00b7rigt", ",", "zum", "A\u00b7tom", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "VVPP", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So wall' ich denn, versenkt in d\u00fcstres Schweigen,", "tokens": ["So", "wall'", "ich", "denn", ",", "ver\u00b7senkt", "in", "d\u00fcst\u00b7res", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Erden hin, ein Schemen, ein Phantom,", "tokens": ["Auf", "Er\u00b7den", "hin", ",", "ein", "Sche\u00b7men", ",", "ein", "Phan\u00b7tom", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bis, fortgerissen von dem Todesreigen,", "tokens": ["Bis", ",", "fort\u00b7ge\u00b7ris\u00b7sen", "von", "dem", "To\u00b7des\u00b7rei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erfa\u00dft von der Vernichtung dunkelm Strome,", "tokens": ["Er\u00b7fa\u00dft", "von", "der", "Ver\u00b7nich\u00b7tung", "dun\u00b7kelm", "Stro\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Qual, die Leben hei\u00dft auf diesem Sterne,", "tokens": ["Die", "Qual", ",", "die", "Le\u00b7ben", "hei\u00dft", "auf", "die\u00b7sem", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In meinem Grabe ich vergessen lerne!", "tokens": ["In", "mei\u00b7nem", "Gra\u00b7be", "ich", "ver\u00b7ges\u00b7sen", "ler\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}