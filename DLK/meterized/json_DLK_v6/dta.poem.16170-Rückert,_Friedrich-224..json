{"dta.poem.16170": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "224.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edu, der du einst geklagt, dich f\u00fchlend unbefriedigt,", "tokens": ["\u201e", "du", ",", "der", "du", "einst", "ge\u00b7klagt", ",", "dich", "f\u00fch\u00b7lend", "un\u00b7be\u00b7frie\u00b7digt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,", "PRF", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun klagest du nicht mehr, und bist du nun befriedigt?\u201c", "tokens": ["Nun", "kla\u00b7gest", "du", "nicht", "mehr", ",", "und", "bist", "du", "nun", "be\u00b7frie\u00b7digt", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Befriedigt bin ich nicht, doch geb' ich mich zufrieden,", "tokens": ["Be\u00b7frie\u00b7digt", "bin", "ich", "nicht", ",", "doch", "geb'", "ich", "mich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nicht Befriedigung zu finden sei hienieden.", "tokens": ["Da\u00df", "nicht", "Be\u00b7frie\u00b7di\u00b7gung", "zu", "fin\u00b7den", "sei", "hien\u00b7ie\u00b7den", ".