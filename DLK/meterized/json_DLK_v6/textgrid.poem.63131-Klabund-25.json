{"textgrid.poem.63131": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "25", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nahte ich als Held und Beter", "tokens": ["Nah\u00b7te", "ich", "als", "Held", "und", "Be\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter St\u00fcrmen und Zypressen,", "tokens": ["Un\u00b7ter", "St\u00fcr\u00b7men", "und", "Zyp\u00b7res\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ach, vergossen! ach, vergessen!", "tokens": ["Ach", ",", "ver\u00b7gos\u00b7sen", "!", "ach", ",", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VVPP", "$.", "XY", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Regen-schnitter! Leise-treter!", "tokens": ["Re\u00b7gen\u00b7schnit\u00b7ter", "!", "Lei\u00b7se\u00b7tre\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir versagts, dich zu begatten,", "tokens": ["Mir", "ver\u00b7sagts", ",", "dich", "zu", "be\u00b7gat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da ich kindlich an dir h\u00e4nge.", "tokens": ["Da", "ich", "kind\u00b7lich", "an", "dir", "h\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirf den Bastard der Ges\u00e4nge", "tokens": ["Wirf", "den", "Bas\u00b7tard", "der", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NE", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu den Molchen und den Ratten.", "tokens": ["Zu", "den", "Mol\u00b7chen", "und", "den", "Rat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Welt schien Schein und Ampel weiland,", "tokens": ["Welt", "schien", "Schein", "und", "Am\u00b7pel", "wei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Br\u00fcste goldne Glocken,", "tokens": ["Dei\u00b7ne", "Br\u00fcs\u00b7te", "gold\u00b7ne", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nacht und Blut und wei\u00dfe Flocken", "tokens": ["Nacht", "und", "Blut", "und", "wei\u00b7\u00dfe", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinken elend auf mein Eiland.", "tokens": ["Sin\u00b7ken", "e\u00b7lend", "auf", "mein", "Ei\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und du l\u00e4chelst meiner Tr\u00e4nen,", "tokens": ["Und", "du", "l\u00e4\u00b7chelst", "mei\u00b7ner", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rufst zum s\u00fc\u00dfesten Alarme.", "tokens": ["Rufst", "zum", "s\u00fc\u00b7\u00dfes\u00b7ten", "Al\u00b7ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich an die Steinwand lehnen,", "tokens": ["La\u00df", "mich", "an", "die", "Stein\u00b7wand", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df den Stein ich doch umarme.", "tokens": ["Da\u00df", "den", "Stein", "ich", "doch", "um\u00b7ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}