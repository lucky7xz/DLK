{"textgrid.poem.39173": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[so unergr\u00fcndlich ist das Meer von Glanze]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So unergr\u00fcndlich ist das Meer von Glanze,", "tokens": ["So", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "ist", "das", "Meer", "von", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das leuchtend wogt in Deiner Reize Borden,", "tokens": ["Das", "leuch\u00b7tend", "wogt", "in", "Dei\u00b7ner", "Rei\u00b7ze", "Bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df mein Gesang ein k\u00fchner Segler worden", "tokens": ["Da\u00df", "mein", "Ge\u00b7sang", "ein", "k\u00fch\u00b7ner", "Seg\u00b7ler", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Deiner Glieder \u00fcpp'gem Wellentanze.", "tokens": ["Auf", "Dei\u00b7ner", "Glie\u00b7der", "\u00fcpp'\u00b7gem", "Wel\u00b7len\u00b7tan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit Deiner Stirne schwarzem Lockenkranze", "tokens": ["Mit", "Dei\u00b7ner", "Stir\u00b7ne", "schwar\u00b7zem", "Lo\u00b7cken\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Buhlt meiner Liebe Sturm in Luftakkorden,", "tokens": ["Buhlt", "mei\u00b7ner", "Lie\u00b7be", "Sturm", "in", "Luf\u00b7tak\u00b7kor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach Deines Herzens eisig kaltem Norden", "tokens": ["Nach", "Dei\u00b7nes", "Her\u00b7zens", "ei\u00b7sig", "kal\u00b7tem", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dreht stets der Kompa\u00df seine Stahleslanze.", "tokens": ["Dreht", "stets", "der", "Kom\u00b7pa\u00df", "sei\u00b7ne", "Stah\u00b7les\u00b7lan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und willst Du wissen, was ich S\u00fcden nenne?", "tokens": ["Und", "willst", "Du", "wis\u00b7sen", ",", "was", "ich", "S\u00fc\u00b7den", "nen\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "PWS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das sind die Augen Dein, voll Glut und Drange,", "tokens": ["Das", "sind", "die", "Au\u00b7gen", "Dein", ",", "voll", "Glut", "und", "Dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daran ich st\u00fcndlich Mast und Kiel verbrenne.", "tokens": ["Da\u00b7ran", "ich", "st\u00fcnd\u00b7lich", "Mast", "und", "Kiel", "ver\u00b7bren\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "NN", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch West und Osten meinem Liederboote,", "tokens": ["Doch", "West", "und", "Os\u00b7ten", "mei\u00b7nem", "Lie\u00b7der\u00b7boo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist die rechte und die linke Wange,", "tokens": ["Das", "ist", "die", "rech\u00b7te", "und", "die", "lin\u00b7ke", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die t\u00e4glich stehn im Fr\u00fch- und Abendrote.", "tokens": ["Die", "t\u00e4g\u00b7lich", "stehn", "im", "Fr\u00fch", "und", "A\u00b7ben\u00b7dro\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPRART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}