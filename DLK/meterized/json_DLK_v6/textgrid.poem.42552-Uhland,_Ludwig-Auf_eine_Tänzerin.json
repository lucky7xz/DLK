{"textgrid.poem.42552": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine T\u00e4nzerin", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du den leichten Reigen f\u00fchrest,", "tokens": ["Wenn", "du", "den", "leich\u00b7ten", "Rei\u00b7gen", "f\u00fch\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du den Boden kaum ber\u00fchrest,", "tokens": ["Wenn", "du", "den", "Bo\u00b7den", "kaum", "be\u00b7r\u00fch\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hinschwebend in der Jugend Glanz:", "tokens": ["Hin\u00b7schwe\u00b7bend", "in", "der", "Ju\u00b7gend", "Glanz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In jedem Aug ist dann zu lesen,", "tokens": ["In", "je\u00b7dem", "Aug", "ist", "dann", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du seiest nicht ein irdisch Wesen,", "tokens": ["Du", "sei\u00b7est", "nicht", "ein", "ir\u00b7disch", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du seiest \u00c4ther, Seele ganz.", "tokens": ["Du", "sei\u00b7est", "\u00c4\u00b7ther", ",", "See\u00b7le", "ganz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir aber grauet: wenn nach oben", "tokens": ["Mir", "a\u00b7ber", "grau\u00b7et", ":", "wenn", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$.", "KOUS", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du w\u00fcrdest pl\u00f6tzlich nun enthoben,", "tokens": ["Du", "w\u00fcr\u00b7dest", "pl\u00f6tz\u00b7lich", "nun", "ent\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie w\u00e4rest, Seele, du bereit? \u2013", "tokens": ["Wie", "w\u00e4\u00b7rest", ",", "See\u00b7le", ",", "du", "be\u00b7reit", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "$,", "NN", "$,", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohlan! der sich auf Blumen schaukelt,", "tokens": ["Wo\u00b7hlan", "!", "der", "sich", "auf", "Blu\u00b7men", "schau\u00b7kelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Schmetterling, der ewig gaukelt,", "tokens": ["Der", "Schmet\u00b7ter\u00b7ling", ",", "der", "e\u00b7wig", "gau\u00b7kelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist Sinnbild der Unsterblichkeit.", "tokens": ["Ist", "Sinn\u00b7bild", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}