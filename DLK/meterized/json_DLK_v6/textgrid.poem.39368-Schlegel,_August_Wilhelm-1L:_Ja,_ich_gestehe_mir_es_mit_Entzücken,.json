{"textgrid.poem.39368": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, ich gestehe mir es mit Entz\u00fccken,", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, ich gestehe mir es mit Entz\u00fccken,", "tokens": ["Ja", ",", "ich", "ge\u00b7ste\u00b7he", "mir", "es", "mit", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin dir hingegeben:", "tokens": ["Ich", "bin", "dir", "hin\u00b7ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch, s\u00fc\u00dfer Freund, bewahr' es wie dein Leben.", "tokens": ["Doch", ",", "s\u00fc\u00b7\u00dfer", "Freund", ",", "be\u00b7wahr'", "es", "wie", "dein", "Le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Lieb' ist nichts so eigen,", "tokens": ["Der", "Lieb'", "ist", "nichts", "so", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als sich mit holder Sch\u00fcchternheit umschleiern.", "tokens": ["Als", "sich", "mit", "hol\u00b7der", "Sch\u00fcch\u00b7tern\u00b7heit", "um\u00b7schlei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat sie schon nicht zu sorgen,", "tokens": ["Hat", "sie", "schon", "nicht", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und d\u00fcrfte k\u00fchn sich zeigen,", "tokens": ["Und", "d\u00fcrf\u00b7te", "k\u00fchn", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Will sie geheim doch ihre Weihe feiern", "tokens": ["Will", "sie", "ge\u00b7heim", "doch", "ih\u00b7re", "Wei\u00b7he", "fei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und bricht der helle Morgen", "tokens": ["Und", "bricht", "der", "hel\u00b7le", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf g\u00fcnst'ge Schatten ein, die sie verborgen", "tokens": ["Auf", "g\u00fcnst'\u00b7ge", "Schat\u00b7ten", "ein", ",", "die", "sie", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So wird verst\u00f6rt, mit Beben,", "tokens": ["So", "wird", "ver\u00b7st\u00f6rt", ",", "mit", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Auch ihrer Tr\u00e4ume zartester entschweben.", "tokens": ["Auch", "ih\u00b7rer", "Tr\u00e4u\u00b7me", "zar\u00b7tes\u00b7ter", "ent\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich scheue nicht das Necken", "tokens": ["Ich", "scheu\u00b7e", "nicht", "das", "Ne\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gespitzter Zungen, noch des P\u00f6bels Tadel.", "tokens": ["Ge\u00b7spitz\u00b7ter", "Zun\u00b7gen", ",", "noch", "des", "P\u00f6\u00b7bels", "Ta\u00b7del", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das wollt' ich muthig leiden", "tokens": ["Das", "wollt'", "ich", "mut\u00b7hig", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns aller Welt entdecken,", "tokens": ["Uns", "al\u00b7ler", "Welt", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Stolz auf die Wahl und meines Herzens Adel.", "tokens": ["Stolz", "auf", "die", "Wahl", "und", "mei\u00b7nes", "Her\u00b7zens", "A\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Allein von diesen Freuden", "tokens": ["Al\u00b7lein", "von", "die\u00b7sen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Beneid' ich Andern selber das Beneiden,", "tokens": ["Be\u00b7neid'", "ich", "An\u00b7dern", "sel\u00b7ber", "das", "Be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kein fremder Sinn soll streben,", "tokens": ["Kein", "frem\u00b7der", "Sinn", "soll", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zur Ahndung unsers Gl\u00fccks sich zu erheben.", "tokens": ["Zur", "Ahn\u00b7dung", "un\u00b7sers", "Gl\u00fccks", "sich", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl mu\u00df es bitter schmerzen,", "tokens": ["Wohl", "mu\u00df", "es", "bit\u00b7ter", "schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erlischt, in des Verge\u00dfens Strom getauchet,", "tokens": ["Er\u00b7lischt", ",", "in", "des", "Ver\u00b7ge\u00b7\u00dfens", "Strom", "ge\u00b7tau\u00b7chet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Liebe sch\u00f6nes Gl\u00fchen.", "tokens": ["Der", "Lie\u00b7be", "sch\u00f6\u00b7nes", "Gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch wenn aus stillem Herzen", "tokens": ["Doch", "wenn", "aus", "stil\u00b7lem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du von den Lippen erst das Wort gehauchet,", "tokens": ["Du", "von", "den", "Lip\u00b7pen", "erst", "das", "Wort", "ge\u00b7hau\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kannst du's zur\u00fcck nicht ziehen:", "tokens": ["Kannst", "du's", "zu\u00b7r\u00fcck", "nicht", "zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKVZ", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum werde nimmer dem Verrath verziehen;", "tokens": ["Drum", "wer\u00b7de", "nim\u00b7mer", "dem", "Ver\u00b7rath", "ver\u00b7zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn neue W\u00fcnsche seinen Schlaf beleben.", "tokens": ["Wenn", "neu\u00b7e", "W\u00fcn\u00b7sche", "sei\u00b7nen", "Schlaf", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es dr\u00e4ngen sich die Seelen", "tokens": ["Es", "dr\u00e4n\u00b7gen", "sich", "die", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Liebenden im Blick an's Licht der Sonne,", "tokens": ["Der", "Lie\u00b7ben\u00b7den", "im", "Blick", "an's", "Licht", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie hinter Schlo\u00df und Riegel;", "tokens": ["Wie", "hin\u00b7ter", "Schlo\u00df", "und", "Rie\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie innigst zu verm\u00e4hlen,", "tokens": ["Sie", "in\u00b7nigst", "zu", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erschlie\u00dfet erst der Ku\u00df das Thor der Wonne,", "tokens": ["Er\u00b7schlie\u00b7\u00dfet", "erst", "der", "Ku\u00df", "das", "Thor", "der", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und l\u00f6set ihre Fl\u00fcgel.", "tokens": ["Und", "l\u00f6\u00b7set", "ih\u00b7re", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt aber sei dir dieser Ku\u00df als Siegel", "tokens": ["Jetzt", "a\u00b7ber", "sei", "dir", "die\u00b7ser", "Ku\u00df", "als", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PDAT", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Schweigens mitgegeben,", "tokens": ["Des", "Schwei\u00b7gens", "mit\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Das keine Macht verm\u00f6ge wegzuheben.", "tokens": ["Das", "kei\u00b7ne", "Macht", "ver\u00b7m\u00f6\u00b7ge", "weg\u00b7zu\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}