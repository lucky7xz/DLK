{"dta.poem.130": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Brief an  Charlotten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Charlotte la\u00df dis Blatt zu deinen H\u00e4nden fliegen/", "tokens": ["Char\u00b7lot\u00b7te", "la\u00df", "dis", "Blatt", "zu", "dei\u00b7nen", "H\u00e4n\u00b7den", "flie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df/ du weist es schon/ was es verborgen h\u00e4lt.", "tokens": ["Ich", "wei\u00df", "/", "du", "weist", "es", "schon", "/", "was", "es", "ver\u00b7bor\u00b7gen", "h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "ADV", "$(", "PWS", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Augen/ so den Geist und den Verstand besiegen/", "tokens": ["Die", "Au\u00b7gen", "/", "so", "den", "Geist", "und", "den", "Ver\u00b7stand", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "KON", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verschaffen/ da\u00df es dir leicht zu errahten f\u00e4llt.", "tokens": ["Ver\u00b7schaf\u00b7fen", "/", "da\u00df", "es", "dir", "leicht", "zu", "er\u00b7rah\u00b7ten", "f\u00e4llt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Zwar schreib ich etwas frey durch Fleisch und Blut getrieben/", "tokens": ["Zwar", "schreib", "ich", "et\u00b7was", "frey", "durch", "Fleisch", "und", "Blut", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Regung/ so mich qu\u00e4hlt/ mehr als zu hefftig ist/", "tokens": ["Die", "Re\u00b7gung", "/", "so", "mich", "qu\u00e4hlt", "/", "mehr", "als", "zu", "heff\u00b7tig", "ist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$(", "PIAT", "KOKOM", "PTKA", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch nein/ es ist nicht frey du wilst ich soll dich lieben/", "tokens": ["Doch", "nein", "/", "es", "ist", "nicht", "frey", "du", "wilst", "ich", "soll", "dich", "lie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "PPER", "VMFIN", "PPER", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und zeigst dadurch/ da\u00df du der Schreiber selber bist.", "tokens": ["Und", "zeigst", "da\u00b7durch", "/", "da\u00df", "du", "der", "Schrei\u00b7ber", "sel\u00b7ber", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$(", "KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich habe dich geliebt/ ich liebe dich noch ferner/", "tokens": ["Ich", "ha\u00b7be", "dich", "ge\u00b7liebt", "/", "ich", "lie\u00b7be", "dich", "noch", "fer\u00b7ner", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$(", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Allein die Gunst wird noch umsonst von mir gesucht.", "tokens": ["Al\u00b7lein", "die", "Gunst", "wird", "noch", "um\u00b7sonst", "von", "mir", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Reichst du mir Salsen her/ und ", "tokens": ["Reichst", "du", "mir", "Sal\u00b7sen", "her", "/", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PPER", "NN", "PTKVZ", "$(", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Es k\u00fcsset zwar mein Geist die angenehmen Wangen/", "tokens": ["Es", "k\u00fcs\u00b7set", "zwar", "mein", "Geist", "die", "an\u00b7ge\u00b7neh\u00b7men", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er k\u00fc\u00dft den Purpur-Mund/ und deine Schwaanen-Brust/", "tokens": ["Er", "k\u00fc\u00dft", "den", "Pur\u00b7pur\u00b7Mund", "/", "und", "dei\u00b7ne", "Schwaa\u00b7nen\u00b7Brust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Allein/ was ists? er kan draus keinen Trost erlangen", "tokens": ["Al\u00b7lein", "/", "was", "ists", "?", "er", "kan", "draus", "kei\u00b7nen", "Trost", "er\u00b7lan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "PWS", "VAFIN", "$.", "PPER", "VMFIN", "PAV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die leere ", "tokens": ["Die", "lee\u00b7re"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Wann der erhitzte Mund die sch\u00f6nen Rosen-Lippen/", "tokens": ["Wann", "der", "er\u00b7hitz\u00b7te", "Mund", "die", "sch\u00f6\u00b7nen", "Ro\u00b7sen\u00b7\u00b7Lip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wo Milch und Honig fleust/ nicht darf zum Labsahl sehn/", "tokens": ["Wo", "Milch", "und", "Ho\u00b7nig", "fleust", "/", "nicht", "darf", "zum", "Lab\u00b7sahl", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$(", "PTKNEG", "VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So gleichet er dem Schiff/ da\u00df zum Magnetschen Klippen/", "tokens": ["So", "glei\u00b7chet", "er", "dem", "Schiff", "/", "da\u00df", "zum", "Mag\u00b7net\u00b7schen", "Klip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "KOUS", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "So solches nach sich ziehn/ mu\u00df ins Verderben gehn.", "tokens": ["So", "sol\u00b7ches", "nach", "sich", "ziehn", "/", "mu\u00df", "ins", "Ver\u00b7der\u00b7ben", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "APPR", "PRF", "VVINF", "$(", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wann aber ihn die Gunst der Sch\u00f6nen lieb gewonnen", "tokens": ["Wann", "a\u00b7ber", "ihn", "die", "Gunst", "der", "Sch\u00f6\u00b7nen", "lieb", "ge\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "ART", "NN", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn ein beliebter Blick aus holden Augen spielt/", "tokens": ["Wenn", "ein", "be\u00b7lieb\u00b7ter", "Blick", "aus", "hol\u00b7den", "Au\u00b7gen", "spielt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die an zu sehen seyn als zweene Gnaden-Sonnen/", "tokens": ["Die", "an", "zu", "se\u00b7hen", "seyn", "als", "zwee\u00b7ne", "Gna\u00b7den\u00b7Son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PTKZU", "VVINF", "VAINF", "KOKOM", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wenn er sich im Schnee des zarten Busens k\u00fchlt:", "tokens": ["Und", "wenn", "er", "sich", "im", "Schnee", "des", "zar\u00b7ten", "Bu\u00b7sens", "k\u00fchlt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So f\u00e4hret er daher in lauter sanfften Wellen/", "tokens": ["So", "f\u00e4h\u00b7ret", "er", "da\u00b7her", "in", "lau\u00b7ter", "sanff\u00b7ten", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das Gl\u00fccke steht ihm bey weil ", "tokens": ["Das", "Gl\u00fc\u00b7cke", "steht", "ihm", "bey", "weil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Kein harter Sturm kan es in seiner Farth zerschellen", "tokens": ["Kein", "har\u00b7ter", "Sturm", "kan", "es", "in", "sei\u00b7ner", "Farth", "zer\u00b7schel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil es sich nicht zu weit von seinen Haven reist.", "tokens": ["Weil", "es", "sich", "nicht", "zu", "weit", "von", "sei\u00b7nen", "Ha\u00b7ven", "reist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nun stehet es bey dir du kanst mich gl\u00fccklich machen", "tokens": ["Nun", "ste\u00b7het", "es", "bey", "dir", "du", "kanst", "mich", "gl\u00fcck\u00b7lich", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sprich nur: ", "tokens": ["Sprich", "nur", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "ADV", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Ich werde gantz entz\u00fcckt vor s\u00fcsser Wollust lachen", "tokens": ["Ich", "wer\u00b7de", "gantz", "ent\u00b7z\u00fcckt", "vor", "s\u00fcs\u00b7ser", "Wol\u00b7lust", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn mich dein sch\u00f6ner Schoo\u00df nimmt in dem Haven ein.", "tokens": ["Wenn", "mich", "dein", "sch\u00f6\u00b7ner", "Schoo\u00df", "nimmt", "in", "dem", "Ha\u00b7ven", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}