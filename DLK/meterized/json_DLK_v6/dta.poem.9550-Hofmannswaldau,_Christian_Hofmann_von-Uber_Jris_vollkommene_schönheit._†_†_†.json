{"dta.poem.9550": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber Jris vollkommene sch\u00f6nheit.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jris (klagen sich die alten)", "tokens": ["Jris", "(", "kla\u00b7gen", "sich", "die", "al\u00b7ten", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PRF", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist in dieser gantzen stadt", "tokens": ["Ist", "in", "die\u00b7ser", "gant\u00b7zen", "stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr das sch\u00f6nste kind zu halten/", "tokens": ["F\u00fcr", "das", "sch\u00f6ns\u00b7te", "kind", "zu", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weill sie gar viel sch\u00f6nes hat.", "tokens": ["Weill", "sie", "gar", "viel", "sch\u00f6\u00b7nes", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey den andern/ die uns fliehen/", "tokens": ["Bey", "den", "an\u00b7dern", "/", "die", "uns", "flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$(", "PRELS", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht man in geringer zahl", "tokens": ["Sieht", "man", "in", "ge\u00b7rin\u00b7ger", "zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur gewisse blumen bl\u00fchen:", "tokens": ["Nur", "ge\u00b7wis\u00b7se", "blu\u00b7men", "bl\u00fc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber Jris/ uns zur qual/", "tokens": ["A\u00b7ber", "Jris", "/", "uns", "zur", "qual", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$(", "PPER", "APPRART", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Ist ein gantzer blumen-thal.", "tokens": ["Ist", "ein", "gant\u00b7zer", "blu\u00b7men\u00b7thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}