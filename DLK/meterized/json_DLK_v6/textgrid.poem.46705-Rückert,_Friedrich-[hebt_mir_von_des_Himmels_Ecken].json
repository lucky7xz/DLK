{"textgrid.poem.46705": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[hebt mir von des Himmels Ecken]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hebt mir von des Himmels Ecken", "tokens": ["Hebt", "mir", "von", "des", "Him\u00b7mels", "E\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur die tr\u00fcben Wolkendecken,", "tokens": ["Nur", "die", "tr\u00fc\u00b7ben", "Wol\u00b7ken\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft mich nur ins Klare sehn,", "tokens": ["La\u00dft", "mich", "nur", "ins", "Kla\u00b7re", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterne drehn,", "tokens": ["Ster\u00b7ne", "drehn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Monde gehn,", "tokens": ["Mon\u00b7de", "gehn", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und die Sonn' im Glanze stehn.", "tokens": ["Und", "die", "Sonn'", "im", "Glan\u00b7ze", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Denn im Sonn- und Mondenglanze", "tokens": ["Denn", "im", "Sonn", "und", "Mon\u00b7den\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "TRUNC", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im stillen Sternenkranze", "tokens": ["Und", "im", "stil\u00b7len", "Ster\u00b7nen\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4chelt mir ein Angesicht;", "tokens": ["L\u00e4\u00b7chelt", "mir", "ein", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6r' ich nicht,", "tokens": ["H\u00f6r'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wie es spricht:", "tokens": ["Wie", "es", "spricht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wir sind ewig Licht im Licht?", "tokens": ["Wir", "sind", "e\u00b7wig", "Licht", "im", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}