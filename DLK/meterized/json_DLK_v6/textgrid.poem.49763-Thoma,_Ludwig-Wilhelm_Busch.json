{"textgrid.poem.49763": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Wilhelm Busch", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst dreimal Hoch und dann ein Tusch", "tokens": ["Erst", "drei\u00b7mal", "Hoch", "und", "dann", "ein", "Tusch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem hochverehrten Meister Busch!", "tokens": ["Dem", "hoch\u00b7ver\u00b7ehr\u00b7ten", "Meis\u00b7ter", "Busch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sitzt du nun seit manchem Tage", "tokens": ["Da", "sitzt", "du", "nun", "seit", "man\u00b7chem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beim Bienenkorb am Rosenhage,", "tokens": ["Beim", "Bie\u00b7nen\u00b7korb", "am", "Ro\u00b7sen\u00b7ha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die laute Welt ist fremd f\u00fcr dich;", "tokens": ["Die", "lau\u00b7te", "Welt", "ist", "fremd", "f\u00fcr", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du fl\u00f6test nur mehr innerlich", "tokens": ["Du", "fl\u00f6\u00b7test", "nur", "mehr", "in\u00b7ner\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und h\u00e4ltst dich fern von dem Bestreben,", "tokens": ["Und", "h\u00e4ltst", "dich", "fern", "von", "dem", "Be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Uns andern auch was abzugeben.", "tokens": ["Uns", "an\u00b7dern", "auch", "was", "ab\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wie ist verschieden doch die Dichtung", "tokens": ["Wie", "ist", "ver\u00b7schie\u00b7den", "doch", "die", "Dich\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In dieser und in jeder Richtung!", "tokens": ["In", "die\u00b7ser", "und", "in", "je\u00b7der", "Rich\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "KON", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der eine wird erst sp\u00e4t Genie,", "tokens": ["Der", "ei\u00b7ne", "wird", "erst", "sp\u00e4t", "Ge\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der andre wird es viel zu fr\u00fch,", "tokens": ["Der", "and\u00b7re", "wird", "es", "viel", "zu", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und man bemerkt nur \u00e4u\u00dferst selten,", "tokens": ["Und", "man", "be\u00b7merkt", "nur", "\u00e4u\u00b7\u00dferst", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df hier Naturgesetze gelten.", "tokens": ["Da\u00df", "hier", "Na\u00b7tur\u00b7ge\u00b7set\u00b7ze", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Oft kommt die Frucht schon vor der Bl\u00fcte", "tokens": ["Oft", "kommt", "die", "Frucht", "schon", "vor", "der", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und ist dann von besondrer G\u00fcte.", "tokens": ["Und", "ist", "dann", "von", "be\u00b7sond\u00b7rer", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wir sehen auch Verschiedenheit", "tokens": ["Wir", "se\u00b7hen", "auch", "Ver\u00b7schie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "In Anbelang der Fruchtbarkeit.", "tokens": ["In", "An\u00b7be\u00b7lang", "der", "Frucht\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Bei diesem geht es \u00e4u\u00dferst sp\u00e4rlich,", "tokens": ["Bei", "die\u00b7sem", "geht", "es", "\u00e4u\u00b7\u00dferst", "sp\u00e4r\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der andere entbindet j\u00e4hrlich", "tokens": ["Der", "an\u00b7de\u00b7re", "ent\u00b7bin\u00b7det", "j\u00e4hr\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und macht dem guten Publiko", "tokens": ["Und", "macht", "dem", "gu\u00b7ten", "Pub\u00b7li\u00b7ko"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "In jedem Herbst das Leben froh.", "tokens": ["In", "je\u00b7dem", "Herbst", "das", "Le\u00b7ben", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Jetzt aber taucht die Frage auf:", "tokens": ["Jetzt", "a\u00b7ber", "taucht", "die", "Fra\u00b7ge", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wann endet wohl des Dichters Lauf?", "tokens": ["Wann", "en\u00b7det", "wohl", "des", "Dich\u00b7ters", "Lauf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Gew\u00f6hnlich mit des Lebens Tagen;", "tokens": ["Ge\u00b7w\u00f6hn\u00b7lich", "mit", "des", "Le\u00b7bens", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Dies l\u00e4\u00dft sich hier authentisch sagen,", "tokens": ["Dies", "l\u00e4\u00dft", "sich", "hier", "au\u00b7then\u00b7tisch", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.27": {"text": "Weil keiner gern die Quelle stopft,", "tokens": ["Weil", "kei\u00b7ner", "gern", "die", "Quel\u00b7le", "stopft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Auch wenn sie noch so \u00e4rmlich tropft;", "tokens": ["Auch", "wenn", "sie", "noch", "so", "\u00e4rm\u00b7lich", "tropft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und mancher Greis saugt noch am Busen", "tokens": ["Und", "man\u00b7cher", "Greis", "saugt", "noch", "am", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Der armen, vielgeplagten Musen", "tokens": ["Der", "ar\u00b7men", ",", "viel\u00b7ge\u00b7plag\u00b7ten", "Mu\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und glaubt, auch wenn er lange soff,", "tokens": ["Und", "glaubt", ",", "auch", "wenn", "er", "lan\u00b7ge", "soff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Es fehle nie an Nahrungsstoff.", "tokens": ["Es", "feh\u00b7le", "nie", "an", "Nah\u00b7rungs\u00b7stoff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Fast jeder nimmt ins k\u00fchle Grab", "tokens": ["Fast", "je\u00b7der", "nimmt", "ins", "k\u00fch\u00b7le", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Ein angefangnes Werk hinab.", "tokens": ["Ein", "an\u00b7ge\u00b7fang\u00b7nes", "Werk", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Dann schreibt der Kritiker: \u00bbWie schade!", "tokens": ["Dann", "schreibt", "der", "Kri\u00b7ti\u00b7ker", ":", "\u00bb", "Wie", "scha\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Dies war sein bestes ja gerade!", "tokens": ["Dies", "war", "sein", "bes\u00b7tes", "ja", "ge\u00b7ra\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Es ist wahrhaftig ungeschickt,", "tokens": ["Es", "ist", "wahr\u00b7haf\u00b7tig", "un\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Da\u00df hier die Parze abgezwickt.\u00ab", "tokens": ["Da\u00df", "hier", "die", "Par\u00b7ze", "ab\u00b7ge\u00b7zwickt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Was aber hat man denn posthum", "tokens": ["Was", "a\u00b7ber", "hat", "man", "denn", "post\u00b7hum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "PIS", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Auch von dem sch\u00f6nsten Dichterruhm?", "tokens": ["Auch", "von", "dem", "sch\u00f6ns\u00b7ten", "Dich\u00b7ter\u00b7ruhm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Du, Meister Busch, hast dies begriffen,", "tokens": ["Du", ",", "Meis\u00b7ter", "Busch", ",", "hast", "dies", "be\u00b7grif\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Du hast vergn\u00fcgt so lang gepfiffen,", "tokens": ["Du", "hast", "ver\u00b7gn\u00fcgt", "so", "lang", "ge\u00b7pfif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Als es dich selber noch erfreute.", "tokens": ["Als", "es", "dich", "sel\u00b7ber", "noch", "er\u00b7freu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Dann sagtest du: \u00bbIhr lieben Leute,", "tokens": ["Dann", "sag\u00b7test", "du", ":", "\u00bb", "Ihr", "lie\u00b7ben", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Ich d\u00e4chte nun, es sei genug,", "tokens": ["Ich", "d\u00e4ch\u00b7te", "nun", ",", "es", "sei", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Wer fr\u00fcher aufh\u00f6rt, handelt klug.", "tokens": ["Wer", "fr\u00fc\u00b7her", "auf\u00b7h\u00f6rt", ",", "han\u00b7delt", "klug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Man wird so mit vergn\u00fcgtem Sinne", "tokens": ["Man", "wird", "so", "mit", "ver\u00b7gn\u00fcg\u00b7tem", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Der Epiloge Sch\u00f6nheit inne", "tokens": ["Der", "E\u00b7pi\u00b7lo\u00b7ge", "Sch\u00f6n\u00b7heit", "in\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Und liest noch selbst den ganzen Mist,", "tokens": ["Und", "liest", "noch", "selbst", "den", "gan\u00b7zen", "Mist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Indessen man am Leben ist.\u00ab", "tokens": ["In\u00b7des\u00b7sen", "man", "am", "Le\u00b7ben", "ist", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}