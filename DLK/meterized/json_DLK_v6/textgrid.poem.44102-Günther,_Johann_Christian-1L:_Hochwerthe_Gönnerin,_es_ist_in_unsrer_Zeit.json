{"textgrid.poem.44102": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hochwerthe G\u00f6nnerin, es ist in unsrer Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochwerthe G\u00f6nnerin, es ist in unsrer Zeit", "tokens": ["Hoch\u00b7wert\u00b7he", "G\u00f6n\u00b7ne\u00b7rin", ",", "es", "ist", "in", "uns\u00b7rer", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein seltnes Wunderwerck, wenn Damen deines gleichen", "tokens": ["Ein", "selt\u00b7nes", "Wun\u00b7der\u00b7werck", ",", "wenn", "Da\u00b7men", "dei\u00b7nes", "glei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "NN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey so viel \u00c4rgern\u00fc\u00df der tollen Eitelkeit", "tokens": ["Bey", "so", "viel", "\u00c4r\u00b7ger\u00b7n\u00fc\u00df", "der", "tol\u00b7len", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Die Hand, so sonst verf\u00fchrt, nach Andachtsschriften reichen.", "tokens": ["Die", "Hand", ",", "so", "sonst", "ver\u00b7f\u00fchrt", ",", "nach", "An\u00b7dachts\u00b7schrif\u00b7ten", "rei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "VVPP", "$,", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die meisten, giebt man Acht, sind auch in unsrer Stadt,", "tokens": ["Die", "meis\u00b7ten", ",", "giebt", "man", "Acht", ",", "sind", "auch", "in", "uns\u00b7rer", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "VVFIN", "PIS", "CARD", "$,", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch keine hier genennt, verdorbne Modeschwestern,", "tokens": ["Doch", "kei\u00b7ne", "hier", "ge\u00b7nennt", ",", "ver\u00b7dorb\u00b7ne", "Mo\u00b7de\u00b7schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADV", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey welchen Scham und Zucht kein ofnes Ohr mehr hat", "tokens": ["Bey", "wel\u00b7chen", "Scham", "und", "Zucht", "kein", "of\u00b7nes", "Ohr", "mehr", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "PIAT", "ADJA", "NN", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die gleichwohl dabey die Allerfr\u00f6mmsten l\u00e4stern.", "tokens": ["Und", "die", "gleich\u00b7wohl", "da\u00b7bey", "die", "Al\u00b7ler\u00b7fr\u00b7\u00f6mms\u00b7ten", "l\u00e4s\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "PAV", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ein s\u00fcndlich-sch\u00f6ner Brief, ein sch\u00e4ndlicher Roman,", "tokens": ["Ein", "s\u00fcnd\u00b7lich\u00b7sch\u00f6\u00b7ner", "Brief", ",", "ein", "sch\u00e4nd\u00b7li\u00b7cher", "Ro\u00b7man", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein schl\u00fcpfrig Buhlerlied f\u00fcllt ihnen Schranck und Taschen,", "tokens": ["Ein", "schl\u00fcpf\u00b7rig", "Buh\u00b7ler\u00b7lied", "f\u00fcllt", "ih\u00b7nen", "Schranck", "und", "Ta\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wenn der Dichter nur fein Zoten rei\u00dfen kan,", "tokens": ["Und", "wenn", "der", "Dich\u00b7ter", "nur", "fein", "Zo\u00b7ten", "rei\u00b7\u00dfen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kommt gleich ein Jahrmarcktslohn von Handschuh, Band und Flaschen.", "tokens": ["Kommt", "gleich", "ein", "Jahr\u00b7marckts\u00b7lohn", "von", "Hand\u00b7schuh", ",", "Band", "und", "Fla\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich habe meinen Kiel der Thorheit oft geliehn", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7nen", "Kiel", "der", "Thor\u00b7heit", "oft", "ge\u00b7liehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mancher Docke schon an den Galan geschrieben;", "tokens": ["Und", "man\u00b7cher", "Do\u00b7cke", "schon", "an", "den", "Ga\u00b7lan", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die S\u00fcnde reut mich jezt, so klein sie ehmahls schien;", "tokens": ["Die", "S\u00fcn\u00b7de", "reut", "mich", "jezt", ",", "so", "klein", "sie", "eh\u00b7mahls", "schien", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch bin ich stets ein Feind der Eitelkeit geblieben.", "tokens": ["Doch", "bin", "ich", "stets", "ein", "Feind", "der", "Ei\u00b7tel\u00b7keit", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dich unterscheidet l\u00e4ngst von solcher b\u00f6sen Art", "tokens": ["Dich", "un\u00b7ter\u00b7schei\u00b7det", "l\u00e4ngst", "von", "sol\u00b7cher", "b\u00f6\u00b7sen", "Art"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Andacht ohne Schein und dies bescheidne Wesen,", "tokens": ["Die", "An\u00b7dacht", "oh\u00b7ne", "Schein", "und", "dies", "be\u00b7scheid\u00b7ne", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wodurch dein kluger Schaz mit Recht gefe\u00dfelt ward,", "tokens": ["Wo\u00b7durch", "dein", "klu\u00b7ger", "Schaz", "mit", "Recht", "ge\u00b7fe\u00b7\u00dfelt", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bevor dich seine Wahl zur Liebsten auserlesen.", "tokens": ["Be\u00b7vor", "dich", "sei\u00b7ne", "Wahl", "zur", "Liebs\u00b7ten", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hier kommt nun, wie du siehst, das nechst begehrte Lied;", "tokens": ["Hier", "kommt", "nun", ",", "wie", "du", "siehst", ",", "das", "nechst", "be\u00b7gehr\u00b7te", "Lied", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "PRELS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wirst's nicht nach Verdienst, nein, nur nach Sanftmuth sch\u00e4zen.", "tokens": ["Du", "wir\u00b7st's", "nicht", "nach", "Ver\u00b7dienst", ",", "nein", ",", "nur", "nach", "Sanft\u00b7muth", "sch\u00e4\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "NN", "$,", "PTKANT", "$,", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Ich weis wohl, da\u00df die Kunst darinnen mager sieht,", "tokens": ["Ich", "weis", "wohl", ",", "da\u00df", "die", "Kunst", "da\u00b7rin\u00b7nen", "ma\u00b7ger", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch mein Gehorsam meint, er k\u00f6nne dies ersezen.", "tokens": ["Doch", "mein", "Ge\u00b7hor\u00b7sam", "meint", ",", "er", "k\u00f6n\u00b7ne", "dies", "er\u00b7se\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VMFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}