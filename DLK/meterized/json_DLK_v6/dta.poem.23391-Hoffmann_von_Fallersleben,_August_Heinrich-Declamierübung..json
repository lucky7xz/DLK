{"dta.poem.23391": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Declamier\u00fcbung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem sch\u00f6nen Land' ein V\u00f6lkchen war,", "tokens": ["In", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Land'", "ein", "V\u00f6lk\u00b7chen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das lebt' in tiefem Frieden manches Jahr.", "tokens": ["Das", "lebt'", "in", "tie\u00b7fem", "Frie\u00b7den", "man\u00b7ches", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An einem K\u00f6nig hatten sie genug,", "tokens": ["An", "ei\u00b7nem", "K\u00f6\u00b7nig", "hat\u00b7ten", "sie", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gemein war allen was der Boden trug,", "tokens": ["Ge\u00b7mein", "war", "al\u00b7len", "was", "der", "Bo\u00b7den", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIAT", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur da\u00df sich jeder zweimal scheren lie\u00df,", "tokens": ["Nur", "da\u00df", "sich", "je\u00b7der", "zwei\u00b7mal", "sche\u00b7ren", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PRF", "PIS", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sonst war's ein Leben wie im Paradies.", "tokens": ["Sonst", "wa\u00b7r's", "ein", "Le\u00b7ben", "wie", "im", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr K\u00f6nig hie\u00df Leithammel nur schlecht weg,", "tokens": ["Ihr", "K\u00f6\u00b7nig", "hie\u00df", "Leit\u00b7ham\u00b7mel", "nur", "schlecht", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er kannt' im Lande jeden Weg und Steg,", "tokens": ["Er", "kannt'", "im", "Lan\u00b7de", "je\u00b7den", "Weg", "und", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "War stets auf seines Volkes Heil bedacht", "tokens": ["War", "stets", "auf", "sei\u00b7nes", "Vol\u00b7kes", "Heil", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und f\u00fchrte sie gar gut bei Tag und Nacht.", "tokens": ["Und", "f\u00fchr\u00b7te", "sie", "gar", "gut", "bei", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nie h\u00f6rte man von Unzufriedenheit,", "tokens": ["Nie", "h\u00f6r\u00b7te", "man", "von", "Un\u00b7zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Umtrieben, Meuterei und Zwist und Streit.", "tokens": ["Um\u00b7trie\u00b7ben", ",", "Meu\u00b7te\u00b7rei", "und", "Zwist", "und", "Streit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch schlichen eines Tags sich B\u00f6ck' herein.", "tokens": ["Doch", "schli\u00b7chen", "ei\u00b7nes", "Tags", "sich", "B\u00f6ck'", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo B\u00f6cke sind, wird immer Zwietracht sein.", "tokens": ["Wo", "B\u00f6\u00b7cke", "sind", ",", "wird", "im\u00b7mer", "Zwiet\u00b7racht", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$,", "VAFIN", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die B\u00f6ck' erhoben bald ein gro\u00df Geschrei:", "tokens": ["Die", "B\u00f6ck'", "er\u00b7ho\u00b7ben", "bald", "ein", "gro\u00df", "Ge\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Schafe, wi\u00dft nur nicht \u2014 ihr seid nicht frei.", "tokens": ["Ihr", "Scha\u00b7fe", ",", "wi\u00dft", "nur", "nicht", "ihr", "seid", "nicht", "frei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "PTKNEG", "$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Das wahre Gl\u00fcck liegt in der Freiheit nur,", "tokens": ["Das", "wah\u00b7re", "Gl\u00fcck", "liegt", "in", "der", "Frei\u00b7heit", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und schuf uns nicht zur Freiheit die Natur?", "tokens": ["Und", "schuf", "uns", "nicht", "zur", "Frei\u00b7heit", "die", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da ward es erst den armen Schafen klar,", "tokens": ["Da", "ward", "es", "erst", "den", "ar\u00b7men", "Scha\u00b7fen", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df frei doch eigentlich kein einzig war.", "tokens": ["Da\u00df", "frei", "doch", "ei\u00b7gent\u00b7lich", "kein", "ein\u00b7zig", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ADV", "PIAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr B\u00f6cke, sprachen sie, ihr habt ganz Recht!", "tokens": ["Ihr", "B\u00f6\u00b7cke", ",", "spra\u00b7chen", "sie", ",", "ihr", "habt", "ganz", "Recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht frei ist, scheint es, unser brav Geschlecht:", "tokens": ["Nicht", "frei", "ist", ",", "scheint", "es", ",", "un\u00b7ser", "brav", "Ge\u00b7schlecht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Thut Alles was ihr wollt, euch sei's verg\u00f6nnt,", "tokens": ["Thut", "Al\u00b7les", "was", "ihr", "wollt", ",", "euch", "sei's", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PWS", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn ihr nur Freiheit uns gewinnen k\u00f6nnt.", "tokens": ["Wenn", "ihr", "nur", "Frei\u00b7heit", "uns", "ge\u00b7win\u00b7nen", "k\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch war es schon vor Anbeginn der Nacht", "tokens": ["Doch", "war", "es", "schon", "vor", "An\u00b7be\u00b7ginn", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem edlen K\u00f6nig Alles hinterbracht.", "tokens": ["Dem", "ed\u00b7len", "K\u00f6\u00b7nig", "Al\u00b7les", "hin\u00b7ter\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er sprach darauf: wohlan, ich danke ab,", "tokens": ["Er", "sprach", "da\u00b7rauf", ":", "wo\u00b7hlan", ",", "ich", "dan\u00b7ke", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$.", "ADV", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich lege heut schon nieder meinen Stab;", "tokens": ["Ich", "le\u00b7ge", "heut", "schon", "nie\u00b7der", "mei\u00b7nen", "Stab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sobald ihr \u00fcber ", "tokens": ["So\u00b7bald", "ihr", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Was dann geschehen soll, bin ich's bereit.", "tokens": ["Was", "dann", "ge\u00b7sche\u00b7hen", "soll", ",", "bin", "ich's", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$,", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da fing im Volke Streit und Hader an,", "tokens": ["Da", "fing", "im", "Vol\u00b7ke", "Streit", "und", "Ha\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df eiligst jeder nach dem Stalle rann.", "tokens": ["Da\u00df", "ei\u00b7ligst", "je\u00b7der", "nach", "dem", "Stal\u00b7le", "rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So war die Revolution vollbracht", "tokens": ["So", "war", "die", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "voll\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und keiner hat an Freiheit mehr gedacht.", "tokens": ["Und", "kei\u00b7ner", "hat", "an", "Frei\u00b7heit", "mehr", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Leithammel thut auch allen Schafen noth,", "tokens": ["Leit\u00b7ham\u00b7mel", "thut", "auch", "al\u00b7len", "Scha\u00b7fen", "noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Drum blieb er K\u00f6nig bis an seinen Tod.", "tokens": ["Drum", "blieb", "er", "K\u00f6\u00b7nig", "bis", "an", "sei\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Am andern Morgen las man \u00fcberall:", "tokens": ["Am", "an\u00b7dern", "Mor\u00b7gen", "las", "man", "\u00fc\u00b7be\u00b7rall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schafhausen hatte gestern auch Cravall;", "tokens": ["Schaf\u00b7hau\u00b7sen", "hat\u00b7te", "ge\u00b7stern", "auch", "Cra\u00b7vall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dank unsrer umsichtsvollen Polizei,", "tokens": ["Dank", "uns\u00b7rer", "um\u00b7sichts\u00b7vol\u00b7len", "Po\u00b7li\u00b7zei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Es blieb beim Alten, Alles ist vorbei.", "tokens": ["Es", "blieb", "beim", "Al\u00b7ten", ",", "Al\u00b7les", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PIS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Meutrer gingen zeitig heim nach Haus", "tokens": ["Die", "Meut\u00b7rer", "gin\u00b7gen", "zei\u00b7tig", "heim", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ruhen noch auf ihren Lorbeern aus.", "tokens": ["Und", "ru\u00b7hen", "noch", "auf", "ih\u00b7ren", "Lor\u00b7beern", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Als Alles l\u00e4ngst nun schon vergessen war,", "tokens": ["Als", "Al\u00b7les", "l\u00e4ngst", "nun", "schon", "ver\u00b7ges\u00b7sen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da ging nach einem vollen halben Jahr", "tokens": ["Da", "ging", "nach", "ei\u00b7nem", "vol\u00b7len", "hal\u00b7ben", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Allgemeine Zeitung in das Land", "tokens": ["Die", "All\u00b7ge\u00b7mei\u00b7ne", "Zei\u00b7tung", "in", "das", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und legte dann den ganzen Thatbestand", "tokens": ["Und", "leg\u00b7te", "dann", "den", "gan\u00b7zen", "That\u00b7be\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So recht loyal und kurz und b\u00fcndig aus,", "tokens": ["So", "recht", "lo\u00b7yal", "und", "kurz", "und", "b\u00fcn\u00b7dig", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr alle Sch\u00f6pf' ein wahrer Ohrenschmaus.", "tokens": ["F\u00fcr", "al\u00b7le", "Sch\u00f6pf'", "ein", "wah\u00b7rer", "Oh\u00b7ren\u00b7schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Von Hand in Hand ging da das Zeitungsblatt,", "tokens": ["Von", "Hand", "in", "Hand", 