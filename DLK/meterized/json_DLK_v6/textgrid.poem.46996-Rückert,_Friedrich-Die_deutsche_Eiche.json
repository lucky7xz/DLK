{"textgrid.poem.46996": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die deutsche Eiche", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ihr zu dem Wahn gekommen,", "tokens": ["Wie", "ihr", "zu", "dem", "Wahn", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutsche, da\u00df f\u00fcr euern Baum", "tokens": ["Deut\u00b7sche", ",", "da\u00df", "f\u00fcr", "eu\u00b7ern", "Baum"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr die Eich' habt angenommen,", "tokens": ["Ihr", "die", "Eich'", "habt", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu begreifen wei\u00df ich's kaum.", "tokens": ["Zu", "be\u00b7grei\u00b7fen", "wei\u00df", "ich's", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie ein Bild von euerm Reiche?", "tokens": ["Sie", "ein", "Bild", "von", "eu\u00b7erm", "Rei\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPOSAT", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch ein kr\u00fcpplig Jammerbild!", "tokens": ["Welch", "ein", "kr\u00fcpp\u00b7lig", "Jam\u00b7mer\u00b7bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn verk\u00fcmmert wie die Eiche", "tokens": ["Denn", "ver\u00b7k\u00fcm\u00b7mert", "wie", "die", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4chst kein Baum im Lenzgefild", "tokens": ["W\u00e4chst", "kein", "Baum", "im", "Lenz\u00b7ge\u00b7fild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Warum nicht, die h\u00f6her strebet,", "tokens": ["Wa\u00b7rum", "nicht", ",", "die", "h\u00f6\u00b7her", "stre\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Buche mit dem Riesenschaft;", "tokens": ["Bu\u00b7che", "mit", "dem", "Rie\u00b7sen\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder die so zierlich schwebet,", "tokens": ["O\u00b7der", "die", "so", "zier\u00b7lich", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Birke, s\u00e4uselnd geisterhaft?", "tokens": ["Bir\u00b7ke", ",", "s\u00e4u\u00b7selnd", "geis\u00b7ter\u00b7haft", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Beide, die dem Blick zu Troste", "tokens": ["Bei\u00b7de", ",", "die", "dem", "Blick", "zu", "Tros\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PRELS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schm\u00fcckt der Lenz mit fr\u00fchstem Laub,", "tokens": ["Schm\u00fcckt", "der", "Lenz", "mit", "fr\u00fchs\u00b7tem", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das nicht zittert vor dem Froste,", "tokens": ["Das", "nicht", "zit\u00b7tert", "vor", "dem", "Fros\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem die Eiche wird zum Raub.", "tokens": ["Dem", "die", "Ei\u00b7che", "wird", "zum", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und dann nagt der Maienk\u00e4fer", "tokens": ["Und", "dann", "nagt", "der", "Mai\u00b7en\u00b7k\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Scharf dem Maienfroste nach;", "tokens": ["Scharf", "dem", "Mai\u00b7en\u00b7fros\u00b7te", "nach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem armen deutschen Sch\u00e4fer", "tokens": ["Und", "dem", "ar\u00b7men", "deut\u00b7schen", "Sch\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt ein sp\u00e4rlich Schattendach;", "tokens": ["Bleibt", "ein", "sp\u00e4r\u00b7lich", "Schat\u00b7ten\u00b7dach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wo im hohen Sommergrase,", "tokens": ["Wo", "im", "ho\u00b7hen", "Som\u00b7mer\u00b7gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hohes tr\u00e4umend, er sich streckt;", "tokens": ["Ho\u00b7hes", "tr\u00e4u\u00b7mend", ",", "er", "sich", "streckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis im Herbstwind auf die Nase", "tokens": ["Bis", "im", "Herbst\u00b7wind", "auf", "die", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fallend ihn die Eichel weckt.", "tokens": ["Fal\u00b7lend", "ihn", "die", "Ei\u00b7chel", "weckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}