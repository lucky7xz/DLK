{"textgrid.poem.46122": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meinen geist, mut, seel und herz", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meinen geist, mut, seel und herz", "tokens": ["Mei\u00b7nen", "geist", ",", "mut", ",", "seel", "und", "herz"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "VVFIN", "$,", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Amor mit klag, forcht und schmerz", "tokens": ["A\u00b7mor", "mit", "klag", ",", "forcht", "und", "schmerz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "VVFIN", "$,", "VVFIN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Recht componieret;", "tokens": ["Recht", "com\u00b7po\u00b7nie\u00b7ret", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In leid \u00e4ndert sich mein scherz,", "tokens": ["In", "leid", "\u00e4n\u00b7dert", "sich", "mein", "scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Angst mit mir accordieret.", "tokens": ["Angst", "mit", "mir", "ac\u00b7cor\u00b7die\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Marter ist mein musikklang,", "tokens": ["Mar\u00b7ter", "ist", "mein", "mu\u00b7sik\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach und weh ist mein gesang", "tokens": ["Ach", "und", "weh", "ist", "mein", "ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "KON", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ganz ohn pausieren,", "tokens": ["Ganz", "ohn", "pau\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dan allein, da\u00df mich oft lang", "tokens": ["Dan", "al\u00b7lein", ",", "da\u00df", "mich", "oft", "lang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Amor macht suspirieren.", "tokens": ["A\u00b7mor", "macht", "sus\u00b7pi\u00b7rie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Lieblich kan es zwar nicht sein,", "tokens": ["Lieb\u00b7lich", "kan", "es", "zwar", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "PTKNEG", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einig singend stets von pein", "tokens": ["Ei\u00b7nig", "sin\u00b7gend", "stets", "von", "pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht zu mutieren;", "tokens": ["Nicht", "zu", "mu\u00b7tie\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Aber weil ich sing allein,", "tokens": ["A\u00b7ber", "weil", "ich", "sing", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df ich wol colorieren.", "tokens": ["Mu\u00df", "ich", "wol", "co\u00b7lo\u00b7rie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach, herzlieb, thu doch mit mir,", "tokens": ["Ach", ",", "herz\u00b7lieb", ",", "thu", "doch", "mit", "mir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Greifend den ton nach geb\u00fchr,", "tokens": ["Grei\u00b7fend", "den", "ton", "nach", "ge\u00b7b\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nu moderieren;", "tokens": ["Nu", "mo\u00b7de\u00b7rie\u00b7ren", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und alsdan will ich mit dir", "tokens": ["Und", "als\u00b7dan", "will", "ich", "mit", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schon tief gnug intonieren.", "tokens": ["Schon", "tief", "gnug", "in\u00b7to\u00b7nie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}