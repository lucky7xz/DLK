{"textgrid.poem.63807": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "18. Auf eine griechische B\u00fcste des Traumgottes", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer dich bildete, D\u00e4mon, gefl\u00fcgelten Hauptes, die Lippen", "tokens": ["Wer", "dich", "bil\u00b7de\u00b7te", ",", "D\u00e4\u00b7mon", ",", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "Haup\u00b7tes", ",", "die", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "NN", "$,", "ADJA", "NN", "$,", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "H\u00f6hnisch pressend, den Blick eisig ins Leere gespannt,", "tokens": ["H\u00f6h\u00b7nisch", "pres\u00b7send", ",", "den", "Blick", "ei\u00b7sig", "ins", "Lee\u00b7re", "ge\u00b7spannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ART", "NN", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ihm umschwirrten das Lager zu Nacht nur tr\u00fcgliche Larven;", "tokens": ["Ihm", "um\u00b7schwirr\u00b7ten", "das", "La\u00b7ger", "zu", "Nacht", "nur", "tr\u00fcg\u00b7li\u00b7che", "Lar\u00b7ven", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "ADV", "ADJA", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Gl\u00fcckweissagend und treu bist du ihm nimmer genaht.", "tokens": ["Gl\u00fcck\u00b7weis\u00b7sa\u00b7gend", "und", "treu", "bist", "du", "ihm", "nim\u00b7mer", "ge\u00b7naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "+----+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Stets nur t\u00e4uschtest du h\u00e4misch ihm vor das Bild der Ersehnten,", "tokens": ["Stets", "nur", "t\u00e4uschtest", "du", "h\u00e4\u00b7misch", "ihm", "vor", "das", "Bild", "der", "Er\u00b7sehn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das mit H\u00e4nden ber\u00fchrt schaurig in Nebel zerflo\u00df,", "tokens": ["Das", "mit", "H\u00e4n\u00b7den", "be\u00b7r\u00fchrt", "schau\u00b7rig", "in", "Ne\u00b7bel", "zer\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Eh' es dem Armen verg\u00f6nnt, an z\u00e4rtlichen Lippen der Sehnsucht", "tokens": ["Eh'", "es", "dem", "Ar\u00b7men", "ver\u00b7g\u00f6nnt", ",", "an", "z\u00e4rt\u00b7li\u00b7chen", "Lip\u00b7pen", "der", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Fieber zu k\u00fchlen, das Haupt bettend der Teuren im Scho\u00df.", "tokens": ["Fie\u00b7ber", "zu", "k\u00fch\u00b7len", ",", "das", "Haupt", "bet\u00b7tend", "der", "Teu\u00b7ren", "im", "Scho\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "ART", "NN", "VVPP", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Oder du hast all das ihm gew\u00e4hrt, da\u00df nur um so bittrer", "tokens": ["O\u00b7der", "du", "hast", "all", "das", "ihm", "ge\u00b7w\u00e4hrt", ",", "da\u00df", "nur", "um", "so", "bit\u00b7trer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "PDS", "PPER", "VVPP", "$,", "KOUS", "ADV", "APPR", "ADV", "ADJA"], "meter": "+-+-+---+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Er aus seligem Wahn wieder erwache zur Qual.", "tokens": ["Er", "aus", "se\u00b7li\u00b7gem", "Wahn", "wie\u00b7der", "er\u00b7wa\u00b7che", "zur", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Bleibe mir stets vor Augen, den Leichtbetrognen zu warnen,", "tokens": ["Blei\u00b7be", "mir", "stets", "vor", "Au\u00b7gen", ",", "den", "Leicht\u00b7be\u00b7trog\u00b7nen", "zu", "war\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Da\u00df auch wachend er nie traue dem Traume des Gl\u00fccks!", "tokens": ["Da\u00df", "auch", "wa\u00b7chend", "er", "nie", "trau\u00b7e", "dem", "Trau\u00b7me", "des", "Gl\u00fccks", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}}}}}