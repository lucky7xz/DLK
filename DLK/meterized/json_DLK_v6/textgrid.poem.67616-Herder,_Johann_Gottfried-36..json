{"textgrid.poem.67616": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdie Sprudelbrunnen sollen Steuer geben;", "tokens": ["\u00bb", "die", "Spru\u00b7del\u00b7brun\u00b7nen", "sol\u00b7len", "Steu\u00b7er", "ge\u00b7ben", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sanften Quellen flie\u00dfen frei!\u00ab", "tokens": ["Die", "sanf\u00b7ten", "Quel\u00b7len", "flie\u00b7\u00dfen", "frei", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Heil ihm! ihm sei", "tokens": ["Heil", "ihm", "!", "ihm", "sei"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ein langes Leben,", "tokens": ["Ein", "lan\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der so befahl!", "tokens": ["Der", "so", "be\u00b7fahl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Jungfrauquelle ist das Himmelskind Genie,", "tokens": ["Die", "Jung\u00b7frau\u00b7quel\u00b7le", "ist", "das", "Him\u00b7mel\u00b7skind", "Ge\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Gottoriginal,", "tokens": ["Das", "Got\u00b7to\u00b7ri\u00b7gi\u00b7nal", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das schenkt und tr\u00e4nkt", "tokens": ["Das", "schenkt", "und", "tr\u00e4nkt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und aus sich selbst sich senkt", "tokens": ["Und", "aus", "sich", "selbst", "sich", "senkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und flie\u00dft so stille.", "tokens": ["Und", "flie\u00dft", "so", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Dort aber jene Gassennymphe, die", "tokens": ["Dort", "a\u00b7ber", "je\u00b7ne", "Gas\u00b7sen\u00b7nym\u00b7phe", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "PDAT", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Aus Bacchusb\u00e4uchen f\u00e4llt", "tokens": ["Aus", "Bac\u00b7chus\u00b7b\u00e4u\u00b7chen", "f\u00e4llt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und vor Pal\u00e4sten in Gel\u00e4ndern bellt", "tokens": ["Und", "vor", "Pa\u00b7l\u00e4s\u00b7ten", "in", "Ge\u00b7l\u00e4n\u00b7dern", "bellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und rauscht und r\u00f6chelt \u2013 zinset sie!", "tokens": ["Und", "rauscht", "und", "r\u00f6\u00b7chelt", "\u2013", "zin\u00b7set", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$(", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Es ist Monarchens Wille.", "tokens": ["Es", "ist", "Mon\u00b7ar\u00b7chens", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}