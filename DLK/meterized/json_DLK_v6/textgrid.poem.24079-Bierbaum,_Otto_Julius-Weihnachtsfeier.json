{"textgrid.poem.24079": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtsfeier", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Berge und W\u00e4lder und Wiesen und See:", "tokens": ["Ber\u00b7ge", "und", "W\u00e4l\u00b7der", "und", "Wie\u00b7sen", "und", "See", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Schnee und Nebel, Nebel und Schnee;", "tokens": ["Schnee", "und", "Ne\u00b7bel", ",", "Ne\u00b7bel", "und", "Schnee", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nieder der Himmel, farblos und fahl;", "tokens": ["Nie\u00b7der", "der", "Him\u00b7mel", ",", "farb\u00b7los", "und", "fahl", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "War er denn heiter und hoch einmal?", "tokens": ["War", "er", "denn", "hei\u00b7ter", "und", "hoch", "ein\u00b7mal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hockende Kr\u00e4hen auf kahlem Ge\u00e4st, \u2013", "tokens": ["Ho\u00b7cken\u00b7de", "Kr\u00e4\u00b7hen", "auf", "kah\u00b7lem", "Ge\u00b7\u00e4st", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Das ist des blutwarmen Lebens der Rest?", "tokens": ["Das", "ist", "des", "blut\u00b7war\u00b7men", "Le\u00b7bens", "der", "Rest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Siehe, die Sonne versinkt hinterm See:", "tokens": ["Sie\u00b7he", ",", "die", "Son\u00b7ne", "ver\u00b7sinkt", "hin\u00b7term", "See", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Broncegold taut auf dem glitzernden Schnee,", "tokens": ["Bron\u00b7ce\u00b7gold", "taut", "auf", "dem", "glit\u00b7zern\u00b7den", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Taut und verflie\u00dft in das flockige Wei\u00df, \u2013", "tokens": ["Taut", "und", "ver\u00b7flie\u00dft", "in", "das", "flo\u00b7cki\u00b7ge", "Wei\u00df", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Rundum umstarrt mich lebloses Eis.", "tokens": ["Run\u00b7dum", "um\u00b7starrt", "mich", "leb\u00b7lo\u00b7ses", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Dampfende Nebel umh\u00fcllen mich dicht,", "tokens": ["Damp\u00b7fen\u00b7de", "Ne\u00b7bel", "um\u00b7h\u00fcl\u00b7len", "mich", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Wehen wie Ha\u00dfhauch mir na\u00df ins Gesicht.", "tokens": ["We\u00b7hen", "wie", "Ha\u00df\u00b7hauch", "mir", "na\u00df", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Stechen nicht Augen hervor aus dem Grau,", "tokens": ["Ste\u00b7chen", "nicht", "Au\u00b7gen", "her\u00b7vor", "aus", "dem", "Grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Augen der lieblosen alten Frau,", "tokens": ["Au\u00b7gen", "der", "lieb\u00b7lo\u00b7sen", "al\u00b7ten", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Die in der knochigen Hand zur\u00fcck", "tokens": ["Die", "in", "der", "kno\u00b7chi\u00b7gen", "Hand", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Grausam mir h\u00e4lt mein bangs\u00fc\u00dfes Gl\u00fcck?", "tokens": ["Grau\u00b7sam", "mir", "h\u00e4lt", "mein", "bang\u00b7s\u00fc\u00b7\u00dfes", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Nein doch und nein! Ein lieberes Licht", "tokens": ["Nein", "doch", "und", "nein", "!", "Ein", "lie\u00b7be\u00b7res", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "ADV", "KON", "PTKANT", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Lacht mir aus Nebelgrau hell ins Gesicht:", "tokens": ["Lacht", "mir", "aus", "Ne\u00b7bel\u00b7grau", "hell", "ins", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-++--+", "measure": "iambic.penta.invert"}, "line.13": {"text": "\u00bbg'rannt bin i schnell wie der Wind \u00fcbern Schnee!\u00ab", "tokens": ["\u00bb", "g'\u00b7rannt", "bin", "i", "schnell", "wie", "der", "Wind", "\u00fc\u00b7bern", "Schnee", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "NE", "ADJD", "KOKOM", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "\u2013 M\u00e4del, oh du meine Weihnachtsfee!", "tokens": ["\u2013", "M\u00e4\u00b7del", ",", "oh", "du", "mei\u00b7ne", "Weih\u00b7nachts\u00b7fee", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Schmiegt sie sich an mich dicht und bang,", "tokens": ["Schmiegt", "sie", "sich", "an", "mich", "dicht", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wandern wir wortlos im Glockenklang,", "tokens": ["Wan\u00b7dern", "wir", "wort\u00b7los", "im", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wandern durch Nebel und Nacht und Wind,", "tokens": ["Wan\u00b7dern", "durch", "Ne\u00b7bel", "und", "Nacht", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Weint an der Brust mir leise das Kind,", "tokens": ["Weint", "an", "der", "Brust", "mir", "lei\u00b7se", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Weint, da\u00df getrennt wir m\u00fcssen, allein,", "tokens": ["Weint", ",", "da\u00df", "ge\u00b7trennt", "wir", "m\u00fcs\u00b7sen", ",", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ADJD", "PPER", "VMFIN", "$,", "ADV", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "In der heiligen Weihenacht sein.", "tokens": ["In", "der", "hei\u00b7li\u00b7gen", "Wei\u00b7he\u00b7nacht", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "--+--+-++", "measure": "anapaest.di.plus"}, "line.7": {"text": "K\u00fc\u00df ich die Thr\u00e4nen ihr lind vom Gesicht:", "tokens": ["K\u00fc\u00df", "ich", "die", "Thr\u00e4\u00b7nen", "ihr", "lind", "vom", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Weine nicht, M\u00e4del, geh, weine nicht!", "tokens": ["Wei\u00b7ne", "nicht", ",", "M\u00e4\u00b7del", ",", "geh", ",", "wei\u00b7ne", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Z\u00fcndet heut Andern der Liebesmann", "tokens": ["Z\u00fcn\u00b7det", "heut", "An\u00b7dern", "der", "Lie\u00b7bes\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJA", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Flimmernde Christkindlkerzen an,", "tokens": ["Flim\u00b7mern\u00b7de", "Christ\u00b7kindl\u00b7ker\u00b7zen", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Hat er in unseren Herzen entfacht", "tokens": ["Hat", "er", "in", "un\u00b7se\u00b7ren", "Her\u00b7zen", "ent\u00b7facht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "Eine ewige Weihenacht.", "tokens": ["Ei\u00b7ne", "e\u00b7wi\u00b7ge", "Wei\u00b7he\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Sind wir auch heute Abend getrennt,", "tokens": ["Sind", "wir", "auch", "heu\u00b7te", "A\u00b7bend", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Doch uns im Herzen ein Christbaum brennt.", "tokens": ["Doch", "uns", "im", "Her\u00b7zen", "ein", "Christ\u00b7baum", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Dir aus dem Auge ja lacht sein Schein,", "tokens": ["Dir", "aus", "dem", "Au\u00b7ge", "ja", "lacht", "sein", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Nein doch, du Meine, wir sind nicht allein.", "tokens": ["Nein", "doch", ",", "du", "Mei\u00b7ne", ",", "wir", "sind", "nicht", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "PPER", "PPOSAT", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Trag ich dein Herz ja in meiner Brust,", "tokens": ["Trag", "ich", "dein", "Herz", "ja", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Du auch das meine tragen mu\u00dft.", "tokens": ["Du", "auch", "das", "mei\u00b7ne", "tra\u00b7gen", "mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "PPOSAT", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Froh mir ein hellwarmes L\u00e4cheln dankt,", "tokens": ["Froh", "mir", "ein", "hell\u00b7war\u00b7mes", "L\u00e4\u00b7cheln", "dankt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Fest mich ihr rundvoller Arm umrankt,", "tokens": ["Fest", "mich", "ihr", "rund\u00b7vol\u00b7ler", "Arm", "um\u00b7rankt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Tief saugt ihr Blick sich in meinen ein:", "tokens": ["Tief", "saugt", "ihr", "Blick", "sich", "in", "mei\u00b7nen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "PPOSAT", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "\u00bbnein, oh du Meiner, wir sind nicht allein.\u00ab", "tokens": ["\u00bb", "nein", ",", "oh", "du", "Mei\u00b7ner", ",", "wir", "sind", "nicht", "al\u00b7lein", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "KOUS", "PPER", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wandern zur\u00fcck wir durch Nebel und Wind,", "tokens": ["Wan\u00b7dern", "zu\u00b7r\u00fcck", "wir", "durch", "Ne\u00b7bel", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Lacht an der Seite mir selig das Kind.", "tokens": ["Lacht", "an", "der", "Sei\u00b7te", "mir", "se\u00b7lig", "das", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}