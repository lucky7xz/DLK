{"textgrid.poem.33458": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Der Fall in den Flu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lene Levi lief besoffen", "tokens": ["Le\u00b7ne", "Le\u00b7vi", "lief", "be\u00b7sof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "N\u00e4chtlich in den Nebenstra\u00dfen", "tokens": ["N\u00e4cht\u00b7lich", "in", "den", "Ne\u00b7ben\u00b7stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hin und wieder \u00bbAuto\u00ab br\u00fcllend.", "tokens": ["Hin", "und", "wie\u00b7der", "\u00bb", "Au\u00b7to", "\u00ab", "br\u00fcl\u00b7lend", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "ADV", "$(", "FM.la", "$(", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihre Bluse war ge\u00f6ffnet,", "tokens": ["Ih\u00b7re", "Blu\u00b7se", "war", "ge\u00b7\u00f6ff\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df man ihre feine, freche", "tokens": ["Da\u00df", "man", "ih\u00b7re", "fei\u00b7ne", ",", "fre\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unterw\u00e4sche und das Fleisch sah.", "tokens": ["Un\u00b7ter\u00b7w\u00e4\u00b7sche", "und", "das", "Fleisch", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieben geile M\u00e4nnlein rannten", "tokens": ["Sie\u00b7ben", "gei\u00b7le", "M\u00e4nn\u00b7lein", "rann\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter Lene Levi her.", "tokens": ["Hin\u00b7ter", "Le\u00b7ne", "Le\u00b7vi", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieben geile M\u00e4nnlein trachten", "tokens": ["Sie\u00b7ben", "gei\u00b7le", "M\u00e4nn\u00b7lein", "trach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lene Levi nach dem Leibe,", "tokens": ["Le\u00b7ne", "Le\u00b7vi", "nach", "dem", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcberlegend, was das kostet.", "tokens": ["\u00dc\u00b7berl\u00b7e\u00b7gend", ",", "was", "das", "kos\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "PDS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieben, sonst sehr ernste M\u00e4nner", "tokens": ["Sie\u00b7ben", ",", "sonst", "sehr", "erns\u00b7te", "M\u00e4n\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "$,", "ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben Kind und Kunst vergessen,", "tokens": ["Ha\u00b7ben", "Kind", "und", "Kunst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wissenschaft und die Fabrik.", "tokens": ["Wis\u00b7sen\u00b7schaft", "und", "die", "Fab\u00b7rik", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und sie rannten wie besessen", "tokens": ["Und", "sie", "rann\u00b7ten", "wie", "be\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter Lene Levi her.", "tokens": ["Hin\u00b7ter", "Le\u00b7ne", "Le\u00b7vi", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lene Levi blieb auf einer", "tokens": ["Le\u00b7ne", "Le\u00b7vi", "blieb", "auf", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Br\u00fccke stehen, atemsch\u00f6pfend,", "tokens": ["Br\u00fc\u00b7cke", "ste\u00b7hen", ",", "a\u00b7tem\u00b7sch\u00f6p\u00b7fend", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie hob die wirren blauen", "tokens": ["Und", "sie", "hob", "die", "wir\u00b7ren", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "S\u00e4uferblicke in die weiten", "tokens": ["S\u00e4u\u00b7fer\u00b7bli\u00b7cke", "in", "die", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfen Dunkelheiten \u00fcber", "tokens": ["S\u00fc\u00b7\u00dfen", "Dun\u00b7kel\u00b7hei\u00b7ten", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den Laternen und den H\u00e4usern.", "tokens": ["Den", "La\u00b7ter\u00b7nen", "und", "den", "H\u00e4u\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sieben geile M\u00e4nnlein aber", "tokens": ["Sie\u00b7ben", "gei\u00b7le", "M\u00e4nn\u00b7lein", "a\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fielen Lenen in die Augen.", "tokens": ["Fie\u00b7len", "Le\u00b7nen", "in", "die", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sieben geile M\u00e4nnlein suchten", "tokens": ["Sie\u00b7ben", "gei\u00b7le", "M\u00e4nn\u00b7lein", "such\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lene Levis Herz zu r\u00fchren.", "tokens": ["Le\u00b7ne", "Le\u00b7vis", "Herz", "zu", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lene Levi blieb unnahbar.", "tokens": ["Le\u00b7ne", "Le\u00b7vi", "blieb", "un\u00b7nah\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Pl\u00f6tzlich springt sie aufs Gel\u00e4nder,", "tokens": ["Pl\u00f6tz\u00b7lich", "springt", "sie", "aufs", "Ge\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dreht der Welt die letzte Nase,", "tokens": ["Dreht", "der", "Welt", "die", "letz\u00b7te", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jauchzend plumpst sie in den Flu\u00df.", "tokens": ["Jauch\u00b7zend", "plumpst", "sie", "in", "den", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Sieben bleiche M\u00e4nnlein rannten,", "tokens": ["Sie\u00b7ben", "blei\u00b7che", "M\u00e4nn\u00b7lein", "rann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sie konnten, aus der Gegend.", "tokens": ["Was", "sie", "konn\u00b7ten", ",", "aus", "der", "Ge\u00b7gend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}