{"dta.poem.3572": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So und so so geht der Wind,               ", "tokens": ["So", "und", "so", "so", "geht", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "So und so pfeift er;", "tokens": ["So", "und", "so", "pfeift", "er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und wenn ich mein Sch\u00e4tzle s\u00e4h,", "tokens": ["Und", "wenn", "ich", "mein", "Sch\u00e4tz\u00b7le", "s\u00e4h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r mirs gleich viel leichter.", "tokens": ["W\u00e4r", "mirs", "gleich", "viel", "leich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "So lieb als mir mein Leben ist,", "tokens": ["So", "lieb", "als", "mir", "mein", "Le\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lieb ist mir mein Schatz;", "tokens": ["So", "lieb", "ist", "mir", "mein", "Schatz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn er auch gestorben ist,", "tokens": ["Und", "wenn", "er", "auch", "ge\u00b7stor\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lieb ich noch den Platz.", "tokens": ["So", "lieb", "ich", "noch", "den", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Liederl ist gesungen,", "tokens": ["Das", "Lie\u00b7derl", "ist", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Kreutzer ist gewunnen;", "tokens": ["Der", "Kreut\u00b7zer", "ist", "ge\u00b7wun\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wer mir ihn nit geit,", "tokens": ["Und", "wer", "mir", "ihn", "nit", "geit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "PTKNEG", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dem singe ich auf Beut.", "tokens": ["Dem", "sin\u00b7ge", "ich", "auf", "Beut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}