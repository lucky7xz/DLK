{"textgrid.poem.24390": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken bey einer gro\u00dfen Feuers-Brunst", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer Stunde wird zu Aschen hier gemacht/", "tokens": ["In", "ei\u00b7ner", "Stun\u00b7de", "wird", "zu", "A\u00b7schen", "hier", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was lange M\u00fch und Zeit zusammen hat gebracht.", "tokens": ["Was", "lan\u00b7ge", "M\u00fch", "und", "Zeit", "zu\u00b7sam\u00b7men", "hat", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "KON", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Reichen setzet nicht auf G\u00fcter das Vertrauen:", "tokens": ["Ihr", "Rei\u00b7chen", "set\u00b7zet", "nicht", "auf", "G\u00fc\u00b7ter", "das", "Ver\u00b7trau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf Gott ist sicherer/ als auf die Weit zu bauen.", "tokens": ["Auf", "Gott", "ist", "si\u00b7che\u00b7rer", "/", "als", "auf", "die", "Weit", "zu", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADJD", "$(", "KOKOM", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}