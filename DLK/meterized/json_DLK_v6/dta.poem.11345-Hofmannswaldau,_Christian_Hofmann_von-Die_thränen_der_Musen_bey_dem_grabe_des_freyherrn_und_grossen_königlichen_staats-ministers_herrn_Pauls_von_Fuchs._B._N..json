{"dta.poem.11345": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die thr\u00e4nen der Musen bey dem grabe  \n des freyherrn und grossen k\u00f6niglichen  \n staats-ministers herrn Pauls  \n von Fuchs.  \n B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weinet! weint! ihr Pierinnen!", "tokens": ["Wei\u00b7net", "!", "weint", "!", "ihr", "Pie\u00b7rin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "$.", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und du gantzes Musen-heer!", "tokens": ["Und", "du", "gant\u00b7zes", "Mu\u00b7sen\u00b7heer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kluge lieder auszusinnen,", "tokens": ["Klu\u00b7ge", "lie\u00b7der", "aus\u00b7zu\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist wohl diesesmahl zu schwer.", "tokens": ["Ist", "wohl", "die\u00b7ses\u00b7mahl", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir sind viel zu sehr betr\u00fcbet:", "tokens": ["Wir", "sind", "viel", "zu", "sehr", "be\u00b7tr\u00fc\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Pindus auen leiden noth;", "tokens": ["Pin\u00b7dus", "au\u00b7en", "lei\u00b7den", "noth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn der sie so hoch geliebet,", "tokens": ["Denn", "der", "sie", "so", "hoch", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Der erlauchte Fuchs ist todt.", "tokens": ["Der", "er\u00b7lauch\u00b7te", "Fuchs", "ist", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Helden r\u00fchmen seine thaten:", "tokens": ["Hel\u00b7den", "r\u00fch\u00b7men", "sei\u00b7ne", "tha\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcrsten sagen, was sein geist", "tokens": ["F\u00fcrs\u00b7ten", "sa\u00b7gen", ",", "was", "sein", "geist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu Europens heil gerathen:", "tokens": ["Zu", "Eu\u00b7ro\u00b7pens", "heil", "ge\u00b7ra\u00b7then", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Andre, was die kirche preist.", "tokens": ["And\u00b7re", ",", "was", "die", "kir\u00b7che", "preist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine treu, (ist es zu wenig?)", "tokens": ["Sei\u00b7ne", "treu", ",", "(", "ist", "es", "zu", "we\u00b7nig", "?", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJD", "$,", "$(", "VAFIN", "PPER", "PTKA", "PIS", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seiner weisheit tieffen grund,", "tokens": ["Sei\u00b7ner", "weis\u00b7heit", "tief\u00b7fen", "grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Klagt ein noch viel weiser k\u00f6nig:", "tokens": ["Klagt", "ein", "noch", "viel", "wei\u00b7ser", "k\u00f6\u00b7nig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wir beklagen seinen mund.", "tokens": ["Wir", "be\u00b7kla\u00b7gen", "sei\u00b7nen", "mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seinen mund, der deinen feinden,", "tokens": ["Sei\u00b7nen", "mund", ",", "der", "dei\u00b7nen", "fein\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Preussen! wie ein donner war:", "tokens": ["Preus\u00b7sen", "!", "wie", "ein", "don\u00b7ner", "war", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen mund, der deinen freunden,", "tokens": ["Sei\u00b7nen", "mund", ",", "der", "dei\u00b7nen", "freun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Teutschland! hertz und muth gebahr:", "tokens": ["Teutschland", "!", "hertz", "und", "muth", "ge\u00b7bahr", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Seinen mund, der, was die Griechen,", "tokens": ["Sei\u00b7nen", "mund", ",", "der", ",", "was", "die", "Grie\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was die R\u00f6mer vorgebracht,", "tokens": ["Was", "die", "R\u00f6\u00b7mer", "vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zwar offt hoch heraus gestrichen,", "tokens": ["Zwar", "offt", "hoch", "he\u00b7raus", "ge\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch viel sch\u00f6ner noch gemacht.", "tokens": ["Doch", "viel", "sch\u00f6\u00b7ner", "noch", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Grosser mann! du wirst versencket:", "tokens": ["Gros\u00b7ser", "mann", "!", "du", "wirst", "ver\u00b7sen\u00b7cket", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! versenckte man doch nicht,", "tokens": ["Ach", "!", "ver\u00b7senck\u00b7te", "man", "doch", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PIS", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was dir GOtt voraus geschencket,", "tokens": ["Was", "dir", "Gott", "vo\u00b7raus", "ge\u00b7schen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch zugleich dein hohes licht!", "tokens": ["Auch", "zu\u00b7gleich", "dein", "ho\u00b7hes", "licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer wird k\u00fcnfftig uns vorstehen?", "tokens": ["Wer", "wird", "k\u00fcnff\u00b7tig", "uns", "vor\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer stimmt unser saiten-spiel,", "tokens": ["Wer", "stimmt", "un\u00b7ser", "sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wenn die kunst zu grabe gehen,", "tokens": ["Wenn", "die", "kunst", "zu", "gra\u00b7be", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der meister sterben will?", "tokens": ["Und", "der", "meis\u00b7ter", "ster\u00b7ben", "will", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was Apollo vor gewesen,", "tokens": ["Was", "A\u00b7pol\u00b7lo", "vor", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "APPR", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warst du w\u00fcrcklich in Berlin.", "tokens": ["Warst", "du", "w\u00fcrck\u00b7lich", "in", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "NE", "$."], "meter": "+-+-+++", "measure": "unknown.measure.penta"}, "line.3": {"text": "Tausend sind durch dich genesen,", "tokens": ["Tau\u00b7send", "sind", "durch", "dich", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die der h\u00fclffe sich verziehn.", "tokens": ["Die", "der", "h\u00fclf\u00b7fe", "sich", "ver\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VVFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Allem zorne fiel der wille,", "tokens": ["Al\u00b7lem", "zor\u00b7ne", "fiel", "der", "wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo dein s\u00fcsses wort erklang:", "tokens": ["Wo", "dein", "s\u00fcs\u00b7ses", "wort", "er\u00b7klang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gantze heere stunden stille,", "tokens": ["Gant\u00b7ze", "hee\u00b7re", "stun\u00b7den", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn dein mund vom frieden sang.", "tokens": ["Wenn", "dein", "mund", "vom", "frie\u00b7den", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "So herrscht nicht ein warmer regen,", "tokens": ["So", "herrscht", "nicht", "ein", "war\u00b7mer", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "ADJA", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn er schnee und ei\u00df durchdringt:", "tokens": ["Wenn", "er", "schnee", "und", "ei\u00df", "durch\u00b7dringt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So kan Orpheus nicht bewegen,", "tokens": ["So", "kan", "Or\u00b7pheus", "nicht", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er vor der h\u00f6llen singt;", "tokens": ["Wenn", "er", "vor", "der", "h\u00f6l\u00b7len", "singt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als ein satz aus deinem munde", "tokens": ["Als", "ein", "satz", "aus", "dei\u00b7nem", "mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeden einwurff \u00fcberwog;", "tokens": ["Je\u00b7den", "ein\u00b7wurff", "\u00fc\u00b7ber\u00b7wog", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als dein mund zur rechten stunde", "tokens": ["Als", "dein", "mund", "zur", "rech\u00b7ten", "stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aller hertzen an sich zog.", "tokens": ["Al\u00b7ler", "hert\u00b7zen", "an", "sich", "zog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Weint! verla\u00dfne Musen! weinet!", "tokens": ["Weint", "!", "ver\u00b7la\u00df\u00b7ne", "Mu\u00b7sen", "!", "wei\u00b7net", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "ADJA", "NN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Di\u00df geschah f\u00fcr hof und land.", "tokens": ["Di\u00df", "ge\u00b7schah", "f\u00fcr", "hof", "und", "land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! was hat er, wie es scheinet,", "tokens": ["Ach", "!", "was", "hat", "er", ",", "wie", "es", "schei\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht f\u00fcr mich an euch gewandt!", "tokens": ["Nicht", "f\u00fcr", "mich", "an", "euch", "ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Ph\u00f6bus muste sich offt kr\u00e4ncken,", "tokens": ["Ph\u00f6\u00b7bus", "mus\u00b7te", "sich", "offt", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenig klang von uns ihm wohl;", "tokens": ["We\u00b7nig", "klang", "von", "uns", "ihm", "wohl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber Fuchs wie\u00df, wie man dencken,", "tokens": ["A\u00b7ber", "Fuchs", "wie\u00df", ",", "wie", "man", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "PWAV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie man artig sprechen soll.", "tokens": ["Wie", "man", "ar\u00b7tig", "spre\u00b7chen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Er vermied die stoltzen grillen,", "tokens": ["Er", "ver\u00b7mied", "die", "stolt\u00b7zen", "gril\u00b7len", ","], "token_info": ["word", "word", "word", 