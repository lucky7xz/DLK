{"dta.poem.23473": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aus Schlesien,  \n  zur Zeit des drei\u00dfigj\u00e4hrigen Krieges.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott, der du bist ein Freund der Menschenkinder,", "tokens": ["Gott", ",", "der", "du", "bist", "ein", "Freund", "der", "Men\u00b7schen\u00b7kin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ein Erbarmer der zerschlagnen S\u00fcnder,", "tokens": ["Und", "ein", "Er\u00b7bar\u00b7mer", "der", "zer\u00b7schlag\u00b7nen", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schau uns doch an, wie wir gedrucket werden", "tokens": ["Schau", "uns", "doch", "an", ",", "wie", "wir", "ge\u00b7dru\u00b7cket", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch viel Beschwerden.", "tokens": ["Durch", "viel", "Be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir haben bisher bei viel langen Jahren", "tokens": ["Wir", "ha\u00b7ben", "bis\u00b7her", "bei", "viel", "lan\u00b7gen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf unserm R\u00fccken deine Streich' erfahren,", "tokens": ["Auf", "un\u00b7serm", "R\u00fc\u00b7cken", "dei\u00b7ne", "Streich'", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deine Hand war uns zur harten Plage", "tokens": ["Und", "dei\u00b7ne", "Hand", "war", "uns", "zur", "har\u00b7ten", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei Nacht und Tage.", "tokens": ["Bei", "Nacht", "und", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Krieg hat dies sch\u00f6ne Land ganz umgekehret,", "tokens": ["Krieg", "hat", "dies", "sch\u00f6\u00b7ne", "Land", "ganz", "um\u00b7ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PDS", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und unser Fleisch und Mark rein ausgezehret;", "tokens": ["Und", "un\u00b7ser", "Fleisch", "und", "Mark", "rein", "aus\u00b7ge\u00b7zeh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Pest hat auch unsre Br\u00fcder weggenommen", "tokens": ["Pest", "hat", "auch", "uns\u00b7re", "Br\u00fc\u00b7der", "weg\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit gro\u00dfen Summen.", "tokens": ["Mit", "gro\u00b7\u00dfen", "Sum\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "In Hungersnoth sind ihrer viel vergangen;", "tokens": ["In", "Hun\u00b7gers\u00b7noth", "sind", "ih\u00b7rer", "viel", "ver\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir, die wir \u00fcbrig, sind zurings umfangen", "tokens": ["Wir", ",", "die", "wir", "\u00fcb\u00b7rig", ",", "sind", "zu\u00b7rings", "um\u00b7fan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "PTKVZ", "$,", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Nattern, die uns ohne Ma\u00df und Z\u00e4hlen", "tokens": ["Mit", "Nat\u00b7tern", ",", "die", "uns", "oh\u00b7ne", "Ma\u00df", "und", "Z\u00e4h\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Martern und qu\u00e4len.", "tokens": ["Mar\u00b7tern", "und", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "O Herr, wie hast du dich uns doch verwandelt", "tokens": ["O", "Herr", ",", "wie", "hast", "du", "dich", "uns", "doch", "ver\u00b7wan\u00b7delt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "PRF", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einen, der sehr streng and grausam handelt?", "tokens": ["In", "ei\u00b7nen", ",", "der", "sehr", "streng", "and", "grau\u00b7sam", "han\u00b7delt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "VVFIN", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach, wo ist doch dein v\u00e4terlich Gem\u00fcthe", "tokens": ["Ach", ",", "wo", "ist", "doch", "dein", "v\u00e4\u00b7ter\u00b7lich", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und milde G\u00fcte?", "tokens": ["Und", "mil\u00b7de", "G\u00fc\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wir m\u00fcssen zwar f\u00fcr unsrer Noth erblassen,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "zwar", "f\u00fcr", "uns\u00b7rer", "Noth", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df wir so sch\u00e4ndlich dein Gebot verlassen;", "tokens": ["Da\u00df", "wir", "so", "sch\u00e4nd\u00b7lich", "dein", "Ge\u00b7bot", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aber wir kehren um und sind beflissen", "tokens": ["A\u00b7ber", "wir", "keh\u00b7ren", "um", "und", "sind", "be\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "VAFIN", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Herzlich zu b\u00fc\u00dfen.", "tokens": ["Herz\u00b7lich", "zu", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "So kehr auch du zu uns nun mit Genaden,", "tokens": ["So", "kehr", "auch", "du", "zu", "uns", "nun", "mit", "Ge\u00b7na\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wend' unsern Jammer und heil' unsern Schaden!", "tokens": ["Wend'", "un\u00b7sern", "Jam\u00b7mer", "und", "heil'", "un\u00b7sern", "Scha\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Sei unser Gott, wie du vor bist gewesen,", "tokens": ["Sei", "un\u00b7ser", "Gott", ",", "wie", "du", "vor", "bist", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "APPR", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df wir genesen!", "tokens": ["Da\u00df", "wir", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Die hier auf Erden deine Stelle halten,", "tokens": ["Die", "hier", "auf", "Er\u00b7den", "dei\u00b7ne", "Stel\u00b7le", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die wollen h\u00f6her, als sie sollen, walten;", "tokens": ["Die", "wol\u00b7len", "h\u00f6\u00b7her", ",", "als", "sie", "sol\u00b7len", ",", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "$,", "KOUS", "PPER", "VMFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Seele, die dir Gott nur will geb\u00fcren,", "tokens": ["Die", "See\u00b7le", ",", "die", "dir", "Gott", "nur", "will", "ge\u00b7b\u00fc\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Woll'n sie regieren.", "tokens": ["Woll'n", "sie", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Drum nimm dich dessen an, das dir geh\u00f6ret!", "tokens": ["Drum", "nimm", "dich", "des\u00b7sen", "an", ",", "das", "dir", "ge\u00b7h\u00f6\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PDS", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhalt' uns das, was dein Mund uns gelehret!", "tokens": ["Er\u00b7halt'", "uns", "das", ",", "was", "dein", "Mund", "uns", "ge\u00b7leh\u00b7ret", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "La\u00df uns von dir durch Zwang, Gewalt und Leiden", "tokens": ["La\u00df", "uns", "von", "dir", "durch", "Zwang", ",", "Ge\u00b7walt", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Keinmal abscheiden!", "tokens": ["Kein\u00b7mal", "ab\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.10": {"line.1": {"text": "Sondern tritt freundlich uns zu unser Seiten,", "tokens": ["Son\u00b7dern", "tritt", "freund\u00b7lich", "uns", "zu", "un\u00b7ser", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hilf wider dein' und unsre Feinde streiten,", "tokens": ["Hilf", "wi\u00b7der", "dein'", "und", "uns\u00b7re", "Fein\u00b7de", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sich zusammenrotten und stark k\u00e4mpfen,", "tokens": ["Die", "sich", "zu\u00b7sam\u00b7men\u00b7rot\u00b7ten", "und", "stark", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+++-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Dein Wort zu d\u00e4mpfen.", "tokens": ["Dein", "Wort", "zu", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Wir wollen hier nach deinem Willen dulden,", "tokens": ["Wir", "wol\u00b7len", "hier", "nach", "dei\u00b7nem", "Wil\u00b7len", "dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was du uns zuerkennst f\u00fcr unsre Schulden,", "tokens": ["Was", "du", "uns", "zu\u00b7er\u00b7kennst", "f\u00fcr", "uns\u00b7re", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur da\u00df uns ", "tokens": ["Nur", "da\u00df", "uns"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Selig gelinget.", "tokens": ["Se\u00b7lig", "ge\u00b7lin\u00b7get", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}