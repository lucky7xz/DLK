{"textgrid.poem.57412": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Ungeheuer wurde zerschmettert, liegt", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Ungeheuer wurde zerschmettert, liegt", "tokens": ["Das", "Un\u00b7ge\u00b7heu\u00b7er", "wur\u00b7de", "zer\u00b7schmet\u00b7tert", ",", "liegt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gestreckt in seiner H\u00f6hle, die Jakobszunft;", "tokens": ["Ge\u00b7streckt", "in", "sei\u00b7ner", "H\u00f6h\u00b7le", ",", "die", "Ja\u00b7kobs\u00b7zunft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch ward der H\u00f6hle Schlund vom Felsen,", "tokens": ["Doch", "ward", "der", "H\u00f6h\u00b7le", "Schlund", "vom", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den sie ihm w\u00e4lzten, nicht ganz gef\u00fcllet.", "tokens": ["Den", "sie", "ihm", "w\u00e4lz\u00b7ten", ",", "nicht", "ganz", "ge\u00b7f\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "PTKNEG", "ADV", "VVPP", "$."], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Er hauchet Pest! Dem korsischen J\u00fcnglinge", "tokens": ["Er", "hau\u00b7chet", "Pest", "!", "Dem", "kor\u00b7si\u00b7schen", "J\u00fcng\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat die sein Haupt so, so ihm das Herz entflammt,", "tokens": ["Hat", "die", "sein", "Haupt", "so", ",", "so", "ihm", "das", "Herz", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPOSAT", "NN", "ADV", "$,", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df er euch mit gehobnem Schwerte,", "tokens": ["Da\u00df", "er", "euch", "mit", "ge\u00b7hob\u00b7nem", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "V\u00f6lker Hesperiens, Freiheit aufjocht.", "tokens": ["V\u00f6l\u00b7ker", "Hes\u00b7pe\u00b7ri\u00b7ens", ",", "Frei\u00b7heit", "auf\u00b7jocht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "VVPP", "$."], "meter": "+-----+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wie schwach sind eines Kriegers Bewunderer,", "tokens": ["Wie", "schwach", "sind", "ei\u00b7nes", "Krie\u00b7gers", "Be\u00b7wun\u00b7de\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der sie, die sch\u00f6nste Sch\u00f6pfung der sp\u00e4ten Welt,", "tokens": ["Der", "sie", ",", "die", "sch\u00f6ns\u00b7te", "Sch\u00f6p\u00b7fung", "der", "sp\u00e4\u00b7ten", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Freiheit, in den Staub tritt, andre", "tokens": ["Die", "Frei\u00b7heit", ",", "in", "den", "Staub", "tritt", ",", "and\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bildung des Staats, als ihr w\u00e4hlt, gebietend!", "tokens": ["Bil\u00b7dung", "des", "Staats", ",", "als", "ihr", "w\u00e4hlt", ",", "ge\u00b7bie\u00b7tend", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Vielleicht verg\u00e4\u00dft ihr, Dulder! die plastischen", "tokens": ["Viel\u00b7leicht", "ver\u00b7g\u00e4\u00dft", "ihr", ",", "Dul\u00b7der", "!", "die", "plas\u00b7ti\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$.", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gewaltsamkeiten: w\u00e4ren sie mehr als Wort,", "tokens": ["Ge\u00b7walt\u00b7sam\u00b7kei\u00b7ten", ":", "w\u00e4\u00b7ren", "sie", "mehr", "als", "Wort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das stumm wird vor der Sklavenkette", "tokens": ["Das", "stumm", "wird", "vor", "der", "Skla\u00b7ven\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rasseln, die euch die Beherrschung anlegt.", "tokens": ["Ras\u00b7seln", ",", "die", "euch", "die", "Be\u00b7herr\u00b7schung", "an\u00b7legt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Da\u00df er sein Volk ganz blende, beschw\u00f6rt er, schickt", "tokens": ["Da\u00df", "er", "sein", "Volk", "ganz", "blen\u00b7de", ",", "be\u00b7schw\u00f6rt", "er", ",", "schickt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJA", "$,", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kunstzauber, reicht Apollo den Wanderstab.", "tokens": ["Kunst\u00b7zau\u00b7ber", ",", "reicht", "A\u00b7pol\u00b7lo", "den", "Wan\u00b7der\u00b7stab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NE", "ART", "NN", "$."], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "O wird die ", "tokens": ["O", "wird", "die"], "token_info": ["word", "word", "word"], "pos": ["NE", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Tilger nicht Lethe, wie dem der Ligue.", "tokens": ["Til\u00b7ger", "nicht", "Le\u00b7the", ",", "wie", "dem", "der", "Li\u00b7gue", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "NN", "$,", "PWAV", "ART", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Nicht Belvederer ist der Apollo dann,", "tokens": ["Nicht", "Bel\u00b7ve\u00b7de\u00b7rer", "ist", "der", "A\u00b7pol\u00b7lo", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "ART", "NE", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn neben Heinrich er in der ", "tokens": ["Wenn", "ne\u00b7ben", "Hein\u00b7rich", "er", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "PPER", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er sieht dann Schlamm nur, und vor Schlamme", "tokens": ["Er", "sieht", "dann", "Schlamm", "nur", ",", "und", "vor", "Schlam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ADV", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kaum den Besieger des zweiten Python.", "tokens": ["Kaum", "den", "Be\u00b7sie\u00b7ger", "des", "zwei\u00b7ten", "Py\u00b7thon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer dieses Grab des lange verg\u00f6tterten", "tokens": ["Wer", "die\u00b7ses", "Grab", "des", "lan\u00b7ge", "ver\u00b7g\u00f6t\u00b7ter\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heinrichs voraussah, mag auch das K\u00fcnftige", "tokens": ["Hein\u00b7richs", "vor\u00b7aus\u00b7sah", ",", "mag", "auch", "das", "K\u00fcnf\u00b7ti\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "VMFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Des Volks weissagen, das in jeder", "tokens": ["Des", "Volks", "weis\u00b7sa\u00b7gen", ",", "das", "in", "je\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leidenschaft Strom unerrettbar treibet!", "tokens": ["Lei\u00b7den\u00b7schaft", "Strom", "un\u00b7er\u00b7rett\u00b7bar", "trei\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Erw\u00e4gt, durchdenkt es, Deutsche, mit euerm Ernst.", "tokens": ["Er\u00b7w\u00e4gt", ",", "durch\u00b7denkt", "es", ",", "Deut\u00b7sche", ",", "mit", "eu\u00b7erm", "Ernst", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "ADJA", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wollt denen euer Schicksal, der Kinder Heil", "tokens": ["Wollt", "de\u00b7nen", "eu\u00b7er", "Schick\u00b7sal", ",", "der", "Kin\u00b7der", "Heil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PDS", "PPOSAT", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr anvertrauen, die in jeder", "tokens": ["Ihr", "an\u00b7ver\u00b7trau\u00b7en", ",", "die", "in", "je\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PRELS", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leidenschaft Strom unerrettbar treiben?", "tokens": ["Lei\u00b7den\u00b7schaft", "Strom", "un\u00b7er\u00b7rett\u00b7bar", "trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}