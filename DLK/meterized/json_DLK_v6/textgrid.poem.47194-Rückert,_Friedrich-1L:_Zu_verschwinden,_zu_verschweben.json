{"textgrid.poem.47194": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu verschwinden, zu verschweben", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu verschwinden, zu verschweben", "tokens": ["Zu", "ver\u00b7schwin\u00b7den", ",", "zu", "ver\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ins glanzvolle Leere;", "tokens": ["Ins", "glanz\u00b7vol\u00b7le", "Lee\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ohne vor dem Tod zu beben,", "tokens": ["Oh\u00b7ne", "vor", "dem", "Tod", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnt' ich mich darein ergeben,", "tokens": ["K\u00f6nnt'", "ich", "mich", "da\u00b7rein", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tropfen gleich im Meere.", "tokens": ["Trop\u00b7fen", "gleich", "im", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Aber seh' ich die erblassen,", "tokens": ["A\u00b7ber", "seh'", "ich", "die", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir teuer waren,", "tokens": ["Die", "mir", "teu\u00b7er", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die will ich nicht schwinden lassen,", "tokens": ["Die", "will", "ich", "nicht", "schwin\u00b7den", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern halten fest und fassen", "tokens": ["Son\u00b7dern", "hal\u00b7ten", "fest", "und", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nicht lassen fahren.", "tokens": ["Und", "nicht", "las\u00b7sen", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Eigene Pers\u00f6nlichkeit", "tokens": ["Ei\u00b7ge\u00b7ne", "Per\u00b7s\u00f6n\u00b7lich\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Lie\u00df ich eh'r mir rauben,", "tokens": ["Lie\u00df", "ich", "eh'r", "mir", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Als, da ihr gestorben seid,", "tokens": ["Als", ",", "da", "ihr", "ge\u00b7stor\u00b7ben", "seid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht an die Unsterblichkeit", "tokens": ["Nicht", "an", "die", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Meiner Toten glauben.", "tokens": ["Mei\u00b7ner", "To\u00b7ten", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}