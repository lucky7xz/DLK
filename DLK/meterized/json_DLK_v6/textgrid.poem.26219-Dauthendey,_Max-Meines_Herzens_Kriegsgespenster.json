{"textgrid.poem.26219": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Meines Herzens Kriegsgespenster", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir war, ich h\u00f6rte drau\u00dfen am Kabinenfenster", "tokens": ["Mir", "war", ",", "ich", "h\u00f6r\u00b7te", "drau\u00b7\u00dfen", "am", "Ka\u00b7bi\u00b7nen\u00b7fens\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Nacht Hurrageschrei im Meer.", "tokens": ["Zur", "Nacht", "Hur\u00b7ra\u00b7ge\u00b7schrei", "im", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es waren meines Herzens Kriegsgespenster,", "tokens": ["Es", "wa\u00b7ren", "mei\u00b7nes", "Her\u00b7zens", "Kriegs\u00b7ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die zogen johlend in der Nacht umher.", "tokens": ["Die", "zo\u00b7gen", "joh\u00b7lend", "in", "der", "Nacht", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie schlugen Schlachten um das Schiff im Wind.", "tokens": ["Sie", "schlu\u00b7gen", "Schlach\u00b7ten", "um", "das", "Schiff", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ach, w\u00fc\u00dft' ich, ob sie Sieger, ob Besiegte sind!", "tokens": ["Ach", ",", "w\u00fc\u00dft'", "ich", ",", "ob", "sie", "Sie\u00b7ger", ",", "ob", "Be\u00b7sieg\u00b7te", "sind", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "$,", "KOUS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}