{"dta.poem.13482": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da fahr' ich still im Wagen,", "tokens": ["Da", "fahr'", "ich", "still", "im", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du bist so weit von mir,", "tokens": ["Du", "bist", "so", "weit", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohin er mich mag tragen,", "tokens": ["Wo\u00b7hin", "er", "mich", "mag", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bleibe doch bei dir.", "tokens": ["Ich", "blei\u00b7be", "doch", "bei", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da fliegen W\u00e4lder, Kl\u00fcfte", "tokens": ["Da", "flie\u00b7gen", "W\u00e4l\u00b7der", ",", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sch\u00f6ne Th\u00e4ler tief,", "tokens": ["Und", "sch\u00f6\u00b7ne", "Th\u00e4\u00b7ler", "tief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Lerchen hoch in L\u00fcften,", "tokens": ["Und", "Ler\u00b7chen", "hoch", "in", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als ob dein' Stimme rief'.", "tokens": ["Als", "ob", "dein'", "Stim\u00b7me", "rie\u00b7f'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Die Sonne lustig scheinet", "tokens": ["Die", "Son\u00b7ne", "lus\u00b7tig", "schei\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit \u00fcber das Revier,", "tokens": ["Weit", "\u00fc\u00b7ber", "das", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich bin so froh verweinet", "tokens": ["Ich", "bin", "so", "froh", "ver\u00b7wei\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und singe still in mir.", "tokens": ["Und", "sin\u00b7ge", "still", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vom Berge geht's hinunter,", "tokens": ["Vom", "Ber\u00b7ge", "geht's", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Posthorn schallt im Grund,", "tokens": ["Das", "Post\u00b7horn", "schallt", "im", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein' Seel' wird mir so munter,", "tokens": ["Mein'", "Seel'", "wird", "mir", "so", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gr\u00fc\u00df' dich aus Herzensgrund!", "tokens": ["Gr\u00fc\u00df'", "dich", "aus", "Her\u00b7zens\u00b7grund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}