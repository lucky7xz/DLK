{"textgrid.poem.53646": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Marke: Essig", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was des Nachts in Nepplokalen", "tokens": ["Was", "des", "Nachts", "in", "Nep\u00b7plo\u00b7ka\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dir die Kellner leis empfahlen \u2013", "tokens": ["dir", "die", "Kell\u00b7ner", "leis", "emp\u00b7fah\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "was dir scheu\u00dflich s\u00fc\u00dflich schmeckt:", "tokens": ["was", "dir", "scheu\u00df\u00b7lich", "s\u00fc\u00df\u00b7lich", "schmeckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "das ist \u2013 Mumm ist allzu teuer \u2013", "tokens": ["das", "ist", "\u2013", "Mumm", "ist", "all\u00b7zu", "teu\u00b7er", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "NE", "VAFIN", "PTKA", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "das ist mit der Stempelsteuer", "tokens": ["das", "ist", "mit", "der", "Stem\u00b7pel\u00b7steu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "deutscher Sekt, deutscher Sekt.", "tokens": ["deut\u00b7scher", "Sekt", ",", "deut\u00b7scher", "Sekt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Was aus Scham vorm Etikette", "tokens": ["Was", "aus", "Scham", "vorm", "E\u00b7ti\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in die wei\u00dfliche Serviette", "tokens": ["in", "die", "wei\u00df\u00b7li\u00b7che", "Ser\u00b7viet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "scheu der Oberkellner steckt \u2013", "tokens": ["scheu", "der", "O\u00b7ber\u00b7kell\u00b7ner", "steckt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "das ist \u2013 wei\u00df man denn, was drin is? \u2013", "tokens": ["das", "ist", "\u2013", "wei\u00df", "man", "denn", ",", "was", "drin", "is", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "$(", "VVFIN", "PIS", "ADV", "$,", "PRELS", "ADV", "FM", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Leibgetr\u00e4nk vom Hause Stinnes:", "tokens": ["Leib\u00b7ge\u00b7tr\u00e4nk", "vom", "Hau\u00b7se", "Stin\u00b7nes", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "deutscher Sekt, deutscher Sekt.", "tokens": ["deut\u00b7scher", "Sekt", ",", "deut\u00b7scher", "Sekt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Eisgek\u00fchlt und innen hitzig.", "tokens": ["Eis\u00b7ge\u00b7k\u00fchlt", "und", "in\u00b7nen", "hit\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Falsche Kohlens\u00e4ure. Spritzig.", "tokens": ["Fal\u00b7sche", "Koh\u00b7len\u00b7s\u00e4u\u00b7re", ".", "Sprit\u00b7zig", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinterh\u00e4ltig. Aufgeregt.", "tokens": ["Hin\u00b7ter\u00b7h\u00e4l\u00b7tig", ".", "Auf\u00b7ge\u00b7regt", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$.", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist nicht nur ein Getr\u00e4nke.", "tokens": ["Das", "ist", "nicht", "nur", "ein", "Ge\u00b7tr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Das ist in der Reichswehrsch\u00e4nke", "tokens": ["Das", "ist", "in", "der", "Reichs\u00b7wehr\u00b7sch\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "deutscher Seeckt. Deutscher Seeckt.", "tokens": ["deut\u00b7scher", "Seeckt", ".", "Deut\u00b7scher", "Seeckt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}