{"textgrid.poem.57811": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Reitersmann", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bl\u00fchen die Rosen,", "tokens": ["Es", "bl\u00fc\u00b7hen", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Nachtigall singt,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "singt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Herz ist voll Freude,", "tokens": ["Mein", "Herz", "ist", "voll", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Vor Freude es springt;", "tokens": ["Vor", "Freu\u00b7de", "es", "springt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ein Reiter zu Pferde,", "tokens": ["Ein", "Rei\u00b7ter", "zu", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So reit' ich durchs Land,", "tokens": ["So", "reit'", "ich", "durchs", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "F\u00fcr Kaiser und K\u00f6nig", "tokens": ["F\u00fcr", "Kai\u00b7ser", "und", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und Vaterland.", "tokens": ["Und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Im Wirtshaus am Wege,", "tokens": ["Im", "Wirts\u00b7haus", "am", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da kehren wir ein,", "tokens": ["Da", "keh\u00b7ren", "wir", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und trinken ein Gl\u00e4slein", "tokens": ["Und", "trin\u00b7ken", "ein", "Gl\u00e4s\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vom goldenen Wein;", "tokens": ["Vom", "gol\u00b7de\u00b7nen", "Wein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Du H\u00fcbsche, du Feine,", "tokens": ["Du", "H\u00fcb\u00b7sche", ",", "du", "Fei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Komm' setz' dich zu mir,", "tokens": ["Komm'", "setz'", "dich", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ein Ringlein von Golde,", "tokens": ["Ein", "Rin\u00b7glein", "von", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das schenke ich dir.", "tokens": ["Das", "schen\u00b7ke", "ich", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und ist sie geschlagen,", "tokens": ["Und", "ist", "sie", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die blutige Schlacht,", "tokens": ["Die", "blu\u00b7ti\u00b7ge", "Schlacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und haben wir Frieden", "tokens": ["Und", "ha\u00b7ben", "wir", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit Frankreich gemacht,", "tokens": ["Mit", "Fran\u00b7kreich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dann binde den Schimmel", "tokens": ["Dann", "bin\u00b7de", "den", "Schim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ich wieder hier an,", "tokens": ["Ich", "wie\u00b7der", "hier", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Denn treu ist, ja treu ist", "tokens": ["Denn", "treu", "ist", ",", "ja", "treu", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "ADV", "ADJD", "VAFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der Reitersmann.", "tokens": ["Der", "Rei\u00b7ters\u00b7mann", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}