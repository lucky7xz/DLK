{"textgrid.poem.67807": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3. Die kranke Braut", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchs Birkenw\u00e4ldchen,", "tokens": ["Durchs", "Bir\u00b7ken\u00b7w\u00e4ld\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Durchs Fichtenw\u00e4ldchen,", "tokens": ["Durchs", "Fich\u00b7ten\u00b7w\u00e4ld\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Trug mich mein Hengst, mein Brauner,", "tokens": ["Trug", "mich", "mein", "Hengst", ",", "mein", "Brau\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Schwiegervaters H\u00f6fchen.", "tokens": ["Zu", "Schwie\u00b7ger\u00b7va\u00b7ters", "H\u00f6f\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n Tag! Sch\u00f6n Abend!", "tokens": ["Sch\u00f6n", "Tag", "!", "Sch\u00f6n", "A\u00b7bend", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Frau Schwieger, liebe,", "tokens": ["Frau", "Schwie\u00b7ger", ",", "lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was macht mein liebes M\u00e4dchen?", "tokens": ["Was", "macht", "mein", "lie\u00b7bes", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was macht mein junges M\u00e4dchen?", "tokens": ["Was", "macht", "mein", "jun\u00b7ges", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Krank ist dein M\u00e4dchen,", "tokens": ["Krank", "ist", "dein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "O! krank von Herzen,", "tokens": ["O", "!", "krank", "von", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Dort in der neuen Tenne,", "tokens": ["Dort", "in", "der", "neu\u00b7en", "Ten\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In ihrem gr\u00fcnen Bettchen.", "tokens": ["In", "ih\u00b7rem", "gr\u00fc\u00b7nen", "Bett\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da \u00fcbern Hof ich,", "tokens": ["Da", "\u00fc\u00b7bern", "Hof", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und herzlich weint' ich,", "tokens": ["Und", "herz\u00b7lich", "weint'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und vor der Th\u00fcre", "tokens": ["Und", "vor", "der", "Th\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wischt' ich die Thr\u00e4nen.", "tokens": ["Wischt'", "ich", "die", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ich dr\u00fcckt' ihr H\u00e4ndchen,", "tokens": ["Ich", "dr\u00fcckt'", "ihr", "H\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Streift' ihr den Ring auf:", "tokens": ["Streift'", "ihr", "den", "Ring", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wirds dir nicht besser, M\u00e4dchen?", "tokens": ["Wirds", "dir", "nicht", "bes\u00b7ser", ",", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Nicht besser, junges M\u00e4dchen?", "tokens": ["Nicht", "bes\u00b7ser", ",", "jun\u00b7ges", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mir wird nicht besser,", "tokens": ["Mir", "wird", "nicht", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nicht deine Braut mehr!", "tokens": ["Nicht", "dei\u00b7ne", "Braut", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Du wirst mich nicht betrauren,", "tokens": ["Du", "wirst", "mich", "nicht", "be\u00b7trau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach andern wirst du gaffen.", "tokens": ["Nach", "an\u00b7dern", "wirst", "du", "gaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Durch diese Th\u00fcre", "tokens": ["Durch", "die\u00b7se", "Th\u00fc\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wirst du mich tragen;", "tokens": ["Wirst", "du", "mich", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durch jene reiten G\u00e4ste.", "tokens": ["Durch", "je\u00b7ne", "rei\u00b7ten", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gef\u00e4llt dir jenes M\u00e4dchen?", "tokens": ["Ge\u00b7f\u00e4llt", "dir", "je\u00b7nes", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gef\u00e4llt dirs junge M\u00e4dchen?", "tokens": ["Ge\u00b7f\u00e4llt", "dirs", "jun\u00b7ge", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}