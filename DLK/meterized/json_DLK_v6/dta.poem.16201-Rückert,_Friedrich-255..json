{"dta.poem.16201": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "255.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer selber sich beherrscht, beherrschet auch die Welt,", "tokens": ["Wer", "sel\u00b7ber", "sich", "be\u00b7herrscht", ",", "be\u00b7herr\u00b7schet", "auch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "VVPP", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil stets das Aeu\u00dfere des Innern Spiegel h\u00e4lt.", "tokens": ["Weil", "stets", "das", "A\u00b7e\u00b7u\u00b7\u00dfe\u00b7re", "des", "In\u00b7nern", "Spie\u00b7gel", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Wer sich beherrscht, den kann beherrschen au\u00dfenher", "tokens": ["Wer", "sich", "be\u00b7herrscht", ",", "den", "kann", "be\u00b7herr\u00b7schen", "au\u00b7\u00dfen\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "VVPP", "$,", "ART", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Herrscher, denn allein im Aeu\u00dfern herrschet der.", "tokens": ["Kein", "Herr\u00b7scher", ",", "denn", "al\u00b7lein", "im", "A\u00b7e\u00b7u\u00b7\u00dfern", "herr\u00b7schet", "der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "$."], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Bedingen kann er dich mit Macht und dich umringen,", "tokens": ["Be\u00b7din\u00b7gen", "kann", "er", "dich", "mit", "Macht", "und", "dich", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eindringen kann er nicht und in dir dich bezwingen.", "tokens": ["Ein\u00b7drin\u00b7gen", "kann", "er", "nicht", "und", "in", "dir", "dich", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "KON", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Antasten kann er nicht dein eignes Herrscherthum,", "tokens": ["An\u00b7tas\u00b7ten", "kann", "er", "nicht", "dein", "eig\u00b7nes", "Herr\u00b7scher\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du aber g\u00f6nnest gern ihm seinen Herrscherruhm;", "tokens": ["Du", "a\u00b7ber", "g\u00f6n\u00b7nest", "gern", "ihm", "sei\u00b7nen", "Herr\u00b7scher\u00b7ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie du dem Blitze g\u00f6nnst, dem Sturmwind seine Fl\u00fcgel:", "tokens": ["Wie", "du", "dem", "Blit\u00b7ze", "g\u00f6nnst", ",", "dem", "Sturm\u00b7wind", "sei\u00b7ne", "Fl\u00fc\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Z\u00fcgel halte dich! Gott h\u00e4lt die Welt im Z\u00fcgel.", "tokens": ["Im", "Z\u00fc\u00b7gel", "hal\u00b7te", "dich", "!", "Gott", "h\u00e4lt", "die", "Welt", "im", "Z\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}