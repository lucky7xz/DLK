{"textgrid.poem.52736": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Matthis-sonate", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am fernen H\u00fcgelp\u00e4rchen", "tokens": ["Am", "fer\u00b7nen", "H\u00fc\u00b7gel\u00b7p\u00e4r\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stirbt Ph\u00f6bus' ros'ger Strahl,", "tokens": ["Stirbt", "Ph\u00f6\u00b7bus'", "ros'\u00b7ger", "Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und duftend wie ein M\u00e4rchen", "tokens": ["Und", "duf\u00b7tend", "wie", "ein", "M\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verschleiert sich das Thal;", "tokens": ["Ver\u00b7schlei\u00b7ert", "sich", "das", "Thal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es l\u00e4uten Heerdenglocken", "tokens": ["Es", "l\u00e4u\u00b7ten", "Heer\u00b7den\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Abschied von der Welt,", "tokens": ["Wie", "Ab\u00b7schied", "von", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und auf den Zwielichtsocken", "tokens": ["Und", "auf", "den", "Zwie\u00b7lich\u00b7tso\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schleicht Pan jetzt \u00fcber's Feld.", "tokens": ["Schleicht", "Pan", "jetzt", "\u00fc\u00b7ber's", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Espe Baumschlag pispert,", "tokens": ["Der", "Es\u00b7pe", "Baum\u00b7schlag", "pis\u00b7pert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Im Mondenglanz gebleicht,", "tokens": ["Im", "Mon\u00b7den\u00b7glanz", "ge\u00b7bleicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo vom Ger\u00f6hr umwispert", "tokens": ["Wo", "vom", "Ge\u00b7r\u00f6hr", "um\u00b7wis\u00b7pert"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Lied der Zirpchen schweigt;", "tokens": ["Das", "Lied", "der", "Zirp\u00b7chen", "schweigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es schl\u00fcrft ein Turtelt\u00e4ubchen", "tokens": ["Es", "schl\u00fcrft", "ein", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Abzugsrinnkanal,", "tokens": ["Im", "Ab\u00b7zugs\u00b7rinn\u00b7ka\u00b7nal", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und selbst das Wieselweibchen", "tokens": ["Und", "selbst", "das", "Wie\u00b7sel\u00b7weib\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verzehrt sein Abendmahl.", "tokens": ["Ver\u00b7zehrt", "sein", "A\u00b7bend\u00b7mahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des Baches munt're Stelze", "tokens": ["Des", "Ba\u00b7ches", "munt'\u00b7re", "Stel\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00fcpft \u00fcber Kies und Schurf,", "tokens": ["H\u00fcpft", "\u00fc\u00b7ber", "Kies", "und", "Schurf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem Sommerpelze", "tokens": ["Und", "in", "dem", "Som\u00b7mer\u00b7pel\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stolziert der M\u00e4ulerwurf;", "tokens": ["Stol\u00b7ziert", "der", "M\u00e4u\u00b7ler\u00b7wurf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Hoch ragen in die Landschaft", "tokens": ["Hoch", "ra\u00b7gen", "in", "die", "Land\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die Triften sanft im Saum,", "tokens": ["Die", "Trif\u00b7ten", "sanft", "im", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Indessen aus dem Sand schafft", "tokens": ["In\u00b7des\u00b7sen", "aus", "dem", "Sand", "schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich die Ameise kaum.", "tokens": ["Sich", "die", "A\u00b7mei\u00b7se", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Gl\u00fchwurm mag beneiden", "tokens": ["Der", "Gl\u00fch\u00b7wurm", "mag", "be\u00b7nei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Irrwischchen \u00fcber'm Teich,", "tokens": ["Irr\u00b7wi\u00b7schchen", "\u00fc\u00b7ber'm", "Teich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drin reiche Trauerweiden", "tokens": ["Drin", "rei\u00b7che", "Trau\u00b7er\u00b7wei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich baden wehmuthweich.", "tokens": ["Sich", "ba\u00b7den", "weh\u00b7muth\u00b7weich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Stechm\u00fccke, Wespe, Spinne,", "tokens": ["Stech\u00b7m\u00fc\u00b7cke", ",", "Wes\u00b7pe", ",", "Spin\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Libelle und Scorpion,", "tokens": ["Li\u00b7bel\u00b7le", "und", "Scor\u00b7pi\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Verauscht von Abendminne,", "tokens": ["Ver\u00b7auscht", "von", "A\u00b7bend\u00b7min\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Lauscht der Cascade Ton.", "tokens": ["Lauscht", "der", "Ca\u00b7sca\u00b7de", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Am eppichlosen Pfosten", "tokens": ["Am", "ep\u00b7pich\u00b7lo\u00b7sen", "Pfos\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ruht unbequem der Pflug,", "tokens": ["Ruht", "un\u00b7be\u00b7quem", "der", "Pflug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In braunen Scheunen rosten", "tokens": ["In", "brau\u00b7nen", "Scheu\u00b7nen", "ros\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bethauter Schaufeln g'nug.", "tokens": ["Be\u00b7thau\u00b7ter", "Schau\u00b7feln", "g'\u00b7nug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Es irrt an meine Scheitel", "tokens": ["Es", "irrt", "an", "mei\u00b7ne", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die flederhafte Maus,", "tokens": ["Die", "fle\u00b7der\u00b7haf\u00b7te", "Maus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Um Alles, weil es eitel,", "tokens": ["Um", "Al\u00b7les", ",", "weil", "es", "ei\u00b7tel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bricht mir das Wasser aus.", "tokens": ["Bricht", "mir", "das", "Was\u00b7ser", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "S\u00fc\u00df hauchen Veilchenraine", "tokens": ["S\u00fc\u00df", "hau\u00b7chen", "Veil\u00b7chen\u00b7rai\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Flieder \u00fcbert\u00e4ubt,", "tokens": ["Von", "Flie\u00b7der", "\u00fc\u00b7bert\u00b7\u00e4ubt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dagegen sich vom Haine", "tokens": ["Da\u00b7ge\u00b7gen", "sich", "vom", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Lege D\u00fcften str\u00e4ubt;", "tokens": ["Der", "Le\u00b7ge", "D\u00fcf\u00b7ten", "str\u00e4ubt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Weihmutstannenwipfel", "tokens": ["Im", "Weih\u00b7muts\u00b7tan\u00b7nen\u00b7wip\u00b7fel"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Girrt zephyrhafter Wind,", "tokens": ["Girrt", "ze\u00b7phyr\u00b7haf\u00b7ter", "Wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und mit dem Schnupftuchzipfel", "tokens": ["Und", "mit", "dem", "Schnupf\u00b7tuch\u00b7zip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wischt sich die Nas' ein Kind.", "tokens": ["Wischt", "sich", "die", "Nas'", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auf seichtem Wellentanze", "tokens": ["Auf", "seich\u00b7tem", "Wel\u00b7len\u00b7tan\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gr\u00fc\u00dft Luna eignen Harm,", "tokens": ["Gr\u00fc\u00dft", "Lu\u00b7na", "eig\u00b7nen", "Harm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und an dem Faselschwanze", "tokens": ["Und", "an", "dem", "Fa\u00b7sel\u00b7schwan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Summt wilder Bremsenschwarm.", "tokens": ["Summt", "wil\u00b7der", "Brem\u00b7sen\u00b7schwarm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Um meine Schl\u00e4fe flechten", "tokens": ["Um", "mei\u00b7ne", "Schl\u00e4\u00b7fe", "flech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schilflilien sich des Teichs,", "tokens": ["Schilf\u00b7li\u00b7li\u00b7en", "sich", "des", "Teichs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo holde Nymphen rechten", "tokens": ["Wo", "hol\u00b7de", "Nym\u00b7phen", "rech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Faunen des Gestr\u00e4uchs.", "tokens": ["Mit", "Fau\u00b7nen", "des", "Ge\u00b7str\u00e4uchs", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dort liegt ein Ziegensch\u00e4fer", "tokens": ["Dort", "liegt", "ein", "Zie\u00b7gen\u00b7sch\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Tr\u00e4umen auf dem Moos,", "tokens": ["In", "Tr\u00e4u\u00b7men", "auf", "dem", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und vierzig Maienk\u00e4fer", "tokens": ["Und", "vier\u00b7zig", "Mai\u00b7en\u00b7k\u00e4\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["KON", "CARD", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Entsurren seinem Schoo\u00df.", "tokens": ["Ent\u00b7sur\u00b7ren", "sei\u00b7nem", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O wundersch\u00f6ner Abend,", "tokens": ["O", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der heute Abend ist,", "tokens": ["Der", "heu\u00b7te", "A\u00b7bend", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und du, zur M\u00fchle trabend,", "tokens": ["Und", "du", ",", "zur", "M\u00fch\u00b7le", "tra\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O Esel, sei gegr\u00fc\u00dft!", "tokens": ["O", "E\u00b7sel", ",", "sei", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}