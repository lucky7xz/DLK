{"dta.poem.11966": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Reisevorrath ist gering und weit die Reise;", "tokens": ["Der", "Rei\u00b7se\u00b7vor\u00b7rath", "ist", "ge\u00b7ring", "und", "weit", "die", "Rei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach, als er sich zum Tod bereitete, der Weise,", "tokens": ["Sprach", ",", "als", "er", "sich", "zum", "Tod", "be\u00b7rei\u00b7te\u00b7te", ",", "der", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und fand, da\u00df Alles was er hier gestrebt im Leben,", "tokens": ["Und", "fand", ",", "da\u00df", "Al\u00b7les", "was", "er", "hier", "ge\u00b7strebt", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "PWS", "PPER", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm wenig F\u00f6rderung f\u00fcr Jenseits konnte geben.", "tokens": ["Ihm", "we\u00b7nig", "F\u00f6r\u00b7de\u00b7rung", "f\u00fcr", "Jen\u00b7seits", "konn\u00b7te", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPR", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}