{"dta.poem.16034": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du ans G\u00f6ttliche stets halten willst dein Streben,", "tokens": ["Wenn", "du", "ans", "G\u00f6tt\u00b7li\u00b7che", "stets", "hal\u00b7ten", "willst", "dein", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVINF", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie kanns davor bestehn? du mu\u00dft es ganz aufgeben.", "tokens": ["Wie", "kanns", "da\u00b7vor", "be\u00b7stehn", "?", "du", "mu\u00dft", "es", "ganz", "auf\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PAV", "VVINF", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "Doch, ist vom G\u00f6ttlichen dein Streben abgekehrt,", "tokens": ["Doch", ",", "ist", "vom", "G\u00f6tt\u00b7li\u00b7chen", "dein", "Stre\u00b7ben", "ab\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hats gar alle Kraft verloren, allen Werth.", "tokens": ["So", "hats", "gar", "al\u00b7le", "Kraft", "ver\u00b7lo\u00b7ren", ",", "al\u00b7len", "Werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In einer Mitte nur von fern und nah gewannst", "tokens": ["In", "ei\u00b7ner", "Mit\u00b7te", "nur", "von", "fern", "und", "nah", "ge\u00b7wannst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "ADJD", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du einen Standpunkt, wo du etwas willst und kannst.", "tokens": ["Du", "ei\u00b7nen", "Stand\u00b7punkt", ",", "wo", "du", "et\u00b7was", "willst", "und", "kannst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PWAV", "PPER", "PIS", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "So hat dich Gott gestellt, und l\u00e4\u00dft dich wirken gerne", "tokens": ["So", "hat", "dich", "Gott", "ge\u00b7stellt", ",", "und", "l\u00e4\u00dft", "dich", "wir\u00b7ken", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVPP", "$,", "KON", "VVFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Werk, und wirkt durch dich, dir nah zugleich und ferne.", "tokens": ["Dein", "Werk", ",", "und", "wirkt", "durch", "dich", ",", "dir", "nah", "zu\u00b7gleich", "und", "fer\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "PPER", "$,", "PPER", "ADJD", "ADV", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sowie ein Wandelstern die Kraft der Sonne braucht,", "tokens": ["So\u00b7wie", "ein", "Wan\u00b7dels\u00b7tern", "die", "Kraft", "der", "Son\u00b7ne", "braucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der er sich nicht entzieht, und nicht hinein sich taucht.", "tokens": ["Der", "er", "sich", "nicht", "ent\u00b7zieht", ",", "und", "nicht", "hin\u00b7ein", "sich", "taucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "PTKNEG", "VVFIN", "$,", "KON", "PTKNEG", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}