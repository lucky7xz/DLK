{"dta.poem.5427": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lehre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Lebens wesentliches Gut ist eine Still' in unsrer\nSeelen,", "tokens": ["Des", "Le\u00b7bens", "we\u00b7sent\u00b7li\u00b7ches", "Gut", "ist", "ei\u00b7ne", "Still'", "in", "uns\u00b7rer", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+---+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Die, wenn man mit Vernunft verf\u00e4hrt, wir uns nach", "tokens": ["Die", ",", "wenn", "man", "mit", "Ver\u00b7nunft", "ver\u00b7f\u00e4hrt", ",", "wir", "uns", "nach"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PIS", "APPR", "NN", "VVPP", "$,", "PPER", "PRF", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und uns zu einem Endzweck setzen. Es ist nichts n\u00f6htigers", "tokens": ["Und", "uns", "zu", "ei\u00b7nem", "End\u00b7zweck", "set\u00b7zen", ".", "Es", "ist", "nichts", "n\u00f6h\u00b7ti\u00b7gers"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVINF", "$.", "PPER", "VAFIN", "PIS", "ADJA"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Als da\u00df wir, dieser k\u00fcnft\u2019gen Ruh, uns, so viel m\u00f6glich", "tokens": ["Als", "da\u00df", "wir", ",", "die\u00b7ser", "k\u00fcnft'\u00b7gen", "Ruh", ",", "uns", ",", "so", "viel", "m\u00f6g\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "$,", "PDAT", "ADJA", "NN", "$,", "PPER", "$,", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Lust und Reitzung auf zu opfern, die uns die Gegen-", "tokens": ["Die", "Lust", "und", "Reit\u00b7zung", "auf", "zu", "op\u00b7fern", ",", "die", "uns", "die", "Ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und uns dadurch st\u00fcrtzen pfleget, eh\u2019 oft kaum wenig", "tokens": ["Und", "uns", "da\u00b7durch", "st\u00fcrt\u00b7zen", "pfle\u00b7get", ",", "eh'", "oft", "kaum", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PAV", "VVINF", "VVFIN", "$,", "KOUS", "ADV", "ADV", "PIS"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Es m\u00fcste keine Leidenschaft so lebhaft und so reitzend", "tokens": ["Es", "m\u00fcs\u00b7te", "kei\u00b7ne", "Lei\u00b7den\u00b7schaft", "so", "leb\u00b7haft", "und", "so", "reit\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "ADJD", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Da\u00df sie, durch sich, uns hindern sollte, das Urtheil nicht", "tokens": ["Da\u00df", "sie", ",", "durch", "sich", ",", "uns", "hin\u00b7dern", "soll\u00b7te", ",", "das", "Ur\u00b7theil", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PRF", "$,", "PPER", "VVINF", "VMFIN", "$,", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das, \u00fcber unser Thun und Lassen, dereinst wird von uns", "tokens": ["Das", ",", "\u00fc\u00b7ber", "un\u00b7ser", "Thun", "und", "Las\u00b7sen", ",", "de\u00b7reinst", "wird", "von", "uns"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$,", "ADV", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn eine kurtze Trunckenheit den Uberlegungen wird", "tokens": ["Wenn", "ei\u00b7ne", "kurt\u00b7ze", "Trun\u00b7cken\u00b7heit", "den", "U\u00b7ber\u00b7le\u00b7gun\u00b7gen", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Die ihr stets pflegen nachzufolgen. Vielleicht wirfst du", "tokens": ["Die", "ihr", "stets", "pfle\u00b7gen", "nach\u00b7zu\u00b7fol\u00b7gen", ".", "Viel\u00b7leicht", "wirfst", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVINF", "VVIZU", "$.", "ADV", "VAFIN", "PPER"], "meter": "--++-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Soll denn ein Trunckener so gut, als ob er n\u00fcchtern w\u00e4re,", "tokens": ["Soll", "denn", "ein", "Trun\u00b7cke\u00b7ner", "so", "gut", ",", "als", "ob", "er", "n\u00fcch\u00b7tern", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Das ist ja schlechter dings nicht m\u00f6glich. Doch h\u00f6r\u2019! ein", "tokens": ["Das", "ist", "ja", "schlech\u00b7ter", "dings", "nicht", "m\u00f6g\u00b7lich", ".", "Doch", "h\u00f6r'", "!", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "NN", "PTKNEG", "ADJD", "$.", "KON", "VVFIN", "$.", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nimmt sich doch auch beym Trunck in acht, in Spott und", "tokens": ["Nimmt", "sich", "doch", "auch", "beym", "Trunck", "in", "acht", ",", "in", "Spott", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "APPRART", "NN", "APPR", "CARD", "$,", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als wie ein wilder Trunckenbold, der stets dadurch in", "tokens": ["Als", "wie", "ein", "wil\u00b7der", "Trun\u00b7cken\u00b7bold", ",", "der", "stets", "da\u00b7durch", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PAV", "APPR"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Drum brauch (jetzt siehst du da\u00df ich dich nicht mit zu stren-", "tokens": ["Drum", "brauch", "(", "jetzt", "siehst", "du", "da\u00df", "ich", "dich", "nicht", "mit", "zu", "stren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "KOUS", "PPER", "PRF", "PTKNEG", "APPR", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der gegenw\u00e4rt\u2019gen Lust! doch so: da\u00df sie der k\u00fcnftigen", "tokens": ["Der", "ge\u00b7gen\u00b7w\u00e4rt'\u00b7gen", "Lust", "!", "doch", "so", ":", "da\u00df", "sie", "der", "k\u00fcnf\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "ADV", "$.", "KOUS", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}