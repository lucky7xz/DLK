{"dta.poem.1713": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Umwechslung aller Dinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Soll wahres Lob die Schuld der Schmeicheley verdienen/", "tokens": ["Soll", "wah\u00b7res", "Lob", "die", "Schuld", "der", "Schmei\u00b7che\u00b7ley", "ver\u00b7die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So darff ich mich forthin zu schreiben nicht erk\u00fchnen.", "tokens": ["So", "darff", "ich", "mich", "for\u00b7thin", "zu", "schrei\u00b7ben", "nicht", "er\u00b7k\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch kenn ich deine List: des klugen Mahlers Hand", "tokens": ["Doch", "kenn", "ich", "dei\u00b7ne", "List", ":", "des", "klu\u00b7gen", "Mah\u00b7lers", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Birgt ihre gr\u00f6ste Kunst in wohlgetheilten Schatten.", "tokens": ["Birgt", "ih\u00b7re", "gr\u00f6s\u00b7te", "Kunst", "in", "wohl\u00b7ge\u00b7theil\u00b7ten", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die dunckle Folg erhebt den hellen Diamant;", "tokens": ["Die", "dunck\u00b7le", "Folg", "er\u00b7hebt", "den", "hel\u00b7len", "Di\u00b7a\u00b7mant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan die Demutt dir auch gr\u00f6ssern Glantz erstatten.", "tokens": ["So", "kan", "die", "De\u00b7mutt", "dir", "auch", "gr\u00f6s\u00b7sern", "Glantz", "er\u00b7stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch will ich \u00fcber dem mit dir nicht weiter zancken/", "tokens": ["Doch", "will", "ich", "\u00fc\u00b7ber", "dem", "mit", "dir", "nicht", "wei\u00b7ter", "zan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "APPR", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die unpartheysche Welt sey Richter der Gedancken.", "tokens": ["Die", "un\u00b7par\u00b7they\u00b7sche", "Welt", "sey", "Rich\u00b7ter", "der", "Ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich nehme den Vergleich f\u00fcr ungewissen Streit", "tokens": ["Ich", "neh\u00b7me", "den", "Ver\u00b7gleich", "f\u00fcr", "un\u00b7ge\u00b7wis\u00b7sen", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gantz willig von dir an! Nur mu\u00df ich di\u00df noch setzen:", "tokens": ["Gantz", "wil\u00b7lig", "von", "dir", "an", "!", "Nur", "mu\u00df", "ich", "di\u00df", "noch", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$.", "ADV", "VMFIN", "PPER", "PDS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Reim von zarter Hand aus freyer Faust bereit/", "tokens": ["Ein", "Reim", "von", "zar\u00b7ter", "Hand", "aus", "frey\u00b7er", "Faust", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist f\u00fcr gelehrte Kunst der M\u00e4nner hoch zu sch\u00e4tzen.", "tokens": ["Ist", "f\u00fcr", "ge\u00b7lehr\u00b7te", "Kunst", "der", "M\u00e4n\u00b7ner", "hoch", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}