{"dta.poem.1685": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "So giebt Lorette nun dem Kr\u00e4ntzgen gutte Nacht/ ", "tokens": ["So", "giebt", "Lo\u00b7ret\u00b7te", "nun", "dem", "Kr\u00e4ntz\u00b7gen", "gut\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und will sich allgemach zum Frauen-Netze schicken?", "tokens": ["Und", "will", "sich", "all\u00b7ge\u00b7mach", "zum", "Frau\u00b7en\u00b7Net\u00b7ze", "schi\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "er G\u00e4rte reicher Schmuck/ der Blumen Krone Pracht", "tokens": ["er", "G\u00e4r\u00b7te", "rei\u00b7cher", "Schmuck", "/", "der", "Blu\u00b7men", "Kro\u00b7ne", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ADJD", "NN", "$(", "ART", "NN", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "oll izt ihr Silber-Haar nicht mehr wie vormahls schm\u00fccken?", "tokens": ["oll", "izt", "ihr", "Sil\u00b7ber\u00b7Haar", "nicht", "mehr", "wie", "vor\u00b7mahls", "schm\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "KOKOM", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "as schadets/ wenn nur noch die Scham-gef\u00e4rbten Wangen/", "tokens": ["as", "scha\u00b7dets", "/", "wenn", "nur", "noch", "die", "Scham\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "KOUS", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "er roth-gek\u00fc\u00dfte Mund mit Ros- und N\u00e4geln prangen!", "tokens": ["er", "ro\u00b7th\u00b7ge\u00b7k\u00fc\u00df\u00b7te", "Mund", "mit", "Ros", "und", "N\u00e4\u00b7geln", "pran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "TRUNC", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Den Lippen gleichet sich die Zucker-Rose nicht/", "tokens": ["Den", "Lip\u00b7pen", "glei\u00b7chet", "sich", "die", "Zu\u00b7cke\u00b7rRo\u00b7se", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liljen/ die bey ihr auff Hals und Br\u00fcsten schweben/", "tokens": ["Die", "Lil\u00b7jen", "/", "die", "bey", "ihr", "auff", "Hals", "und", "Br\u00fcs\u00b7ten", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "PPOSAT", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Tulipen/ die ihr bebl\u00fcmen das Gesicht/", "tokens": ["Die", "Tu\u00b7li\u00b7pen", "/", "die", "ihr", "be\u00b7bl\u00fc\u00b7men", "das", "Ge\u00b7sicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind Blumen die mehr Glantz und Anmutt von sich geben;", "tokens": ["Sind", "Blu\u00b7men", "die", "mehr", "Glantz", "und", "An\u00b7mutt", "von", "sich", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "PIAT", "NN", "KON", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als wenn ihr Rubars Zier und Spanjens Gelsaminen/", "tokens": ["Als", "wenn", "ihr", "Ru\u00b7bars", "Zier", "und", "Span\u00b7jens", "Gel\u00b7sa\u00b7mi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "NN", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja Florens gantzes Reich zum Krantze m\u00fcsten dienen.", "tokens": ["Ja", "Flo\u00b7rens", "gant\u00b7zes", "Reich", "zum", "Krant\u00b7ze", "m\u00fcs\u00b7ten", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "ADJA", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zwar Kr\u00e4ntze sind beliebt/ auch in der G\u00f6tter Hau\u00df/", "tokens": ["Zwar", "Kr\u00e4nt\u00b7ze", "sind", "be\u00b7liebt", "/", "auch", "in", "der", "G\u00f6t\u00b7ter", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADJD", "$(", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein rund-geflochtner Zweig mu\u00df Nimph- und Helden zieren/", "tokens": ["Ein", "run\u00b7dge\u00b7flocht\u00b7ner", "Zweig", "mu\u00df", "Nim\u00b7ph", "und", "Hel\u00b7den", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "TRUNC", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Himmel schm\u00fcckt sich selbst mit Sternen-Kr\u00e4ntzen aus/", "tokens": ["Der", "Him\u00b7mel", "schm\u00fcckt", "sich", "selbst", "mit", "Ster\u00b7nen\u00b7Kr\u00e4nt\u00b7zen", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "L\u00e4st Kr\u00e4ntz um Sonn und Mond und andre Lichter sp\u00fcren;", "tokens": ["L\u00e4st", "Kr\u00e4ntz", "um", "Sonn", "und", "Mond", "und", "and\u00b7re", "Lich\u00b7ter", "sp\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wenn die Sonne brennt/ w\u00fcnscht man ihn mit Verlange", "tokens": ["Doch", "wenn", "die", "Son\u00b7ne", "brennt", "/", "w\u00fcnscht", "man", "ihn", "mit", "Ver\u00b7lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$(", "VVFIN", "PIS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vom Schleyer brauner Nacht/ der Wolcken Haub/ umhange", "tokens": ["Vom", "Schle\u00b7yer", "brau\u00b7ner", "Nacht", "/", "der", "Wol\u00b7cken", "Haub", "/", "um\u00b7han\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "$(", "ART", "NN", "NN", "$(", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es h\u00fcllt sich alle Welt in Schaub- und Hauben ein;", "tokens": ["Es", "h\u00fcllt", "sich", "al\u00b7le", "Welt", "in", "Schaub", "und", "Hau\u00b7ben", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN", "APPR", "TRUNC", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll sich Lorett allein der Mode nicht bequemen.", "tokens": ["Soll", "sich", "Lo\u00b7rett", "al\u00b7lein", "der", "Mo\u00b7de", "nicht", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "NN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Der Himmel will heut selbst mit ihr gehaubet seyn/", "tokens": ["Der", "Him\u00b7mel", "will", "heut", "selbst", "mit", "ihr", "ge\u00b7hau\u00b7bet", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie sich nicht allein des H\u00e4ubgens d\u00f6rffe sch\u00e4men.", "tokens": ["Da\u00df", "sie", "sich", "nicht", "al\u00b7lein", "des", "H\u00e4ub\u00b7gens", "d\u00f6rf\u00b7fe", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir sehn auch izt das Feld mit Faden \u00fcbersponnen/", "tokens": ["Wir", "sehn", "auch", "izt", "das", "Feld", "mit", "Fa\u00b7den", "\u00fc\u00b7bers\u00b7pon\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was Wunder/ wenn Lorett ein gleiches hat begonnen.", "tokens": ["Was", "Wun\u00b7der", "/", "wenn", "Lo\u00b7rett", "ein", "glei\u00b7ches", "hat", "be\u00b7gon\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "KOUS", "NN", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den Erden-Krantz verderbt der ungest\u00fcme Nord/", "tokens": ["Den", "Er\u00b7den\u00b7Krantz", "ver\u00b7derbt", "der", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Nord", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Herbstes rauhe Lufft macht Blum und Laub entfliehen:", "tokens": ["Des", "Herbs\u00b7tes", "rau\u00b7he", "Lufft", "macht", "Blum", "und", "Laub", "ent\u00b7flie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier bl\u00e4st ein warmer Sud die welcken Bl\u00e4tter fort/", "tokens": ["Hier", "bl\u00e4st", "ein", "war\u00b7mer", "Sud", "die", "wel\u00b7cken", "Bl\u00e4t\u00b7ter", "fort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und macht den bunten Krantz der Liebe Glutt verbl\u00fchen:", "tokens": ["Und", "macht", "den", "bun\u00b7ten", "Krantz", "der", "Lie\u00b7be", "Glutt", "ver\u00b7bl\u00fc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann jene fr\u00fcher Frost und scharffer Reiff entf\u00e4rben/", "tokens": ["Wann", "je\u00b7ne", "fr\u00fc\u00b7her", "Frost", "und", "scharf\u00b7fer", "Reiff", "ent\u00b7f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan wohl dieser auch von heissem Thau ersterben.", "tokens": ["So", "kan", "wohl", "die\u00b7ser", "auch", "von", "heis\u00b7sem", "Thau", "ers\u00b7ter\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PDAT", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Man geht bey Tag und Nacht mit Garn und Netzen aus/", "tokens": ["Man", "geht", "bey", "Tag", "und", "Nacht", "mit", "Garn", "und", "Net\u00b7zen", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Manch leichtes Feder-Kind mit Lust und List zu fangen;", "tokens": ["Manch", "leich\u00b7tes", "Fe\u00b7der\u00b7Kind", "mit", "Lust", "und", "List", "zu", "fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ist die Haube denn? der Freyheit Vogel-Hau\u00df/", "tokens": ["Was", "ist", "die", "Hau\u00b7be", "denn", "?", "der", "Frey\u00b7heit", "Vo\u00b7gel\u00b7Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "KON", "$.", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Netze/ nach der Jagt zur Siegs-Pracht ausgehangen.", "tokens": ["Ein", "Net\u00b7ze", "/", "nach", "der", "Jagt", "zur", "Siegs\u00b7Pracht", "aus\u00b7ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "ART", "VVFIN", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mu\u00df hier die J\u00e4gerin gleich selbst im Netze liegen/", "tokens": ["Mu\u00df", "hier", "die", "J\u00e4\u00b7ge\u00b7rin", "gleich", "selbst", "im", "Net\u00b7ze", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So zeuget solches doch von ihrem Fang- und Siegen.", "tokens": ["So", "zeu\u00b7get", "sol\u00b7ches", "doch", "von", "ih\u00b7rem", "Fang", "und", "Sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nun Br\u00e4utel/ gib mit Lust dem Kr\u00e4ntzgen gutte Nacht/", "tokens": ["Nun", "Br\u00e4u\u00b7tel", "/", "gib", "mit", "Lust", "dem", "Kr\u00e4ntz\u00b7gen", "gut\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$(", "VVIMP", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df Venus Sieges-Fahn um deine Scheitel fliegen/", "tokens": ["La\u00df", "Ve\u00b7nus", "Sie\u00b7ges\u00b7Fahn", "um", "dei\u00b7ne", "Schei\u00b7tel", "flie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dencke/ weist der Krantz gleich mehr von Gelder-Pracht/", "tokens": ["Und", "den\u00b7cke", "/", "weist", "der", "Krantz", "gleich", "mehr", "von", "Gel\u00b7der\u00b7Pracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wei\u00df die Haube mehr von w\u00fcrcklichem Vergn\u00fcgen.", "tokens": ["So", "wei\u00df", "die", "Hau\u00b7be", "mehr", "von", "w\u00fcrck\u00b7li\u00b7chem", "Ver\u00b7gn\u00fc\u