{"dta.poem.13794": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Entschlu\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Gebannt im stillen Kreise sanfter H\u00fcgel,", "tokens": ["Ge\u00b7bannt", "im", "stil\u00b7len", "Krei\u00b7se", "sanf\u00b7ter", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlingt sich ein Strom von ewig gleichen Tagen,", "tokens": ["Schlingt", "sich", "ein", "Strom", "von", "e\u00b7wig", "glei\u00b7chen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da mag die Brust nicht nach der Ferne fragen", "tokens": ["Da", "mag", "die", "Brust", "nicht", "nach", "der", "Fer\u00b7ne", "fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00e4chelnd senkt die Sehnsucht ihre Fl\u00fcgel.", "tokens": ["Und", "l\u00e4\u00b7chelnd", "senkt", "die", "Sehn\u00b7sucht", "ih\u00b7re", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}