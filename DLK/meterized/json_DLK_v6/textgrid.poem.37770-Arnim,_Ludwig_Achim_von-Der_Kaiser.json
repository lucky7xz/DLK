{"textgrid.poem.37770": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kaiser", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kaiser flieht vertrieben,", "tokens": ["Der", "Kai\u00b7ser", "flieht", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Flieht das eigne Land;", "tokens": ["Flieht", "das", "eig\u00b7ne", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das Heer ist aufgerieben", "tokens": ["Das", "Heer", "ist", "auf\u00b7ge\u00b7rie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fliehend seine Schand.", "tokens": ["Flie\u00b7hend", "sei\u00b7ne", "Schand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Die er oft verkannt,", "tokens": ["Die", "er", "oft", "ver\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Denn streng sind, die uns lieben,", "tokens": ["Denn", "streng", "sind", ",", "die", "uns", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noth hat Lieb erkannt.", "tokens": ["Noth", "hat", "Lieb", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Er gr\u00fc\u00dft die alten Tage", "tokens": ["Er", "gr\u00fc\u00dft", "die", "al\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Seiner Jugendzeit,", "tokens": ["Sei\u00b7ner", "Ju\u00b7gend\u00b7zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Vergi\u00dft der Zeiten Plage", "tokens": ["Ver\u00b7gi\u00dft", "der", "Zei\u00b7ten", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "In Vertraulichkeit.", "tokens": ["In", "Ver\u00b7trau\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Zum Flu\u00df ist er gekommen,", "tokens": ["Zum", "Flu\u00df", "ist", "er", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Findet keine Br\u00fcck,", "tokens": ["Fin\u00b7det", "kei\u00b7ne", "Br\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da wird sein Herz beklommen,", "tokens": ["Da", "wird", "sein", "Herz", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er kann ", "tokens": ["Er", "kann"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Da kommt ein Schiff mit Netzen:", "tokens": ["Da", "kommt", "ein", "Schiff", "mit", "Net\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbschiffer nimm zum Lohn,", "tokens": ["\u00bb", "schif\u00b7fer", "nimm", "zum", "Lohn", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Willst du uns \u00fcbersetzen,", "tokens": ["Willst", "du", "uns", "\u00fc\u00b7bers\u00b7et\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Meine goldne Kron.\u00ab", "tokens": ["Mei\u00b7ne", "gold\u00b7ne", "Kron", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Der Schiffer hat genommen", "tokens": ["Der", "Schif\u00b7fer", "hat", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Seine goldne Kron", "tokens": ["Sei\u00b7ne", "gold\u00b7ne", "Kron"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Doch eh er \u00fcber kommen,", "tokens": ["Doch", "eh", "er", "\u00fc\u00b7ber", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "War der Feind dort schon.", "tokens": ["War", "der", "Feind", "dort", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbso lieb dir ist dein Leben,", "tokens": ["\u00bb", "so", "lieb", "dir", "ist", "dein", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fahr zur\u00fcck ans Land,", "tokens": ["Fahr", "zu\u00b7r\u00fcck", "ans", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Den Schifflohn will ich geben", "tokens": ["Den", "Schif\u00b7flohn", "will", "ich", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit der eignen Hand.\u00ab", "tokens": ["Mit", "der", "eig\u00b7nen", "Hand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Der Kaiser droht zu schlagen", "tokens": ["Der", "Kai\u00b7ser", "droht", "zu", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit dem goldnen Stab,", "tokens": ["Mit", "dem", "gold\u00b7nen", "Stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Doch schnell zur\u00fcckgetragen,", "tokens": ["Doch", "schnell", "zu\u00b7r\u00fcck\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihn dem Schiffer gab.", "tokens": ["Ihn", "dem", "Schif\u00b7fer", "gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Jetzt sah er wie die Feinde", "tokens": ["Jetzt", "sah", "er", "wie", "die", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ihn am Ufer sehn,", "tokens": ["Ihn", "am", "U\u00b7fer", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "An Freundes Busen weinte,", "tokens": ["An", "Freun\u00b7des", "Bu\u00b7sen", "wein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wollte schier vergehn.", "tokens": ["Woll\u00b7te", "schier", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbich hab nichts mehr zu geben,", "tokens": ["\u00bb", "ich", "hab", "nichts", "mehr", "zu", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIS", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als den Mantel mein,", "tokens": ["Als", "den", "Man\u00b7tel", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der giebt mir Noth im Leben,", "tokens": ["Der", "giebt", "mir", "Noth", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald auch Todespein:", "tokens": ["Bald", "auch", "To\u00b7de\u00b7spein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "War meiner Noth Begl\u00fccken", "tokens": ["War", "mei\u00b7ner", "Noth", "Be\u00b7gl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Eurer Tage Preis,", "tokens": ["Eu\u00b7rer", "Ta\u00b7ge", "Preis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Den Purpur rei\u00dft in St\u00fccken,", "tokens": ["Den", "Pur\u00b7pur", "rei\u00dft", "in", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Geb ihn allen preis!\u00ab", "tokens": ["Geb", "ihn", "al\u00b7len", "preis", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKVZ", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Er fa\u00dft soviel er konnte,", "tokens": ["Er", "fa\u00dft", "so\u00b7viel", "er", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Jeder ri\u00df sein St\u00fcck,", "tokens": ["Je\u00b7der", "ri\u00df", "sein", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Es auf dem Herzen sonnte,", "tokens": ["Es", "auf", "dem", "Her\u00b7zen", "sonn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wie ein Stern im Gl\u00fcck.", "tokens": ["Wie", "ein", "Stern", "im", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Die St\u00fccke heften Alle", "tokens": ["Die", "St\u00fc\u00b7cke", "hef\u00b7ten", "Al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf die Kleider fest,", "tokens": ["Auf", "die", "Klei\u00b7der", "fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und vor dem Feind mit Schalle", "tokens": ["Und", "vor", "dem", "Feind", "mit", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Halten Ordensfest.", "tokens": ["Hal\u00b7ten", "Or\u00b7dens\u00b7fest", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dann stellen sie sich Alle", "tokens": ["Dann", "stel\u00b7len", "sie", "sich", "Al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Rings zum Kaiser treu,", "tokens": ["Rings", "zum", "Kai\u00b7ser", "treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df er von einem Walle", "tokens": ["Da\u00df", "er", "von", "ei\u00b7nem", "Wal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Rings gesch\u00fctzet sey.", "tokens": ["Rings", "ge\u00b7sch\u00fct\u00b7zet", "sey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Der Purpurstern kann blitzen,", "tokens": ["Der", "Pur\u00b7purs\u00b7tern", "kann", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "W\u00e4rmt auch wohl das Herz,", "tokens": ["W\u00e4rmt", "auch", "wohl", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Kann ", "tokens": ["Kann"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Vor der Pfeile Erz.", "tokens": ["Vor", "der", "Pfei\u00b7le", "Erz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbjetzt flieht!\u00ab befiehlt der Kaiser,", "tokens": ["\u00bb", "jetzt", "flieht", "!", "\u00ab", "be\u00b7fiehlt", "der", "Kai\u00b7ser", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$.", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbflieht, ich sterb allein!\u00ab", "tokens": ["\u00bb", "flieht", ",", "ich", "sterb", "al\u00b7lein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sie rufen all zum Kaiser:", "tokens": ["Sie", "ru\u00b7fen", "all", "zum", "Kai\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbdas soll nimmer seyn,", "tokens": ["\u00bb", "das", "soll", "nim\u00b7mer", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "ADV", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Der Purpur ist zerrissen,", "tokens": ["Der", "Pur\u00b7pur", "ist", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus ist nun ", "tokens": ["Aus", "ist", "nun"], "token_info": ["word", "word", "word"], "pos": ["APPR", "VAFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Bald mit ", "tokens": ["Bald", "mit"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wir wollen hier vergehen,", "tokens": ["Wir", "wol\u00b7len", "hier", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Froh des ewgen Muths;", "tokens": ["Froh", "des", "ew\u00b7gen", "Muths", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "R\u00e4cher ", "tokens": ["R\u00e4\u00b7cher"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Die Feinde sehn sie blicken,", "tokens": ["Die", "Fein\u00b7de", "sehn", "sie", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sehn die Sterne hell,", "tokens": ["Sehn", "die", "Ster\u00b7ne", "hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und ihre Pfeile dr\u00fccken", "tokens": ["Und", "ih\u00b7re", "Pfei\u00b7le", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In die Herzen schnell.", "tokens": ["In", "die", "Her\u00b7zen", "schnell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nach aller Edlen Falle,", "tokens": ["Nach", "al\u00b7ler", "Ed\u00b7len", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00e4llt der Kaiser auch,", "tokens": ["F\u00e4llt", "der", "Kai\u00b7ser", "auch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sein Segen \u00fcber alle", "tokens": ["Sein", "Se\u00b7gen", "\u00fc\u00b7ber", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist sein letzter Hauch.", "tokens": ["Ist", "sein", "letz\u00b7ter", "Hauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Die blutgen Purpurst\u00fccke", "tokens": ["Die", "blut\u00b7gen", "Pur\u00b7pur\u00b7st\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Halten frisch die Farb,", "tokens": ["Hal\u00b7ten", "frisch", "die", "Farb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Der Feind ist gro\u00df im Gl\u00fccke,", "tokens": ["Der", "Feind", "ist", "gro\u00df", "im", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Nicht den Schmuck verdarb.", "tokens": ["Nicht", "den", "Schmuck", "ver\u00b7darb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}