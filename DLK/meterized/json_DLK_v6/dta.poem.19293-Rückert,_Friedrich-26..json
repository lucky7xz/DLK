{"dta.poem.19293": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Meister, als er war gestorben, ist erschienen", "tokens": ["Der", "Meis\u00b7ter", ",", "als", "er", "war", "ge\u00b7stor\u00b7ben", ",", "ist", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$,", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem J\u00fcnger in der Nacht mit sonnenhellen Mienen.", "tokens": ["Dem", "J\u00fcn\u00b7ger", "in", "der", "Nacht", "mit", "son\u00b7nen\u00b7hel\u00b7len", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Meister, wie stralest du! von wannen ist dein Licht?", "tokens": ["Meis\u00b7ter", ",", "wie", "stra\u00b7lest", "du", "!", "von", "wan\u00b7nen", "ist", "dein", "Licht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "$.", "APPR", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Er sprach: von wannen als von Gottes Angesicht! \u2014", "tokens": ["Er", "sprach", ":", "von", "wan\u00b7nen", "als", "von", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPR", "ADV", "KOKOM", "APPR", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und hast du und wodurch den Zutritt dort erlangt?", "tokens": ["Und", "hast", "du", "und", "wo\u00b7durch", "den", "Zu\u00b7tritt", "dort", "er\u00b7langt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "KON", "PWAV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sprach: dadurch weil ich nach andrem nicht verlangt.", "tokens": ["Er", "sprach", ":", "da\u00b7durch", "weil", "ich", "nach", "an\u00b7drem", "nicht", "ver\u00b7langt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PAV", "KOUS", "PPER", "APPR", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich ward von Glanz zu Glanz die Himmel durch gef\u00fchrt,", "tokens": ["Ich", "ward", "von", "Glanz", "zu", "Glanz", "die", "Him\u00b7mel", "durch", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "NN", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor\u00fcber aber gieng ich allem unger\u00fchrt.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "a\u00b7ber", "gieng", "ich", "al\u00b7lem", "un\u00b7ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich ward gefragt: Was hat vor allem dir gefallen?", "tokens": ["Ich", "ward", "ge\u00b7fragt", ":", "Was", "hat", "vor", "al\u00b7lem", "dir", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PWS", "VAFIN", "APPR", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich aber sagte: Nichts gef\u00e4llt mir von dem allen.", "tokens": ["Ich", "a\u00b7ber", "sag\u00b7te", ":", "Nichts", "ge\u00b7f\u00e4llt", "mir", "von", "dem", "al\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$.", "PIS", "VVFIN", "PPER", "APPR", "ART", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da rief der Herr: So f\u00fchrt ihn nur zu mir herein!", "tokens": ["Da", "rief", "der", "Herr", ":", "So", "f\u00fchrt", "ihn", "nur", "zu", "mir", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sei bei mir, weil er will nirgend anders seyn.", "tokens": ["Er", "sei", "bei", "mir", ",", "weil", "er", "will", "nir\u00b7gend", "an\u00b7ders", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "VMFIN", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und h\u00e4tte drau\u00dfen dir gen\u00fcgt ein ander Licht,", "tokens": ["Und", "h\u00e4t\u00b7te", "drau\u00b7\u00dfen", "dir", "ge\u00b7n\u00fcgt", "ein", "an\u00b7der", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00e4tt' ich dir's verliehn, und zu mir kamst du nicht.", "tokens": ["So", "h\u00e4tt'", "ich", "dir's", "ver\u00b7liehn", ",", "und", "zu", "mir", "kamst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF", "$,", "KON", "APPR", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}