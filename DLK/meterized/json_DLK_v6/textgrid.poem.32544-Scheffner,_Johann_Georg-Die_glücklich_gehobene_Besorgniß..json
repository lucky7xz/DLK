{"textgrid.poem.32544": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die gl\u00fccklich gehobene Besorgni\u00df.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal, doch wo, das wei\u00df ich nicht gewi\u00df,", "tokens": ["Es", "war", "ein\u00b7mal", ",", "doch", "wo", ",", "das", "wei\u00df", "ich", "nicht", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KON", "PWAV", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die S\u00fcntfluth hat ohndem den Ort l\u00e4ngst weggesp\u00fchlet,", "tokens": ["Die", "S\u00fcnt\u00b7fluth", "hat", "ohn\u00b7dem", "den", "Ort", "l\u00e4ngst", "weg\u00b7ge\u00b7sp\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein M\u00e4dchen, das mit Recht das Wunderm\u00e4dchen hie\u00df,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "das", "mit", "Recht", "das", "Wun\u00b7der\u00b7m\u00e4d\u00b7chen", "hie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil noch ihr zwanzigj\u00e4hrger Mund", "tokens": ["Weil", "noch", "ihr", "zwan\u00b7zig\u00b7j\u00e4hr\u00b7ger", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die S\u00fc\u00dfigkeit des Honigs nicht gef\u00fchlet,", "tokens": ["Die", "S\u00fc\u00b7\u00dfig\u00b7keit", "des", "Ho\u00b7nigs", "nicht", "ge\u00b7f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Adam einst in Ewens K\u00f6rbchen trug.", "tokens": ["Den", "A\u00b7dam", "einst", "in", "E\u00b7wens", "K\u00f6rb\u00b7chen", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie war so reich, sch\u00f6n und hatte Freyer gnug;", "tokens": ["Sie", "war", "so", "reich", ",", "sch\u00f6n", "und", "hat\u00b7te", "Frey\u00b7er", "gnug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "KON", "VAFIN", "NN", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Allein, da sie beym Antrag jeden frug,", "tokens": ["Al\u00b7lein", ",", "da", "sie", "beym", "An\u00b7trag", "je\u00b7den", "frug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie gro\u00df der Finger sey?", "tokens": ["Wie", "gro\u00df", "der", "Fin\u00b7ger", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "So wollte, weil die Herrn aus Freyersprahlerey", "tokens": ["So", "woll\u00b7te", ",", "weil", "die", "Herrn", "aus", "Frey\u00b7er\u00b7sprah\u00b7le\u00b7rey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "$,", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des Dinges Maa\u00df und Ziel verga\u00dfen,", "tokens": ["Des", "Din\u00b7ges", "Maa\u00df", "und", "Ziel", "ver\u00b7ga\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Kein einziger in ihren Fingerhut,", "tokens": ["Kein", "ein\u00b7zi\u00b7ger", "in", "ih\u00b7ren", "Fin\u00b7ger\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Den sie durchaus nicht wollte weiten lassen,", "tokens": ["Den", "sie", "durc\u00b7haus", "nicht", "woll\u00b7te", "wei\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.14": {"text": "So recht bequem nach ihrem Sinne pa\u00dfen.", "tokens": ["So", "recht", "be\u00b7quem", "nach", "ih\u00b7rem", "Sin\u00b7ne", "pa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Doch Amor der nicht eher ruht", "tokens": ["Doch", "A\u00b7mor", "der", "nicht", "e\u00b7her", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "ART", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Bis M\u00e4dchen ihm ", "tokens": ["Bis", "M\u00e4d\u00b7chen", "ihm"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Bracht' den Amint auf eine List. Er meldte sich", "tokens": ["Bracht'", "den", "A\u00b7mint", "auf", "ei\u00b7ne", "List", ".", "Er", "meld\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PRF"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.18": {"text": "Und sprach: \u00bbO Sch\u00f6ne w\u00e4hle mich", "tokens": ["Und", "sprach", ":", "\u00bb", "O", "Sch\u00f6\u00b7ne", "w\u00e4h\u00b7le", "mich"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "NE", "NN", "VVIMP", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ich habe dreyfach das, was andre einfach haben,", "tokens": ["Ich", "ha\u00b7be", "drey\u00b7fach", "das", ",", "was", "and\u00b7re", "ein\u00b7fach", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PDS", "$,", "PRELS", "PIS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und glaub gewi\u00df die kleinste dieser Gaben", "tokens": ["Und", "glaub", "ge\u00b7wi\u00df", "die", "kleins\u00b7te", "die\u00b7ser", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wird deinem Fingerhut recht angemessen seyn.\u00ab", "tokens": ["Wird", "dei\u00b7nem", "Fin\u00b7ger\u00b7hut", "recht", "an\u00b7ge\u00b7mes\u00b7sen", "seyn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Zugleich reicht er den Ri\u00df der dreyen Finger ein.", "tokens": ["Zu\u00b7gleich", "reicht", "er", "den", "Ri\u00df", "der", "drey\u00b7en", "Fin\u00b7ger", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "CARD", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Sie nimmt den Ri\u00df in hohen Augenschein,", "tokens": ["Sie", "nimmt", "den", "Ri\u00df", "in", "ho\u00b7hen", "Au\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und w\u00e4hlt, weil ihr vielleicht das ", "tokens": ["Und", "w\u00e4hlt", ",", "weil", "ihr", "viel\u00b7leicht", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Aminten, der zuvor ihr eidlich zugesaget", "tokens": ["A\u00b7min\u00b7ten", ",", "der", "zu\u00b7vor", "ihr", "eid\u00b7lich", "zu\u00b7ge\u00b7sa\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nur ganz allein den kleinsten zu gebrauchen.", "tokens": ["Nur", "ganz", "al\u00b7lein", "den", "kleins\u00b7ten", "zu", "ge\u00b7brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Er nahm ihn auch, lie\u00df sanft ihn untertauchen,", "tokens": ["Er", "nahm", "ihn", "auch", ",", "lie\u00df", "sanft", "ihn", "un\u00b7ter\u00b7tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Man fand ihn gut \u2013 der Fingerhut ward feucht,", "tokens": ["Man", "fand", "ihn", "gut", "\u2013", "der", "Fin\u00b7ger\u00b7hut", "ward", "feucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$(", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und Phyllis zischelte: \u00bb", "tokens": ["Und", "Phyl\u00b7lis", "zi\u00b7schel\u00b7te", ":", "\u00bb"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.30": {"text": "Und k\u00fctzelt frisch den Liebeszwinger;", "tokens": ["Und", "k\u00fct\u00b7zelt", "frisch", "den", "Lie\u00b7bes\u00b7zwin\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Da wurde aus Erkenntlichkeit", "tokens": ["Da", "wur\u00b7de", "aus", "Er\u00b7kennt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Der rosenfarbne Rand des Ringchens ziemlich weit.", "tokens": ["Der", "ro\u00b7sen\u00b7farb\u00b7ne", "Rand", "des", "Ring\u00b7chens", "ziem\u00b7lich", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Kaum sprach sie es; so stach er schon im Ziel", "tokens": ["Kaum", "sprach", "sie", "es", ";", "so", "stach", "er", "schon", "im", "Ziel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Vermehrte da der L\u00fcsternheit Gef\u00fchl", "tokens": ["Ver\u00b7mehr\u00b7te", "da", "der", "L\u00fcs\u00b7tern\u00b7heit", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Drang weit empfindlicher zum Herzen,", "tokens": ["Drang", "weit", "emp\u00b7find\u00b7li\u00b7cher", "zum", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "APPRART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Und Wollust half die kleine Pein verschmerzen,", "tokens": ["Und", "Wol\u00b7lust", "half", "die", "klei\u00b7ne", "Pein", "ver\u00b7schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Indem sie Balsam, der wie Milch und Honig flo\u00df", "tokens": ["In\u00b7dem", "sie", "Bal\u00b7sam", ",", "der", "wie", "Milch", "und", "Ho\u00b7nig", "flo\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "$,", "PRELS", "KOKOM", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "In Phyllis Rosenwunde go\u00df.", "tokens": ["In", "Phyl\u00b7lis", "Ro\u00b7sen\u00b7wun\u00b7de", "go\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Das weichliche sittsame Kind zerflo\u00df,", "tokens": ["Das", "weich\u00b7li\u00b7che", "sitt\u00b7sa\u00b7me", "Kind", "zer\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Und starb vor Lust, doch bald, erweckt von neuen Flammen,", "tokens": ["Und", "starb", "vor", "Lust", ",", "doch", "bald", ",", "er\u00b7weckt", "von", "neu\u00b7en", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "ADV", "ADV", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Schien jetzt der Fingerhut ein niedlicher Pokal,", "tokens": ["Schien", "jetzt", "der", "Fin\u00b7ger\u00b7hut", "ein", "nied\u00b7li\u00b7cher", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und leise sprach sie: ", "tokens": ["Und", "lei\u00b7se", "sprach", "sie", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}