{"textgrid.poem.34118": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Goethes Biographie", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anfangs ist es ein Punct, der leise zum Kreise sich \u00f6ffnet", "tokens": ["An\u00b7fangs", "ist", "es", "ein", "Punct", ",", "der", "lei\u00b7se", "zum", "Krei\u00b7se", "sich", "\u00f6ff\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "PRF", "VVFIN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber, wachsend, umfa\u00dft dieser am Ende die Welt.", "tokens": ["A\u00b7ber", ",", "wach\u00b7send", ",", "um\u00b7fa\u00dft", "die\u00b7ser", "am", "En\u00b7de", "die", "Welt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "$,", "VVFIN", "PDAT", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}}}}}