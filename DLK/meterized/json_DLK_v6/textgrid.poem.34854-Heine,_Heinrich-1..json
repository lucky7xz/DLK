{"textgrid.poem.34854": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Vater, wie ein jeder wei\u00df,", "tokens": ["Dein", "Va\u00b7ter", ",", "wie", "ein", "je\u00b7der", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Esel leider war der Gute;", "tokens": ["Ein", "E\u00b7sel", "lei\u00b7der", "war", "der", "Gu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch deine Mutter, hochgesinnt,", "tokens": ["Doch", "dei\u00b7ne", "Mut\u00b7ter", ",", "hoch\u00b7ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War eine edle Vollblutstute.", "tokens": ["War", "ei\u00b7ne", "ed\u00b7le", "Voll\u00b7blut\u00b7stu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Tatsache ist dein Maultiertum,", "tokens": ["Tat\u00b7sa\u00b7che", "ist", "dein", "Maul\u00b7tier\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sehr du dessen dich erwehrest;", "tokens": ["Wie", "sehr", "du", "des\u00b7sen", "dich", "er\u00b7weh\u00b7rest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PDS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sagen darfst du guten Fugs,", "tokens": ["Doch", "sa\u00b7gen", "darfst", "du", "gu\u00b7ten", "Fugs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du den Pferden angeh\u00f6rest \u2013", "tokens": ["Da\u00df", "du", "den", "Pfer\u00b7den", "an\u00b7ge\u00b7h\u00f6\u00b7rest", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df du abstammst vom Bucephal,", "tokens": ["Da\u00df", "du", "ab\u00b7stammst", "vom", "Bu\u00b7ce\u00b7phal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem stolzen Gaul, da\u00df deine Ahnen", "tokens": ["Dem", "stol\u00b7zen", "Gaul", ",", "da\u00df", "dei\u00b7ne", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Geharnischt nach dem Heil'gen Grab", "tokens": ["Ge\u00b7har\u00b7nischt", "nach", "dem", "Heil'\u00b7gen", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gefolgt den frommen Kreuzzugfahnen \u2013", "tokens": ["Ge\u00b7folgt", "den", "from\u00b7men", "Kreuz\u00b7zug\u00b7fah\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df du zu deiner Sippschaft z\u00e4hlst", "tokens": ["Da\u00df", "du", "zu", "dei\u00b7ner", "Sipp\u00b7schaft", "z\u00e4hlst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den hohen Schimmel, den geritten", "tokens": ["Den", "ho\u00b7hen", "Schim\u00b7mel", ",", "den", "ge\u00b7rit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Herr Gottfried von Bouillon, am Tag,", "tokens": ["Herr", "Gott\u00b7fried", "von", "Bou\u00b7il\u00b7lon", ",", "am", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NE", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo er die Gottesstatt erstritten; \u2013", "tokens": ["Wo", "er", "die", "Got\u00b7tes\u00b7statt", "er\u00b7strit\u00b7ten", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kannst sagen auch, da\u00df Ro\u00df-Bayard", "tokens": ["Kannst", "sa\u00b7gen", "auch", ",", "da\u00df", "Ro\u00df\u00b7Ba\u00b7yard"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$,", "KOUS", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Vetter war, da\u00df deine Tante", "tokens": ["Dein", "Vet\u00b7ter", "war", ",", "da\u00df", "dei\u00b7ne", "Tan\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Ritter Don Quixote trug,", "tokens": ["Den", "Rit\u00b7ter", "Don", "Qui\u00b7xo\u00b7te", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die heldenm\u00fct'ge Rosinante.", "tokens": ["Die", "hel\u00b7den\u00b7m\u00fct'\u00b7ge", "Ro\u00b7si\u00b7nan\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Freilich, da\u00df Sanchos Grauchen auch", "tokens": ["Frei\u00b7lich", ",", "da\u00df", "San\u00b7chos", "Grau\u00b7chen", "auch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "NE", "NN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit dir verwandt, mu\u00dft du nicht sagen;", "tokens": ["Mit", "dir", "ver\u00b7wandt", ",", "mu\u00dft", "du", "nicht", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verleugne gar das Eselein,", "tokens": ["Ver\u00b7leug\u00b7ne", "gar", "das", "E\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das unsern Heiland einst getragen.", "tokens": ["Das", "un\u00b7sern", "Hei\u00b7land", "einst", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auch ist nicht n\u00f6tig, da\u00df du just", "tokens": ["Auch", "ist", "nicht", "n\u00f6\u00b7tig", ",", "da\u00df", "du", "just"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Langohr in dein Wappen setzest.", "tokens": ["Ein", "Lan\u00b7gohr", "in", "dein", "Wap\u00b7pen", "set\u00b7zest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sei deines eignen Werts Wardein \u2013", "tokens": ["Sei", "dei\u00b7nes", "eig\u00b7nen", "Werts", "War\u00b7dein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du giltst so hoch, wie du dich sch\u00e4tzest.", "tokens": ["Du", "giltst", "so", "hoch", ",", "wie", "du", "dich", "sch\u00e4t\u00b7zest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}