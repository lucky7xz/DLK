{"textgrid.poem.37062": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Bund", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Ku\u00dfgelispel \u00e4hnlich, wenn Freunde sich", "tokens": ["Dem", "Ku\u00df\u00b7ge\u00b7lis\u00b7pel", "\u00e4hn\u00b7lich", ",", "wenn", "Freun\u00b7de", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "KOUS", "NN", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Umarmen, rausche, Harfe! Du Lindenbaum,", "tokens": ["Um\u00b7ar\u00b7men", ",", "rau\u00b7sche", ",", "Har\u00b7fe", "!", "Du", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$.", "PPER", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Geu\u00df dein Gefl\u00fcster in die Saiten", "tokens": ["Geu\u00df", "dein", "Ge\u00b7fl\u00fcs\u00b7ter", "in", "die", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hainings! Er gl\u00fchet im Wonnetaumel.", "tokens": ["Hai\u00b7nings", "!", "Er", "gl\u00fc\u00b7het", "im", "Won\u00b7ne\u00b7tau\u00b7mel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Mein Gottschalk nennt mich Bruder! Der Name t\u00f6nt", "tokens": ["Mein", "Gott\u00b7schalk", "nennt", "mich", "Bru\u00b7der", "!", "Der", "Na\u00b7me", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mir s\u00fc\u00dfre Lispel, als mir das L\u00fcftchen t\u00f6nt,", "tokens": ["Mir", "s\u00fc\u00df\u00b7re", "Lis\u00b7pel", ",", "als", "mir", "das", "L\u00fcft\u00b7chen", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das durch die jungen Mayen s\u00e4uselt,", "tokens": ["Das", "durch", "die", "jun\u00b7gen", "Ma\u00b7yen", "s\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo sich mein M\u00e4dchen des Lenzen freuet.", "tokens": ["Wo", "sich", "mein", "M\u00e4d\u00b7chen", "des", "Len\u00b7zen", "freu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Weg, Liederharfe! \u2013 \u2013 Teuthard und Minnehold,", "tokens": ["Weg", ",", "Lie\u00b7der\u00b7har\u00b7fe", "!", "\u2013", "\u2013", "Teu\u00b7thard", "und", "Min\u00b7ne\u00b7hold", ","], "token_info": ["word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "$(", "$(", "NE", "KON", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Herz im Auge, taumeln mir an die Brust,", "tokens": ["Das", "Herz", "im", "Au\u00b7ge", ",", "tau\u00b7meln", "mir", "an", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und unsre deutschen Liederseelen", "tokens": ["Und", "uns\u00b7re", "deut\u00b7schen", "Lie\u00b7der\u00b7see\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Str\u00f6men in Gluthen. Was weilst du, Raimund,", "tokens": ["Str\u00f6\u00b7men", "in", "Glu\u00b7then", ".", "Was", "weilst", "du", ",", "Rai\u00b7mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "PWS", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Im Mondglanzschatten! \u2013 Rei\u00df dich, o Bardenhold,", "tokens": ["Im", "Mond\u00b7glanz\u00b7schat\u00b7ten", "!", "\u2013", "Rei\u00df", "dich", ",", "o", "Bar\u00b7den\u00b7hold", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "$(", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus Raimunds Armen! Fl\u00fcgle dich, schnelles Flugs,", "tokens": ["Aus", "Rai\u00b7munds", "Ar\u00b7men", "!", "Fl\u00fcg\u00b7le", "dich", ",", "schnel\u00b7les", "Flugs", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$.", "NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "An meinen Busen! \u2013 Ah, du str\u00f6mest", "tokens": ["An", "mei\u00b7nen", "Bu\u00b7sen", "!", "\u2013", "Ah", ",", "du", "str\u00f6\u00b7mest"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir in die bebenden, offnen Arme!", "tokens": ["Mir", "in", "die", "be\u00b7ben\u00b7den", ",", "off\u00b7nen", "Ar\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Verla\u00dft mich, Freunde, da\u00df mir die trunkne Brust", "tokens": ["Ver\u00b7la\u00dft", "mich", ",", "Freun\u00b7de", ",", "da\u00df", "mir", "die", "trunk\u00b7ne", "Brust"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im k\u00fchlern Taumel brenne! \u2013 \u2013 Mein Bardenhold", "tokens": ["Im", "k\u00fch\u00b7lern", "Tau\u00b7mel", "bren\u00b7ne", "!", "\u2013", "\u2013", "Mein", "Bar\u00b7den\u00b7hold"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$.", "$(", "$(", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entheb mein Spiel dem Eichenspr\u00f6\u00dfling,", "tokens": ["Ent\u00b7heb", "mein", "Spiel", "dem", "Ei\u00b7chen\u00b7spr\u00f6\u00df\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich mein Herz in die Saiten singe!", "tokens": ["Da\u00df", "ich", "mein", "Herz", "in", "die", "Sai\u00b7ten", "sin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Noch Einen Rundku\u00df, Freunde, bevor mein Schwur", "tokens": ["Noch", "Ei\u00b7nen", "Rund\u00b7ku\u00df", ",", "Freun\u00b7de", ",", "be\u00b7vor", "mein", "Schwur"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Bund versiegelt, welchen die Tugend kn\u00fcpft;", "tokens": ["Den", "Bund", "ver\u00b7sie\u00b7gelt", ",", "wel\u00b7chen", "die", "Tu\u00b7gend", "kn\u00fcpft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PWAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch Einen Handschlag vor den Augen", "tokens": ["Noch", "Ei\u00b7nen", "Hand\u00b7schlag", "vor", "den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gottes, der unsichtbar um uns wandelt!", "tokens": ["Got\u00b7tes", ",", "der", "un\u00b7sicht\u00b7bar", "um", "uns", "wan\u00b7delt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Durch alle Sterne hallt er! Eloa blickt", "tokens": ["Durch", "al\u00b7le", "Ster\u00b7ne", "hallt", "er", "!", "E\u00b7loa", "blickt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "$.", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus seiner Wolke nieder, und segnet uns;", "tokens": ["Aus", "sei\u00b7ner", "Wol\u00b7ke", "nie\u00b7der", ",", "und", "seg\u00b7net", "uns", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Geister unsrer V\u00e4ter schweben", "tokens": ["Die", "Geis\u00b7ter", "uns\u00b7rer", "V\u00e4\u00b7ter", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lichthell und lispelnd um unsre Saiten.", "tokens": ["Licht\u00b7hell", "und", "lis\u00b7pelnd", "um", "uns\u00b7re", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Seyd Zeugen, Engel! \u2013 \u2013 Haining beschw\u00f6rt den Bund! \u2013", "tokens": ["Seyd", "Zeu\u00b7gen", ",", "En\u00b7gel", "!", "\u2013", "\u2013", "Hai\u00b7ning", "be\u00b7schw\u00f6rt", "den", "Bund", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "NN", "$,", "NE", "$.", "$(", "$(", "NE", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Mond blinkt heller, goldner und goldner malt", "tokens": ["Der", "Mond", "blinkt", "hel\u00b7ler", ",", "gold\u00b7ner", "und", "gold\u00b7ner", "malt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sich jede Wolke, die melodisch", "tokens": ["Sich", "je\u00b7de", "Wol\u00b7ke", ",", "die", "me\u00b7lo\u00b7disch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "PIAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In das Gewirbel der Harfe murmelt.", "tokens": ["In", "das", "Ge\u00b7wir\u00b7bel", "der", "Har\u00b7fe", "mur\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}}, "stanza.9": {"line.1": {"text": "Seyd Zeugen, Geister! \u2013 \u2013 Haining beschw\u00f6rt den Bund! \u2013", "tokens": ["Seyd", "Zeu\u00b7gen", ",", "Geis\u00b7ter", "!", "\u2013", "\u2013", "Hai\u00b7ning", "be\u00b7schw\u00f6rt", "den", "Bund", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "NN", "$,", "NN", "$.", "$(", "$(", "NE", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Spiel verstumme flugs, mein Ged\u00e4chtni\u00df sey", "tokens": ["Mein", "Spiel", "ver\u00b7stum\u00b7me", "flugs", ",", "mein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "sey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Brandmahl, und mein Name Schande:", "tokens": ["Ein", "Brand\u00b7mahl", ",", "und", "mein", "Na\u00b7me", "Schan\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Falls ich die Freunde nicht ewig liebe!", "tokens": ["Falls", "ich", "die", "Freun\u00b7de", "nicht", "e\u00b7wig", "lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Kein blaues Auge weine die Blumen na\u00df,", "tokens": ["Kein", "blau\u00b7es", "Au\u00b7ge", "wei\u00b7ne", "die", "Blu\u00b7men", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die meinen Todtenh\u00fcgel beduften; falls", "tokens": ["Die", "mei\u00b7nen", "Tod\u00b7ten\u00b7h\u00fc\u00b7gel", "be\u00b7duf\u00b7ten", ";", "falls"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "KOUS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich Lieder t\u00f6ne, welche Deutschland", "tokens": ["Ich", "Lie\u00b7der", "t\u00f6\u00b7ne", ",", "wel\u00b7che", "Deutschland"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "$,", "PRELS", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00e4nden und Laster und Wollust hauchen!", "tokens": ["Sch\u00e4n\u00b7den", "und", "Las\u00b7ter", "und", "Wol\u00b7lust", "hau\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Der Enkel stampfte zornig auf meine Gruft,", "tokens": ["Der", "En\u00b7kel", "stampf\u00b7te", "zor\u00b7nig", "auf", "mei\u00b7ne", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wann meine Lieder Gift in das weiche Herz", "tokens": ["Wann", "mei\u00b7ne", "Lie\u00b7der", "Gift", "in", "das", "wei\u00b7che", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des M\u00e4dchens tr\u00e4ufeln, und verfluche", "tokens": ["Des", "M\u00e4d\u00b7chens", "tr\u00e4u\u00b7feln", ",", "und", "ver\u00b7flu\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Meine zerst\u00e4ubende kalte Asche! \u2013 \u2013", "tokens": ["Mei\u00b7ne", "zer\u00b7st\u00e4u\u00b7ben\u00b7de", "kal\u00b7te", "A\u00b7sche", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$.", "$(", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Die Geister fl\u00fcstern lauter \u2013 \u2013 die Linde haucht", "tokens": ["Die", "Geis\u00b7ter", "fl\u00fcs\u00b7tern", "lau\u00b7ter", "\u2013", "\u2013", "die", "Lin\u00b7de", "haucht"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "$(", "$(", "ART", "NE", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mir tiefre Schauer \u2013 Schleu\u00df mich in deinen Arm,", "tokens": ["Mir", "tief\u00b7re", "Schau\u00b7er", "\u2013", "Schleu\u00df", "mich", "in", "dei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O Gottschalk, da\u00df die Seelenschauer", "tokens": ["O", "Gott\u00b7schalk", ",", "da\u00df", "die", "See\u00b7len\u00b7schau\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "ART", "NN"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich in Entz\u00fcckung der Freude wandeln!", "tokens": ["Sich", "in", "Ent\u00b7z\u00fc\u00b7ckung", "der", "Freu\u00b7de", "wan\u00b7deln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}