{"dta.poem.1683": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sinnen-Bilder auff ein Braut-  \n Bette.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Spiegel:", "tokens": ["Ein", "Spie\u00b7gel", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Hierwider scheint", "tokens": ["Hier\u00b7wi\u00b7der", "scheint"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was du gemeynt.", "tokens": ["Was", "du", "ge\u00b7meynt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Eine Laute:", "tokens": ["Ei\u00b7ne", "Lau\u00b7te", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Deren Steigen/ jener Fall", "tokens": ["De\u00b7ren", "Stei\u00b7gen", "/", "je\u00b7ner", "Fall"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$(", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Giebt den angenehmen Schall.", "tokens": ["Giebt", "den", "an\u00b7ge\u00b7neh\u00b7men", "Schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Pfersing-Baum/ dessen Bl\u00e4tter der Zunge/ die Frucht", "tokens": ["Ein", "Pfer\u00b7sing\u00b7Baum", "/", "des\u00b7sen", "Bl\u00e4t\u00b7ter", "der", "Zun\u00b7ge", "/", "die", "Frucht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "PRELAT", "NN", "ART", "NN", "$(", "ART", "NN"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "dem Hertzen gleichet:", "tokens": ["dem", "Hert\u00b7zen", "glei\u00b7chet", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Des treuen Hertzens Grund", "tokens": ["Des", "treu\u00b7en", "Hert\u00b7zens", "Grund"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Macht Ku\u00df und Zunge kund.", "tokens": ["Macht", "Ku\u00df", "und", "Zun\u00b7ge", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Paradie\u00df-Vogel/ eine N\u00e4gel-Blum im Munde f\u00fch-", "tokens": ["Ein", "Pa\u00b7ra\u00b7die\u00df\u00b7Vo\u00b7gel", "/", "ei\u00b7ne", "N\u00e4\u00b7gel\u00b7Blum", "im", "Mun\u00b7de", "f\u00fch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Die Liebe f\u00fchrt darvon", "tokens": ["Die", "Lie\u00b7be", "f\u00fchrt", "dar\u00b7von"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der treuen M\u00fche Lohn.", "tokens": ["Der", "treu\u00b7en", "M\u00fc\u00b7he", "Lohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Cupido mit verbundenen Augen/ auff einem Felsen im ", "tokens": ["Cu\u00b7pi\u00b7do", "mit", "ver\u00b7bun\u00b7de\u00b7nen", "Au\u00b7gen", "/", "auff", "ei\u00b7nem", "Fel\u00b7sen", "im"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "$(", "APPR", "ART", "NN", "APPRART"], "meter": "+----+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wer mich nicht wehlt", "tokens": ["Wer", "mich", "nicht", "wehlt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den Weg verfehlt.", "tokens": ["Den", "Weg", "ver\u00b7fehlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Die Sonne im Thier-Krei\u00df:", "tokens": ["Die", "Son\u00b7ne", "im", "Thier\u00b7Krei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--++", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht neben sich/", "tokens": ["Nicht", "ne\u00b7ben", "sich", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PRF", "$("], "meter": "+---", "measure": "dactylic.init"}, "line.3": {"text": "Nicht hinter mich.", "tokens": ["Nicht", "hin\u00b7ter", "mich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Eine Pfauin mit ihren Jungen:", "tokens": ["Ei\u00b7ne", "Pfau\u00b7in", "mit", "ih\u00b7ren", "Jun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der keuschen Zucht", "tokens": ["Der", "keu\u00b7schen", "Zucht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gew\u00fcnschte Frucht.", "tokens": ["Ge\u00b7w\u00fcnschte", "Frucht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Ein Krantz/ welchen die Spinne mit ihrem Gewebe um-", "tokens": ["Ein", "Krantz", "/", "wel\u00b7chen", "die", "Spin\u00b7ne", "mit", "ih\u00b7rem", "Ge\u00b7we\u00b7be", "um"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PWAT", "ART", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+---+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So haubt sich eine Nacht", "tokens": ["So", "haubt", "sich", "ei\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Krantzes edle Pracht.", "tokens": ["Des", "Krant\u00b7zes", "ed\u00b7le", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}