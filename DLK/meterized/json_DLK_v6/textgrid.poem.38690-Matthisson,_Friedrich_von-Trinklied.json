{"textgrid.poem.38690": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Gram soll heute,", "tokens": ["Der", "Gram", "soll", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bei goldnem Wein,", "tokens": ["Bei", "gold\u00b7nem", "Wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Des Windes Beute", "tokens": ["Des", "Win\u00b7des", "Beu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie gestern seyn!", "tokens": ["Wie", "ge\u00b7stern", "seyn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Dahinten lasse,", "tokens": ["Da\u00b7hin\u00b7ten", "las\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wer hoch sich freut,", "tokens": ["Wer", "hoch", "sich", "freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die leichenblasse", "tokens": ["Die", "lei\u00b7chen\u00b7blas\u00b7se"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vergangenheit!", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Kein roher Scythe", "tokens": ["Kein", "ro\u00b7her", "Scy\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nah' diesem Kreis!", "tokens": ["Nah'", "die\u00b7sem", "Kreis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Des Frohsinns Bl\u00fcthe", "tokens": ["Des", "Froh\u00b7sinns", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Liebt Mittelgleis.", "tokens": ["Liebt", "Mit\u00b7tel\u00b7gleis", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wir ziehn die Segel,", "tokens": ["Wir", "ziehn", "die", "Se\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wenn Taumel dr\u00e4un,", "tokens": ["Wenn", "Tau\u00b7mel", "dr\u00e4un", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nach Flaccus Regel,", "tokens": ["Nach", "Flac\u00b7cus", "Re\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Bed\u00e4chtig ein.", "tokens": ["Be\u00b7d\u00e4ch\u00b7tig", "ein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Der Gram soll heute,", "tokens": ["Der", "Gram", "soll", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bei goldnem Wein,", "tokens": ["Bei", "gold\u00b7nem", "Wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Des Windes Beute", "tokens": ["Des", "Win\u00b7des", "Beu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie gestern seyn!", "tokens": ["Wie", "ge\u00b7stern", "seyn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}