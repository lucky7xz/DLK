{"textgrid.poem.61475": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh, das ist es, was auf Erden", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, das ist es, was auf Erden", "tokens": ["Sieh", ",", "das", "ist", "es", ",", "was", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PDS", "VAFIN", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jung dich h\u00e4lt zu jeder Frist,", "tokens": ["Jung", "dich", "h\u00e4lt", "zu", "je\u00b7der", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du ewig bleibst im Werden,", "tokens": ["Da\u00df", "du", "e\u00b7wig", "bleibst", "im", "Wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Welt im Wandeln ist.", "tokens": ["Wie", "die", "Welt", "im", "Wan\u00b7deln", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was dich r\u00fchrt im Herzensgrunde,", "tokens": ["Was", "dich", "r\u00fchrt", "im", "Her\u00b7zens\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal kommt's und nimmer so;", "tokens": ["Ein\u00b7mal", "kommt's", "und", "nim\u00b7mer", "so", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum ergreife k\u00fchn die Stunde,", "tokens": ["Drum", "er\u00b7grei\u00b7fe", "k\u00fchn", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heute weine, heut sei froh!", "tokens": ["Heu\u00b7te", "wei\u00b7ne", ",", "heut", "sei", "froh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gib dem Gl\u00fcck dich voll und innig,", "tokens": ["Gib", "dem", "Gl\u00fcck", "dich", "voll", "und", "in\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trag es, wenn der Schmerz dich pre\u00dft,", "tokens": ["Trag", "es", ",", "wenn", "der", "Schmerz", "dich", "pre\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nimmer eigensinnig", "tokens": ["A\u00b7ber", "nim\u00b7mer", "ei\u00b7gen\u00b7sin\u00b7nig"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren Schatten halte fest.", "tokens": ["Ih\u00b7ren", "Schat\u00b7ten", "hal\u00b7te", "fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Heiter senke, was vergangen,", "tokens": ["Hei\u00b7ter", "sen\u00b7ke", ",", "was", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Abgrund jeder Nacht!", "tokens": ["In", "den", "Ab\u00b7grund", "je\u00b7der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll der Tag dich frisch empfangen,", "tokens": ["Soll", "der", "Tag", "dich", "frisch", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei getreu, doch neu, erwacht.", "tokens": ["Sei", "ge\u00b7treu", ",", "doch", "neu", ",", "er\u00b7wacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADV", "ADJD", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Frei dich wandelnd und entfaltend,", "tokens": ["Frei", "dich", "wan\u00b7delnd", "und", "ent\u00b7fal\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Lilie w\u00e4chst im Feld,", "tokens": ["Wie", "die", "Li\u00b7lie", "w\u00e4chst", "im", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wachse fort, und nie veraltend", "tokens": ["Wach\u00b7se", "fort", ",", "und", "nie", "ver\u00b7al\u00b7tend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "KON", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht und klingt f\u00fcr dich die Welt.", "tokens": ["Bl\u00fcht", "und", "klingt", "f\u00fcr", "dich", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}