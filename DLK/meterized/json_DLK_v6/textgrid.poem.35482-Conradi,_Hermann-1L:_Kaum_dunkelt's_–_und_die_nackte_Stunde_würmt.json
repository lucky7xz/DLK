{"textgrid.poem.35482": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kaum dunkelt's \u2013 und die nackte Stunde w\u00fcrmt", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum dunkelt's \u2013 und die nackte Stunde w\u00fcrmt", "tokens": ["Kaum", "dun\u00b7kelt's", "\u2013", "und", "die", "nack\u00b7te", "Stun\u00b7de", "w\u00fcrmt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "$(", "KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Goldhungrig wieder durch die Gassen ...", "tokens": ["Gold\u00b7hung\u00b7rig", "wie\u00b7der", "durch", "die", "Gas\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In mir war's still, als h\u00e4tt' es ausgest\u00fcrmt \u2013", "tokens": ["In", "mir", "wa\u00b7r's", "still", ",", "als", "h\u00e4tt'", "es", "aus\u00b7ge\u00b7st\u00fcrmt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "$,", "KOKOM", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Als h\u00e4tt' ich allen Zwiestreit einget\u00fcrmt \u2013", "tokens": ["Als", "h\u00e4tt'", "ich", "al\u00b7len", "Zwies\u00b7treit", "ein\u00b7ge\u00b7t\u00fcrmt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun w\u00e4hn' ich wieder mich so gottverlassen ...", "tokens": ["Nun", "w\u00e4hn'", "ich", "wie\u00b7der", "mich", "so", "gott\u00b7ver\u00b7las\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "PPER", "ADV", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ins Abendrot, das kaum die Nacht verschlang,", "tokens": ["Ins", "A\u00b7ben\u00b7drot", ",", "das", "kaum", "die", "Nacht", "ver\u00b7schlang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hatt' ich ein M\u00e4rchen k\u00fchn von Gott getr\u00e4umt ...", "tokens": ["Hatt'", "ich", "ein", "M\u00e4r\u00b7chen", "k\u00fchn", "von", "Gott", "ge\u00b7tr\u00e4umt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und der Begeistrung Flammen\u00fcberschwang,", "tokens": ["Und", "der", "Be\u00b7geis\u00b7trung", "Flam\u00b7men\u00b7\u00fc\u00b7bersc\u00b7hwang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des heil'gen Geistes hei\u00dfer Gipfeldrang,", "tokens": ["Des", "heil'\u00b7gen", "Geis\u00b7tes", "hei\u00b7\u00dfer", "Gip\u00b7fel\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hatt' himmel\u00f6ffnend mir die Welt versch\u00e4umt ...", "tokens": ["Hatt'", "him\u00b7me\u00b7l\u00f6ff\u00b7nend", "mir", "die", "Welt", "ver\u00b7sch\u00e4umt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf stillen Pfaden, wo vom lauten Markt", "tokens": ["Auf", "stil\u00b7len", "Pfa\u00b7den", ",", "wo", "vom", "lau\u00b7ten", "Markt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur selten ein verirrtes Klingen t\u00f6nt,", "tokens": ["Nur", "sel\u00b7ten", "ein", "ver\u00b7irr\u00b7tes", "Klin\u00b7gen", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "War ich in Gott gewachsen und erstarkt,", "tokens": ["War", "ich", "in", "Gott", "ge\u00b7wach\u00b7sen", "und", "er\u00b7starkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hatt' alle Unrast tapfer eingesargt,", "tokens": ["Hatt'", "al\u00b7le", "Un\u00b7rast", "tap\u00b7fer", "ein\u00b7ge\u00b7sargt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und in mir war der Geist, der da ", "tokens": ["Und", "in", "mir", "war", "der", "Geist", ",", "der", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun schweif' ich wieder durch das Stadtgew\u00fchl ...", "tokens": ["Nun", "schweif'", "ich", "wie\u00b7der", "durch", "das", "Stadt\u00b7ge\u00b7w\u00fchl", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kaum dunkelt's, und der Fr\u00fchlingshimmel hat", "tokens": ["Kaum", "dun\u00b7kelt's", ",", "und", "der", "Fr\u00fch\u00b7lings\u00b7him\u00b7mel", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "$,", "KON", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch keine Sterne ... Und mein Kraftgef\u00fchl", "tokens": ["Noch", "kei\u00b7ne", "Ster\u00b7ne", "...", "Und", "mein", "Kraft\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zerschellt an dieser Toren seichtem Spiel \u2013", "tokens": ["Zer\u00b7schellt", "an", "die\u00b7ser", "To\u00b7ren", "seich\u00b7tem", "Spiel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Schwingen meiner Seele sinken matt ...", "tokens": ["Die", "Schwin\u00b7gen", "mei\u00b7ner", "See\u00b7le", "sin\u00b7ken", "matt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das alte Lied! ... Auf allen Lippen liegt", "tokens": ["Das", "al\u00b7te", "Lied", "!", "...", "Auf", "al\u00b7len", "Lip\u00b7pen", "liegt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach Gold, nach rotem Gold der wilde Ruf ...", "tokens": ["Nach", "Gold", ",", "nach", "ro\u00b7tem", "Gold", "der", "wil\u00b7de", "Ruf", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die S\u00fcnde hat jedwedes Herz besiegt,", "tokens": ["Die", "S\u00fcn\u00b7de", "hat", "jed\u00b7we\u00b7des", "Herz", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wie ein Schrei durch alle L\u00fcfte fliegt:", "tokens": ["Und", "wie", "ein", "Schrei", "durch", "al\u00b7le", "L\u00fcf\u00b7te", "fliegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir sind die Frucht, die Kains Same schuf! ...", "tokens": ["Wir", "sind", "die", "Frucht", ",", "die", "Kains", "Sa\u00b7me", "schuf", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Mit frechem Blick l\u00e4dt sich die Schande ein,", "tokens": ["Mit", "fre\u00b7chem", "Blick", "l\u00e4dt", "sich", "die", "Schan\u00b7de", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Winkel hockt das Elend, ein Fragment ...", "tokens": ["Im", "Win\u00b7kel", "hockt", "das", "E\u00b7lend", ",", "ein", "Frag\u00b7ment", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und dr\u00fcber nun ein heller Sternenschein \u2013 \u2013", "tokens": ["Und", "dr\u00fc\u00b7ber", "nun", "ein", "hel\u00b7ler", "Ster\u00b7nen\u00b7schein", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PAV", "ADV", "ART", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich treibe br\u00fctend durch der Menschen Reihn ...", "tokens": ["Ich", "trei\u00b7be", "br\u00fc\u00b7tend", "durch", "der", "Men\u00b7schen", "Reihn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}}}}