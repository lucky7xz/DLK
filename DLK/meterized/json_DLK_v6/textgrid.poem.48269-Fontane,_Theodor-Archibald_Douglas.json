{"textgrid.poem.48269": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Archibald Douglas", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich hab' es getragen sieben Jahr", "tokens": ["\u00bb", "ich", "hab'", "es", "ge\u00b7tra\u00b7gen", "sie\u00b7ben", "Jahr"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PPER", "VVPP", "CARD", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ich kann es nicht tragen mehr,", "tokens": ["Und", "ich", "kann", "es", "nicht", "tra\u00b7gen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo immer die Welt am sch\u00f6nsten war,", "tokens": ["Wo", "im\u00b7mer", "die", "Welt", "am", "sch\u00f6ns\u00b7ten", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPRART", "ADJA", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da war sie \u00f6d' und leer.", "tokens": ["Da", "war", "sie", "\u00f6d'", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich will hintreten vor sein Gesicht", "tokens": ["Ich", "will", "hin\u00b7tre\u00b7ten", "vor", "sein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In dieser Knechtsgestalt,", "tokens": ["In", "die\u00b7ser", "Knechts\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kann meine Bitte versagen nicht,", "tokens": ["Er", "kann", "mei\u00b7ne", "Bit\u00b7te", "ver\u00b7sa\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich bin ja worden alt.", "tokens": ["Ich", "bin", "ja", "wor\u00b7den", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VAPP", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und tr\u00fcg' er noch den alten Groll,", "tokens": ["Und", "tr\u00fcg'", "er", "noch", "den", "al\u00b7ten", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frisch wie am ersten Tag,", "tokens": ["Frisch", "wie", "am", "ers\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So komme, was da kommen soll,", "tokens": ["So", "kom\u00b7me", ",", "was", "da", "kom\u00b7men", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und komme, was da mag.\u00ab", "tokens": ["Und", "kom\u00b7me", ",", "was", "da", "mag.", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "abbreviation", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "NE", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Graf Douglas spricht's. Am Weg ein Stein", "tokens": ["Graf", "Doug\u00b7las", "spricht'", "s.", "Am", "Weg", "ein", "Stein"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "NE", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lud ihn zu harter Ruh,", "tokens": ["Lud", "ihn", "zu", "har\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sah in Wald und Feld hinein,", "tokens": ["Er", "sah", "in", "Wald", "und", "Feld", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Augen fielen ihm zu.", "tokens": ["Die", "Au\u00b7gen", "fie\u00b7len", "ihm", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Er trug einen Harnisch, rostig und schwer,", "tokens": ["Er", "trug", "ei\u00b7nen", "Har\u00b7nisch", ",", "ros\u00b7tig", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dar\u00fcber ein Pilgerkleid \u2013", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "ein", "Pil\u00b7ger\u00b7kleid", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da horch, vom Waldrand scholl es her.", "tokens": ["Da", "horch", ",", "vom", "Wald\u00b7rand", "scholl", "es", "her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie von H\u00f6rnern und Jagdgeleit.", "tokens": ["Wie", "von", "H\u00f6r\u00b7nern", "und", "Jagd\u00b7ge\u00b7leit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Und Kies und Staub aufwirbelte dicht,", "tokens": ["Und", "Kies", "und", "Staub", "auf\u00b7wir\u00b7bel\u00b7te", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Her jagte Meut' und Mann,", "tokens": ["Her", "jag\u00b7te", "Meut'", "und", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ehe der Graf sich aufgericht't,", "tokens": ["Und", "e\u00b7he", "der", "Graf", "sich", "auf\u00b7ge\u00b7richt't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Waren Ro\u00df und Reiter heran.", "tokens": ["Wa\u00b7ren", "Ro\u00df", "und", "Rei\u00b7ter", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "K\u00f6nig Jakob sa\u00df auf hohem Ro\u00df,", "tokens": ["K\u00f6\u00b7nig", "Ja\u00b7kob", "sa\u00df", "auf", "ho\u00b7hem", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Graf Douglas gr\u00fc\u00dfte tief,", "tokens": ["Graf", "Doug\u00b7las", "gr\u00fc\u00df\u00b7te", "tief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem K\u00f6nig das Blut in die Wange scho\u00df,", "tokens": ["Dem", "K\u00f6\u00b7nig", "das", "Blut", "in", "die", "Wan\u00b7ge", "scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Douglas aber rief:", "tokens": ["Der", "Doug\u00b7las", "a\u00b7ber", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbk\u00f6nig Jakob, schaue mich gn\u00e4dig an", "tokens": ["\u00bb", "k\u00f6\u00b7nig", "Ja\u00b7kob", ",", "schau\u00b7e", "mich", "gn\u00e4\u00b7dig", "an"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VMFIN", "NE", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Und h\u00f6re mich in Geduld,", "tokens": ["Und", "h\u00f6\u00b7re", "mich", "in", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Was meine Br\u00fcder dir angetan,", "tokens": ["Was", "mei\u00b7ne", "Br\u00fc\u00b7der", "dir", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es war nicht meine Schuld.", "tokens": ["Es", "war", "nicht", "mei\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Denk nicht an den alten Douglas-Neid,", "tokens": ["Denk", "nicht", "an", "den", "al\u00b7ten", "Dou\u00b7glas\u00b7Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der trotzig dich bekriegt,", "tokens": ["Der", "trot\u00b7zig", "dich", "be\u00b7kriegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denk lieber an deine Kinderzeit,", "tokens": ["Denk", "lie\u00b7ber", "an", "dei\u00b7ne", "Kin\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo ich dich auf den Knien gewiegt.", "tokens": ["Wo", "ich", "dich", "auf", "den", "Kni\u00b7en", "ge\u00b7wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.10": {"line.1": {"text": "Denk lieber zur\u00fcck an Stirling-Schlo\u00df,", "tokens": ["Denk", "lie\u00b7ber", "zu\u00b7r\u00fcck", "an", "Stir\u00b7ling\u00b7Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo ich Spielzeug dir geschnitzt,", "tokens": ["Wo", "ich", "Spiel\u00b7zeug", "dir", "ge\u00b7schnitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich gehoben auf deines Vaters Ro\u00df", "tokens": ["Dich", "ge\u00b7ho\u00b7ben", "auf", "dei\u00b7nes", "Va\u00b7ters", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und Pfeile dir zugespitzt.", "tokens": ["Und", "Pfei\u00b7le", "dir", "zu\u00b7ge\u00b7spitzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Denk lieber zur\u00fcck an Linlithgow,", "tokens": ["Denk", "lie\u00b7ber", "zu\u00b7r\u00fcck", "an", "Lin\u00b7lith\u00b7gow", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An den See und den Vogelherd,", "tokens": ["An", "den", "See", "und", "den", "Vo\u00b7gel\u00b7herd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo ich dich fischen und jagen froh", "tokens": ["Wo", "ich", "dich", "fi\u00b7schen", "und", "ja\u00b7gen", "froh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "KON", "VVFIN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und schwimmen und springen gelehrt.", "tokens": ["Und", "schwim\u00b7men", "und", "sprin\u00b7gen", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "O denk an alles, was einsten war,", "tokens": ["O", "denk", "an", "al\u00b7les", ",", "was", "eins\u00b7ten", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIS", "$,", "PRELS", "PIS", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und s\u00e4nftige deinen Sinn,", "tokens": ["Und", "s\u00e4nf\u00b7ti\u00b7ge", "dei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich hab' es geb\u00fc\u00dfet sieben Jahr,", "tokens": ["Ich", "hab'", "es", "ge\u00b7b\u00fc\u00b7\u00dfet", "sie\u00b7ben", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "CARD", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df ich ein Douglas bin.\u00ab", "tokens": ["Da\u00df", "ich", "ein", "Doug\u00b7las", "bin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbich seh' dich nicht, Graf Archibald,", "tokens": ["\u00bb", "ich", "seh'", "dich", "nicht", ",", "Graf", "Ar\u00b7chi\u00b7bald", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich h\u00f6r' deine Stimme nicht,", "tokens": ["Ich", "h\u00f6r'", "dei\u00b7ne", "Stim\u00b7me", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mir ist, als ob ein Rauschen im Wald", "tokens": ["Mir", "ist", ",", "als", "ob", "ein", "Rau\u00b7schen", "im", "Wald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Von alten Zeiten spricht.", "tokens": ["Von", "al\u00b7ten", "Zei\u00b7ten", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Mir klingt das Rauschen s\u00fc\u00df und traut,", "tokens": ["Mir", "klingt", "das", "Rau\u00b7schen", "s\u00fc\u00df", "und", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich lausch' ihm immer noch,", "tokens": ["Ich", "lausch'", "ihm", "im\u00b7mer", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dazwischen aber klingt es laut:", "tokens": ["Da\u00b7zwi\u00b7schen", "a\u00b7ber", "klingt", "es", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist ein Douglas doch.", "tokens": ["Er", "ist", "ein", "Doug\u00b7las", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Ich seh' dich nicht, ich h\u00f6re dich nicht,", "tokens": ["Ich", "seh'", "dich", "nicht", ",", "ich", "h\u00f6\u00b7re", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das ist alles, was ich kann,", "tokens": ["Das", "ist", "al\u00b7les", ",", "was", "ich", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Douglas vor meinem Angesicht", "tokens": ["Ein", "Doug\u00b7las", "vor", "mei\u00b7nem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4r' ein verlorener Mann.\u00ab", "tokens": ["W\u00e4r'", "ein", "ver\u00b7lo\u00b7re\u00b7ner", "Mann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "K\u00f6nig Jakob gab seinem Ro\u00df den Sporn,", "tokens": ["K\u00f6\u00b7nig", "Ja\u00b7kob", "gab", "sei\u00b7nem", "Ro\u00df", "den", "Sporn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPOSAT",