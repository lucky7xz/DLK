{"textgrid.poem.48676": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "39. Anemone und Neren", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Anemone", "tokens": ["Als", "A\u00b7ne\u00b7mo\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "der Venus kleinem Sohne", "tokens": ["der", "Ve\u00b7nus", "klei\u00b7nem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "zuwider war,", "tokens": ["zu\u00b7wi\u00b7der", "war", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "weil sie Nerenen,", "tokens": ["weil", "sie", "Ne\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "des z\u00fcchtigen, des sch\u00f6nen,", "tokens": ["des", "z\u00fcch\u00b7ti\u00b7gen", ",", "des", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "verga\u00df sogar,", "tokens": ["ver\u00b7ga\u00df", "so\u00b7gar", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "indem er sie", "tokens": ["in\u00b7dem", "er", "sie"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "durch Scheiden mu\u00dfte lassen,", "tokens": ["durch", "Schei\u00b7den", "mu\u00df\u00b7te", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "hub sie ihn an je mehr und mehr zu hassen,", "tokens": ["hub", "sie", "ihn", "an", "je", "mehr", "und", "mehr", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADV", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "die Falsche, die.", "tokens": ["die", "Fal\u00b7sche", ",", "die", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Neren, der schiede.", "tokens": ["Ne\u00b7ren", ",", "der", "schie\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie ward des Liebsten m\u00fcde", "tokens": ["Sie", "ward", "des", "Liebs\u00b7ten", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und ihrer Pflicht.", "tokens": ["und", "ih\u00b7rer", "Pflicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Auf bunte Kr\u00e4uter,", "tokens": ["Auf", "bun\u00b7te", "Kr\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "auf Blumen und nichts weiter", "tokens": ["auf", "Blu\u00b7men", "und", "nichts", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "PIS", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "war sie erpicht,", "tokens": ["war", "sie", "er\u00b7picht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "bis einer Zeit", "tokens": ["bis", "ei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Kupido ihr ward innen,", "tokens": ["Ku\u00b7pi\u00b7do", "ihr", "ward", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "als sie allein um ihre Blumenbr\u00fcnnen", "tokens": ["als", "sie", "al\u00b7lein", "um", "ih\u00b7re", "Blu\u00b7men\u00b7br\u00fcn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "spaziert erfreut.", "tokens": ["spa\u00b7ziert", "er\u00b7freut", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nach dir, du Harte,", "tokens": ["Nach", "dir", ",", "du", "Har\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "bin ich es, der ich warte!", "tokens": ["bin", "ich", "es", ",", "der", "ich", "war\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "fuhr Amor auf.", "tokens": ["fuhr", "A\u00b7mor", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Stracks sank sie nieder,", "tokens": ["Stracks", "sank", "sie", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "kam auch zu sich nicht wieder,", "tokens": ["kam", "auch", "zu", "sich", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PRF", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "so starb sie drauf.", "tokens": ["so", "starb", "sie", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Den toten Geist", "tokens": ["Den", "to\u00b7ten", "Geist"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "streut Amor aus f\u00fcr Samen,", "tokens": ["streut", "A\u00b7mor", "aus", "f\u00fcr", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "bald wuchs ein Kraut, das nach der Nymfen Namen", "tokens": ["bald", "wuchs", "ein", "Kraut", ",", "das", "nach", "der", "Nym\u00b7fen", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "noch itzund hei\u00dft.", "tokens": ["noch", "it\u00b7zund", "hei\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ach, Anemone,", "tokens": ["Ach", ",", "A\u00b7ne\u00b7mo\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "du aller Sch\u00f6nen Krone,", "tokens": ["du", "al\u00b7ler", "Sch\u00f6\u00b7nen", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "halt Ja und Nein.", "tokens": ["halt", "Ja", "und", "Nein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKANT", "KON", "PTKANT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "La\u00df dir, o Blume,", "tokens": ["La\u00df", "dir", ",", "o", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "f\u00fcr aller Blumen Ruhme", "tokens": ["f\u00fcr", "al\u00b7ler", "Blu\u00b7men", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die Treue sein.", "tokens": ["die", "Treu\u00b7e", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Neren ist tot", "tokens": ["Ne\u00b7ren", "ist", "tot"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "von Anemonens Schmerze:", "tokens": ["von", "A\u00b7ne\u00b7mo\u00b7nens", "Schmer\u00b7ze", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "ich werd' entfreit durch Anemonens Herze", "tokens": ["ich", "werd'", "ent\u00b7freit", "durch", "A\u00b7ne\u00b7mo\u00b7nens", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "von aller Not.", "tokens": ["von", "al\u00b7ler", "Not", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}