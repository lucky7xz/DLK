{"textgrid.poem.40479": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwie soll ich liebend dich umfassen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie soll ich liebend dich umfassen", "tokens": ["\u00bb", "wie", "soll", "ich", "lie\u00b7bend", "dich", "um\u00b7fas\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und glauben, was dein Mund verspricht,", "tokens": ["Und", "glau\u00b7ben", ",", "was", "dein", "Mund", "ver\u00b7spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da treulos du selbst die verlassen,", "tokens": ["Da", "treu\u00b7los", "du", "selbst", "die", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die einst dein Leben, Lied und Licht?\u00ab", "tokens": ["Die", "einst", "dein", "Le\u00b7ben", ",", "Lied", "und", "Licht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl hie\u00df mein Lied sie Licht und Leben,", "tokens": ["Wohl", "hie\u00df", "mein", "Lied", "sie", "Licht", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie damals l\u00fcg' ich jetzt auch nicht:", "tokens": ["Wie", "da\u00b7mals", "l\u00fcg'", "ich", "jetzt", "auch", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum ruf' ich k\u00fchn: du bist mir werther", "tokens": ["Drum", "ruf'", "ich", "k\u00fchn", ":", "du", "bist", "mir", "wert\u00b7her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VAFIN", "PPER", "ADJA"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als all mein Leben, Lied und Licht!", "tokens": ["Als", "all", "mein", "Le\u00b7ben", ",", "Lied", "und", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbdem Tag' hast du ihr Aug' verglichen,", "tokens": ["\u00bb", "dem", "Tag'", "hast", "du", "ihr", "Aug'", "ver\u00b7gli\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Haar den Sonnenstrahlen mild;", "tokens": ["Ihr", "Haar", "den", "Son\u00b7nen\u00b7strah\u00b7len", "mild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ei, ist's schon deinem Sinn entwichen,", "tokens": ["Ei", ",", "ist's", "schon", "dei\u00b7nem", "Sinn", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Sonn' und Tag der Treue Bild!\u00ab", "tokens": ["Da\u00df", "Sonn'", "und", "Tag", "der", "Treu\u00b7e", "Bild", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Nacht vergleich' ich deine Locken,", "tokens": ["Der", "Nacht", "ver\u00b7gleich'", "ich", "dei\u00b7ne", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Aug' dem Mond in n\u00e4cht'ger Luft;", "tokens": ["Dein", "Aug'", "dem", "Mond", "in", "n\u00e4cht'\u00b7ger", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ei, sollt ich's dir wohl erst noch sagen,", "tokens": ["Ei", ",", "sollt", "ich's", "dir", "wohl", "erst", "noch", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PIS", "PPER", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Nacht und Mond zur Liebe ruft?", "tokens": ["Da\u00df", "Nacht", "und", "Mond", "zur", "Lie\u00b7be", "ruft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbund schwurst du nicht, eh' zu erbleichen,", "tokens": ["\u00bb", "und", "schwurst", "du", "nicht", ",", "eh'", "zu", "er\u00b7blei\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als dich zu wenden je von ihr?", "tokens": ["Als", "dich", "zu", "wen\u00b7den", "je", "von", "ihr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum gingst du mir l\u00e4ngst zu den Leichen,", "tokens": ["Drum", "gingst", "du", "mir", "l\u00e4ngst", "zu", "den", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Drum, todter Mann, hinweg von mir!\u00ab", "tokens": ["Drum", ",", "tod\u00b7ter", "Mann", ",", "hin\u00b7weg", "von", "mir", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "$,", "ADJA", "NN", "$,", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohl schien ich selbst mir ein Begrab'ner,", "tokens": ["Wohl", "schien", "ich", "selbst", "mir", "ein", "Be\u00b7grab'\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der l\u00e4ngst schon unterm Rasen schlief,", "tokens": ["Der", "l\u00e4ngst", "schon", "un\u00b7term", "Ra\u00b7sen", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wecktest mich, ein milder Engel,", "tokens": ["Du", "weck\u00b7test", "mich", ",", "ein", "mil\u00b7der", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der mich ins sch\u00f6n're Leben rief.", "tokens": ["Der", "mich", "ins", "sch\u00f6n'\u00b7re", "Le\u00b7ben", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}