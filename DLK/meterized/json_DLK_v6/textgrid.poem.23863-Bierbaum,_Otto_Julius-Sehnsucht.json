{"textgrid.poem.23863": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eine leise Glocke klingt", "tokens": ["Wie", "ei\u00b7ne", "lei\u00b7se", "Glo\u00b7cke", "klingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sehnsucht in mir an;", "tokens": ["Die", "Sehn\u00b7sucht", "in", "mir", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wei\u00df nicht, woher, wohin sie singt,", "tokens": ["Wei\u00df", "nicht", ",", "wo\u00b7her", ",", "wo\u00b7hin", "sie", "singt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ich nicht lauschen kann.", "tokens": ["Weil", "ich", "nicht", "lau\u00b7schen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es treibt das Leben mich wild um,", "tokens": ["Es", "treibt", "das", "Le\u00b7ben", "mich", "wild", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dr\u00f6hnt um mich mit Gebraus,", "tokens": ["Dr\u00f6hnt", "um", "mich", "mit", "Ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APPR", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und m\u00e4hlich wird die Glocke stumm,", "tokens": ["Und", "m\u00e4h\u00b7lich", "wird", "die", "Glo\u00b7cke", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leise klingt sie aus.", "tokens": ["Und", "lei\u00b7se", "klingt", "sie", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie ist nur f\u00fcr den Feiertag", "tokens": ["Sie", "ist", "nur", "f\u00fcr", "den", "Fei\u00b7er\u00b7tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gemacht und viel zu fein,", "tokens": ["Ge\u00b7macht", "und", "viel", "zu", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als da\u00df ihr bebebanger Schlag", "tokens": ["Als", "da\u00df", "ihr", "be\u00b7be\u00b7ban\u00b7ger", "Schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Dr\u00e4ng in die L\u00e4rmlust ein.", "tokens": ["Dr\u00e4ng", "in", "die", "L\u00e4rm\u00b7lust", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie ist ein Ton von dorten her,", "tokens": ["Sie", "ist", "ein", "Ton", "von", "dor\u00b7ten", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo alles Feier ist;", "tokens": ["Wo", "al\u00b7les", "Fei\u00b7er", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wollte, da\u00df ich dorten w\u00e4r,", "tokens": ["Ich", "woll\u00b7te", ",", "da\u00df", "ich", "dor\u00b7ten", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo man den L\u00e4rm vergi\u00dft.", "tokens": ["Wo", "man", "den", "L\u00e4rm", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}