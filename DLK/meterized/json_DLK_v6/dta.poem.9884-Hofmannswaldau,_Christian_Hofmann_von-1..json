{"dta.poem.9884": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll Doris ich denn stets in banden gehn/", "tokens": ["Soll", "Do\u00b7ris", "ich", "denn", "stets", "in", "ban\u00b7den", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Soll schertz und schmertz stets an der spitze stehn/", "tokens": ["Soll", "schertz", "und", "schmertz", "stets", "an", "der", "spit\u00b7ze", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "KON", "ADJD", "ADV", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Soll seel und mund so hart gekr\u00e4ncket seyn/", "tokens": ["Soll", "seel", "und", "mund", "so", "hart", "ge\u00b7kr\u00e4n\u00b7cket", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "KON", "NN", "ADV", "ADJD", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So f\u00e4llt gewi\u00df der liebe grundstein ein.", "tokens": ["So", "f\u00e4llt", "ge\u00b7wi\u00df", "der", "lie\u00b7be", "grund\u00b7stein", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Entbinde mich durch deiner sch\u00f6nheit macht/", "tokens": ["Ent\u00b7bin\u00b7de", "mich", "durch", "dei\u00b7ner", "sch\u00f6n\u00b7heit", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sprich mich frey von dieser tr\u00fcben nacht:", "tokens": ["Und", "sprich", "mich", "frey", "von", "die\u00b7ser", "tr\u00fc\u00b7ben", "nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn wo dein aug nicht pol und leitstern ist/", "tokens": ["Denn", "wo", "dein", "aug", "nicht", "pol", "und", "leits\u00b7tern", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "PTKNEG", "ADJD", "KON", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So hat mein mast den schiffbruch schon erkist.", "tokens": ["So", "hat", "mein", "mast", "den", "schiff\u00b7bruch", "schon", "er\u00b7kist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dein mund der ist ein nachen von rubin/", "tokens": ["Dein", "mund", "der", "ist", "ein", "na\u00b7chen", "von", "ru\u00b7bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "ART", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der meiner seelen schiff kan an sich ziehn/", "tokens": ["Der", "mei\u00b7ner", "see\u00b7len", "schiff", "kan", "an", "sich", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "VMFIN", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die muscheln/ so der hafen in sich tr\u00e4gt/", "tokens": ["Die", "mu\u00b7scheln", "/", "so", "der", "ha\u00b7fen", "in", "sich", "tr\u00e4gt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat die natur mit perlen angelegt.", "tokens": ["Hat", "die", "na\u00b7tur", "mit", "per\u00b7len", "an\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Allein mir ist mein mund durch dich verh\u00fcllt/", "tokens": ["Al\u00b7lein", "mir", "ist", "mein", "mund", "durch", "dich", "ver\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der zucker/ der aus deinen lippen qvillt/", "tokens": ["Der", "zu\u00b7cker", "/", "der", "aus", "dei\u00b7nen", "lip\u00b7pen", "qvillt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df ein verg\u00e4lltes gifft vor artzney seyn/", "tokens": ["Mu\u00df", "ein", "ver\u00b7g\u00e4ll\u00b7tes", "gifft", "vor", "artz\u00b7ney", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "APPR", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So st\u00fcrtzt man mich ins todten-hau\u00df hinein.", "tokens": ["So", "st\u00fcrtzt", "man", "mich", "ins", "tod\u00b7ten\u00b7hau\u00df", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Drum sch\u00f6nstes kind/ entbinde mich von mir/", "tokens": ["Drum", "sch\u00f6ns\u00b7tes", "kind", "/", "ent\u00b7bin\u00b7de", "mich", "von", "mir", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$(", "VVFIN", "PPER", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nim/ als unterthan/ mich gantz zu dir:", "tokens": ["Und", "nim", "/", "als", "un\u00b7ter\u00b7than", "/", "mich", "gantz", "zu", "dir", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$(", "KOUS", "ADJD", "$(", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein sclave/ den der ketten klang bewegt/", "tokens": ["Ein", "scla\u00b7ve", "/", "den", "der", "ket\u00b7ten", "klang", "be\u00b7wegt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ART", "ART", "ADJA", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Tr\u00e4gt durch gedult viel leichter was er tr\u00e4gt.", "tokens": ["Tr\u00e4gt", "durch", "ge\u00b7dult", "viel", "leich\u00b7ter", "was", "er", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "ADJD", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}