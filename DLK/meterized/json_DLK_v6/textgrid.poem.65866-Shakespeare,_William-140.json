{"textgrid.poem.65866": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "140", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei klug, wie grausam! Sprenge nicht die Pforte", "tokens": ["Sei", "klug", ",", "wie", "grau\u00b7sam", "!", "Spren\u00b7ge", "nicht", "die", "Pfor\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "ADJD", "$.", "NN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stummer Geduld mit allzu bitterm Hohn:", "tokens": ["Stum\u00b7mer", "Ge\u00b7duld", "mit", "all\u00b7zu", "bit\u00b7term", "Hohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PTKA", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Damit mir Gram nicht Worte leih', und Worte", "tokens": ["Da\u00b7mit", "mir", "Gram", "nicht", "Wor\u00b7te", "leih'", ",", "und", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "NN", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein unerweichtes Herz zu schildern drohn.", "tokens": ["Dein", "un\u00b7er\u00b7weich\u00b7tes", "Herz", "zu", "schil\u00b7dern", "drohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sollt' ich dir raten, besser w\u00e4r's, wo nicht", "tokens": ["Sollt'", "ich", "dir", "ra\u00b7ten", ",", "bes\u00b7ser", "w\u00e4r's", ",", "wo", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "$,", "ADJD", "VAFIN", "$,", "PWAV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu lieben, doch mir Liebe vorzuheucheln;", "tokens": ["Zu", "lie\u00b7ben", ",", "doch", "mir", "Lie\u00b7be", "vor\u00b7zu\u00b7heu\u00b7cheln", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie bangen Kranken, wenn das Auge bricht,", "tokens": ["Wie", "ban\u00b7gen", "Kran\u00b7ken", ",", "wenn", "das", "Au\u00b7ge", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die \u00c4rzte noch mit Auferstehung schmeicheln.", "tokens": ["Die", "\u00c4rz\u00b7te", "noch", "mit", "Auf\u00b7er\u00b7ste\u00b7hung", "schmei\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn ich verzweifeln m\u00fc\u00dfte, w\u00fcrd' ich toll;", "tokens": ["Wenn", "ich", "ver\u00b7zwei\u00b7feln", "m\u00fc\u00df\u00b7te", ",", "w\u00fcrd'", "ich", "toll", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und in der Tollheit k\u00f6nnt' ich dich verklagen:", "tokens": ["Und", "in", "der", "Toll\u00b7heit", "k\u00f6nnt'", "ich", "dich", "ver\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und diese Spottwelt ist so r\u00e4nkevoll,", "tokens": ["Und", "die\u00b7se", "Spott\u00b7welt", "ist", "so", "r\u00e4n\u00b7ke\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df tolle L\u00fcgen tollem Ohr behagen.", "tokens": ["Da\u00df", "tol\u00b7le", "L\u00fc\u00b7gen", "tol\u00b7lem", "Ohr", "be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Dies meide! flieh Verleumdung, halte stet", "tokens": ["Dies", "mei\u00b7de", "!", "flieh", "Ver\u00b7leum\u00b7dung", ",", "hal\u00b7te", "stet"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "VVFIN", "NN", "$,", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die Augen, wie auch weit dein stolzes Herz sich bl\u00e4ht.", "tokens": ["Die", "Au\u00b7gen", ",", "wie", "auch", "weit", "dein", "stol\u00b7zes", "Herz", "sich", "bl\u00e4ht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}