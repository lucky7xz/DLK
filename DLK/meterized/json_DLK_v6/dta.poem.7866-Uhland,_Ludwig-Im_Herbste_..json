{"dta.poem.7866": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbste .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Seyd gegr\u00fc\u00dft mit Fr\u00fchlingswonne,", "tokens": ["Seyd", "ge\u00b7gr\u00fc\u00dft", "mit", "Fr\u00fch\u00b7lings\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blauer Himmel, goldne Sonne!", "tokens": ["Blau\u00b7er", "Him\u00b7mel", ",", "gold\u00b7ne", "Son\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcben auch aus Gartenhallen", "tokens": ["Dr\u00fc\u00b7ben", "auch", "aus", "Gar\u00b7ten\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6r\u2019 ich frohe Saiten schallen.", "tokens": ["H\u00f6r'", "ich", "fro\u00b7he", "Sai\u00b7ten", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ahnest du, o Seele, wieder", "tokens": ["Ah\u00b7nest", "du", ",", "o", "See\u00b7le", ",", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sanfte, s\u00fc\u00dfe Fr\u00fchlingslieder?", "tokens": ["Sanf\u00b7te", ",", "s\u00fc\u00b7\u00dfe", "Fr\u00fch\u00b7lings\u00b7lie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh umher die falben B\u00e4ume!", "tokens": ["Sieh", "um\u00b7her", "die", "fal\u00b7ben", "B\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! es waren holde Tr\u00e4ume.", "tokens": ["Ach", "!", "es", "wa\u00b7ren", "hol\u00b7de", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}