{"dta.poem.13489": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n  Auf einer Burg .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Eingeschlafen auf der Lauer", "tokens": ["Ein\u00b7ge\u00b7schla\u00b7fen", "auf", "der", "Lau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oben ist der alte Ritter;", "tokens": ["O\u00b7ben", "ist", "der", "al\u00b7te", "Rit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcber gehen Regenschauer,", "tokens": ["Dr\u00fc\u00b7ber", "ge\u00b7hen", "Re\u00b7gen\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Wald rauscht durch das Gitter.", "tokens": ["Und", "der", "Wald", "rauscht", "durch", "das", "Git\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Eingewachsen Bart und Haare,", "tokens": ["Ein\u00b7ge\u00b7wach\u00b7sen", "Bart", "und", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und versteinert Brust und Krause,", "tokens": ["Und", "ver\u00b7stei\u00b7nert", "Brust", "und", "Krau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzt er viele hundert Jahre", "tokens": ["Sitzt", "er", "vie\u00b7le", "hun\u00b7dert", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oben in der stillen Klause.", "tokens": ["O\u00b7ben", "in", "der", "stil\u00b7len", "Klau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Drau\u00dfen ist es still und friedlich,", "tokens": ["Drau\u00b7\u00dfen", "ist", "es", "still", "und", "fried\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle sind in's Thal gezogen,", "tokens": ["Al\u00b7le", "sind", "in's", "Thal", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Waldesv\u00f6gel einsam singen", "tokens": ["Wal\u00b7des\u00b7v\u00f6\u00b7gel", "ein\u00b7sam", "sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den leeren Fensterbogen.", "tokens": ["In", "den", "lee\u00b7ren", "Fens\u00b7ter\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine Hochzeit f\u00e4hrt da unten", "tokens": ["Ei\u00b7ne", "Hoch\u00b7zeit", "f\u00e4hrt", "da", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Rhein im Sonnenscheine,", "tokens": ["Auf", "dem", "Rhein", "im", "Son\u00b7nen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Musikanten spielen munter,", "tokens": ["Mu\u00b7si\u00b7kan\u00b7ten", "spie\u00b7len", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die sch\u00f6ne Braut die weinet.", "tokens": ["Und", "die", "sch\u00f6\u00b7ne", "Braut", "die", "wei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}