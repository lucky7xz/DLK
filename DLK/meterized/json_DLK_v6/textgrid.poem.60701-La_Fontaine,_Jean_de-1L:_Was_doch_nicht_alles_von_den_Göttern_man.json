{"textgrid.poem.60701": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was doch nicht alles von den G\u00f6ttern man", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was doch nicht alles von den G\u00f6ttern man", "tokens": ["Was", "doch", "nicht", "al\u00b7les", "von", "den", "G\u00f6t\u00b7tern", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PTKNEG", "PIS", "APPR", "ART", "NN", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich w\u00fcnschen m\u00f6chte! Mancher geht sie an", "tokens": ["Sich", "w\u00fcn\u00b7schen", "m\u00f6ch\u00b7te", "!", "Man\u00b7cher", "geht", "sie", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "VVINF", "VMFIN", "$.", "PIS", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um Nichtigkeiten, grad als ob der Himmel", "tokens": ["Um", "Nich\u00b7tig\u00b7kei\u00b7ten", ",", "grad", "als", "ob", "der", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "$,", "ADV", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ganze sterbliche Gewimmel", "tokens": ["Das", "gan\u00b7ze", "sterb\u00b7li\u00b7che", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ohn Unterla\u00df im Auge haben m\u00fc\u00dfte", "tokens": ["Ohn", "Un\u00b7ter\u00b7la\u00df", "im", "Au\u00b7ge", "ha\u00b7ben", "m\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN", "VAINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und nichts Gescheiteres zu schaffen w\u00fc\u00dfte,", "tokens": ["Und", "nichts", "Ge\u00b7schei\u00b7te\u00b7res", "zu", "schaf\u00b7fen", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als voller Neugier auf des Kleinsten Tun zu achten,", "tokens": ["Als", "vol\u00b7ler", "Neu\u00b7gier", "auf", "des", "Kleins\u00b7ten", "Tun", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie die Olympier mit begreiflichem Gel\u00fcste", "tokens": ["Wie", "die", "O\u00b7lym\u00b7pier", "mit", "be\u00b7greif\u00b7li\u00b7chem", "Ge\u00b7l\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es einst vor Troja bei den Griechen machten.", "tokens": ["Es", "einst", "vor", "Tro\u00b7ja", "bei", "den", "Grie\u00b7chen", "mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Dummkopf ward von einem Floh gestochen,", "tokens": ["Ein", "Dumm\u00b7kopf", "ward", "von", "ei\u00b7nem", "Floh", "ge\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der unauffindbar dann sich im Gewand verkrochen.", "tokens": ["Der", "un\u00b7auf\u00b7find\u00b7bar", "dann", "sich", "im", "Ge\u00b7wand", "ver\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er rief Herkules an: \u00bbGewaltiger, r\u00fchre dich,", "tokens": ["Er", "rief", "Her\u00b7ku\u00b7les", "an", ":", "\u00bb", "Ge\u00b7wal\u00b7ti\u00b7ger", ",", "r\u00fch\u00b7re", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PTKVZ", "$.", "$(", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Welt von dieser neuen Hydra zu befrein!\u00ab", "tokens": ["Die", "Welt", "von", "die\u00b7ser", "neu\u00b7en", "Hyd\u00b7ra", "zu", "be\u00b7fr\u00b7ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und gar den Zeus: \u00bbWas tust du, Gott im Wetterschein,", "tokens": ["Und", "gar", "den", "Zeus", ":", "\u00bb", "Was", "tust", "du", ",", "Gott", "im", "Wet\u00b7ter\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NE", "$.", "$(", "PWS", "VVFIN", "PPER", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn jetzt du z\u00f6gerst?! Komm herab und r\u00e4che mich!\u00ab", "tokens": ["Wenn", "jetzt", "du", "z\u00f6\u00b7gerst", "?!", "Komm", "her\u00b7ab", "und", "r\u00e4\u00b7che", "mich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "$.", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er w\u00fcnschte, da\u00df ihm G\u00f6tter Blitz und Keule b\u00f6ten,", "tokens": ["Er", "w\u00fcnschte", ",", "da\u00df", "ihm", "G\u00f6t\u00b7ter", "Blitz", "und", "Keu\u00b7le", "b\u00f6\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Einzig um einen Floh zu t\u00f6ten.", "tokens": ["Ein\u00b7zig", "um", "ei\u00b7nen", "Floh", "zu", "t\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}