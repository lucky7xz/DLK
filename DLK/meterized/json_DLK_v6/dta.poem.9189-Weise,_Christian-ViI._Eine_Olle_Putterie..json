ine.6": {"text": "Botz tausend thut mir zorn.", "tokens": ["Botz", "tau\u00b7send", "thut", "mir", "zorn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "5. Botz tausend seht mich an/ botz tausend lachet nicht/", "tokens": ["Botz", "tau\u00b7send", "seht", "mich", "an", "/", "botz", "tau\u00b7send", "la\u00b7chet", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "VVFIN", "PPER", "PTKVZ", "$(", "APPR", "CARD", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Botz tausend stutzt es nicht/ wenn man botz tausend", "tokens": ["Botz", "tau\u00b7send", "stutzt", "es", "nicht", "/", "wenn", "man", "botz", "tau\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "CARD", "VVFIN", "PPER", "PTKNEG", "$(", "KOUS", "PIS", "APPR", "CARD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Botz tausend sack voll enden/ ", "tokens": ["Botz", "tau\u00b7send", "sack", "voll", "en\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Botz tausend mahl gelacht.", "tokens": ["Botz", "tau\u00b7send", "mahl", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Botz tausend complimenten/", "tokens": ["Botz", "tau\u00b7send", "com\u00b7pli\u00b7men\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Botz