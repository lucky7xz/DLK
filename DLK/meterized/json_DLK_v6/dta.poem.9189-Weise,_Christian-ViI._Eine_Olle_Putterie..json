: {"text": "Botz tausend nicht zu weit.", "tokens": ["Botz", "tau\u00b7send", "nicht", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. Botz tausend ist geflucht/ botz tausend ist gebet/", "tokens": ["Botz", "tau\u00b7send", "ist", "ge\u00b7flucht", "/", "botz", "tau\u00b7send", "ist", "ge\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "VVPP", "$(", "APPR", "CARD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Botz tausend geht noch hin/ botz tausend klingt noch", "tokens": ["Botz", "tau\u00b7send", "geht", "noch", "hin", "/", "botz", "tau\u00b7send", "klingt", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "CARD", "VVFIN", "ADV", "PTKVZ", "$(", "APPR", "CARD", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Botz tausend hin und wieder/ ", "tokens": ["Botz", "tau\u00b7send", "hin", "und", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "KON", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Botz tausend da und dort/", "tokens": ["Botz", "tau\u00b7send", "da", "und", "dort", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Botz tausend auff und nieder/", "tokens": ["Botz", "tau\u00b7send", "auff", "und", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "APPR", "KON", "PTKVZ", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6":