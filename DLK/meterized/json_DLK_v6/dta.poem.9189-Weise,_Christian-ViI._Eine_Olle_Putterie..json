{"dta.poem.9189": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n Eine Olle Putterie.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Botz tausend heist es nun/ botz tausend wieder was/", "tokens": ["Botz", "tau\u00b7send", "heist", "es", "nun", "/", "botz", "tau\u00b7send", "wie\u00b7der", "was", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "PPER", "ADV", "$(", "APPR", "CARD", "ADV", "PWS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Botz tausend noch einmal/ botz tausend wz ist das/", "tokens": ["Botz", "tau\u00b7send", "noch", "ein\u00b7mal", "/", "botz", "tau\u00b7send", "wz", "ist", "das", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADV", "ADV", "$(", "APPR", "CARD", "NN", "VAFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Botz tausend heut und morgen/ botz tausend im\u0303erdar/", "tokens": ["Botz", "tau\u00b7send", "heut", "und", "mor\u00b7gen", "/", "botz", "tau\u00b7send", "i\u00b7m\u0303er\u00b7dar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADV", "KON", "ADV", "$(", "FM", "FM", "FM", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Botz tausend ohne s\u00f6rgen/ botz tausend guter jahr.", "tokens": ["Botz", "tau\u00b7send", "oh\u00b7ne", "s\u00f6r\u00b7gen", "/", "botz", "tau\u00b7send", "gu\u00b7ter", "jahr", "."], "token_info": ["word", "word", "word