{"textgrid.poem.58499": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "537. Kindlein komm, sei nicht dumm!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gr\u00e4mst dich hin und her, bleibst doch in dir elendig,", "tokens": ["Du", "gr\u00e4mst", "dich", "hin", "und", "her", ",", "bleibst", "doch", "in", "dir", "e\u00b7len\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,", "VVFIN", "ADV", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Glaub's, Gottes Mutterherz ist dir ganz nah inwendig;", "tokens": ["Glaub's", ",", "Got\u00b7tes", "Mut\u00b7ter\u00b7herz", "ist", "dir", "ganz", "nah", "in\u00b7wen\u00b7dig", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "NN", "VAFIN", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Neig s\u00fc\u00df und sanft hinein, kehr dich nur nicht an Jammer,", "tokens": ["Neig", "s\u00fc\u00df", "und", "sanft", "hin\u00b7ein", ",", "kehr", "dich", "nur", "nicht", "an", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "PTKVZ", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Mutter h\u00f6rt's und sieht's auch bei verschloss'ner Kammer!", "tokens": ["Die", "Mut\u00b7ter", "h\u00f6rt's", "und", "sieht's", "auch", "bei", "ver\u00b7schloss'\u00b7ner", "Kam\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NE", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}