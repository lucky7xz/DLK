{"textgrid.poem.48547": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. Auf des 8. Psalms Melodei", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wilst du erst itzt, o Seele, dich beschweren,", "tokens": ["Wilst", "du", "erst", "itzt", ",", "o", "See\u00b7le", ",", "dich", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "$,", "FM", "NN", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df deinen Leib die Erde soll verzehren?", "tokens": ["da\u00df", "dei\u00b7nen", "Leib", "die", "Er\u00b7de", "soll", "ver\u00b7zeh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bef\u00e4llt dich erst die Furcht des Todes itzt,", "tokens": ["Be\u00b7f\u00e4llt", "dich", "erst", "die", "Furcht", "des", "To\u00b7des", "itzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "da er erbla\u00dft den Todesschwei\u00df schon schwitzt?", "tokens": ["da", "er", "er\u00b7bla\u00dft", "den", "To\u00b7des\u00b7schwei\u00df", "schon", "schwitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sein Ende war ihm da schon auserkoren,", "tokens": ["Sein", "En\u00b7de", "war", "ihm", "da", "schon", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "eh' als ihm noch sein Anfang war geboren;", "tokens": ["eh'", "als", "ihm", "noch", "sein", "An\u00b7fang", "war", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das war der Bund: du soltest wieder fort,", "tokens": ["das", "war", "der", "Bund", ":", "du", "sol\u00b7test", "wie\u00b7der", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "PPER", "VMFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "so balde dir dein Gott rief zu ein Wort.", "tokens": ["so", "bal\u00b7de", "dir", "dein", "Gott", "rief", "zu", "ein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er hat ein Ziel gestecket allen Sachen,", "tokens": ["Er", "hat", "ein", "Ziel", "ge\u00b7ste\u00b7cket", "al\u00b7len", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "er wird in dem ihm nichts Besonders machen;", "tokens": ["er", "wird", "in", "dem", "ihm", "nichts", "Be\u00b7son\u00b7ders", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PRELS", "PPER", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "spricht er, so mu\u00df die Welt auch untergehn:", "tokens": ["spricht", "er", ",", "so", "mu\u00df", "die", "Welt", "auch", "un\u00b7ter\u00b7gehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und er vermeint ihm ewig zu bestehn?", "tokens": ["und", "er", "ver\u00b7meint", "ihm", "e\u00b7wig", "zu", "be\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie kanst du dir, was unverweslich, hoffen?", "tokens": ["Wie", "kanst", "du", "dir", ",", "was", "un\u00b7ver\u00b7wes\u00b7lich", ",", "hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "$,", "PRELS", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein schwacher Leib steht allen Toden offen:", "tokens": ["Dein", "schwa\u00b7cher", "Leib", "steht", "al\u00b7len", "To\u00b7den", "of\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "tuts einer nicht, so stehn hier zehen noch,", "tokens": ["tuts", "ei\u00b7ner", "nicht", ",", "so", "stehn", "hier", "ze\u00b7hen", "noch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PTKNEG", "$,", "ADV", "VVFIN", "ADV", "CARD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die stark sind satt zu sto\u00dfen ihn ins Loch.", "tokens": ["die", "stark", "sind", "satt", "zu", "sto\u00b7\u00dfen", "ihn", "ins", "Loch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADJD", "PTKZU", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Schmerzt dieses dich, da\u00df er so viel ertragen,", "tokens": ["Schmerzt", "die\u00b7ses", "dich", ",", "da\u00df", "er", "so", "viel", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df er von nichts als Leide wei\u00df zu sagen,", "tokens": ["da\u00df", "er", "von", "nichts", "als", "Lei\u00b7de", "wei\u00df", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "KOKOM", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so kehr in dich und frag dich selbst um Rat,", "tokens": ["so", "kehr", "in", "dich", "und", "frag", "dich", "selbst", "um", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "KON", "VVIMP", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ob sein Gericht' in dem denn Unrecht hat!", "tokens": ["ob", "sein", "Ge\u00b7richt'", "in", "dem", "denn", "Un\u00b7recht", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Schau Alles an, worauf ein Herze schauet,", "tokens": ["Schau", "Al\u00b7les", "an", ",", "wo\u00b7rauf", "ein", "Her\u00b7ze", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "das mehr auf Schein als wahre Sch\u00f6nheit trauet,", "tokens": ["das", "mehr", "auf", "Schein", "als", "wah\u00b7re", "Sch\u00f6n\u00b7heit", "trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kunst, Ehre, Lust, Verm\u00f6gen und fortan:", "tokens": ["Kunst", ",", "Eh\u00b7re", ",", "Lust", ",", "Ver\u00b7m\u00f6\u00b7gen", "und", "for\u00b7tan", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ist alles di\u00df auch mehr als nur ein Wahn?", "tokens": ["ist", "al\u00b7les", "di\u00df", "auch", "mehr", "als", "nur", "ein", "Wahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PDS", "ADV", "PIAT", "KOKOM", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das solte dich was H\u00f6hers unterweisen?", "tokens": ["Das", "sol\u00b7te", "dich", "was", "H\u00f6\u00b7hers", "un\u00b7ter\u00b7wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "PWS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So \u00fcberhoch war solches nicht zu preisen,", "tokens": ["So", "\u00fc\u00b7ber\u00b7hoch", "war", "sol\u00b7ches", "nicht", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIS", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df leichter Wind sich scheuet des Gewichts,", "tokens": ["da\u00df", "leich\u00b7ter", "Wind", "sich", "scheu\u00b7et", "des", "Ge\u00b7wichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und selbst sich hei\u00dft noch weniger als nichts.", "tokens": ["und", "selbst", "sich", "hei\u00dft", "noch", "we\u00b7ni\u00b7ger", "als", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "VVFIN", "ADV", "PIS", "KOKOM", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ich bin gewi\u00df, da\u00df meine Seele lebet,", "tokens": ["Ich", "bin", "ge\u00b7wi\u00df", ",", "da\u00df", "mei\u00b7ne", "See\u00b7le", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wenn mir mehr nichts an diesem Leibe webet;", "tokens": ["wenn", "mir", "mehr", "nichts", "an", "die\u00b7sem", "Lei\u00b7be", "we\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "kein Geist verwest, als den der Himmel gab;", "tokens": ["kein", "Geist", "ver\u00b7west", ",", "als", "den", "der", "Him\u00b7mel", "gab", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KOUS", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "sein \u00dcberzug, der Leib, der mu\u00df ins Grab.", "tokens": ["sein", "\u00dc\u00b7ber\u00b7zug", ",", "der", "Leib", ",", "der", "mu\u00df", "ins", "Grab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "ART", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Auf di\u00df sei froh, da\u00df, da du warst verfluchet,", "tokens": ["Auf", "di\u00df", "sei", "froh", ",", "da\u00df", ",", "da", "du", "warst", "ver\u00b7flu\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "ADJD", "$,", "KOUS", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dein Heiland dir den Segen hat gesuchet,", "tokens": ["dein", "Hei\u00b7land", "dir", "den", "Se\u00b7gen", "hat", "ge\u00b7su\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df, da du schon wie warst ein Hellenbrand,", "tokens": ["da\u00df", ",", "da", "du", "schon", "wie", "warst", "ein", "Hel\u00b7len\u00b7brand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "KOKOM", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der Himmel dir durch Gott war zuerkant!", "tokens": ["der", "Him\u00b7mel", "dir", "durch", "Gott", "war", "zu\u00b7er\u00b7kant", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ie mehr du hier vor Schmerzen must verbei\u00dfen,", "tokens": ["Ie", "mehr", "du", "hier", "vor", "Schmer\u00b7zen", "must", "ver\u00b7bei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ie mehr du dort ein Freudenkind wirst hei\u00dfen;", "tokens": ["ie", "mehr", "du", "dort", "ein", "Freu\u00b7den\u00b7kind", "wirst", "hei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wei\u00df dir die Welt nichts anzutun als Leid,", "tokens": ["wei\u00df", "dir", "die", "Welt", "nichts", "an\u00b7zu\u00b7tun", "als", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PIS", "VVIZU", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gott hat Lust gnung f\u00fcr dich in jener Zeit.", "tokens": ["Gott", "hat", "Lust", "gnung", "f\u00fcr", "dich", "in", "je\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "ADV", "APPR", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.11": {"line.1": {"text": "Hab alle Welt, hab alle Macht zu Feinde,", "tokens": ["Hab", "al\u00b7le", "Welt", ",", "hab", "al\u00b7le", "Macht", "zu", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "VAFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "es schadet nichts, hastu nur ihn zum Freunde;", "tokens": ["es", "scha\u00b7det", "nichts", ",", "has\u00b7tu", "nur", "ihn", "zum", "Freun\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "VAFIN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "es wird dein Fall dich dennoch sehen stehn,", "tokens": ["es", "wird", "dein", "Fall", "dich", "den\u00b7noch", "se\u00b7hen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wenn er durch sich wird einst zu Grunde gehn.", "tokens": ["wenn", "er", "durch", "sich", "wird", "einst", "zu", "Grun\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "VAFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Trit nur hervor und beichte deine Schulden!", "tokens": ["Trit", "nur", "her\u00b7vor", "und", "beich\u00b7te", "dei\u00b7ne", "Schul\u00b7den", "!"], "token_info": ["word", "word", "word",