{"textgrid.poem.59068": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "287. Probe des reinen Lichts", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das reine Licht hei\u00dft flei\u00dfig sein", "tokens": ["Das", "rei\u00b7ne", "Licht", "hei\u00dft", "flei\u00b7\u00dfig", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch auf seinen Flei\u00df nicht bauen,", "tokens": ["Und", "doch", "auf", "sei\u00b7nen", "Flei\u00df", "nicht", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ohn' Triftigkeit, ohn' Sorg' und Pein", "tokens": ["Ohn'", "Trif\u00b7tig\u00b7keit", ",", "ohn'", "Sor\u00b7g'", "und", "Pein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den Ausgang kindlich Gott vertrauen.", "tokens": ["Den", "Aus\u00b7gang", "kind\u00b7lich", "Gott", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}