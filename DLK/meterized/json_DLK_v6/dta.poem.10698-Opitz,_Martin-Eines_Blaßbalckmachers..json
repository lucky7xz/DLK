{"dta.poem.10698": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Eines Bla\u00dfbalckmachers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Olieber Mensch/ dein Leben ja betrachte/", "tokens": ["O\u00b7lie\u00b7ber", "Mensch", "/", "dein", "Le\u00b7ben", "ja", "be\u00b7trach\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier liget/ der die Bla\u00dfeb\u00e4lge machte/", "tokens": ["Hier", "li\u00b7get", "/", "der", "die", "Bla\u00b7\u00dfe\u00b7b\u00e4l\u00b7ge", "mach\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt aber nun zuletzt es doch sich findt/", "tokens": ["Jetzt", "a\u00b7ber", "nun", "zu\u00b7letzt", "es", "doch", "sich", "findt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "PPER", "ADV", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Meister/ Schau/ gebrist noch selber Windt.", "tokens": ["Dem", "Meis\u00b7ter", "/", "Schau", "/", "ge\u00b7brist", "noch", "sel\u00b7ber", "Windt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "VVFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}