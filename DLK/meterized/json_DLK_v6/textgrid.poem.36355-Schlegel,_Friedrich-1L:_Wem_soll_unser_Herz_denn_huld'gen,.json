{"textgrid.poem.36355": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wem soll unser Herz denn huld'gen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem soll unser Herz denn huld'gen,", "tokens": ["Wem", "soll", "un\u00b7ser", "Herz", "denn", "huld'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer errettet uns die Welt?", "tokens": ["Wer", "er\u00b7ret\u00b7tet", "uns", "die", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon vergehn die S\u00fcndenschuld'gen,", "tokens": ["Schon", "ver\u00b7gehn", "die", "S\u00fcn\u00b7den\u00b7schuld'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber wann erscheint der Held?", "tokens": ["A\u00b7ber", "wann", "er\u00b7scheint", "der", "Held", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoffnung zeigt sich schon von weitem,", "tokens": ["Hoff\u00b7nung", "zeigt", "sich", "schon", "von", "wei\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn der Glaube nur nicht sinkt;", "tokens": ["Wenn", "der", "Glau\u00b7be", "nur", "nicht", "sinkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00dft den Schwur uns ihm bereiten,", "tokens": ["La\u00dft", "den", "Schwur", "uns", "ihm", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der die Feinde einst bezwingt.", "tokens": ["Der", "die", "Fein\u00b7de", "einst", "be\u00b7zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einmal wird das Gl\u00fcck sich wenden,", "tokens": ["Ein\u00b7mal", "wird", "das", "Gl\u00fcck", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnelle wankt des B\u00f6sen Macht,", "tokens": ["Schnel\u00b7le", "wankt", "des", "B\u00f6\u00b7sen", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einmal mu\u00df der Greuel enden,", "tokens": ["Ein\u00b7mal", "mu\u00df", "der", "Greu\u00b7el", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht immer bleibt es Nacht.", "tokens": ["Und", "nicht", "im\u00b7mer", "bleibt", "es", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsrer Ahnen alte Kunde", "tokens": ["Uns\u00b7rer", "Ah\u00b7nen", "al\u00b7te", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist es, was mir Hoffnung gibt;", "tokens": ["Ist", "es", ",", "was", "mir", "Hoff\u00b7nung", "gibt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PWS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wann, belehrt in treuem Bunde,", "tokens": ["Wann", ",", "be\u00b7lehrt", "in", "treu\u00b7em", "Bun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Man das Alte wieder liebt.", "tokens": ["Man", "das", "Al\u00b7te", "wie\u00b7der", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schm\u00e4hlich, zu der R\u00f6mer Tagen,", "tokens": ["Schm\u00e4h\u00b7lich", ",", "zu", "der", "R\u00f6\u00b7mer", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sank die sch\u00f6ne Freiheit hin;", "tokens": ["Sank", "die", "sch\u00f6\u00b7ne", "Frei\u00b7heit", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutschland mu\u00dfte Fesseln tragen,", "tokens": ["Deutschland", "mu\u00df\u00b7te", "Fes\u00b7seln", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NN", "VVINF", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Doch es blieb der mut'ge Sinn.", "tokens": ["Doch", "es", "blieb", "der", "mut'\u00b7ge", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus des Vaterlandes W\u00e4ldern", "tokens": ["Aus", "des", "Va\u00b7ter\u00b7lan\u00b7des", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drang der Helden Schar hervor,", "tokens": ["Drang", "der", "Hel\u00b7den", "Schar", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo wir wandeln, auf den Feldern,", "tokens": ["Wo", "wir", "wan\u00b7deln", ",", "auf", "den", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "War des Ruhmes h\u00f6chster Flor.", "tokens": ["War", "des", "Ruh\u00b7mes", "h\u00f6chs\u00b7ter", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Als durch B\u00fcrgerkrieg im Reiche", "tokens": ["Als", "durch", "B\u00fcr\u00b7ger\u00b7krieg", "im", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "APPRART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses edle Volk zerf\u00e4llt,", "tokens": ["Die\u00b7ses", "ed\u00b7le", "Volk", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter eignem, grimm'gem Streiche", "tokens": ["Un\u00b7ter", "eig\u00b7nem", ",", "grim\u00b7m'\u00b7gem", "Strei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sich zerst\u00f6rt die deutsche Welt;", "tokens": ["Sich", "zer\u00b7st\u00f6rt", "die", "deut\u00b7sche", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja auch da noch bl\u00fcht die Ehre,", "tokens": ["Ja", "auch", "da", "noch", "bl\u00fcht", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Und es geht vom Kaiserhaus,", "tokens": ["Und", "es", "geht", "vom", "Kai\u00b7ser\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie die Zwietracht sich vermehre,", "tokens": ["Wie", "die", "Zwiet\u00b7racht", "sich", "ver\u00b7meh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mancher hohe Retter aus.", "tokens": ["Man\u00b7cher", "ho\u00b7he", "Ret\u00b7ter", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Rudolf, dessen feste Tugend", "tokens": ["Ru\u00b7dolf", ",", "des\u00b7sen", "fes\u00b7te", "Tu\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenkt die Welt zum Recht zur\u00fcck;", "tokens": ["Lenkt", "die", "Welt", "zum", "Recht", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jener Ritter, dessen Jugend", "tokens": ["Je\u00b7ner", "Rit\u00b7ter", ",", "des\u00b7sen", "Ju\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reich umg\u00fcrtete das Gl\u00fcck;", "tokens": ["Reich", "um\u00b7g\u00fcr\u00b7te\u00b7te", "das", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch vor allen unersch\u00fcttert", "tokens": ["Doch", "vor", "al\u00b7len", "un\u00b7er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ferdinandes hoher Mut,", "tokens": ["Fer\u00b7di\u00b7nan\u00b7des", "ho\u00b7her", "Mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In dem wildsten Kriegsgewitter", "tokens": ["In", "dem", "wilds\u00b7ten", "Kriegs\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alten Glaubens Schirm und Hut.", "tokens": ["Al\u00b7ten", "Glau\u00b7bens", "Schirm", "und", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jetzt noch leben Heldensprossen", "tokens": ["Jetzt", "noch", "le\u00b7ben", "Hel\u00b7den\u00b7spros\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem heiligen Geschlecht,", "tokens": ["Von", "dem", "hei\u00b7li\u00b7gen", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das, so oft auch Blut geflossen,", "tokens": ["Das", ",", "so", "oft", "auch", "Blut", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "ADV", "ADV", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiederbrachte Fried' und Recht.", "tokens": ["Wie\u00b7der\u00b7brach\u00b7te", "Fried'", "und", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum, bis zu den letzten Tagen,", "tokens": ["Drum", ",", "bis", "zu", "den", "letz\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wachse dieses Adlers Kraft!", "tokens": ["Wach\u00b7se", "die\u00b7ses", "Ad\u00b7lers", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles la\u00dft f\u00fcr die uns wagen,", "tokens": ["Al\u00b7les", "la\u00dft", "f\u00fcr", "die", "uns", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die bis jetzt uns Heil geschafft.", "tokens": ["Die", "bis", "jetzt", "uns", "Heil", "ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PPER", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Auch noch andre St\u00e4mme gr\u00fcnen", "tokens": ["Auch", "noch", "and\u00b7re", "St\u00e4m\u00b7me", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des alten Ruhmes Wald;", "tokens": ["Von", "des", "al\u00b7ten", "Ruh\u00b7mes", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcrsten sind die Frei' und K\u00fchnen", "tokens": ["F\u00fcrs\u00b7ten", "sind", "die", "Frei'", "und", "K\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Edel ist des Muts Gewalt.", "tokens": ["E\u00b7del", "ist", "des", "Muts", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer uns rettet von dem Feinde,", "tokens": ["Wer", "uns", "ret\u00b7tet", "von", "dem", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wann die Schulden all' geb\u00fc\u00dft,", "tokens": ["Wann", "die", "Schul\u00b7den", "all'", "ge\u00b7b\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wer die lang Getrennten einte,", "tokens": ["Wer", "die", "lang", "Ge\u00b7trenn\u00b7ten", "ein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei als K\u00f6nig uns begr\u00fc\u00dft.", "tokens": ["Sei", "als", "K\u00f6\u00b7nig", "uns", "be\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}