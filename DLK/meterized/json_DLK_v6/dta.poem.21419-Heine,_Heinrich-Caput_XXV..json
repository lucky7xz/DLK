{"dta.poem.21419": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eauch die Censur ist nicht mehr streng,", "tokens": ["\u201e", "auch", "die", "Cen\u00b7sur", "ist", "nicht", "mehr", "streng", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Hoffmann wird \u00e4lter und milder,", "tokens": ["Hoff\u00b7mann", "wird", "\u00e4l\u00b7ter", "und", "mil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und streicht nicht mehr mit Jugendzorn", "tokens": ["Und", "streicht", "nicht", "mehr", "mit", "Ju\u00b7gend\u00b7zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir deine Reisebilder.", "tokens": ["Dir", "dei\u00b7ne", "Rei\u00b7se\u00b7bil\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201edu selbst bist \u00e4lter und milder jetzt,", "tokens": ["\u201e", "du", "selbst", "bist", "\u00e4l\u00b7ter", "und", "mil\u00b7der", "jetzt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wirst dich in manches schicken,", "tokens": ["Wirst", "dich", "in", "man\u00b7ches", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIS", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und wirst sogar die Vergangenheit", "tokens": ["Und", "wirst", "so\u00b7gar", "die", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In besserem Lichte erblicken.", "tokens": ["In", "bes\u00b7se\u00b7rem", "Lich\u00b7te", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "\u201eja, da\u00df es uns fr\u00fcher so schrecklich ging,", "tokens": ["\u201e", "ja", ",", "da\u00df", "es", "uns", "fr\u00fc\u00b7her", "so", "schreck\u00b7lich", "ging", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "KOUS", "PPER", "PRF", "ADJD", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Deutschland, ist Uebertreibung;", "tokens": ["In", "Deutschland", ",", "ist", "Ue\u00b7bert\u00b7rei\u00b7bung", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man konnte entrinnen der Knechtschaft, wie einst", "tokens": ["Man", "konn\u00b7te", "ent\u00b7rin\u00b7nen", "der", "Knecht\u00b7schaft", ",", "wie", "einst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VMFIN", "VVFIN", "ART", "NN", "$,", "PWAV", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "In Rom, durch Selbstentleibung.", "tokens": ["In", "Rom", ",", "durch", "Selbs\u00b7tent\u00b7lei\u00b7bung", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201egedankenfreiheit geno\u00df das Volk,", "tokens": ["\u201e", "ge\u00b7dan\u00b7ken\u00b7frei\u00b7heit", "ge\u00b7no\u00df", "das", "Volk", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie war f\u00fcr die gro\u00dfen Massen,", "tokens": ["Sie", "war", "f\u00fcr", "die", "gro\u00b7\u00dfen", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Beschr\u00e4nkung traf nur die g\u2019ringe Zahl", "tokens": ["Be\u00b7schr\u00e4n\u00b7kung", "traf", "nur", "die", "g'\u00b7rin\u00b7ge", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Derjen\u2019gen, die drucken lassen.", "tokens": ["Der\u00b7jen'\u00b7gen", ",", "die", "dru\u00b7cken", "las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "VVINF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}