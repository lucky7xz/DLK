{"textgrid.poem.46403": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An einen einf\u00e4ltigen Helden", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es werden deine Heldenthaten,", "tokens": ["Es", "wer\u00b7den", "dei\u00b7ne", "Hel\u00b7den\u00b7tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Einst in die lange Nacht gerathen,", "tokens": ["Einst", "in", "die", "lan\u00b7ge", "Nacht", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Muse sieht und r\u00fchmt sie nicht;", "tokens": ["Die", "Mu\u00b7se", "sieht", "und", "r\u00fchmt", "sie", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast des L\u00f6wen Muth und St\u00e4rke,", "tokens": ["Du", "hast", "des", "L\u00f6\u00b7wen", "Muth", "und", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Cesar thust du Wunderwerke,", "tokens": ["Wie", "Ce\u00b7sar", "thust", "du", "Wun\u00b7der\u00b7wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur, da\u00df dir Cesars Kopf gebricht!", "tokens": ["Nur", ",", "da\u00df", "dir", "Ce\u00b7sars", "Kopf", "ge\u00b7bricht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}