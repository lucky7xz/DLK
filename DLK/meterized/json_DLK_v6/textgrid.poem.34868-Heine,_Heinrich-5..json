{"textgrid.poem.34868": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir seufzen nicht, das Aug' ist trocken,", "tokens": ["Wir", "seuf\u00b7zen", "nicht", ",", "das", "Aug'", "ist", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir l\u00e4cheln oft, wir lachen gar!", "tokens": ["Wir", "l\u00e4\u00b7cheln", "oft", ",", "wir", "la\u00b7chen", "gar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In keinem Blick, in keiner Miene,", "tokens": ["In", "kei\u00b7nem", "Blick", ",", "in", "kei\u00b7ner", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird das Geheimnis offenbar.", "tokens": ["Wird", "das", "Ge\u00b7heim\u00b7nis", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit seinen stummen Qualen liegt es", "tokens": ["Mit", "sei\u00b7nen", "stum\u00b7men", "Qua\u00b7len", "liegt", "es"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In unsrer Seele blut'gem Grund;", "tokens": ["In", "uns\u00b7rer", "See\u00b7le", "blut'\u00b7gem", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird es auch laut im wilden Herzen,", "tokens": ["Wird", "es", "auch", "laut", "im", "wil\u00b7den", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Krampfhaft verschlossen bleibt der Mund.", "tokens": ["Krampf\u00b7haft", "ver\u00b7schlos\u00b7sen", "bleibt", "der", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Frag du den S\u00e4ugling in der Wiege,", "tokens": ["Frag", "du", "den", "S\u00e4ug\u00b7ling", "in", "der", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Frag du die Toten in dem Grab,", "tokens": ["Frag", "du", "die", "To\u00b7ten", "in", "dem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht da\u00df diese dir entdecken,", "tokens": ["Viel\u00b7leicht", "da\u00df", "die\u00b7se", "dir", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PDAT", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ich dir stets verschwiegen hab.", "tokens": ["Was", "ich", "dir", "stets", "ver\u00b7schwie\u00b7gen", "hab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}