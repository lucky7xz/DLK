{"dta.poem.7456": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XiII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O lobt Euch nur des Westes Schmeichelwehen,", "tokens": ["O", "lobt", "Euch", "nur", "des", "Wes\u00b7tes", "Schmei\u00b7chel\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn kr\u00e4uselnd er ob blauen Fl\u00e4chen zittert", "tokens": ["Wenn", "kr\u00e4u\u00b7selnd", "er", "ob", "blau\u00b7en", "Fl\u00e4\u00b7chen", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "KOUS", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kaum dem Schilf ein welkes Blatt zerknittert \u2014", "tokens": ["Und", "kaum", "dem", "Schilf", "ein", "wel\u00b7kes", "Blatt", "zer\u00b7knit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr stillen Seelen, m\u00f6g's Euch wohl ergehen!", "tokens": ["Ihr", "stil\u00b7len", "See\u00b7len", ",", "m\u00f6g's", "Euch", "wohl", "er\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich aber mu\u00df das Meer im Sturme sehen,", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "das", "Meer", "im", "Stur\u00b7me", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Segel rei\u00dfen, wenn der Mast zersplittert,", "tokens": ["Wenn", "Se\u00b7gel", "rei\u00b7\u00dfen", ",", "wenn", "der", "Mast", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn's in mir, um mich, \u00fcber mir gewittert,", "tokens": ["Wenn's", "in", "mir", ",", "um", "mich", ",", "\u00fc\u00b7ber", "mir", "ge\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "$,", "KOUI", "PPER", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Luft und Wasser hell im Brande stehen.", "tokens": ["Wenn", "Luft", "und", "Was\u00b7ser", "hell", "im", "Bran\u00b7de", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr m\u00f6gt ein ungleich gr\u00f6\u00dfer Gl\u00fcck erfahren,", "tokens": ["Ihr", "m\u00f6gt", "ein", "un\u00b7gleich", "gr\u00f6\u00b7\u00dfer", "Gl\u00fcck", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Eure Gluten lange schon verlodert,", "tokens": ["Da\u00df", "Eu\u00b7re", "Glu\u00b7ten", "lan\u00b7ge", "schon", "ver\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh' Euer Leib im Schoos der Erde modert.", "tokens": ["Eh'", "Eu\u00b7er", "Leib", "im", "Schoos", "der", "Er\u00b7de", "mo\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich werd' nun einmal wilder mit den Jahren,", "tokens": ["Ich", "werd'", "nun", "ein\u00b7mal", "wil\u00b7der", "mit", "den", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}