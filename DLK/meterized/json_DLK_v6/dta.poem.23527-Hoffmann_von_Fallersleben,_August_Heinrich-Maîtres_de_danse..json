{"dta.poem.23527": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ma\u00eetres de danse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, es war ein tolles Tanzen,", "tokens": ["Ja", ",", "es", "war", "ein", "tol\u00b7les", "Tan\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Rast und ohne Ruh;", "tokens": ["Oh\u00b7ne", "Rast", "und", "oh\u00b7ne", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den W\u00e4llen, aus den Schanzen", "tokens": ["Von", "den", "W\u00e4l\u00b7len", ",", "aus", "den", "Schan\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tanzten sie nach Frankreich zu.", "tokens": ["Tanz\u00b7ten", "sie", "nach", "Fran\u00b7kreich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welche Schmach f\u00fcr eure V\u00e4ter,", "tokens": ["Wel\u00b7che", "Schmach", "f\u00fcr", "eu\u00b7re", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie dumm und wie verkehrt,", "tokens": ["O", "wie", "dumm", "und", "wie", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "KON", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ihr lernt von Frankreichs ", "tokens": ["Da\u00df", "ihr", "lernt", "von", "Fran\u00b7kreichs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Was wir selber sie gelehrt!", "tokens": ["Was", "wir", "sel\u00b7ber", "sie", "ge\u00b7lehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Pfui! welche Schmach und Schande,", "tokens": ["Pfui", "!", "wel\u00b7che", "Schmach", "und", "Schan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAT", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "O wie dumm und wie verkehrt,", "tokens": ["O", "wie", "dumm", "und", "wie", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "KON", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ihr lernt die ", "tokens": ["Da\u00df", "ihr", "lernt", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Die wir selber sie gelehrt!", "tokens": ["Die", "wir", "sel\u00b7ber", "sie", "ge\u00b7lehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sparet euren Flei\u00df und Eifer,", "tokens": ["Spa\u00b7ret", "eu\u00b7ren", "Flei\u00df", "und", "Ei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis der Feind uns kommt ins Haus,", "tokens": ["Bis", "der", "Feind", "uns", "kommt", "ins", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tanzt mit ihm dann einen Schleifer", "tokens": ["Tanzt", "mit", "ihm", "dann", "ei\u00b7nen", "Schlei\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hopsasa! zum Land' hinaus!", "tokens": ["Hop\u00b7sa\u00b7sa", "!", "zum", "Land'", "hin\u00b7aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPRART", "NN", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}}}}