{"textgrid.poem.56789": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Schlaf schickt seine Scharen in die Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schlaf schickt seine Scharen in die Nacht,", "tokens": ["Der", "Schlaf", "schickt", "sei\u00b7ne", "Scha\u00b7ren", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unholde, Legionen auf Legion ...", "tokens": ["Un\u00b7hol\u00b7de", ",", "Le\u00b7gi\u00b7o\u00b7nen", "auf", "Le\u00b7gi\u00b7on", "..."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom R\u00fccken schleichen sie ihr Opfer an,", "tokens": ["Vom", "R\u00fc\u00b7cken", "schlei\u00b7chen", "sie", "ihr", "Op\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "auf leisen Tatzen, und umarmen es,", "tokens": ["auf", "lei\u00b7sen", "Tat\u00b7zen", ",", "und", "um\u00b7ar\u00b7men", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "wie B\u00e4ren, unentrinnbar und ger\u00e4uschlos, \u2013", "tokens": ["wie", "B\u00e4\u00b7ren", ",", "un\u00b7en\u00b7trinn\u00b7bar", "und", "ge\u00b7r\u00e4uschlos", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "bis alle Muskeln ihm erschlafft, und stumm", "tokens": ["bis", "al\u00b7le", "Mus\u00b7keln", "ihm", "er\u00b7schlafft", ",", "und", "stumm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "von ihrer Brust der Leib zu Boden rollt ...", "tokens": ["von", "ih\u00b7rer", "Brust", "der", "Leib", "zu", "Bo\u00b7den", "rollt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und wenn so alles hingebettet liegt,", "tokens": ["Und", "wenn", "so", "al\u00b7les", "hin\u00b7ge\u00b7bet\u00b7tet", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "so traben sie zu ihrem Herrn zur\u00fcck,", "tokens": ["so", "tra\u00b7ben", "sie", "zu", "ih\u00b7rem", "Herrn", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "und ihr Gebrumm erf\u00fcllt wie dumpfer Donner", "tokens": ["und", "ihr", "Ge\u00b7brumm", "er\u00b7f\u00fcllt", "wie", "dum\u00b7pfer", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "die d\u00fcstren Waldgebirge seines Reichs.", "tokens": ["die", "d\u00fcst\u00b7ren", "Wald\u00b7ge\u00b7bir\u00b7ge", "sei\u00b7nes", "Reichs", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}