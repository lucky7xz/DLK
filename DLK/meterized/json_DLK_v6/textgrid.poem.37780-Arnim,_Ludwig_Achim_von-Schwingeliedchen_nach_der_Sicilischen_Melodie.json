{"textgrid.poem.37780": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Schwingeliedchen nach der Sicilischen Melodie", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werf ich ab den Kittel", "tokens": ["Werf", "ich", "ab", "den", "Kit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "M\u00e4dchen schaue an", "tokens": ["M\u00e4d\u00b7chen", "schau\u00b7e", "an"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie ich ohne Knittel", "tokens": ["Wie", "ich", "oh\u00b7ne", "Knit\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "M\u00e4nner schlagen kann:", "tokens": ["M\u00e4n\u00b7ner", "schla\u00b7gen", "kann", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schau die Sehnen schwellen", "tokens": ["Schau", "die", "Seh\u00b7nen", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "An der Schenkel Muth,", "tokens": ["An", "der", "Schen\u00b7kel", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "Wie zur Erde quellen", "tokens": ["Wie", "zur", "Er\u00b7de", "quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Sie in fester Wuth.", "tokens": ["Sie", "in", "fes\u00b7ter", "Wuth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Singend schwinget schlinget", "tokens": ["Sin\u00b7gend", "schwin\u00b7get", "schlin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ringet Brust an Brust,", "tokens": ["Rin\u00b7get", "Brust", "an", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wer den andern zwinget", "tokens": ["Wer", "den", "an\u00b7dern", "zwin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "F\u00fchlet frei die Lust", "tokens": ["F\u00fch\u00b7let", "frei", "die", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Denn wer hingestrecket", "tokens": ["Denn", "wer", "hin\u00b7ge\u00b7stre\u00b7cket"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "VVFIN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Reichet ihm den Kranz,", "tokens": ["Rei\u00b7chet", "ihm", "den", "Kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Eichengr\u00fcn bedecket,", "tokens": ["Ei\u00b7chen\u00b7gr\u00fcn", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Seiner Stirne Glanz.", "tokens": ["Sei\u00b7ner", "Stir\u00b7ne", "Glanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Welchem M\u00e4dchen bringet", "tokens": ["Wel\u00b7chem", "M\u00e4d\u00b7chen", "brin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Er den Siegerkranz,", "tokens": ["Er", "den", "Sie\u00b7ger\u00b7kranz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Jedes ihn umschlinget", "tokens": ["Je\u00b7des", "ihn", "um\u00b7schlin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["PIS", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Froh zum Schlingetanz:", "tokens": ["Froh", "zum", "Schlin\u00b7ge\u00b7tanz", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dreht ihn schnell im Kreise", "tokens": ["Dreht", "ihn", "schnell", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ringt im schnellen Sprung,", "tokens": ["Ringt", "im", "schnel\u00b7len", "Sprung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sucht nach M\u00e4dchenweise", "tokens": ["Sucht", "nach", "M\u00e4d\u00b7chen\u00b7wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Sieg in Tanzesschwung.", "tokens": ["Sieg", "in", "Tan\u00b7zes\u00b7schwung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und wer heut' besieget", "tokens": ["Und", "wer", "heut'", "be\u00b7sie\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Suchet morgen Sieg,", "tokens": ["Su\u00b7chet", "mor\u00b7gen", "Sieg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Keiner stets erlieget", "tokens": ["Kei\u00b7ner", "stets", "er\u00b7lie\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ADV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "David kr\u00f6nte Sieg:", "tokens": ["Da\u00b7vid", "kr\u00f6n\u00b7te", "Sieg", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schwachen hilft die Schnelle,", "tokens": ["Schwa\u00b7chen", "hilft", "die", "Schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dauer giebt der Muth;", "tokens": ["Dau\u00b7er", "giebt", "der", "Muth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Schweizer von der Stelle", "tokens": ["Schwei\u00b7zer", "von", "der", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Nimmer weich' uns Blut.", "tokens": ["Nim\u00b7mer", "weich'", "uns", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Singet: Ringet schlinget", "tokens": ["Sin\u00b7get", ":", "Rin\u00b7get", "schlin\u00b7get"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "NE", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dringet Brust an Brust", "tokens": ["Drin\u00b7get", "Brust", "an", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf den Feind, und bringet", "tokens": ["Auf", "den", "Feind", ",", "und", "brin\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sieg und Siegerlust:", "tokens": ["Sieg", "und", "Sie\u00b7ger\u00b7lust", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Andacht doppelt Kr\u00e4fte,", "tokens": ["An\u00b7dacht", "dop\u00b7pelt", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Muth nur sichert Gut,", "tokens": ["Muth", "nur", "si\u00b7chert", "Gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "CARD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Treue im Gesch\u00e4fte", "tokens": ["Treu\u00b7e", "im", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Leiht euch Himmels Huth.", "tokens": ["Leiht", "euch", "Him\u00b7mels", "Huth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}