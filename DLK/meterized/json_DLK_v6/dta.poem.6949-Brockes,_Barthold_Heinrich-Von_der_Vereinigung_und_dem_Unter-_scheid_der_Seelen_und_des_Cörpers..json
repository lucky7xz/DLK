{"dta.poem.6949": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So werden Fr\u00fccht' und Kr\u00e4uter m\u00fcssen,", "tokens": ["So", "wer\u00b7den", "Fr\u00fccht'", "und", "Kr\u00e4u\u00b7ter", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zusammt dem Wasser aus den Fl\u00fcssen,", "tokens": ["Zu\u00b7sammt", "dem", "Was\u00b7ser", "aus", "den", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie in unserm Magen nur", "tokens": ["Wenn", "sie", "in", "un\u00b7serm", "Ma\u00b7gen", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gekocht, gemischt, verdaut, und die Natur", "tokens": ["Ge\u00b7kocht", ",", "ge\u00b7mischt", ",", "ver\u00b7daut", ",", "und", "die", "Na\u00b7tur"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vom Fleisch und Blut und Geistern angenommen,", "tokens": ["Vom", "Fleisch", "und", "Blut", "und", "Geis\u00b7tern", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dadurch da\u00df sie stets mehr gesiebt, verkleint, zerrieben,", "tokens": ["Da\u00b7durch", "da\u00df", "sie", "stets", "mehr", "ge\u00b7siebt", ",", "ver\u00b7kleint", ",", "zer\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dieselbe Zartheit \u00fcberkommen,", "tokens": ["Die\u00b7sel\u00b7be", "Zar\u00b7theit", "\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die man bisher, mit Recht, der Seelen zugeschrieben.", "tokens": ["Die", "man", "bis\u00b7her", ",", "mit", "Recht", ",", "der", "See\u00b7len", "zu\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "$,", "APPR", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es folgt, da\u00df die Materie sich gantz", "tokens": ["Es", "folgt", ",", "da\u00df", "die", "Ma\u00b7te\u00b7rie", "sich", "gantz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Von ihrem vorgen Wesen trenne,", "tokens": ["Von", "ih\u00b7rem", "vor\u00b7gen", "We\u00b7sen", "tren\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df sie so gar der Weisheit Glantz", "tokens": ["Da\u00df", "sie", "so", "gar", "der", "Weis\u00b7heit", "Glantz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Empfang\u2019, und auf einmal verstehn und f\u00fchlen k\u00f6nne.", "tokens": ["Emp\u00b7fang'", ",", "und", "auf", "ein\u00b7mal", "ver\u00b7stehn", "und", "f\u00fch\u00b7len", "k\u00f6n\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "APPR", "ADV", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}