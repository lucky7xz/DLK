{"textgrid.poem.44909": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt, f\u00fcr seinen Ruhm zu sp\u00e4t,", "tokens": ["Hier", "liegt", ",", "f\u00fcr", "sei\u00b7nen", "Ruhm", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Don Quixotte der Legitimit\u00e4t,", "tokens": ["Der", "Don", "Qui\u00b7xot\u00b7te", "der", "Le\u00b7gi\u00b7ti\u00b7mi\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der Falsch und Wahr nach seinem Sinne bog,", "tokens": ["Der", "Falsch", "und", "Wahr", "nach", "sei\u00b7nem", "Sin\u00b7ne", "bog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zuerst die andern, dann sich selbst belog,", "tokens": ["Zu\u00b7erst", "die", "an\u00b7dern", ",", "dann", "sich", "selbst", "be\u00b7log", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vom Schelm zum Toren ward bei grauem Haupte,", "tokens": ["Vom", "Schelm", "zum", "To\u00b7ren", "ward", "bei", "grau\u00b7em", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weil er zuletzt die eignen L\u00fcgen glaubte.", "tokens": ["Weil", "er", "zu\u00b7letzt", "die", "eig\u00b7nen", "L\u00fc\u00b7gen", "glaub\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}