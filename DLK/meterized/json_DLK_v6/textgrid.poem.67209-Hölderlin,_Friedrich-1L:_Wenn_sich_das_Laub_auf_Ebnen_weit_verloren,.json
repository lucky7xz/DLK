{"textgrid.poem.67209": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn sich das Laub auf Ebnen weit verloren,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich das Laub auf Ebnen weit verloren,", "tokens": ["Wenn", "sich", "das", "Laub", "auf", "Eb\u00b7nen", "weit", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So f\u00e4llt das Wei\u00df herunter auf die Tale,", "tokens": ["So", "f\u00e4llt", "das", "Wei\u00df", "her\u00b7un\u00b7ter", "auf", "die", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch gl\u00e4nzend ist der Tag vom hohen Sonnenstrahle,", "tokens": ["Doch", "gl\u00e4n\u00b7zend", "ist", "der", "Tag", "vom", "ho\u00b7hen", "Son\u00b7nen\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es gl\u00e4nzt das Fest den St\u00e4dten aus den Toren.", "tokens": ["Es", "gl\u00e4nzt", "das", "Fest", "den", "St\u00e4d\u00b7ten", "aus", "den", "To\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist die Ruhe der Natur, des Feldes Schweigen", "tokens": ["Es", "ist", "die", "Ru\u00b7he", "der", "Na\u00b7tur", ",", "des", "Fel\u00b7des", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist wie des Menschen Geistigkeit, und h\u00f6her zeigen", "tokens": ["Ist", "wie", "des", "Men\u00b7schen", "Geis\u00b7tig\u00b7keit", ",", "und", "h\u00f6\u00b7her", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "NN", "$,", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Unterschiede sich, da\u00df sich zu hohem Bilde", "tokens": ["Die", "Un\u00b7ter\u00b7schie\u00b7de", "sich", ",", "da\u00df", "sich", "zu", "ho\u00b7hem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "$,", "KOUS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich zeiget die Natur, statt mit des Fr\u00fchlings Milde.", "tokens": ["Sich", "zei\u00b7get", "die", "Na\u00b7tur", ",", "statt", "mit", "des", "Fr\u00fch\u00b7lings", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "$,", "KOUI", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}