{"dta.poem.10617": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Vber den Abschied einer Edlen Jungfrawen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie zu Sommerszeit/ wann alles fr\u00f6lich bl\u00fchet/", "tokens": ["Gleich", "wie", "zu", "Som\u00b7mers\u00b7zeit", "/", "wann", "al\u00b7les", "fr\u00f6\u00b7lich", "bl\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "$(", "PWAV", "PIS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd Wald/ Berg/ Feld vnd Thal anm\u00fctig sch\u00f6n au\u00dfsiehet/", "tokens": ["Vnd", "Wald", "/", "Berg", "/", "Feld", "vnd", "Thal", "an\u00b7m\u00fc\u00b7tig", "sch\u00f6n", "au\u00df\u00b7sie\u00b7het", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "ADJD", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch vnder anderen sehr zarten Bl\u00fcmelein", "tokens": ["Auch", "vn\u00b7der", "an\u00b7de\u00b7ren", "sehr", "zar\u00b7ten", "Bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sch\u00f6ne Lilie l\u00e4st blicken jhren schein:", "tokens": ["Die", "sch\u00f6\u00b7ne", "Li\u00b7lie", "l\u00e4st", "bli\u00b7cken", "jhren", "schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es fliegen auff sie zu die Bienen hauffen weise/", "tokens": ["Es", "flie\u00b7gen", "auff", "sie", "zu", "die", "Bie\u00b7nen", "hauf\u00b7fen", "wei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd saugen au\u00df mit flei\u00df die angenehme Speise/", "tokens": ["Vnd", "sau\u00b7gen", "au\u00df", "mit", "flei\u00df", "die", "an\u00b7ge\u00b7neh\u00b7me", "Spei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den angenehmen Safft: Sie steht in h\u00f6chstem Flor/", "tokens": ["Den", "an\u00b7ge\u00b7neh\u00b7men", "Safft", ":", "Sie", "steht", "in", "h\u00f6chs\u00b7tem", "Flor", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es glentzt jhr weisses Kleyd f\u00fcr allen Blumen vor.", "tokens": ["Es", "glentzt", "jhr", "weis\u00b7ses", "Kleyd", "f\u00fcr", "al\u00b7len", "Blu\u00b7men", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr g\u00fcnstiger Geruch erfrewet Hertz vnd Sinnen/", "tokens": ["Ihr", "g\u00fcns\u00b7ti\u00b7ger", "Ge\u00b7ruch", "er\u00b7fre\u00b7wet", "Hertz", "vnd", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man mu\u00df jhr werden hold/ man mu\u00df sie lieb gewinnen/", "tokens": ["Man", "mu\u00df", "jhr", "wer\u00b7den", "hold", "/", "man", "mu\u00df", "sie", "lieb", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VAFIN", "ADJD", "$(", "PIS", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der sch\u00f6ne Zephyrus wirdt gegen jhr entz\u00fcndt/", "tokens": ["Der", "sch\u00f6\u00b7ne", "Ze\u00b7phy\u00b7rus", "wirdt", "ge\u00b7gen", "jhr", "ent\u00b7z\u00fcndt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Vnd wehet au\u00df Favor jhr zue den Liebes Windt.", "tokens": ["Vnd", "we\u00b7het", "au\u00df", "Fa\u00b7vor", "jhr", "zue", "den", "Lie\u00b7bes", "Windt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bald aber vnverhofft da kompt einher gebrauset", "tokens": ["Bald", "a\u00b7ber", "vn\u00b7ver\u00b7hofft", "da", "kompt", "ein\u00b7her", "ge\u00b7brau\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ADV", "VVFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der vngeheure Nort/ er pfeifft/ er heult/ er sauset/", "tokens": ["Der", "vn\u00b7ge\u00b7heu\u00b7re", "Nort", "/", "er", "pfeifft", "/", "er", "heult", "/", "er", "sau\u00b7set", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vnd nimpt mit vngest\u00fcmm die Lilie dahin/", "tokens": ["Vnd", "nimpt", "mit", "vn\u00b7ge\u00b7st\u00fcmm", "die", "Li\u00b7lie", "da\u00b7hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "ART", "NN", "PAV", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Die liebliche Gestallt bricht jhm gar nicht den Sinn/", "tokens": ["Die", "lieb\u00b7li\u00b7che", "Ge\u00b7stallt", "bricht", "jhm", "gar", "nicht", "den", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das gr\u00fcne Feld beginnt vmb seine Zier zutrauren/", "tokens": ["Das", "gr\u00fc\u00b7ne", "Feld", "be\u00b7ginnt", "vmb", "sei\u00b7ne", "Zier", "zu\u00b7trau\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die andre Bl\u00fcmelein thut jhrer Schwester tauren/", "tokens": ["Die", "and\u00b7re", "Bl\u00fc\u00b7me\u00b7lein", "thut", "jhrer", "Schwes\u00b7ter", "tau\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Die Bienen fliegen auch vor schmertz vnd Trawrigkeit", "tokens": ["Die", "Bie\u00b7nen", "flie\u00b7gen", "auch", "vor", "schmertz", "vnd", "Traw\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Verjrrt jetzt hin/ jetzt her/ vnd sind in grossem Leidt.", "tokens": ["Ver\u00b7jrrt", "jetzt", "hin", "/", "jetzt", "her", "/", "vnd", "sind", "in", "gros\u00b7sem", "Leidt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$(", "ADV", "PTKVZ", "$(", "KON", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So bistu auch zuvor/ du sch\u00f6neste/ gewesen/", "tokens": ["So", "bis\u00b7tu", "auch", "zu\u00b7vor", "/", "du", "sch\u00f6\u00b7nes\u00b7te", "/", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "$(", "PPER", "VVFIN", "$(", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du stirbst/ durch welch\u2019 ich mir verhoffte zugenesen/", "tokens": ["Du", "stirbst", "/", "durch", "welch'", "ich", "mir", "ver\u00b7hoff\u00b7te", "zu\u00b7ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "O du mein einig all/ jetzt bistu nackt vnd blo\u00df/", "tokens": ["O", "du", "mein", "ei\u00b7nig", "all", "/", "jetzt", "bis\u00b7tu", "nackt", "vnd", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "ADJD", "PIAT", "$(", "ADV", "ADV", "ADJD", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vnd kriegest einen Sarck vor deines Liebsten Scho\u00df.", "tokens": ["Vnd", "krie\u00b7gest", "ei\u00b7nen", "Sarck", "vor", "dei\u00b7nes", "Liebs\u00b7ten", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du weisse Lilie/ du Spiegel aller Tugendt/", "tokens": ["Du", "weis\u00b7se", "Li\u00b7lie", "/", "du", "Spie\u00b7gel", "al\u00b7ler", "Tu\u00b7gendt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "NN", "PIAT", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "In deiner besten Bl\u00fcht/ vnd in der gr\u00fcnen Jugendt/", "tokens": ["In", "dei\u00b7ner", "bes\u00b7ten", "Bl\u00fcht", "/", "vnd", "in", "der", "gr\u00fc\u00b7nen", "Ju\u00b7gendt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "KON", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.27": {"text": "K\u00fcrtzt dir der grimme Todt dein junges Leben ab/", "tokens": ["K\u00fcrtzt", "dir", "der", "grim\u00b7me", "Todt", "dein", "jun\u00b7ges", "Le\u00b7ben", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Vnd f\u00fchret dich behendt au\u00df meinem Arm ins Grab.", "tokens": ["Vnd", "f\u00fch\u00b7ret", "dich", "be\u00b7hendt", "au\u00df", "mei\u00b7nem", "Arm", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch bistu von der Welt vnd jhrer Noth gerissen/", "tokens": ["Doch", "bis\u00b7tu", "von", "der", "Welt", "vnd", "jhrer", "Noth", "ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Ich aber mu\u00df allhier in Qual vnd trawren b\u00fcssen/", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "all\u00b7hier", "in", "Qual", "vnd", "traw\u00b7ren", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "APPR", "NN", "KON", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wall im weiten Meer/ im weiten Meer der Noth/", "tokens": ["Ich", "wall", "im", "wei\u00b7ten", "Meer", "/", "im", "wei\u00b7ten", "Meer", "der", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$(", "APPRART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du bist todt lebendig/ ich bin lebendig todt.", "tokens": ["Du", "bist", "todt", "le\u00b7ben\u00b7dig", "/", "ich", "bin", "le\u00b7ben\u00b7dig", "todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "$(", "PPER", "VAFIN", "ADJD", "ADJD", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}