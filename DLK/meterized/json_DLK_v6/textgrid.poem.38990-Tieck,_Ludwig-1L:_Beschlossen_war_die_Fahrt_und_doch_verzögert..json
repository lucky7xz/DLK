{"textgrid.poem.38990": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Beschlossen war die Fahrt und doch verz\u00f6gert.", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beschlossen war die Fahrt und doch verz\u00f6gert.", "tokens": ["Be\u00b7schlos\u00b7sen", "war", "die", "Fahrt", "und", "doch", "ver\u00b7z\u00f6\u00b7gert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wundersame M\u00e4hren,", "tokens": ["Wun\u00b7der\u00b7sa\u00b7me", "M\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie aus dem Dunkel fr\u00fcher Jahrhunderte,", "tokens": ["Wie", "aus", "dem", "Dun\u00b7kel", "fr\u00fc\u00b7her", "Jahr\u00b7hun\u00b7der\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Leben wieder auf und wandeln uns nah.", "tokens": ["Le\u00b7ben", "wie\u00b7der", "auf", "und", "wan\u00b7deln", "uns", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Es sch\u00fcttelt bedenklich", "tokens": ["Es", "sch\u00fct\u00b7telt", "be\u00b7denk\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Vetturin das Haupt,", "tokens": ["Der", "Vet\u00b7tu\u00b7rin", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wirth und die G\u00e4ste", "tokens": ["Der", "Wirth", "und", "die", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schauen sich ernsten langen Blickes an,", "tokens": ["Schau\u00b7en", "sich", "erns\u00b7ten", "lan\u00b7gen", "Bli\u00b7ckes", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und an der Furcht des einen", "tokens": ["Und", "an", "der", "Furcht", "des", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Z\u00fcndet jener am Funken", "tokens": ["Z\u00fcn\u00b7det", "je\u00b7ner", "am", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Die Fackel seiner Angst.", "tokens": ["Die", "Fa\u00b7ckel", "sei\u00b7ner", "Angst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein scheu\u00dflich gro\u00dfes Ungeheuer", "tokens": ["Ein", "scheu\u00df\u00b7lich", "gro\u00b7\u00dfes", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lagert auf dem Wege,", "tokens": ["La\u00b7gert", "auf", "dem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unbeschreiblich ist es, aber kr\u00e4ftig, wild:", "tokens": ["Un\u00b7be\u00b7schreib\u00b7lich", "ist", "es", ",", "a\u00b7ber", "kr\u00e4f\u00b7tig", ",", "wild", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADV", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Erst nur verschlang es Schaafe und Hammel,", "tokens": ["Erst", "nur", "ver\u00b7schlang", "es", "Schaa\u00b7fe", "und", "Ham\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dann auch die Menschen,", "tokens": ["Dann", "auch", "die", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Wagen und Pferde.", "tokens": ["Wa\u00b7gen", "und", "Pfer\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ist es ein Lindwurm?", "tokens": ["Ist", "es", "ein", "Lind\u00b7wurm", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Kehren die Drachen denn wieder,", "tokens": ["Keh\u00b7ren", "die", "Dra\u00b7chen", "denn", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ADV", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Die wilden W\u00fcrme,", "tokens": ["Die", "wil\u00b7den", "W\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Die Dietrich von Bern so fr\u00fch schon vertilgte?", "tokens": ["Die", "Diet\u00b7rich", "von", "Bern", "so", "fr\u00fch", "schon", "ver\u00b7tilg\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "NE", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Jammer auf Jammer!", "tokens": ["Jam\u00b7mer", "auf", "Jam\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schon wieder ein Fuhrmann,", "tokens": ["Schon", "wie\u00b7der", "ein", "Fuhr\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Angst und Roth", "tokens": ["Der", "Angst", "und", "Roth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mit zitternden Lippen berichtet.", "tokens": ["Mit", "zit\u00b7tern\u00b7den", "Lip\u00b7pen", "be\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Von Mantua aus zog ein Geschwader,", "tokens": ["Von", "Man\u00b7tua", "aus", "zog", "ein", "Ge\u00b7schwa\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APZR", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wohl ger\u00fcstet,", "tokens": ["Wohl", "ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Mit Schwert und Lanze,", "tokens": ["Mit", "Schwert", "und", "Lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und neuen Flinten,", "tokens": ["Und", "neu\u00b7en", "Flin\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Nicht wenige Mannschaft.", "tokens": ["Nicht", "we\u00b7ni\u00b7ge", "Mann\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Und Lieutnant weder,", "tokens": ["Und", "Li\u00b7eut\u00b7nant", "we\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "$,"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.11": {"text": "Noch Sergeant, Corporal,", "tokens": ["Noch", "Ser\u00b7ge\u00b7ant", ",", "Cor\u00b7po\u00b7ral", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Am wenigsten die Gemeinen,", "tokens": ["Am", "we\u00b7nigs\u00b7ten", "die", "Ge\u00b7mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Haben die Thore der Stadt je wiedergesehn:", "tokens": ["Ha\u00b7ben", "die", "Tho\u00b7re", "der", "Stadt", "je", "wie\u00b7der\u00b7ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.14": {"text": "Wo sind sie geblieben?", "tokens": ["Wo", "sind", "sie", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Mit welchem neuen Kriegesmuth", "tokens": ["Mit", "wel\u00b7chem", "neu\u00b7en", "Krie\u00b7ges\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df der Gr\u00e4uelwurm nun schreiten,", "tokens": ["Mu\u00df", "der", "Gr\u00e4u\u00b7el\u00b7wurm", "nun", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Un\u00fcberwindlich trotzen,", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "trot\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So viele Helden im Bauch!", "tokens": ["So", "vie\u00b7le", "Hel\u00b7den", "im", "Bauch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nun beschw\u00f6rt uns unser Florentiner,", "tokens": ["Nun", "be\u00b7schw\u00f6rt", "uns", "un\u00b7ser", "Flo\u00b7ren\u00b7ti\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der selbst gern dem Vaterlande zueilt,", "tokens": ["Der", "selbst", "gern", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "zu\u00b7eilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ja zu warten und zu harren,", "tokens": ["Ja", "zu", "war\u00b7ten", "und", "zu", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Still ergeben,", "tokens": ["Still", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Bis man merkt wohin sichs wende,", "tokens": ["Bis", "man", "merkt", "wo\u00b7hin", "sichs", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn das neue Commando", "tokens": ["Wenn", "das", "neu\u00b7e", "Com\u00b7man\u00b7do"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Dort aus der Vestung", "tokens": ["Dort", "aus", "der", "Ves\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Mit Artillerie reichlich begleitet,", "tokens": ["Mit", "Ar\u00b7til\u00b7le\u00b7rie", "reich\u00b7lich", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Dem Ungeth\u00fcm entgegen eilt.", "tokens": ["Dem", "Un\u00b7ge\u00b7th\u00fcm", "ent\u00b7ge\u00b7gen", "eilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch mit Bitten, Lachen, Drohen,", "tokens": ["Doch", "mit", "Bit\u00b7ten", ",", "La\u00b7chen", ",", "Dro\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schimpfen, Zank und vielem Scherz,", "tokens": ["Schimp\u00b7fen", ",", "Zank", "und", "vie\u00b7lem", "Scherz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "PIS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird der Zitternde doch bewogen,", "tokens": ["Wird", "der", "Zit\u00b7tern\u00b7de", "doch", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die Thiere einzuspannen.", "tokens": ["Die", "Thie\u00b7re", "ein\u00b7zu\u00b7span\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Senat sch\u00fcttelt das Haupt,", "tokens": ["Der", "Se\u00b7nat", "sch\u00fct\u00b7telt", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und sieht uns weislich nach,", "tokens": ["Und", "sieht", "uns", "weis\u00b7lich", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Meint am Ende,", "tokens": ["Meint", "am", "En\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "An th\u00f6richten Deutschen sei freilich nicht viel verloren.", "tokens": ["An", "th\u00f6\u00b7rich\u00b7ten", "Deut\u00b7schen", "sei", "frei\u00b7lich", "nicht", "viel", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Hell scheint die Sonne,", "tokens": ["Hell", "scheint", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Schnell l\u00e4uft das Fuhrwerk,", "tokens": ["Schnell", "l\u00e4uft", "das", "Fuhr\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Und der Regierer", "tokens": ["Und", "der", "Re\u00b7gie\u00b7rer"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Hat Augen rechts und links und allerseits.", "tokens": ["Hat", "Au\u00b7gen", "rechts", "und", "links", "und", "al\u00b7ler\u00b7seits", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "KON", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Alles in Ruhe,", "tokens": ["Al\u00b7les", "in", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Doch naht nur ein Reiter,", "tokens": ["Doch", "naht", "nur", "ein", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "So h\u00e4lt er sinnig an.", "tokens": ["So", "h\u00e4lt", "er", "sin\u00b7nig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Schon entwickelt sich in gr\u00fcner Ebne", "tokens": ["Schon", "ent\u00b7wi\u00b7ckelt", "sich", "in", "gr\u00fc\u00b7ner", "Eb\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die Vestung dort,", "tokens": ["Die", "Ves\u00b7tung", "dort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sein Muth erw\u00e4chst so mehr und mehr,", "tokens": ["Sein", "Muth", "er\u00b7w\u00e4chst", "so", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er treibt die Rosse", "tokens": ["Er", "treibt", "die", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und an dem Thore", "tokens": ["Und", "an", "dem", "Tho\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sind wir gefl\u00fcgelten Laufs.", "tokens": ["Sind", "wir", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "Laufs", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Welch Menschengedr\u00e4nge!", "tokens": ["Welch", "Men\u00b7schen\u00b7ge\u00b7dr\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Welch Toben! Welch Erz\u00e4hlen!", "tokens": ["Welch", "To\u00b7ben", "!", "Welch", "Er\u00b7z\u00e4h\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Welch Jubelgeschrei!", "tokens": ["Welch", "Ju\u00b7bel\u00b7ge\u00b7schrei", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und aus dem Irrsaal", "tokens": ["Und", "aus", "dem", "Irr\u00b7saal"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Vernimmt man die M\u00e4hre,", "tokens": ["Ver\u00b7nimmt", "man", "die", "M\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das Furchtgethier", "tokens": ["Das", "Furcht\u00b7ge\u00b7thier"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sei eingefangen,", "tokens": ["Sei", "ein\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Von k\u00fchner Heldenfaust erlegt,", "tokens": ["Von", "k\u00fch\u00b7ner", "Hel\u00b7den\u00b7faust", "er\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und dort auf dem Rathhaus f\u00fcr wenige Groschen zu sehn.", "tokens": ["Und", "dort", "auf", "dem", "Rath\u00b7haus", "f\u00fcr", "we\u00b7ni\u00b7ge", "Gro\u00b7schen", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}}, "stanza.9": {"line.1": {"text": "Wir steigen ab,", "tokens": ["Wir", "stei\u00b7gen", "ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und folgen dem Zuge.", "tokens": ["Und", "fol\u00b7gen", "dem", "Zu\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Was war das Gespenst?", "tokens": ["Was", "war", "das", "Ge\u00b7spenst", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein m\u00e4\u00dfiges W\u00f6lflein,", "tokens": ["Ein", "m\u00e4\u00b7\u00dfi\u00b7ges", "W\u00f6lf\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem man mit Pfl\u00f6cken", "tokens": ["Dem", "man", "mit", "Pfl\u00f6\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Rachen aufgesperrt,", "tokens": ["Den", "Ra\u00b7chen", "auf\u00b7ge\u00b7sperrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df die poetischen Menschen,", "tokens": ["Da\u00df", "die", "po\u00b7e\u00b7ti\u00b7schen", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die Phantasie begabten,", "tokens": ["Die", "Phan\u00b7ta\u00b7sie", "be\u00b7gab\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "An seinem nicht gro\u00dfen Gezahn", "tokens": ["An", "sei\u00b7nem", "nicht", "gro\u00b7\u00dfen", "Ge\u00b7zahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "PTKNEG", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Sich schaudernd erg\u00f6tzten.", "tokens": ["Sich", "schau\u00b7dernd", "er\u00b7g\u00f6tz\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}