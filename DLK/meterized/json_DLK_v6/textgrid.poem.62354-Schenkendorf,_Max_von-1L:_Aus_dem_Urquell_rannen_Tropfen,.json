{"textgrid.poem.62354": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus dem Urquell rannen Tropfen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem Urquell rannen Tropfen,", "tokens": ["Aus", "dem", "Ur\u00b7quell", "ran\u00b7nen", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seelen, die gleich hellen Thr\u00e4nen,", "tokens": ["See\u00b7len", ",", "die", "gleich", "hel\u00b7len", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Farben spiegeln und sich sehnen", "tokens": ["Far\u00b7ben", "spie\u00b7geln", "und", "sich", "seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach den Schwesterthr\u00e4nen,", "tokens": ["Nach", "den", "Schwes\u00b7ter\u00b7thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nach dem Thr\u00e4nenmeer.", "tokens": ["Nach", "dem", "Thr\u00e4\u00b7nen\u00b7meer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Aus dem Urquell rannen Thr\u00e4nen,", "tokens": ["Aus", "dem", "Ur\u00b7quell", "ran\u00b7nen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die, zu Steinen schnell erkaltet,", "tokens": ["Die", ",", "zu", "Stei\u00b7nen", "schnell", "er\u00b7kal\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig Thr\u00e4nenfarbe tragen,", "tokens": ["E\u00b7wig", "Thr\u00e4\u00b7nen\u00b7far\u00b7be", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sich ewig sehnen", "tokens": ["Die", "sich", "e\u00b7wig", "seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nach der Schmelzung Glut.", "tokens": ["Nach", "der", "Schmel\u00b7zung", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hat sie nicht ein Ziel gefunden", "tokens": ["Hat", "sie", "nicht", "ein", "Ziel", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr die ewig rege Sehnsucht", "tokens": ["F\u00fcr", "die", "e\u00b7wig", "re\u00b7ge", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Seele? Schwesterthr\u00e4ne,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "?", "Schwes\u00b7ter\u00b7thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Holde Schwesterseele,", "tokens": ["Hol\u00b7de", "Schwes\u00b7ter\u00b7see\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wurden wir nicht eins?", "tokens": ["Wur\u00b7den", "wir", "nicht", "eins", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PIS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sieh die zarten Perlenschn\u00fcre", "tokens": ["Sieh", "die", "zar\u00b7ten", "Per\u00b7len\u00b7schn\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich um deinen Busen schlingen,", "tokens": ["Sich", "um", "dei\u00b7nen", "Bu\u00b7sen", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie gl\u00e4nzen, wie sie gl\u00fchen,", "tokens": ["Wie", "sie", "gl\u00e4n\u00b7zen", ",", "wie", "sie", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie Leben finden", "tokens": ["Wie", "sie", "Le\u00b7ben", "fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dort, wo ich sie fand?", "tokens": ["Dort", ",", "wo", "ich", "sie", "fand", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}