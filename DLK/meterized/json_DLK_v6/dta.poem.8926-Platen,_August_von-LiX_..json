{"dta.poem.8926": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LiX .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Da Trost mir Keiner doch vermag zu schenken?", "tokens": ["Da", "Trost", "mir", "Kei\u00b7ner", "doch", "ver\u00b7mag", "zu", "schen\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PIS", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich will mich ganz in meinen Schmerz versenken,", "tokens": ["Ich", "will", "mich", "ganz", "in", "mei\u00b7nen", "Schmerz", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Stillen weinen und im Stillen trauern.", "tokens": ["Im", "Stil\u00b7len", "wei\u00b7nen", "und", "im", "Stil\u00b7len", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "KON", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht w\u00fcrdig bin ich, l\u00e4nger fortzudauern,", "tokens": ["Nicht", "w\u00fcr\u00b7dig", "bin", "ich", ",", "l\u00e4n\u00b7ger", "fort\u00b7zu\u00b7dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seitdem ich starb in seinem Angedenken,", "tokens": ["Seit\u00b7dem", "ich", "starb", "in", "sei\u00b7nem", "An\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in den schon ermattenden Gelenken", "tokens": ["Und", "in", "den", "schon", "er\u00b7mat\u00b7ten\u00b7den", "Ge\u00b7len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchl' ich die Keime der Zerst\u00f6rung schauern.", "tokens": ["F\u00fchl'", "ich", "die", "Kei\u00b7me", "der", "Zer\u00b7st\u00f6\u00b7rung", "schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihn aber, himmlische Gewalten, lasset", "tokens": ["Ihn", "a\u00b7ber", ",", "himm\u00b7li\u00b7sche", "Ge\u00b7wal\u00b7ten", ",", "las\u00b7set"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz gl\u00fccklich werden, und versagt ihm keinen", "tokens": ["Ganz", "gl\u00fcck\u00b7lich", "wer\u00b7den", ",", "und", "ver\u00b7sagt", "ihm", "kei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAINF", "$,", "KON", "VVFIN", "PPER", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von allen W\u00fcnschen, die sein Herz umfasset!", "tokens": ["Von", "al\u00b7len", "W\u00fcn\u00b7schen", ",", "die", "sein", "Herz", "um\u00b7fas\u00b7set", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nie soll mein Blick begegnen mehr dem seinen,", "tokens": ["Nie", "soll", "mein", "Blick", "be\u00b7geg\u00b7nen", "mehr", "dem", "sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "ADV", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ach, das Bild des Menschen, den er hasset,", "tokens": ["Und", "ach", ",", "das", "Bild", "des", "Men\u00b7schen", ",", "den", "er", "has\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es soll ihm nicht einmal im Traum erscheinen!", "tokens": ["Es", "soll", "ihm", "nicht", "ein\u00b7mal", "im", "Traum", "er\u00b7schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}