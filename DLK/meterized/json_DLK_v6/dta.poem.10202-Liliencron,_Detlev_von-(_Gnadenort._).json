{"dta.poem.10202": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Gnadenort. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfmutter wird nun t\u00e4glich immer schlimmer,", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "wird", "nun", "t\u00e4g\u00b7lich", "im\u00b7mer", "schlim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch z\u00f6gert noch der Alles\u00fcberwinder.", "tokens": ["Doch", "z\u00f6\u00b7gert", "noch", "der", "Al\u00b7le\u00b7s\u00fc\u00b7berw\u00b7in\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dicht vor dem Spiegel stehn im Nebenzimmer", "tokens": ["Dicht", "vor", "dem", "Spie\u00b7gel", "stehn", "im", "Ne\u00b7ben\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mamachen und drei h\u00fcbsche blonde Kinder,", "tokens": ["Ma\u00b7ma\u00b7chen", "und", "drei", "h\u00fcb\u00b7sche", "blon\u00b7de", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "CARD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und proben emsig, wie der schwarze Flimmer", "tokens": ["Und", "pro\u00b7ben", "em\u00b7sig", ",", "wie", "der", "schwar\u00b7ze", "Flim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So reizend putzt als Kleid, als Hut nicht minder.", "tokens": ["So", "rei\u00b7zend", "putzt", "als", "Kleid", ",", "als", "Hut", "nicht", "min\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KOUS", "NN", "$,", "KOUS", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gro\u00dfmutter stirbt \u2014 es konnte nimmer grimmer", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "stirbt", "es", "konn\u00b7te", "nim\u00b7mer", "grim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "PPER", "VMFIN", "ADV", "ADJA"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Damen Trauer sein, das sieht ein Blinder.", "tokens": ["Der", "Da\u00b7men", "Trau\u00b7er", "sein", ",", "das", "sieht", "ein", "Blin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$,", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}