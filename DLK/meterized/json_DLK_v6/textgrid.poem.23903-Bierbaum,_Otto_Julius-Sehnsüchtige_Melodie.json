{"textgrid.poem.23903": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Sehns\u00fcchtige Melodie", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Roseninsel, schwanumschwommen,", "tokens": ["Ro\u00b7se\u00b7nin\u00b7sel", ",", "schwa\u00b7num\u00b7schwom\u00b7men", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Roseninsel im gr\u00fcnen Meere,", "tokens": ["Ro\u00b7se\u00b7nin\u00b7sel", "im", "gr\u00fc\u00b7nen", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Roseninsel, d\u00fcfteschwere,", "tokens": ["Ro\u00b7se\u00b7nin\u00b7sel", ",", "d\u00fcf\u00b7tesc\u00b7hwe\u00b7re", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonnenhei\u00dfe,", "tokens": ["Son\u00b7nen\u00b7hei\u00b7\u00dfe", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Felsenwei\u00dfe,", "tokens": ["Fel\u00b7sen\u00b7wei\u00b7\u00dfe", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Heckenheimliche Roseninsel ...", "tokens": ["He\u00b7cken\u00b7heim\u00b7li\u00b7che", "Ro\u00b7se\u00b7nin\u00b7sel", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Rote Rosen, rankenwilde,", "tokens": ["Ro\u00b7te", "Ro\u00b7sen", ",", "ran\u00b7ken\u00b7wil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rote Rosen, herzenhei\u00dfe,", "tokens": ["Ro\u00b7te", "Ro\u00b7sen", ",", "her\u00b7zen\u00b7hei\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rote Rosen auf S\u00e4ulenwei\u00dfe,", "tokens": ["Ro\u00b7te", "Ro\u00b7sen", "auf", "S\u00e4u\u00b7len\u00b7wei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Stengelhohe,", "tokens": ["Sten\u00b7gel\u00b7ho\u00b7he", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Sch\u00f6nheitfrohe,", "tokens": ["Sch\u00f6n\u00b7heit\u00b7fro\u00b7he", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Glutensammelnde rote Rosen ...", "tokens": ["Glu\u00b7ten\u00b7sam\u00b7meln\u00b7de", "ro\u00b7te", "Ro\u00b7sen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Tempelhallen, marmorhelle,", "tokens": ["Tem\u00b7pel\u00b7hal\u00b7len", ",", "mar\u00b7mo\u00b7rhel\u00b7le", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tempelhallen in heiligem Schweigen,", "tokens": ["Tem\u00b7pel\u00b7hal\u00b7len", "in", "hei\u00b7li\u00b7gem", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Tempelhallen, von Lorbeerzweigen", "tokens": ["Tem\u00b7pel\u00b7hal\u00b7len", ",", "von", "Lor\u00b7beer\u00b7zwei\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Eingeschlossene,", "tokens": ["Ein\u00b7ge\u00b7schlos\u00b7se\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Sonn\u00fcbergossene,", "tokens": ["Sonn\u00b7\u00fc\u00b7ber\u00b7gos\u00b7se\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Lautlose, leuchtende Tempelhallen ...", "tokens": ["Laut\u00b7lo\u00b7se", ",", "leuch\u00b7ten\u00b7de", "Tem\u00b7pel\u00b7hal\u00b7len", "..."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Wei\u00dfe Leiber, hei\u00dfe, nackte,", "tokens": ["Wei\u00b7\u00dfe", "Lei\u00b7ber", ",", "hei\u00b7\u00dfe", ",", "nack\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dfe Leiber, rosenumr\u00f6tet,", "tokens": ["Wei\u00b7\u00dfe", "Lei\u00b7ber", ",", "ro\u00b7sen\u00b7um\u00b7r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wei\u00dfe Leiber, tanzumfl\u00f6tet,", "tokens": ["Wei\u00b7\u00dfe", "Lei\u00b7ber", ",", "tan\u00b7zum\u00b7fl\u00f6\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlanke, hohe,", "tokens": ["Schlan\u00b7ke", ",", "ho\u00b7he", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Sch\u00f6nheitfrohe,", "tokens": ["Sch\u00f6n\u00b7heit\u00b7fro\u00b7he", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Glutenhauchende wei\u00dfe Leiber ...", "tokens": ["Glu\u00b7ten\u00b7hau\u00b7chen\u00b7de", "wei\u00b7\u00dfe", "Lei\u00b7ber", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}