{"textgrid.poem.67186": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn aus dem Himmel hellere Wonne sich", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn aus dem Himmel hellere Wonne sich", "tokens": ["Wenn", "aus", "dem", "Him\u00b7mel", "hel\u00b7le\u00b7re", "Won\u00b7ne", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "PRF"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Herabgie\u00dft, eine Freude den Menschen kommt,", "tokens": ["Her\u00b7ab\u00b7gie\u00dft", ",", "ei\u00b7ne", "Freu\u00b7de", "den", "Men\u00b7schen", "kommt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df sie sich wundern \u00fcber manches", "tokens": ["Da\u00df", "sie", "sich", "wun\u00b7dern", "\u00fc\u00b7ber", "man\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "APPR", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sichtbares, H\u00f6heres, Angenehmes:", "tokens": ["Sicht\u00b7ba\u00b7res", ",", "H\u00f6\u00b7he\u00b7res", ",", "An\u00b7ge\u00b7neh\u00b7mes", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wie t\u00f6net lieblich heilger Gesang dazu!", "tokens": ["Wie", "t\u00f6\u00b7net", "lieb\u00b7lich", "heil\u00b7ger", "Ge\u00b7sang", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie lacht das Herz in Liedern die Wahrheit an,", "tokens": ["Wie", "lacht", "das", "Herz", "in", "Lie\u00b7dern", "die", "Wahr\u00b7heit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df Freudigkeit an einem Bildnis \u2013", "tokens": ["Da\u00df", "Freu\u00b7dig\u00b7keit", "an", "ei\u00b7nem", "Bild\u00b7nis", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber dem Stege beginnen Schafe", "tokens": ["\u00dc\u00b7ber", "dem", "Ste\u00b7ge", "be\u00b7gin\u00b7nen", "Scha\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Den Zug, der fast in d\u00e4mmernde W\u00e4lder geht.", "tokens": ["Den", "Zug", ",", "der", "fast", "in", "d\u00e4m\u00b7mern\u00b7de", "W\u00e4l\u00b7der", "geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Wiesen aber, welche mit lautrem Gr\u00fcn", "tokens": ["Die", "Wie\u00b7sen", "a\u00b7ber", ",", "wel\u00b7che", "mit", "laut\u00b7rem", "Gr\u00fcn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bedeckt sind, sind wie jene Heide,", "tokens": ["Be\u00b7deckt", "sind", ",", "sind", "wie", "je\u00b7ne", "Hei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VAFIN", "KOKOM", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welche gew\u00f6hnlicher Weise nah ist", "tokens": ["Wel\u00b7che", "ge\u00b7w\u00f6hn\u00b7li\u00b7cher", "Wei\u00b7se", "nah", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "ADJD", "VAFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Dem dunkeln Walde. Da, auf den Wiesen auch", "tokens": ["Dem", "dun\u00b7keln", "Wal\u00b7de", ".", "Da", ",", "auf", "den", "Wie\u00b7sen", "auch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "$,", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verweilen diese Schafe. Die Gipfel, die", "tokens": ["Ver\u00b7wei\u00b7len", "die\u00b7se", "Scha\u00b7fe", ".", "Die", "Gip\u00b7fel", ",", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PDAT", "NN", "$.", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umher sind, nackte H\u00f6hen sind mit", "tokens": ["Um\u00b7her", "sind", ",", "nack\u00b7te", "H\u00f6\u00b7hen", "sind", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "ADJA", "NN", "VAFIN", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Eichen bedecket und seltnen Tannen.", "tokens": ["Ei\u00b7chen", "be\u00b7de\u00b7cket", "und", "selt\u00b7nen", "Tan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Da, wo des Stromes regsame Wellen sind,", "tokens": ["Da", ",", "wo", "des", "Stro\u00b7mes", "reg\u00b7sa\u00b7me", "Wel\u00b7len", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "VAFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df einer, der vor\u00fcber des Weges kommt,", "tokens": ["Da\u00df", "ei\u00b7ner", ",", "der", "vor\u00b7\u00fc\u00b7ber", "des", "We\u00b7ges", "kommt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Froh hinschaut, da erhebt der Berge", "tokens": ["Froh", "hin\u00b7schaut", ",", "da", "er\u00b7hebt", "der", "Ber\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sanfte Gestalt und der Weinberg hoch sich.", "tokens": ["Sanf\u00b7te", "Ge\u00b7stalt", "und", "der", "Wein\u00b7berg", "hoch", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "NN", "ADJD", "PRF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Zwar gehn die Treppen unter den Reben hoch", "tokens": ["Zwar", "gehn", "die", "Trep\u00b7pen", "un\u00b7ter", "den", "Re\u00b7ben", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Herunter, wo der Obstbaum bl\u00fchend dar\u00fcber steht", "tokens": ["Her\u00b7un\u00b7ter", ",", "wo", "der", "Obst\u00b7baum", "bl\u00fc\u00b7hend", "da\u00b7r\u00fc\u00b7ber", "steht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADJD", "PAV", "VVFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und Duft an wilden Hecken weilet,", "tokens": ["Und", "Duft", "an", "wil\u00b7den", "He\u00b7cken", "wei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo die verborgenen Veilchen sprossen;", "tokens": ["Wo", "die", "ver\u00b7bor\u00b7ge\u00b7nen", "Veil\u00b7chen", "spros\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Gew\u00e4sser aber rieseln herab, und sanft", "tokens": ["Ge\u00b7w\u00e4s\u00b7ser", "a\u00b7ber", "rie\u00b7seln", "her\u00b7ab", ",", "und", "sanft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ADV", "$,", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist h\u00f6rbar dort ein Rauschen den ganzen Tag;", "tokens": ["Ist", "h\u00f6r\u00b7bar", "dort", "ein", "Rau\u00b7schen", "den", "gan\u00b7zen", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Orte aber in der Gegend", "tokens": ["Die", "Or\u00b7te", "a\u00b7ber", "in", "der", "Ge\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruhen und schweigen den Nachmittag durch.", "tokens": ["Ru\u00b7hen", "und", "schwei\u00b7gen", "den", "Nach\u00b7mit\u00b7tag", "durch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}}}}}