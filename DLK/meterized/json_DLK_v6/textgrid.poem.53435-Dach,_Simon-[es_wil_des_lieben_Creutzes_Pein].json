{"textgrid.poem.53435": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[es wil des lieben Creutzes Pein]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wil des lieben Creutzes Pein", "tokens": ["Es", "wil", "des", "lie\u00b7ben", "Creut\u00b7zes", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar bey uns Menschen sauer ein:", "tokens": ["Zwar", "bey", "uns", "Men\u00b7schen", "sau\u00b7er", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen gern die Rosen brechen,", "tokens": ["Wir", "wol\u00b7len", "gern", "die", "Ro\u00b7sen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch da\u00df kein Dorn uns m\u00f6ge stechen,", "tokens": ["Doch", "da\u00df", "kein", "Dorn", "uns", "m\u00f6\u00b7ge", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als k\u00f6nn ein Christ ohn Ungl\u00fcck seyn.", "tokens": ["Als", "k\u00f6nn", "ein", "Christ", "ohn", "Un\u00b7gl\u00fcck", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ist auff Erden hoch und werth,", "tokens": ["Was", "ist", "auff", "Er\u00b7den", "hoch", "und", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das uns ohn Arbeit wird beschehrt?", "tokens": ["Das", "uns", "ohn", "Ar\u00b7beit", "wird", "be\u00b7schehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir m\u00fcssen viel dar\u00fcber leiden,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "viel", "da\u00b7r\u00fc\u00b7ber", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schlaff sambt aller Wollust meiden,", "tokens": ["Den", "Schlaff", "sambt", "al\u00b7ler", "Wol\u00b7lust", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht scheuen Hunger oder Schwerdt.", "tokens": ["Nicht", "scheu\u00b7en", "Hun\u00b7ger", "o\u00b7der", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer in dem Wettlauff eilt zum Ziel,", "tokens": ["Wer", "in", "dem", "Wett\u00b7lauff", "eilt", "zum", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gibt auff den dicken Staub nicht viel:", "tokens": ["Gibt", "auff", "den", "di\u00b7cken", "Staub", "nicht", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wer im k\u00e4mpffen oder ringen", "tokens": ["Und", "wer", "im", "k\u00e4mpf\u00b7fen", "o\u00b7der", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPRART", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Kleinoht sucht davon zu bringen,", "tokens": ["Das", "Klei\u00b7noht", "sucht", "da\u00b7von", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sch\u00e4tzt die Gefahr f\u00fcr Kinderspiel.", "tokens": ["Sch\u00e4tzt", "die", "Ge\u00b7fahr", "f\u00fcr", "Kin\u00b7der\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun ist der Himmel unser Gut,", "tokens": ["Nun", "ist", "der", "Him\u00b7mel", "un\u00b7ser", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sich durch geiles Fleisch und Blut", "tokens": ["Das", "sich", "durch", "gei\u00b7les", "Fleisch", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Z\u00e4rtlichkeit nicht l\u00e4st erhalten,", "tokens": ["Und", "Z\u00e4rt\u00b7lich\u00b7keit", "nicht", "l\u00e4st", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hie mu\u00df das Creutz ohn Ende walten", "tokens": ["Hie", "mu\u00df", "das", "Creutz", "ohn", "En\u00b7de", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und zwingen uns den weichen Muht.", "tokens": ["Und", "zwin\u00b7gen", "uns", "den", "wei\u00b7chen", "Muht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Silber, sol es k\u00f6stlich seyn,", "tokens": ["Das", "Sil\u00b7ber", ",", "sol", "es", "k\u00f6st\u00b7lich", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht siebenmahl das Feuer ein:", "tokens": ["Geht", "sie\u00b7ben\u00b7mahl", "das", "Feu\u00b7er", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein rechter Christ mu\u00df auch auff Erden", "tokens": ["Ein", "rech\u00b7ter", "Christ", "mu\u00df", "auch", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Noht und Todt gel\u00e4utert werden,", "tokens": ["Durch", "Noht", "und", "Todt", "ge\u00b7l\u00e4u\u00b7tert", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sol er bestehn im Werth und Schein.", "tokens": ["Sol", "er", "be\u00b7stehn", "im", "Werth", "und", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und selig ist derselbe Mann,", "tokens": ["Und", "se\u00b7lig", "ist", "der\u00b7sel\u00b7be", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der die Anfechtung dulden kan:", "tokens": ["Der", "die", "An\u00b7fech\u00b7tung", "dul\u00b7den", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Denn wenn er nun nach vielen Wunden", "tokens": ["Denn", "wenn", "er", "nun", "nach", "vie\u00b7len", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bew\u00e4hrt und lauter ist gefunden,", "tokens": ["Be\u00b7w\u00e4hrt", "und", "lau\u00b7ter", "ist", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nimbt ihn der Himmel herrlich an.", "tokens": ["Nimbt", "ihn", "der", "Him\u00b7mel", "herr\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er \u00fcberk\u00f6mbt von Gottes Sohn", "tokens": ["Er", "\u00fc\u00b7ber\u00b7k\u00f6mbt", "von", "Got\u00b7tes", "Sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens unverwelckte Krohn,", "tokens": ["Des", "Le\u00b7bens", "un\u00b7ver\u00b7welck\u00b7te", "Krohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Kleinod keinem zu vergleichen,", "tokens": ["Ein", "Klei\u00b7nod", "kei\u00b7nem", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wollte f\u00fcr dem Creutz erbleichen", "tokens": ["Wer", "woll\u00b7te", "f\u00fcr", "dem", "Creutz", "er\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Umb diesen allersch\u00f6nsten Lohn?", "tokens": ["Umb", "die\u00b7sen", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Welt, Pracht und Wollust z\u00fcrnt mit mir,", "tokens": ["Welt", ",", "Pracht", "und", "Wol\u00b7lust", "z\u00fcrnt", "mit", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Komm liebes Creutz, der Frommen Zier,", "tokens": ["Komm", "lie\u00b7bes", "Creutz", ",", "der", "From\u00b7men", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit der selbst Christus wollen prangen,", "tokens": ["Mit", "der", "selbst", "Chris\u00b7tus", "wol\u00b7len", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "NE", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Komm, la\u00df dich stets von mir umbfangen,", "tokens": ["Komm", ",", "la\u00df", "dich", "stets", "von", "mir", "umb\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Am besten halt ich es mit dir.", "tokens": ["Am", "bes\u00b7ten", "halt", "ich", "es", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}