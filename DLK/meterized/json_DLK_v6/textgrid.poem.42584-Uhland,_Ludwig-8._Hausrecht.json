{"textgrid.poem.42584": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "8. Hausrecht", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tritt ein zu dieser Schwelle!", "tokens": ["Tritt", "ein", "zu", "die\u00b7ser", "Schwel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Willkommen hierzuland!", "tokens": ["Will\u00b7kom\u00b7men", "hier\u00b7zu\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leg ab den Mantel, stelle", "tokens": ["Leg", "ab", "den", "Man\u00b7tel", ",", "stel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Stab an diese Wand!", "tokens": ["Den", "Stab", "an", "die\u00b7se", "Wand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sitz obenan zu Tische!", "tokens": ["Sitz", "o\u00b7be\u00b7nan", "zu", "Ti\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Ehre ziemt dem Gast.", "tokens": ["Die", "Eh\u00b7re", "ziemt", "dem", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ich vermag, erfrische", "tokens": ["Was", "ich", "ver\u00b7mag", ",", "er\u00b7fri\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich nach des Tages Last!", "tokens": ["Dich", "nach", "des", "Ta\u00b7ges", "Last", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn ungerechte Rache", "tokens": ["Wenn", "un\u00b7ge\u00b7rech\u00b7te", "Ra\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich aus der Heimat trieb,", "tokens": ["Dich", "aus", "der", "Hei\u00b7mat", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Nimm unter meinem Dache", "tokens": ["Nimm", "un\u00b7ter", "mei\u00b7nem", "Da\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als teurer Freund vorlieb!", "tokens": ["Als", "teu\u00b7rer", "Freund", "vor\u00b7lieb", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nur eins ist, was ich bitte:", "tokens": ["Nur", "eins", "ist", ",", "was", "ich", "bit\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "$,", "PWS", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df du mir ungeschw\u00e4cht", "tokens": ["La\u00df", "du", "mir", "un\u00b7ge\u00b7schw\u00e4cht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der V\u00e4ter fromme Sitte,", "tokens": ["Der", "V\u00e4\u00b7ter", "from\u00b7me", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Hauses heilig Recht!", "tokens": ["Des", "Hau\u00b7ses", "hei\u00b7lig", "Recht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}