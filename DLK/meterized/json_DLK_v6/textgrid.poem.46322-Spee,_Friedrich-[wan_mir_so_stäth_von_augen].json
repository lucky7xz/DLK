{"textgrid.poem.46322": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wan mir so st\u00e4th von augen]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wan mir so st\u00e4th von augen", "tokens": ["Wan", "mir", "so", "st\u00e4\u00b7th", "von", "au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Die regen fallen ab,", "tokens": ["Die", "re\u00b7gen", "fal\u00b7len", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd gsicht will nit mehr taugen,", "tokens": ["Vnd", "gsicht", "will", "nit", "mehr", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch kaum mehr geister hab;", "tokens": ["Auch", "kaum", "mehr", "geis\u00b7ter", "hab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df ich mein haupt geschwinde", "tokens": ["La\u00df", "ich", "mein", "haupt", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zur seiten sincken hinn,", "tokens": ["Zur", "sei\u00b7ten", "sin\u00b7cken", "hinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd seufftz dan also linde,", "tokens": ["Vnd", "seufftz", "dan", "al\u00b7so", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit sanfftem anbegin.", "tokens": ["Mit", "sanff\u00b7tem", "an\u00b7be\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das hertz hebt an zu zagen,", "tokens": ["Das", "hertz", "hebt", "an", "zu", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schlagt an so sittiglich", "tokens": ["Schlagt", "an", "so", "sit\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd fahret fortt zu schlagen", "tokens": ["Vnd", "fah\u00b7ret", "fortt", "zu", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df es laut j\u00e4merlich,", "tokens": ["Da\u00df", "es", "laut", "j\u00e4\u00b7mer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Bald immer, immer weiter", "tokens": ["Bald", "im\u00b7mer", ",", "im\u00b7mer", "wei\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nimbt zu der hertzenschlag;", "tokens": ["Nimbt", "zu", "der", "hert\u00b7zen\u00b7schlag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Die seufftzer auch zur leiter", "tokens": ["Die", "seufft\u00b7zer", "auch", "zur", "lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auffklimmen allgemach.", "tokens": ["Auf\u00b7fklim\u00b7men", "all\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In lufft sie sich erschwingen,", "tokens": ["In", "lufft", "sie", "sich", "er\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit einer m\u00e4nge gro\u00df;", "tokens": ["Mit", "ei\u00b7ner", "m\u00e4n\u00b7ge", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch wolcken sich erdringen,", "tokens": ["Durch", "wol\u00b7cken", "sich", "er\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bi\u00df recht in Gottes scho\u00df.", "tokens": ["Bi\u00df", "recht", "in", "Got\u00b7tes", "scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da zeigens ihm das hertze,", "tokens": ["Da", "zei\u00b7gens", "ihm", "das", "hert\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So sie mitt weg gef\u00fchrt;", "tokens": ["So", "sie", "mitt", "weg", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das brint gleich einer kertze,", "tokens": ["Das", "brint", "gleich", "ei\u00b7ner", "kert\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von Jesu lieb ger\u00fchrt.", "tokens": ["Von", "Je\u00b7su", "lieb", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O frommer Gott so milde,", "tokens": ["O", "from\u00b7mer", "Gott", "so", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Di\u00df hertz wir schencken dir:", "tokens": ["Di\u00df", "hertz", "wir", "schen\u00b7cken", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nims vnter deinem schilde,", "tokens": ["Nims", "vn\u00b7ter", "dei\u00b7nem", "schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wend ab all b\u00f6\u00df begier.", "tokens": ["Wend", "ab", "all", "b\u00f6\u00df", "be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es brint in Jesu liebe,", "tokens": ["Es", "brint", "in", "Je\u00b7su", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von flammen also klar;", "tokens": ["Von", "flam\u00b7men", "al\u00b7so", "klar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wans nur dein athem triebe,", "tokens": ["Wans", "nur", "dein", "at\u00b7hem", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wurds brinnen immerdar.", "tokens": ["Wurds", "brin\u00b7nen", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach bla\u00df hinein so linde,", "tokens": ["Ach", "bla\u00df", "hin\u00b7ein", "so", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df es noch brinnet mehr,", "tokens": ["Da\u00df", "es", "noch", "brin\u00b7net", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd schaff das Jesum finde,", "tokens": ["Vnd", "schaff", "das", "Je\u00b7sum", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warnachs verlanget sehr:", "tokens": ["War\u00b7nachs", "ver\u00b7lan\u00b7get", "sehr", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "M\u00f6gts ie nur einmahl fassen", "tokens": ["M\u00f6gts", "ie", "nur", "ein\u00b7mahl", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jesum sein Ehrenholt,", "tokens": ["Je\u00b7sum", "sein", "Eh\u00b7ren\u00b7holt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nie wurd es dan ablassen,", "tokens": ["Nie", "wurd", "es", "dan", "ab\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es ewig brennen solt.", "tokens": ["Es", "e\u00b7wig", "bren\u00b7nen", "solt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}