{"textgrid.poem.25565": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Engel, der mit leichtem Flug'", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Engel, der mit leichtem Flug'", "tokens": ["Der", "En\u00b7gel", ",", "der", "mit", "leich\u00b7tem", "Flug'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fackel seines sch\u00f6nen Lebens", "tokens": ["Die", "Fa\u00b7ckel", "sei\u00b7nes", "sch\u00f6\u00b7nen", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Helleuchtend sonst voran vor einem ", "tokens": ["Hel\u00b7leuch\u00b7tend", "sonst", "vo\u00b7ran", "vor", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kehrt rasch mit einem mal sie um,", "tokens": ["Kehrt", "rasch", "mit", "ei\u00b7nem", "mal", "sie", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und das Erstaunen fragt vergebens:", "tokens": ["Und", "das", "Er\u00b7stau\u00b7nen", "fragt", "ver\u00b7ge\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Warum so fr\u00fch, o Gott! warum?", "tokens": ["Wa\u00b7rum", "so", "fr\u00fch", ",", "o", "Gott", "!", "wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "FM", "NN", "$.", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu k\u00fchn ist zwar schon diese Frage,", "tokens": ["Zu", "k\u00fchn", "ist", "zwar", "schon", "die\u00b7se", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ADV", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und nur der unersetzliche Verlust", "tokens": ["Und", "nur", "der", "un\u00b7er\u00b7setz\u00b7li\u00b7che", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Macht sie verzeihlich; doch der Klage", "tokens": ["Macht", "sie", "ver\u00b7zeih\u00b7lich", ";", "doch", "der", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "$.", "ADV", "ART", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bleibt's wohl erlaubt, da\u00df sie sich vor die Brust,", "tokens": ["Bleibt's", "wohl", "er\u00b7laubt", ",", "da\u00df", "sie", "sich", "vor", "die", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$,", "KOUS", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "An eines ", "tokens": ["An", "ei\u00b7nes"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "So vielen Witz, so viel Verstand,", "tokens": ["So", "vie\u00b7len", "Witz", ",", "so", "viel", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als wir in ihm verloren haben,", "tokens": ["Als", "wir", "in", "ihm", "ver\u00b7lo\u00b7ren", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Kann selten, selten nur ein Land", "tokens": ["Kann", "sel\u00b7ten", ",", "sel\u00b7ten", "nur", "ein", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "$,", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In einem einz'gen Mann begraben.", "tokens": ["In", "ei\u00b7nem", "einz'\u00b7gen", "Mann", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Denn Einer Kunst nur Meister seyn,", "tokens": ["Denn", "Ei\u00b7ner", "Kunst", "nur", "Meis\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So viel das ist, war dennoch ihm zu wenig,", "tokens": ["So", "viel", "das", "ist", ",", "war", "den\u00b7noch", "ihm", "zu", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PDS", "VAFIN", "$,", "VAFIN", "ADV", "PPER", "APPR", "PIS", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Und schritt er in ein neues Feld hinein:", "tokens": ["Und", "schritt", "er", "in", "ein", "neu\u00b7es", "Feld", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Erobert ward es ganz, und er darin der K\u00f6nig.", "tokens": ["Er\u00b7o\u00b7bert", "ward", "es", "ganz", ",", "und", "er", "da\u00b7rin", "der", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "$,", "KON", "PPER", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Hat er nicht oft in zwanzig Mauren", "tokens": ["Hat", "er", "nicht", "oft", "in", "zwan\u00b7zig", "Mau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die Abende dem Volk' durch Spott verk\u00fcrzt?", "tokens": ["Die", "A\u00b7ben\u00b7de", "dem", "Vol\u00b7k'", "durch", "Spott", "ver\u00b7k\u00fcrzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.22": {"text": "Und das Vergn\u00fcgen, Unschuld zu bedauren,", "tokens": ["Und", "das", "Ver\u00b7gn\u00fc\u00b7gen", ",", "Un\u00b7schuld", "zu", "be\u00b7dau\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Mit s\u00fc\u00dfen Thr\u00e4nen ihm gew\u00fcrzt?", "tokens": ["Mit", "s\u00fc\u00b7\u00dfen", "Thr\u00e4\u00b7nen", "ihm", "ge\u00b7w\u00fcrzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Doch alles das hei\u00dft halb den Mann nur kennen.", "tokens": ["Doch", "al\u00b7les", "das", "hei\u00dft", "halb", "den", "Mann", "nur", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "VVFIN", "ADJD", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ach! wer ihn selbst, wie ich, gekannt,", "tokens": ["Ach", "!", "wer", "ihn", "selbst", ",", "wie", "ich", ",", "ge\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "PPER", "ADV", "$,", "PWAV", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ihn, den wir kaum den Engeln g\u00f6nnen,", "tokens": ["Ihn", ",", "den", "wir", "kaum", "den", "En\u00b7geln", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Der f\u00fchlet, nun er uns entschwand,", "tokens": ["Der", "f\u00fch\u00b7let", ",", "nun", "er", "uns", "ent\u00b7schwand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Im Auge hei\u00dfe Thr\u00e4nen brennen.", "tokens": ["Im", "Au\u00b7ge", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "So heiter, und so offen, und so bieder,", "tokens": ["So", "hei\u00b7ter", ",", "und", "so", "of\u00b7fen", ",", "und", "so", "bie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Wie ihn mein Auge hier im Bilde noch erblickt,", "tokens": ["Wie", "ihn", "mein", "Au\u00b7ge", "hier", "im", "Bil\u00b7de", "noch", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "War auch sein Herz. Ach! nie seh' ich ihn wieder!", "tokens": ["War", "auch", "sein", "Herz", ".", "Ach", "!", "nie", "seh'", "ich", "ihn", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$.", "ITJ", "$.", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Auf immer ist er mir entr\u00fcckt!", "tokens": ["Auf", "im\u00b7mer", "ist", "er", "mir", "ent\u00b7r\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wenn noch zuletzt mit einem Lorbeerkranze", "tokens": ["Wenn", "noch", "zu\u00b7letzt", "mit", "ei\u00b7nem", "Lor\u00b7beer\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Die Dankbarkeit den Sarg des Dichters schm\u00fcckt,", "tokens": ["Die", "Dank\u00b7bar\u00b7keit", "den", "Sarg", "des", "Dich\u00b7ters", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "So ist das mehr, als wenn bei Kerzenglanze", "tokens": ["So", "ist", "das", "mehr", ",", "als", "wenn", "bei", "Ker\u00b7zen\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADV", "$,", "KOKOM", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Die Eitelkeit mit ihrem Firlefanze", "tokens": ["Die", "Ei\u00b7tel\u00b7keit", "mit", "ih\u00b7rem", "Fir\u00b7le\u00b7fan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Des Menschenqu\u00e4lers Leiche dr\u00fcckt.", "tokens": ["Des", "Men\u00b7schen\u00b7qu\u00e4\u00b7lers", "Lei\u00b7che", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Wann tausend F\u00fcrsten l\u00e4ngst vergessen,", "tokens": ["Wann", "tau\u00b7send", "F\u00fcrs\u00b7ten", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Mit sammt dem sammtnen Sarg' voll Schilder und voll Tressen,", "tokens": ["Mit", "sammt", "dem", "sammt\u00b7nen", "Sar\u00b7g'", "voll", "Schil\u00b7der", "und", "voll", "Tres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "In ihrem eignen Lande sind,", "tokens": ["In", "ih\u00b7rem", "eig\u00b7nen", "Lan\u00b7de", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und nach der Zeit ein Sandkorn nur hernieder", "tokens": ["Und", "nach", "der", "Zeit", "ein", "Sand\u00b7korn", "nur", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Aus ihrem Stundenglase rinnt,", "tokens": ["Aus", "ih\u00b7rem", "Stun\u00b7den\u00b7gla\u00b7se", "rinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Schallt ", "tokens": ["Schallt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.44": {"text": "Wohl dir, da\u00df dort in gl\u00fccklichern Gefilden,", "tokens": ["Wohl", "dir", ",", "da\u00df", "dort", "in", "gl\u00fcck\u00b7li\u00b7chern", "Ge\u00b7fil\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Als die du hier durchwandelt hast,", "tokens": ["Als", "die", "du", "hier", "durch\u00b7wan\u00b7delt", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Du ruhig deinen Geist kannst bilden,", "tokens": ["Du", "ru\u00b7hig", "dei\u00b7nen", "Geist", "kannst", "bil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Befreiet von des K\u00f6rpers Last,", "tokens": ["Be\u00b7frei\u00b7et", "von", "des", "K\u00f6r\u00b7pers", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Befreiet von den heuchelnden Zeloten,", "tokens": ["Be\u00b7frei\u00b7et", "von", "den", "heu\u00b7cheln\u00b7den", "Ze\u00b7lo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Die, Sanftmuth lehrten, und voll Wuth,", "tokens": ["Die", ",", "Sanft\u00b7muth", "lehr\u00b7ten", ",", "und", "voll", "Wuth", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "VVFIN", "$,", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Als w\u00e4rest du der H\u00f6lle Brut,", "tokens": ["Als", "w\u00e4\u00b7rest", "du", "der", "H\u00f6l\u00b7le", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Dir mit dem Scheiterhaufen drohten;", "tokens": ["Dir", "mit", "dem", "Schei\u00b7ter\u00b7hau\u00b7fen", "droh\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.52": {"text