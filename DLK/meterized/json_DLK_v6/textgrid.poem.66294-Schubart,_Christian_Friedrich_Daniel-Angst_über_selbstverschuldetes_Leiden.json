{"textgrid.poem.66294": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Angst \u00fcber selbstverschuldetes Leiden", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott! darf ich dir mein schweres Leiden klagen?", "tokens": ["Gott", "!", "darf", "ich", "dir", "mein", "schwe\u00b7res", "Lei\u00b7den", "kla\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die S\u00fcnde hat mich j\u00e4mmerlich zerschlagen;", "tokens": ["Die", "S\u00fcn\u00b7de", "hat", "mich", "j\u00e4m\u00b7mer\u00b7lich", "zer\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich leide schwer,", "tokens": ["Ich", "lei\u00b7de", "schwer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und kann nicht mehr", "tokens": ["Und", "kann", "nicht", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die B\u00fcrde meiner eignen Thorheit tragen.", "tokens": ["Die", "B\u00fcr\u00b7de", "mei\u00b7ner", "eig\u00b7nen", "Thor\u00b7heit", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vor Schaam mu\u00df ich mein Angesicht verh\u00fcllen!", "tokens": ["Vor", "Schaam", "mu\u00df", "ich", "mein", "An\u00b7ge\u00b7sicht", "ver\u00b7h\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich leide nicht um Jesu Christi willen;", "tokens": ["Ich", "lei\u00b7de", "nicht", "um", "Je\u00b7su", "Chris\u00b7ti", "wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wollte gern", "tokens": ["Ich", "woll\u00b7te", "gern"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "F\u00fcr meinen Herrn", "tokens": ["F\u00fcr", "mei\u00b7nen", "Herrn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit M\u00e4rt'rerblut die Opferschale f\u00fcllen.", "tokens": ["Mit", "M\u00e4rt'\u00b7rerblut", "die", "Op\u00b7fer\u00b7scha\u00b7le", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch, ach! ich leide eigner Schulden wegen;", "tokens": ["Doch", ",", "ach", "!", "ich", "lei\u00b7de", "eig\u00b7ner", "Schul\u00b7den", "we\u00b7gen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "PPER", "VVFIN", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein inn'rer Zeuge donnert mir entgegen:", "tokens": ["Mein", "inn'\u00b7rer", "Zeu\u00b7ge", "don\u00b7nert", "mir", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Klag nicht im Wahn", "tokens": ["Klag", "nicht", "im", "Wahn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Den Himmel an;", "tokens": ["Den", "Him\u00b7mel", "an", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Du tr\u00e4gst die Last von wohlverdienten Schl\u00e4gen.", "tokens": ["Du", "tr\u00e4gst", "die", "Last", "von", "wohl\u00b7ver\u00b7dien\u00b7ten", "Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Thorheit hat die Fesseln dir geschmiedet:", "tokens": ["Die", "Thor\u00b7heit", "hat", "die", "Fes\u00b7seln", "dir", "ge\u00b7schmie\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wollust, die in deinem Blute siedet,", "tokens": ["Die", "Wol\u00b7lust", ",", "die", "in", "dei\u00b7nem", "Blu\u00b7te", "sie\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat dich entnervt;", "tokens": ["Hat", "dich", "ent\u00b7nervt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die S\u00fcnde sch\u00e4rft", "tokens": ["Die", "S\u00fcn\u00b7de", "sch\u00e4rft"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das Natterngift, das dir im Herzen w\u00fcthet.", "tokens": ["Das", "Nat\u00b7tern\u00b7gift", ",", "das", "dir", "im", "Her\u00b7zen", "w\u00fct\u00b7het", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gott! es ist wahr, ich habe mich vergangen;", "tokens": ["Gott", "!", "es", "ist", "wahr", ",", "ich", "ha\u00b7be", "mich", "ver\u00b7gan\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Aus eigner Thorheit bin ich krank, gefangen,", "tokens": ["Aus", "eig\u00b7ner", "Thor\u00b7heit", "bin", "ich", "krank", ",", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Geist geschw\u00e4cht;", "tokens": ["Am", "Geist", "ge\u00b7schw\u00e4cht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die S\u00fcnde r\u00e4cht", "tokens": ["Die", "S\u00fcn\u00b7de", "r\u00e4cht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sich f\u00fcrchterlich, und gei\u00dfelt mich mit Schlangen.", "tokens": ["Sich", "f\u00fcrch\u00b7ter\u00b7lich", ",", "und", "gei\u00b7\u00dfelt", "mich", "mit", "Schlan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch eigne Schuld vergr\u00f6\u00dfert nur die Qualen;", "tokens": ["Doch", "eig\u00b7ne", "Schuld", "ver\u00b7gr\u00f6\u00b7\u00dfert", "nur", "die", "Qua\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O! la\u00df mir, Gott, dein Friedensantlitz strahlen!", "tokens": ["O", "!", "la\u00df", "mir", ",", "Gott", ",", "dein", "Frie\u00b7den\u00b7sant\u00b7litz", "strah\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schenk mir die Schuld", "tokens": ["Schenk", "mir", "die", "Schuld"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Nach deiner Huld,", "tokens": ["Nach", "dei\u00b7ner", "Huld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wie kann ich denn mit Leiden dich bezahlen?", "tokens": ["Wie", "kann", "ich", "denn", "mit", "Lei\u00b7den", "dich", "be\u00b7zah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wenn sich ein Kind im Leichtsinn hat verloffen,", "tokens": ["Wenn", "sich", "ein", "Kind", "im", "Leicht\u00b7sinn", "hat", "ver\u00b7lof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So stehn ihm doch die Mutterarme offen,", "tokens": ["So", "stehn", "ihm", "doch", "die", "Mut\u00b7ter\u00b7ar\u00b7me", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So bald es schreit:", "tokens": ["So", "bald", "es", "schreit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Barmherzigkeit!", "tokens": ["Barm\u00b7her\u00b7zig\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sollt' ich von dir nicht gleiches Mitleid hoffen?", "tokens": ["Sollt'", "ich", "von", "dir", "nicht", "glei\u00b7ches", "Mit\u00b7leid", "hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Du hast ja nicht, Mitleidigster von allen,", "tokens": ["Du", "hast", "ja", "nicht", ",", "Mit\u00b7lei\u00b7digs\u00b7ter", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "$,", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Tode eines S\u00fcnders Wohlgefallen;", "tokens": ["Am", "To\u00b7de", "ei\u00b7nes", "S\u00fcn\u00b7ders", "Wohl\u00b7ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du bist ja gut,", "tokens": ["Du", "bist", "ja", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und l\u00e4\u00dfst die Ruth'", "tokens": ["Und", "l\u00e4\u00df\u00b7st", "die", "Ruth'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Im Schwunge oft aus deiner Rechten fallen.", "tokens": ["Im", "Schwun\u00b7ge", "oft", "aus", "dei\u00b7ner", "Rech\u00b7ten", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Die ganze Welt, von Schlangenlist betrogen,", "tokens": ["Die", "gan\u00b7ze", "Welt", ",", "von", "Schlan\u00b7gen\u00b7list", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat sich ihr Elend selber zugezogen;", "tokens": ["Hat", "sich", "ihr", "E\u00b7lend", "sel\u00b7ber", "zu\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch tilgt sie nicht", "tokens": ["Doch", "tilgt", "sie", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dein Zorngericht!", "tokens": ["Dein", "Zorn\u00b7ge\u00b7richt", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Um Jesu willen bist du ihr gewogen.", "tokens": ["Um", "Je\u00b7su", "wil\u00b7len", "bist", "du", "ihr", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Des Sohnes Blut trof auf die Erde nieder", "tokens": ["Des", "Soh\u00b7nes", "Blut", "trof", "auf", "die", "Er\u00b7de", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und heiligte die Abgefallnen wieder;", "tokens": ["Und", "hei\u00b7lig\u00b7te", "die", "Ab\u00b7ge\u00b7fall\u00b7nen", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun sind sie dein,", "tokens": ["Nun", "sind", "sie", "dein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ents\u00fcndigt, rein;", "tokens": ["Ent\u00b7s\u00fcn\u00b7digt", ",", "rein", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Nun sind wir alle deines Sohnes Br\u00fcder.", "tokens": ["Nun", "sind", "wir", "al\u00b7le", "dei\u00b7nes", "Soh\u00b7nes", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "O du, vor dem der Br\u00fcder Seelen schweben,", "tokens": ["O", "du", ",", "vor", "dem", "der", "Br\u00fc\u00b7der", "See\u00b7len", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ART", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieh mich in meiner Trauerzelle beben,", "tokens": ["Sieh", "mich", "in", "mei\u00b7ner", "Trau\u00b7er\u00b7zel\u00b7le", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erbarme dich", "tokens": ["Er\u00b7bar\u00b7me", "dich"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Auch meiner; sprich:", "tokens": ["Auch", "mei\u00b7ner", ";", "sprich", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "$.", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Getrost, mein Sohn, die S\u00fcnd' ist dir vergeben!", "tokens": ["Ge\u00b7trost", ",", "mein", "Sohn", ",", "die", "S\u00fcnd'", "ist", "dir", "ver\u00b7ge\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.pen