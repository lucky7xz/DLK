{"textgrid.poem.37557": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Kapitel", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Pfeife in dem Munde,", "tokens": ["Ei\u00b7ne", "Pfei\u00b7fe", "in", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unterm Arm zwei junge Hunde", "tokens": ["Un\u00b7term", "Arm", "zwei", "jun\u00b7ge", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trug der alte Kaspar Schlich. \u2013", "tokens": ["Trug", "der", "al\u00b7te", "Kas\u00b7par", "Schlich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NE", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauchen kann er f\u00fcrchterlich.", "tokens": ["Rau\u00b7chen", "kann", "er", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, obschon die Pfeife gl\u00fcht,", "tokens": ["Doch", ",", "ob\u00b7schon", "die", "Pfei\u00b7fe", "gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Oh, wie kalt ist sein Gem\u00fct! \u2013", "tokens": ["Oh", ",", "wie", "kalt", "ist", "sein", "Ge\u00b7m\u00fct", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbwozu\u00ab \u2013 lauten seine Worte \u2013", "tokens": ["\u00bb", "wo\u00b7zu", "\u00ab", "\u2013", "lau\u00b7ten", "sei\u00b7ne", "Wor\u00b7te", "\u2013"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$(", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00bbwozu n\u00fctzt mir diese Sorte?", "tokens": ["\u00bb", "wo\u00b7zu", "n\u00fctzt", "mir", "die\u00b7se", "Sor\u00b7te", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Macht sie mir vielleicht Pl\u00e4sier?", "tokens": ["Macht", "sie", "mir", "viel\u00b7leicht", "Pl\u00e4\u00b7sier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Einfach nein! erwidr' ich mir.", "tokens": ["Ein\u00b7fach", "nein", "!", "er\u00b7widr'", "ich", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKANT", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wenn mir aber was nicht lieb,", "tokens": ["Wenn", "mir", "a\u00b7ber", "was", "nicht", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PWS", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Weg damit! ist mein Prinzip.\u00ab", "tokens": ["Weg", "da\u00b7mit", "!", "ist", "mein", "Prin\u00b7zip", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PAV", "$.", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An dem Teiche steht er still,", "tokens": ["An", "dem", "Tei\u00b7che", "steht", "er", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil er sie ertr\u00e4nken will.", "tokens": ["Weil", "er", "sie", "er\u00b7tr\u00e4n\u00b7ken", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00c4ngstlich strampeln beide kleinen", "tokens": ["\u00c4ngst\u00b7lich", "stram\u00b7peln", "bei\u00b7de", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Quadrupeden mit den Beinen;", "tokens": ["Quad\u00b7ru\u00b7pe\u00b7den", "mit", "den", "Bei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die innre Stimme spricht:", "tokens": ["Denn", "die", "inn\u00b7re", "Stim\u00b7me", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Geschichte trau ich nicht! \u2013", "tokens": ["Der", "Ge\u00b7schich\u00b7te", "trau", "ich", "nicht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hubs! fliegt einer schon im Bogen.", "tokens": ["Hubs", "!", "fliegt", "ei\u00b7ner", "schon", "im", "Bo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Plisch! \u2013 da glitscht er in die Wogen.", "tokens": ["Plisch", "!", "\u2013", "da", "glitscht", "er", "in", "die", "Wo\u00b7gen", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hubs! der zweite hinterher.", "tokens": ["Hubs", "!", "der", "zwei\u00b7te", "hin\u00b7ter\u00b7her", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Plum! damit verschwindet er.", "tokens": ["Plum", "!", "da\u00b7mit", "ver\u00b7schwin\u00b7det", "er", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PAV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbabgemacht!\u00ab rief Kaspar Schlich,", "tokens": ["\u00bb", "ab\u00b7ge\u00b7macht", "!", "\u00ab", "rief", "Kas\u00b7par", "Schlich", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$.", "$(", "VVFIN", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dampfte und entfernte sich.", "tokens": ["Dampf\u00b7te", "und", "ent\u00b7fern\u00b7te", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber hier, wie \u00fcberhaupt,", "tokens": ["A\u00b7ber", "hier", ",", "wie", "\u00fc\u00b7ber\u00b7haupt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt es anders, als man glaubt.", "tokens": ["Kommt", "es", "an\u00b7ders", ",", "als", "man", "glaubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Paul und Peter, welche grade", "tokens": ["Paul", "und", "Pe\u00b7ter", ",", "wel\u00b7che", "gra\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KON", "NE", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich entbl\u00f6\u00dft zu einem Bade,", "tokens": ["Sich", "ent\u00b7bl\u00f6\u00dft", "zu", "ei\u00b7nem", "Ba\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gaben still verborgen acht,", "tokens": ["Ga\u00b7ben", "still", "ver\u00b7bor\u00b7gen", "acht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was der b\u00f6se Schlich gemacht.", "tokens": ["Was", "der", "b\u00f6\u00b7se", "Schlich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hurtig und den Fr\u00f6schen gleich", "tokens": ["Hur\u00b7tig", "und", "den", "Fr\u00f6\u00b7schen", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hupfen beide in den Teich.", "tokens": ["Hup\u00b7fen", "bei\u00b7de", "in", "den", "Teich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Jeder bringt in seiner Hand", "tokens": ["Je\u00b7der", "bringt", "in", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen kleinen Hund ans Land.", "tokens": ["Ei\u00b7nen", "klei\u00b7nen", "Hund", "ans", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbpilsen\u00ab \u2013 rief Paul \u2013 \u00bbso nenn ich meinen.\u00ab", "tokens": ["\u00bb", "pil\u00b7sen", "\u00ab", "\u2013", "rief", "Paul", "\u2013", "\u00bb", "so", "nenn", "ich", "mei\u00b7nen", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$(", "$(", "VVFIN", "NE", "$(", "$(", "ADV", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Plum \u2013 so nannte Peter seinen.", "tokens": ["Plum", "\u2013", "so", "nann\u00b7te", "Pe\u00b7ter", "sei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VVFIN", "NE", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und so tragen Paul und Peter", "tokens": ["Und", "so", "tra\u00b7gen", "Paul", "und", "Pe\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NE", "KON", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre beiden kleinen K\u00f6ter", "tokens": ["Ih\u00b7re", "bei\u00b7den", "klei\u00b7nen", "K\u00f6\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eilig, doch mit aller Schonung,", "tokens": ["Ei\u00b7lig", ",", "doch", "mit", "al\u00b7ler", "Scho\u00b7nung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin zur elterlichen Wohnung.", "tokens": ["Hin", "zur", "el\u00b7ter\u00b7li\u00b7chen", "Woh\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}