{"textgrid.poem.59380": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du am Felsenhange standst alleine,", "tokens": ["Wenn", "du", "am", "Fel\u00b7sen\u00b7han\u00b7ge", "standst", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unten im Walde V\u00f6gel seltsam sangen", "tokens": ["Un\u00b7ten", "im", "Wal\u00b7de", "V\u00f6\u00b7gel", "selt\u00b7sam", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "NN", "ADJD", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und H\u00f6rner aus der Ferne irrend klangen,", "tokens": ["Und", "H\u00f6r\u00b7ner", "aus", "der", "Fer\u00b7ne", "ir\u00b7rend", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ob die Heimat dr\u00fcben nach dir weine,", "tokens": ["Als", "ob", "die", "Hei\u00b7mat", "dr\u00fc\u00b7ben", "nach", "dir", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "War's niemals da, als rief die Eine, Deine?", "tokens": ["Wa\u00b7r's", "nie\u00b7mals", "da", ",", "als", "rief", "die", "Ei\u00b7ne", ",", "Dei\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$,", "KOUS", "VVFIN", "ART", "NN", "$,", "PPOSAT", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Lockt dich kein Weh, kein br\u00fcnstiges Verlangen", "tokens": ["Lockt", "dich", "kein", "Weh", ",", "kein", "br\u00fcns\u00b7ti\u00b7ges", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach andrer Zeit, die lange schon vergangen,", "tokens": ["Nach", "an\u00b7drer", "Zeit", ",", "die", "lan\u00b7ge", "schon", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf ewig einzugehn in gr\u00fcne Scheine?", "tokens": ["Auf", "e\u00b7wig", "ein\u00b7zu\u00b7gehn", "in", "gr\u00fc\u00b7ne", "Schei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVIZU", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gebirge dunkelblau steigt aus der Ferne,", "tokens": ["Ge\u00b7bir\u00b7ge", "dun\u00b7kel\u00b7blau", "steigt", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und von den Gipfeln f\u00fchrt des Bundes Bogen", "tokens": ["Und", "von", "den", "Gip\u00b7feln", "f\u00fchrt", "des", "Bun\u00b7des", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Br\u00fccke weit in unbekannte Lande.", "tokens": ["Als", "Br\u00fc\u00b7cke", "weit", "in", "un\u00b7be\u00b7kann\u00b7te", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Geheimnisvoll gehn oben goldne Sterne,", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "gehn", "o\u00b7ben", "gold\u00b7ne", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unten erbraust viel Land in dunklen Wogen \u2013", "tokens": ["Un\u00b7ten", "er\u00b7braust", "viel", "Land", "in", "dunk\u00b7len", "Wo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Was z\u00f6gerst du am unbekannten Rande?", "tokens": ["Was", "z\u00f6\u00b7gerst", "du", "am", "un\u00b7be\u00b7kann\u00b7ten", "Ran\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}