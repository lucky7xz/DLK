{"textgrid.poem.25131": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Morgen war, da sah ich drei Gestalten,", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Morgen war, da sah ich drei Gestalten,", "tokens": ["Ein", "Mor\u00b7gen", "war", ",", "da", "sah", "ich", "drei", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Haupt gesenkt und Hand in Hand geschmiegt,", "tokens": ["Das", "Haupt", "ge\u00b7senkt", "und", "Hand", "in", "Hand", "ge\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie sie feierlich vor\u00fcber wallten", "tokens": ["Und", "wie", "sie", "fei\u00b7er\u00b7lich", "vor\u00b7\u00fc\u00b7ber", "wall\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit sanftem Schritt, von wei\u00dfem Kleid umwiegt,", "tokens": ["Mit", "sanf\u00b7tem", "Schritt", ",", "von", "wei\u00b7\u00dfem", "Kleid", "um\u00b7wiegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wars so, als w\u00fcrde marmornes Gef\u00e4\u00df", "tokens": ["Wars", "so", ",", "als", "w\u00fcr\u00b7de", "mar\u00b7mor\u00b7nes", "Ge\u00b7f\u00e4\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "$,", "KOKOM", "VAFIN", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Rundum gedreht, den Bildschmuck zu besehen,", "tokens": ["Run\u00b7dum", "ge\u00b7dreht", ",", "den", "Bild\u00b7schmuck", "zu", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bis da\u00df des Reigens Anfang wiederkehrt;", "tokens": ["Bis", "da\u00df", "des", "Rei\u00b7gens", "An\u00b7fang", "wie\u00b7der\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So kamen sie, dem Urnenbild gem\u00e4\u00df.", "tokens": ["So", "ka\u00b7men", "sie", ",", "dem", "Ur\u00b7nen\u00b7bild", "ge\u00b7m\u00e4\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wie wir fremd vor mancher Urne stehen,", "tokens": ["Und", "wie", "wir", "fremd", "vor", "man\u00b7cher", "Ur\u00b7ne", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So war auch hier Verstehen mir verwehrt.", "tokens": ["So", "war", "auch", "hier", "Ver\u00b7ste\u00b7hen", "mir", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie kams, da\u00df ich euch Schatten nicht erkannte?", "tokens": ["Wie", "kams", ",", "da\u00df", "ich", "euch", "Schat\u00b7ten", "nicht", "er\u00b7kann\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "KOUS", "PPER", "PPER", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wars Absicht, da\u00df wie starres Maskenbild,", "tokens": ["Wars", "Ab\u00b7sicht", ",", "da\u00df", "wie", "star\u00b7res", "Mas\u00b7ken\u00b7bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Blick verh\u00fcllt, sich keine n\u00e4her wandte,", "tokens": ["Den", "Blick", "ver\u00b7h\u00fcllt", ",", "sich", "kei\u00b7ne", "n\u00e4\u00b7her", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRF", "PIAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Damit nun Tr\u00e4gheit meine Tage f\u00fcllt?", "tokens": ["Da\u00b7mit", "nun", "Tr\u00e4g\u00b7heit", "mei\u00b7ne", "Ta\u00b7ge", "f\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr stahlt euch fort; die Stunde trug so schwer:", "tokens": ["Ihr", "stahlt", "euch", "fort", ";", "die", "Stun\u00b7de", "trug", "so", "schwer", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie Wolkenschwall kam Indolenz geschwommen,", "tokens": ["Wie", "Wol\u00b7ken\u00b7schwall", "kam", "In\u00b7do\u00b7lenz", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In Sommerseligkeit ertrank mein Blick,", "tokens": ["In", "Som\u00b7mer\u00b7se\u00b7lig\u00b7keit", "er\u00b7trank", "mein", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Leid und Freude schmolz im sonnigen Meer.", "tokens": ["Und", "Leid", "und", "Freu\u00b7de", "schmolz", "im", "son\u00b7ni\u00b7gen", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Was mu\u00dftet ihr so mahnend wiederkommen?", "tokens": ["Was", "mu\u00df\u00b7tet", "ihr", "so", "mah\u00b7nend", "wie\u00b7der\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Entschwebt, und la\u00dft mir nichts als nichts zur\u00fcck!", "tokens": ["Ent\u00b7schwebt", ",", "und", "la\u00dft", "mir", "nichts", "als", "nichts", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVIMP", "PPER", "PIS", "KOKOM", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie nahten sich zum drittenmal und wandten", "tokens": ["Sie", "nah\u00b7ten", "sich", "zum", "drit\u00b7ten\u00b7mal", "und", "wand\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Blick nach mir \u2013 und wandelten vorbei;", "tokens": ["Den", "Blick", "nach", "mir", "\u2013", "und", "wan\u00b7del\u00b7ten", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$(", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wollte folgen, meine Pulse brannten.", "tokens": ["Ich", "woll\u00b7te", "fol\u00b7gen", ",", "mei\u00b7ne", "Pul\u00b7se", "brann\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Euch nach! so riefs in mir, ich kenn euch drei!", "tokens": ["Euch", "nach", "!", "so", "riefs", "in", "mir", ",", "ich", "kenn", "euch", "drei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "ADV", "ADV", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "CARD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du bist die Liebe, erste \u2013 sch\u00f6nste Maid!", "tokens": ["Du", "bist", "die", "Lie\u00b7be", ",", "ers\u00b7te", "\u2013", "sch\u00f6ns\u00b7te", "Maid", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADJA", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du zweite: Ehrsucht mit den bleichen Wangen", "tokens": ["Du", "zwei\u00b7te", ":", "Ehr\u00b7sucht", "mit", "den", "blei\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und m\u00fcden Augen, \u2013 ach, sie schlummert nie!", "tokens": ["Und", "m\u00fc\u00b7den", "Au\u00b7gen", ",", "\u2013", "ach", ",", "sie", "schlum\u00b7mert", "nie", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "$(", "XY", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du letzte, viel geschm\u00e4ht in Ha\u00df und Streit,", "tokens": ["Du", "letz\u00b7te", ",", "viel", "ge\u00b7schm\u00e4ht", "in", "Ha\u00df", "und", "Streit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von mir geliebt in schmerzlich s\u00fc\u00dfem Bangen,", "tokens": ["Von", "mir", "ge\u00b7liebt", "in", "schmerz\u00b7lich", "s\u00fc\u00b7\u00dfem", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Du bist mein D\u00e4mon \u2013 du bist Poesie!", "tokens": ["Du", "bist", "mein", "D\u00e4\u00b7mon", "\u2013", "du", "bist", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Sie schwanden \u2013 und ich sehnte mich nach Schwingen.", "tokens": ["Sie", "schwan\u00b7den", "\u2013", "und", "ich", "sehn\u00b7te", "mich", "nach", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Torheit! \u2013 Liebe? Wem erbl\u00fcht sie je? \u2013", "tokens": ["O", "Tor\u00b7heit", "!", "\u2013", "Lie\u00b7be", "?", "Wem", "er\u00b7bl\u00fcht", "sie", "je", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$.", "$(", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Ehrsucht? Was kann arme Ehrsucht bringen?", "tokens": ["Und", "Ehr\u00b7sucht", "?", "Was", "kann", "ar\u00b7me", "Ehr\u00b7sucht", "brin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "PWS", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ist sie mehr als eine Wahnidee! \u2013", "tokens": ["Was", "ist", "sie", "mehr", "als", "ei\u00b7ne", "Wah\u00b7ni\u00b7dee", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PIAT", "KOKOM", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Poesie? Nein, so begl\u00fcckt sie nie \u2013", "tokens": ["Und", "Poe\u00b7sie", "?", "Nein", ",", "so", "be\u00b7gl\u00fcckt", "sie", "nie", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "PTKANT", "$,", "ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mich sicher nie \u2013 wie s\u00fc\u00dfe Sommerstunden,", "tokens": ["Mich", "si\u00b7cher", "nie", "\u2013", "wie", "s\u00fc\u00b7\u00dfe", "Som\u00b7mer\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "$(", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In die des Nichtstuns goldner Honig taut.", "tokens": ["In", "die", "des", "Nichts\u00b7tuns", "gold\u00b7ner", "Ho\u00b7nig", "taut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O hinter Mauern seliger Lethargie", "tokens": ["O", "hin\u00b7ter", "Mau\u00b7ern", "se\u00b7li\u00b7ger", "Le\u00b7thar\u00b7gie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ein Leben leben, fern von Qual und Wunden,", "tokens": ["Ein", "Le\u00b7ben", "le\u00b7ben", ",", "fern", "von", "Qual", "und", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Von Tag und Nacht und hastigem Menschenlaut!", "tokens": ["Von", "Tag", "und", "Nacht", "und", "has\u00b7ti\u00b7gem", "Men\u00b7schen\u00b7laut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Noch einmal nahten sie wie stumme Frage.", "tokens": ["Noch", "ein\u00b7mal", "nah\u00b7ten", "sie", "wie", "stum\u00b7me", "Fra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weshalb? Mit Tr\u00e4umen war mein Schlaf bestickt,", "tokens": ["We\u00b7shalb", "?", "Mit", "Tr\u00e4u\u00b7men", "war", "mein", "Schlaf", "be\u00b7stickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die Seele lag gleich buntdurchbl\u00fchtem Hage,", "tokens": ["Die", "See\u00b7le", "lag", "gleich", "bunt\u00b7durch\u00b7bl\u00fch\u00b7tem", "Ha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Sonnenglanz und Schattenspiel durchblickt;", "tokens": ["Von", "Son\u00b7nen\u00b7glanz", "und", "Schat\u00b7ten\u00b7spiel", "durch\u00b7blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Morgen war bew\u00f6lkt, sein Auge schwer", "tokens": ["Der", "Mor\u00b7gen", "war", "be\u00b7w\u00f6lkt", ",", "sein", "Au\u00b7ge", "schwer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Tr\u00e4nen, doch sie flossen nicht hernieder;", "tokens": ["Von", "Tr\u00e4\u00b7nen", ",", "doch", "sie", "flos\u00b7sen", "nicht", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "punc