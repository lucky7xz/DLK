{"textgrid.poem.26590": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei nicht so mild mit mir, so gut \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei nicht so mild mit mir, so gut \u2013", "tokens": ["Sei", "nicht", "so", "mild", "mit", "mir", ",", "so", "gut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "APPR", "PPER", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn Liebe wird durch leises Hoffen,", "tokens": ["Denn", "Lie\u00b7be", "wird", "durch", "lei\u00b7ses", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wallt auch stiller schon mein Blut:", "tokens": ["Und", "wallt", "auch", "stil\u00b7ler", "schon", "mein", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch bin ich s\u00fc\u00dfer T\u00e4uschung offen.", "tokens": ["Noch", "bin", "ich", "s\u00fc\u00b7\u00dfer", "T\u00e4u\u00b7schung", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch kann ein holdes Angesicht", "tokens": ["Noch", "kann", "ein", "hol\u00b7des", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ruhe tiefes Gl\u00fcck mir rauben,", "tokens": ["Der", "Ru\u00b7he", "tie\u00b7fes", "Gl\u00fcck", "mir", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wei\u00df ich auch, du liebst mich nicht \u2013", "tokens": ["Und", "wei\u00df", "ich", "auch", ",", "du", "liebst", "mich", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich k\u00f6nnte doch vielleicht es glauben.", "tokens": ["Ich", "k\u00f6nn\u00b7te", "doch", "viel\u00b7leicht", "es", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sei nicht so mild mit mir, so gut \u2013", "tokens": ["Sei", "nicht", "so", "mild", "mit", "mir", ",", "so", "gut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "APPR", "PPER", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch bin ich s\u00fc\u00dfer T\u00e4uschung offen.", "tokens": ["Noch", "bin", "ich", "s\u00fc\u00b7\u00dfer", "T\u00e4u\u00b7schung", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wallt auch stiller schon mein Blut:", "tokens": ["Und", "wallt", "auch", "stil\u00b7ler", "schon", "mein", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe wird durch leises Hoffen.", "tokens": ["Die", "Lie\u00b7be", "wird", "durch", "lei\u00b7ses", "Hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}