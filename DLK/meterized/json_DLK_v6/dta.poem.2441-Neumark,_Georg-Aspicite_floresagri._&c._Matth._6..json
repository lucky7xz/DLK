{"dta.poem.2441": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Aspicite floresagri. &c.  Matth.  6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt ihr Bek\u00fcmmerten/ ihr sorgen volle Sinnen/", "tokens": ["Kommt", "ihr", "Be\u00b7k\u00fcm\u00b7mer\u00b7ten", "/", "ihr", "sor\u00b7gen", "vol\u00b7le", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schaut in das weite Feld/ mein/ seht die Blumen", "tokens": ["Schaut", "in", "das", "wei\u00b7te", "Feld", "/", "mein", "/", "seht", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$(", "PPOSAT", "$(", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "an/", "tokens": ["an", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wie sie gekleidet stehn/ da keine doch kan spinnen/", "tokens": ["Wie", "sie", "ge\u00b7klei\u00b7det", "stehn", "/", "da", "kei\u00b7ne", "doch", "kan", "spin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VVINF", "$(", "ADV", "PIAT", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So gehet doch in euch/ und d\u00e4nket wol daran/", "tokens": ["So", "ge\u00b7het", "doch", "in", "euch", "/", "und", "d\u00e4n\u00b7ket", "wol", "da\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "$(", "KON", "VVFIN", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie weit ihr besser seit last eure M\u00fch zerrinnen/", "tokens": ["Wie", "weit", "ihr", "bes\u00b7ser", "seit", "last", "eu\u00b7re", "M\u00fch", "zer\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wandelt nur getrost in rechter Lebensbahn", "tokens": ["Und", "wan\u00b7delt", "nur", "ge\u00b7trost", "in", "rech\u00b7ter", "Le\u00b7bens\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gott siehet eure Noht last ihn davor nur sorgen;", "tokens": ["Gott", "sie\u00b7het", "eu\u00b7re", "Noht", "last", "ihn", "da\u00b7vor", "nur", "sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "PPER", "PAV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gibt er es heute nicht so hoffet auff den Morgen.", "tokens": ["Gibt", "er", "es", "heu\u00b7te", "nicht", "so", "hof\u00b7fet", "auff", "den", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}