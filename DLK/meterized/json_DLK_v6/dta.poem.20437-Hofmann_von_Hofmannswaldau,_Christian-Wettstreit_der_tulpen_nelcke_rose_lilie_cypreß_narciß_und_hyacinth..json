{"dta.poem.20437": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wettstreit der tulpen/ nelcke/ rose/  \n lilie/ cypre\u00df/ narci\u00df und  \n hyacinth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.86", "da:0.13"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der sonnen liebster sohn/ holdselig von gestalt/", "tokens": ["Der", "son\u00b7nen", "liebs\u00b7ter", "sohn", "/", "hold\u00b7se\u00b7lig", "von", "ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ADJD", "APPR", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zarter jahre lentz ist auff dem spiel geblieben:", "tokens": ["Und", "zar\u00b7ter", "jah\u00b7re", "lentz", "ist", "auff", "dem", "spiel", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein eintzig wurff zerstie\u00df das ewig treue lieben.", "tokens": ["Ein", "eint\u00b7zig", "wurff", "zer\u00b7stie\u00df", "das", "e\u00b7wig", "treu\u00b7e", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVFIN", "ART", "ADJD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich sch\u00f6ne Hyacinth ward augenblicklich kalt.", "tokens": ["Ich", "sch\u00f6\u00b7ne", "Hya\u00b7cin\u00b7th", "ward", "au\u00b7gen\u00b7blick\u00b7lich", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ob mein liebster zwar in heisser liebs-gewalt", "tokens": ["Und", "ob", "mein", "liebs\u00b7ter", "zwar", "in", "heis\u00b7ser", "liebs\u00b7ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mich tausendmal gek\u00fcst/ und mund auff mund gerieben/", "tokens": ["Mich", "tau\u00b7send\u00b7mal", "ge\u00b7k\u00fcst", "/", "und", "mund", "auff", "mund", "ge\u00b7rie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$(", "KON", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So muste doch der geist hin in die lufft verstieben.", "tokens": ["So", "mus\u00b7te", "doch", "der", "geist", "hin", "in", "die", "lufft", "ver\u00b7stie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Ph\u00f6bus nahms in acht/ und \u00e4nderte mich bald.", "tokens": ["Der", "Ph\u00f6\u00b7bus", "nahms", "in", "acht", "/", "und", "\u00e4n\u00b7der\u00b7te", "mich", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "APPR", "CARD", "$(", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er sprach: mein trost und licht! du solt noch weiter schimmern/", "tokens": ["Er", "sprach", ":", "mein", "trost", "und", "licht", "!", "du", "solt", "noch", "wei\u00b7ter", "schim\u00b7mern", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "KON", "NN", "$.", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich gebe dir ein kleid/ als wie mein himmel tr\u00e4gt/", "tokens": ["Ich", "ge\u00b7be", "dir", "ein", "kleid", "/", "als", "wie", "mein", "him\u00b7mel", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$(", "KOUS", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die hochverpflichte gunst bleibt bey dir unbewegt.", "tokens": ["Die", "hoch\u00b7ver\u00b7pflich\u00b7te", "gunst", "bleibt", "bey", "dir", "un\u00b7be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wachs/ edle blume/ wachs in Cloris holden zimmern!", "tokens": ["Wachs", "/", "ed\u00b7le", "blu\u00b7me", "/", "wachs", "in", "Clo\u00b7ris", "hol\u00b7den", "zim\u00b7mern", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$(", "PWS", "APPR", "NE", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein gnaden-auge soll dich unabl\u00e4\u00dflich sehn/", "tokens": ["Mein", "gna\u00b7den\u00b7au\u00b7ge", "soll", "dich", "un\u00b7ab\u00b7l\u00e4\u00df\u00b7lich", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der west wird lebhafft warm um deine stengel wehn.", "tokens": ["Der", "west", "wird", "leb\u00b7hafft", "warm", "um", "dei\u00b7ne", "sten\u00b7gel", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}