{"textgrid.poem.53958": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Don't gish me \u2013!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsieh mich nicht so an", "tokens": ["\u00bb", "sieh", "mich", "nicht", "so", "an"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "PTKNEG", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "\u2013 ich kann es nicht ertragen!", "tokens": ["\u2013", "ich", "kann", "es", "nicht", "er\u00b7tra\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sieh mich nicht so an", "tokens": ["Sieh", "mich", "nicht", "so", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "\u2013 mit so viel Schmalz und Schmerz!", "tokens": ["\u2013", "mit", "so", "viel", "Schmalz", "und", "Schmerz", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sieh mich nicht so an", "tokens": ["Sieh", "mich", "nicht", "so", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "\u2013 sonst mu\u00df ich sagen:", "tokens": ["\u2013", "sonst", "mu\u00df", "ich", "sa\u00b7gen", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Schmei\u00dft ihn raus", "tokens": ["Schmei\u00dft", "ihn", "raus"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "\u2013 er zerrei\u00dft mirs Herz \u2013!\u00ab", "tokens": ["\u2013", "er", "zer\u00b7rei\u00dft", "mirs", "Herz", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "NE", "NN", "$(", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn die Amerikanerin an einen Mann ger\u00e4t,", "tokens": ["Wenn", "die", "A\u00b7me\u00b7ri\u00b7ka\u00b7ne\u00b7rin", "an", "ei\u00b7nen", "Mann", "ge\u00b7r\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "an einen richtigen Mann;", "tokens": ["an", "ei\u00b7nen", "rich\u00b7ti\u00b7gen", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "wenn er f\u00fcr sie nicht jede Kiste dreht,", "tokens": ["wenn", "er", "f\u00fcr", "sie", "nicht", "je\u00b7de", "Kis\u00b7te", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKNEG", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "weil er nicht will, weil er nicht kann . . .", "tokens": ["weil", "er", "nicht", "will", ",", "weil", "er", "nicht", "kann", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "$,", "KOUS", "PPER", "PTKNEG", "VMFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "dann schl\u00e4gt sie wie die Gish die Augen auf,", "tokens": ["dann", "schl\u00e4gt", "sie", "wie", "die", "Gish", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "feucht, in der Gro\u00dfaufnahme \u2013", "tokens": ["feucht", ",", "in", "der", "Gro\u00df\u00b7auf\u00b7nah\u00b7me", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "und protzt erfreut", "tokens": ["und", "protzt", "er\u00b7freut"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "mit ihrem Bauch aus Zelluloid", "tokens": ["mit", "ih\u00b7rem", "Bauch", "aus", "Zel\u00b7lu\u00b7lo\u00b7id"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "und ist ein Drittel Kind, ein Drittel Luder und ein Drittel Dame . . .", "tokens": ["und", "ist", "ein", "Drit\u00b7tel", "Kind", ",", "ein", "Drit\u00b7tel", "Lu\u00b7der", "und", "ein", "Drit\u00b7tel", "Da\u00b7me", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "KON", "ART", "NN", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.10": {"text": "\u00bbsieh mich nicht so an", "tokens": ["\u00bb", "sieh", "mich", "nicht", "so", "an"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "PTKNEG", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "\u2013 ich kann es nicht ertragen!", "tokens": ["\u2013", "ich", "kann", "es", "nicht", "er\u00b7tra\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Pl\u00fcsch ist in deinem Aug'", "tokens": ["Pl\u00fcsch", "ist", "in", "dei\u00b7nem", "Aug'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "\u2013 und so viel Gish und Schmerz!", "tokens": ["\u2013", "und", "so", "viel", "Gish", "und", "Schmerz", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.14": {"text": "Trifft mich dein krummbeiniger Blick", "tokens": ["Trifft", "mich", "dein", "krumm\u00b7bei\u00b7ni\u00b7ger", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "\u2013 so mu\u00df ich sagen:", "tokens": ["\u2013", "so", "mu\u00df", "ich", "sa\u00b7gen", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Schmei\u00dft sie raus", "tokens": ["Schmei\u00dft", "sie", "raus"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "\u2013 sie zerrei\u00dft mirs Herz \u2013!\u00ab", "tokens": ["\u2013", "sie", "zer\u00b7rei\u00dft", "mirs", "Herz", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "NE", "NN", "$(", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hat der Germane die Partie verloren", "tokens": ["Hat", "der", "Ger\u00b7ma\u00b7ne", "die", "Par\u00b7tie", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in Fu\u00dfball oder Politik \u2013:", "tokens": ["in", "Fu\u00df\u00b7ball", "o\u00b7der", "Po\u00b7li\u00b7tik", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dann \u00fcbermannt ihn das Gef\u00fchl bis \u00fcber beide Ohren,", "tokens": ["dann", "\u00fc\u00b7berm\u00b7annt", "ihn", "das", "Ge\u00b7f\u00fchl", "bis", "\u00fc\u00b7ber", "bei\u00b7de", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "dann ist er fromm und philosophisch (mit Musik).", "tokens": ["dann", "ist", "er", "fromm", "und", "phi\u00b7lo\u00b7so\u00b7phisch", "(", "mit", "Mu\u00b7sik", ")", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$(", "APPR", "NN", "$(", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Gehts gut, schl\u00e4gt er des Gegners Augen auf;", "tokens": ["Gehts", "gut", ",", "schl\u00e4gt", "er", "des", "Geg\u00b7ners", "Au\u00b7gen", "auf", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "gehts schief, dann wird gesungen", "tokens": ["gehts", "schief", ",", "dann", "wird", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "ein doitsches Lied,", "tokens": ["ein", "doit\u00b7sches", "Lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "weil das ja immer zieht \u2013", "tokens": ["weil", "das", "ja", "im\u00b7mer", "zieht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "er ist ein Drittel Held, ein Drittel Kellner und ein Drittel Nibelungen . . .", "tokens": ["er", "ist", "ein", "Drit\u00b7tel", "Held", ",", "ein", "Drit\u00b7tel", "Kell\u00b7ner", "und", "ein", "Drit\u00b7tel", "Ni\u00b7be\u00b7lun\u00b7gen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "KON", "ART", "NN", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.10": {"text": "\u00bbsieh mich nicht so an", "tokens": ["\u00bb", "sieh", "mich", "nicht", "so", "an"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "PTKNEG", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "\u2013 ich kann es nicht ertragen!", "tokens": ["\u2013", "ich", "kann", "es", "nicht", "er\u00b7tra\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "so mit dem treuen Blick von unten rauf", "tokens": ["so", "mit", "dem", "treu\u00b7en", "Blick", "von", "un\u00b7ten", "rauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "APPR", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "\u2013 und mit dem Wackelsterz!", "tokens": ["\u2013", "und", "mit", "dem", "Wa\u00b7ckels\u00b7terz", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Ich kenn dich noch aus alten, b\u00f6sen Tagen \u2013", "tokens": ["Ich", "kenn", "dich", "noch", "aus", "al\u00b7ten", ",", "b\u00f6\u00b7sen", "Ta\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "die Hand in der Bilanz \u2013 das Auge himmelw\u00e4rts!", "tokens": ["die", "Hand", "in", "der", "Bi\u00b7lanz", "\u2013", "das", "Au\u00b7ge", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und ist das Ausland klug, so wird es sagen:", "tokens": ["Und", "ist", "das", "Aus\u00b7land", "klug", ",", "so", "wird", "es", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Schmei\u00dft ihn raus \u2013 er bricht mirs Herz \u2013!\u00ab", "tokens": ["Schmei\u00dft", "ihn", "raus", "\u2013", "er", "bricht", "mirs", "Herz", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$(", "PPER", "VVFIN", "NE", "NN", "$(", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur ungern nimmt der Handelsmann", "tokens": ["Nur", "un\u00b7gern", "nimmt", "der", "Han\u00b7dels\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "statt baren Geldes Breitscheid an.", "tokens": ["statt", "ba\u00b7ren", "Gel\u00b7des", "Breit\u00b7scheid", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "PTKVZ", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}}}}