{"textgrid.poem.49826": {"metadata": {"author": {"name": "Sachs, Hans", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein hausmaus die gieng \u00fcber felt,", "genre": "verse", "period": "N.A.", "pub_year": 1558, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein hausmaus die gieng \u00fcber felt,", "tokens": ["Ein", "haus\u00b7maus", "die", "gieng", "\u00fc\u00b7ber", "felt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "het doch weder zerung noch gelt;", "tokens": ["het", "doch", "we\u00b7der", "ze\u00b7rung", "noch", "gelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der begegnet da ein feltmaus,", "tokens": ["der", "be\u00b7geg\u00b7net", "da", "ein", "felt\u00b7maus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "dieselbige bat sie zu haus,", "tokens": ["die\u00b7sel\u00b7bi\u00b7ge", "bat", "sie", "zu", "haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "die nachtherberg bei ir zu han.", "tokens": ["die", "nacht\u00b7her\u00b7berg", "bei", "ir", "zu", "han", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "das nam die hausmaus willig an,", "tokens": ["das", "nam", "die", "haus\u00b7maus", "wil\u00b7lig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "gieng mit ir in ein hecken nein,", "tokens": ["gieng", "mit", "ir", "in", "ein", "he\u00b7cken", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "PTKANT", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "da schloffen sie in ein l\u00f6chlein.", "tokens": ["da", "schlof\u00b7fen", "sie", "in", "ein", "l\u00f6c\u00b7hlein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "die feltmaus gar freuntlicher weis", "tokens": ["die", "felt\u00b7maus", "gar", "freunt\u00b7li\u00b7cher", "weis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "PTKVZ"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "ir f\u00fcrsetzt ir geringe speis,", "tokens": ["ir", "f\u00fcr\u00b7setzt", "ir", "ge\u00b7rin\u00b7ge", "speis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "als eicheln, haseln\u00fc\u00df und koren.", "tokens": ["als", "ei\u00b7cheln", ",", "ha\u00b7sel\u00b7n\u00fc\u00df", "und", "ko\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "als sie waren gesettigt woren,", "tokens": ["als", "sie", "wa\u00b7ren", "ge\u00b7set\u00b7tigt", "wo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "VAPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "schliefens dahin in senfter ru;", "tokens": ["schlie\u00b7fens", "da\u00b7hin", "in", "senf\u00b7ter", "ru", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "aber des andren tages fru", "tokens": ["a\u00b7ber", "des", "an\u00b7dren", "ta\u00b7ges", "fru"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "nam urlaub und ir danken was", "tokens": ["nam", "ur\u00b7laub", "und", "ir", "dan\u00b7ken", "was"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "PPER", "VVFIN", "PWS"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "die hausmaus und zog hin ir stras.", "tokens": ["die", "haus\u00b7maus", "und", "zog", "hin", "ir", "stras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.17": {"text": "als sie nun ir sach richtet aus", "tokens": ["als", "sie", "nun", "ir", "sach", "rich\u00b7tet", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "und wolt widerumb heim zu haus,", "tokens": ["und", "wolt", "wi\u00b7de\u00b7rumb", "heim", "zu", "haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "kerts wider bei der feltmaus ein", "tokens": ["kerts", "wi\u00b7der", "bei", "der", "felt\u00b7maus", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "und saget: liebe schwester mein,", "tokens": ["und", "sa\u00b7get", ":", "lie\u00b7be", "schwes\u00b7ter", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "ADJD", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "du hast mir mitteilt dein armut,", "tokens": ["du", "hast", "mir", "mit\u00b7teilt", "dein", "ar\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.22": {"text": "kom mit mir heim, da ich als gut", "tokens": ["kom", "mit", "mir", "heim", ",", "da", "ich", "als", "gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "KOUS", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.23": {"text": "dir auch wil tun und herberg geben,", "tokens": ["dir", "auch", "wil", "tun", "und", "her\u00b7berg", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "da du solt fr\u00f6lich und wol leben!", "tokens": ["da", "du", "solt", "fr\u00f6\u00b7lich", "und", "wol", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "da gieng mit ir heim die feltmaus", "tokens": ["da", "gieng", "mit", "ir", "heim", "die", "felt\u00b7maus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "in ein sch\u00f6n k\u00f6stliches steinhaus,", "tokens": ["in", "ein", "sch\u00f6n", "k\u00f6st\u00b7li\u00b7ches", "stein\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.27": {"text": "in die speiskamer schloffens frei,", "tokens": ["in", "die", "speis\u00b7ka\u00b7mer", "schlof\u00b7fens", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "darin sie funden mancherlei", "tokens": ["da\u00b7rin", "sie", "fun\u00b7den", "man\u00b7cher\u00b7lei"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "der guten speis von fleisch und fisch,", "tokens": ["der", "gu\u00b7ten", "speis", "von", "fleisch", "und", "fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "was man aufhub vons herren tisch,", "tokens": ["was", "man", "auf\u00b7hub", "vons", "her\u00b7ren", "tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "NE", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.31": {"text": "confect, rosin, mandel und feigen,", "tokens": ["con\u00b7fect", ",", "ro\u00b7sin", ",", "man\u00b7del", "und", "fei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "das tet sie als der feltmaus zeigen", "tokens": ["das", "tet", "sie", "als", "der", "felt\u00b7maus", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "KOUS", "ART", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.33": {"text": "und sprach: hie tu trinken und e\u00dfen", "tokens": ["und", "sprach", ":", "hie", "tu", "trin\u00b7ken", "und", "e\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADV", "ADJD", "VVINF", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.34": {"text": "und deiner armut gar verge\u00dfen.", "tokens": ["und", "dei\u00b7ner", "ar\u00b7mut", "gar", "ver\u00b7ge\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "die feltmaus a\u00df, war wolgemut", "tokens": ["die", "felt\u00b7maus", "a\u00df", ",", "war", "wol\u00b7ge\u00b7mut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "und sprach: wie hast du es so gut!", "tokens": ["und", "sprach", ":", "wie", "hast", "du", "es", "so", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "n\u00f6ten bist du so feist und vol.", "tokens": ["n\u00f6\u00b7ten", "bist", "du", "so", "feist", "und", "vol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.38": {"text": "sie sprach: teglich leb ich so wol,", "tokens": ["sie", "sprach", ":", "teg\u00b7lich", "leb", "ich", "so", "wol", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADJD", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.39": {"text": "so must du mit hartseling dingen", "tokens": ["so", "must", "du", "mit", "hart\u00b7se\u00b7ling", "din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.40": {"text": "dein spr\u00f6d narung zu wegen bringen.", "tokens": ["dein", "spr\u00f6d", "na\u00b7rung", "zu", "we\u00b7gen", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "APPR", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.41": {"text": "wilt du, so magst du bei mir bleiben,", "tokens": ["wilt", "du", ",", "so", "magst", "du", "bei", "mir", "blei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "dein zeit in disem haus vertreiben,", "tokens": ["dein", "zeit", "in", "di\u00b7sem", "haus", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "also wol leben f\u00fcr und f\u00fcr.", "tokens": ["al\u00b7so", "wol", "le\u00b7ben", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.44": {"text": "in dem da rumpelt an der t\u00fcr", "tokens": ["in", "dem", "da", "rum\u00b7pelt", "an", "der", "t\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "der kelner, spert auf, gieng hinein;", "tokens": ["der", "kel\u00b7ner", ",", "spert", "auf", ",", "gieng", "hin\u00b7ein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "die meus erschrakn, doch schlof balt ein", "tokens": ["die", "meus", "er\u00b7schrakn", ",", "doch", "schlof", "balt", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "ADV", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "die hausmaus unten in der want", "tokens": ["die", "haus\u00b7maus", "un\u00b7ten", "in", "der", "want"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "in ein l\u00f6chlein, ir wolbekant;", "tokens": ["in", "ein", "l\u00f6c\u00b7hlein", ",", "ir", "wol\u00b7be\u00b7kant", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "die feltmaus stunt in gro\u0