{"textgrid.poem.65406": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Troph\u00e4en des Trinkers", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So hab' ich endlich ihn bezwungen,", "tokens": ["So", "hab'", "ich", "end\u00b7lich", "ihn", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Knaben, der die Welt bezwingt!", "tokens": ["Den", "Kna\u00b7ben", ",", "der", "die", "Welt", "be\u00b7zwingt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe m\u00fcde mich gerungen,", "tokens": ["Ich", "ha\u00b7be", "m\u00fc\u00b7de", "mich", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum, Br\u00fcder, kommt zu mir und trinkt!", "tokens": ["Drum", ",", "Br\u00fc\u00b7der", ",", "kommt", "zu", "mir", "und", "trinkt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er griff mich an in diesem Keller", "tokens": ["Er", "griff", "mich", "an", "in", "die\u00b7sem", "Kel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und stie\u00df an's Glas mir ohne Scham,", "tokens": ["Und", "stie\u00df", "an's", "Glas", "mir", "oh\u00b7ne", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als eben meinen leeren Teller", "tokens": ["Als", "e\u00b7ben", "mei\u00b7nen", "lee\u00b7ren", "Tel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kellnerin vom Tische nahm.", "tokens": ["Die", "Kell\u00b7ne\u00b7rin", "vom", "Ti\u00b7sche", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Schaum bespritzte mir die Nase,", "tokens": ["Der", "Schaum", "be\u00b7spritz\u00b7te", "mir", "die", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und solch ein Nie\u00dfen kam mich an,", "tokens": ["Und", "solch", "ein", "Nie\u00b7\u00dfen", "kam", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df aus dem \u00fcbervollen Glase", "tokens": ["Da\u00df", "aus", "dem", "\u00fc\u00b7ber\u00b7vol\u00b7len", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wein mir in den \u00c4rmel rann.", "tokens": ["Der", "Wein", "mir", "in", "den", "\u00c4r\u00b7mel", "rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er lachte hinter meinem Stuhle,", "tokens": ["Er", "lach\u00b7te", "hin\u00b7ter", "mei\u00b7nem", "Stuh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sprang ich auf und fa\u00dft' ihn ba\u00df,", "tokens": ["Da", "sprang", "ich", "auf", "und", "fa\u00dft'", "ihn", "ba\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und leicht, wie eine Federspule,", "tokens": ["Und", "leicht", ",", "wie", "ei\u00b7ne", "Fe\u00b7der\u00b7spu\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Warf ich ihn nieder auf ein Fa\u00df.", "tokens": ["Warf", "ich", "ihn", "nie\u00b7der", "auf", "ein", "Fa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da lag er, ohne sich zu regen,", "tokens": ["Da", "lag", "er", ",", "oh\u00b7ne", "sich", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUI", "PRF", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schrie und schluchzte j\u00e4mmerlich.", "tokens": ["Und", "schrie", "und", "schluchz\u00b7te", "j\u00e4m\u00b7mer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lie\u00df mein gutes Herz bewegen,", "tokens": ["Ich", "lie\u00df", "mein", "gu\u00b7tes", "Herz", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach zu ihm: So trolle dich!", "tokens": ["Und", "sprach", "zu", "ihm", ":", "So", "trol\u00b7le", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch seht, was ich ihm abgenommen,", "tokens": ["Doch", "seht", ",", "was", "ich", "ihm", "ab\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eh' ich ihn aus der Th\u00fcre lie\u00df!", "tokens": ["Eh'", "ich", "ihn", "aus", "der", "Th\u00fc\u00b7re", "lie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun mag er immer wiederkommen,", "tokens": ["Nun", "mag", "er", "im\u00b7mer", "wie\u00b7der\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ritter ohne Schild und Spie\u00df!", "tokens": ["Der", "Rit\u00b7ter", "oh\u00b7ne", "Schild", "und", "Spie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Zum ersten seine Augenbinde,", "tokens": ["Zum", "ers\u00b7ten", "sei\u00b7ne", "Au\u00b7gen\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die dient mir jetzt zum Tellertuch,", "tokens": ["Die", "dient", "mir", "jetzt", "zum", "Tel\u00b7ler\u00b7tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, wenn ich abgenutzt sie finde,", "tokens": ["Und", ",", "wenn", "ich", "ab\u00b7ge\u00b7nutzt", "sie", "fin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVPP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr einen Spund zum \u00dcberzug.", "tokens": ["F\u00fcr", "ei\u00b7nen", "Spund", "zum", "\u00dc\u00b7ber\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mit seinen scharf gespitzten Pfeilen", "tokens": ["Mit", "sei\u00b7nen", "scharf", "ge\u00b7spitz\u00b7ten", "Pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da bohr' ich meine F\u00e4sser an,", "tokens": ["Da", "bohr'", "ich", "mei\u00b7ne", "F\u00e4s\u00b7ser", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht, da\u00df ich sie auch zuweilen", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "ich", "sie", "auch", "zu\u00b7wei\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Propfenzieher brauchen kann.", "tokens": ["Als", "Prop\u00b7fen\u00b7zie\u00b7her", "brau\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und seine Fackel soll mir leuchten", "tokens": ["Und", "sei\u00b7ne", "Fa\u00b7ckel", "soll", "mir", "leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In schwarzer Nacht aus jedem Schmaus,", "tokens": ["In", "schwar\u00b7zer", "Nacht", "aus", "je\u00b7dem", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn mir der Weg zu glatt will d\u00e4uchten,", "tokens": ["Wenn", "mir", "der", "Weg", "zu", "glatt", "will", "d\u00e4uch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKA", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sich im Wirbel dreht mein Haus.", "tokens": ["Und", "sich", "im", "Wir\u00b7bel", "dreht", "mein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}