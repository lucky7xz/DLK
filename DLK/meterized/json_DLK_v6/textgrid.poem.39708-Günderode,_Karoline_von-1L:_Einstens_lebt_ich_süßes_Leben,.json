{"textgrid.poem.39708": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einstens lebt ich s\u00fc\u00dfes Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einstens lebt ich s\u00fc\u00dfes Leben,", "tokens": ["Eins\u00b7tens", "lebt", "ich", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "denn mir war, als sey ich pl\u00f6tzlich", "tokens": ["denn", "mir", "war", ",", "als", "sey", "ich", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "nur ein duftiges Gew\u00f6lke.", "tokens": ["nur", "ein", "duf\u00b7ti\u00b7ges", "Ge\u00b7w\u00f6l\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uber mir war nichts zu schauen", "tokens": ["U\u00b7ber", "mir", "war", "nichts", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "als ein tiefes blaues Meer", "tokens": ["als", "ein", "tie\u00b7fes", "blau\u00b7es", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und ich schiffte auf den Woogen", "tokens": ["und", "ich", "schiff\u00b7te", "auf", "den", "Woo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "dieses Meeres leicht umher.", "tokens": ["die\u00b7ses", "Mee\u00b7res", "leicht", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Lustig in des Himmels L\u00fcften", "tokens": ["Lus\u00b7tig", "in", "des", "Him\u00b7mels", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "gaukelt ich den ganzen Tag,", "tokens": ["gau\u00b7kelt", "ich", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "lagerte dann froh und gaukelnd", "tokens": ["la\u00b7ger\u00b7te", "dann", "froh", "und", "gau\u00b7kelnd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "hin mich um den Rand der Erde,", "tokens": ["hin", "mich", "um", "den", "Rand", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.12": {"text": "als sie sich der Sonne Armen", "tokens": ["als", "sie", "sich", "der", "Son\u00b7ne", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "dampfend und voll Gluth entri\u00df,", "tokens": ["damp\u00b7fend", "und", "voll", "Gluth", "ent\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "sich zu baden in n\u00e4chtlicher K\u00fchle,", "tokens": ["sich", "zu", "ba\u00b7den", "in", "n\u00e4cht\u00b7li\u00b7cher", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.15": {"text": "sich zu erlaben im Abendwind.", "tokens": ["sich", "zu", "er\u00b7la\u00b7ben", "im", "Ab\u00b7end\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Da umarmte mich die Sonne,", "tokens": ["Da", "um\u00b7arm\u00b7te", "mich", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.17": {"text": "von des Scheidens Weh ergriffen,", "tokens": ["von", "des", "Schei\u00b7dens", "Weh", "er\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "und die sch\u00f6nen hellen Strahlen", "tokens": ["und", "die", "sch\u00f6\u00b7nen", "hel\u00b7len", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "liebten all und k\u00fc\u00dften mich.", "tokens": ["lieb\u00b7ten", "all", "und", "k\u00fc\u00df\u00b7ten", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Farbige Lichter", "tokens": ["Far\u00b7bi\u00b7ge", "Lich\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "stiegen hernieder,", "tokens": ["stie\u00b7gen", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "h\u00fcpfend und spielend,", "tokens": ["h\u00fcp\u00b7fend", "und", "spie\u00b7lend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "wiegend auf L\u00fcften", "tokens": ["wie\u00b7gend", "auf", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "duftige Glieder.", "tokens": ["duf\u00b7ti\u00b7ge", "Glie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Ihre Gewande", "tokens": ["Ih\u00b7re", "Ge\u00b7wan\u00b7de"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Purpur und Golden", "tokens": ["Pur\u00b7pur", "und", "Gol\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "und wie des Feuers", "tokens": ["und", "wie", "des", "Feu\u00b7ers"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "tiefere Gluthen.", "tokens": ["tie\u00b7fe\u00b7re", "Glu\u00b7then", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Aber sie wurden", "tokens": ["A\u00b7ber", "sie", "wur\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VAFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "bl\u00e4sser und bl\u00e4sser,", "tokens": ["bl\u00e4s\u00b7ser", "und", "bl\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "bleicher die Wangen,", "tokens": ["blei\u00b7cher", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "sterbend die Augen.", "tokens": ["ster\u00b7bend", "die", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "Pl\u00f6tzlich verschwanden", "tokens": ["Pl\u00f6tz\u00b7lich", "ver\u00b7schwan\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.34": {"text": "mir die Gespielen,", "tokens": ["mir", "die", "Ge\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "und als ich traurend", "tokens": ["und", "als", "ich", "trau\u00b7rend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.36": {"text": "nach ihnen blickte,", "tokens": ["nach", "ih\u00b7nen", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.37": {"text": "sah ich den gro\u00dfen", "tokens": ["sah", "ich", "den", "gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.38": {"text": "eilenden Schatten,", "tokens": ["ei\u00b7len\u00b7den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.39": {"text": "der sie verfolgte,", "tokens": ["der", "sie", "ver\u00b7folg\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.40": {"text": "sie zu erhaschen.", "tokens": ["sie", "zu", "er\u00b7ha\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.41": {"text": "Tief noch im Westen", "tokens": ["Tief", "noch", "im", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.42": {"text": "sah ich den goldnen", "tokens": ["sah", "ich", "den", "gold\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "Saum der Gew\u00e4nder.", "tokens": ["Saum", "der", "Ge\u00b7w\u00e4n\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Da erhub ich kleine Schwingen,", "tokens": ["Da", "er\u00b7hub", "ich", "klei\u00b7ne", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "flatterte bald hie bald dort hin,", "tokens": ["flat\u00b7ter\u00b7te", "bald", "hie", "bald", "dort", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.46": {"text": "freute mich des leichten Lebens,", "tokens": ["freu\u00b7te", "mich", "des", "leich\u00b7ten", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "ruhend in dem klaren Aether.", "tokens": ["ru\u00b7hend", "in", "dem", "kla\u00b7ren", "A\u00b7e\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.48": {"text": "Sah jetzt in dem heilig tiefen", "tokens": ["Sah", "jetzt", "in", "dem", "hei\u00b7lig", "tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "unnennbaren Raum der Himmel", "tokens": ["un\u00b7nenn\u00b7ba\u00b7ren", "Raum", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "wunderseltsame Gebilde", "tokens": ["wun\u00b7der\u00b7selt\u00b7sa\u00b7me", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.51": {"text": "und Gestalten sich bewegen.", "tokens": ["und", "Ge\u00b7stal\u00b7ten", "sich", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Ewige G\u00f6tter", "tokens": ["E\u00b7wi\u00b7ge", "G\u00f6t\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.53": {"text": "sa\u00dfen auf Thronen", "tokens": ["sa\u00b7\u00dfen", "auf", "Thro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.54": {"text": "gl\u00e4nzender Sterne,", "tokens": ["gl\u00e4n\u00b7zen\u00b7der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.55": {"text": "schauten einander", "tokens": ["schau\u00b7ten", "ein\u00b7an\u00b7der"], "token_info": ["word", "word"], "pos": ["VVFIN", "PRF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.56": {"text": "seelig und l\u00e4chelnd.", "tokens": ["see\u00b7lig", "und", "l\u00e4\u00b7chelnd", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.57": {"text": "T\u00f6nende Schilde,", "tokens": ["T\u00f6\u00b7nen\u00b7de", "Schil\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.58": {"text": "klingende Speere", "tokens": ["klin\u00b7gen\u00b7de", "Spee\u00b7re"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.59": {"text": "huben gewaltige,", "tokens": ["hu\u00b7ben", "ge\u00b7wal\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJA", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.60": {"text": "streitende Helden;", "tokens": ["strei\u00b7ten\u00b7de", "Hel\u00b7den", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.61": {"text": "Vor ihnen flohen", "tokens": ["Vor", "ih\u00b7nen", "flo\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.62": {"text": "gewaltige Thiere,", "tokens": ["ge\u00b7wal\u00b7ti\u00b7ge", "Thie\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.63": {"text": "andre umwanden", "tokens": ["and\u00b7re", "um\u00b7wan\u00b7den"], "token_info": ["word", "word"], "pos": ["PIS", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.64": {"text": "in breiten Ringen", "tokens": ["in", "brei\u00b7ten", "Rin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.65": {"text": "Erde und Himmel,", "tokens": ["Er\u00b7de", "und", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.66": {"text": "selbst sich verfolgend", "tokens": ["selbst", "sich", "ver\u00b7fol\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PRF", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.67": {"text": "ewig im Kreise.", "tokens": ["e\u00b7wig", "im", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.68": {"text": "Bl\u00fchend voll Anmuth", "tokens": ["Bl\u00fc\u00b7hend", "voll", "An\u00b7muth"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJD", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.69": {"text": "unter den Rohen", "tokens": ["un\u00b7ter", "den", "Ro\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.70": {"text": "stand eine Jungfrau,",