"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.108": {"text": "hin durch die Blumen,", "tokens": ["hin", "durch", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.109": {"text": "tiefer und tiefer,", "tokens": ["tie\u00b7fer", "und", "tie\u00b7fer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.110": {"text": "bis zu dem Schoo\u00dfe", "tokens": ["bis", "zu", "dem", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.111": {"text": "hin, der verh\u00fcllten", "tokens": ["hin", ",", "der", "ver\u00b7h\u00fcll\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKVZ", "$,", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.112": {"text": "Quelle des Lebens.", "tokens": ["Quel\u00b7le", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}