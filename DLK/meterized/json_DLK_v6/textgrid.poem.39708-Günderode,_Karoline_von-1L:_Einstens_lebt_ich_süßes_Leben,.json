ord", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.86": {"text": "Und ich wandte mich zur Erde,", "tokens": ["Und", "ich", "wand\u00b7te", "mich", "zur", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.87": {"text": "wie sie s\u00fc\u00df im trunknen Schlafe", "tokens": ["wie", "sie", "s\u00fc\u00df", "im", "trunk\u00b7nen", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.88": {"text": "sich im Arm des Himmels wiegte.", "tokens": ["sich", "im", "Arm", "des", "Him\u00b7mels", "wieg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.89": {"text": "Leis erklungen nun die Sterne,", "tokens": ["Leis", "er\u00b7klun\u00b7gen", "nun", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.90": {"text": "nicht die sch\u00f6ne Braut zu weken,", "tokens": ["nicht", "die", "sch\u00f6\u00b7ne", "Braut", "zu", "we\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.91": {"text": "und des Himmels L\u00fcfte spielten", "tokens": ["und", "des", "Him\u00b7mels", "L\u00fcf\u00b7te", "spiel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "lin