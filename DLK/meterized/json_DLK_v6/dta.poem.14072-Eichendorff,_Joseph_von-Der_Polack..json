{"dta.poem.14072": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Polack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich aber trank aus Deinem Schuh,", "tokens": ["Ich", "a\u00b7ber", "trank", "aus", "Dei\u00b7nem", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lie\u00df meinen Pelz im Haus", "tokens": ["Lie\u00df", "mei\u00b7nen", "Pelz", "im", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eine Handvoll Haar' dazu,", "tokens": ["Und", "ei\u00b7ne", "Hand\u00b7voll", "Haa\u00b7r'", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ich mach' mir gar nichts draus.", "tokens": ["Ich", "mach'", "mir", "gar", "nichts", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}