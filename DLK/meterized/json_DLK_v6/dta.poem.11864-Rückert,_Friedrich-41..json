{"dta.poem.11864": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was wird nun dieser Tag, der heutige, dir bringen?", "tokens": ["Was", "wird", "nun", "die\u00b7ser", "Tag", ",", "der", "heu\u00b7ti\u00b7ge", ",", "dir", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PDAT", "NN", "$,", "ART", "ADJA", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was wird er lassen dir gelingen und mislingen?", "tokens": ["Was", "wird", "er", "las\u00b7sen", "dir", "ge\u00b7lin\u00b7gen", "und", "mis\u00b7lin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was wirst du Sch\u00f6nes sehn, was wirst du Wahres denken?", "tokens": ["Was", "wirst", "du", "Sch\u00f6\u00b7nes", "sehn", ",", "was", "wirst", "du", "Wah\u00b7res", "den\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJA", "VVINF", "$,", "PWS", "VAFIN", "PPER", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohin wird Geist und Sinn sich heben und sich senken?", "tokens": ["Wo\u00b7hin", "wird", "Geist", "und", "Sinn", "sich", "he\u00b7ben", "und", "sich", "sen\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "NN", "PRF", "VVINF", "KON", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Was er auch bringen mag, du sammle den Ertrag!", "tokens": ["Was", "er", "auch", "brin\u00b7gen", "mag", ",", "du", "samm\u00b7le", "den", "Er\u00b7trag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein jeder Tag ist f\u00fcr den Geist ein Erntetag.", "tokens": ["Ein", "je\u00b7der", "Tag", "ist", "f\u00fcr", "den", "Geist", "ein", "Ern\u00b7te\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}