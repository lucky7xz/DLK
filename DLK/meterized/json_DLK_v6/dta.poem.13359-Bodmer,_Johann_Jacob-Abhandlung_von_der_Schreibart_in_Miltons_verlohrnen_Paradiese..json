{"dta.poem.13359": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Abhandlung  \n von der Schreibart in Miltons  \n verlohrnen Paradiese.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Purpur, der im Westen funkelt,", "tokens": ["Der", "Pur\u00b7pur", ",", "der", "im", "Wes\u00b7ten", "fun\u00b7kelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erblasset in ein falbes ", "tokens": ["Er\u00b7blas\u00b7set", "in", "ein", "fal\u00b7bes"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der unerf\u00fcllte Raum, das ungeheure ", "tokens": ["Der", "un\u00b7er\u00b7f\u00fcll\u00b7te", "Raum", ",", "das", "un\u00b7ge\u00b7heu\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6rt auf, und ist nicht mehr.", "tokens": ["H\u00f6rt", "auf", ",", "und", "ist", "nicht", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KON", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u2012 \u2012 \u2012 Der Wahrheit Heiterkeit", "tokens": ["\u2012", "\u2012", "\u2012", "Der", "Wahr\u00b7heit", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "$(", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "F\u00e4ngt in dem ", "tokens": ["F\u00e4ngt", "in", "dem"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Am ", "tokens": ["Am"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Das Uebel in dem Theil ist Wohl im ", "tokens": ["Das", "Ue\u00b7bel", "in", "dem", "Theil", "ist", "Wohl", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "ADV", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ich will nicht, da\u00df mir zu gefallen,", "tokens": ["Ich", "will", "nicht", ",", "da\u00df", "mir", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mir St\u00e4ubgen von dem grossen ", "tokens": ["Mir", "St\u00e4ub\u00b7gen", "von", "dem", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}