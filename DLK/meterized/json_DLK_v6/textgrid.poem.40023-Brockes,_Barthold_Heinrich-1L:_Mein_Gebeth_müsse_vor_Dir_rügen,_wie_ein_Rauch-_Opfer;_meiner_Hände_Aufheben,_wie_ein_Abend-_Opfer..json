{"textgrid.poem.40023": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Gebeth m\u00fcsse vor Dir r\u00fcgen, wie ein Rauch- Opfer; meiner H\u00e4nde Aufheben, wie ein Abend- Opfer.", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gebeth m\u00fcsse vor Dir r\u00fcgen, wie ein Rauch- Opfer; meiner H\u00e4nde Aufheben, wie ein Abend- Opfer.", "tokens": ["Mein", "Ge\u00b7beth", "m\u00fcs\u00b7se", "vor", "Dir", "r\u00fc\u00b7gen", ",", "wie", "ein", "Rauch", "Op\u00b7fer", ";", "mei\u00b7ner", "H\u00e4n\u00b7de", "Auf\u00b7he\u00b7ben", ",", "wie", "ein", "A\u00b7ben\u00b7d", "Op\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$,", "PWAV", "ART", "TRUNC", "NN", "$.", "PPOSAT", "NN", "NN", "$,", "PWAV", "ART", "TRUNC", "NN", "$."], "meter": "--+---+--+-+--+-+-+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Die schw\u00fchle Mittags-Luft ward allgem\u00e4hlig k\u00fchl,", "tokens": ["Die", "schw\u00fch\u00b7le", "Mit\u00b7tags\u00b7Luft", "ward", "all\u00b7ge\u00b7m\u00e4h\u00b7lig", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil der fast brennende beflammte Sonnen-Strahl", "tokens": ["Weil", "der", "fast", "bren\u00b7nen\u00b7de", "be\u00b7flamm\u00b7te", "Son\u00b7nen\u00b7Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schon Seiten-w\u00e4rts, und nicht wie vor, von oben fiel.", "tokens": ["Schon", "Sei\u00b7ten\u00b7w\u00e4rts", ",", "und", "nicht", "wie", "vor", ",", "von", "o\u00b7ben", "fiel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "KON", "PTKNEG", "KOKOM", "APPR", "$,", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im lichten Schatten lag bereits das nied're Thal;", "tokens": ["Im", "lich\u00b7ten", "Schat\u00b7ten", "lag", "be\u00b7reits", "das", "nie\u00b7d'\u00b7re", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Das flache Feld war, nebst den gr\u00fcnen H\u00f6hen,", "tokens": ["Das", "fla\u00b7che", "Feld", "war", ",", "nebst", "den", "gr\u00fc\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch einen g\u00fcld'nen Glantz, der, wie ein' helle Fluth,", "tokens": ["Durch", "ei\u00b7nen", "g\u00fcld'\u00b7nen", "Glantz", ",", "der", ",", "wie", "ein'", "hel\u00b7le", "Fluth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auf dem bebl\u00fchmten Grase ruht,", "tokens": ["Auf", "dem", "be\u00b7bl\u00fchm\u00b7ten", "Gra\u00b7se", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch eins so herrlich anzusehen.", "tokens": ["Noch", "eins", "so", "herr\u00b7lich", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die, durch das nied're Licht, gezeugten langen Schatten,", "tokens": ["Die", ",", "durch", "das", "nie\u00b7d'\u00b7re", "Licht", ",", "ge\u00b7zeug\u00b7ten", "lan\u00b7gen", "Schat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Die hie und da, wie ungemess'ne Riesen,", "tokens": ["Die", "hie", "und", "da", ",", "wie", "un\u00b7ge\u00b7mess'\u00b7ne", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KON", "ADV", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Von allen H\u00f6hen sich schon ausgestrecket hatten,", "tokens": ["Von", "al\u00b7len", "H\u00f6\u00b7hen", "sich", "schon", "aus\u00b7ge\u00b7stre\u00b7cket", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verdoppelten den Glantz der hell-bestrahlten Wiesen", "tokens": ["Ver\u00b7dop\u00b7pel\u00b7ten", "den", "Glantz", "der", "hell\u00b7be\u00b7strahl\u00b7ten", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.13": {"text": "Durch ihren Gegen-Satz; Der Sonne holder Brand", "tokens": ["Durch", "ih\u00b7ren", "Ge\u00b7gen\u00b7Satz", ";", "Der", "Son\u00b7ne", "hol\u00b7der", "Brand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bestrahlete noch hie und da das Land,", "tokens": ["Be\u00b7strah\u00b7le\u00b7te", "noch", "hie", "und", "da", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Zusamt dem glatten Vieh,", "tokens": ["Zu\u00b7samt", "dem", "glat\u00b7ten", "Vieh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Das Heerden-weis', im Klee, bis an das Knie", "tokens": ["Das", "He\u00b7er\u00b7den\u00b7weis'", ",", "im", "Klee", ",", "bis", "an", "das", "Knie"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Mit regen Kiefern geht,", "tokens": ["Mit", "re\u00b7gen", "Kie\u00b7fern", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.18": {"text": "Und mit der scharfen Zung' sein Futter selber m\u00e4ht.", "tokens": ["Und", "mit", "der", "schar\u00b7fen", "Zung'", "sein", "Fut\u00b7ter", "sel\u00b7ber", "m\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Glantz der rothen Haut, worauf ein Schlag-Licht lag,", "tokens": ["Der", "Glantz", "der", "ro\u00b7then", "Haut", ",", "wo\u00b7rauf", "ein", "Schlag\u00b7Licht", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ward, durch den dunckeln Schatten-Schlag,", "tokens": ["Ward", ",", "durch", "den", "dun\u00b7ckeln", "Schat\u00b7ten\u00b7Schlag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Den es im Grase macht', noch eins so sch\u00f6n.", "tokens": ["Den", "es", "im", "Gra\u00b7se", "macht'", ",", "noch", "eins", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von H\u00e4usern, die entfernet stehn,", "tokens": ["Von", "H\u00e4u\u00b7sern", ",", "die", "ent\u00b7fer\u00b7net", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Worauf der Ziegel Roth, recht wie Zinnober, gl\u00e4ntzet,", "tokens": ["Wo\u00b7rauf", "der", "Zie\u00b7gel", "Roth", ",", "recht", "wie", "Zin\u00b7no\u00b7ber", ",", "gl\u00e4nt\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$,", "ADJD", "KOKOM", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Strahlt durch ein dick Geb\u00fcsch, das ihren Fu\u00df bekr\u00e4ntzet,", "tokens": ["Strahlt", "durch", "ein", "dick", "Ge\u00b7b\u00fcsch", ",", "das", "ih\u00b7ren", "Fu\u00df", "be\u00b7kr\u00b7\u00e4nt\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Aus ihrer Fenster Meng', der Sonne g\u00fcld'ner Blick", "tokens": ["Aus", "ih\u00b7rer", "Fens\u00b7ter", "Meng'", ",", "der", "Son\u00b7ne", "g\u00fcld'\u00b7ner", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So heiter wiederum zur\u00fcck,", "tokens": ["So", "hei\u00b7ter", "wie\u00b7de\u00b7rum", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und blitzt mit solcher glimmen Pracht,", "tokens": ["Und", "blitzt", "mit", "sol\u00b7cher", "glim\u00b7men", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus dem so angenehm bestrahl'ten Gr\u00fcnen,", "tokens": ["Aus", "dem", "so", "an\u00b7ge\u00b7nehm", "be\u00b7strahl'\u00b7ten", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In reger rother Gluht, wie zitternde Rubinen,", "tokens": ["In", "re\u00b7ger", "ro\u00b7ther", "Gluht", ",", "wie", "zit\u00b7tern\u00b7de", "Ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df jedem, der es sieht, das Hertz vor Freude lacht.", "tokens": ["Da\u00df", "je\u00b7dem", ",", "der", "es", "sieht", ",", "das", "Hertz", "vor", "Freu\u00b7de", "lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Recht wie von eckigten geschliff'nen Steinen", "tokens": ["Recht", "wie", "von", "ec\u00b7kig\u00b7ten", "ge\u00b7schliff'\u00b7nen", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Gebroch'ne Strahlen spielend scheinen;", "tokens": ["Ge\u00b7bro\u00b7ch'\u00b7ne", "Strah\u00b7len", "spie\u00b7lend", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "So schienen diese Fenster hier.", "tokens": ["So", "schie\u00b7nen", "die\u00b7se", "Fens\u00b7ter", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Durch die\u00df Gesicht, wie halb entz\u00fccket,", "tokens": ["Durch", "die\u00df", "Ge\u00b7sicht", ",", "wie", "halb", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "$,", "PWAV", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Fieng Belisander an,", "tokens": ["Fi\u00b7eng", "Be\u00b7li\u00b7san\u00b7der", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Sein Abend-Opfer Gott zu bringen,", "tokens": ["Sein", "A\u00b7ben\u00b7dOp\u00b7fer", "Gott", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und Dessen Wercke zu besingen.", "tokens": ["Und", "Des\u00b7sen", "Wer\u00b7cke", "zu", "be\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}