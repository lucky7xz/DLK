{"textgrid.poem.65704": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Musen helft! Der Verse Tyranney", "genre": "verse", "period": "N.A.", "pub_year": 1741, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Musen helft! Der Verse Tyranney", "tokens": ["Ihr", "Mu\u00b7sen", "helft", "!", "Der", "Ver\u00b7se", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist allzu schw\u00e4r. O macht uns endlich frey!", "tokens": ["Ist", "all\u00b7zu", "schw\u00e4r", ".", "O", "macht", "uns", "end\u00b7lich", "frey", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "$.", "NE", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Uns plagt ja schon mit seinem Schellenklang", "tokens": ["Uns", "plagt", "ja", "schon", "mit", "sei\u00b7nem", "Schel\u00b7len\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Feind von Geist und Witz, der Reim, zu lang,", "tokens": ["Der", "Feind", "von", "Geist", "und", "Witz", ",", "der", "Reim", ",", "zu", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,", "ART", "NN", "$,", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der, von den rauhen Barden ausgeheckt,", "tokens": ["Der", ",", "von", "den", "rau\u00b7hen", "Bar\u00b7den", "aus\u00b7ge\u00b7heckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die strenge Herrschaft bis auf uns erstreckt.", "tokens": ["Die", "stren\u00b7ge", "Herr\u00b7schaft", "bis", "auf", "uns", "er\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was schreibt doch noch der de\u00fctsche Dichter-Chor", "tokens": ["Was", "schreibt", "doch", "noch", "der", "de\u00fct\u00b7sche", "Dich\u00b7ter\u00b7Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr eine Versart sich zur Strafe vor;", "tokens": ["F\u00fcr", "ei\u00b7ne", "Ver\u00b7sart", "sich", "zur", "Stra\u00b7fe", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Ein Doppelvers, erdacht zu unsrer Pein!", "tokens": ["Ein", "Dop\u00b7pel\u00b7vers", ",", "er\u00b7dacht", "zu", "uns\u00b7rer", "Pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Zu gro\u00df f\u00fcr Einen und f\u00fcr Zween zu klein.", "tokens": ["Zu", "gro\u00df", "f\u00fcr", "Ei\u00b7nen", "und", "f\u00fcr", "Zween", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "KON", "APPR", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Je mehr er hat, je mehr ihm stets gebricht.", "tokens": ["Je", "mehr", "er", "hat", ",", "je", "mehr", "ihm", "stets", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VAFIN", "$,", "ADV", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Zwelf F\u00fcsse helfen ihm zum lauffen nicht.", "tokens": ["Zwelf", "F\u00fcs\u00b7se", "hel\u00b7fen", "ihm", "zum", "lauf\u00b7fen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPRART", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ihn macht dem Ohr kein Wechsel angenem,", "tokens": ["Ihn", "macht", "dem", "Ohr", "kein", "Wech\u00b7sel", "an\u00b7ge\u00b7nem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIAT", "NN", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und kein geschicktes Ma\u00df dem Sinn bequem.", "tokens": ["Und", "kein", "ge\u00b7schick\u00b7tes", "Ma\u00df", "dem", "Sinn", "be\u00b7quem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Er trabt betr\u00fcbt daher mit schw\u00e4rem Schritt.", "tokens": ["Er", "trabt", "be\u00b7tr\u00fcbt", "da\u00b7her", "mit", "schw\u00e4\u00b7rem", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ein gleicher Tact bestimmt ihm jeden Tritt.", "tokens": ["Ein", "glei\u00b7cher", "Tact", "be\u00b7stimmt", "ihm", "je\u00b7den", "Tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Beym Sechsten stellt auch, wenn er lauffen will,", "tokens": ["Beym", "Sechs\u00b7ten", "stellt", "auch", ",", "wenn", "er", "lauf\u00b7fen", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Das strenge Reimges\u00e4tz ihn immer still.", "tokens": ["Das", "stren\u00b7ge", "Reim\u00b7ge\u00b7s\u00e4tz", "ihn", "im\u00b7mer", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Vernunft und Witz entweicht vor seinem Zwang,", "tokens": ["Ver\u00b7nunft", "und", "Witz", "ent\u00b7weicht", "vor", "sei\u00b7nem", "Zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und findt ihn bald zu kurz, und bald zu lang;", "tokens": ["Und", "findt", "ihn", "bald", "zu", "kurz", ",", "und", "bald", "zu", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,", "KON", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und, wenn sein Tic und Tac best\u00e4ndig schallt,", "tokens": ["Und", ",", "wenn", "sein", "Tic", "und", "Tac", "be\u00b7st\u00e4n\u00b7dig", "schallt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "KON", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Gleich einer Glocke, so entschl\u00e4ft man bald.", "tokens": ["Gleich", "ei\u00b7ner", "Glo\u00b7cke", ",", "so", "ent\u00b7schl\u00e4ft", "man", "bald", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Schau, wie so oft ein Dichter \u00e4ngstlich ringt,", "tokens": ["Schau", ",", "wie", "so", "oft", "ein", "Dich\u00b7ter", "\u00e4ngst\u00b7lich", "ringt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Bis nach den Regeln ihm ein Vers gelingt!", "tokens": ["Bis", "nach", "den", "Re\u00b7geln", "ihm", "ein", "Vers", "ge\u00b7lingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Er martert sich, verdreht, versetzt, verschr\u00e4nkt;", "tokens": ["Er", "mar\u00b7tert", "sich", ",", "ver\u00b7dreht", ",", "ver\u00b7setzt", ",", "ver\u00b7schr\u00e4nkt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Der Sinn wird schwach; die Sprache wird gekr\u00e4nkt.", "tokens": ["Der", "Sinn", "wird", "schwach", ";", "die", "Spra\u00b7che", "wird", "ge\u00b7kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ein Einfall flie\u00dft. Doch kan er nicht bestehn.", "tokens": ["Ein", "Ein\u00b7fall", "flie\u00dft", ".", "Doch", "kan", "er", "nicht", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Warum; Zween F\u00fcsse fehlen noch zu Zehn.", "tokens": ["Wa\u00b7rum", ";", "Zween", "F\u00fcs\u00b7se", "feh\u00b7len", "noch", "zu", "Zehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "NN", "VVFIN", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Was ist zu tuhn? Ein Flickwort k\u00f6mmt herbey,", "tokens": ["Was", "ist", "zu", "tuhn", "?", "Ein", "Flick\u00b7wort", "k\u00f6mmt", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKZU", "VVINF", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Da\u00df die geschworne Zahl nur richtig sey.", "tokens": ["Da\u00df", "die", "ge\u00b7schwor\u00b7ne", "Zahl", "nur", "rich\u00b7tig", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Die Zahl ist ganz. Das Werk will doch nicht fort.", "tokens": ["Die", "Zahl", "ist", "ganz", ".", "Das", "Werk", "will", "doch", "nicht", "fort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "ART", "NN", "VMFIN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Der Abschnidt f\u00e4llt nicht recht auf seinen Ort.", "tokens": ["Der", "Ab\u00b7schnidt", "f\u00e4llt", "nicht", "recht", "auf", "sei\u00b7nen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Nach langer M\u00fch gebihrt man eine Brut,", "tokens": ["Nach", "lan\u00b7ger", "M\u00fch", "ge\u00b7bihrt", "man", "ei\u00b7ne", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.34": {"text": "Von Wind und Luft erf\u00fcllt, f\u00fcr Geist und Blut.", "tokens": ["Von", "Wind", "und", "Luft", "er\u00b7f\u00fcllt", ",", "f\u00fcr", "Geist", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Und ist sie nicht an Kraft und Geiste leer,", "tokens": ["Und", "ist", "sie", "nicht", "an", "Kraft", "und", "Geis\u00b7te", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "So zeigt ihr Leib den Zwang nur desto mehr.", "tokens": ["So", "zeigt", "ihr", "Leib", "den", "Zwang", "nur", "des\u00b7to", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Was Wunder! da\u00df der Britten feiner Ohr", "tokens": ["Was", "Wun\u00b7der", "!", "da\u00df", "der", "Brit\u00b7ten", "fei\u00b7ner", "Ohr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$.", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Ein Reimgeb\u00e4nde sich vorl\u00e4ngst erkohr,", "tokens": ["Ein", "Reim\u00b7ge\u00b7b\u00e4n\u00b7de", "sich", "vor\u00b7l\u00e4ngst", "er\u00b7kohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Das, nicht so sehr vom Regelzwang beschr\u00e4nkt,", "tokens": ["Das", ",", "nicht", "so", "sehr", "vom", "Re\u00b7gel\u00b7zwang", "be\u00b7schr\u00e4nkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PTKNEG", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Sich nach des Dichters Wunsch bequemer lenkt,", "tokens": ["Sich", "nach", "des", "Dich\u00b7ters", "Wunsch", "be\u00b7que\u00b7mer", "lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Bald hier, bald dort den Abschnidt wechselnd stellt,", "tokens": ["Bald", "hier", ",", "bald", "dort", "den", "Ab\u00b7schnidt", "wech\u00b7selnd", "stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Und, wie die Regung will, so la\u00fcfft, als h\u00e4lt.", "tokens": ["Und", ",", "wie", "die", "Re\u00b7gung", "will", ",", "so", "la\u00fcfft", ",", "als", "h\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "VMFIN", "$,", "ADV", "VVFIN", "$,", "KOUS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}