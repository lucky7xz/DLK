{"dta.poem.2228": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man hasse Laster, nicht die Menschen! Ich stimme", "tokens": ["Man", "has\u00b7se", "Las\u00b7ter", ",", "nicht", "die", "Men\u00b7schen", "!", "Ich", "stim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "NN", "$,", "PTKNEG", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "dieser Wahrheit bey,", "tokens": ["die\u00b7ser", "Wahr\u00b7heit", "bey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df Menschen Hassen keine Tugend, wohl aber eine", "tokens": ["Da\u00df", "Men\u00b7schen", "Has\u00b7sen", "kei\u00b7ne", "Tu\u00b7gend", ",", "wohl", "a\u00b7ber", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "PIAT", "NN", "$,", "ADV", "ADV", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Krankheit sey.", "tokens": ["Krank\u00b7heit", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}