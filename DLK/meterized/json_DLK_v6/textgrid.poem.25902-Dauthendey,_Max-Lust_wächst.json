{"textgrid.poem.25902": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Lust w\u00e4chst", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im tiefen Blauen hocken", "tokens": ["Im", "tie\u00b7fen", "Blau\u00b7en", "ho\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wolken mit wei\u00dfen Flie\u00dfen,", "tokens": ["Wol\u00b7ken", "mit", "wei\u00b7\u00dfen", "Flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Sonne sitzt am Wocken,", "tokens": ["Die", "Son\u00b7ne", "sitzt", "am", "Wo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lust w\u00e4chst auf allen Wiesen.", "tokens": ["Lust", "w\u00e4chst", "auf", "al\u00b7len", "Wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Der Roggen reicht uns schon ans Herz,", "tokens": ["Der", "Rog\u00b7gen", "reicht", "uns", "schon", "ans", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Salbei duftet himmelw\u00e4rts,", "tokens": ["Und", "Sal\u00b7bei", "duf\u00b7tet", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Menschen und Tier gehn Zwei und Zwei,", "tokens": ["Men\u00b7schen", "und", "Tier", "gehn", "Zwei", "und", "Zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "CARD", "KON", "CARD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Kein Weg ist mehr von Freude frei.", "tokens": ["Kein", "Weg", "ist", "mehr", "von", "Freu\u00b7de", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}