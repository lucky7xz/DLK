{"textgrid.poem.65123": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df ich l\u00e4ngst schon z\u00e4hle zu den Leichen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich l\u00e4ngst schon z\u00e4hle zu den Leichen,", "tokens": ["Da\u00df", "ich", "l\u00e4ngst", "schon", "z\u00e4h\u00b7le", "zu", "den", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sagt dir's nicht der dorngekr\u00f6nten, bleichen", "tokens": ["Sagt", "dir's", "nicht", "der", "dorn\u00b7ge\u00b7kr\u00f6n\u00b7ten", ",", "blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIS", "PTKNEG", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Stirne Trauermal und Kainszeichen?", "tokens": ["Stir\u00b7ne", "Trau\u00b7er\u00b7mal", "und", "Ka\u00b7ins\u00b7zei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Hat dir meiner Lippen zuckend Beben", "tokens": ["Hat", "dir", "mei\u00b7ner", "Lip\u00b7pen", "zu\u00b7ckend", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tr\u00fcbe Kunde nicht schon l\u00e4ngst gegeben,", "tokens": ["Tr\u00fc\u00b7be", "Kun\u00b7de", "nicht", "schon", "l\u00e4ngst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df vom Schmerz und Schuld zerst\u00f6rt mein Leben?", "tokens": ["Da\u00df", "vom", "Schmerz", "und", "Schuld", "zer\u00b7st\u00f6rt", "mein", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und du wolltest dennoch, dennoch wagen,", "tokens": ["Und", "du", "woll\u00b7test", "den\u00b7noch", ",", "den\u00b7noch", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine Seele jener zuzusagen", "tokens": ["Dei\u00b7ne", "See\u00b7le", "je\u00b7ner", "zu\u00b7zu\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PDAT", "VVIZU"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die des Friedens Gottesbild zerschlagen?", "tokens": ["Die", "des", "Frie\u00b7dens", "Got\u00b7tes\u00b7bild", "zer\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NE", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und so m\u00e4chtig w\u00e4hnst du deine Rechte,", "tokens": ["Und", "so", "m\u00e4ch\u00b7tig", "w\u00e4hnst", "du", "dei\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df sie siegreich aus der Nacht der N\u00e4chte", "tokens": ["Da\u00df", "sie", "sieg\u00b7reich", "aus", "der", "Nacht", "der", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mich zur\u00fcck zum Quell des Lichtes br\u00e4chte?", "tokens": ["Mich", "zu\u00b7r\u00fcck", "zum", "Quell", "des", "Lich\u00b7tes", "br\u00e4ch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Mag dein Angesicht sich drob entf\u00e4rben,", "tokens": ["Mag", "dein", "An\u00b7ge\u00b7sicht", "sich", "drob", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wissen mu\u00dft du's: jedes fromme Werben", "tokens": ["Wis\u00b7sen", "mu\u00dft", "du's", ":", "je\u00b7des", "from\u00b7me", "Wer\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PIS", "$.", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zahlt' ich noch mit Jammer und Verderben.", "tokens": ["Zahlt'", "ich", "noch", "mit", "Jam\u00b7mer", "und", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Meiner nicht, es war des Schicksals Wille!", "tokens": ["Mei\u00b7ner", "nicht", ",", "es", "war", "des", "Schick\u00b7sals", "Wil\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Treten nicht aus ihrer Schauerstille,", "tokens": ["Tre\u00b7ten", "nicht", "aus", "ih\u00b7rer", "Schau\u00b7er\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Soll die einsam tr\u00e4umende Sibylle!", "tokens": ["Soll", "die", "ein\u00b7sam", "tr\u00e4u\u00b7men\u00b7de", "Si\u00b7byl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Wer dem dunkeln Geisterreich verfallen,", "tokens": ["Wer", "dem", "dun\u00b7keln", "Geis\u00b7ter\u00b7reich", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Geb' es auf, in festgeschm\u00fcckten Hallen", "tokens": ["Geb'", "es", "auf", ",", "in", "fest\u00b7ge\u00b7schm\u00fcck\u00b7ten", "Hal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "An Geliebter Seite hinzuwallen.", "tokens": ["An", "Ge\u00b7lieb\u00b7ter", "Sei\u00b7te", "hin\u00b7zu\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Geisterlaute, welche zu ihm dringen,", "tokens": ["Geis\u00b7ter\u00b7lau\u00b7te", ",", "wel\u00b7che", "zu", "ihm", "drin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oeffnen zwischen ihm und ird'schen Dingen", "tokens": ["Oeff\u00b7nen", "zwi\u00b7schen", "ihm", "und", "ird'\u00b7schen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eine Kluft, die nicht zu \u00fcberspringen.", "tokens": ["Ei\u00b7ne", "Kluft", ",", "die", "nicht", "zu", "\u00fc\u00b7bers\u00b7prin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Will er Segen dir und Liebe spenden,", "tokens": ["Will", "er", "Se\u00b7gen", "dir", "und", "Lie\u00b7be", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "PPER", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wehe dir und ihm! In seinen H\u00e4nden", "tokens": ["We\u00b7he", "dir", "und", "ihm", "!", "In", "sei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "KON", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wird das Heil sich bald in Jammer wenden.", "tokens": ["Wird", "das", "Heil", "sich", "bald", "in", "Jam\u00b7mer", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Eifers\u00fcchtig sind die dunkeln M\u00e4chte,", "tokens": ["Ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "sind", "die", "dun\u00b7keln", "M\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unzerrei\u00dfbar ihres Bann's Geflechte,", "tokens": ["Un\u00b7zer\u00b7rei\u00df\u00b7bar", "ih\u00b7res", "Bann's", "Ge\u00b7flech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Strenge wahren sie erworb'ne Rechte.", "tokens": ["Stren\u00b7ge", "wah\u00b7ren", "sie", "er\u00b7wor\u00b7b'\u00b7ne", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "PPER", "ADJA", "NN", "$."], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "F\u00fcr das Herz, das sehnend und verblendet,", "tokens": ["F\u00fcr", "das", "Herz", ",", "das", "seh\u00b7nend", "und", "ver\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihren Walten einmal sich verpf\u00e4ndet,", "tokens": ["Ih\u00b7ren", "Wal\u00b7ten", "ein\u00b7mal", "sich", "ver\u00b7pf\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ist der Erde s\u00fc\u00dfer Traum beendet.", "tokens": ["Ist", "der", "Er\u00b7de", "s\u00fc\u00b7\u00dfer", "Traum", "be\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Seine Liebe kann sein Gl\u00fcck nicht dauern,", "tokens": ["Sei\u00b7ne", "Lie\u00b7be", "kann", "sein", "Gl\u00fcck", "nicht", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus der holden T\u00e4uschung wird's mit Trauern", "tokens": ["Aus", "der", "hol\u00b7den", "T\u00e4u\u00b7schung", "wird's", "mit", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In der Wahrheit Reich zur\u00fcckeschauern.", "tokens": ["In", "der", "Wahr\u00b7heit", "Reich", "zu\u00b7r\u00fc\u00b7cke\u00b7schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Was es aufgebaut, wird es verheeren,", "tokens": ["Was", "es", "auf\u00b7ge\u00b7baut", ",", "wird", "es", "ver\u00b7hee\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wird belastet mit den fremden Z\u00e4hren", "tokens": ["Wird", "be\u00b7las\u00b7tet", "mit", "den", "frem\u00b7den", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und dem eignen Fluche heimw\u00e4rts kehren.", "tokens": ["Und", "dem", "eig\u00b7nen", "Flu\u00b7che", "heim\u00b7w\u00e4rts", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Um in still geheimni\u00dfvollem Walten", "tokens": ["Um", "in", "still", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7lem", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zu vergessen bald, da\u00df sein Erkalten", "tokens": ["Zu", "ver\u00b7ges\u00b7sen", "bald", ",", "da\u00df", "sein", "Er\u00b7kal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ein vertrauend, liebend Herz gespalten.", "tokens": ["Ein", "ver\u00b7trau\u00b7end", ",", "lie\u00b7bend", "Herz", "ge\u00b7spal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "$,", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Was von mir und meiner Lieb' zu hoffen,", "tokens": ["Was", "von", "mir", "und", "mei\u00b7ner", "Lieb'", "zu", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liegt vor deinem Blick nun klar und offen.", "tokens": ["Liegt", "vor", "dei\u00b7nem", "Blick", "nun", "klar", "und", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Flieh den Pfeil, bevor er dich getroffen.", "tokens": ["Flieh", "den", "Pfeil", ",", "be\u00b7vor", "er", "dich", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}