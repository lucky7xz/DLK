{"dta.poem.11427": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der kriegs-knechte einer er\u00f6ffnete seine  \n seite mit einem speer.  \n Joh. 19 v. 34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O ungerechter stich vor dich!", "tokens": ["O", "un\u00b7ge\u00b7rech\u00b7ter", "stich", "vor", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den deine seit\u2019, o Heyland! f\u00fchlet;", "tokens": ["Den", "dei\u00b7ne", "seit'", ",", "o", "Hey\u00b7land", "!", "f\u00fch\u00b7let", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "$,", "FM", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch gantz erw\u00fcnscht vor mich,", "tokens": ["Und", "den\u00b7noch", "gantz", "er\u00b7w\u00fcnscht", "vor", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil er den zorn des vaters k\u00fchlet!", "tokens": ["Weil", "er", "den", "zorn", "des", "va\u00b7ters", "k\u00fch\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Verha\u00dfter und auch liebster spie\u00df!", "tokens": ["Ver\u00b7ha\u00df\u00b7ter", "und", "auch", "liebs\u00b7ter", "spie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du stichst zwar in das wahre leben;", "tokens": ["Du", "stichst", "zwar", "in", "das", "wah\u00b7re", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch nur, um in das paradies", "tokens": ["Doch", "nur", ",", "um", "in", "das", "pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUI", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Uns einen schl\u00fcssel abzugeben.", "tokens": ["Uns", "ei\u00b7nen", "schl\u00fcs\u00b7sel", "ab\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Lag unser heyl bi\u00dfher versteckt?", "tokens": ["Lag", "un\u00b7ser", "heyl", "bi\u00df\u00b7her", "ver\u00b7steckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Lag unser schatz in GOtt vergraben?", "tokens": ["Lag", "un\u00b7ser", "schatz", "in", "Gott", "ver\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So hat, was nirgends war zu haben,", "tokens": ["So", "hat", ",", "was", "nir\u00b7gends", "war", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRELS", "ADV", "VAFIN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Nunmehr des feindes speer entdeckt.", "tokens": ["Nun\u00b7mehr", "des", "fein\u00b7des", "speer", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich k\u00fc\u00df, o himmel! deinen schlu\u00df!", "tokens": ["Ich", "k\u00fc\u00df", ",", "o", "him\u00b7mel", "!", "dei\u00b7nen", "schlu\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Nachdem durch dich der welt zu gute", "tokens": ["Nach\u00b7dem", "durch", "dich", "der", "welt", "zu", "gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ein mord-gewehr zur w\u00fcnschel-ruthe", "tokens": ["Ein", "mord\u00b7ge\u00b7wehr", "zur", "w\u00fcn\u00b7schel\u00b7ru\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und blut zu golde werden mu\u00df.", "tokens": ["Und", "blut", "zu", "gol\u00b7de", "wer\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}