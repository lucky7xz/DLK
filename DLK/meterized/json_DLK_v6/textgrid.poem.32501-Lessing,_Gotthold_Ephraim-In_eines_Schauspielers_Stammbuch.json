{"textgrid.poem.32501": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "In eines Schauspielers Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kunst und Natur", "tokens": ["Kunst", "und", "Na\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Sei auf der B\u00fchne Eines nur;", "tokens": ["Sei", "auf", "der", "B\u00fch\u00b7ne", "Ei\u00b7nes", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Kunst sich in Natur verwandelt,", "tokens": ["Wenn", "Kunst", "sich", "in", "Na\u00b7tur", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann hat Natur mit Kunst gehandelt.", "tokens": ["Dann", "hat", "Na\u00b7tur", "mit", "Kunst", "ge\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}