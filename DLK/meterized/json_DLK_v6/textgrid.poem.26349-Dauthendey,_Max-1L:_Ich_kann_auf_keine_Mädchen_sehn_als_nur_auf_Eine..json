{"text": "Denn was ich stotternd in mir trug,", "tokens": ["Denn", "was", "ich", "stot\u00b7ternd", "in", "mir", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zwingt schon im Flug ein Blick von dir ins Reine.", "tokens": ["Zwingt", "schon", "im", "Flug", "ein", "Blick", "von", "dir", "ins", "Rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich finde keine Wege gut und ohne Steine,", "tokens": ["Ich", "fin\u00b7de", "kei\u00b7ne", "We\u00b7ge", "gut", "und", "oh\u00b7ne", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur den mich dr\u00e4ngt die ", "tokens": ["Nur", "den", "mich", "dr\u00e4ngt", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "PPER", "VVFIN", "ART"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df mein Mund auf dem deinen ruht", "tokens": ["Da\u00df", "mein", "Mund", "auf", "dem", "dei\u00b7nen", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "PPOSAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und dein Blut wird das meine.", "tokens": ["Und", "dein", "Blut", "wird", "das", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PDS", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}