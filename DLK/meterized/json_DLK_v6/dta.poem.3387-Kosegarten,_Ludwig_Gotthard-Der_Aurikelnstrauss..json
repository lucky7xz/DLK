{"dta.poem.3387": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Aurikelnstrauss.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die noch gestern an Sophiens Busen,", "tokens": ["Die", "noch", "ge\u00b7stern", "an", "So\u00b7phi\u00b7ens", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "NE", "NE", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Diesem stolzesten aller Sitze, pr\u00e4chtig bl\u00fchend,", "tokens": ["Die\u00b7sem", "stol\u00b7zes\u00b7ten", "al\u00b7ler", "Sit\u00b7ze", ",", "pr\u00e4ch\u00b7tig", "bl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "PIAT", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aus best\u00e4ubten Kelchen der s\u00fcssen D\u00fcfte", "tokens": ["Aus", "be\u00b7st\u00e4ub\u00b7ten", "Kel\u00b7chen", "der", "s\u00fcs\u00b7sen", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "F\u00fclle verstr\u00f6mtet,", "tokens": ["F\u00fcl\u00b7le", "ver\u00b7str\u00f6m\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Bl\u00fcmchen, wie bleich, wie schlaff und eingesunken,", "tokens": ["Bl\u00fcm\u00b7chen", ",", "wie", "bleich", ",", "wie", "schlaff", "und", "ein\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "KON", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie vergr\u00fcnt und vertrauert hangt ihr heute", "tokens": ["Wie", "ver\u00b7gr\u00fcnt", "und", "ver\u00b7trau\u00b7ert", "hangt", "ihr", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "KON", "VVPP", "VVFIN", "PPER", "ADV"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit gel\u00e4hmtem Stengel, verschrumpften", "tokens": ["Mit", "ge\u00b7l\u00e4hm\u00b7tem", "Sten\u00b7gel", ",", "ver\u00b7schrumpf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Glocken,", "tokens": ["Glo\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Mir an dem Arme!", "tokens": ["Mir", "an", "dem", "Ar\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Bl\u00fcmchen, ihr welktet, und kein s\u00fcsses D\u00fcften", "tokens": ["Bl\u00fcm\u00b7chen", ",", "ihr", "welk\u00b7tet", ",", "und", "kein", "s\u00fcs\u00b7ses", "D\u00fcf\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "KON", "PIAT", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wird dem silbernen Kelch hinfort entschweben,", "tokens": ["Wird", "dem", "sil\u00b7ber\u00b7nen", "Kelch", "hin\u00b7fort", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Kein vor\u00fcberwandelndes M\u00e4gdlein wird euch,", "tokens": ["Kein", "vor\u00b7\u00fc\u00b7ber\u00b7wan\u00b7deln\u00b7des", "M\u00e4gd\u00b7lein", "wird", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Freundliche, pfl\u00fccken.", "tokens": ["Freund\u00b7li\u00b7che", ",", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Bl\u00fcmchen, ihr welkt, und keines M\u00e4gdleins Busen", "tokens": ["Bl\u00fcm\u00b7chen", ",", "ihr", "welkt", ",", "und", "kei\u00b7nes", "M\u00e4gd\u00b7leins", "Bu\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "KON", "PIAT", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wird euch heben hinfort mit schwerem Seufzen,", "tokens": ["Wird", "euch", "he\u00b7ben", "hin\u00b7fort", "mit", "schwe\u00b7rem", "Seuf\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Noch der seufzersteigenden Brust ein trunkner", "tokens": ["Noch", "der", "seuf\u00b7zer\u00b7stei\u00b7gen\u00b7den", "Brust", "ein", "trunk\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "J\u00fcngling euch rauben.", "tokens": ["J\u00fcng\u00b7ling", "euch", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Klage, Sophie, um des jungen Lenzen", "tokens": ["Kla\u00b7ge", ",", "So\u00b7phie", ",", "um", "des", "jun\u00b7gen", "Len\u00b7zen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "KOUI", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Fr\u00fcherbl\u00fchete, fr\u00fchgewelkte Kinder.", "tokens": ["Fr\u00fc\u00b7her\u00b7bl\u00fc\u00b7he\u00b7te", ",", "fr\u00fch\u00b7ge\u00b7welk\u00b7te", "Kin\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Klag' um sie, um jeglicher Erdensch\u00f6ne", "tokens": ["Klag'", "um", "sie", ",", "um", "jeg\u00b7li\u00b7cher", "Er\u00b7den\u00b7sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "KOUI", "PIAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Fl\u00fcchtigen Schimmer.", "tokens": ["Fl\u00fcch\u00b7ti\u00b7gen", "Schim\u00b7mer", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Blumen verwelken. Und der Herbststurm st\u00f6ret", "tokens": ["Blu\u00b7men", "ver\u00b7wel\u00b7ken", ".", "Und", "der", "Herbst\u00b7sturm", "st\u00f6\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$.", "KON", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ab die Bl\u00e4tter. Und Jugendbl\u00fcthen knicket", "tokens": ["Ab", "die", "Bl\u00e4t\u00b7ter", ".", "Und", "Ju\u00b7gend\u00b7bl\u00fc\u00b7then", "kni\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "KON", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Frost des Alters. Alles vergeht \u2014 Sophie,", "tokens": ["Frost", "des", "Al\u00b7ters", ".", "Al\u00b7les", "ver\u00b7geht", "So\u00b7phie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PIS", "VVFIN", "$(", "NE", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ach, auch die Treue?", "tokens": ["Ach", ",", "auch", "die", "Treu\u00b7e", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}