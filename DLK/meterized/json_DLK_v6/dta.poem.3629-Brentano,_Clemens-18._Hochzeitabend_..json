{"dta.poem.3629": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "18.  Hochzeitabend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun mu\u00df ich ihn lieben, nun mu\u00df ich allein,               ", "tokens": ["Nun", "mu\u00df", "ich", "ihn", "lie\u00b7ben", ",", "nun", "mu\u00df", "ich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,", "ADV", "VMFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Des g\u00f6ttlichen Br\u00e4utgams Verlobete seyn!", "tokens": ["Des", "g\u00f6tt\u00b7li\u00b7chen", "Br\u00e4ut\u00b7gams", "Ver\u00b7lo\u00b7be\u00b7te", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "VAINF", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihn lieben ist Freude und selig genug,", "tokens": ["Ihn", "lie\u00b7ben", "ist", "Freu\u00b7de", "und", "se\u00b7lig", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "NN", "KON", "ADJD", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Drum folg ich mit Lust dem heiligen Zug.", "tokens": ["Drum", "folg", "ich", "mit", "Lust", "dem", "hei\u00b7li\u00b7gen", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Was bringet die irdische Liebe als Tod?", "tokens": ["Was", "brin\u00b7get", "die", "ir\u00b7di\u00b7sche", "Lie\u00b7be", "als", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "KOUS", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Was wirken die fleischlichen L\u00fcste als Noth?", "tokens": ["Was", "wir\u00b7ken", "die", "fleischli\u00b7chen", "L\u00fcs\u00b7te", "als", "Noth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "KOUS", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie bald ist ein Blick der Freude vorbei?", "tokens": ["Wie", "bald", "ist", "ein", "Blick", "der", "Freu\u00b7de", "vor\u00b7bei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Da sieht man wie kurz die Eitelkeit sey.", "tokens": ["Da", "sieht", "man", "wie", "kurz", "die", "Ei\u00b7tel\u00b7keit", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der g\u00f6ttliche Funken kann nimmermehr ruhn,", "tokens": ["Der", "g\u00f6tt\u00b7li\u00b7che", "Fun\u00b7ken", "kann", "nim\u00b7mer\u00b7mehr", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als wenn er zum Ursprung sich wieder kann thun;", "tokens": ["Als", "wenn", "er", "zum", "Ur\u00b7sprung", "sich", "wie\u00b7der", "kann", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPRART", "NN", "PRF", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da findet er Lust, da giebt er sich ein,", "tokens": ["Da", "fin\u00b7det", "er", "Lust", ",", "da", "giebt", "er", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da w\u00e4chset sein Licht vom lieblichsten Schein.", "tokens": ["Da", "w\u00e4ch\u00b7set", "sein", "Licht", "vom", "lieb\u00b7lichs\u00b7ten", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wenn er nun w\u00e4chset, so mehrt sich die Kraft,", "tokens": ["Und", "wenn", "er", "nun", "w\u00e4ch\u00b7set", ",", "so", "mehrt", "sich", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Gottes liebreitzendes K\u00fcssen verschafft,", "tokens": ["Die", "Got\u00b7tes", "lieb\u00b7reit\u00b7zen\u00b7des", "K\u00fcs\u00b7sen", "ver\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da stirbet das Fleisch, da lebet der Geist,", "tokens": ["Da", "stir\u00b7bet", "das", "Fleisch", ",", "da", "le\u00b7bet", "der", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Christi Verlobte nun ewiglich hei\u00dft.", "tokens": ["Der", "Chris\u00b7ti", "Ver\u00b7lob\u00b7te", "nun", "e\u00b7wig\u00b7lich", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Und ist dem Verliebten nur Reinheit bewu\u00dft,", "tokens": ["Und", "ist", "dem", "Ver\u00b7lieb\u00b7ten", "nur", "Rein\u00b7heit", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "NN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So \u00f6ffnet sich rein paradiesische Lust;", "tokens": ["So", "\u00f6ff\u00b7net", "sich", "rein", "pa\u00b7ra\u00b7die\u00b7si\u00b7sche", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da k\u00e4mpfet und siegt vereinigte St\u00e4rk,", "tokens": ["Da", "k\u00e4mp\u00b7fet", "und", "siegt", "ver\u00b7ei\u00b7nig\u00b7te", "St\u00e4rk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wird t\u00e4glich erfrischt zum g\u00f6ttlichen Werk.", "tokens": ["Wird", "t\u00e4g\u00b7lich", "er\u00b7frischt", "zum", "g\u00f6tt\u00b7li\u00b7chen", "Werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Bewegst du o Jesu den innersten Grund,", "tokens": ["Be\u00b7wegst", "du", "o", "Je\u00b7su", "den", "in\u00b7ners\u00b7ten", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "FM", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So \u00f6ffnet des Glaubens erweiterter Mund,", "tokens": ["So", "\u00f6ff\u00b7net", "des", "Glau\u00b7bens", "er\u00b7wei\u00b7ter\u00b7ter", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Erf\u00fclle das Herz mit Liebe zu dir,", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "das", "Herz", "mit", "Lie\u00b7be", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und bleibe im Schmerz und Freude bei mir.", "tokens": ["Und", "blei\u00b7be", "im", "Schmerz", "und", "Freu\u00b7de", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KON", "NN", "APPR", "PPER", "$."], "meter": "-+--+-+---", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Genug hast du Liebe, o Liebe f\u00fcr mich,", "tokens": ["Ge\u00b7nug", "hast", "du", "Lie\u00b7be", ",", "o", "Lie\u00b7be", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "FM", "NN", "APPR", "PPER", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Drum such ich sie bei dir mein anderes Ich,", "tokens": ["Drum", "such", "ich", "sie", "bei", "dir", "mein", "an\u00b7de\u00b7res", "Ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "APPR", "PPER", "PPOSAT", "ADJA", "PPER", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Nun sink ich in deine Vollkommenheit ein,", "tokens": ["Nun", "sink", "ich", "in", "dei\u00b7ne", "Voll\u00b7kom\u00b7men\u00b7heit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich kann nicht ohn dich, mein Leben, mehr seyn.", "tokens": ["Ich", "kann", "nicht", "ohn", "dich", ",", "mein", "Le\u00b7ben", ",", "mehr", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "VAINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}