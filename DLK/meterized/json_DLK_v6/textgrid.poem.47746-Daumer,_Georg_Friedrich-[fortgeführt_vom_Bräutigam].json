{"textgrid.poem.47746": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[fortgef\u00fchrt vom Br\u00e4utigam]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fortgef\u00fchrt vom Br\u00e4utigam", "tokens": ["Fort\u00b7ge\u00b7f\u00fchrt", "vom", "Br\u00e4u\u00b7ti\u00b7gam"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Maid, die allgeliebte,", "tokens": ["Ist", "die", "Maid", ",", "die", "all\u00b7ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es h\u00e4rmt sich im Vereine", "tokens": ["Und", "es", "h\u00e4rmt", "sich", "im", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Boden und bewegte Schaar.", "tokens": ["Bo\u00b7den", "und", "be\u00b7weg\u00b7te", "Schaar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wiese trauert, Halme thr\u00e4nen,", "tokens": ["Wie\u00b7se", "trau\u00b7ert", ",", "Hal\u00b7me", "thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00fcbe blicken Waldes S\u00e4ume.", "tokens": ["Tr\u00fc\u00b7be", "bli\u00b7cken", "Wal\u00b7des", "S\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6re, wie die F\u00fcllen wiehern,", "tokens": ["H\u00f6\u00b7re", ",", "wie", "die", "F\u00fcl\u00b7len", "wie\u00b7hern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6re, wie die F\u00e4rsen \u00e4chzen,", "tokens": ["H\u00f6\u00b7re", ",", "wie", "die", "F\u00e4r\u00b7sen", "\u00e4ch\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Herde mit Gebr\u00fclle", "tokens": ["Wie", "die", "Her\u00b7de", "mit", "Ge\u00b7br\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Harret auf die Hei\u00dfersehnte,", "tokens": ["Har\u00b7ret", "auf", "die", "Hei\u00b7\u00dfer\u00b7sehn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf die holde Tr\u00e4nkerin,", "tokens": ["Auf", "die", "hol\u00b7de", "Tr\u00e4n\u00b7ke\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf die treue N\u00e4hrerin,", "tokens": ["Auf", "die", "treu\u00b7e", "N\u00e4h\u00b7re\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Auf die kluge W\u00e4rterin!", "tokens": ["Auf", "die", "klu\u00b7ge", "W\u00e4r\u00b7te\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber, ach, nie kehrt sie wieder,", "tokens": ["A\u00b7ber", ",", "ach", ",", "nie", "kehrt", "sie", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "All zu \u00fcben ihre Pflichten", "tokens": ["All", "zu", "\u00fc\u00b7ben", "ih\u00b7re", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Unerm\u00fcdlich ohne Fehl.", "tokens": ["Un\u00b7er\u00b7m\u00fcd\u00b7lich", "oh\u00b7ne", "Fehl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ehe noch die Sonne blickte,", "tokens": ["E\u00b7he", "noch", "die", "Son\u00b7ne", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "War sie wach und war sie munter,", "tokens": ["War", "sie", "wach", "und", "war", "sie", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Eilte sie zu Stall und Herde,", "tokens": ["Eil\u00b7te", "sie", "zu", "Stall", "und", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "S\u00e4mmtliches Gesch\u00e4ft zu thun.", "tokens": ["S\u00e4mmt\u00b7li\u00b7ches", "Ge\u00b7sch\u00e4ft", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Ohne da\u00df der Vater ahnte,", "tokens": ["Oh\u00b7ne", "da\u00df", "der", "Va\u00b7ter", "ahn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ohne da\u00df die Mutter wu\u00dfte,", "tokens": ["Oh\u00b7ne", "da\u00df", "die", "Mut\u00b7ter", "wu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Wie sie sich den Schlummer raubte,", "tokens": ["Wie", "sie", "sich", "den", "Schlum\u00b7mer", "raub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Reichte sie das Heu, den Hafer,", "tokens": ["Reich\u00b7te", "sie", "das", "Heu", ",", "den", "Ha\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "F\u00fcllte sie den Wassereimer;", "tokens": ["F\u00fcll\u00b7te", "sie", "den", "Was\u00b7se\u00b7rei\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Nichts versah sie, nichts verga\u00df sie,", "tokens": ["Nichts", "ver\u00b7sah", "sie", ",", "nichts", "ver\u00b7ga\u00df", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "PIS", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "So das Eine, wie das Andre,", "tokens": ["So", "das", "Ei\u00b7ne", ",", "wie", "das", "And\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Wichtiges und Geringes that sie", "tokens": ["Wich\u00b7ti\u00b7ges", "und", "Ge\u00b7rin\u00b7ges", "that", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "KON", "NN", "VVFIN", "PPER"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Mit gelenker, stiller Hand.", "tokens": ["Mit", "ge\u00b7len\u00b7ker", ",", "stil\u00b7ler", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Br\u00e4utigam, du hochbegl\u00fcckter!", "tokens": ["Br\u00e4u\u00b7ti\u00b7gam", ",", "du", "hoch\u00b7be\u00b7gl\u00fcck\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Ach, was hast du f\u00fcr ein M\u00e4dchen", "tokens": ["Ach", ",", "was", "hast", "du", "f\u00fcr", "ein", "M\u00e4d\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Unserem Revier entwandt!", "tokens": ["Un\u00b7se\u00b7rem", "Re\u00b7vier", "ent\u00b7wandt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}