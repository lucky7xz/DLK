{"textgrid.poem.42841": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Meine erste Liebe?", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erste Liebe? Ach, ein W\u00fcstling, dessen", "tokens": ["Ers\u00b7te", "Lie\u00b7be", "?", "Ach", ",", "ein", "W\u00fcst\u00b7ling", ",", "des\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "ITJ", "$,", "ART", "NN", "$,", "PRELAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herz so wahllos ist wie meins, so weit,", "tokens": ["Herz", "so", "wahl\u00b7los", "ist", "wie", "meins", ",", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VAFIN", "KOKOM", "NE", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hat die erste Liebe l\u00e4ngst vergessen,", "tokens": ["Hat", "die", "ers\u00b7te", "Lie\u00b7be", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ihn intressiert nur seine Zeit.", "tokens": ["Und", "ihn", "in\u00b7tres\u00b7siert", "nur", "sei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Meine letzte Liebe zu beschreiben,", "tokens": ["Mei\u00b7ne", "letz\u00b7te", "Lie\u00b7be", "zu", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00e4re just so leicht wie indiskret.", "tokens": ["W\u00e4\u00b7re", "just", "so", "leicht", "wie", "in\u00b7dis\u00b7kret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "KOKOM", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Au\u00dferdem? Wird sie die letzte bleiben,", "tokens": ["Au\u00b7\u00dfer\u00b7dem", "?", "Wird", "sie", "die", "letz\u00b7te", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "VAFIN", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Bis ihr Name in der \u00bbWoche\u00ab steht?", "tokens": ["Bis", "ihr", "Na\u00b7me", "in", "der", "\u00bb", "Wo\u00b7che", "\u00ab", "steht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "$(", "NN", "$(", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Meine Abenteuer in der Minne", "tokens": ["Mei\u00b7ne", "A\u00b7bent\u00b7eu\u00b7er", "in", "der", "Min\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00fcssen sehr gedr\u00e4ngt gewesen sein.", "tokens": ["M\u00fcs\u00b7sen", "sehr", "ge\u00b7dr\u00e4ngt", "ge\u00b7we\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "VAPP", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ich auf das erste mich besinne,", "tokens": ["Wenn", "ich", "auf", "das", "ers\u00b7te", "mich", "be\u00b7sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00e4llt mir immer noch ein fr\u00fchres ein.", "tokens": ["F\u00e4llt", "mir", "im\u00b7mer", "noch", "ein", "fr\u00fch\u00b7res", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}