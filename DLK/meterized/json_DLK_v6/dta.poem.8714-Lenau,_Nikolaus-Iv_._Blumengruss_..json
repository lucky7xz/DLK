{"dta.poem.8714": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Iv .  \n  Blumengruss .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Jener Abend war entschwunden;", "tokens": ["Je\u00b7ner", "A\u00b7bend", "war", "ent\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch mit jedem Morgenlichte", "tokens": ["Doch", "mit", "je\u00b7dem", "Mor\u00b7gen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fand Johannes im Gef\u00e4ngni\u00df", "tokens": ["Fand", "Jo\u00b7han\u00b7nes", "im", "Ge\u00b7f\u00e4ng\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NE", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frische Blumen, s\u00fc\u00dfe Fr\u00fcchte.", "tokens": ["Fri\u00b7sche", "Blu\u00b7men", ",", "s\u00fc\u00b7\u00dfe", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind es Fr\u00fcchte nicht von B\u00e4umen,", "tokens": ["Sind", "es", "Fr\u00fcch\u00b7te", "nicht", "von", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die er sah auf seinen Wegen?", "tokens": ["Die", "er", "sah", "auf", "sei\u00b7nen", "We\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hauchten diese Blumen nie noch", "tokens": ["Hauch\u00b7ten", "die\u00b7se", "Blu\u00b7men", "nie", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre D\u00fcfte ihm entgegen? \u2014", "tokens": ["Ih\u00b7re", "D\u00fcf\u00b7te", "ihm", "ent\u00b7ge\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gleich als h\u00e4tte heimlich jemand", "tokens": ["Gleich", "als", "h\u00e4t\u00b7te", "heim\u00b7lich", "je\u00b7mand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VAFIN", "ADJD", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abgeschmeichelt jeder Stelle", "tokens": ["Ab\u00b7ge\u00b7schmei\u00b7chelt", "je\u00b7der", "Stel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine freundlichere Miene,", "tokens": ["Ei\u00b7ne", "freund\u00b7li\u00b7che\u00b7re", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heitert sich die Kerkerzelle.", "tokens": ["Hei\u00b7tert", "sich", "die", "Ker\u00b7ker\u00b7zel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dieses ewig wache Sorgen,", "tokens": ["Die\u00b7ses", "e\u00b7wig", "wa\u00b7che", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob ein Geist es heimlich \u00fcbe,", "tokens": ["Ob", "ein", "Geist", "es", "heim\u00b7lich", "\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allgew\u00e4rtig, ungesehen,", "tokens": ["All\u00b7ge\u00b7w\u00e4r\u00b7tig", ",", "un\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann es jemand als die Liebe? \u2014", "tokens": ["Kann", "es", "je\u00b7mand", "als", "die", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PIS", "KOKOM", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "J\u00fcngling, mit den edlen Freunden,", "tokens": ["J\u00fcng\u00b7ling", ",", "mit", "den", "ed\u00b7len", "Freun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die getreu dir auch im Leide,", "tokens": ["Die", "ge\u00b7treu", "dir", "auch", "im", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist noch eine treue Seele", "tokens": ["Ist", "noch", "ei\u00b7ne", "treu\u00b7e", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir gefolgt in fremdem Kleide.", "tokens": ["Dir", "ge\u00b7folgt", "in", "frem\u00b7dem", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihre Sehnsucht will die Jungfrau", "tokens": ["Ih\u00b7re", "Sehn\u00b7sucht", "will", "die", "Jung\u00b7frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Deinem Blick verborgen halten,", "tokens": ["Dei\u00b7nem", "Blick", "ver\u00b7bor\u00b7gen", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Pflicht des Pagen h\u00fcllen", "tokens": ["In", "die", "Pflicht", "des", "Pa\u00b7gen", "h\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer Liebe stilles Walten.", "tokens": ["Ih\u00b7rer", "Lie\u00b7be", "stil\u00b7les", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und es deckt die Rosenwangen", "tokens": ["Und", "es", "deckt", "die", "Ro\u00b7sen\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gelbe, anget\u00fcnchte Farbe,", "tokens": ["Gel\u00b7be", ",", "an\u00b7ge\u00b7t\u00fcnch\u00b7te", "Far\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es fl\u00fcchtet ihre Stirne", "tokens": ["Und", "es", "fl\u00fcch\u00b7tet", "ih\u00b7re", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter die gemalte Narbe. \u2014", "tokens": ["Un\u00b7ter", "die", "ge\u00b7mal\u00b7te", "Nar\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Kaum erwacht der Tag im Osten,", "tokens": ["Kaum", "er\u00b7wacht", "der", "Tag", "im", "Os\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Schwalbe fr\u00fches Rufen,", "tokens": ["Und", "der", "Schwal\u00b7be", "fr\u00fc\u00b7hes", "Ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eilt auch schon das gute Kl\u00e4rchen", "tokens": ["Eilt", "auch", "schon", "das", "gu\u00b7te", "Kl\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nieder die granitnen Stufen.", "tokens": ["Nie\u00b7der", "die", "gra\u00b7nit\u00b7nen", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ueber Felsen, Thal und Wiesen", "tokens": ["Ue\u00b7ber", "Fel\u00b7sen", ",", "Thal", "und", "Wie\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandert sie wohl eine Meile", "tokens": ["Wan\u00b7dert", "sie", "wohl", "ei\u00b7ne", "Mei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Garten ihrer Mutter", "tokens": ["Nach", "dem", "Gar\u00b7ten", "ih\u00b7rer", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fort in rastlos froher Eile.", "tokens": ["Fort", "in", "rast\u00b7los", "fro\u00b7her", "Ei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Was an sch\u00f6nen, frischen Blumen", "tokens": ["Was", "an", "sch\u00f6\u00b7nen", ",", "fri\u00b7schen", "Blu\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Beeten ist zu finden,", "tokens": ["In", "den", "Bee\u00b7ten", "ist", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfl\u00fccket sie, mit klugem Finger", "tokens": ["Pfl\u00fc\u00b7cket", "sie", ",", "mit", "klu\u00b7gem", "Fin\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm den Morgengru\u00df zu winden.", "tokens": ["Ihm", "den", "Mor\u00b7gen\u00b7gru\u00df", "zu", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und sie blicket, Fr\u00fcchte suchend", "tokens": ["Und", "sie", "bli\u00b7cket", ",", "Fr\u00fcch\u00b7te", "su\u00b7chend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach den B\u00e4umen in der Runde,", "tokens": ["Nach", "den", "B\u00e4u\u00b7men", "in", "der", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sinnend h\u00e4lt sie manchmal inne,", "tokens": ["Sin\u00b7nend", "h\u00e4lt", "sie", "manch\u00b7mal", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingedenk der s\u00fc\u00dfen Stunde.", "tokens": ["Ein\u00b7ge\u00b7denk", "der", "s\u00fc\u00b7\u00dfen", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und die Wonne jener Stunde,", "tokens": ["Und", "die", "Won\u00b7ne", "je\u00b7ner", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das mitleidvolle Bangen", "tokens": ["Und", "das", "mit\u00b7leid\u00b7vol\u00b7le", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Um den Theuren mengen ihre", "tokens": ["Um", "den", "Theu\u00b7ren", "men\u00b7gen", "ih\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thr\u00e4nen auf des M\u00e4dchens Wangen. \u2014", "tokens": ["Thr\u00e4\u00b7nen", "auf", "des", "M\u00e4d\u00b7chens", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Nun erwacht der Prinz vom Traume,", "tokens": ["Nun", "er\u00b7wacht", "der", "Prinz", "vom", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ihn lie\u00df sein Kl\u00e4rchen schauen,", "tokens": ["Der", "ihn", "lie\u00df", "sein", "Kl\u00e4r\u00b7chen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der ihn wandeln, frei und selig,", "tokens": ["Der", "ihn", "wan\u00b7deln", ",", "frei", "und", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lie\u00df in heimatlichen Auen.", "tokens": ["Lie\u00df", "in", "hei\u00b7mat\u00b7li\u00b7chen", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Des Erwachten Blicke schweifen", "tokens": ["Des", "Er\u00b7wach\u00b7ten", "Bli\u00b7cke", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Finster an den Kerkerw\u00e4nden;", "tokens": ["Fins\u00b7ter", "an", "den", "Ker\u00b7ker\u00b7w\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sie werden pl\u00f6tzlich heiter,", "tokens": ["Doch", "sie", "wer\u00b7den", "pl\u00f6tz\u00b7lich", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treffen sie die Morgenspenden.", "tokens": ["Tref\u00b7fen", "sie", "die", "Mor\u00b7gen\u00b7spen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Still und sch\u00fcchtern in der Ferne", "tokens": ["Still", "und", "sch\u00fcch\u00b7tern", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht der Page, wills kaum wagen,", "tokens": ["Steht", "der", "Pa\u00b7ge", ",", "wills", "kaum", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie nicht Verr\u00e4ther w\u00fcrden,", "tokens": ["Da\u00df", "sie", "nicht", "Ver\u00b7r\u00e4\u00b7ther", "w\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Augen aufzuschlagen.", "tokens": ["Sei\u00b7ne", "Au\u00b7gen", "auf\u00b7zu\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Klara sieht es freudebebend,", "tokens": ["Kla\u00b7ra", "sieht", "es", "freu\u00b7de\u00b7be\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Liebe stumme Gaben", "tokens": ["Wie", "der", "Lie\u00b7be", "stum\u00b7me", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm das Angesicht erheitern,", "tokens": ["Ihm", "das", "An\u00b7ge\u00b7sicht", "er\u00b7hei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die kranke Seele laben.", "tokens": ["Und", "die", "kran\u00b7ke", "See\u00b7le", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}