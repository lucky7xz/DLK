{"textgrid.poem.58452": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "490. Unerm\u00fcdet!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh noch ein wenig berghinan", "tokens": ["Geh", "noch", "ein", "we\u00b7nig", "berg\u00b7hi\u00b7nan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "PIS", "VVINF"], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sacht aus dir selbst und allen Dingen,", "tokens": ["Sacht", "aus", "dir", "selbst", "und", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gewi\u00df, die schmale Kreuzesbahn", "tokens": ["Ge\u00b7wi\u00df", ",", "die", "schma\u00b7le", "Kreu\u00b7zes\u00b7bahn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird endlich dich in Gott daheime bringen;", "tokens": ["Wird", "end\u00b7lich", "dich", "in", "Gott", "da\u00b7hei\u00b7me", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Geh ", "tokens": ["Geh"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Bald ist's getan!", "tokens": ["Bald", "ist's", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}