{"dta.poem.21061": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Sternhimmel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, in welchen fernen Regionen,               ", "tokens": ["Freund", ",", "in", "wel\u00b7chen", "fer\u00b7nen", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welchen sterngestickten Himmelszonen,", "tokens": ["Wel\u00b7chen", "stern\u00b7ge\u00b7stick\u00b7ten", "Him\u00b7mels\u00b7zo\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schwebst du itzt auf unersp\u00e4hter Bahn?", "tokens": ["Schwebst", "du", "itzt", "auf", "un\u00b7er\u00b7sp\u00e4h\u00b7ter", "Bahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schaust im ungeheuren Weltenraume", "tokens": ["Schaust", "im", "un\u00b7ge\u00b7heu\u00b7ren", "Wel\u00b7ten\u00b7rau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ebentheuer, welche selbst im Traume", "tokens": ["E\u00b7bent\u00b7heu\u00b7er", ",", "wel\u00b7che", "selbst", "im", "Trau\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kepler nicht, noch Galil\u00e4i sahn.", "tokens": ["Ke\u00b7pler", "nicht", ",", "noch", "Ga\u00b7li\u00b7l\u00e4i", "sahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "ADV", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Musterst du der Allmacht Kriegesheere?", "tokens": ["Mus\u00b7terst", "du", "der", "All\u00b7macht", "Krie\u00b7ges\u00b7hee\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Siehst den drangen Halmwald ihrer Speere?", "tokens": ["Siehst", "den", "dran\u00b7gen", "Halm\u00b7wald", "ih\u00b7rer", "Spee\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Horchst dem Sturmwind ihres Riesengangs?", "tokens": ["Horchst", "dem", "Sturm\u00b7wind", "ih\u00b7res", "Rie\u00b7sen\u00b7gangs", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Siehst das Funkeln ihrer Blitzgeschosse,", "tokens": ["Siehst", "das", "Fun\u00b7keln", "ih\u00b7rer", "Blitz\u00b7ge\u00b7schos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "H\u00f6rst das Wiehern ihrer Flammenrosse,", "tokens": ["H\u00f6rst", "das", "Wie\u00b7hern", "ih\u00b7rer", "Flam\u00b7men\u00b7ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die Donner ihres Schlachtgesangs?", "tokens": ["Und", "die", "Don\u00b7ner", "ih\u00b7res", "Schlacht\u00b7ge\u00b7sangs", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Fliegst du mit ", "tokens": ["Fliegst", "du", "mit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Weit und breit umher im Schlangenringe", "tokens": ["Weit", "und", "breit", "um\u00b7her", "im", "Schlan\u00b7gen\u00b7rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "An dem Finger der Unendlichkeit?", "tokens": ["An", "dem", "Fin\u00b7ger", "der", "Un\u00b7end\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ruderst du mit ", "tokens": ["Ru\u00b7derst", "du", "mit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In dem inselvollen Oceane", "tokens": ["In", "dem", "in\u00b7sel\u00b7vol\u00b7len", "O\u00b7cea\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Uferloser Unermesslichkeit?", "tokens": ["U\u00b7fer\u00b7lo\u00b7ser", "Un\u00b7er\u00b7mess\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Landest itzt am Ufer der ", "tokens": ["Lan\u00b7dest", "itzt", "am", "U\u00b7fer", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ART"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Itzt am Archipelag der ", "tokens": ["Itzt", "am", "Ar\u00b7chi\u00b7pe\u00b7lag", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Itzt an ", "tokens": ["Itzt", "an"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Klimmst empor auf ", "tokens": ["Klimmst", "em\u00b7por", "auf"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKVZ", "APPR"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Rollst hinab auf ", "tokens": ["Rollst", "hin\u00b7ab", "auf"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "APPR"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "An des Horizontes fernsten Rand?", "tokens": ["An", "des", "Ho\u00b7ri\u00b7zon\u00b7tes", "ferns\u00b7ten", "Rand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Tauchest dich in ", "tokens": ["Tau\u00b7chest", "dich", "in"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Badest dich in ", "tokens": ["Ba\u00b7dest", "dich", "in"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schwimmst im silbernen ", "tokens": ["Schwimmst", "im", "sil\u00b7ber\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Steigst empor mit ", "tokens": ["Steigst", "em\u00b7por", "mit"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKVZ", "APPR"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Schiffst hinab des Milchwegs schroffen Bogen", "tokens": ["Schiffst", "hin\u00b7ab", "des", "Milch\u00b7wegs", "schrof\u00b7fen", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bis zum Flammenschleudrer ", "tokens": ["Bis", "zum", "Flam\u00b7men\u00b7schleu\u00b7drer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Staunst du ob ", "tokens": ["Staunst", "du", "ob"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Folgst begeistert seinem Siegeszuge", "tokens": ["Folgst", "be\u00b7geis\u00b7tert", "sei\u00b7nem", "Sie\u00b7ges\u00b7zu\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Durch der Urnacht alte D\u00e4mmerung?", "tokens": ["Durch", "der", "Ur\u00b7nacht", "al\u00b7te", "D\u00e4m\u00b7me\u00b7rung", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Bebst nicht vor ", "tokens": ["Bebst", "nicht", "vor"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PTKNEG", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Nicht vor ", "tokens": ["Nicht", "vor"], "token_info": ["word", "word"], "pos": ["PTKNEG", "PTKVZ"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Nicht vor ", "tokens": ["Nicht", "vor"], "token_info": ["word", "word"], "pos": ["PTKNEG", "PTKVZ"], "meter": "-+", "measure": "iambic.single"}}, "stanza.7": {"line.1": {"text": "F\u00e4hrst du mit Allvaters Sieges", "tokens": ["F\u00e4hrst", "du", "mit", "All\u00b7va\u00b7ters", "Sie\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Siehst ", "tokens": ["Siehst"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Siehst, wie ", "tokens": ["Siehst", ",", "wie"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "PWAV"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wie vor ", "tokens": ["Wie", "vor"], "token_info": ["word", "word"], "pos": ["PWAV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Dubhe's goldne M&#228;hnen auseinander stieben,               ", "tokens": ["Dubhe's", "gold\u00b7ne", "M", "&#228;", "hnen", "aus\u00b7ein\u00b7an\u00b7der", "stie\u00b7ben", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NE", "$(", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und der trotz'ge ", "tokens": ["Und", "der", "trotz'\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Oder fl\u00fcchtetest du wallfahrtm\u00fcde", "tokens": ["O\u00b7der", "fl\u00fcch\u00b7te\u00b7test", "du", "wall\u00b7fahrt\u00b7m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zu des ", "tokens": ["Zu", "des"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Pflegst auf seinem S\u00f6ller stolzer Ruh?", "tokens": ["Pflegst", "auf", "sei\u00b7nem", "S\u00f6l\u00b7ler", "stol\u00b7zer", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Siehst der Welten Labyrinthent\u00e4nzen", "tokens": ["Siehst", "der", "Wel\u00b7ten", "La\u00b7by\u00b7rin\u00b7then\u00b7t\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sonder Stillstand, sonder Ziel und Gr\u00e4nzen", "tokens": ["Son\u00b7der", "Still\u00b7stand", ",", "son\u00b7der", "Ziel", "und", "Gr\u00e4n\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In erhabener Bewundrung zu?", "tokens": ["In", "er\u00b7ha\u00b7be\u00b7ner", "Be\u00b7wund\u00b7rung", "zu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}}, "stanza.9": {"line.1": {"text": "Schwebe, wo du schwebst, in welchen Fernen,", "tokens": ["Schwe\u00b7be", ",", "wo", "du", "schwebst", ",", "in", "wel\u00b7chen", "Fer\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PWAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Walle, wo du wallst, auf welchen Sternen \u2014", "tokens": ["Wal\u00b7le", ",", "wo", "du", "wallst", ",", "auf", "wel\u00b7chen", "Ster\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PWAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weiss ich doch, dein wonnetrunkner Blick", "tokens": ["Weiss", "ich", "doch", ",", "dein", "won\u00b7ne\u00b7trunk\u00b7ner", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schauet oft aus jenen Glanzgefilden", "tokens": ["Schau\u00b7et", "oft", "aus", "je\u00b7nen", "Glanz\u00b7ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wehmuthd\u00e4mmernd nach dem blassen milden", "tokens": ["Weh\u00b7muth\u00b7d\u00e4m\u00b7mernd", "nach", "dem", "blas\u00b7sen", "mil\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mutterstern, der dich gebar, zur\u00fcck.", "tokens": ["Mut\u00b7ters\u00b7tern", ",", "der", "dich", "ge\u00b7bar", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Wo dir sieben und zwanzig Lenze sprossten,", "tokens": ["Wo", "dir", "sie\u00b7ben", "und", "zwan\u00b7zig", "Len\u00b7ze", "spross\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "KON", "CARD", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sieben und zwanzig Winter dich umschlossten,", "tokens": ["Sie\u00b7ben", "und", "zwan\u00b7zig", "Win\u00b7ter", "dich", "um\u00b7schloss\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "CARD", "NN", "PPER", "VVFIN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Sieben und zwanzigmal der Herbst dein Haupt umflog;", "tokens": ["Sie\u00b7ben", "und", "zwan\u00b7zig\u00b7mal", "der", "Herbst", "dein", "Haupt", "um\u00b7flog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "ADV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Wo in ihrem warmen Mutterschoosse", "tokens": ["Wo", "in", "ih\u00b7rem", "war\u00b7men", "Mut\u00b7ter\u00b7schoos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "F\u00fcr das Gute, Wahre, Sch\u00f6ne, Grosse", "tokens": ["F\u00fcr", "das", "Gu\u00b7te", ",", "Wah\u00b7re", ",", "Sch\u00f6\u00b7ne", ",", "Gros\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADJA", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dich die freundliche Natur erzog;", "tokens": ["Dich", "die", "freund\u00b7li\u00b7che", "Na\u00b7tur", "er\u00b7zog", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Wo manch gutes Herz sich zu dir f\u00fcgte,", "tokens": ["Wo", "manch", "gu\u00b7tes", "Herz", "sich", "zu", "dir", "f\u00fcg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Manches Freundes Arm sich um dich schmiegte,", "tokens": ["Man\u00b7ches", "Freun\u00b7des", "Arm", "sich", "um", "dich", "schmieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mancher F\u00fchlende sich an dich hing;", "tokens": ["Man\u00b7cher", "F\u00fch\u00b7len\u00b7de", "sich", "an", "dich", "hing", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und mein Jugendfreund auch dich umfing \u2014", "tokens": ["Und", "mein", "Ju\u00b7gend\u00b7freund", "auch", "dich", "um\u00b7fing"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Ja, ich weiss, du schaust mit sanftem Sehnen", "tokens": ["Ja", ",", "ich", "weiss", ",", "du", "schaust", "mit", "sanf\u00b7tem", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oft hinunter nach dem Stern voll Thr\u00e4nen,", "tokens": ["Oft", "hin\u00b7un\u00b7ter", "nach", "dem", "Stern", "voll", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der dich keimen, bl\u00fchn und welken sah.", "tokens": ["Der", "dich", "kei\u00b7men", ",", "bl\u00fchn", "und", "wel\u00b7ken", "sah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "VVFIN", "KON", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Reifte doch dein Geist in seinen Strahlen,", "tokens": ["Reif\u00b7te", "doch", "dein", "Geist", "in", "sei\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wandeln doch in seinen stillen Thalen", "tokens": ["Wan\u00b7deln", "doch", "in", "sei\u00b7nen", "stil\u00b7len", "Tha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Noch Ellwina und Rosalia!", "tokens": ["Noch", "Ell\u00b7wi\u00b7na", "und", "Ro\u00b7sa\u00b7lia", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}