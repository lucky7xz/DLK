{"dta.poem.266": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Lesbis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nstes Kind ich bin entz\u00fcndet/", "tokens": ["Sch\u00f6ns\u00b7tes", "Kind", "ich", "bin", "ent\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Sch\u00f6nheit hats gethan/", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", "hats", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Brennen/ das die Seel empfindet/", "tokens": ["Bren\u00b7nen", "/", "das", "die", "Seel", "emp\u00b7fin\u00b7det", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00fcndt dein sch\u00f6nes Auge an;", "tokens": ["Z\u00fcndt", "dein", "sch\u00f6\u00b7nes", "Au\u00b7ge", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So! da\u00df ich bald mu\u00df vergehen/", "tokens": ["So", "!", "da\u00df", "ich", "bald", "mu\u00df", "ver\u00b7ge\u00b7hen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wenn ich nicht kan H\u00fclffe sehen.", "tokens": ["Wenn", "ich", "nicht", "kan", "H\u00fclf\u00b7fe", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Reiche deinen Mund und Lippen", "tokens": ["Rei\u00b7che", "dei\u00b7nen", "Mund", "und", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir zu einem Labsahl her/", "tokens": ["Mir", "zu", "ei\u00b7nem", "Lab\u00b7sahl", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sammt des Busens ", "tokens": ["Sammt", "des", "Bu\u00b7sens"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und der Br\u00fcste K\u00fchlungs-Meer;", "tokens": ["Und", "der", "Br\u00fcs\u00b7te", "K\u00fch\u00b7lungs\u00b7Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Ist es? da\u00df ich die umfange/", "tokens": ["Ist", "es", "?", "da\u00df", "ich", "die", "um\u00b7fan\u00b7ge", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$.", "KOUS", "PPER", "ART", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alsdenn bin ich nicht mehr bange.", "tokens": ["Als\u00b7denn", "bin", "ich", "nicht", "mehr", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deine Brust dem Himmel gleichet", "tokens": ["Dei\u00b7ne", "Brust", "dem", "Him\u00b7mel", "glei\u00b7chet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An Vergn\u00fcgen sch\u00f6nstes Kind/", "tokens": ["An", "Ver\u00b7gn\u00fc\u00b7gen", "sch\u00f6ns\u00b7tes", "Kind", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Stirn der ", "tokens": ["Dei\u00b7ner", "Stirn", "der"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Den man ohne Flecken findt;", "tokens": ["Den", "man", "oh\u00b7ne", "Fle\u00b7cken", "findt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja dein angenehmes Wesen/", "tokens": ["Ja", "dein", "an\u00b7ge\u00b7neh\u00b7mes", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4st nur lauter Anmuth lesen.", "tokens": ["L\u00e4st", "nur", "lau\u00b7ter", "An\u00b7muth", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lesbis deinen sch\u00f6nen Mienen/", "tokens": ["Les\u00b7bis", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Mie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der trefflichen Gestalt", "tokens": ["Und", "der", "treff\u00b7li\u00b7chen", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcnschen G\u00f6tter selbst zu dienen/", "tokens": ["W\u00fcn\u00b7schen", "G\u00f6t\u00b7ter", "selbst", "zu", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn wer k\u00e4mpfft mit der Gewalt/", "tokens": ["Denn", "wer", "k\u00e4mpfft", "mit", "der", "Ge\u00b7walt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "So dein Auge bey sich tr\u00e4get", "tokens": ["So", "dein", "Au\u00b7ge", "bey", "sich", "tr\u00e4\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn es heisse Blitze heget.", "tokens": ["Wenn", "es", "heis\u00b7se", "Blit\u00b7ze", "he\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "F\u00e4lt nun jeder vor dir nieder", "tokens": ["F\u00e4lt", "nun", "je\u00b7der", "vor", "dir", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIS", "APPR", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach so sey doch nicht so stoltz/", "tokens": ["Ach", "so", "sey", "doch", "nicht", "so", "stoltz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bedencke bey dir wieder/", "tokens": ["Und", "be\u00b7den\u00b7cke", "bey", "dir", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du nicht von Stein und Holtz/", "tokens": ["Da\u00df", "du", "nicht", "von", "Stein", "und", "Holtz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sondern auch ein Mensch gebohren/", "tokens": ["Son\u00b7dern", "auch", "ein", "Mensch", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht zum Qu\u00e4hlen auserkohren.", "tokens": ["Nicht", "zum", "Qu\u00e4h\u00b7len", "au\u00b7ser\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Grausahms Kind ach la\u00df dich lieben/", "tokens": ["Grau\u00b7sahms", "Kind", "ach", "la\u00df", "dich", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VVIMP", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach erkenne meine Brunst!", "tokens": ["Ach", "er\u00b7ken\u00b7ne", "mei\u00b7ne", "Brunst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Venus heist dir dieses \u00fcben", "tokens": ["Ve\u00b7nus", "heist", "dir", "die\u00b7ses", "\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "PDS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und befiehlt die Gegen-Gunst;", "tokens": ["Und", "be\u00b7fiehlt", "die", "Ge\u00b7gen\u00b7Gunst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun so halte ihr Gesetze", "tokens": ["Nun", "so", "hal\u00b7te", "ihr", "Ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df mich deine Gynst erg\u00f6tze.", "tokens": ["Da\u00df", "mich", "dei\u00b7ne", "Gynst", "er\u00b7g\u00f6t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}