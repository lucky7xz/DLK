{"dta.poem.3278": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Das bedr\u00e4ngte Deutschland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lang zerfleischt mit schwerer Hand", "tokens": ["Wie", "lang", "zer\u00b7fleischt", "mit", "schwe\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Germanien sein Eingeweide?", "tokens": ["Ger\u00b7ma\u00b7ni\u00b7en", "sein", "Ein\u00b7ge\u00b7wei\u00b7de", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Besiegt ", "tokens": ["Be\u00b7siegt"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Sich selbst und seinen Ruhm, zu schlauer Feinde Freude?", "tokens": ["Sich", "selbst", "und", "sei\u00b7nen", "Ruhm", ",", "zu", "schlau\u00b7er", "Fein\u00b7de", "Freu\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sind, wo die Donau, wo der Mayn", "tokens": ["Sind", ",", "wo", "die", "Do\u00b7nau", ",", "wo", "der", "Mayn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NE", "$,", "PWAV", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Voll fauler Leichen langsam flie\u00dfet;", "tokens": ["Voll", "fau\u00b7ler", "Lei\u00b7chen", "lang\u00b7sam", "flie\u00b7\u00dfet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo um den rebenreichen Rhein", "tokens": ["Wo", "um", "den", "re\u00b7ben\u00b7rei\u00b7chen", "Rhein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst Bacchus fr\u00f6hlich gieng, und sich die Elb\u2019 ergie\u00dfet:", "tokens": ["Sonst", "Bac\u00b7chus", "fr\u00f6h\u00b7lich", "gieng", ",", "und", "sich", "die", "Elb'", "er\u00b7gie\u00b7\u00dfet", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJD", "VVFIN", "$,", "KON", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sind nicht die Spuren unsrer Wuth", "tokens": ["Sind", "nicht", "die", "Spu\u00b7ren", "uns\u00b7rer", "Wuth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf ieder Flur, an iedem Strande?", "tokens": ["Auf", "ie\u00b7der", "Flur", ",", "an", "ie\u00b7dem", "Stran\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo str\u00f6mte nicht das deutsche Blut?", "tokens": ["Wo", "str\u00f6m\u00b7te", "nicht", "das", "deut\u00b7sche", "Blut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht zu Deutschlands Ruhm: Nein! meistens ihm", "tokens": ["Und", "nicht", "zu", "Deutschlands", "Ruhm", ":", "Nein", "!", "meis\u00b7tens", "ihm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "NE", "NN", "$.", "PTKANT", "$.", "ADV", "PPER"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "zur Schande!", "tokens": ["zur", "Schan\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Wem ist nicht Deutschland unterthan!", "tokens": ["Wem", "ist", "nicht", "Deutschland", "un\u00b7ter\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "NE", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es wimmelt stets von zwanzig Heeren:", "tokens": ["Es", "wim\u00b7melt", "stets", "von", "zwan\u00b7zig", "Hee\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verw\u00fcstung zeichnet ihre Bahn;", "tokens": ["Ver\u00b7w\u00fcs\u00b7tung", "zeich\u00b7net", "ih\u00b7re", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was die Armuth spart, hilft Uebermuth verzehren.", "tokens": ["Und", "was", "die", "Ar\u00b7muth", "spart", ",", "hilft", "Ue\u00b7ber\u00b7muth", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vor ihnen her entflieht die Lust;", "tokens": ["Vor", "ih\u00b7nen", "her", "ent\u00b7flieht", "die", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APZR", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in den B\u00fcschen \u00f6der Auen,", "tokens": ["Und", "in", "den", "B\u00fc\u00b7schen", "\u00f6\u00b7der", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo vormals an geliebter Brust", "tokens": ["Wo", "vor\u00b7mals", "an", "ge\u00b7lieb\u00b7ter", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der satte Landmann sang, herrscht Einsamkeit und Grauen.", "tokens": ["Der", "sat\u00b7te", "Land\u00b7mann", "sang", ",", "herrscht", "Ein\u00b7sam\u00b7keit", "und", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Adler sieht entschlafen zu,", "tokens": ["Der", "Ad\u00b7ler", "sieht", "ent\u00b7schla\u00b7fen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bleibt bey ganzer L\u00e4nder Schreyen", "tokens": ["Und", "bleibt", "bey", "gan\u00b7zer", "L\u00e4n\u00b7der", "Schre\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Stets unerz\u00fcrnt in tr\u00e4ger Ruh,", "tokens": ["Stets", "un\u00b7er\u00b7z\u00fcrnt", "in", "tr\u00e4\u00b7ger", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entwaffnet und gez\u00e4hmt von falschen Schmeicheleyen.", "tokens": ["Ent\u00b7waff\u00b7net", "und", "ge\u00b7z\u00e4hmt", "von", "fal\u00b7schen", "Schmei\u00b7che\u00b7le\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.7": {"line.1": {"text": "O Schande! sind wir euch verwandt,", "tokens": ["O", "Schan\u00b7de", "!", "sind", "wir", "euch", "ver\u00b7wandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Deutschen jener bessern Zeiten,", "tokens": ["Ihr", "Deut\u00b7schen", "je\u00b7ner", "bes\u00b7sern", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die feiger Knechtschaft eisern Band", "tokens": ["Die", "fei\u00b7ger", "Knecht\u00b7schaft", "ei\u00b7sern", "Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr, als den h\u00e4rtsten Tod im Arm der Freyheit scheuten?", "tokens": ["Mehr", ",", "als", "den", "h\u00e4rts\u00b7ten", "Tod", "im", "Arm", "der", "Frey\u00b7heit", "scheu\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wir, die uns kranker Wollust weihn,", "tokens": ["Wir", ",", "die", "uns", "kran\u00b7ker", "Wol\u00b7lust", "weihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschw\u00e4cht vom Gifte weicher Sitten;", "tokens": ["Ge\u00b7schw\u00e4cht", "vom", "Gif\u00b7te", "wei\u00b7cher", "Sit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen deren Enkel seyn,", "tokens": ["Wir", "wol\u00b7len", "de\u00b7ren", "En\u00b7kel", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRELAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die, rauh, doch furchtbarfrey, f\u00fcr ihre W\u00e4lder stritten?", "tokens": ["Die", ",", "rauh", ",", "doch", "furcht\u00b7bar\u00b7frey", ",", "f\u00fcr", "ih\u00b7re", "W\u00e4l\u00b7der", "strit\u00b7ten", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "$,", "ADV", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die W\u00e4lder, wo ihr Ruhm noch izt", "tokens": ["Die", "W\u00e4l\u00b7der", ",", "wo", "ihr", "Ruhm", "noch", "izt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um die bemoosten Eichen schwebet,", "tokens": ["Um", "die", "be\u00b7moos\u00b7ten", "Ei\u00b7chen", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo, als ihr Stahl vereint geblitzt,", "tokens": ["Wo", ",", "als", "ihr", "Stahl", "ver\u00b7eint", "ge\u00b7blitzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr ehrner Arm gesiegt und Latium gebebet?", "tokens": ["Ihr", "ehr\u00b7ner", "Arm", "ge\u00b7siegt", "und", "La\u00b7ti\u00b7um", "ge\u00b7be\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wir schlafen, da die Zwietracht wacht,", "tokens": ["Wir", "schla\u00b7fen", ",", "da", "die", "Zwiet\u00b7racht", "wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihre bleiche Fackel schwinget,", "tokens": ["Und", "ih\u00b7re", "blei\u00b7che", "Fa\u00b7ckel", "schwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und, seit sie uns den Krieg gebracht,", "tokens": ["Und", ",", "seit", "sie", "uns", "den", "Krieg", "ge\u00b7bracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhm stets zur Seite schleicht, von Furien umringet.", "tokens": ["Jhm", "stets", "zur", "Sei\u00b7te", "schleicht", ",", "von", "Fu\u00b7ri\u00b7en", "um\u00b7rin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Jhr Natternheer zischt uns ums Ohr,", "tokens": ["Ihr", "Nat\u00b7tern\u00b7heer", "zischt", "uns", "ums", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die deutschen Herzen zu vergiften;", "tokens": ["Die", "deut\u00b7schen", "Her\u00b7zen", "zu", "ver\u00b7gif\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wird, kommt ihr kein Hermann vor,", "tokens": ["Und", "wird", ",", "kommt", "ihr", "kein", "Her\u00b7mann", "vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Hermanns Vaterland ein schm\u00e4hlig Denkmaal stiften.", "tokens": ["An", "Her\u00b7manns", "Va\u00b7ter\u00b7land", "ein", "schm\u00e4h\u00b7lig", "Denk\u00b7maal", "stif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Doch mein Gesang wagt allzuviel!", "tokens": ["Doch", "mein", "Ge\u00b7sang", "wagt", "all\u00b7zu\u00b7viel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Muse! fleuch zu diesen Zeiten", "tokens": ["O", "Mu\u00b7se", "!", "fleuch", "zu", "die\u00b7sen", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Alk\u00e4ens kriegrisch Saitenspiel,", "tokens": ["Al\u00b7k\u00e4\u00b7ens", "krie\u00b7grisch", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das die Tyrannen schalt, und scherz auf sanftern Saiten.", "tokens": ["Das", "die", "Ty\u00b7ran\u00b7nen", "schalt", ",", "und", "scherz", "auf", "sanf\u00b7tern", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}