{"textgrid.poem.46733": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[niemand soll mich weinen sehn]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemand soll mich weinen sehn", "tokens": ["Nie\u00b7mand", "soll", "mich", "wei\u00b7nen", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als in Feld und Aue", "tokens": ["Als", "in", "Feld", "und", "Au\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Blumen, deren Augen stehn,", "tokens": ["Blu\u00b7men", ",", "de\u00b7ren", "Au\u00b7gen", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "VVINF", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Meinen gleich, im Thaue.", "tokens": ["Mei\u00b7nen", "gleich", ",", "im", "Thau\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sollt' ich vor den Leuten weinen,", "tokens": ["Sollt'", "ich", "vor", "den", "Leu\u00b7ten", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die, ich wei\u00df nicht, wie sie's meinen,", "tokens": ["Die", ",", "ich", "wei\u00df", "nicht", ",", "wie", "sie's", "mei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie mir zu trauern scheinen?", "tokens": ["Wenn", "sie", "mir", "zu", "trau\u00b7ern", "schei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu den Blumen will ich gehn,", "tokens": ["Zu", "den", "Blu\u00b7men", "will", "ich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denen ich vertraue:", "tokens": ["De\u00b7nen", "ich", "ver\u00b7trau\u00b7e", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Niemand soll mich weinen sehn,", "tokens": ["Nie\u00b7mand", "soll", "mich", "wei\u00b7nen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als in Feld und Aue.", "tokens": ["Als", "in", "Feld", "und", "Au\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Soll mein Leid ich ihnen klagen?", "tokens": ["Soll", "mein", "Leid", "ich", "ih\u00b7nen", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eignes haben sie zu tragen,", "tokens": ["Eig\u00b7nes", "ha\u00b7ben", "sie", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrden mir ihr eignes sagen,", "tokens": ["W\u00fcr\u00b7den", "mir", "ihr", "eig\u00b7nes", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich will nur meines sehn,", "tokens": ["Und", "ich", "will", "nur", "mei\u00b7nes", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das in euch ich schaue,", "tokens": ["Das", "in", "euch", "ich", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Blumen, deren Augen stehn", "tokens": ["Blu\u00b7men", ",", "de\u00b7ren", "Au\u00b7gen", "stehn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELAT", "NN", "VVINF"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Meinen gleich, im Thaue.", "tokens": ["Mei\u00b7nen", "gleich", ",", "im", "Thau\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Blumen schweigen still bescheiden,", "tokens": ["Blu\u00b7men", "schwei\u00b7gen", "still", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollen tr\u00f6sten nicht mein Leiden,", "tokens": ["Wol\u00b7len", "tr\u00f6s\u00b7ten", "nicht", "mein", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch an meinem Weh sich weiden.", "tokens": ["Noch", "an", "mei\u00b7nem", "Weh", "sich", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand soll mich weinen sehn", "tokens": ["Nie\u00b7mand", "soll", "mich", "wei\u00b7nen", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als in Feld und Aue", "tokens": ["Als", "in", "Feld", "und", "Au\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Blumen, deren Augen stehn,", "tokens": ["Blu\u00b7men", ",", "de\u00b7ren", "Au\u00b7gen", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "VVINF", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Meinen gleich, im Thaue.", "tokens": ["Mei\u00b7nen", "gleich", ",", "im", "Thau\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}