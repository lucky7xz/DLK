{"dta.poem.7619": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einem K\u00f6nig wird erz\u00e4hlt, da\u00df im Pallast", "tokens": ["Von", "ei\u00b7nem", "K\u00f6\u00b7nig", "wird", "er\u00b7z\u00e4hlt", ",", "da\u00df", "im", "Pal\u00b7last"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hatte sich geh\u00e4uft die gr\u00f6\u00dfte B\u00fccherlast.", "tokens": ["Er", "hat\u00b7te", "sich", "ge\u00b7h\u00e4uft", "die", "gr\u00f6\u00df\u00b7te", "B\u00fc\u00b7cher\u00b7last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und zog der K\u00f6nig aus, so zogen auf den Pfaden", "tokens": ["Und", "zog", "der", "K\u00f6\u00b7nig", "aus", ",", "so", "zo\u00b7gen", "auf", "den", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hundert und ein Kamel mit B\u00fcchern nach beladen.", "tokens": ["Hun\u00b7dert", "und", "ein", "Ka\u00b7mel", "mit", "B\u00fc\u00b7chern", "nach", "be\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "ART", "NN", "APPR", "NN", "APPR", "ADJA", "$."], "meter": "----+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Da ward er doch gewahr am Ende da\u00df ihm sei", "tokens": ["Da", "ward", "er", "doch", "ge\u00b7wahr", "am", "En\u00b7de", "da\u00df", "ihm", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "KOUS", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beschwerlich auf der Fahrt die gro\u00dfe B\u00fccherei.", "tokens": ["Be\u00b7schwer\u00b7lich", "auf", "der", "Fahrt", "die", "gro\u00b7\u00dfe", "B\u00fc\u00b7che\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und lie\u00df zu besserer Bequemlichkeit beim Reisen", "tokens": ["Und", "lie\u00df", "zu", "bes\u00b7se\u00b7rer", "Be\u00b7quem\u00b7lich\u00b7keit", "beim", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ausz\u00fcge machen von hundert und Einem Weisen.", "tokens": ["Aus\u00b7z\u00fc\u00b7ge", "ma\u00b7chen", "von", "hun\u00b7dert", "und", "Ei\u00b7nem", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "CARD", "KON", "ART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Von diesen ward gemacht ein Auszug, den beim Zug", "tokens": ["Von", "die\u00b7sen", "ward", "ge\u00b7macht", "ein", "Aus\u00b7zug", ",", "den", "beim", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "VAFIN", "VVPP", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des K\u00f6niges gemach ein starkes Maulthier trug.", "tokens": ["Des", "K\u00f6\u00b7ni\u00b7ges", "ge\u00b7mach", "ein", "star\u00b7kes", "Mault\u00b7hier", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch noch bequemer wollt' er haben seine Sachen,", "tokens": ["Doch", "noch", "be\u00b7que\u00b7mer", "wollt'", "er", "ha\u00b7ben", "sei\u00b7ne", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VMFIN", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus dem Auszug lie\u00df er einen Auszug machen.", "tokens": ["Und", "aus", "dem", "Aus\u00b7zug", "lie\u00df", "er", "ei\u00b7nen", "Aus\u00b7zug", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ein art'ges B\u00fcchlein ward nun aus der Maulthierb\u00fcrde,", "tokens": ["Ein", "art'\u00b7ges", "B\u00fcch\u00b7lein", "ward", "nun", "aus", "der", "Mault\u00b7hier\u00b7b\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das auf der Reise selbst der K\u00f6nig trug mit W\u00fcrde.", "tokens": ["Das", "auf", "der", "Rei\u00b7se", "selbst", "der", "K\u00f6\u00b7nig", "trug", "mit", "W\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Doch immer noch zu sehr bel\u00e4stigte das ihn,", "tokens": ["Doch", "im\u00b7mer", "noch", "zu", "sehr", "be\u00b7l\u00e4s\u00b7tig\u00b7te", "das", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PTKA", "ADV", "VVFIN", "ART", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Auszugs Auszug lie\u00df er aus noch einmal ziehn.", "tokens": ["Des", "Aus\u00b7zugs", "Aus\u00b7zug", "lie\u00df", "er", "aus", "noch", "ein\u00b7mal", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPR", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Da zogen sie ihm aus dem ausgezognen Buch", "tokens": ["Da", "zo\u00b7gen", "sie", "ihm", "aus", "dem", "aus\u00b7ge\u00b7zog\u00b7nen", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Kern zusammen kurz in einen einz'gen Spruch.", "tokens": ["Den", "Kern", "zu\u00b7sam\u00b7men", "kurz", "in", "ei\u00b7nen", "einz'\u00b7gen", "Spruch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Den fa\u00dft' er ins Gem\u00fcth, und konnt' ihn leicht behalten,", "tokens": ["Den", "fa\u00dft'", "er", "ins", "Ge\u00b7m\u00fcth", ",", "und", "konnt'", "ihn", "leicht", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um seines Heils danach und seines Reichs zu walten.", "tokens": ["Um", "sei\u00b7nes", "Heils", "da\u00b7nach", "und", "sei\u00b7nes", "Reichs", "zu", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PAV", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Ob ihm dis Heil gelang? Wenn er's nicht ganz vollbracht,", "tokens": ["Ob", "ihm", "dis", "Heil", "ge\u00b7lang", "?", "Wenn", "er's", "nicht", "ganz", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVFIN", "$.", "KOUS", "PIS", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So war's nur, weil er selbst den Auszug nicht gemacht.", "tokens": ["So", "wa\u00b7r's", "nur", ",", "weil", "er", "selbst", "den", "Aus\u00b7zug", "nicht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.12": {"line.1": {"text": "Das aber ist gewis, da\u00df aus dem B\u00fccherwust", "tokens": ["Das", "a\u00b7ber", "ist", "ge\u00b7wis", ",", "da\u00df", "aus", "dem", "B\u00fc\u00b7cher\u00b7wust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "ADV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du machen f\u00fcr dein Heil solch einen Auszug mu\u00dft.", "tokens": ["Du", "ma\u00b7chen", "f\u00fcr", "dein", "Heil", "solch", "ei\u00b7nen", "Aus\u00b7zug", "mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "APPR", "PPOSAT", "NN", "PIAT", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}