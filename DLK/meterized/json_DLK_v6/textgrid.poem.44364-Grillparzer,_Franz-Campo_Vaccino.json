{"textgrid.poem.44364": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Campo Vaccino", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid gegr\u00fc\u00dft, ihr heilgen Tr\u00fcmmer,", "tokens": ["Seid", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "heil\u00b7gen", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch als Tr\u00fcmmer mir gegr\u00fc\u00dft,", "tokens": ["Auch", "als", "Tr\u00fcm\u00b7mer", "mir", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Obgleich nur noch Mondenschimmer", "tokens": ["Ob\u00b7gleich", "nur", "noch", "Mon\u00b7den\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einer Sonn, die nicht mehr ist.", "tokens": ["Ei\u00b7ner", "Sonn", ",", "die", "nicht", "mehr", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nennt euch mir, ich will euch kennen,", "tokens": ["Nennt", "euch", "mir", ",", "ich", "will", "euch", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich will wissen, was ihr wart,", "tokens": ["Ich", "will", "wis\u00b7sen", ",", "was", "ihr", "wart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was ihr ", "tokens": ["Was", "ihr"], "token_info": ["word", "word"], "pos": ["PWS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Da die Schmach euch gleich gepaart.", "tokens": ["Da", "die", "Schmach", "euch", "gleich", "ge\u00b7paart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eintrachtstempel, du der erste,", "tokens": ["Ein\u00b7trachts\u00b7tem\u00b7pel", ",", "du", "der", "ers\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der sich meinem Blick enth\u00fcllt;", "tokens": ["Der", "sich", "mei\u00b7nem", "Blick", "ent\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine letzte S\u00e4ule berste!", "tokens": ["Dei\u00b7ne", "letz\u00b7te", "S\u00e4u\u00b7le", "bers\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlecht hast du dein Amt erf\u00fcllt!", "tokens": ["Schlecht", "hast", "du", "dein", "Amt", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Solltest deine Br\u00fcder h\u00fcten,", "tokens": ["Soll\u00b7test", "dei\u00b7ne", "Br\u00fc\u00b7der", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wardst als W\u00e4chter hingesetzt,", "tokens": ["Wardst", "als", "W\u00e4ch\u00b7ter", "hin\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NE", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und du lie\u00dfest Zwietracht w\u00fcten,", "tokens": ["Und", "du", "lie\u00b7\u00dfest", "Zwiet\u00b7racht", "w\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die sie f\u00e4llt und dich zuletzt.", "tokens": ["Die", "sie", "f\u00e4llt", "und", "dich", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jupiter, aus deinem Tempel,", "tokens": ["Ju\u00b7pi\u00b7ter", ",", "aus", "dei\u00b7nem", "Tem\u00b7pel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Stator, der zu stehn gebeut,", "tokens": ["Sta\u00b7tor", ",", "der", "zu", "stehn", "ge\u00b7beut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Brich des Schweigens Sklaven-Stempel,", "tokens": ["Brich", "des", "Schwei\u00b7gens", "Skla\u00b7ven\u00b7Stem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei\u00df sie stehn die neue Zeit!", "tokens": ["Hei\u00df", "sie", "stehn", "die", "neu\u00b7e", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch umsonst ist hier dein Walten,", "tokens": ["Doch", "um\u00b7sonst", "ist", "hier", "dein", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Du stehst selber nur mit M\u00fch,", "tokens": ["Du", "stehst", "sel\u00b7ber", "nur", "mit", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Unaufhaltsam gehn die Alten", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "gehn", "die", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und das Neue \u00fcber sie.", "tokens": ["Und", "das", "Neu\u00b7e", "\u00fc\u00b7ber", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Warum in dies Feld der Leichen", "tokens": ["Wa\u00b7rum", "in", "dies", "Feld", "der", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PDS", "NN", "ART", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Ist, Septimius Sever,", "tokens": ["Ist", ",", "Sep\u00b7ti\u00b7mi\u00b7us", "Se\u00b7ver", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als dem letzten, ders zu fassen,", "tokens": ["Als", "dem", "letz\u00b7ten", ",", "ders", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$,", "PRELS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn auch nicht zu tun verstand,", "tokens": ["Wenn", "auch", "nicht", "zu", "tun", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei ein Pl\u00e4tzchen dir gelassen,", "tokens": ["Sei", "ein", "Pl\u00e4tz\u00b7chen", "dir", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch nicht hier, am \u00e4u\u00dfern Rand.", "tokens": ["Doch", "nicht", "hier", ",", "am", "\u00e4u\u00b7\u00dfern", "Rand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Titus, nicht dem Ruhm, dem Frieden", "tokens": ["Ti\u00b7tus", ",", "nicht", "dem", "Ruhm", ",", "dem", "Frie\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PTKNEG", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bautest du dein Heiligtum:", "tokens": ["Bau\u00b7test", "du", "dein", "Hei\u00b7lig\u00b7tum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch dir ward, was du vermieden,", "tokens": ["Doch", "dir", "ward", ",", "was", "du", "ver\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder Stein spricht deinen Ruhm.", "tokens": ["Je\u00b7der", "Stein", "spricht", "dei\u00b7nen", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch den Frieden in dem Munde", "tokens": ["Auch", "den", "Frie\u00b7den", "in", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ging ein andrer drauf ins Haus,", "tokens": ["Ging", "ein", "an\u00b7drer", "drauf", "ins", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch der Frieden zog zur Stunde", "tokens": ["Doch", "der", "Frie\u00b7den", "zog", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus dem Friedenstempel aus.", "tokens": ["Aus", "dem", "Frie\u00b7dens\u00b7tem\u00b7pel", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Curia, die aus ihren Toren", "tokens": ["Cu\u00b7ria", ",", "die", "aus", "ih\u00b7ren", "To\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Krieg der Welt und Frieden lie\u00df,", "tokens": ["Krieg", "der", "Welt", "und", "Frie\u00b7den", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Harrst du deiner Senatoren?", "tokens": ["Harrst", "du", "dei\u00b7ner", "Se\u00b7na\u00b7to\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer doch ist dir gewi\u00df.", "tokens": ["Ei\u00b7ner", "doch", "ist", "dir", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Sieh ihn stehn dort an den Stufen,", "tokens": ["Sieh", "ihn", "stehn", "dort", "an", "den", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei dem Mann im Priesterkleid,", "tokens": ["Bei", "dem", "Mann", "im", "Pries\u00b7ter\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieh, er kommt, wird er gerufen,", "tokens": ["Sieh", ",", "er", "kommt", ",", "wird", "er", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und er geht, wenn man gebeut;", "tokens": ["Und", "er", "geht", ",", "wenn", "man", "ge\u00b7beut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh des Purpurs reiche Falten!", "tokens": ["Sieh", "des", "Pur\u00b7purs", "rei\u00b7che", "Fal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Majest\u00e4tisch steht er da!", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "steht", "er", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, du suchst nach deinen Alten?", "tokens": ["Ja", ",", "du", "suchst", "nach", "dei\u00b7nen", "Al\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlie\u00df die Pforten, Curia!", "tokens": ["Schlie\u00df", "die", "Pfor\u00b7ten", ",", "Cu\u00b7ria", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Unten such, die unten wohnen,", "tokens": ["Un\u00b7ten", "such", ",", "die", "un\u00b7ten", "woh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wir sind oben leicht und froh;", "tokens": ["Wir", "sind", "o\u00b7ben", "leicht", "und", "froh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rom hat nur noch Ciceronen,", "tokens": ["Rom", "hat", "nur", "noch", "Ci\u00b7ce\u00b7ro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber keinen Cicero.", "tokens": ["A\u00b7ber", "kei\u00b7nen", "Ci\u00b7ce\u00b7ro", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hat der Bruder dich erstochen,", "tokens": ["Hat", "der", "Bru\u00b7der", "dich", "er\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Remus, mit dem weichen Sinn?", "tokens": ["Re\u00b7mus", ",", "mit", "dem", "wei\u00b7chen", "Sinn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh vom Schicksal dich gerochen,", "tokens": ["Sieh", "vom", "Schick\u00b7sal", "dich", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er, sein Reich, gleich dir, dahin.", "tokens": ["Er", ",", "sein", "Reich", ",", "gleich", "dir", ",", "da\u00b7hin", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "$,", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh! in seines Tempels Hallen,", "tokens": ["Sieh", "!", "in", "sei\u00b7nes", "Tem\u00b7pels", "Hal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie in deinem, M\u00f6nchezug;", "tokens": ["Wie", "in", "dei\u00b7nem", ",", "M\u00f6n\u00b7che\u00b7zug", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Horch! des K\u00fcsters Gl\u00f6cklein schallen!", "tokens": ["Horch", "!", "des", "K\u00fcs\u00b7ters", "Gl\u00f6c\u00b7klein", "schal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "D\u00fcnkt die Rache dir genug?", "tokens": ["D\u00fcnkt", "die", "Ra\u00b7che", "dir", "ge\u00b7nug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Roma, Venus \u2013 Sch\u00f6nheit, St\u00e4rke,", "tokens": ["Ro\u00b7ma", ",", "Ve\u00b7nus", "\u2013", "Sch\u00f6n\u00b7heit", ",", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$(", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pulse ihr der alten Welt,", "tokens": ["Pul\u00b7s