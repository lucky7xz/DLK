{"dta.poem.967": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "Blumen", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "In m&#228;rzentagen streuten wir die samen                 ", "tokens": ["In", "m", "&#228;", "rzen\u00b7ta\u00b7gen", "streu\u00b7ten", "wir", "die", "sa\u00b7men"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$(", "VVINF", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann unser herz noch einmal heftig litt                 ", "tokens": ["Wann", "un\u00b7ser", "herz", "noch", "ein\u00b7mal", "hef\u00b7tig", "litt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An wehen die vom toten jahre kamen                 ", "tokens": ["An", "we\u00b7hen", "die", "vom", "to\u00b7ten", "jah\u00b7re", "ka\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am lezten kampf den eis und sonne stritt               ", "tokens": ["Am", "lez\u00b7ten", "kampf", "den", "eis", "und", "son\u00b7ne", "stritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "An schlanken st&#228;bchen wollten wir sie ziehen                 ", "tokens": ["An", "schlan\u00b7ken", "st", "&#228;", "bchen", "woll\u00b7ten", "wir", "sie", "zie\u00b7hen"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVINF", "VVFIN", "$(", "VVINF", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir suchten ihnen reinen wasserquell                 ", "tokens": ["Wir", "such\u00b7ten", "ih\u00b7nen", "rei\u00b7nen", "was\u00b7ser\u00b7quell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir wussten dass sie unterm licht gediehen                 ", "tokens": ["Wir", "wuss\u00b7ten", "dass", "sie", "un\u00b7term", "licht", "ge\u00b7die\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und unter blicken liebevoll und hell               ", "tokens": ["Und", "un\u00b7ter", "bli\u00b7cken", "lie\u00b7be\u00b7voll", "und", "hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit frohem fleisse wurden sie begossen                 ", "tokens": ["Mit", "fro\u00b7hem", "fleis\u00b7se", "wur\u00b7den", "sie", "be\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sahen zu den wolken forschend bang                 ", "tokens": ["Wir", "sa\u00b7hen", "zu", "den", "wol\u00b7ken", "for\u00b7schend", "bang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVPP", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zusammen auf und harrten unverdrossen                 ", "tokens": ["Zu\u00b7sam\u00b7men", "auf", "und", "harr\u00b7ten", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob sich ein blatt entrollt ein trieb entsprang               ", "tokens": ["Ob", "sich", "ein", "blatt", "ent\u00b7rollt", "ein", "trieb", "ent\u00b7sprang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJD", "VVFIN", "ART", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir haben in dem garten sie gepfl&#252;ckt                 ", "tokens": ["Wir", "ha\u00b7ben", "in", "dem", "gar\u00b7ten", "sie", "ge\u00b7pfl", "&#252;", "ckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "VVFIN", "PPER", "NE", "$(", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und an den nachbarlichen weingel&#228;nden                 ", "tokens": ["Und", "an", "den", "nach\u00b7bar\u00b7li\u00b7chen", "wein\u00b7gel", "&#228;", "nden"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$(", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir wandelten vom glanz der nacht entz&#252;ckt                 ", "tokens": ["Wir", "wan\u00b7del\u00b7ten", "vom", "glanz", "der", "nacht", "entz", "&#252;", "ckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "NE", "$(", "NE"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und trugen sie in unsren kinderh&#228;nden.               ", "tokens": ["Und", "tru\u00b7gen", "sie", "in", "un\u00b7sren", "kin\u00b7derh", "&#228;", "nden", "."], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}