{"textgrid.poem.56266": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Minnedienst", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4hrend dort im hellen Saale", "tokens": ["W\u00e4h\u00b7rend", "dort", "im", "hel\u00b7len", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustberauscht die G\u00e4ste wogen,", "tokens": ["Lust\u00b7be\u00b7rauscht", "die", "G\u00e4s\u00b7te", "wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4lt ein Ritter vom Gedr\u00e4nge", "tokens": ["H\u00e4lt", "ein", "Rit\u00b7ter", "vom", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam sich zur\u00fcckgezogen.", "tokens": ["Ein\u00b7sam", "sich", "zu\u00b7r\u00fcck\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie er von dem Sofa aufblickt,", "tokens": ["Wie", "er", "von", "dem", "So\u00b7fa", "auf\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wo er ruhet in Gedanken,", "tokens": ["Wo", "er", "ru\u00b7het", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht er neben sich die Dame,", "tokens": ["Sieht", "er", "ne\u00b7ben", "sich", "die", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "ART", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Der er dienet sonder Wanken.", "tokens": ["Der", "er", "die\u00b7net", "son\u00b7der", "Wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbsind es Sterne, sind es Sonnen,", "tokens": ["\u00bb", "sind", "es", "Ster\u00b7ne", ",", "sind", "es", "Son\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in meiner Nacht sich zeigen?", "tokens": ["Die", "in", "mei\u00b7ner", "Nacht", "sich", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind's die Augen meiner Herrin,", "tokens": ["Sin\u00b7d's", "die", "Au\u00b7gen", "mei\u00b7ner", "Her\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.4": {"text": "Welche \u00fcber mich sich neigen?\u00ab", "tokens": ["Wel\u00b7che", "\u00fc\u00b7ber", "mich", "sich", "nei\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAT", "APPR", "PPER", "PRF", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbschmeichler, Schmeichler! Sterne, Sonnen", "tokens": ["\u00bb", "schmeich\u00b7ler", ",", "Schmeich\u00b7ler", "!", "Ster\u00b7ne", ",", "Son\u00b7nen"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["$(", "ADJD", "$,", "NN", "$.", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind es nicht, wovon Ihr dichtet;", "tokens": ["Sind", "es", "nicht", ",", "wo\u00b7von", "Ihr", "dich\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind die Augen einer Dame,", "tokens": ["Sind", "die", "Au\u00b7gen", "ei\u00b7ner", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die auf Euch sie bittend richtet.\u00ab \u2013", "tokens": ["Die", "auf", "Euch", "sie", "bit\u00b7tend", "rich\u00b7tet", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "APPR", "PPER", "PPER", "ADJD", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbherz und Klinge sind Euch eigen,", "tokens": ["\u00bb", "herz", "und", "Klin\u00b7ge", "sind", "Euch", "ei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "KON", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schickt mich aus auf Abenteuer,", "tokens": ["Schickt", "mich", "aus", "auf", "A\u00b7bent\u00b7eu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00dft im Kampfe mich bestehen", "tokens": ["Hei\u00dft", "im", "Kamp\u00b7fe", "mich", "be\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Riesen, Drachen, Ungeheuer.\u00ab \u2013", "tokens": ["Rie\u00b7sen", ",", "Dra\u00b7chen", ",", "Un\u00b7ge\u00b7heu\u00b7er", ".", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbnein, um mich, mein werter Ritter,", "tokens": ["\u00bb", "nein", ",", "um", "mich", ",", "mein", "wer\u00b7ter", "Rit\u00b7ter", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "KOUI", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll kein Blut den Boden f\u00e4rben;", "tokens": ["Soll", "kein", "Blut", "den", "Bo\u00b7den", "f\u00e4r\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um ein Glas Gefrornes bitt ich,", "tokens": ["Um", "ein", "Glas", "Ge\u00b7fror\u00b7nes", "bitt", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lasset nicht vor Durst mich sterben.\u00ab \u2013", "tokens": ["Las\u00b7set", "nicht", "vor", "Durst", "mich", "ster\u00b7ben", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "PPER", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbherrin, in dem Dienst der Minne", "tokens": ["\u00bb", "her\u00b7rin", ",", "in", "dem", "Dienst", "der", "Min\u00b7ne"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wollt ich gern mein Leben wagen,", "tokens": ["Wollt", "ich", "gern", "mein", "Le\u00b7ben", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber hier durch das Gedr\u00e4nge", "tokens": ["A\u00b7ber", "hier", "durch", "das", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Wird es schwer sich durchzuschlagen.\u00ab", "tokens": ["Wird", "es", "schwer", "sich", "durch\u00b7zu\u00b7schla\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und sie bittet, und er gehet, \u2013", "tokens": ["Und", "sie", "bit\u00b7tet", ",", "und", "er", "ge\u00b7het", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt zur\u00fcck, wie er gegangen:", "tokens": ["Kommt", "zu\u00b7r\u00fcck", ",", "wie", "er", "ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbnein! ich konnte, hohe Herrin,", "tokens": ["\u00bb", "nein", "!", "ich", "konn\u00b7te", ",", "ho\u00b7he", "Her\u00b7rin", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "PPER", "VMFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Kein Gefrorenes erlangen.\u00ab", "tokens": ["Kein", "Ge\u00b7fro\u00b7re\u00b7nes", "er\u00b7lan\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVINF", "$.", "$("], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.9": {"line.1": {"text": "Und sie bittet wieder, wieder", "tokens": ["Und", "sie", "bit\u00b7tet", "wie\u00b7der", ",", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wagt er's, immer noch vergebens:", "tokens": ["Wagt", "er's", ",", "im\u00b7mer", "noch", "ver\u00b7ge\u00b7bens", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbnein! man dringt durch jene T\u00fcre", "tokens": ["\u00bb", "nein", "!", "man", "dringt", "durch", "je\u00b7ne", "T\u00fc\u00b7re"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$.", "PIS", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Gefahr nur seines Lebens.\u00ab", "tokens": ["Mit", "Ge\u00b7fahr", "nur", "sei\u00b7nes", "Le\u00b7bens", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbritter, Ritter, von Gefahren", "tokens": ["\u00bb", "rit\u00b7ter", ",", "Rit\u00b7ter", ",", "von", "Ge\u00b7fah\u00b7ren"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "ADJA", "$,", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprachet Ihr, von K\u00e4mpfen, Schlachten;", "tokens": ["Spra\u00b7chet", "Ihr", ",", "von", "K\u00e4mp\u00b7fen", ",", "Schlach\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Ihr la\u00dft vor Euren Augen", "tokens": ["Und", "Ihr", "la\u00dft", "vor", "Eu\u00b7ren", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne H\u00fclfe mich verschmachten.\u00ab", "tokens": ["Oh\u00b7ne", "H\u00fcl\u00b7fe", "mich", "ver\u00b7schmach\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und ins wogende Gew\u00fchle", "tokens": ["Und", "ins", "wo\u00b7gen\u00b7de", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.2": {"text": "Ist der Ritter vorgedrungen,", "tokens": ["Ist", "der", "Rit\u00b7ter", "vor\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort verfolgt er einen Diener,", "tokens": ["Dort", "ver\u00b7folgt", "er", "ei\u00b7nen", "Die\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat den Raub ihm abgerungen.", "tokens": ["Hat", "den", "Raub", "ihm", "ab\u00b7ge\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und die Dame schaut von ferne,", "tokens": ["Und", "die", "Da\u00b7me", "schaut", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mit hochgehaltner Schale", "tokens": ["Wie", "mit", "hoch\u00b7ge\u00b7halt\u00b7ner", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er sich durch den Reigen windet", "tokens": ["Er", "sich", "durch", "den", "Rei\u00b7gen", "win\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem engen, vollen Saale;", "tokens": ["In", "dem", "en\u00b7gen", ",", "vol\u00b7len", "Saa\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.13": {"line.1": {"text": "Sieht in eines Fensters Ecke", "tokens": ["Sieht", "in", "ei\u00b7nes", "Fens\u00b7ters", "E\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fccklich seinen Fang ihn bergen,", "tokens": ["Gl\u00fcck\u00b7lich", "sei\u00b7nen", "Fang", "ihn", "ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht ihn hinter die Gardine", "tokens": ["Sieht", "ihn", "hin\u00b7ter", "die", "Gar\u00b7di\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren Augen sich verbergen;", "tokens": ["Ih\u00b7ren", "Au\u00b7gen", "sich", "ver\u00b7ber\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Sieht ihn selber dort gem\u00e4chlich", "tokens": ["Sieht", "ihn", "sel\u00b7ber", "dort", "ge\u00b7m\u00e4ch\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Eroberte verschlingen,", "tokens": ["Das", "Er\u00b7o\u00b7ber\u00b7te", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wischen sich den Mund und kommen,", "tokens": ["Wi\u00b7schen", "sich", "den", "Mund", "und", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr betr\u00fcbte Kunde bringen:", "tokens": ["Ihr", "be\u00b7tr\u00fcb\u00b7te", "Kun\u00b7de", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00bbgern will ich mein Leben wagen,", "tokens": ["\u00bb", "gern", "will", "ich", "mein", "Le\u00b7ben", "wa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schickt mich aus auf Abenteuer,", "tokens": ["Schickt", "mich", "aus", "auf", "A\u00b7bent\u00b7eu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00dft im Kampfe mich bestehen", "tokens": ["Hei\u00dft", "im", "Kamp\u00b7fe", "mich", "be\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Riesen, Drachen, Ungeheuer.", "tokens": ["Rie\u00b7sen", ",", "Dra\u00b7chen", ",", "Un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Aber hier, o meine Herrin,", "tokens": ["A\u00b7ber", "hier", ",", "o", "mei\u00b7ne", "Her\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hier ist alles doch vergebens,", "tokens": ["Hier", "ist", "al\u00b7les", "doch", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und man dringt durch jene T\u00fcre", "tokens": ["Und", "man", "dringt", "durch", "je\u00b7ne", "T\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Gefahr nur seines Lebens.\u00ab", "tokens": ["Mit", "Ge\u00b7fahr", "nur", "sei\u00b7nes", "Le\u00b7bens", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}