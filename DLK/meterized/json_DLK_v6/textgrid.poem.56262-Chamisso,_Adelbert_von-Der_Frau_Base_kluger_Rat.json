{"textgrid.poem.56262": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der Frau Base kluger Rat", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6chtest du den Jungen haben?", "tokens": ["M\u00f6ch\u00b7test", "du", "den", "Jun\u00b7gen", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den gesunden, frischen, \u00fcpp'gen,", "tokens": ["Den", "ge\u00b7sun\u00b7den", ",", "fri\u00b7schen", ",", "\u00fcpp'\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blondgelockten, sch\u00f6nen Knaben?", "tokens": ["Blond\u00b7ge\u00b7lock\u00b7ten", ",", "sch\u00f6\u00b7nen", "Kna\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, ein wahres Zuckerp\u00fcppchen!", "tokens": ["Ei", ",", "ein", "wah\u00b7res", "Zu\u00b7cker\u00b7p\u00fcpp\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Lust mit dem zu leben!", "tokens": ["Ei\u00b7ne", "Lust", "mit", "dem", "zu", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00dft um ihn dir M\u00fche geben;", "tokens": ["Mu\u00dft", "um", "ihn", "dir", "M\u00fc\u00b7he", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja, der ist ein schmucker Mann!", "tokens": ["Ja", ",", "der", "ist", "ein", "schmu\u00b7cker", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kratze, kratze, kratze, Trulle,", "tokens": ["Krat\u00b7ze", ",", "krat\u00b7ze", ",", "krat\u00b7ze", ",", "Trul\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dir den h\u00fcbschen Jungen an!", "tokens": ["Dir", "den", "h\u00fcb\u00b7schen", "Jun\u00b7gen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Oder den, nach altem Brauche,", "tokens": ["O\u00b7der", "den", ",", "nach", "al\u00b7tem", "Brau\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Dreimaster, Puderzopfe,", "tokens": ["Mit", "Drei\u00b7mas\u00b7ter", ",", "Pu\u00b7der\u00b7zop\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcnnen Beinen, dickem Bauche,", "tokens": ["D\u00fcn\u00b7nen", "Bei\u00b7nen", ",", "di\u00b7ckem", "Bau\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kupfernas und Wackelkopfe?", "tokens": ["Kup\u00b7fer\u00b7nas", "und", "Wa\u00b7ckel\u00b7kop\u00b7fe", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stirbt er, gibt es viel zu erben;", "tokens": ["Stirbt", "er", ",", "gibt", "es", "viel", "zu", "er\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und was sollte der nicht sterben?", "tokens": ["Und", "was", "soll\u00b7te", "der", "nicht", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "ART", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja, der ist ein reicher Mann!", "tokens": ["Ja", ",", "der", "ist", "ein", "rei\u00b7cher", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kratze, kratze, kratze, Trulle,", "tokens": ["Krat\u00b7ze", ",", "krat\u00b7ze", ",", "krat\u00b7ze", ",", "Trul\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kratze dir den Alten an.", "tokens": ["Krat\u00b7ze", "dir", "den", "Al\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder den vom Milit\u00e4re?", "tokens": ["O\u00b7der", "den", "vom", "Mi\u00b7li\u00b7t\u00e4\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Silber auf dreifarb'gem Tuche \u2013", "tokens": ["Sil\u00b7ber", "auf", "drei\u00b7fa\u00b7rb'\u00b7gem", "Tu\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Federhut \u2013 \u00bbauf meine Ehre!\u00ab", "tokens": ["Fe\u00b7der\u00b7hut", "\u2013", "\u00bb", "auf", "mei\u00b7ne", "Eh\u00b7re", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$(", "$(", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00fcgt er auch, wie aus dem Buche.", "tokens": ["L\u00fcgt", "er", "auch", ",", "wie", "aus", "dem", "Bu\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt das B\u00fcrgergrob zu Schaden,", "tokens": ["Kommt", "das", "B\u00fcr\u00b7ger\u00b7grob", "zu", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4ltst du's mit dem Edelmann.", "tokens": ["H\u00e4ltst", "du's", "mit", "dem", "E\u00b7del\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kratze, kratze, kratze, Trulle,", "tokens": ["Krat\u00b7ze", ",", "krat\u00b7ze", ",", "krat\u00b7ze", ",", "Trul\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kratze dir den Leutnant an!", "tokens": ["Krat\u00b7ze", "dir", "den", "Leut\u00b7nant", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oder wen du kannst, den Lahmen", "tokens": ["O\u00b7der", "wen", "du", "kannst", ",", "den", "Lah\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "PPER", "VMFIN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie den Krummen, la\u00df dich warnen:", "tokens": ["Wie", "den", "Krum\u00b7men", ",", "la\u00df", "dich", "war\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft von allen, die da kamen,", "tokens": ["Oft", "von", "al\u00b7len", ",", "die", "da", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt nicht einer in den Garnen.", "tokens": ["Bleibt", "nicht", "ei\u00b7ner", "in", "den", "Gar\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen Mann nur! heut zu Tage", "tokens": ["Ei\u00b7nen", "Mann", "nur", "!", "heut", "zu", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geht die allgemeine Klage:", "tokens": ["Geht", "die", "all\u00b7ge\u00b7mei\u00b7ne", "Kla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jede kriegt nicht einen Mann.", "tokens": ["Je\u00b7de", "kriegt", "nicht", "ei\u00b7nen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kratze, kratze, kratze, Trulle,", "tokens": ["Krat\u00b7ze", ",", "krat\u00b7ze", ",", "krat\u00b7ze", ",", "Trul\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dir den ersten besten an!", "tokens": ["Dir", "den", "ers\u00b7ten", "bes\u00b7ten", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}