{"textgrid.poem.34780": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neue Melodien spiel ich", "tokens": ["Neu\u00b7e", "Me\u00b7lo\u00b7dien", "spiel", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der neugestimmten Zither.", "tokens": ["Auf", "der", "neu\u00b7ges\u00b7timm\u00b7ten", "Zi\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alt ist der Text! Es sind die Worte", "tokens": ["Alt", "ist", "der", "Text", "!", "Es", "sind", "die", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Salomos: Das Weib ist bitter.", "tokens": ["Sa\u00b7lo\u00b7mos", ":", "Das", "Weib", "ist", "bit\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ungetreu ist sie dem Freunde,", "tokens": ["Un\u00b7ge\u00b7treu", "ist", "sie", "dem", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie treulos dem Gemahle!", "tokens": ["Wie", "sie", "treu\u00b7los", "dem", "Ge\u00b7mah\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wermut sind die letzten Tropfen", "tokens": ["Wer\u00b7mut", "sind", "die", "letz\u00b7ten", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In der Liebe Goldpokale.", "tokens": ["In", "der", "Lie\u00b7be", "Gold\u00b7po\u00b7ka\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also wahr ist jene Sage", "tokens": ["Al\u00b7so", "wahr", "ist", "je\u00b7ne", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem dunklen S\u00fcndenfluche,", "tokens": ["Von", "dem", "dunk\u00b7len", "S\u00fcn\u00b7den\u00b7flu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den die Schlange dir bereitet,", "tokens": ["Den", "die", "Schlan\u00b7ge", "dir", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie es steht im alten Buche?", "tokens": ["Wie", "es", "steht", "im", "al\u00b7ten", "Bu\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kriechend auf dem Bauch, die Schlange,", "tokens": ["Krie\u00b7chend", "auf", "dem", "Bauch", ",", "die", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauscht sie noch in allen B\u00fcschen,", "tokens": ["Lauscht", "sie", "noch", "in", "al\u00b7len", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kost mit dir noch jetzt wie weiland,", "tokens": ["Kost", "mit", "dir", "noch", "jetzt", "wie", "wei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "ADV", "KOKOM", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du h\u00f6rst sie gerne zischen.", "tokens": ["Und", "du", "h\u00f6rst", "sie", "ger\u00b7ne", "zi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, es wird so kalt und dunkel!", "tokens": ["Ach", ",", "es", "wird", "so", "kalt", "und", "dun\u00b7kel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Sonne flattern Raben,", "tokens": ["Um", "die", "Son\u00b7ne", "flat\u00b7tern", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVINF", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie kr\u00e4chzen. Lust und Liebe", "tokens": ["Und", "sie", "kr\u00e4ch\u00b7zen", ".", "Lust", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$.", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist auf lange jetzt begraben.", "tokens": ["Ist", "auf", "lan\u00b7ge", "jetzt", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}