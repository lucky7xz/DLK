{"dta.poem.4682": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die Fahrt der K\u00f6niglichen Braut  \n nach Engelland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn' am blauen himmlischen Gew\u00f6lbe\nStand majest\u00e4tisch, sah herab\nAls Englands K\u00f6nigin auf der beschiften Elbe\nAus sanftem Auge Gr\u00fcsse gab.", "tokens": ["Die", "Sonn'", "am", "blau\u00b7en", "himm\u00b7li\u00b7schen", "Ge\u00b7w\u00f6l\u00b7be", "Stand", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", ",", "sah", "her\u00b7ab", "Als", "En\u00b7glands", "K\u00f6\u00b7ni\u00b7gin", "auf", "der", "be\u00b7schif\u00b7ten", "El\u00b7be", "Aus", "sanf\u00b7tem", "Au\u00b7ge", "Gr\u00fcs\u00b7se", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "ADJA", "NN", "NN", "ADJD", "$,", "VVFIN", "ADV", "KOUS", "NE", "NN", "APPR", "ART", "ADJA", "NE", "APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-+-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}