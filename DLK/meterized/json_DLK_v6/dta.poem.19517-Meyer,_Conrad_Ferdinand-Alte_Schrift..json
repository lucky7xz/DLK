{"dta.poem.19517": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Alte Schrift.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst verlockt' es mich im Abendglimmen,", "tokens": ["J\u00fcngst", "ver\u00b7lockt'", "es", "mich", "im", "A\u00b7bend\u00b7glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zum Lombardenthurm emporzuklimmen,", "tokens": ["Zum", "Lom\u00b7bar\u00b7den\u00b7thurm", "em\u00b7por\u00b7zu\u00b7klim\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem verschollnen Herrscher hier im Gaue,", "tokens": ["Dem", "ver\u00b7scholl\u00b7nen", "Herr\u00b7scher", "hier", "im", "Gau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der die Ferne noch beherrscht, die blaue.", "tokens": ["Der", "die", "Fer\u00b7ne", "noch", "be\u00b7herrscht", ",", "die", "blau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "In den Tr\u00fcmmern bin ich lang geblieben:", "tokens": ["In", "den", "Tr\u00fcm\u00b7mern", "bin", "ich", "lang", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wandrernamen standen rings geschrieben", "tokens": ["Wand\u00b7rer\u00b7na\u00b7men", "stan\u00b7den", "rings", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hoch im Raum \u2014 der Boden war gewichen,", "tokens": ["Hoch", "im", "Raum", "der", "Bo\u00b7den", "war", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Lettern und Gebilde halb erblichen.", "tokens": ["Let\u00b7tern", "und", "Ge\u00b7bil\u00b7de", "halb", "er\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Einer dichtet ", "tokens": ["Ei\u00b7ner", "dich\u00b7tet"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "\u201egott hab' ich in der Natur bewundert!\u201c", "tokens": ["\u201e", "gott", "hab'", "ich", "in", "der", "Na\u00b7tur", "be\u00b7wun\u00b7dert", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u201egaudeamus!\u201c gr\u00e4bt ein flotter Zecher", "tokens": ["\u201e", "gau\u00b7dea\u00b7mus", "!", "\u201c", "gr\u00e4bt", "ein", "flot\u00b7ter", "Ze\u00b7cher"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "NE", "$.", "$(", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Um den keck entworfnen Riesenbecher.", "tokens": ["Um", "den", "keck", "ent\u00b7worf\u00b7nen", "Rie\u00b7sen\u00b7be\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dort ein Herz von einem Pfeil durchschnitten:", "tokens": ["Dort", "ein", "Herz", "von", "ei\u00b7nem", "Pfeil", "durch\u00b7schnit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u201ehedewig\u201c steht auf des Bolzes Mitten;", "tokens": ["\u201e", "he\u00b7de\u00b7wig", "\u201c", "steht", "auf", "des", "Bol\u00b7zes", "Mit\u00b7ten", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$(", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dicht daneben setzt ein Zeitgenosse", "tokens": ["Dicht", "da\u00b7ne\u00b7ben", "setzt", "ein", "Zeit\u00b7ge\u00b7nos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PAV", "VVFIN", "ART", "NN"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Gut lateinisch eine derbe Posse \u2014", "tokens": ["Gut", "la\u00b7tei\u00b7nisch", "ei\u00b7ne", "der\u00b7be", "Pos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Dann zur Rast in des Castelles Schatten", "tokens": ["Dann", "zur", "Rast", "in", "des", "Cas\u00b7tel\u00b7les", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Legten sich die Sch\u00fcler auf die Matten,", "tokens": ["Leg\u00b7ten", "sich", "die", "Sch\u00fc\u00b7ler", "auf", "die", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schl\u00fcrften eines Humpens rothe Wellen", "tokens": ["Schl\u00fcrf\u00b7ten", "ei\u00b7nes", "Hum\u00b7pens", "ro\u00b7the", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mir ist: ich trink' mit den Gesellen.", "tokens": ["Und", "mir", "ist", ":", "ich", "trink'", "mit", "den", "Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}}}}