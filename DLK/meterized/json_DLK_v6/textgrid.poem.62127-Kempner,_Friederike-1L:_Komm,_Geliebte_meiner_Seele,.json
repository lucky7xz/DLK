{"textgrid.poem.62127": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm, Geliebte meiner Seele,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Geliebte meiner Seele,", "tokens": ["Komm", ",", "Ge\u00b7lieb\u00b7te", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm und still' die Sehnsucht mir,", "tokens": ["Komm", "und", "still'", "die", "Sehn\u00b7sucht", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinen Schmerz ich nicht verhehle,", "tokens": ["Mei\u00b7nen", "Schmerz", "ich", "nicht", "ver\u00b7heh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn Du, ach, so fern von hier!", "tokens": ["Wenn", "Du", ",", "ach", ",", "so", "fern", "von", "hier", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ITJ", "$,", "ADV", "ADJD", "APPR", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dieses Hoffen, dieses Bangen,", "tokens": ["Die\u00b7ses", "Hof\u00b7fen", ",", "die\u00b7ses", "Ban\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese ew'ge Qual und Lust,", "tokens": ["Die\u00b7se", "ew'\u00b7ge", "Qual", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses m\u00e4chtige Verlangen,", "tokens": ["Die\u00b7ses", "m\u00e4ch\u00b7ti\u00b7ge", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses Klopfen meiner Brust.", "tokens": ["Die\u00b7ses", "Klop\u00b7fen", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch es ist kein leeres Sehnen,", "tokens": ["Doch", "es", "ist", "kein", "lee\u00b7res", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja, Du kehrst, Du kehrst zur\u00fcck,", "tokens": ["Ja", ",", "Du", "kehrst", ",", "Du", "kehrst", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaust in meine Freudentr\u00e4nen,", "tokens": ["Schaust", "in", "mei\u00b7ne", "Freu\u00b7den\u00b7tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem sch\u00f6nen, lieben Blick!", "tokens": ["Mit", "dem", "sch\u00f6\u00b7nen", ",", "lie\u00b7ben", "Blick", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wirst schon nie mehr von mir weichen,", "tokens": ["Wirst", "schon", "nie", "mehr", "von", "mir", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirst schon niemals fort von mir,", "tokens": ["Wirst", "schon", "nie\u00b7mals", "fort", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKVZ", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, es giebt nicht Deines Gleichen,", "tokens": ["Ach", ",", "es", "giebt", "nicht", "Dei\u00b7nes", "Glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir ist doch nur wohl bei Dir!", "tokens": ["Mir", "ist", "doch", "nur", "wohl", "bei", "Dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}