{"dta.poem.15718": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Herrscher der Natur war einst der Mensch geboren,", "tokens": ["Zum", "Herr\u00b7scher", "der", "Na\u00b7tur", "war", "einst", "der", "Mensch", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Stuhl der Herrschaft hat er durch den Fall verloren.", "tokens": ["Den", "Stuhl", "der", "Herr\u00b7schaft", "hat", "er", "durch", "den", "Fall", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Solang als in ihm rein das G\u00f6ttliche gebrannt,", "tokens": ["So\u00b7lang", "als", "in", "ihm", "rein", "das", "G\u00f6tt\u00b7li\u00b7che", "ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "APPR", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War von der Gottgemein' er Herrscher anerkannt.", "tokens": ["War", "von", "der", "Gott\u00b7ge\u00b7mein'", "er", "Herr\u00b7scher", "an\u00b7er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als diesen Talisman er in sich selbst zerst\u00f6rt,", "tokens": ["Als", "die\u00b7sen", "Ta\u00b7lis\u00b7man", "er", "in", "sich", "selbst", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat gegen ihn im Grimm sich bald sein Reich emp\u00f6rt.", "tokens": ["Hat", "ge\u00b7gen", "ihn", "im", "Grimm", "sich", "bald", "sein", "Reich", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "APPRART", "NE", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Aus seinem Fall hat er nun lang empor gerungen,", "tokens": ["Aus", "sei\u00b7nem", "Fall", "hat", "er", "nun", "lang", "em\u00b7por", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wieder auf den Thron hat er sich halb geschwungen.", "tokens": ["Und", "wie\u00b7der", "auf", "den", "Thron", "hat", "er", "sich", "halb", "ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mit H\u00fclfe der Vernunft ist er ein Herrscher worden,", "tokens": ["Mit", "H\u00fcl\u00b7fe", "der", "Ver\u00b7nunft", "ist", "er", "ein", "Herr\u00b7scher", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Herrscher der Gewalt unwill'ger Sklavenhorden.", "tokens": ["Ein", "Herr\u00b7scher", "der", "Ge\u00b7walt", "un\u00b7will'\u00b7ger", "Skla\u00b7ven\u00b7hor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Erst, wann er menschlich rein ist g\u00f6ttlich umgewandt,", "tokens": ["Erst", ",", "wann", "er", "menschlich", "rein", "ist", "g\u00f6tt\u00b7lich", "um\u00b7ge\u00b7wandt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADJD", "ADJD", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+--++-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Wird er als Herrscher seyn mit Freuden anerkannt.", "tokens": ["Wird", "er", "als", "Herr\u00b7scher", "seyn", "mit", "Freu\u00b7den", "an\u00b7er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOUS", "NN", "VAINF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}