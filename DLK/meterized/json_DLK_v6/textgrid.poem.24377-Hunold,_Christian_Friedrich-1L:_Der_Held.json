{"textgrid.poem.24377": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Held ", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Held ", "tokens": ["Der", "Held"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Gib/ spricht er/ Wehrter Printz/ nicht die Gesundheit auf.", "tokens": ["Gib", "/", "spricht", "er", "/", "Wehr\u00b7ter", "Printz", "/", "nicht", "die", "Ge\u00b7sund\u00b7heit", "auf", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "VVFIN", "PPER", "$(", "NN", "NN", "$(", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich werde gleich gesund/ gab dieser Hertzog drauf/", "tokens": ["Ich", "wer\u00b7de", "gleich", "ge\u00b7sund", "/", "gab", "die\u00b7ser", "Hert\u00b7zog", "drauf", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "VVFIN", "PDAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn uns der Himmel wird durch einen Sieg belohnen.", "tokens": ["Wenn", "uns", "der", "Him\u00b7mel", "wird", "durch", "ei\u00b7nen", "Sieg", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}