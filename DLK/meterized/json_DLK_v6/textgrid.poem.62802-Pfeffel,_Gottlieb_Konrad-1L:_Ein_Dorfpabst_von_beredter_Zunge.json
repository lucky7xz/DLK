{"textgrid.poem.62802": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Dorfpabst von beredter Zunge", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Dorfpabst von beredter Zunge", "tokens": ["Ein", "Dorf\u00b7pabst", "von", "be\u00b7red\u00b7ter", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schalt einst, kraft seiner Amtesmacht,", "tokens": ["Schalt", "einst", ",", "kraft", "sei\u00b7ner", "Am\u00b7tes\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus allen T\u00f6nen seiner Lunge,", "tokens": ["Aus", "al\u00b7len", "T\u00f6\u00b7nen", "sei\u00b7ner", "Lun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Ketzer, die er selbst gemacht:", "tokens": ["Auf", "Ket\u00b7zer", ",", "die", "er", "selbst", "ge\u00b7macht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und rief die schwarzen Legionen", "tokens": ["Und", "rief", "die", "schwar\u00b7zen", "Le\u00b7gi\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der ziegenf\u00fc\u00dfigen D\u00e4monen", "tokens": ["Der", "zie\u00b7gen\u00b7f\u00fc\u00b7\u00dfi\u00b7gen", "D\u00e4\u00b7mo\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als R\u00e4cher aus dem H\u00f6llenschlund.", "tokens": ["Als", "R\u00e4\u00b7cher", "aus", "dem", "H\u00f6l\u00b7len\u00b7schlund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein M\u00fctterlein, das vor ihm stund,", "tokens": ["Ein", "M\u00fct\u00b7ter\u00b7lein", ",", "das", "vor", "ihm", "stund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Erbebte hier am ganzen Leibe", "tokens": ["Er\u00b7beb\u00b7te", "hier", "am", "gan\u00b7zen", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und schwamm in einem Thr\u00e4nenstrom.", "tokens": ["Und", "schwamm", "in", "ei\u00b7nem", "Thr\u00e4\u00b7nen\u00b7strom", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was weint ihr? sprach der Chrysostom", "tokens": ["Was", "weint", "ihr", "?", "sprach", "der", "Chry\u00b7so\u00b7stom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Beym Schlu\u00df der Predigt zu dem Weibe.", "tokens": ["Beym", "Schlu\u00df", "der", "Pre\u00b7digt", "zu", "dem", "Wei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ach, lieber Gott! erwiedert sie", "tokens": ["Ach", ",", "lie\u00b7ber", "Gott", "!", "er\u00b7wie\u00b7dert", "sie"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ADV", "NN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und heult, da\u00df die Gew\u00f6lber hallen:", "tokens": ["Und", "heult", ",", "da\u00df", "die", "Ge\u00b7w\u00f6l\u00b7ber", "hal\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Es ist ein Stier mir umgefallen,", "tokens": ["Es", "ist", "ein", "Stier", "mir", "um\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der just wie der Herr Pfarrer schrie.", "tokens": ["Der", "just", "wie", "der", "Herr", "Pfar\u00b7rer", "schrie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}