{"textgrid.poem.42272": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Gesang am heiligen Vorabend des funfzigj\u00e4hrigen Jubelfestes der Georgia Augusta", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgen, o festlicher Tag,", "tokens": ["Mor\u00b7gen", ",", "o", "fest\u00b7li\u00b7cher", "Tag", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Morgen entschwebe", "tokens": ["Mor\u00b7gen", "ent\u00b7schwe\u00b7be"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Herrlich und hehr der Nacht!", "tokens": ["Herr\u00b7lich", "und", "hehr", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Komm in Titans Strahlenkranze,", "tokens": ["Komm", "in", "Ti\u00b7tans", "Strah\u00b7len\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Komm im blauen \u00c4thermantel,", "tokens": ["Komm", "im", "blau\u00b7en", "\u00c4\u00b7ther\u00b7man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In des Urlichts reinstem Glanze!", "tokens": ["In", "des", "Ur\u00b7lichts", "reins\u00b7tem", "Glan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So entsteige der Grotte der Nacht", "tokens": ["So", "ent\u00b7stei\u00b7ge", "der", "Grot\u00b7te", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Unter dem Meer!", "tokens": ["Un\u00b7ter", "dem", "Meer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "So entschwebe dem Wogentanze", "tokens": ["So", "ent\u00b7schwe\u00b7be", "dem", "Wo\u00b7gen\u00b7tan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Herrlich und hehr,", "tokens": ["Herr\u00b7lich", "und", "hehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Hehr und herrlich in Br\u00e4utigamspracht!", "tokens": ["Hehr", "und", "herr\u00b7lich", "in", "Br\u00e4u\u00b7ti\u00b7gam\u00b7spracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es harret dein,", "tokens": ["Es", "har\u00b7ret", "dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Voll Lieb' und Lust,", "tokens": ["Voll", "Lieb'", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die hohe Jubelk\u00f6nigin.", "tokens": ["Die", "ho\u00b7he", "Ju\u00b7bel\u00b7k\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor br\u00e4utlichem Entz\u00fccken", "tokens": ["Vor", "br\u00e4ut\u00b7li\u00b7chem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "H\u00fcpft ihr die Brust.", "tokens": ["H\u00fcpft", "ihr", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sie harret dein,", "tokens": ["Sie", "har\u00b7ret", "dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mit wonnegl\u00e4nzenden Wangen und Blicken,", "tokens": ["Mit", "won\u00b7ne\u00b7gl\u00e4n\u00b7zen\u00b7den", "Wan\u00b7gen", "und", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Georgia Augusta harret dein!", "tokens": ["Ge\u00b7or\u00b7gia", "Au\u00b7gus\u00b7ta", "har\u00b7ret", "dein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPOSAT", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Als sie vor funfzig ruhmbestrahlten Jahren", "tokens": ["Als", "sie", "vor", "funf\u00b7zig", "ruhm\u00b7bes\u00b7trahl\u00b7ten", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein sch\u00f6nes Kind,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein wundersch\u00f6nes G\u00f6tterkind,", "tokens": ["Ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "G\u00f6t\u00b7ter\u00b7kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geboren war,", "tokens": ["Ge\u00b7bo\u00b7ren", "war", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da brachten sie in dieses Tempels Halle,", "tokens": ["Da", "brach\u00b7ten", "sie", "in", "die\u00b7ses", "Tem\u00b7pels", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vor Gottes Hochaltar,", "tokens": ["Vor", "Got\u00b7tes", "Hoc\u00b7hal\u00b7tar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr gro\u00dfer Vater und die Hochber\u00fchmten alle,", "tokens": ["Ihr", "gro\u00b7\u00dfer", "Va\u00b7ter", "und", "die", "Hoch\u00b7be\u00b7r\u00fchm\u00b7ten", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die ihrer Kindheit Pfleger waren,", "tokens": ["Die", "ih\u00b7rer", "Kind\u00b7heit", "Pfle\u00b7ger", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dem Segenspender dar,", "tokens": ["Dem", "Se\u00b7gen\u00b7spen\u00b7der", "dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und auf der Andacht Fl\u00fcgel schwang", "tokens": ["Und", "auf", "der", "An\u00b7dacht", "Fl\u00fc\u00b7gel", "schwang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sich himmelan ihr flehender Gesang.", "tokens": ["Sich", "him\u00b7me\u00b7lan", "ihr", "fle\u00b7hen\u00b7der", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Herr, erf\u00fclle sie mit Weisheit,", "tokens": ["Herr", ",", "er\u00b7f\u00fcl\u00b7le", "sie", "mit", "Weis\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Adle sie, o Herr, durch Sch\u00f6nheit,", "tokens": ["Ad\u00b7le", "sie", ",", "o", "Herr", ",", "durch", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fcste sie mit Heldenst\u00e4rke,", "tokens": ["R\u00fcs\u00b7te", "sie", "mit", "Hel\u00b7den\u00b7st\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den gro\u00dfen Gang zum Ziele", "tokens": ["F\u00fcr", "den", "gro\u00b7\u00dfen", "Gang", "zum", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Strahlender Vollkommenheit!", "tokens": ["Strah\u00b7len\u00b7der", "Voll\u00b7kom\u00b7men\u00b7heit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn der Geist gedeiht durch Weisheit,", "tokens": ["Denn", "der", "Geist", "ge\u00b7deiht", "durch", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Herz gedeiht durch Sch\u00f6nheit,", "tokens": ["Und", "das", "Herz", "ge\u00b7deiht", "durch", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser Einklang rauscht in St\u00e4rke;", "tokens": ["Die\u00b7ser", "Ein\u00b7klang", "rauscht", "in", "St\u00e4r\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Adel f\u00fchrt zum Ziele", "tokens": ["Die\u00b7ser", "A\u00b7del", "f\u00fchrt", "zum", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dauernder Gl\u00fcckseligkei", "tokens": ["Dau\u00b7ern\u00b7der", "Gl\u00fcck\u00b7se\u00b7lig\u00b7kei"], "token_info": ["word", "word"], "pos": ["PAV", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Und als das Lied der frommen Schar,", "tokens": ["Und", "als", "das", "Lied", "der", "from\u00b7men", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Lied der hei\u00dfen Inbrunst,", "tokens": ["Das", "Lied", "der", "hei\u00b7\u00dfen", "In\u00b7brunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hinauf gesungen war,", "tokens": ["Hin\u00b7auf", "ge\u00b7sun\u00b7gen", "war", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da wallte Gottes Flamme,", "tokens": ["Da", "wall\u00b7te", "Got\u00b7tes", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sanft wallte von des Gebers Thron", "tokens": ["Sanft", "wall\u00b7te", "von", "des", "Ge\u00b7bers", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des herzlichen Gebetes Lohn,", "tokens": ["Des", "herz\u00b7li\u00b7chen", "Ge\u00b7be\u00b7tes", "Lohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Flamme, die noch nie verlosch,", "tokens": ["Die", "Flam\u00b7me", ",", "die", "noch", "nie", "ver\u00b7losch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Segens Flamm' herab auf den Altar.", "tokens": ["Des", "Se\u00b7gens", "Flamm'", "her\u00b7ab", "auf", "den", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O Flamme, die vom Himmel sank,", "tokens": ["O", "Flam\u00b7me", ",", "die", "vom", "Him\u00b7mel", "sank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entlodre hoch und weh' umher!", "tokens": ["Ent\u00b7lod\u00b7re", "hoch", "und", "weh'", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umher, umher!", "tokens": ["Um\u00b7her", ",", "um\u00b7her", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Entz\u00fcnde jedes Herz umher", "tokens": ["Ent\u00b7z\u00fcn\u00b7de", "je\u00b7des", "Herz", "um\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu hei\u00dfem Dank!", "tokens": ["Zu", "hei\u00b7\u00dfem", "Dank", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Dem Geber zu unaussprechlichem Dank!", "tokens": ["Dem", "Ge\u00b7ber", "zu", "un\u00b7aus\u00b7sprech\u00b7li\u00b7chem", "Dank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Der k\u00f6nigliche Herrscher auf dem Thron", "tokens": ["Der", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Herr\u00b7scher", "auf", "dem", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Albion", "tokens": ["Von", "Al\u00b7bi\u00b7on"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Trat v\u00e4terlich herzu, und gab", "tokens": ["Trat", "v\u00e4\u00b7ter\u00b7lich", "her\u00b7zu", ",", "und", "gab"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr reichlich mildes \u00d6l zur Nahrung.", "tokens": ["Ihr", "reich\u00b7lich", "mil\u00b7des", "\u00d6l", "zur", "Nah\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wetteifernd trat herzu die Schar", "tokens": ["Wett\u00b7ei\u00b7fernd", "trat", "her\u00b7zu", "die", "Schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Pfleger und der Priester am Altar,", "tokens": ["Der", "Pfle\u00b7ger", "und", "der", "Pries\u00b7ter", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der sie zu heiliger, zu ewiger Bewahrung", "tokens": ["Der", "sie", "zu", "hei\u00b7li\u00b7ger", ",", "zu", "e\u00b7wi\u00b7ger", "Be\u00b7wah\u00b7rung"], "token_info": ["word", "word", "word",