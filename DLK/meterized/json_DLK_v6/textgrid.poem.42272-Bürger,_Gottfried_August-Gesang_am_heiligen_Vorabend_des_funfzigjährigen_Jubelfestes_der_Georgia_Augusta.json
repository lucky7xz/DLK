": ["Sie", "f\u00fch\u00b7ren", "den", "gl\u00e4n\u00b7zen\u00b7den", "Br\u00e4u\u00b7ti\u00b7gam", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Schon wehet der heilige Schauer voran.", "tokens": ["Schon", "we\u00b7het", "der", "hei\u00b7li\u00b7ge", "Schau\u00b7er", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.22": {"line.1": {"text": "Schaut auf! Die Himmlischen steigen,", "tokens": ["Schaut", "auf", "!", "Die", "Himm\u00b7li\u00b7schen", "stei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein feierlich schwebender Reigen,", "tokens": ["Ein", "fei\u00b7er\u00b7lich", "schwe\u00b7ben\u00b7der", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein t\u00f6nender, Seelen entz\u00fcckender Chor,", "tokens": ["Ein", "t\u00f6\u00b7nen\u00b7der", ",", "See\u00b7len", "ent\u00b7z\u00fc\u00b7cken\u00b7der", "Chor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Auf purpurnen Wolken in Osten empor.", "tokens": ["Auf", "pur\u00b7pur\u00b7nen", "Wol\u00b7ken", "in", "Os\u00b7ten", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.23": {"line.1": {"text": "Schlagt hoch, ihr lodernden Flammen", "tokens": ["Schlagt", "hoch", ",", "ihr", "lo\u00b7dern\u00b7den", "Flam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Herzen und Lieder, zusammen!", "tokens": ["Der", "Her\u00b7zen", "und", "Lie\u00b7der", ",", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "F\u00fchrt, Orgel und Pauke, mit festlichem Klang", "tokens": ["F\u00fchrt", ",", "Or\u00b7gel", "und", "Pau\u00b7ke", ",", "mit", "fest\u00b7li\u00b7chem", "Klang"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "KON", "NE", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Entgegen des frohen Willkommens Gesang!", "tokens": ["Ent\u00b7ge\u00b7gen", "des", "fro\u00b7hen", "Will\u00b7kom\u00b7mens", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}