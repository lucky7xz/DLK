{"textgrid.poem.27813": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tichtern und Mahlern war weiland leichte geg\u00fcnnet", "tokens": ["Tich\u00b7tern", "und", "Mah\u00b7lern", "war", "wei\u00b7land", "leich\u00b7te", "ge\u00b7g\u00fcn\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "ADJA", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ans Licht zu bringen, was nur ihr Gehirne gek\u00fcnnet.", "tokens": ["Ans", "Licht", "zu", "brin\u00b7gen", ",", "was", "nur", "ihr", "Ge\u00b7hir\u00b7ne", "ge\u00b7k\u00fcn\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Soldaten steht ietzt durch sich selbst frey,", "tokens": ["Sol\u00b7da\u00b7ten", "steht", "ietzt", "durch", "sich", "selbst", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bald Teuffel, bald Gott, bald Mensch zu seyn.", "tokens": ["Bald", "Teuf\u00b7fel", ",", "bald", "Gott", ",", "bald", "Mensch", "zu", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "PTKZU", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}