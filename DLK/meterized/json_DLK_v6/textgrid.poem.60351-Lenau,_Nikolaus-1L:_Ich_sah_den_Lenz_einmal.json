{"textgrid.poem.60351": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sah den Lenz einmal", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah den Lenz einmal", "tokens": ["Ich", "sah", "den", "Lenz", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erwacht im sch\u00f6nsten Tal;", "tokens": ["Er\u00b7wacht", "im", "sch\u00f6ns\u00b7ten", "Tal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah der Liebe Licht", "tokens": ["Ich", "sah", "der", "Lie\u00b7be", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im sch\u00f6nsten Angesicht.", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wandl ich nun allein", "tokens": ["Und", "wandl", "ich", "nun", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Fr\u00fchling durch den Hain,", "tokens": ["Im", "Fr\u00fch\u00b7ling", "durch", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erscheint aus jedem Strauch", "tokens": ["Er\u00b7scheint", "aus", "je\u00b7dem", "Strauch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Angesicht mir auch.", "tokens": ["Ihr", "An\u00b7ge\u00b7sicht", "mir", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und seh ich sie am Ort,", "tokens": ["Und", "seh", "ich", "sie", "am", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo l\u00e4ngst der Fr\u00fchling fort,", "tokens": ["Wo", "l\u00e4ngst", "der", "Fr\u00fch\u00b7ling", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So sprie\u00dft ein Lenz und schallt", "tokens": ["So", "sprie\u00dft", "ein", "Lenz", "und", "schallt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Um ihre s\u00fc\u00dfe Gestalt.", "tokens": ["Um", "ih\u00b7re", "s\u00fc\u00b7\u00dfe", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}