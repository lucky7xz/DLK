{"dta.poem.1229": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die bestraffte N\u00e4scherey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem/ der nicht vonn\u00f6then hat", "tokens": ["Wohl", "dem", "/", "der", "nicht", "von\u00b7n\u00f6\u00b7then", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$(", "ART", "PTKNEG", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesunde Kost zu nehmen ein/", "tokens": ["Ge\u00b7sun\u00b7de", "Kost", "zu", "neh\u00b7men", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem an der herben Pillen statt", "tokens": ["Dem", "an", "der", "her\u00b7ben", "Pil\u00b7len", "statt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gelinde Zucker-K\u00f6rner seyn/", "tokens": ["Ge\u00b7lin\u00b7de", "Zu\u00b7cke\u00b7rK\u00f6r\u00b7ner", "seyn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem der beliebte Reben-Safft", "tokens": ["Dem", "der", "be\u00b7lieb\u00b7te", "Re\u00b7ben\u00b7\u00b7S\u00b7afft"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Vor s\u00fcssen Julep giebet Krafft.", "tokens": ["Vor", "s\u00fcs\u00b7sen", "Ju\u00b7lep", "gie\u00b7bet", "Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es schmeckte n\u00e4chst Clorellens Mund", "tokens": ["Es", "schmeck\u00b7te", "n\u00e4chst", "Clo\u00b7rel\u00b7lens", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus ohngef\u00e4hr gesch\u00f6pffter Lust/", "tokens": ["Aus", "ohn\u00b7ge\u00b7f\u00e4hr", "ge\u00b7sch\u00f6pff\u00b7ter", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was krancke Leute macht gesund.", "tokens": ["Was", "kran\u00b7cke", "Leu\u00b7te", "macht", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie schlecht bekam ihr diese Kost.", "tokens": ["Wie", "schlecht", "be\u00b7kam", "ihr", "die\u00b7se", "Kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was andern Krafft und St\u00e4rcke bracht/", "tokens": ["Was", "an\u00b7dern", "Krafft", "und", "St\u00e4r\u00b7cke", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das hatte sie bald schwach gemacht.", "tokens": ["Das", "hat\u00b7te", "sie", "bald", "schwach", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch geht es dir nicht so allein/", "tokens": ["Doch", "geht", "es", "dir", "nicht", "so", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Clorelle/ meine s\u00fcsse Zier:", "tokens": ["Clo\u00b7rel\u00b7le", "/", "mei\u00b7ne", "s\u00fcs\u00b7se", "Zier", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df auch so gestraffet seyn/", "tokens": ["Ich", "mu\u00df", "auch", "so", "ge\u00b7straf\u00b7fet", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leide gleiche Pein mit dir:", "tokens": ["Und", "lei\u00b7de", "glei\u00b7che", "Pein", "mit", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Blick/ der andre laben kan/", "tokens": ["Dein", "Blick", "/", "der", "and\u00b7re", "la\u00b7ben", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat meinem Hertzen weh gethan.", "tokens": ["Hat", "mei\u00b7nem", "Hert\u00b7zen", "weh", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der s\u00fcsse Vorschmack deiner Gunst", "tokens": ["Der", "s\u00fcs\u00b7se", "Vor\u00b7schmack", "dei\u00b7ner", "Gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erreget mir den kalten Brand;", "tokens": ["Er\u00b7re\u00b7get", "mir", "den", "kal\u00b7ten", "Brand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier hilfft mir keines Arztes Kunst/", "tokens": ["Hier", "hilfft", "mir", "kei\u00b7nes", "Arz\u00b7tes", "Kunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Wohlseyn steht in deiner Hand/", "tokens": ["Mein", "Wohl\u00b7seyn", "steht", "in", "dei\u00b7ner", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Eh ich kan deinen Zucker-Mund", "tokens": ["Eh", "ich", "kan", "dei\u00b7nen", "Zu\u00b7cke\u00b7rMund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}