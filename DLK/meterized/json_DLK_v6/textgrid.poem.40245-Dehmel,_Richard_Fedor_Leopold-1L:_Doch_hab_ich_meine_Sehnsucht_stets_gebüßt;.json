{"textgrid.poem.40245": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doch hab ich meine Sehnsucht stets geb\u00fc\u00dft;", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch hab ich meine Sehnsucht stets geb\u00fc\u00dft;", "tokens": ["Doch", "hab", "ich", "mei\u00b7ne", "Sehn\u00b7sucht", "stets", "ge\u00b7b\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ich ging nach Liebe aus auf allen Wegen,", "tokens": ["ich", "ging", "nach", "Lie\u00b7be", "aus", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "auf allen kam die Liebe mir entgegen,", "tokens": ["auf", "al\u00b7len", "kam", "die", "Lie\u00b7be", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "doch hab ich meine Sehnsucht stets geb\u00fc\u00dft ...", "tokens": ["doch", "hab", "ich", "mei\u00b7ne", "Sehn\u00b7sucht", "stets", "ge\u00b7b\u00fc\u00dft", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es stand ein Baum in einem Zaubergarten,", "tokens": ["Es", "stand", "ein", "Baum", "in", "ei\u00b7nem", "Zau\u00b7berg\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von tausend Bl\u00fcten duftete sein Bild,", "tokens": ["von", "tau\u00b7send", "Bl\u00fc\u00b7ten", "duf\u00b7te\u00b7te", "sein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "doch eine leuchtete vor allen mild;", "tokens": ["doch", "ei\u00b7ne", "leuch\u00b7te\u00b7te", "vor", "al\u00b7len", "mild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "APPR", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "es stand ein Baum in einem Zaubergarten.", "tokens": ["es", "stand", "ein", "Baum", "in", "ei\u00b7nem", "Zau\u00b7berg\u00b7ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und aus den tausend pfl\u00fcckte ich die eine,", "tokens": ["Und", "aus", "den", "tau\u00b7send", "pfl\u00fcck\u00b7te", "ich", "die", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "CARD", "VVFIN", "PPER", "ART", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sie war noch sch\u00f6ner mir in meinen H\u00e4nden;", "tokens": ["sie", "war", "noch", "sch\u00f6\u00b7ner", "mir", "in", "mei\u00b7nen", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ich aber kniete, Dank dem Baum zu spenden,", "tokens": ["ich", "a\u00b7ber", "knie\u00b7te", ",", "Dank", "dem", "Baum", "zu", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "von dem aus tausend ich gepfl\u00fcckt die eine.", "tokens": ["von", "dem", "aus", "tau\u00b7send", "ich", "ge\u00b7pfl\u00fcckt", "die", "ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "CARD", "PPER", "VVPP", "ART", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich hob die Augen zu dem Zauberbaume,", "tokens": ["Ich", "hob", "die", "Au\u00b7gen", "zu", "dem", "Zau\u00b7ber\u00b7bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und wieder schien vor allen Eine licht,", "tokens": ["und", "wie\u00b7der", "schien", "vor", "al\u00b7len", "Ei\u00b7ne", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und meine welkte schon \u2013 ich dankte nicht;", "tokens": ["und", "mei\u00b7ne", "welk\u00b7te", "schon", "\u2013", "ich", "dank\u00b7te", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ich hob die Augen zu dem Zauberbaume ...", "tokens": ["ich", "hob", "die", "Au\u00b7gen", "zu", "dem", "Zau\u00b7ber\u00b7bau\u00b7me", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch hab ich meine Sehnsucht nie verlernt;", "tokens": ["Doch", "hab", "ich", "mei\u00b7ne", "Sehn\u00b7sucht", "nie", "ver\u00b7lernt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ich ging nach Liebe aus auf allen Wegen,", "tokens": ["ich", "ging", "nach", "Lie\u00b7be", "aus", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "auf jedem gl\u00e4nzte mir ein andrer Segen,", "tokens": ["auf", "je\u00b7dem", "gl\u00e4nz\u00b7te", "mir", "ein", "an\u00b7drer", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "drum hab ich meine Sehnsucht nie verlernt.", "tokens": ["drum", "hab", "ich", "mei\u00b7ne", "Sehn\u00b7sucht", "nie", "ver\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}