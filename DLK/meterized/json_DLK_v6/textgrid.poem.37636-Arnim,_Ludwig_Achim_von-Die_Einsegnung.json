{"textgrid.poem.37636": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Einsegnung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise tret ich in die heil'gen Hallen,", "tokens": ["Lei\u00b7se", "tret", "ich", "in", "die", "heil'\u00b7gen", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kleine Kinder stehen vor der Th\u00fcr,", "tokens": ["Klei\u00b7ne", "Kin\u00b7der", "ste\u00b7hen", "vor", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4hnen, da\u00df ihr Beten nicht gefalle,", "tokens": ["W\u00e4h\u00b7nen", ",", "da\u00df", "ihr", "Be\u00b7ten", "nicht", "ge\u00b7fal\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wissen noch kein richtig Wort daf\u00fcr.", "tokens": ["Wis\u00b7sen", "noch", "kein", "rich\u00b7tig", "Wort", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJD", "NN", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Lasset doch die Kindlein zu mir kommen,", "tokens": ["Las\u00b7set", "doch", "die", "Kin\u00b7dlein", "zu", "mir", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wehret ihnen nicht! geschrieben steht", "tokens": ["Weh\u00b7ret", "ih\u00b7nen", "nicht", "!", "ge\u00b7schrie\u00b7ben", "steht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "VVPP", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf der Pforte ihnen zum Willkommen,", "tokens": ["Auf", "der", "Pfor\u00b7te", "ih\u00b7nen", "zum", "Will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wer nicht lesen kann, es nicht versteht.", "tokens": ["Wer", "nicht", "le\u00b7sen", "kann", ",", "es", "nicht", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVINF", "VMFIN", "$,", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wahrlich, wahrlich, das sei euch gesaget,", "tokens": ["Wahr\u00b7lich", ",", "wahr\u00b7lich", ",", "das", "sei", "euch", "ge\u00b7sa\u00b7get", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "PDS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wer nicht ist wie ihr, der tret' nicht ein,", "tokens": ["Wer", "nicht", "ist", "wie", "ihr", ",", "der", "tret'", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VAFIN", "KOKOM", "PPER", "$,", "PRELS", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo bewundrungsvoll so k\u00fchl es taget", "tokens": ["Wo", "be\u00b7wund\u00b7rungs\u00b7voll", "so", "k\u00fchl", "es", "ta\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ADJD", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch der Fenster bilderreichen Schein.", "tokens": ["Durch", "der", "Fens\u00b7ter", "bil\u00b7der\u00b7rei\u00b7chen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wie die Kr\u00e4nze mit dem goldnen Schimmer,", "tokens": ["Wie", "die", "Kr\u00e4n\u00b7ze", "mit", "dem", "gold\u00b7nen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die Gitterst\u00fchle f\u00fcr so manch Geschlecht,", "tokens": ["Wo", "die", "Git\u00b7ter\u00b7st\u00fch\u00b7le", "f\u00fcr", "so", "manch", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Gr\u00e4ber zahllos, ordnungslos wie Tr\u00fcmmer,", "tokens": ["Gr\u00e4\u00b7ber", "zahl\u00b7los", ",", "ord\u00b7nungs\u00b7los", "wie", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Orgel wandelnd rein und recht.", "tokens": ["Und", "die", "Or\u00b7gel", "wan\u00b7delnd", "rein", "und", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Nur in diesen ernsten, sanften T\u00f6nen", "tokens": ["Nur", "in", "die\u00b7sen", "erns\u00b7ten", ",", "sanf\u00b7ten", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wandelt noch von Gott ein Ebenbild,", "tokens": ["Wan\u00b7delt", "noch", "von", "Gott", "ein", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nur dein Lispeln kann, was lebt, vers\u00f6hnen,", "tokens": ["Nur", "dein", "Lis\u00b7peln", "kann", ",", "was", "lebt", ",", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "$,", "PWS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dein Ernst ist dieser Gr\u00e4ber Schild.", "tokens": ["Und", "dein", "Ernst", "ist", "die\u00b7ser", "Gr\u00e4\u00b7ber", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Alles, was hier dauret, ist vergangen,", "tokens": ["Al\u00b7les", ",", "was", "hier", "dau\u00b7ret", ",", "ist", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADV", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die da drau\u00dfen standen jugendfromm,", "tokens": ["Die", "da", "drau\u00b7\u00dfen", "stan\u00b7den", "ju\u00b7gend\u00b7fromm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gehen nicht mehr ein mit dem Verlangen,", "tokens": ["Ge\u00b7hen", "nicht", "mehr", "ein", "mit", "dem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ART", "APPR", "ART", "NN", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Weil der Flamme Funken hier entglomm.", "tokens": ["Weil", "der", "Flam\u00b7me", "Fun\u00b7ken", "hier", "ent\u00b7glomm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Seht sie heilig an, ihr frommen Bilder,", "tokens": ["Seht", "sie", "hei\u00b7lig", "an", ",", "ihr", "from\u00b7men", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sie verstehn nicht eurer Sehnsucht Blick,", "tokens": ["Sie", "ver\u00b7stehn", "nicht", "eu\u00b7rer", "Sehn\u00b7sucht", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und sie rei\u00dfen euch in St\u00fccken wilder,", "tokens": ["Und", "sie", "rei\u00b7\u00dfen", "euch", "in", "St\u00fc\u00b7cken", "wil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn ihr steht im Wege ihrem Gl\u00fcck.", "tokens": ["Denn", "ihr", "steht", "im", "We\u00b7ge", "ih\u00b7rem", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Schl\u00e4gt ein W\u00fcrgeengel die Gebornen,", "tokens": ["Schl\u00e4gt", "ein", "W\u00fcr\u00b7geen\u00b7gel", "die", "Ge\u00b7bor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rieselt auf den Saaten rothes Blut,", "tokens": ["Rie\u00b7selt", "auf", "den", "Saa\u00b7ten", "ro\u00b7thes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dann so zeigen sich auch die Erkornen,", "tokens": ["Dann", "so", "zei\u00b7gen", "sich", "auch", "die", "Er\u00b7kor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ich hab sie zu erwarten Muth.", "tokens": ["Und", "ich", "hab", "sie", "zu", "er\u00b7war\u00b7ten", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PTKZU", "VVINF", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Milde hat die Orgel euch gemahnet,", "tokens": ["Mil\u00b7de", "hat", "die", "Or\u00b7gel", "euch", "ge\u00b7mah\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und ich finde doch die Kirche leer,", "tokens": ["Und", "ich", "fin\u00b7de", "doch", "die", "Kir\u00b7che", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ein hoher Traum darin geahndet,", "tokens": ["Wenn", "ein", "ho\u00b7her", "Traum", "da\u00b7rin", "ge\u00b7ahn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn ein Geist darin gebannet w\u00e4r.", "tokens": ["Wenn", "ein", "Geist", "da\u00b7rin", "ge\u00b7ban\u00b7net", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PAV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Nein, wie sollt' ich das alleine h\u00f6ren,", "tokens": ["Nein", ",", "wie", "sollt'", "ich", "das", "al\u00b7lei\u00b7ne", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "VMFIN", "PPER", "PDS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sollt' ich sein alleine so entz\u00fcckt,", "tokens": ["Sollt'", "ich", "sein", "al\u00b7lei\u00b7ne", "so", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der die Falschen alle kann bekehren", "tokens": ["Der", "die", "Fal\u00b7schen", "al\u00b7le", "kann", "be\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "PIS", "VMFIN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Wahren allesammt begl\u00fcckt.", "tokens": ["Und", "die", "Wah\u00b7ren", "al\u00b7le\u00b7sammt", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Nein, gelobt der Herr, die Frommen kommen,", "tokens": ["Nein", ",", "ge\u00b7lobt", "der", "Herr", ",", "die", "From\u00b7men", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVPP", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00e4dchenschaaren paarweis, sch\u00fcchtern, bleich,", "tokens": ["M\u00e4d\u00b7chen\u00b7schaa\u00b7ren", "paar\u00b7weis", ",", "sch\u00fcch\u00b7tern", ",", "bleich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wollen sein zur Kirche aufgenommen,", "tokens": ["Wol\u00b7len", "sein", "zur", "Kir\u00b7che", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sind am Lebensbaum die frischen Zweig.", "tokens": ["Sind", "am", "Le\u00b7bens\u00b7baum", "die", "fri\u00b7schen", "Zweig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Wie der Engel \u00fcberm Taufstein bebet,", "tokens": ["Wie", "der", "En\u00b7gel", "\u00fc\u00b7berm", "Tauf\u00b7stein", "be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wie er freudig seine Hand bewegt,", "tokens": ["Wie", "er", "freu\u00b7dig", "sei\u00b7ne", "Hand", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die heil'ge, lichte Taube schwebet,", "tokens": ["Und", "die", "heil'\u00b7ge", ",", "lich\u00b7te", "Tau\u00b7be", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00dcber diese Schaar im Kreis sich tr\u00e4gt.", "tokens": ["\u00dc\u00b7ber", "die\u00b7se", "Schaar", "im", "Kreis", "sich", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Und von allen diesen glatten Stirnen,", "tokens": ["Und", "von", "al\u00b7len", "die\u00b7sen", "glat\u00b7ten", "Stir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist nur eine, wo ein Fl\u00e4mmlein brennt,", "tokens": ["Ist", "nur", "ei\u00b7ne", ",", "wo", "ein", "Fl\u00e4mm\u00b7lein", "brennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sch\u00f6n Gestade, la\u00df die Winde st\u00fcrmen,", "tokens": ["Sch\u00f6n", "Ge\u00b7sta\u00b7de", ",", "la\u00df", "die", "Win\u00b7de", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der erfahrne Schiffer dich erkennt.", "tokens": ["Der", "er\u00b7fahr\u00b7ne", "Schif\u00b7fer", "dich", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "H\u00fclflos war sie einst wie andre Kinder,", "tokens": ["H\u00fcl\u00b7flos", "war", "sie", "einst", "wie", "and\u00b7re", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eh' das heil'ge Wasser sie getauft,", "tokens": ["Eh'", "das", "heil'\u00b7ge", "Was\u00b7ser", "sie", "ge\u00b7tauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch das Feuer taufet den geschwinder,", "tokens": ["Doch", "das", "Feu\u00b7er", "tau\u00b7fet", "den", "ge\u00b7schwin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der's mit seiner ganzen Seele kauft.", "tokens": ["Der's", "mit", "sei\u00b7ner", "gan\u00b7zen", "See\u00b7le", "kauft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Ernst spricht jetzt der Pred'ger in der Mitte,", "tokens": ["Ernst", "spricht", "jetzt", "der", "Pre\u00b7d'\u00b7ger", "in", "der", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00c4ltern, Diener, Freunde weinen fern,", "tokens": ["\u00c4l\u00b7tern", ",", "Die\u00b7ner", ",", "Freun\u00b7de", "wei\u00b7nen", "fern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Beten, da\u00df der Herr erh\u00f6r' die Bitte,", "tokens": ["Be\u00b7ten", ",", "da\u00df", "der", "Herr", "er\u00b7h\u00f6r'", "die", "Bit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "S\u00e4h' in ihrer Pr\u00fcfungsstund' sie gern.", "tokens": ["S\u00e4h'", "in", "ih\u00b7rer", "Pr\u00fc\u00b7fungs\u00b7stund'", "sie", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "Alle sagen an die Glaubensworte,", "tokens": ["Al\u00b7le", "sa\u00b7gen", "an", "die", "Glau\u00b7bens\u00b7wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Geben nach der Vorschrift hin die Hand,", "tokens": ["Ge\u00b7ben", "nach", "der", "Vor\u00b7schrift", "hin", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ach wo bleibt die Eine und ich warte,", "tokens": ["Ach", "wo", "bleibt", "die", "Ei\u00b7ne", "und", "ich", "war\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VVFIN", "ART", "NN", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach ich seh im \u00f6den Meere Land.", "tokens": ["Ach", "ich", "seh", "im", "\u00f6\u00b7den", "Mee\u00b7re", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.17": {"line.1": {"text": "Eine, jene, k\u00f6nnt' ich sie nur nennen,", "tokens": ["Ei\u00b7ne", ",", "je\u00b7ne", ",", "k\u00f6nnt'", "ich", "sie", "nur", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PDS", "$,", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ist es jener reine Orgelgeist,", "tokens": ["Ist", "es", "je\u00b7ner", "rei\u00b7ne", "Or\u00b7gel\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht in Sch\u00f6nheit will die Heil'ge brennen,", "tokens": ["Nicht", "in", "Sch\u00f6n\u00b7heit", "will", "die", "Heil'\u00b7ge", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Locken sind verstecket meist.", "tokens": ["Ih\u00b7re", "Lo\u00b7cken", "sind", "ver\u00b7ste\u00b7cket", "meist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.18": {"line.1": {"text": "Eine Trauerahndung in den Augen,", "tokens": ["Ei\u00b7ne", "Trau\u00b7e\u00b7rahn\u00b7dung", "in", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Um die Lippen sanfter Duldersinn,", "tokens": ["Um", "die", "Lip\u00b7pen", "sanf\u00b7ter", "Dul\u00b7der\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch zum Sprechen nicht die Lippen taugen,", "tokens": ["Doch", "zum", "Spre\u00b7chen", "nicht", "die", "Lip\u00b7pen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie err\u00f6thet schwebend bis zum Kinn.", "tokens": ["Sie", "er\u00b7r\u00f6t\u00b7het", "schwe\u00b7bend", "bis", "zum", "Kinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.19": {"line.1": {"text": "Beide H\u00e4nde legt sie vor die Stirne,", "tokens": ["Bei\u00b7de", "H\u00e4n\u00b7de", "legt", "sie", "vor", "die", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sinket vor dem Pred'ger sprachlos hin,", "tokens": ["Sin\u00b7ket", "vor", "dem", "Pre\u00b7d'\u00b7ger", "sprach\u00b7los", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mitleid l\u00e4chelt manche kluge Dirne,", "tokens": ["Mit\u00b7leid", "l\u00e4\u00b7chelt", "man\u00b7che", "klu\u00b7ge", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch die Taube schwebet in dein Sinn.", "tokens": ["Doch", "die", "Tau\u00b7be", "schwe\u00b7bet", "in", "dein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.20": {"line.1": {"text": "\u00dcber ihr mit gleichen Fl\u00fcgeln schwebet,", "tokens": ["\u00dc\u00b7ber", "ihr", "mit", "glei\u00b7chen", "Fl\u00fc\u00b7geln", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hebt die Nacht von ihrem Auge weg,", "tokens": ["Hebt", "die", "Nacht", "von", "ih\u00b7rem", "Au\u00b7ge", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df sie auch f\u00fcr alle heilig lebet,", "tokens": ["Da\u00df", "sie", "auch", "f\u00fcr", "al\u00b7le", "hei\u00b7lig", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nimmt sie Schwindel von dem Himmelssteg.", "tokens": ["Nimmt", "sie", "Schwin\u00b7del", "von", "dem", "Him\u00b7melss\u00b7teg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.21": {"line.1": {"text": "Denn sie sah erhoben sich zum Blauen", "tokens": ["Denn", "sie", "sah", "er\u00b7ho\u00b7ben", "sich", "zum", "Blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und den Pred'ger sah sie unter sich,", "tokens": ["Und", "den", "Pre\u00b7d'\u00b7ger", "sah", "sie", "un\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Beide H\u00e4nde reicht sie mit Vertrauen,", "tokens": ["Bei\u00b7de", "H\u00e4n\u00b7de", "reicht", "sie", "mit", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eine ihm, die andre fasse ich.", "tokens": ["Ei\u00b7ne", "ihm", ",", "die", "and\u00b7re", "fas\u00b7se", "ich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PRELS", "PIS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.22": {"line.1": {"text": "La\u00df du heil'ger Geist sie nimmer sinken,", "tokens": ["La\u00df", "du", "heil'\u00b7ger", "Geist", "sie", "nim\u00b7mer", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O so bleibt die Hand auch immer mein,", "tokens": ["O", "so", "bleibt", "die", "Hand", "auch", "im\u00b7mer", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hohes Kind, du lallest, kannst nur winken,", "tokens": ["Ho\u00b7hes", "Kind", ",", "du", "lal\u00b7lest", ",", "kannst", "nur", "win\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mache uns von allen S\u00fcnden rein.", "tokens": ["Ma\u00b7che", "uns", "von", "al\u00b7len", "S\u00fcn\u00b7den", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.23": {"line.1": {"text": "Wie, du h\u00f6rst nicht in den k\u00fchlen Fluthen,", "tokens": ["Wie", ",", "du", "h\u00f6rst", "nicht", "in", "den", "k\u00fch\u00b7len", "Flut\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus der hei\u00dfen Welt hinaufgeweht,", "tokens": ["Aus", "der", "hei\u00b7\u00dfen", "Welt", "hin\u00b7auf\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcber dir die ew'gen Morgengluthen", "tokens": ["\u00dc\u00b7ber", "dir", "die", "ew'\u00b7gen", "Mor\u00b7gen\u00b7glu\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Thaues Perlen auf dem Busen seht.", "tokens": ["Thau\u00b7es", "Per\u00b7len", "auf", "dem", "Bu\u00b7sen", "seht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.24": {"line.1": {"text": "H\u00f6r' ich doch den Geist noch wieder t\u00f6nen,", "tokens": ["H\u00f6r'", "ich", "doch", "den", "Geist", "noch", "wie\u00b7der", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Orgel ernstem Trauerspiel,", "tokens": ["In", "der", "Or\u00b7gel", "erns\u00b7tem", "Trau\u00b7er\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Was die Welt im Leichtsinn kann vers\u00f6hnen,", "tokens": ["Was", "die", "Welt", "im", "Leicht\u00b7sinn", "kann", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist ein Tod in seligem Gef\u00fchl.", "tokens": ["Ist", "ein", "Tod", "in", "se\u00b7li\u00b7gem", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.25": {"line.1": {"text": "Nein, du lebst noch, aber diese Schrecken", "tokens": ["Nein", ",", "du", "lebst", "noch", ",", "a\u00b7ber", "die\u00b7se", "Schre\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV", "$,", "ADV", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Haben ausgetilgt die irdsche Lieb',", "tokens": ["Ha\u00b7ben", "aus\u00b7ge\u00b7tilgt", "die", "ird\u00b7sche", "Lieb'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Keine Rosen k\u00f6nnen Sehnsucht wecken,", "tokens": ["Kei\u00b7ne", "Ro\u00b7sen", "k\u00f6n\u00b7nen", "Sehn\u00b7sucht", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gegen Himmelsrosen sind sie tr\u00fcb'.", "tokens": ["Ge\u00b7gen", "Him\u00b7mels\u00b7ro\u00b7sen", "sind", "sie", "tr\u00fcb'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.26": {"line.1": {"text": "O so sei dein Leben ein Verk\u00fcnden,", "tokens": ["O", "so", "sei", "dein", "Le\u00b7ben", "ein", "Ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und mein Leben meine Demuth sei,", "tokens": ["Und", "mein", "Le\u00b7ben", "mei\u00b7ne", "De\u00b7muth", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Liebe kann uns nimmermehr verbinden,", "tokens": ["Lie\u00b7be", "kann", "uns", "nim\u00b7mer\u00b7mehr", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00f6h're Liebe l\u00e4\u00dft dich nimmer frei.", "tokens": ["H\u00f6h'\u00b7re", "Lie\u00b7be", "l\u00e4\u00dft", "dich", "nim\u00b7mer", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.27": {"line.1": {"text": "Sei gegr\u00fc\u00dft wie jene Gnadenbilder,", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", "wie", "je\u00b7ne", "Gna\u00b7den\u00b7bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KOKOM", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus der Vorzeit ihr verlassen steht,", "tokens": ["Aus", "der", "Vor\u00b7zeit", "ihr", "ver\u00b7las\u00b7sen", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "T\u00e4glich seht ihr gnadenreich und milder,", "tokens": ["T\u00e4g\u00b7lich", "seht", "ihr", "gna\u00b7den\u00b7reich", "und", "mil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn auch keiner hier euch mehr versteht.", "tokens": ["Wenn", "auch", "kei\u00b7ner", "hier", "euch", "mehr", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.28": {"line.1": {"text": "Sieh, Maria, wer den Blick empfindet,", "tokens": ["Sieh", ",", "Ma\u00b7ria", ",", "wer", "den", "Blick", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den du nieder auf die Welt gesandt,", "tokens": ["Den", "du", "nie\u00b7der", "auf", "die", "Welt", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKVZ", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als der Engel sich dir hat verk\u00fcndet,", "tokens": ["Als", "der", "En\u00b7gel", "sich", "dir", "hat", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hat dich auch in seiner H\u00f6h' erkannt.", "tokens": ["Hat", "dich", "auch", "in", "sei\u00b7ner", "H\u00f6h'", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.29": {"line.1": {"text": "Nicht um Dienste, nicht um Wallfahrtgaben,", "tokens": ["Nicht", "um", "Diens\u00b7te", ",", "nicht", "um", "Wall\u00b7fahrt\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nein, dem Treuen zeigtest du den Sinn,", "tokens": ["Nein", ",", "dem", "Treu\u00b7en", "zeig\u00b7test", "du", "den", "Sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4hrend auf dem Haupt dir nisten Raben,", "tokens": ["W\u00e4h\u00b7rend", "auf", "dem", "Haupt", "dir", "nis\u00b7ten", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00dcberm Herzen h\u00e4ngt ihr Netz die Spinn.", "tokens": ["\u00dc\u00b7berm", "Her\u00b7zen", "h\u00e4ngt", "ihr", "Netz", "die", "Spinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.30": {"line.1": {"text": "Alle Stern' des heiligen Gewandes", "tokens": ["Al\u00b7le", "Stern'", "des", "hei\u00b7li\u00b7gen", "Ge\u00b7wan\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sind erloschen vor dem h\u00f6hern Sohn,", "tokens": ["Sind", "er\u00b7lo\u00b7schen", "vor", "dem", "h\u00f6\u00b7hern", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dessen Bild noch heut das Gl\u00fcck des Landes,", "tokens": ["Des\u00b7sen", "Bild", "noch", "heut", "das", "Gl\u00fcck", "des", "Lan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sieht sie ihn am Kreuz im h\u00f6chsten Thron.", "tokens": ["Sieht", "sie", "ihn", "am", "Kreuz", "im", "h\u00f6chs\u00b7ten", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.31": {"line.1": {"text": "Ja auf Erden, was aus Liebe kommen,", "tokens": ["Ja", "auf", "Er\u00b7den", ",", "was", "aus", "Lie\u00b7be", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wird in Leiden sich allein bewu\u00dft,", "tokens": ["Wird", "in", "Lei\u00b7den", "sich", "al\u00b7lein", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ich in der Kirche aufgenommen,", "tokens": ["Wie", "ich", "in", "der", "Kir\u00b7che", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fchlt ich nichts von all der hohen Lust.", "tokens": ["F\u00fchlt", "ich", "nichts", "von", "all", "der", "ho\u00b7hen", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.32": {"line.1": {"text": "Trostlos Kl\u00fcgeln war in aller Herzen,", "tokens": ["Trost\u00b7los", "Kl\u00fc\u00b7geln", "war", "in", "al\u00b7ler", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und ein Streiten, was es doch bedeut,", "tokens": ["Und", "ein", "Strei\u00b7ten", ",", "was", "es", "doch", "be\u00b7deut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Was dreieinig, was das Blut der Schmerzen,", "tokens": ["Was", "drei\u00b7ei\u00b7nig", ",", "was", "das", "Blut", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "PRELS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was das Brod des Lebens uns bereit.", "tokens": ["Was", "das", "Brod", "des", "Le\u00b7bens", "uns", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.33": {"line.1": {"text": "Alle Stimmen sinken, wenn sie lange,", "tokens": ["Al\u00b7le", "Stim\u00b7men", "sin\u00b7ken", ",", "wenn", "sie", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sangen so alleine in der Ruh;", "tokens": ["San\u00b7gen", "so", "al\u00b7lei\u00b7ne", "in", "der", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch die Orgel h\u00e4lt sie fest im Klange,", "tokens": ["Doch", "die", "Or\u00b7gel", "h\u00e4lt", "sie", "fest", "im", "Klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Noth l\u00e4\u00dft kein Versinken zu.", "tokens": ["Und", "die", "Noth", "l\u00e4\u00dft", "kein", "Ver\u00b7sin\u00b7ken", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.34": {"line.1": {"text": "Harte Zeit nur kann die Kirche f\u00fcllen,", "tokens": ["Har\u00b7te", "Zeit", "nur", "kann", "die", "Kir\u00b7che", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die ver\u00f6det stand nach leerem Streit,", "tokens": ["Die", "ver\u00b7\u00f6\u00b7det", "stand", "nach", "lee\u00b7rem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und ich ahnd' es gl\u00e4ubig hier im Stillen,", "tokens": ["Und", "ich", "ahnd'", "es", "gl\u00e4u\u00b7big", "hier", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich und mich trennt heil'gend diese Zeit.", "tokens": ["Dich", "und", "mich", "trennt", "heil'\u00b7gend", "die\u00b7se", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "VVFIN", "ADJD", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}