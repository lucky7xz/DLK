{"textgrid.poem.62215": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Die Spitzen-Kl\u00f6pplerin im Harz", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im wei\u00dfen Gewande von Spitzenzeug,", "tokens": ["Im", "wei\u00b7\u00dfen", "Ge\u00b7wan\u00b7de", "von", "Spit\u00b7zen\u00b7zeug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Bl\u00fcten in braunen Locken,", "tokens": ["Die", "Bl\u00fc\u00b7ten", "in", "brau\u00b7nen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So sieht man das Bildnis, das sch\u00f6ne Weib,", "tokens": ["So", "sieht", "man", "das", "Bild\u00b7nis", ",", "das", "sch\u00f6\u00b7ne", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dort oben hoch thronen am Brocken.", "tokens": ["Dort", "o\u00b7ben", "hoch", "thro\u00b7nen", "am", "Bro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Was ist Dir denn heut, und was weinest Du Kind?", "tokens": ["Was", "ist", "Dir", "denn", "heut", ",", "und", "was", "wei\u00b7nest", "Du", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "$,", "KON", "PWS", "VVFIN", "PPER", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich liebe nur lustige Leute,", "tokens": ["Ich", "lie\u00b7be", "nur", "lus\u00b7ti\u00b7ge", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dein Auge ist na\u00df, und Dein L\u00e4cheln ist tr\u00fcb,", "tokens": ["Dein", "Au\u00b7ge", "ist", "na\u00df", ",", "und", "Dein", "L\u00e4\u00b7cheln", "ist", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Du bist ja so schwerm\u00fctig heute. \u2013", "tokens": ["Du", "bist", "ja", "so", "schwer\u00b7m\u00fc\u00b7tig", "heu\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "ADV", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Mein Auge ist na\u00df und mein L\u00e4cheln ist tr\u00fcb,", "tokens": ["Mein", "Au\u00b7ge", "ist", "na\u00df", "und", "mein", "L\u00e4\u00b7cheln", "ist", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich bin, ach, so schwerm\u00fctig heute,", "tokens": ["Ich", "bin", ",", "ach", ",", "so", "schwer\u00b7m\u00fc\u00b7tig", "heu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ITJ", "$,", "ADV", "ADJD", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mich plaget ein Leid, ach, ein m\u00e4chtiges Leid,", "tokens": ["Mich", "pla\u00b7get", "ein", "Leid", ",", "ach", ",", "ein", "m\u00e4ch\u00b7ti\u00b7ges", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ITJ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich hasse den Ballanzug heute.", "tokens": ["Ich", "has\u00b7se", "den", "Bal\u00b7lan\u00b7zug", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Ich hasse die Spitzen aus Tr\u00e4nen gewebt,", "tokens": ["Ich", "has\u00b7se", "die", "Spit\u00b7zen", "aus", "Tr\u00e4\u00b7nen", "ge\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Drin werden zu Wasser die Freuden,", "tokens": ["Drin", "wer\u00b7den", "zu", "Was\u00b7ser", "die", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein Wehe, ein Seufzen da drinnen lebt,", "tokens": ["Ein", "We\u00b7he", ",", "ein", "Seuf\u00b7zen", "da", "drin\u00b7nen", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ein Chaos von bittersten Leiden! \u2013", "tokens": ["Ein", "Chaos", "von", "bit\u00b7ters\u00b7ten", "Lei\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}