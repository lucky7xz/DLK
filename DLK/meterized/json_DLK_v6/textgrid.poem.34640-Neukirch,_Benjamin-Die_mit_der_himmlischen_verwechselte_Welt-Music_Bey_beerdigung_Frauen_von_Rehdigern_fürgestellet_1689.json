{"textgrid.poem.34640": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Die mit der himmlischen verwechselte Welt-Music/ Bey beerdigung Frauen von Rehdigern f\u00fcrgestellet 1689", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachdem des gl\u00fcckes ball/ der liebe gauckel-spiel/", "tokens": ["Nach\u00b7dem", "des", "gl\u00fc\u00b7ckes", "ball", "/", "der", "lie\u00b7be", "gau\u00b7ckel\u00b7spiel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Prince\u00dfin Anna/ ward aus Engelland vertrieben/", "tokens": ["Prin\u00b7ce\u00b7\u00dfin", "An\u00b7na", "/", "ward", "aus", "En\u00b7gel\u00b7land", "ver\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VAFIN", "APPR", "NE", "VVPP", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Weil k\u00f6nig Heinrich nicht ihr wesen konte lieben/", "tokens": ["Weil", "k\u00f6\u00b7nig", "Hein\u00b7rich", "nicht", "ihr", "we\u00b7sen", "kon\u00b7te", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "PTKNEG", "PPER", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seiner alten haut ihr marmel mi\u00dfgefiel;", "tokens": ["Und", "sei\u00b7ner", "al\u00b7ten", "haut", "ihr", "mar\u00b7mel", "mi\u00df\u00b7ge\u00b7fiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ri\u00df das betr\u00fcbte kind den wechsel ihres orden/", "tokens": ["Ri\u00df", "das", "be\u00b7tr\u00fcb\u00b7te", "kind", "den", "wech\u00b7sel", "ih\u00b7res", "or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der ihr vor sonnenschein nun drachen-blicke gab/", "tokens": ["Der", "ihr", "vor", "son\u00b7nen\u00b7schein", "nun", "dra\u00b7chen\u00b7bli\u00b7cke", "gab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADV", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vor grosser hertzens-angst in einer lauten ab/", "tokens": ["Vor", "gros\u00b7ser", "hert\u00b7zens\u00b7angst", "in", "ei\u00b7ner", "lau\u00b7ten", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit dieser \u00fcberschrifft: Sie ist zu thr\u00e4nen worden.", "tokens": ["Mit", "die\u00b7ser", "\u00fc\u00b7bersc\u00b7hrifft", ":", "Sie", "ist", "zu", "thr\u00e4\u00b7nen", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVPP", "$.", "PPER", "VAFIN", "APPR", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer heute noch den thon der sterbligkeit erreicht/", "tokens": ["Wer", "heu\u00b7te", "noch", "den", "thon", "der", "ster\u00b7blig\u00b7keit", "er\u00b7reicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird wie taranteln auch leicht in den adern f\u00fchlen/", "tokens": ["Wird", "wie", "ta\u00b7ran\u00b7teln", "auch", "leicht", "in", "den", "a\u00b7dern", "f\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df unser gantzes thun nur s\u00fcssen saiten-spielen/", "tokens": ["Da\u00df", "un\u00b7ser", "gant\u00b7zes", "thun", "nur", "s\u00fcs\u00b7sen", "sai\u00b7ten\u00b7spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und unser gl\u00fccke sich mit schwachen lauten gleicht.", "tokens": ["Und", "un\u00b7ser", "gl\u00fc\u00b7cke", "sich", "mit", "schwa\u00b7chen", "lau\u00b7ten", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn wenn die saiten offt am allerhellsten klingen/", "tokens": ["Denn", "wenn", "die", "sai\u00b7ten", "offt", "am", "al\u00b7ler\u00b7hells\u00b7ten", "klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "VVFIN", "ADV", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wird das gantze spiel durch einen bruch verr\u00fcckt;", "tokens": ["So", "wird", "das", "gant\u00b7ze", "spiel", "durch", "ei\u00b7nen", "bruch", "ver\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So/ wenn den sterblichen der freuden anfang gl\u00fcckt/", "tokens": ["So", "/", "wenn", "den", "sterb\u00b7li\u00b7chen", "der", "freu\u00b7den", "an\u00b7fang", "gl\u00fcckt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "ADJA", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mu\u00df offt das ende nichts als schwere thr\u00e4nen bringen.", "tokens": ["Mu\u00df", "offt", "das", "en\u00b7de", "nichts", "als", "schwe\u00b7re", "thr\u00e4\u00b7nen", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PIS", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wir fangen schon die lust in kinder-r\u00f6cken an/", "tokens": ["Wir", "fan\u00b7gen", "schon", "die", "lust", "in", "kin\u00b7der\u00b7r\u00f6\u00b7cken", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wissen weder maa\u00df noch grentzen auszusetzen;", "tokens": ["Und", "wis\u00b7sen", "we\u00b7der", "maa\u00df", "noch", "grent\u00b7zen", "aus\u00b7zu\u00b7set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVFIN", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn bald ein apffel uns/ bald zucker mehr ergetzen/", "tokens": ["Wenn", "bald", "ein", "apf\u00b7fel", "uns", "/", "bald", "zu\u00b7cker", "mehr", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "$(", "ADV", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als affen honigseim im hunger tr\u00f6sten kan.", "tokens": ["Als", "af\u00b7fen", "ho\u00b7ni\u00b7gseim", "im", "hun\u00b7ger", "tr\u00f6s\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die erste stimmung sind die lustigen geberden/", "tokens": ["Die", "ers\u00b7te", "stim\u00b7mung", "sind", "die", "lus\u00b7ti\u00b7gen", "ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das lachen aber ist das wahre saiten-spiel;", "tokens": ["Das", "la\u00b7chen", "a\u00b7ber", "ist", "das", "wah\u00b7re", "sai\u00b7ten\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch wenn die mutter uns das gringste nehmen will/", "tokens": ["Doch", "wenn", "die", "mut\u00b7ter", "uns", "das", "grings\u00b7te", "neh\u00b7men", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ART", "ADJA", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So sieht man spiel und lust zu saltz und thr\u00e4nen werden.", "tokens": ["So", "sieht", "man", "spiel", "und", "lust", "zu", "saltz", "und", "thr\u00e4\u00b7nen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "KON", "VVFIN", "PTKA", "ADJD", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mit zeit und alter w\u00e4chst auch die ergetzligkeit/", "tokens": ["Mit", "zeit", "und", "al\u00b7ter", "w\u00e4chst", "auch", "die", "er\u00b7getz\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie farben mit der frucht und schatten mit den zweigen/", "tokens": ["Wie", "far\u00b7ben", "mit", "der", "frucht", "und", "schat\u00b7ten", "mit", "den", "zwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der l\u00e4st sein hochmuths-lied bi\u00df an die wolcken steigen/", "tokens": ["Der", "l\u00e4st", "sein", "hoch\u00b7muths\u00b7lied", "bi\u00df", "an", "die", "wol\u00b7cken", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein ander wird durch gold- und silber-klang erfreut;", "tokens": ["Ein", "an\u00b7der", "wird", "durch", "gold", "und", "sil\u00b7ber\u00b7klang", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPR", "TRUNC", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch/ weil man ohne tact das beste lied verderben/", "tokens": ["Doch", "/", "weil", "man", "oh\u00b7ne", "tact", "das", "bes\u00b7te", "lied", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PIS", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit vielem klange nur die ohren schw\u00e4chen kan;", "tokens": ["Mit", "vie\u00b7lem", "klan\u00b7ge", "nur", "die", "oh\u00b7ren", "schw\u00e4\u00b7chen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was wunder ist es denn/ da\u00df aberwitz und wahn/", "tokens": ["Was", "wun\u00b7der", "ist", "es", "denn", "/", "da\u00df", "a\u00b7ber\u00b7witz", "und", "wahn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "ADV", "$(", "KOUS", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nach unterbrochner lust/ auch saure thr\u00e4nen erben?", "tokens": ["Nach", "un\u00b7ter\u00b7broch\u00b7ner", "lust", "/", "auch", "sau\u00b7re", "thr\u00e4\u00b7nen", "er\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die sch\u00f6nste stimmung ist/ die nach der liebe klingt/", "tokens": ["Die", "sch\u00f6ns\u00b7te", "stim\u00b7mung", "ist", "/", "die", "nach", "der", "lie\u00b7be", "klingt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$(", "ART", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was aber mu\u00df auch hier vor lange zeit verschwinden/", "tokens": ["Was", "a\u00b7ber", "mu\u00df", "auch", "hier", "vor", "lan\u00b7ge", "zeit", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Eh man den rechten thon der hertzen lernt ergr\u00fcnden/", "tokens": ["Eh", "man", "den", "rech\u00b7ten", "thon", "der", "hert\u00b7zen", "lernt", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und alle regungen in reine noten bringt?", "tokens": ["Und", "al\u00b7le", "re\u00b7gun\u00b7gen", "in", "rei\u00b7ne", "no\u00b7ten", "bringt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ja wenn auch mann und weib wie der magnet mit norden/", "tokens": ["Ja", "wenn", "auch", "mann", "und", "weib", "wie", "der", "mag\u00b7net", "mit", "nor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "ADV", "NN", "KON", "VVFIN", "KOKOM", "ART", "VVFIN", "APPR", "ADJA", "$("], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "In ihrer liebe gleich/ und beyde stimmig seyn:", "tokens": ["In", "ih\u00b7rer", "lie\u00b7be", "gleich", "/", "und", "bey\u00b7de", "stim\u00b7mig", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "ADV", "$(", "KON", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So stellt der blasse tod das gantze spielen ein/", "tokens": ["So", "stellt", "der", "blas\u00b7se", "tod", "das", "gant\u00b7ze", "spie\u00b7len", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und schreibt auff ihre lust: sie ist zu thr\u00e4nen worden.", "tokens": ["Und", "schreibt", "auff", "ih\u00b7re", "lust", ":", "sie", "ist", "zu", "thr\u00e4\u00b7nen", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "APPR", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mein Herr/ sein liebster schatz/ der auff der bahre liegt/", "tokens": ["Mein", "Herr", "/", "sein", "liebs\u00b7ter", "schatz", "/", "der", "auff", "d