{"textgrid.poem.55410": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Bergesh\u00f6hn warum so schwarz?", "tokens": ["Die", "Ber\u00b7ges\u00b7h\u00f6hn", "wa\u00b7rum", "so", "schwarz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Woher die Wolkenwoge?", "tokens": ["Wo\u00b7her", "die", "Wol\u00b7ken\u00b7wo\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist es der Sturm, der droben k\u00e4mpft,", "tokens": ["Ist", "es", "der", "Sturm", ",", "der", "dro\u00b7ben", "k\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Regen, Gipfel peitschend?", "tokens": ["Der", "Re\u00b7gen", ",", "Gip\u00b7fel", "peit\u00b7schend", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht ist's der Sturm, der droben k\u00e4mpft,", "tokens": ["Nicht", "ist's", "der", "Sturm", ",", "der", "dro\u00b7ben", "k\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht Regen, Gipfel peitschend;", "tokens": ["Nicht", "Re\u00b7gen", ",", "Gip\u00b7fel", "peit\u00b7schend", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nein, Charon ist's, er saust einher,", "tokens": ["Nein", ",", "Cha\u00b7ron", "ist's", ",", "er", "saust", "ein\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "NE", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Entf\u00fchret die Verblichnen;", "tokens": ["Ent\u00b7f\u00fch\u00b7ret", "die", "Ver\u00b7blich\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Jungen treibt er vor sich hin,", "tokens": ["Die", "Jun\u00b7gen", "treibt", "er", "vor", "sich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Schleppt hinter sich die Alten;", "tokens": ["Schleppt", "hin\u00b7ter", "sich", "die", "Al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die J\u00fcngsten aber, S\u00e4uglinge,", "tokens": ["Die", "J\u00fcng\u00b7sten", "a\u00b7ber", ",", "S\u00e4ug\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "NN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.12": {"text": "In Reih gehenkt am Sattel.", "tokens": ["In", "Reih", "ge\u00b7henkt", "am", "Sat\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Da riefen ihm die Greise zu,", "tokens": ["Da", "rie\u00b7fen", "ihm", "die", "Grei\u00b7se", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die J\u00fcnglinge, sie knieten:", "tokens": ["Die", "J\u00fcng\u00b7lin\u00b7ge", ",", "sie", "knie\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.15": {"text": "\u00bbo Charon, halt'! halt' am Geheg,", "tokens": ["\u00bb", "o", "Cha\u00b7ron", ",", "halt'", "!", "halt'", "am", "Ge\u00b7heg", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$,", "VVFIN", "$.", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.16": {"text": "Halt an beim k\u00fchlen Brunnen!", "tokens": ["Halt", "an", "beim", "k\u00fch\u00b7len", "Brun\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Die Alten da erquicken sich,", "tokens": ["Die", "Al\u00b7ten", "da", "er\u00b7qui\u00b7cken", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Jugend schleudert Steine,", "tokens": ["Die", "Ju\u00b7gend", "schleu\u00b7dert", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Die Knaben zart zerstreuen sich", "tokens": ["Die", "Kna\u00b7ben", "zart", "zer\u00b7streu\u00b7en", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und pfl\u00fccken bunte Bl\u00fcmchen.\u00ab", "tokens": ["Und", "pfl\u00fc\u00b7cken", "bun\u00b7te", "Bl\u00fcm\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbnicht am Gehege halt ich still,", "tokens": ["\u00bb", "nicht", "am", "Ge\u00b7he\u00b7ge", "halt", "ich", "still", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ich halte nicht am Brunnen;", "tokens": ["Ich", "hal\u00b7te", "nicht", "am", "Brun\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu sch\u00f6pfen kommen Weiber an,", "tokens": ["Zu", "sch\u00f6p\u00b7fen", "kom\u00b7men", "Wei\u00b7ber", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erkennen ihre Kinder,", "tokens": ["Er\u00b7ken\u00b7nen", "ih\u00b7re", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die M\u00e4nner auch erkennen sie,", "tokens": ["Die", "M\u00e4n\u00b7ner", "auch", "er\u00b7ken\u00b7nen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Trennen wird unm\u00f6glich.\u00ab", "tokens": ["Das", "Tren\u00b7nen", "wird", "un\u00b7m\u00f6g\u00b7lich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}