{"textgrid.poem.52119": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Frieden, darauff wir so flei\u00dfig untergehn,", "tokens": ["Den", "Frie\u00b7den", ",", "dar\u00b7auff", "wir", "so", "flei\u00b7\u00dfig", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Frieden, darnach wir schon drei\u00dfig Jahre stehn:", "tokens": ["Den", "Frie\u00b7den", ",", "dar\u00b7nach", "wir", "schon", "drei\u00b7\u00dfig", "Jah\u00b7re", "stehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Frieden, darum wir mit Gold und Silber lauffen,", "tokens": ["Den", "Frie\u00b7den", ",", "da\u00b7rum", "wir", "mit", "Gold", "und", "Sil\u00b7ber", "lauf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird man zu allerletzt umb einen B\u00f6hmen kauffen.", "tokens": ["Wird", "man", "zu", "al\u00b7ler\u00b7letzt", "umb", "ei\u00b7nen", "B\u00f6h\u00b7men", "kauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKA", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+