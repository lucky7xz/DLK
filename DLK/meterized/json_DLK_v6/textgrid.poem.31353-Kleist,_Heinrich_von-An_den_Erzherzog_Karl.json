{"textgrid.poem.31353": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Erzherzog Karl", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schauerlich ins Rad des Weltgeschickes", "tokens": ["Schau\u00b7er\u00b7lich", "ins", "Rad", "des", "Welt\u00b7ge\u00b7schi\u00b7ckes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Greifst du am Entscheidungstage ein,", "tokens": ["Greifst", "du", "am", "Ent\u00b7schei\u00b7dungs\u00b7ta\u00b7ge", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und dein Volk lauscht, angsterf\u00fcllten Blickes,", "tokens": ["Und", "dein", "Volk", "lauscht", ",", "angs\u00b7ter\u00b7f\u00fcll\u00b7ten", "Bli\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Welch ein Los ihm wird gefallen sein.", "tokens": ["Welch", "ein", "Los", "ihm", "wird", "ge\u00b7fal\u00b7len", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "VAFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aber leicht, o Herr, gleich deinem Leben", "tokens": ["A\u00b7ber", "leicht", ",", "o", "Herr", ",", "gleich", "dei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "FM", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wage du das heilge Vaterland!", "tokens": ["Wa\u00b7ge", "du", "das", "heil\u00b7ge", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sein Panier wirf, wenn die Scharen beben,", "tokens": ["Sein", "Pa\u00b7nier", "wirf", ",", "wenn", "die", "Scha\u00b7ren", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In der Feinde dichtsten Lanzenstand.", "tokens": ["In", "der", "Fein\u00b7de", "dichts\u00b7ten", "Lan\u00b7zen\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht der Sieg ists, den der Deutsche fodert,", "tokens": ["Nicht", "der", "Sieg", "ists", ",", "den", "der", "Deut\u00b7sche", "fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00fclflos, wie er schon am Abgrund steht;", "tokens": ["H\u00fcl\u00b7flos", ",", "wie", "er", "schon", "am", "Ab\u00b7grund", "steht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn der Kampf nur, fackelgleich, entlodert,", "tokens": ["Wenn", "der", "Kampf", "nur", ",", "fa\u00b7ckel\u00b7gleich", ",", "ent\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wert der Leiche, die zu Grabe geht.", "tokens": ["Wert", "der", "Lei\u00b7che", ",", "die", "zu", "Gra\u00b7be", "geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Mag er dann in finstre Nacht auch sinken,", "tokens": ["Mag", "er", "dann", "in", "finst\u00b7re", "Nacht", "auch", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von dem Gipfel, halb bereits erklimmt;", "tokens": ["Von", "dem", "Gip\u00b7fel", ",", "halb", "be\u00b7reits", "er\u00b7klimmt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Herr! Die Tr\u00e4ne wird noch Dank dir blinken,", "tokens": ["Herr", "!", "Die", "Tr\u00e4\u00b7ne", "wird", "noch", "Dank", "dir", "blin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn dein Schwert daf\u00fcr nur Rache nimmt.", "tokens": ["Wenn", "dein", "Schwert", "da\u00b7f\u00fcr", "nur", "Ra\u00b7che", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PAV", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}