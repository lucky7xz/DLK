{"textgrid.poem.43712": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaukle auf und schaukle nieder!", "tokens": ["Schauk\u00b7le", "auf", "und", "schauk\u00b7le", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dem B\u00f6sen flieh zur\u00fcck,", "tokens": ["Vor", "dem", "B\u00f6\u00b7sen", "flieh", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem Guten kehre wieder,", "tokens": ["Zu", "dem", "Gu\u00b7ten", "keh\u00b7re", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn das Gute sei dein Gl\u00fcck.", "tokens": ["Denn", "das", "Gu\u00b7te", "sei", "dein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Findet sich auch Leid mitunter \u2013", "tokens": ["Fin\u00b7det", "sich", "auch", "Leid", "mi\u00b7tun\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "NN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frisch! Bewegung giebt dir Kraft;", "tokens": ["Frisch", "!", "Be\u00b7we\u00b7gung", "giebt", "dir", "Kraft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaukle fr\u00f6hlich, schaukle munter,", "tokens": ["Schauk\u00b7le", "fr\u00f6h\u00b7lich", ",", "schauk\u00b7le", "mun\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werde stark und ritterhaft!", "tokens": ["Wer\u00b7de", "stark", "und", "rit\u00b7ter\u00b7haft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht am Staube sollst du wallen", "tokens": ["Nicht", "am", "Stau\u00b7be", "sollst", "du", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein unst\u00e4t schwankend Rohr!", "tokens": ["Wie", "ein", "un\u00b7st\u00e4t", "schwan\u00b7kend", "Rohr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Himmels blaue Hallen", "tokens": ["In", "des", "Him\u00b7mels", "blau\u00b7e", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaukle gern dich stets empor!", "tokens": ["Schauk\u00b7le", "gern", "dich", "stets", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}