{"textgrid.poem.50915": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie nun alles stirbt und endet", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie nun alles stirbt und endet", "tokens": ["Wie", "nun", "al\u00b7les", "stirbt", "und", "en\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das letzte Rosenblatt", "tokens": ["Und", "das", "letz\u00b7te", "Ro\u00b7sen\u00b7blatt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcd sich an die Erde wendet,", "tokens": ["M\u00fcd", "sich", "an", "die", "Er\u00b7de", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die warme Ruhestatt:", "tokens": ["In", "die", "war\u00b7me", "Ru\u00b7he\u00b7statt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So auch unser Tun und Lassen,", "tokens": ["So", "auch", "un\u00b7ser", "Tun", "und", "Las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was uns hei\u00df und wild erregt,", "tokens": ["Was", "uns", "hei\u00df", "und", "wild", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unser Lieben, unser Hassen", "tokens": ["Un\u00b7ser", "Lie\u00b7ben", ",", "un\u00b7ser", "Has\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei ins welke Laub gelegt!", "tokens": ["Sei", "ins", "wel\u00b7ke", "Laub", "ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Reiner, wei\u00dfer Schnee, o schneie,", "tokens": ["Rei\u00b7ner", ",", "wei\u00b7\u00dfer", "Schnee", ",", "o", "schnei\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "FM", "FM", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schneie beide Gr\u00e4ber zu,", "tokens": ["Schnei\u00b7e", "bei\u00b7de", "Gr\u00e4\u00b7ber", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Seele uns gedeihe", "tokens": ["Da\u00df", "die", "See\u00b7le", "uns", "ge\u00b7dei\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still und k\u00fchl in Winterruh!", "tokens": ["Still", "und", "k\u00fchl", "in", "Win\u00b7ter\u00b7ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bald kommt jene Fr\u00fchlingswende,", "tokens": ["Bald", "kommt", "je\u00b7ne", "Fr\u00fch\u00b7lings\u00b7wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Die allein die Liebe weckt,", "tokens": ["Die", "al\u00b7lein", "die", "Lie\u00b7be", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wo der Ha\u00df umsonst die H\u00e4nde", "tokens": ["Wo", "der", "Ha\u00df", "um\u00b7sonst", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tr\u00e4umend aus dem Grabe streckt!", "tokens": ["Tr\u00e4u\u00b7mend", "aus", "dem", "Gra\u00b7be", "streckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}