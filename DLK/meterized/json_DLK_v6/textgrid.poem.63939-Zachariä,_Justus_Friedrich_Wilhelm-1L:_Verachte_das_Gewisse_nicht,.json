{"textgrid.poem.63939": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verachte das Gewisse nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verachte das Gewisse nicht,", "tokens": ["Ver\u00b7ach\u00b7te", "das", "Ge\u00b7wis\u00b7se", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So viel auch Hoffnung mehr verspricht;", "tokens": ["So", "viel", "auch", "Hoff\u00b7nung", "mehr", "ver\u00b7spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie t\u00e4uscht mit jedem Augenblick;", "tokens": ["Sie", "t\u00e4uscht", "mit", "je\u00b7dem", "Au\u00b7gen\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was du verschm\u00e4hst, ist oft dein Gl\u00fcck.", "tokens": ["Was", "du", "ver\u00b7schm\u00e4hst", ",", "ist", "oft", "dein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein reicher Schulthei\u00df ging von Haus", "tokens": ["Ein", "rei\u00b7cher", "Schult\u00b7hei\u00df", "ging", "von", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar fr\u00fch zu einem Kirmesschmaus.", "tokens": ["Gar", "fr\u00fch", "zu", "ei\u00b7nem", "Kir\u00b7messc\u00b7hmaus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Dorf lag weit von seinem Ort,", "tokens": ["Das", "Dorf", "lag", "weit", "von", "sei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Inde\u00df lief er doch n\u00fcchtern fort;", "tokens": ["In\u00b7de\u00df", "lief", "er", "doch", "n\u00fcch\u00b7tern", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Denn schon sa\u00df er im Geist am Tisch,", "tokens": ["Denn", "schon", "sa\u00df", "er", "im", "Geist", "am", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Bedeckt mit Braten, Fleisch und Fisch.", "tokens": ["Be\u00b7deckt", "mit", "Bra\u00b7ten", ",", "Fleisch", "und", "Fisch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da (dacht' er) sollst du sanft dich ruhn,", "tokens": ["Da", "(", "dacht'", "er", ")", "sollst", "du", "sanft", "dich", "ruhn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "VVFIN", "PPER", "$(", "VMFIN", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dir was Recht's zu Gute thun,", "tokens": ["Und", "dir", "was", "Recht's", "zu", "Gu\u00b7te", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PWS", "NE", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer wollte nicht mit leerem Magen", "tokens": ["Wer", "woll\u00b7te", "nicht", "mit", "lee\u00b7rem", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auf einen Schmaus zu hungern wagen?", "tokens": ["Auf", "ei\u00b7nen", "Schmaus", "zu", "hun\u00b7gern", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So strich er mit vergn\u00fcgtem Sinn,", "tokens": ["So", "strich", "er", "mit", "ver\u00b7gn\u00fcg\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Durch Haide, Wald und Fluren hin.", "tokens": ["Durch", "Hai\u00b7de", ",", "Wald", "und", "Flu\u00b7ren", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der Mittag nahte sich nunmehr,", "tokens": ["Der", "Mit\u00b7tag", "nah\u00b7te", "sich", "nun\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und sieh'! ihm f\u00e4llt von ohngef\u00e4hr,", "tokens": ["Und", "sieh'", "!", "ihm", "f\u00e4llt", "von", "ohn\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da schon die Sonne brennend sticht,", "tokens": ["Da", "schon", "die", "Son\u00b7ne", "bren\u00b7nend", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Am Weg ein Birnbaum in's Gesicht,", "tokens": ["Am", "Weg", "ein", "Birn\u00b7baum", "in's", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Den k\u00fcrzlich brav der Wind durchr\u00fcttelt", "tokens": ["Den", "k\u00fcrz\u00b7lich", "brav", "der", "Wind", "durch\u00b7r\u00fct\u00b7telt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und manche Birn' herabgesch\u00fcttelt.", "tokens": ["Und", "man\u00b7che", "Birn'", "her\u00b7ab\u00b7ge\u00b7sch\u00fct\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Sie schienen reif und sch\u00f6n zu seyn,", "tokens": ["Sie", "schie\u00b7nen", "reif", "und", "sch\u00f6n", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und luden unsern Wand'rer ein,", "tokens": ["Und", "lu\u00b7den", "un\u00b7sern", "Wan\u00b7d'\u00b7rer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Bei leerem Bauch davon zu essen!", "tokens": ["Bei", "lee\u00b7rem", "Bauch", "da\u00b7von", "zu", "es\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Allein er stie\u00df sie ganz vermessen", "tokens": ["Al\u00b7lein", "er", "stie\u00df", "sie", "ganz", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Mit seinen F\u00fc\u00dfen fort und sprach:", "tokens": ["Mit", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "fort", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ich geh' ganz andern Essen nach!", "tokens": ["Ich", "geh'", "ganz", "an\u00b7dern", "Es\u00b7sen", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ihr seyd mir sonst ein gut Gericht,", "tokens": ["Ihr", "seyd", "mir", "sonst", "ein", "gut", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Doch meiner Treu', nur heute nicht!", "tokens": ["Doch", "mei\u00b7ner", "Treu'", ",", "nur", "heu\u00b7te", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Er eilte fort, und kam gar bald", "tokens": ["Er", "eil\u00b7te", "fort", ",", "und", "kam", "gar", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "An einen Strom, wo, durch Gewalt", "tokens": ["An", "ei\u00b7nen", "Strom", ",", "wo", ",", "durch", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Der Fluth, die Br\u00fccke weggeschwommen.", "tokens": ["Der", "Fluth", ",", "die", "Br\u00fc\u00b7cke", "weg\u00b7ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Er konnte nicht dar\u00fcber kommen,", "tokens": ["Er", "konn\u00b7te", "nicht", "da\u00b7r\u00fc\u00b7ber", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Lief lang' am Ufer auf und ab,", "tokens": ["Lief", "lang'", "am", "U\u00b7fer", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Bis er zuletzt sich d'rein ergab,", "tokens": ["Bis", "er", "zu\u00b7letzt", "sich", "d'\u00b7rein", "er\u00b7gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.33": {"text": "Ungern den Weg zur\u00fccke nahm,", "tokens": ["Un\u00b7gern", "den", "Weg", "zu\u00b7r\u00fc\u00b7cke", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Und wieder zu dem Birnbaum kam,", "tokens": ["Und", "wie\u00b7der", "zu", "dem", "Birn\u00b7baum", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Den er geschm\u00e4ht vor wenig Stunden.", "tokens": ["Den", "er", "ge\u00b7schm\u00e4ht", "vor", "we\u00b7nig", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "H\u00e4tt' er da nicht die Birn' gefunden,", "tokens": ["H\u00e4tt'", "er", "da", "nicht", "die", "Birn'", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Die er getreten erst mit F\u00fc\u00dfen,", "tokens": ["Die", "er", "ge\u00b7tre\u00b7ten", "erst", "mit", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "So h\u00e4tt' er halb verhungern m\u00fcssen.", "tokens": ["So", "h\u00e4tt'", "er", "halb", "ver\u00b7hun\u00b7gern", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}