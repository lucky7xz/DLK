{"dta.poem.6458": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In der Sabeer Feld, auf dem mit reichen Sch\u00e4tzen", "tokens": ["In", "der", "Sa\u00b7beer", "Feld", ",", "auf", "dem", "mit", "rei\u00b7chen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Die Pflantzen unsern Sinn bezaubern und ergetzen", "tokens": ["Die", "Pflant\u00b7zen", "un\u00b7sern", "Sinn", "be\u00b7zau\u00b7bern", "und", "er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch lieblichen Geruch, wo gleichsam allemahl", "tokens": ["Durch", "lieb\u00b7li\u00b7chen", "Ge\u00b7ruch", ",", "wo", "gleich\u00b7sam", "al\u00b7le\u00b7mahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Weyrauch-B\u00e4ume sich bem\u00fchn,", "tokens": ["Die", "Wey\u00b7rauch\u00b7B\u00e4u\u00b7me", "sich", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Himmels Gunst auf uns herab zu ziehn,", "tokens": ["Des", "Him\u00b7mels", "Gunst", "auf", "uns", "her\u00b7ab", "zu", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was thut daselbst der hellen Sonnen Strahl?", "tokens": ["Was", "thut", "da\u00b7selbst", "der", "hel\u00b7len", "Son\u00b7nen", "Strahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PAV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er hemmet da, und h\u00e4lt den Lauff", "tokens": ["Er", "hem\u00b7met", "da", ",", "und", "h\u00e4lt", "den", "Lauff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf den geliebten B\u00e4umen auf.", "tokens": ["Auf", "den", "ge\u00b7lieb\u00b7ten", "B\u00e4u\u00b7men", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Noch sch\u00f6ner gl\u00e4ntzt daselbst sein wundersch\u00f6ner Schein,", "tokens": ["Noch", "sch\u00f6\u00b7ner", "gl\u00e4ntzt", "da\u00b7selbst", "sein", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er \u00e4ndert, bessert, nimmt, verd\u00fcnnet, machet rein,", "tokens": ["Er", "\u00e4n\u00b7dert", ",", "bes\u00b7sert", ",", "nimmt", ",", "ver\u00b7d\u00fcn\u00b7net", ",", "ma\u00b7chet", "rein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was an demselbigen zu grob und feucht.", "tokens": ["Was", "an", "dem\u00b7sel\u00b7bi\u00b7gen", "zu", "grob", "und", "feucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PDS", "PTKA", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Man erndtet dann von ihnen ein,", "tokens": ["Man", "ernd\u00b7tet", "dann", "von", "ih\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die so gesch\u00e4tzte Frucht, die sich so leicht", "tokens": ["Die", "so", "ge\u00b7sch\u00e4tz\u00b7te", "Frucht", ",", "die", "sich", "so", "leicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "In kleine Theilchen theilt. Zur heilgen Pfanne Gluht,", "tokens": ["In", "klei\u00b7ne", "Theil\u00b7chen", "theilt", ".", "Zur", "heil\u00b7gen", "Pfan\u00b7ne", "Gluht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "APPRART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Durch welche wir den Himmel ehren,", "tokens": ["Durch", "wel\u00b7che", "wir", "den", "Him\u00b7mel", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wie man in unsern Kirchen thut,", "tokens": ["Wie", "man", "in", "un\u00b7sern", "Kir\u00b7chen", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mu\u00df uns Arabien sein s\u00fc\u00df Geschenck gew\u00e4hren.", "tokens": ["Mu\u00df", "uns", "A\u00b7ra\u00b7bi\u00b7en", "sein", "s\u00fc\u00df", "Ge\u00b7schenck", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}}}}