{"textgrid.poem.37078": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Minnelied", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfer klingt der Vogelsang,", "tokens": ["S\u00fc\u00b7\u00dfer", "klingt", "der", "Vo\u00b7gel\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann die gute, reine,", "tokens": ["Wann", "die", "gu\u00b7te", ",", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die mein J\u00fcnglingsherz bezwang,", "tokens": ["Die", "mein", "J\u00fcng\u00b7lings\u00b7herz", "be\u00b7zwang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt durch die Haine.", "tokens": ["Wan\u00b7delt", "durch", "die", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "R\u00f6ther bl\u00fchet Thal und Au,", "tokens": ["R\u00f6\u00b7ther", "bl\u00fc\u00b7het", "Thal", "und", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcner wird der Wasen,", "tokens": ["Gr\u00fc\u00b7ner", "wird", "der", "Wa\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo die Finger meiner Frau", "tokens": ["Wo", "die", "Fin\u00b7ger", "mei\u00b7ner", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mayenblumen lasen.", "tokens": ["Ma\u00b7yen\u00b7blu\u00b7men", "la\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Freude flie\u00dft aus ihrem Blick", "tokens": ["Freu\u00b7de", "flie\u00dft", "aus", "ih\u00b7rem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die bunte Weide,", "tokens": ["Auf", "die", "bun\u00b7te", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber fliehet sie zur\u00fcck,", "tokens": ["A\u00b7ber", "flie\u00b7het", "sie", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, so flieht die Freude.", "tokens": ["Ach", ",", "so", "flieht", "die", "Freu\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Alles ist dann f\u00fcr mich todt,", "tokens": ["Al\u00b7les", "ist", "dann", "f\u00fcr", "mich", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "APPR", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welk sind alle Kr\u00e4uter,", "tokens": ["Welk", "sind", "al\u00b7le", "Kr\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und kein Sommerabendroth", "tokens": ["Und", "kein", "Som\u00b7mer\u00b7a\u00b7ben\u00b7droth"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcnkt mir sch\u00f6n und heiter.", "tokens": ["D\u00fcnkt", "mir", "sch\u00f6n", "und", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Liebe, minnigliche Frau,", "tokens": ["Lie\u00b7be", ",", "min\u00b7nig\u00b7li\u00b7che", "Frau", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wollest nimmer fliehen,", "tokens": ["Wol\u00b7lest", "nim\u00b7mer", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df mein Herz, gleich dieser Au,", "tokens": ["Da\u00df", "mein", "Herz", ",", "gleich", "die\u00b7ser", "Au", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer m\u00f6ge bl\u00fchen.", "tokens": ["Im\u00b7mer", "m\u00f6\u00b7ge", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}