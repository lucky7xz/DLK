{"textgrid.poem.56739": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stille, herrliche Sommernacht!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stille, herrliche Sommernacht!", "tokens": ["Stil\u00b7le", ",", "herr\u00b7li\u00b7che", "Som\u00b7mer\u00b7nacht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Silberfischlein springen lustig", "tokens": ["Sil\u00b7ber\u00b7fisc\u00b7hlein", "sprin\u00b7gen", "lus\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "in dem himmlischen Meer.", "tokens": ["in", "dem", "himm\u00b7li\u00b7schen", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hochauf schnellen", "tokens": ["Hoch\u00b7auf", "schnel\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "die zierlichen Leibchen sich,", "tokens": ["die", "zier\u00b7li\u00b7chen", "Leib\u00b7chen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "blitzschnell", "tokens": ["blitz\u00b7schnell"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "wieder verschwindend.", "tokens": ["wie\u00b7der", "ver\u00b7schwin\u00b7dend", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Hinter grauen Wolkenklippen", "tokens": ["Hin\u00b7ter", "grau\u00b7en", "Wol\u00b7ken\u00b7klip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "glei\u00dft es verd\u00e4chtig.", "tokens": ["glei\u00dft", "es", "ver\u00b7d\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Da kauert arglistig", "tokens": ["Da", "kau\u00b7ert", "arg\u00b7lis\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "der Mann im Mond \u2013", "tokens": ["der", "Mann", "im", "Mond", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "und fischt.", "tokens": ["und", "fischt", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Verstohlene, seidene", "tokens": ["Ver\u00b7stoh\u00b7le\u00b7ne", ",", "sei\u00b7de\u00b7ne"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.14": {"text": "Angelschn\u00fcre", "tokens": ["An\u00b7gel\u00b7schn\u00fc\u00b7re"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "wirft er hinab", "tokens": ["wirft", "er", "hin\u00b7ab"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "in die arglose Flut.", "tokens": ["in", "die", "arg\u00b7lo\u00b7se", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Ach! und nun", "tokens": ["Ach", "!", "und", "nun"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "KON", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "zappelt auch schon", "tokens": ["zap\u00b7pelt", "auch", "schon"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "ein armer Wei\u00dfling", "tokens": ["ein", "ar\u00b7mer", "Wei\u00df\u00b7ling"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "am Haken", "tokens": ["am", "Ha\u00b7ken"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "und fliegt", "tokens": ["und", "fliegt"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "in weitem Bogen", "tokens": ["in", "wei\u00b7tem", "Bo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "hinauf zu den grauen,", "tokens": ["hin\u00b7auf", "zu", "den", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "h\u00e4\u00dflichen Klippen ...", "tokens": ["h\u00e4\u00df\u00b7li\u00b7chen", "Klip\u00b7pen", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "mir ist,", "tokens": ["mir", "ist", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "ich h\u00f6re ein leises,", "tokens": ["ich", "h\u00f6\u00b7re", "ein", "lei\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "beh\u00e4biges Lachen.", "tokens": ["be\u00b7h\u00e4\u00b7bi\u00b7ges", "La\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}