{"textgrid.poem.56211": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "11", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Rosenhag im Tal, am Quell der Linden,", "tokens": ["Am", "Ro\u00b7sen\u00b7hag", "im", "Tal", ",", "am", "Quell", "der", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da haben meine Lieder oft gerauscht;", "tokens": ["Da", "ha\u00b7ben", "mei\u00b7ne", "Lie\u00b7der", "oft", "ge\u00b7rauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie hofften gl\u00e4ubig Widerhall zu finden;", "tokens": ["Sie", "hoff\u00b7ten", "gl\u00e4u\u00b7big", "Wi\u00b7der\u00b7hall", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hast, Widerhall, den Liedern du gelauscht,", "tokens": ["Hast", ",", "Wi\u00b7der\u00b7hall", ",", "den", "Lie\u00b7dern", "du", "ge\u00b7lauscht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ahndungsvoll gebebt bei ihrem Klange? \u2013", "tokens": ["Und", "ahn\u00b7dungs\u00b7voll", "ge\u00b7bebt", "bei", "ih\u00b7rem", "Klan\u00b7ge", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Lange!", "tokens": ["Lan\u00b7ge", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Geahndet h\u00e4ttest du, da\u00df ich dich meinte,", "tokens": ["Ge\u00b7ahn\u00b7det", "h\u00e4t\u00b7test", "du", ",", "da\u00df", "ich", "dich", "mein\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dich in Schmerz und Lust mit mir vereint?", "tokens": ["Und", "dich", "in", "Schmerz", "und", "Lust", "mit", "mir", "ver\u00b7eint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00e4ttest bald, wann ich verzagend weinte,", "tokens": ["Und", "h\u00e4t\u00b7test", "bald", ",", "wann", "ich", "ver\u00b7za\u00b7gend", "wein\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Betr\u00fcbet und verzagend auch geweint?", "tokens": ["Be\u00b7tr\u00fc\u00b7bet", "und", "ver\u00b7za\u00b7gend", "auch", "ge\u00b7weint", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und bald gehofft, wann ich ermutigt hoffte? \u2013", "tokens": ["Und", "bald", "ge\u00b7hofft", ",", "wann", "ich", "er\u00b7mu\u00b7tigt", "hoff\u00b7te", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "PWAV", "PPER", "VVFIN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ofte!", "tokens": ["Of\u00b7te", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Du kennst das unbegriffne bange Sehnen,", "tokens": ["Du", "kennst", "das", "un\u00b7be\u00b7griff\u00b7ne", "ban\u00b7ge", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Widerstreit in der bewegten Brust?", "tokens": ["Den", "Wi\u00b7der\u00b7streit", "in", "der", "be\u00b7weg\u00b7ten", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Hochgesang der Freuden und die Tr\u00e4nen,", "tokens": ["Den", "Hoch\u00b7ge\u00b7sang", "der", "Freu\u00b7den", "und", "die", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den liebgehegten Schmerz, die herbe Lust?", "tokens": ["Den", "lieb\u00b7ge\u00b7heg\u00b7ten", "Schmerz", ",", "die", "her\u00b7be", "Lust", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Hoffnung Honigseim, des Zweifels Galle? \u2013", "tokens": ["Der", "Hoff\u00b7nung", "Ho\u00b7ni\u00b7gseim", ",", "des", "Zwei\u00b7fels", "Gal\u00b7le", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Alle!", "tokens": ["Al\u00b7le", "!"], "token_info": ["word", "punct"], "pos": ["PIAT", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Wohlan! Ich werde gehn, mein Haus zu bauen;", "tokens": ["Wo\u00b7hlan", "!", "Ich", "wer\u00b7de", "gehn", ",", "mein", "Haus", "zu", "bau\u00b7en", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "VVINF", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sei fest, wie ich es bin, gedenke mein.", "tokens": ["Sei", "fest", ",", "wie", "ich", "es", "bin", ",", "ge\u00b7den\u00b7ke", "mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "PPER", "PPER", "VAFIN", "$,", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den dreien Sternen will ich fest vertrauen,", "tokens": ["Den", "drei\u00b7en", "Ster\u00b7nen", "will", "ich", "fest", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die dort der Liebe geben ihren Schein;", "tokens": ["Die", "dort", "der", "Lie\u00b7be", "ge\u00b7ben", "ih\u00b7ren", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wirst auch du vertrauen ihrem Schimmer? \u2013", "tokens": ["Und", "wirst", "auch", "du", "ver\u00b7trau\u00b7en", "ih\u00b7rem", "Schim\u00b7mer", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Immer!", "tokens": ["Im\u00b7mer", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "So lebe wohl, du Seele meiner Lieder,", "tokens": ["So", "le\u00b7be", "wohl", ",", "du", "See\u00b7le", "mei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nur auf kurze Zeit verstumme du,", "tokens": ["Und", "nur", "auf", "kur\u00b7ze", "Zeit", "ver\u00b7stum\u00b7me", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gar bald erweckt dich meine Stimme wieder,", "tokens": ["Gar", "bald", "er\u00b7weckt", "dich", "mei\u00b7ne", "Stim\u00b7me", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann rufen wir es laut einander zu,", "tokens": ["Dann", "ru\u00b7fen", "wir", "es", "laut", "ein\u00b7an\u00b7der", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was ungesagt verschwiegen nicht geblieben, \u2013", "tokens": ["Was", "un\u00b7ge\u00b7sagt", "ver\u00b7schwie\u00b7gen", "nicht", "ge\u00b7blie\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "PTKNEG", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Lieben!", "tokens": ["Lie\u00b7ben", "!"], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}