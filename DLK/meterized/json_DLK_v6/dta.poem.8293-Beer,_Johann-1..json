{"dta.poem.8293": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Heute gehn wir zu den Schmause/", "tokens": ["Heu\u00b7te", "gehn", "wir", "zu", "den", "Schmau\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da werd ich gesch\u00e4fftig seyn", "tokens": ["Da", "werd", "ich", "ge\u00b7sch\u00e4ff\u00b7tig", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und in den Falerner Wein", "tokens": ["Und", "in", "den", "Fa\u00b7ler\u00b7ner", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Trincken eine tapffre Kraufe.", "tokens": ["Trin\u00b7cken", "ei\u00b7ne", "tapf\u00b7fre", "Krau\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute gehn wir zu den Schmause.", "tokens": ["Heu\u00b7te", "gehn", "wir", "zu", "den", "Schmau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heute leer ich manches K\u00e4nnchen/", "tokens": ["Heu\u00b7te", "leer", "ich", "man\u00b7ches", "K\u00e4nn\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erfrische mein Gem\u00fcth/", "tokens": ["Und", "er\u00b7fri\u00b7sche", "mein", "Ge\u00b7m\u00fcth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bi\u00df der Kamm fein roth aussieht/", "tokens": ["Bi\u00df", "der", "Kamm", "fein", "roth", "aus\u00b7sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn mirs steigt ins Ober-M\u00e4nnchen.", "tokens": ["Wenn", "mirs", "steigt", "ins", "O\u00b7ber\u00b7M\u00e4nn\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute leer ich manches K\u00e4nnchen.", "tokens": ["Heu\u00b7te", "leer", "ich", "man\u00b7ches", "K\u00e4nn\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}