{"textgrid.poem.56746": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gro\u00df \u00fcber schweigenden", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df \u00fcber schweigenden", "tokens": ["Gro\u00df", "\u00fc\u00b7ber", "schwei\u00b7gen\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "W\u00e4ldern und Wassern", "tokens": ["W\u00e4l\u00b7dern", "und", "Was\u00b7sern"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "lastet der Vollmond,", "tokens": ["las\u00b7tet", "der", "Voll\u00b7mond", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "eine \u00c4gis,", "tokens": ["ei\u00b7ne", "\u00c4\u00b7gis", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "mit d\u00fcsterem Goldschein", "tokens": ["mit", "d\u00fcs\u00b7te\u00b7rem", "Gold\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "alles in reglosen Bann", "tokens": ["al\u00b7les", "in", "reg\u00b7lo\u00b7sen", "Bann"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "APPR", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "verstrickend.", "tokens": ["ver\u00b7stri\u00b7ckend", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Die Winde", "tokens": ["Die", "Win\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "halten den Atem.", "tokens": ["hal\u00b7ten", "den", "A\u00b7tem", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Die W\u00e4lder ducken sich", "tokens": ["Die", "W\u00e4l\u00b7der", "du\u00b7cken", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "scheu in sich selbst hinein.", "tokens": ["scheu", "in", "sich", "selbst", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "ADV", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Das Auge des Sees", "tokens": ["Das", "Au\u00b7ge", "des", "Sees"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "wird stier und glasig \u2013:", "tokens": ["wird", "stier", "und", "gla\u00b7sig", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$(", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "als ob eine Ahnung", "tokens": ["als", "ob", "ei\u00b7ne", "Ah\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "die Erde durchfr\u00f6re,", "tokens": ["die", "Er\u00b7de", "durch\u00b7fr\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "da\u00df dieser Gorgoschild", "tokens": ["da\u00df", "die\u00b7ser", "Gor\u00b7go\u00b7schild"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "einst ihren Leib", "tokens": ["einst", "ih\u00b7ren", "Leib"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "zertr\u00fcmmern werde ...", "tokens": ["zer\u00b7tr\u00fcm\u00b7mern", "wer\u00b7de", "..."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VAFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Als ob eines Schreies", "tokens": ["Als", "ob", "ei\u00b7nes", "Schrei\u00b7es"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "sie schwanger l\u00e4ge,", "tokens": ["sie", "schwan\u00b7ger", "l\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "eines Schreies voll Grausen,", "tokens": ["ei\u00b7nes", "Schrei\u00b7es", "voll", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.22": {"text": "Voll Todesentsetzen ...", "tokens": ["Voll", "To\u00b7de\u00b7sent\u00b7set\u00b7zen", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "\u1f1c\u03c3\u03c3\u03b5\u03c4\u03b1\u03b9 \u1f26\u03bc\u03b1\u03c1!", "tokens": ["\u1f1c\u03c3\u03c3\u03b5\u03c4\u03b1\u03b9", "\u1f26\u03bc\u03b1\u03c1", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}