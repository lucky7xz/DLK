{"textgrid.poem.66209": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Herzberg", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer verdient so sehr", "tokens": ["Wer", "ver\u00b7dient", "so", "sehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Den Hymnuspreis, wie Er?", "tokens": ["Den", "Hym\u00b7nus\u00b7preis", ",", "wie", "Er", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat Er nicht, von Gottes Gluth durchdrungen,", "tokens": ["Hat", "Er", "nicht", ",", "von", "Got\u00b7tes", "Gluth", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ueber eine bange Welt", "tokens": ["Ue\u00b7ber", "ei\u00b7ne", "ban\u00b7ge", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Den Oelzweig des Friedens geschwungen?", "tokens": ["Den", "O\u00b7el\u00b7zweig", "des", "Frie\u00b7dens", "ge\u00b7schwun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mehr als der blutige Held,", "tokens": ["Mehr", "als", "der", "blu\u00b7ti\u00b7ge", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Mehr als der Eroberer, mehr", "tokens": ["Mehr", "als", "der", "Er\u00b7o\u00b7be\u00b7rer", ",", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "KOKOM", "ART", "NN", "$,", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Ist Herzberg der friedliche; Er", "tokens": ["Ist", "Herz\u00b7berg", "der", "fried\u00b7li\u00b7che", ";", "Er"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "NN", "ART", "ADJA", "$.", "PPER"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Sprach mit gebietender Rede", "tokens": ["Sprach", "mit", "ge\u00b7bie\u00b7ten\u00b7der", "Re\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Zur m\u00e4chtig sich r\u00fcstenden Fehde:", "tokens": ["Zur", "m\u00e4ch\u00b7tig", "sich", "r\u00fcs\u00b7ten\u00b7den", "Feh\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "PRF", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Verstumme, Tochter des Orkus!", "tokens": ["Ver\u00b7stum\u00b7me", ",", "Toch\u00b7ter", "des", "Or\u00b7kus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Schweige, Krieg!", "tokens": ["Schwei\u00b7ge", ",", "Krieg", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Er schwieg.", "tokens": ["Er", "schwieg", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}