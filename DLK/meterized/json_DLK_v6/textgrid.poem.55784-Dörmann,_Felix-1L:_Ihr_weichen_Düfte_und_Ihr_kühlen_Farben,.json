{"textgrid.poem.55784": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr weichen D\u00fcfte und Ihr k\u00fchlen Farben,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr weichen D\u00fcfte und Ihr k\u00fchlen Farben,", "tokens": ["Ihr", "wei\u00b7chen", "D\u00fcf\u00b7te", "und", "Ihr", "k\u00fch\u00b7len", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch dank' ich meine letzte Seligkeit,", "tokens": ["Euch", "dank'", "ich", "mei\u00b7ne", "letz\u00b7te", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Eurem wonneschw\u00fclen Bannkreis starben", "tokens": ["In", "Eu\u00b7rem", "won\u00b7ne\u00b7schw\u00fc\u00b7len", "Bann\u00b7kreis", "star\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die rothen Qualen, die mich wild umwarben,", "tokens": ["Die", "ro\u00b7then", "Qua\u00b7len", ",", "die", "mich", "wild", "um\u00b7war\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der graue Jammer und das bleiche Leid.", "tokens": ["Der", "grau\u00b7e", "Jam\u00b7mer", "und", "das", "blei\u00b7che", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Bettler war ich \u2013 martervolles Darben", "tokens": ["Ein", "Bett\u00b7ler", "war", "ich", "\u2013", "mar\u00b7ter\u00b7vol\u00b7les", "Dar\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "$(", "PIS", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War meines Lebens traurig-\u00f6des Loos.", "tokens": ["War", "mei\u00b7nes", "Le\u00b7bens", "trau\u00b7rig\u00b7\u00f6\u00b7des", "Loos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun spr\u00fcht mit eins in \u00fcppig-reichen Garben,", "tokens": ["Nun", "spr\u00fcht", "mit", "eins", "in", "\u00fcp\u00b7pig\u00b7rei\u00b7chen", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In weichen D\u00fcften und in k\u00fchlen Farben", "tokens": ["In", "wei\u00b7chen", "D\u00fcf\u00b7ten", "und", "in", "k\u00fch\u00b7len", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das gro\u00dfe Gl\u00fcck in meiner Seele Scho\u00df.", "tokens": ["Das", "gro\u00b7\u00dfe", "Gl\u00fcck", "in", "mei\u00b7ner", "See\u00b7le", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Tuberosen, s\u00fc\u00dfe, w\u00e4chsernbleiche,", "tokens": ["O", "Tu\u00b7be\u00b7ro\u00b7sen", ",", "s\u00fc\u00b7\u00dfe", ",", "w\u00e4ch\u00b7sern\u00b7blei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O hei\u00dfgeliebte, regungslose Schar!", "tokens": ["O", "hei\u00df\u00b7ge\u00b7lieb\u00b7te", ",", "re\u00b7gungs\u00b7lo\u00b7se", "Schar", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dass Euer Anblick nimmer mir entweiche!", "tokens": ["Dass", "Eu\u00b7er", "An\u00b7blick", "nim\u00b7mer", "mir", "ent\u00b7wei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Euer Hauch, der feuchte, z\u00e4rtlich-reiche,", "tokens": ["Und", "Eu\u00b7er", "Hauch", ",", "der", "feuch\u00b7te", ",", "z\u00e4rt\u00b7lich\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "S\u00fc\u00df-duftig wie die Haarflut einer Leiche,", "tokens": ["S\u00fc\u00df\u00b7duf\u00b7tig", "wie", "die", "Haar\u00b7flut", "ei\u00b7ner", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er m\u00f6ge mich umzittern immerdar.", "tokens": ["Er", "m\u00f6\u00b7ge", "mich", "um\u00b7zit\u00b7tern", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Tuberosen, s\u00fc\u00dfe, w\u00e4chsernbleiche,", "tokens": ["O", "Tu\u00b7be\u00b7ro\u00b7sen", ",", "s\u00fc\u00b7\u00dfe", ",", "w\u00e4ch\u00b7sern\u00b7blei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O hei\u00dfgeliebte, regungslose Schar!", "tokens": ["O", "hei\u00df\u00b7ge\u00b7lieb\u00b7te", ",", "re\u00b7gungs\u00b7lo\u00b7se", "Schar", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In grauer Flut ist mir die Welt versunken,", "tokens": ["In", "grau\u00b7er", "Flut", "ist", "mir", "die", "Welt", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein nebeltr\u00fcbes, \u00f6des Traumgebild,", "tokens": ["Ein", "ne\u00b7belt\u00b7r\u00fc\u00b7bes", ",", "\u00f6\u00b7des", "Traum\u00b7ge\u00b7bild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und farbenjauchzend, schwerer D\u00fcfte trunken", "tokens": ["Und", "far\u00b7ben\u00b7jauch\u00b7zend", ",", "schwe\u00b7rer", "D\u00fcf\u00b7te", "trun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die neue Welt aus meiner Seele quillt. \u2013", "tokens": ["Die", "neu\u00b7e", "Welt", "aus", "mei\u00b7ner", "See\u00b7le", "quillt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O Silberlila, Deine weichen Wellen,", "tokens": ["O", "Sil\u00b7ber\u00b7li\u00b7la", ",", "Dei\u00b7ne", "wei\u00b7chen", "Wel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Kinderseelen lilienkeusch und klar,", "tokens": ["Wie", "Kin\u00b7der\u00b7see\u00b7len", "li\u00b7li\u00b7en\u00b7keusch", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In meine flammenm\u00fcde Seele quellen,", "tokens": ["In", "mei\u00b7ne", "flam\u00b7men\u00b7m\u00fc\u00b7de", "See\u00b7le", "quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Seele wird zum Hochaltar,", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "wird", "zum", "Hoc\u00b7hal\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wo Jubelhymnen Deiner S\u00fc\u00dfe schwellen.", "tokens": ["Wo", "Ju\u00b7bel\u00b7hym\u00b7nen", "Dei\u00b7ner", "S\u00fc\u00b7\u00dfe", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "O lasst mich, lasst mich ruh'n auf gr\u00fcnem Rasen,", "tokens": ["O", "lasst", "mich", ",", "lasst", "mich", "ruh'n", "auf", "gr\u00fc\u00b7nem", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In seinen Farbenzauber mich versenken,", "tokens": ["In", "sei\u00b7nen", "Far\u00b7ben\u00b7zau\u00b7ber", "mich", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entfliehen allem qualvoll-hei\u00dfen Denken", "tokens": ["Ent\u00b7flie\u00b7hen", "al\u00b7lem", "qua\u00b7lvoll\u00b7hei\u00b7\u00dfen", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu meiner Seele schweigenden Extasen.", "tokens": ["Zu", "mei\u00b7ner", "See\u00b7le", "schwei\u00b7gen\u00b7den", "Ex\u00b7ta\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O lichtes Gr\u00fcn, wie Du die Seele weitest,", "tokens": ["O", "lich\u00b7tes", "Gr\u00fcn", ",", "wie", "Du", "die", "See\u00b7le", "wei\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um jede Nervenfaser z\u00e4rtlich kost,", "tokens": ["Um", "je\u00b7de", "Ner\u00b7ven\u00b7fa\u00b7ser", "z\u00e4rt\u00b7lich", "kost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In's Unermess'ne das Gef\u00fchl verbreitest,", "tokens": ["In's", "Un\u00b7er\u00b7mess'\u00b7ne", "das", "Ge\u00b7f\u00fchl", "ver\u00b7brei\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O lichtes Wiesengr\u00fcn \u2013 mein treuer Trost.", "tokens": ["O", "lich\u00b7tes", "Wie\u00b7sen\u00b7gr\u00fcn", "\u2013", "mein", "treu\u00b7er", "Trost", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wenn meine Seele sich vor Grausen str\u00e4ubet,", "tokens": ["Wenn", "mei\u00b7ne", "See\u00b7le", "sich", "vor", "Grau\u00b7sen", "str\u00e4u\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn alles \u00f6d und ekel ist geworden,", "tokens": ["Wenn", "al\u00b7les", "\u00f6d", "und", "e\u00b7kel", "ist", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "KON", "ADJD", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Qual und Sehnsucht jedes Gl\u00fcck ermorden,", "tokens": ["Wenn", "Qual", "und", "Sehn\u00b7sucht", "je\u00b7des", "Gl\u00fcck", "er\u00b7mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein sanfter Schleier einzig sie bet\u00e4ubet.", "tokens": ["Dein", "sanf\u00b7ter", "Schlei\u00b7er", "ein\u00b7zig", "sie", "be\u00b7t\u00e4u\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}