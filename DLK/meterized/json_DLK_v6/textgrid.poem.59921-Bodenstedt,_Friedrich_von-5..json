{"textgrid.poem.59921": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Leben ist ein Darlehn, keine Gabe \u2013", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "Dar\u00b7lehn", ",", "kei\u00b7ne", "Ga\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wei\u00dft nicht, wieviel Schritt du gehst zum Grabe,", "tokens": ["Du", "wei\u00dft", "nicht", ",", "wie\u00b7viel", "Schritt", "du", "gehst", "zum", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum n\u00fctze klug die Zeit: auf jedem Schritt", "tokens": ["Drum", "n\u00fct\u00b7ze", "klug", "die", "Zeit", ":", "auf", "je\u00b7dem", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "ART", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nimm das Bewu\u00dftsein deiner Pflichten mit.", "tokens": ["Nimm", "das", "Be\u00b7wu\u00df\u00b7tsein", "dei\u00b7ner", "Pflich\u00b7ten", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Gew\u00f6hne dich \u2013 da stets der Tod dir dr\u00e4ut \u2013", "tokens": ["Ge\u00b7w\u00f6h\u00b7ne", "dich", "\u2013", "da", "stets", "der", "Tod", "dir", "dr\u00e4ut", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dankbar zu nehmen, was das Leben beut;", "tokens": ["Dank\u00b7bar", "zu", "neh\u00b7men", ",", "was", "das", "Le\u00b7ben", "beut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die W\u00fcnsche nicht nach \u00c4u\u00dferm zu gestalten,", "tokens": ["Die", "W\u00fcn\u00b7sche", "nicht", "nach", "\u00c4u\u00b7\u00dferm", "zu", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sondern den Kern im Innern zu entfalten;", "tokens": ["Son\u00b7dern", "den", "Kern", "im", "In\u00b7nern", "zu", "ent\u00b7fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Nicht fremder Meinung untertan zu sein,", "tokens": ["Nicht", "frem\u00b7der", "Mei\u00b7nung", "un\u00b7ter\u00b7tan", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Dinge nicht zu sch\u00e4tzen nach dem Schein;", "tokens": ["Die", "Din\u00b7ge", "nicht", "zu", "sch\u00e4t\u00b7zen", "nach", "dem", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nicht zu verlangen, da\u00df sie sollen gehn,", "tokens": ["Nicht", "zu", "ver\u00b7lan\u00b7gen", ",", "da\u00df", "sie", "sol\u00b7len", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wie wir es w\u00fcnschen \u2013 sondern sie verstehn,", "tokens": ["Wie", "wir", "es", "w\u00fcn\u00b7schen", "\u2013", "son\u00b7dern", "sie", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "$(", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df wir uns bei Erf\u00fcllung unsrer Pflichten", "tokens": ["Da\u00df", "wir", "uns", "bei", "Er\u00b7f\u00fcl\u00b7lung", "uns\u00b7rer", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "(da sie's nach uns nicht tun) nach ", "tokens": ["(", "da", "sie's", "nach", "uns", "nicht", "tun", ")", "nach"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "KOUS", "PIS", "APPR", "PPER", "PTKNEG", "VVINF", "$(", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}