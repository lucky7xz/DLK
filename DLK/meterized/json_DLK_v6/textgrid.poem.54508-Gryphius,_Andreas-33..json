{"textgrid.poem.54508": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welt r\u00fchme was du wilt! ich mu\u00df die Tr\u00fcbsal preisen/", "tokens": ["Welt", "r\u00fch\u00b7me", "was", "du", "wilt", "!", "ich", "mu\u00df", "die", "Tr\u00fcb\u00b7sal", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PWS", "PPER", "VMFIN", "$.", "PPER", "VMFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Tr\u00fcbsal die vns lehrt; voll sanffter Sinnen seyn.", "tokens": ["Die", "Tr\u00fcb\u00b7sal", "die", "vns", "lehrt", ";", "voll", "sanff\u00b7ter", "Sin\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$.", "ADJD", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn aller Plitzen Macht f\u00e4llt h\u00e4uffig bey vns eyn/", "tokens": ["Wenn", "al\u00b7ler", "Plit\u00b7zen", "Macht", "f\u00e4llt", "h\u00e4uf\u00b7fig", "bey", "vns", "eyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VVFIN", "ADJD", "APPR", "PPER", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wen schier die Seele gantz wil au\u00df dem Leibe reisen/", "tokens": ["Wen", "schier", "die", "See\u00b7le", "gantz", "wil", "au\u00df", "dem", "Lei\u00b7be", "rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ART", "NN", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn vns die Feinde nichts als Flamm' vnd R\u00e4der weisen/", "tokens": ["Wenn", "vns", "die", "Fein\u00b7de", "nichts", "als", "Flamm'", "vnd", "R\u00e4\u00b7der", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIS", "KOKOM", "NE", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn schaut ein stiller Geist recht mitten in der Pein/", "tokens": ["Denn", "schaut", "ein", "stil\u00b7ler", "Geist", "recht", "mit\u00b7ten", "in", "der", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie die/ die Gott erkist nicht ewig stehn allein/", "tokens": ["Wie", "die", "/", "die", "Gott", "er\u00b7kist", "nicht", "e\u00b7wig", "stehn", "al\u00b7lein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "$(", "ART", "NN", "VVFIN", "PTKNEG", "ADJD", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd wie er sie nicht st\u00e4ts mit Tr\u00e4nen pflegt zu speisen.", "tokens": ["Vnd", "wie", "er", "sie", "nicht", "st\u00e4ts", "mit", "Tr\u00e4\u00b7nen", "pflegt", "zu", "spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "PTKNEG", "ADV", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer denn de\u00df H\u00f6chsten Treu einmal in Angst erf\u00e4hrt/", "tokens": ["Wer", "denn", "de\u00df", "H\u00f6chs\u00b7ten", "Treu", "ein\u00b7mal", "in", "Angst", "er\u00b7f\u00e4hrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hofft iede zeit auff H\u00fclff/ ob gleich der Jammer wehrt/", "tokens": ["Hofft", "ie\u00b7de", "zeit", "auff", "H\u00fclff", "/", "ob", "gleich", "der", "Jam\u00b7mer", "wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ob gleich das Schwerdt entbl\u00f6st/ ob gleich der Holtzsto\u00df brennet.", "tokens": ["Ob", "gleich", "das", "Schwerdt", "ent\u00b7bl\u00f6st", "/", "ob", "gleich", "der", "Holtz\u00b7sto\u00df", "bren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer fest in vnfall hofft/ hat/ wenn er frey/ betracht/", "tokens": ["Wer", "fest", "in", "vn\u00b7fall", "hofft", "/", "hat", "/", "wenn", "er", "frey", "/", "be\u00b7tracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "NN", "VVFIN", "$(", "VAFIN", "$(", "KOUS", "PPER", "ADJD", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das Hoffnung nimmermehr den Geist zu schanden macht/", "tokens": ["Das", "Hoff\u00b7nung", "nim\u00b7mer\u00b7mehr", "den", "Geist", "zu", "schan\u00b7den", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Geist/ den keine Furcht/ noch Lust von Gott abtrennet.", "tokens": ["Den", "Geist", "/", "den", "kei\u00b7ne", "Furcht", "/", "noch", "Lust", "von", "Gott", "ab\u00b7tren\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PIAT", "NN", "$(", "ADV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}