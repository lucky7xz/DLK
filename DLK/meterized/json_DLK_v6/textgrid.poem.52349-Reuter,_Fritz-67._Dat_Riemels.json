{"textgrid.poem.52349": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "67. Dat Riemels", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Eddelmann, so'n recht vedreihten,", "tokens": ["Ein", "Ed\u00b7del\u00b7mann", ",", "so'n", "recht", "ved\u00b7reih\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herr von Sylvester was hei heiten,", "tokens": ["Herr", "von", "Syl\u00b7ves\u00b7ter", "was", "hei", "hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "PWS", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dei hahr den Kopp so vull von Schrullen,", "tokens": ["Dei", "hahr", "den", "Kopp", "so", "vull", "von", "Schrul\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dat man sich't kuum inbillen kann,", "tokens": ["Dat", "man", "sich't", "ku\u00b7um", "in\u00b7bil\u00b7len", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Un wenn hei hahr so recht den Dullen,", "tokens": ["Un", "wenn", "hei", "hahr", "so", "recht", "den", "Dul\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ADJA", "NN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Denn f\u00fcng hei an tau riemen an. \u2013", "tokens": ["Denn", "f\u00fcng", "hei", "an", "tau", "rie\u00b7men", "an", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "FM", "FM", "APPR", "NE", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So dick un fett un ok mit so 'ne Hast", "tokens": ["So", "dick", "un", "fett", "un", "ok", "mit", "so", "'ne", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "FM", "FM", "FM", "FM", "APPR", "ADV", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Qu\u00fcll denn ut em herut sien Riemels,", "tokens": ["Qu\u00fcll", "denn", "ut", "em", "he\u00b7rut", "sien", "Rie\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "As ut 'ne brahren Gans dat Ingeth\u00fcmels,", "tokens": ["As", "ut", "'ne", "brah\u00b7ren", "Gans", "dat", "In\u00b7ge\u00b7th\u00fc\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wenn hinne ehr dei St\u00fc\u00fctz is bast. \u2013", "tokens": ["Wenn", "hin\u00b7ne", "ehr", "dei", "St\u00fc\u00fctz", "is", "bast", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "FM", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Hei riemte, wo hei g\u00fcng un st\u00fcnn,", "tokens": ["Hei", "riem\u00b7te", ",", "wo", "hei", "g\u00fcng", "un", "st\u00fcnn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "FM", "FM", "FM", "FM", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Ob hei hett f\u00fchrt, ob hei hett r\u00e4ren;", "tokens": ["Ob", "hei", "hett", "f\u00fchrt", ",", "ob", "hei", "hett", "r\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "VVFIN", "$,", "KOUS", "NE", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Un wenn hei denn man Einen f\u00fcnn,", "tokens": ["Un", "wenn", "hei", "denn", "man", "Ei\u00b7nen", "f\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "KON", "PIS", "ART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.14": {"text": "Den'n hei sien Riem v\u00f6rbehren k\u00fcnn,", "tokens": ["Den'n", "hei", "sien", "Riem", "v\u00f6r\u00b7beh\u00b7ren", "k\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Denn wier hei gl\u00fccklich un taufrehren.", "tokens": ["Denn", "wier", "hei", "gl\u00fcck\u00b7lich", "un", "tauf\u00b7reh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJD", "FM", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Eins reer uns' Eddelmann woll \u00e4we Land,", "tokens": ["Eins", "reer", "un\u00b7s'", "Ed\u00b7del\u00b7mann", "woll", "\u00e4\u00b7we", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "Sien Rietknecht folgt em gliek tau Hand,", "tokens": ["Sien", "Riet\u00b7knecht", "folgt", "em", "gliek", "tau", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJA", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Den'n hahr hei \u00fcmme bi sich, wenn hei reer,", "tokens": ["Den'n", "hahr", "hei", "\u00fcm\u00b7me", "bi", "sich", ",", "wenn", "hei", "reer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "PRF", "$,", "KOUS", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Doamit hei sien Geriem bewunnern dehr.", "tokens": ["Do\u00b7a\u00b7mit", "hei", "sien", "Ge\u00b7riem", "be\u00b7wun\u00b7nern", "dehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "As sei nu'n B\u00e4ten r\u00e4ren wieren,", "tokens": ["As", "sei", "nu'n", "B\u00e4\u00b7ten", "r\u00e4\u00b7ren", "wie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Dunn dreiht dei Herr sich \u00fcm un s\u00e4hr:", "tokens": ["Dunn", "dreiht", "dei", "Herr", "sich", "\u00fcm", "un", "s\u00e4hr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PRF", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "\u00bbjehann! Du m\u00f6st Di ok dat Riemen liehren,", "tokens": ["\u00bb", "je\u00b7hann", "!", "Du", "m\u00f6st", "Di", "ok", "dat", "Rie\u00b7men", "li\u00b7eh\u00b7ren", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "PPER", "VMFIN", "NE", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Du gl\u00f6wst nich, wat dat maakt v\u00f6r Jux,", "tokens": ["Du", "gl\u00f6wst", "nich", ",", "wat", "dat", "maakt", "v\u00f6r", "Jux", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wenn Eine riemt so recht wat Schmucks,", "tokens": ["Wenn", "Ei\u00b7ne", "riemt", "so", "recht", "wat", "Schmucks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "ADJD", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Un wenn dei Anne em dorup antwurt't", "tokens": ["Un", "wenn", "dei", "An\u00b7ne", "em", "do\u00b7rup", "ant\u00b7wurt't"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.26": {"text": "Un wenn dat \u00fcmme geiht so furt.", "tokens": ["Un", "wenn", "dat", "\u00fcm\u00b7me", "geiht", "so", "furt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "ADJA", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.27": {"text": "Ick wahr mal gliek doamit beginnen.", "tokens": ["Ick", "wahr", "mal", "gliek", "do\u00b7a\u00b7mit", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVFIN", "PAV", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Pa\u00df Du man up, dat Anne wad sich finnen;", "tokens": ["Pa\u00df", "Du", "man", "up", ",", "dat", "An\u00b7ne", "wad", "sich", "fin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "NE", "$,", "ART", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Pa\u00df Du man up, wo ick dat maak:", "tokens": ["Pa\u00df", "Du", "man", "up", ",", "wo", "ick", "dat", "maak", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "NE", "$,", "PWAV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Ich hei\u00df' Phillippus Karl Johannes von Sylvester", "tokens": ["Ich", "hei\u00df'", "Phil\u00b7lip\u00b7pus", "Karl", "Jo\u00b7han\u00b7nes", "von", "Syl\u00b7ves\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "NE", "NE", "APPR", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Un k\u00fc\u00dfte gestern Abend Deine Schwester.\u00ab \u2013", "tokens": ["Un", "k\u00fc\u00df\u00b7te", "ge\u00b7stern", "A\u00b7bend", "Dei\u00b7ne", "Schwes\u00b7ter", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "\u00bbih, gneed'ge Herr, so kann ick ok!\u00ab", "tokens": ["\u00bb", "ih", ",", "gnee\u00b7d'\u00b7ge", "Herr", ",", "so", "kann", "ick", "ok", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "VMFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "R\u00f6pt Jehann Schult un maakt en schlau Gesicht.", "tokens": ["R\u00f6pt", "Je\u00b7hann", "Schult", "un", "maakt", "en", "schlau", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "FM", "FM", "FM", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "\u00bbna, denn man 'raus damit, genir' Dich nicht,", "tokens": ["\u00bb", "na", ",", "denn", "man", "'", "raus", "da\u00b7mit", ",", "ge\u00b7nir", "'", "Dich", "nicht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "KON", "PIS", "$(", "ADV", "PAV", "$,", "NE", "$(", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Fang Du nur an und dreist d'rauf zu!\u00ab \u2013", "tokens": ["Fang", "Du", "nur", "an", "und", "dreist", "d'\u00b7rauf", "zu", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "PAV", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "\u00bbick heit Jehann un k\u00fcss' dei gneedge Fru.\u00ab \u2013", "tokens": ["\u00bb", "ick", "heit", "Je\u00b7hann", "un", "k\u00fcss'", "dei", "gneed\u00b7ge", "Fru", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$(", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "\u00bbdu dummer Kerl! Das reimt sich nicht.\u00ab \u2013", "tokens": ["\u00bb", "du", "dum\u00b7mer", "Kerl", "!", "Das", "reimt", "sich", "nicht", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$.", "PDS", "VVFIN", "PRF", "PTKNEG", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "\u00bbdat schadt em nich, as Pogge seggt\u00ab,", "tokens": ["\u00bb", "dat", "schadt", "em", "nich", ",", "as", "Pog\u00b7ge", "seggt", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Meint uns' Jehann un kratzt sich in dat Hoa,", "tokens": ["Meint", "un\u00b7s'", "Je\u00b7hann", "un", "kratzt", "sich", "in", "dat", "Hoa", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "\u00bbwenn ok mien ", "tokens": ["\u00bb", "wenn", "ok", "mien"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "KOUS", "NE", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "So is dei ", "tokens": ["So", "is", "dei"], "token_info": ["word", "word", "word"], "pos": ["ADV", "FM", "FM"], "meter": "+-+", "measure": "trochaic.di"}}}}}