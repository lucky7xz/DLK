{"textgrid.poem.46271": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von demselbigen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was doch der Witzleer", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "doch", "der", "Witz\u00b7leer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "den h\u00fcbschen m\u00e4dlin hin und her,", "tokens": ["den", "h\u00fcb\u00b7schen", "m\u00e4d\u00b7lin", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "als er sich r\u00fchmet, wol zuschreibet:", "tokens": ["als", "er", "sich", "r\u00fch\u00b7met", ",", "wol", "zu\u00b7schrei\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "jedoch das wei\u00df ich wol, da\u00df er", "tokens": ["je\u00b7doch", "das", "wei\u00df", "ich", "wol", ",", "da\u00df", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "von ihrer aller neuen m\u00e4r,", "tokens": ["von", "ih\u00b7rer", "al\u00b7ler", "neu\u00b7en", "m\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "gru\u00df und liebbriefen sicher bleibet.", "tokens": ["gru\u00df", "und", "lieb\u00b7brie\u00b7fen", "si\u00b7cher", "blei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}