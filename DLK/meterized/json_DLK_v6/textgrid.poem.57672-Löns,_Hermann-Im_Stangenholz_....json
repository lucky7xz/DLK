{"textgrid.poem.57672": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Im Stangenholz ...", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Stangenholz ruschelt es leise,", "tokens": ["Im", "Stan\u00b7gen\u00b7holz", "ru\u00b7schelt", "es", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Uhlenflucht beginnt,", "tokens": ["Die", "Uh\u00b7len\u00b7flucht", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu sch\u00fcchtern fl\u00fcsternder Weise", "tokens": ["Zu", "sch\u00fcch\u00b7tern", "fl\u00fcs\u00b7tern\u00b7der", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Erstirbt der Tagewind.", "tokens": ["Er\u00b7stirbt", "der", "Ta\u00b7ge\u00b7wind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun, Liebste, la\u00df uns gehen", "tokens": ["Nun", ",", "Liebs\u00b7te", ",", "la\u00df", "uns", "ge\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tief in den Wald hinein,", "tokens": ["Tief", "in", "den", "Wald", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wo die Buchen wie S\u00e4ulen stehen,", "tokens": ["Wo", "die", "Bu\u00b7chen", "wie", "S\u00e4u\u00b7len", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Versilbert vom Mondenschein.", "tokens": ["Ver\u00b7sil\u00b7bert", "vom", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo schwarze Schatten liegen", "tokens": ["Wo", "schwar\u00b7ze", "Schat\u00b7ten", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf der Wege hellem Grund,", "tokens": ["Auf", "der", "We\u00b7ge", "hel\u00b7lem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da will ich mich an dich schmiegen", "tokens": ["Da", "will", "ich", "mich", "an", "dich", "schmie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Herz und Hand und Mund.", "tokens": ["Mit", "Herz", "und", "Hand", "und", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kein W\u00f6rtchen will ich sagen,", "tokens": ["Kein", "W\u00f6rt\u00b7chen", "will", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will sein ganz stumm und still,", "tokens": ["Will", "sein", "ganz", "stumm", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Meines Herzens lautes Schlagen", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "lau\u00b7tes", "Schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allein zu dir sprechen will.", "tokens": ["Al\u00b7lein", "zu", "dir", "spre\u00b7chen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}