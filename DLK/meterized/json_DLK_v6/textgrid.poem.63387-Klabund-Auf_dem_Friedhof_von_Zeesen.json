{"textgrid.poem.63387": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Friedhof von Zeesen", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich steig vom Rad.", "tokens": ["Ich", "steig", "vom", "Rad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein Grab im m\u00e4rkischen Sande.", "tokens": ["Ein", "Grab", "im", "m\u00e4r\u00b7ki\u00b7schen", "San\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hier ruht ein Wesen:", "tokens": ["Hier", "ruht", "ein", "We\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "M\u00e4dchen, Kind und Weib.", "tokens": ["M\u00e4d\u00b7chen", ",", "Kind", "und", "Weib", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sie wurde vierzehn Jahre alt \u2013", "tokens": ["Sie", "wur\u00b7de", "vier\u00b7zehn", "Jah\u00b7re", "alt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und tanzte im Takt des Pulsschlags in den Fiebertod.", "tokens": ["Und", "tanz\u00b7te", "im", "Takt", "des", "Puls\u00b7schlags", "in", "den", "Fie\u00b7ber\u00b7tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie hatte Augen, um das Licht zu halten.", "tokens": ["Sie", "hat\u00b7te", "Au\u00b7gen", ",", "um", "das", "Licht", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Auge brach.", "tokens": ["Das", "Au\u00b7ge", "brach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das Licht gl\u00e4nzt ungebrochen.", "tokens": ["Das", "Licht", "gl\u00e4nzt", "un\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie hatte zarte F\u00fc\u00dfe, auf der Erde zu schreiten \u2013", "tokens": ["Sie", "hat\u00b7te", "zar\u00b7te", "F\u00fc\u00b7\u00dfe", ",", "auf", "der", "Er\u00b7de", "zu", "schrei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und die Erde rollt noch immer.", "tokens": ["Und", "die", "Er\u00b7de", "rollt", "noch", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "--+-+-++", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Sie hatte H\u00e4nde, einen Zweig zu biegen.", "tokens": ["Sie", "hat\u00b7te", "H\u00e4n\u00b7de", ",", "ei\u00b7nen", "Zweig", "zu", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Zweig weht immer noch im Sommerwinde.", "tokens": ["Der", "Zweig", "weht", "im\u00b7mer", "noch", "im", "Som\u00b7mer\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie hatte Lippen, einen Mann zu k\u00fcssen.", "tokens": ["Sie", "hat\u00b7te", "Lip\u00b7pen", ",", "ei\u00b7nen", "Mann", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie ging hinab, eh' sie ein J\u00fcngling k\u00fc\u00dfte.", "tokens": ["Sie", "ging", "hin\u00b7ab", ",", "eh'", "sie", "ein", "J\u00fcng\u00b7ling", "k\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir werfen Netze, um den Wind zu fangen.", "tokens": ["Wir", "wer\u00b7fen", "Net\u00b7ze", ",", "um", "den", "Wind", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir stellen Schlingen f\u00fcr die Wolkenv\u00f6gel.", "tokens": ["Wir", "stel\u00b7len", "Schlin\u00b7gen", "f\u00fcr", "die", "Wol\u00b7ken\u00b7v\u00f6\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir schreien, um an Gottes Ohr zu r\u00fchren. \u2013", "tokens": ["Wir", "schrei\u00b7en", ",", "um", "an", "Got\u00b7tes", "Ohr", "zu", "r\u00fch\u00b7ren", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "APPR", "NN", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir steigen Berge, himmelst\u00fcrmende,", "tokens": ["Wir", "stei\u00b7gen", "Ber\u00b7ge", ",", "him\u00b7mel\u00b7st\u00fcr\u00b7men\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Um j\u00e4h in einem feuchten Loch zu enden.", "tokens": ["Um", "j\u00e4h", "in", "ei\u00b7nem", "feuch\u00b7ten", "Loch", "zu", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Libellen schaukelten um unsern Morgen,", "tokens": ["Li\u00b7bel\u00b7len", "schau\u00b7kel\u00b7ten", "um", "un\u00b7sern", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und unsere Nacht umschwirren Flederm\u00e4use.", "tokens": ["Und", "un\u00b7se\u00b7re", "Nacht", "um\u00b7schwir\u00b7ren", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}