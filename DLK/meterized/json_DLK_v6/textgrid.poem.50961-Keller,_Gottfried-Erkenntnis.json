{"textgrid.poem.50961": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Erkenntnis", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du, o Herz! ein heitres Ziel erreichen,", "tokens": ["Willst", "du", ",", "o", "Herz", "!", "ein", "heit\u00b7res", "Ziel", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "FM", "NN", "$.", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00dft du in eigner Angel schwebend ruhn;", "tokens": ["Mu\u00dft", "du", "in", "eig\u00b7ner", "An\u00b7gel", "schwe\u00b7bend", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Tor versucht zu gehn in fremden Schuhn,", "tokens": ["Ein", "Tor", "ver\u00b7sucht", "zu", "gehn", "in", "frem\u00b7den", "Schuhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur mit sich selbst kann sich der Mann vergleichen!", "tokens": ["Nur", "mit", "sich", "selbst", "kann", "sich", "der", "Mann", "ver\u00b7glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADV", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Tor, der aus des Nachbars Bubenstreichen", "tokens": ["Ein", "Tor", ",", "der", "aus", "des", "Nach\u00b7bars", "Bu\u00b7ben\u00b7strei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich Trost nimmt f\u00fcr das eigne schwache Tun!", "tokens": ["Sich", "Trost", "nimmt", "f\u00fcr", "das", "eig\u00b7ne", "schwa\u00b7che", "Tun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der immer um sich sp\u00e4ht und lauscht und nun", "tokens": ["Der", "im\u00b7mer", "um", "sich", "sp\u00e4ht", "und", "lauscht", "und", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PRF", "ADJD", "KON", "VVFIN", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich seinen Wert bestimmt nach falschen Zeichen!", "tokens": ["Sich", "sei\u00b7nen", "Wert", "be\u00b7stimmt", "nach", "fal\u00b7schen", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Tu frei und offen, was du nicht kannst lassen,", "tokens": ["Tu", "frei", "und", "of\u00b7fen", ",", "was", "du", "nicht", "kannst", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wandle streng auf selbstbeschr\u00e4nkten Wegen", "tokens": ["Doch", "wand\u00b7le", "streng", "auf", "selbst\u00b7be\u00b7schr\u00e4nk\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lerne fr\u00fch nur ", "tokens": ["Und", "ler\u00b7ne", "fr\u00fch", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Dann gehe mild den anderen entgegen!", "tokens": ["Dann", "ge\u00b7he", "mild", "den", "an\u00b7de\u00b7ren", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kannst du dich selbst nur fest zusammenfassen,", "tokens": ["Kannst", "du", "dich", "selbst", "nur", "fest", "zu\u00b7sam\u00b7men\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So h\u00e4ngt an deine Schritte sich der Segen.", "tokens": ["So", "h\u00e4ngt", "an", "dei\u00b7ne", "Schrit\u00b7te", "sich", "der", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}