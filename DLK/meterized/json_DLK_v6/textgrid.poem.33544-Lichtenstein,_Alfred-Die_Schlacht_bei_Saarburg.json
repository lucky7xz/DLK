{"textgrid.poem.33544": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlacht bei Saarburg", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde verschimmelt im Nebel.", "tokens": ["Die", "Er\u00b7de", "ver\u00b7schim\u00b7melt", "im", "Ne\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Abend dr\u00fcckt wie Blei.", "tokens": ["Der", "A\u00b7bend", "dr\u00fcckt", "wie", "Blei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rings rei\u00dft elektrisches Krachen", "tokens": ["Rings", "rei\u00dft", "e\u00b7lekt\u00b7ri\u00b7sches", "Kra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und wimmernd bricht alles entzwei.", "tokens": ["Und", "wim\u00b7mernd", "bricht", "al\u00b7les", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Wie schlechte Lumpen qualmen", "tokens": ["Wie", "schlech\u00b7te", "Lum\u00b7pen", "qual\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die D\u00f6rfer am Horizont.", "tokens": ["Die", "D\u00f6r\u00b7fer", "am", "Ho\u00b7ri\u00b7zont", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich liege gottverlassen", "tokens": ["Ich", "lie\u00b7ge", "gott\u00b7ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In der knatternden Sch\u00fctzenfront.", "tokens": ["In", "der", "knat\u00b7tern\u00b7den", "Sch\u00fct\u00b7zen\u00b7front", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Viel kupferne feindliche V\u00f6gelein", "tokens": ["Viel", "kup\u00b7fer\u00b7ne", "feind\u00b7li\u00b7che", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Surren um Herz und Hirn.", "tokens": ["Sur\u00b7ren", "um", "Herz", "und", "Hirn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich stemme mich steil in das Graue", "tokens": ["Ich", "stem\u00b7me", "mich", "steil", "in", "das", "Grau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und biete dem Tode die Stirn.", "tokens": ["Und", "bie\u00b7te", "dem", "To\u00b7de", "die", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}