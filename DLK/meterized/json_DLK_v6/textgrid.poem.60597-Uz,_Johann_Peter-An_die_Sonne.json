{"textgrid.poem.60597": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An die Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Sonne, K\u00f6niginn der Welt,", "tokens": ["O", "Son\u00b7ne", ",", "K\u00f6\u00b7ni\u00b7ginn", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die unser dunkles Rund erhellt,", "tokens": ["Die", "un\u00b7ser", "dunk\u00b7les", "Rund", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In lichter Majest\u00e4t;", "tokens": ["In", "lich\u00b7ter", "Ma\u00b7jes\u00b7t\u00e4t", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erhabnes Wunder einer Hand,", "tokens": ["Er\u00b7hab\u00b7nes", "Wun\u00b7der", "ei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die jene Himmel ausgespannt,", "tokens": ["Die", "je\u00b7ne", "Him\u00b7mel", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Sterne hinges\u00e4t!", "tokens": ["Und", "Ster\u00b7ne", "hin\u00b7ge\u00b7s\u00e4t", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch heute seh ich deinen Glanz:", "tokens": ["Noch", "heu\u00b7te", "seh", "ich", "dei\u00b7nen", "Glanz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir lacht in ihrem Bluhmenkranz", "tokens": ["Mir", "lacht", "in", "ih\u00b7rem", "Bluh\u00b7men\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch heute die Natur.", "tokens": ["Noch", "heu\u00b7te", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der V\u00f6gel buntgefiedert Heer", "tokens": ["Der", "V\u00f6\u00b7gel", "bunt\u00b7ge\u00b7fie\u00b7dert", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Singt morgen mir vielleicht nicht mehr,", "tokens": ["Singt", "mor\u00b7gen", "mir", "viel\u00b7leicht", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Wald und auf der Flur.", "tokens": ["Im", "Wald", "und", "auf", "der", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchle, da\u00df ich sterblich bin!", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "da\u00df", "ich", "sterb\u00b7lich", "bin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leben welkt, wie Gras, dahin,", "tokens": ["Mein", "Le\u00b7ben", "welkt", ",", "wie", "Gras", ",", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "NN", "$,", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ein verschmachtend Laub.", "tokens": ["Wie", "ein", "ver\u00b7schmach\u00b7tend", "Laub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "VVPP", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer weis, wie unerwartet bald", "tokens": ["Wer", "weis", ",", "wie", "un\u00b7er\u00b7war\u00b7tet", "bald"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PTKVZ", "$,", "PWAV", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des H\u00f6chsten Wort an mich erschallt:", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Wort", "an", "mich", "er\u00b7schallt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Komm wieder in den Staub!", "tokens": ["Komm", "wie\u00b7der", "in", "den", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn mich das finstre Grab verschlingt,", "tokens": ["Wenn", "mich", "das", "finst\u00b7re", "Grab", "ver\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ewig Schweigen mich umringt,", "tokens": ["Ein", "e\u00b7wig", "Schwei\u00b7gen", "mich", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich die Verwesung nagt:", "tokens": ["Mich", "die", "Ver\u00b7we\u00b7sung", "nagt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Alsdann bleibt alles doch zur\u00fcck,", "tokens": ["Als\u00b7dann", "bleibt", "al\u00b7les", "doch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und h\u00e4tte gleich ein l\u00e4chelnd Gl\u00fcck", "tokens": ["Und", "h\u00e4t\u00b7te", "gleich", "ein", "l\u00e4\u00b7chelnd", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir keinen Wunsch versagt!", "tokens": ["Mir", "kei\u00b7nen", "Wunsch", "ver\u00b7sagt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Thorheit, wenn ich mich verkannt,", "tokens": ["O", "Thor\u00b7heit", ",", "wenn", "ich", "mich", "ver\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nach der Erde Lieblingstand,", "tokens": ["Und", "nach", "der", "Er\u00b7de", "Lieb\u00b7lings\u00b7tand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach gro\u00dfem Gut gegeizt!", "tokens": ["Nach", "gro\u00b7\u00dfem", "Gut", "ge\u00b7geizt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn mich der Ehre schimmernd Kleid", "tokens": ["Wenn", "mich", "der", "Eh\u00b7re", "schim\u00b7mernd", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und aller Prunk der Eitelkeit", "tokens": ["Und", "al\u00b7ler", "Prunk", "der", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu niedrem Neid gereizt!", "tokens": ["Zu", "nied\u00b7rem", "Neid", "ge\u00b7reizt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Verlangt mein leiser Wunsch zu viel?", "tokens": ["Ver\u00b7langt", "mein", "lei\u00b7ser", "Wunsch", "zu", "viel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verfolg ich ein zu weites Ziel,", "tokens": ["Ver\u00b7folg", "ich", "ein", "zu", "wei\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ungewissem Pfad?", "tokens": ["Auf", "un\u00b7ge\u00b7wis\u00b7sem", "Pfad", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O Gott, ich beuge mich vor dir!", "tokens": ["O", "Gott", ",", "ich", "beu\u00b7ge", "mich", "vor", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier bin ich, es geschehe mir", "tokens": ["Hier", "bin", "ich", ",", "es", "ge\u00b7sche\u00b7he", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach deinem bessern Rath!", "tokens": ["Nach", "dei\u00b7nem", "bes\u00b7sern", "Rath", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Mensch, der aufgeblasne Thor,", "tokens": ["Der", "Mensch", ",", "der", "auf\u00b7ge\u00b7blas\u00b7ne", "Thor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schreibt seinem Sch\u00f6pfer Weisheit vor?", "tokens": ["Schreibt", "sei\u00b7nem", "Sch\u00f6p\u00b7fer", "Weis\u00b7heit", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir, gro\u00dfer Menschenfreund?", "tokens": ["Dir", ",", "gro\u00b7\u00dfer", "Men\u00b7schen\u00b7freund", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du liebst ihn mehr, als er sich liebt,", "tokens": ["Du", "liebst", "ihn", "mehr", ",", "als", "er", "sich", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann deine Huld nicht immer giebt,", "tokens": ["Wann", "dei\u00b7ne", "Huld", "nicht", "im\u00b7mer", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was jedem n\u00fctzlich scheint.", "tokens": ["Was", "je\u00b7dem", "n\u00fctz\u00b7lich", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wann der bethaute Morgen lacht,", "tokens": ["Wann", "der", "be\u00b7thau\u00b7te", "Mor\u00b7gen", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann von den Fittigen der Nacht", "tokens": ["Wann", "von", "den", "Fit\u00b7ti\u00b7gen", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Stunden k\u00fchler sind;", "tokens": ["Die", "Stun\u00b7den", "k\u00fch\u00b7ler", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Spricht mir die Weisheit liebreich zu:", "tokens": ["Spricht", "mir", "die", "Weis\u00b7heit", "lieb\u00b7reich", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Sterblicher, was sorgest du,", "tokens": ["O", "Sterb\u00b7li\u00b7cher", ",", "was", "sor\u00b7gest", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00fcnschest in den Wind?", "tokens": ["Und", "w\u00fcn\u00b7schest", "in", "den", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der dich gemacht, sorgt auch f\u00fcr dich!", "tokens": ["Der", "dich", "ge\u00b7macht", ",", "sorgt", "auch", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht auf die Erde schr\u00e4nket sich", "tokens": ["Nicht", "auf", "die", "Er\u00b7de", "schr\u00e4n\u00b7ket", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Plan des Himmels ein.", "tokens": ["Der", "Plan", "des", "Him\u00b7mels", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die\u00df Leben ist ein Augenblick,", "tokens": ["Die\u00df", "Le\u00b7ben", "ist", "ein", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Fr\u00fchlingstraum das l\u00e4ngste Gl\u00fcck:", "tokens": ["Ein", "Fr\u00fch\u00b7lings\u00b7traum", "das", "l\u00e4ngs\u00b7te", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du sollst unsterblich seyn!", "tokens": ["Du", "sollst", "uns\u00b7terb\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Gedanke der Unsterblichkeit,", "tokens": ["Ge\u00b7dan\u00b7ke", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Der \u00fcber Erde, Welt und Zeit", "tokens": ["Der", "\u00fc\u00b7ber", "Er\u00b7de", ",", "Welt", "und", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein edles Herz erhebt!", "tokens": ["Ein", "ed\u00b7les", "Herz", "er\u00b7hebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Emp\u00f6re dich in meiner Brust,", "tokens": ["Em\u00b7p\u00f6\u00b7re", "dich", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn die Sirene falscher Lust", "tokens": ["Wenn", "die", "Si\u00b7re\u00b7ne", "fal\u00b7scher", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "AR