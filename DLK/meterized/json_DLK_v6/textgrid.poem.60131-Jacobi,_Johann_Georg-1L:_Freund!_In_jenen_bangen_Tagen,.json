{"textgrid.poem.60131": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund! In jenen bangen Tagen,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund! In jenen bangen Tagen,", "tokens": ["Freund", "!", "In", "je\u00b7nen", "ban\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als so tief die Menschheit fiel,", "tokens": ["Als", "so", "tief", "die", "Menschheit", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ehrt ich deine frommen Klagen,", "tokens": ["Ehrt", "ich", "dei\u00b7ne", "from\u00b7men", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fchrte nicht mein Saitenspiel;", "tokens": ["R\u00fchr\u00b7te", "nicht", "mein", "Sai\u00b7ten\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber, hohen Muthes voll,", "tokens": ["A\u00b7ber", ",", "ho\u00b7hen", "Mu\u00b7thes", "voll", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlag' ich lauter nun die Leyer,", "tokens": ["Schlag'", "ich", "lau\u00b7ter", "nun", "die", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Weil kein H\u00f6llen-Ungeheuer", "tokens": ["Weil", "kein", "H\u00f6l\u00b7len\u00b7Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unser Gl\u00fcck uns rauben soll.", "tokens": ["Un\u00b7ser", "Gl\u00fcck", "uns", "rau\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bleibt doch Gottes Sonne stehen,", "tokens": ["Bleibt", "doch", "Got\u00b7tes", "Son\u00b7ne", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sie unsre V\u00e4ter sahn,", "tokens": ["Wo", "sie", "uns\u00b7re", "V\u00e4\u00b7ter", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird der Mond doch gl\u00e4nzend gehen,", "tokens": ["Wird", "der", "Mond", "doch", "gl\u00e4n\u00b7zend", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie vor Alters, seine Bahn;", "tokens": ["Wie", "vor", "Al\u00b7ters", ",", "sei\u00b7ne", "Bahn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch der Sternlein goldnes Chor,", "tokens": ["Auch", "der", "Stern\u00b7lein", "gold\u00b7nes", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn die B\u00fcsche friedlich thauen,", "tokens": ["Wenn", "die", "B\u00fc\u00b7sche", "fried\u00b7lich", "thau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Redet mit uns im Vertrauen,", "tokens": ["Re\u00b7det", "mit", "uns", "im", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hebt den Geist zu sich empor.", "tokens": ["Hebt", "den", "Geist", "zu", "sich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df der Zwietracht Fackel w\u00fcthen", "tokens": ["La\u00df", "der", "Zwiet\u00b7racht", "Fa\u00b7ckel", "w\u00fct\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis zur letzten Gr\u00e4uelthat!", "tokens": ["Bis", "zur", "letz\u00b7ten", "Gr\u00e4u\u00b7elt\u00b7hat", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandelt nicht im Kranz von Bl\u00fcthen", "tokens": ["Wan\u00b7delt", "nicht", "im", "Kranz", "von", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottes Segen um die Saat?", "tokens": ["Got\u00b7tes", "Se\u00b7gen", "um", "die", "Saat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann des Aufruhrs Feldgeschrey", "tokens": ["Kann", "des", "Auf\u00b7ruhrs", "Feld\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wider uns den West emp\u00f6ren", "tokens": ["Wi\u00b7der", "uns", "den", "West", "em\u00b7p\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Ger\u00e4usch der B\u00e4che st\u00f6hren", "tokens": ["Das", "Ge\u00b7r\u00e4usch", "der", "B\u00e4\u00b7che", "st\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und den Waldgesang im May?", "tokens": ["Und", "den", "Wald\u00b7ge\u00b7sang", "im", "May", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Was da lispelt, singt und rauschet,", "tokens": ["Was", "da", "lis\u00b7pelt", ",", "singt", "und", "rau\u00b7schet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcndigt dem geweihten Mann,", "tokens": ["K\u00fcn\u00b7digt", "dem", "ge\u00b7weih\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der auf jedes Bl\u00e4ttchen lauschet,", "tokens": ["Der", "auf", "je\u00b7des", "Bl\u00e4tt\u00b7chen", "lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freude nur und Eintracht an;", "tokens": ["Freu\u00b7de", "nur", "und", "Ein\u00b7tracht", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freude s\u00e4uselt durch das Feld,", "tokens": ["Freu\u00b7de", "s\u00e4u\u00b7selt", "durch", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn vorbey die St\u00fcrme zogen,", "tokens": ["Wenn", "vor\u00b7bey", "die", "St\u00fcr\u00b7me", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Friede seinen Bogen", "tokens": ["Und", "der", "Frie\u00b7de", "sei\u00b7nen", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In die Wetterwolke stellt.", "tokens": ["In", "die", "Wet\u00b7ter\u00b7wol\u00b7ke", "stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus des P\u00f6bels tollen H\u00e4nden,", "tokens": ["Aus", "des", "P\u00f6\u00b7bels", "tol\u00b7len", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die am selbst gest\u00fcrzten Herd", "tokens": ["Die", "am", "selbst", "ge\u00b7st\u00fcrz\u00b7ten", "Herd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADV", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Vaterland und Freyheit sch\u00e4nden,", "tokens": ["Va\u00b7ter\u00b7land", "und", "Frey\u00b7heit", "sch\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Winde F\u00fcrsten-Macht das Schwert;", "tokens": ["Win\u00b7de", "F\u00fcrs\u00b7ten\u00b7Macht", "das", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der stolze K\u00f6nigssohn", "tokens": ["Und", "der", "stol\u00b7ze", "K\u00f6\u00b7nigs\u00b7sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Spreche da, wo seine Blitze", "tokens": ["Spre\u00b7che", "da", ",", "wo", "sei\u00b7ne", "Blit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Trafen, vom Tyrannen- Sitze", "tokens": ["Tra\u00b7fen", ",", "vom", "Ty\u00b7ran\u00b7nen", "Sit\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPRART", "TRUNC", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Feig gewordnen V\u00f6lkern Hohn!", "tokens": ["Feig", "ge\u00b7word\u00b7nen", "V\u00f6l\u00b7kern", "Hohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Keiner Lerche Lied verstummer", "tokens": ["Kei\u00b7ner", "Ler\u00b7che", "Lied", "ver\u00b7stum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dem Wink der Majest\u00e4t;", "tokens": ["Vor", "dem", "Wink", "der", "Ma\u00b7jes\u00b7t\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Honig sucht die Bien' und summet", "tokens": ["Ho\u00b7nig", "sucht", "die", "Bi\u00b7en'", "und", "sum\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Fort auf ihrem Blumenbeet;", "tokens": ["Fort", "auf", "ih\u00b7rem", "Blu\u00b7men\u00b7beet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Holder Freyheit Lobgesang", "tokens": ["Hol\u00b7der", "Frey\u00b7heit", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schallt von allen H\u00fcgeln nieder,", "tokens": ["Schallt", "von", "al\u00b7len", "H\u00fc\u00b7geln", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "T\u00f6nt in M\u00e4nner-Herzen wieder", "tokens": ["T\u00f6nt", "in", "M\u00e4n\u00b7ner\u00b7Her\u00b7zen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bey der Sklaven Ketten-Klang.", "tokens": ["Bey", "der", "Skla\u00b7ven", "Ket\u00b7ten\u00b7Klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sollt' herauf aus ihren N\u00e4chten", "tokens": ["Sollt'", "her\u00b7auf", "aus", "ih\u00b7ren", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die ganze H\u00f6lle ziehn,", "tokens": ["Auch", "die", "gan\u00b7ze", "H\u00f6l\u00b7le", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das H\u00e4uflein der Gerechten", "tokens": ["Und", "das", "H\u00e4uf\u00b7lein", "der", "Ge\u00b7rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit geschm\u00e4hter Tugend fliehn;", "tokens": ["Mit", "ge\u00b7schm\u00e4h\u00b7ter", "Tu\u00b7gend", "fliehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00fcbte sich des Tages Licht,", "tokens": ["Tr\u00fcb\u00b7te", "sich", "des", "Ta\u00b7ges", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo der Unschuld H\u00fctten sanken,", "tokens": ["Wo", "der", "Un\u00b7schuld", "H\u00fct\u00b7ten", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo Altar und Tempel wanken:", "tokens": ["Wo", "Al\u00b7tar", "und", "Tem\u00b7pel", "wan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dennoch siegt das Laster nicht.", "tokens": ["Den\u00b7noch", "siegt", "das", "Las\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Tugend, weggescheucht in H\u00f6hlen,", "tokens": ["Tu\u00b7gend", ",", "weg\u00b7ge\u00b7scheucht", "in", "H\u00f6h\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schafft noch himmlischen Genu\u00df,", "tokens": ["Schafft", "noch", "himm\u00b7li\u00b7schen", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht das B\u00fcndni\u00df sch\u00f6ner Seelen", "tokens": ["Macht", "das", "B\u00fcn\u00b7dni\u00df", "sch\u00f6\u00b7ner", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Enger, treuer ihren Ku\u00df;", "tokens": ["En\u00b7ger", ",", "treu\u00b7er", "ih\u00b7ren", 