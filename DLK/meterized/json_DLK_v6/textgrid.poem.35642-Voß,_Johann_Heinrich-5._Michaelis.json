{"textgrid.poem.35642": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5. Michaelis", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jehovahs Wagschal' klang; und nicht w\u00fcrdig war", "tokens": ["Je\u00b7ho\u00b7vahs", "Wag\u00b7schal'", "klang", ";", "und", "nicht", "w\u00fcr\u00b7dig", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$.", "KON", "PTKNEG", "ADJD", "VAFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des edlen J\u00fcnglings dieses entnervte Volk,", "tokens": ["Des", "ed\u00b7len", "J\u00fcng\u00b7lings", "die\u00b7ses", "ent\u00b7nerv\u00b7te", "Volk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Wielands Buhlges\u00e4ngen horchet,", "tokens": ["Das", "Wie\u00b7lands", "Buhl\u00b7ge\u00b7s\u00e4n\u00b7gen", "hor\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Daniens K\u00f6nigen Klopstocks Lied schenkt!", "tokens": ["Da\u00b7ni\u00b7ens", "K\u00f6\u00b7ni\u00b7gen", "Klops\u00b7tocks", "Lied", "schenkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "In Edens Lichtkranz strahlt der Verkl\u00e4rte jetzt,", "tokens": ["In", "E\u00b7dens", "Licht\u00b7kranz", "strahlt", "der", "Ver\u00b7kl\u00e4r\u00b7te", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nennt Engel Br\u00fcder, Vater den Ewigen;", "tokens": ["Nennt", "En\u00b7gel", "Br\u00fc\u00b7der", ",", "Va\u00b7ter", "den", "E\u00b7wi\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$,", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Denn keinem Lotterbuben fr\u00f6nen", "tokens": ["Denn", "kei\u00b7nem", "Lot\u00b7ter\u00b7bu\u00b7ben", "fr\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Konnt' er, noch betteln im F\u00fcrstenvorsaal!", "tokens": ["Konnt'", "er", ",", "noch", "bet\u00b7teln", "im", "F\u00fcrs\u00b7ten\u00b7vor\u00b7saal", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "VVINF", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}