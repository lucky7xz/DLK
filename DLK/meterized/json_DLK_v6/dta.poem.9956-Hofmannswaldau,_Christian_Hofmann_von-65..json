{"dta.poem.9956": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin keine knosp und bl\u00fcte/ weder rinde/ frucht noch blatt/", "tokens": ["Ich", "bin", "kei\u00b7ne", "knosp", "und", "bl\u00fc\u00b7te", "/", "we\u00b7der", "rin\u00b7de", "/", "frucht", "noch", "blatt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "VVFIN", "$(", "KON", "VVFIN", "$(", "NN", "ADV", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und doch ist kein baum zu finden/ so mich nicht gezeuget hatr", "tokens": ["Und", "doch", "ist", "kein", "baum", "zu", "fin\u00b7den", "/", "so", "mich", "nicht", "ge\u00b7zeu\u00b7get", "hatr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$(", "ADV", "PPER", "PTKNEG", "VVPP", "VAFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ich bin dicke/ d\u00fcnne/ lang/ ich bin kurtz/ bin gro\u00df und kleine/", "tokens": ["Ich", "bin", "di\u00b7cke", "/", "d\u00fcn\u00b7ne", "/", "lang", "/", "ich", "bin", "kurtz", "/", "bin", "gro\u00df", "und", "klei\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "$(", "VVFIN", "$(", "ADJD", "$(", "PPER", "VAFIN", "ADJD", "$(", "VAFIN", "ADJD", "KON", "ADJA", "$("], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Alle farben nehm ich an/ bin auf erden sehr gemeine.", "tokens": ["Al\u00b7le", "far\u00b7ben", "nehm", "ich", "an", "/", "bin", "auf", "er\u00b7den", "sehr", "ge\u00b7mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "VAFIN", "APPR", "NN", "ADV", "ADJA", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}}}}}