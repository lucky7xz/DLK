{"dta.poem.7953": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "In dieser Zeit, so reich an sch\u00f6nem Sterben,", "tokens": ["In", "die\u00b7ser", "Zeit", ",", "so", "reich", "an", "sch\u00f6\u00b7nem", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Heldentod in fr\u00fchen Jugendtagen,", "tokens": ["An", "Hel\u00b7den\u00b7tod", "in", "fr\u00fc\u00b7hen", "Ju\u00b7gend\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ward dir\u2019s nicht, auf dem Siegesfeld erschlagen,", "tokens": ["Ward", "dir's", "nicht", ",", "auf", "dem", "Sie\u00b7ges\u00b7feld", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den heil\u2019gen Eichenkranz dir zu erwerben.", "tokens": ["Den", "heil'\u00b7gen", "Ei\u00b7chen\u00b7kranz", "dir", "zu", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Beschleichend Fieber brachte dir Verderben,", "tokens": ["Be\u00b7schlei\u00b7chend", "Fie\u00b7ber", "brach\u00b7te", "dir", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wurdest bei der Eltern Weheklagen", "tokens": ["Du", "wur\u00b7dest", "bei", "der", "El\u00b7tern", "We\u00b7he\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus deinem Heimathhause hingetragen", "tokens": ["Aus", "dei\u00b7nem", "Hei\u00b7math\u00b7hau\u00b7se", "hin\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur St\u00e4tte, die nicht Blut, nur Blumen f\u00e4rben.", "tokens": ["Zur", "St\u00e4t\u00b7te", ",", "die", "nicht", "Blut", ",", "nur", "Blu\u00b7men", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PTKNEG", "NN", "$,", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch nein! auch dich ergriff die Zeit des Ruhmes,", "tokens": ["Doch", "nein", "!", "auch", "dich", "er\u00b7griff", "die", "Zeit", "des", "Ruh\u00b7mes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "ADV", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich dr\u00e4ngt\u2019 es, eine Hermannsschlacht zu schaffen,", "tokens": ["Dich", "dr\u00e4ngt'", "es", ",", "ei\u00b7ne", "Her\u00b7manns\u00b7schlacht", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein sinnig Denkmal deutschen Heldenthumes.", "tokens": ["Ein", "sin\u00b7nig", "Denk\u00b7mal", "deut\u00b7schen", "Hel\u00b7den\u00b7thu\u00b7mes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl h\u00f6rtest du noch scheidend Kampfruf schallen,", "tokens": ["Wohl", "h\u00f6r\u00b7test", "du", "noch", "schei\u00b7dend", "Kampf\u00b7ruf", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es wogt\u2019 um dich von M\u00e4nnern, Rossen, Waffen:", "tokens": ["Es", "wogt'", "um", "dich", "von", "M\u00e4n\u00b7nern", ",", "Ros\u00b7sen", ",", "Waf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So bist du in der Hermannsschlacht gefallen.", "tokens": ["So", "bist", "du", "in", "der", "Her\u00b7manns\u00b7schlacht", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}