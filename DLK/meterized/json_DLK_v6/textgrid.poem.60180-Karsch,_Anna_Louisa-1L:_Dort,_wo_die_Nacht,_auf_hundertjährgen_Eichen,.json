{"textgrid.poem.60180": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort, wo die Nacht, auf hundertj\u00e4hrgen Eichen,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort, wo die Nacht, auf hundertj\u00e4hrgen Eichen,", "tokens": ["Dort", ",", "wo", "die", "Nacht", ",", "auf", "hun\u00b7dert\u00b7j\u00e4hr\u00b7gen", "Ei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einem heilgen Dunkel thront,", "tokens": ["In", "ei\u00b7nem", "heil\u00b7gen", "Dun\u00b7kel", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Klagt melancholisch \u00fcber ihre Leichen", "tokens": ["Klagt", "me\u00b7lan\u00b7cho\u00b7lisch", "\u00fc\u00b7ber", "ih\u00b7re", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die Taube, die den Wald bewohnt.", "tokens": ["Die", "Tau\u00b7be", ",", "die", "den", "Wald", "be\u00b7wohnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach Futter war sie ausgeflogen,", "tokens": ["Nach", "Fut\u00b7ter", "war", "sie", "aus\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Inde\u00df der Sturm herauf die Wolke trug,", "tokens": ["In\u00b7de\u00df", "der", "Sturm", "her\u00b7auf", "die", "Wol\u00b7ke", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mit Ei\u00dfkugeln, die ein halbes Pfund gewogen", "tokens": ["Und", "mit", "Ei\u00df\u00b7ku\u00b7geln", ",", "die", "ein", "hal\u00b7bes", "Pfund", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Baum bescho\u00df, und ihre Jungen schlug!", "tokens": ["Den", "Baum", "be\u00b7scho\u00df", ",", "und", "ih\u00b7re", "Jun\u00b7gen", "schlug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort giengen wir, und Gram, wie ihn die Taube", "tokens": ["Dort", "gien\u00b7gen", "wir", ",", "und", "Gram", ",", "wie", "ihn", "die", "Tau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "NN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem dunkeln Hayn auf d\u00fcrren Aesten girrt", "tokens": ["Dem", "dun\u00b7keln", "Hayn", "auf", "d\u00fcr\u00b7ren", "A\u00b7es\u00b7ten", "girrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Gram einer Braut, die in noch gr\u00fcner Laube,", "tokens": ["Gram", "ei\u00b7ner", "Braut", ",", "die", "in", "noch", "gr\u00fc\u00b7ner", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mit ihrem Herzen bey den Todten irrt,", "tokens": ["Mit", "ih\u00b7rem", "Her\u00b7zen", "bey", "den", "Tod\u00b7ten", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gieng Freund! in dir! \u2013 \u2013 Durch jene Kr\u00fcmmen", "tokens": ["Gieng", "Freund", "!", "in", "dir", "!", "\u2013", "\u2013", "Durch", "je\u00b7ne", "Kr\u00fcm\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "APPR", "PPER", "$.", "$(", "$(", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der dickbelaubten B\u00e4ume h\u00f6rtest du", "tokens": ["Der", "dick\u00b7be\u00b7laub\u00b7ten", "B\u00e4u\u00b7me", "h\u00f6r\u00b7test", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In ihren Bl\u00e4ttern tausend sanft gerauschte Stimmen,", "tokens": ["In", "ih\u00b7ren", "Bl\u00e4t\u00b7tern", "tau\u00b7send", "sanft", "ge\u00b7rauschte", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "CARD", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und jede Stimme rief dir traurig zu!", "tokens": ["Und", "je\u00b7de", "Stim\u00b7me", "rief", "dir", "trau\u00b7rig", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier ging er einst an deinem Arm, und fester", "tokens": ["Hier", "ging", "er", "einst", "an", "dei\u00b7nem", "Arm", ",", "und", "fes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch an dein Herz geschlungen! ach! hier gieng", "tokens": ["Noch", "an", "dein", "Herz", "ge\u00b7schlun\u00b7gen", "!", "ach", "!", "hier", "gieng"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$.", "XY", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Freund, der z\u00e4rter noch, als eine Schwester", "tokens": ["Dein", "Freund", ",", "der", "z\u00e4r\u00b7ter", "noch", ",", "als", "ei\u00b7ne", "Schwes\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit seinem Geist an deinem Geiste hieng!", "tokens": ["Mit", "sei\u00b7nem", "Geist", "an", "dei\u00b7nem", "Geis\u00b7te", "hieng", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier f\u00fchltest du mit ihm zugleich das Sch\u00f6ne", "tokens": ["Hier", "f\u00fchl\u00b7test", "du", "mit", "ihm", "zu\u00b7gleich", "das", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Sch\u00f6pfung; o, hier standet ihr,", "tokens": ["Der", "Sch\u00f6p\u00b7fung", ";", "o", ",", "hier", "stan\u00b7det", "ihr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "FM", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie zwo vom besten Vater gleich gebohrne S\u00f6hne", "tokens": ["Wie", "zwo", "vom", "bes\u00b7ten", "Va\u00b7ter", "gleich", "ge\u00b7bohr\u00b7ne", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "CARD", "APPRART", "ADJA", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und spracht von Gott. Hier, sagst du, war es; hier!", "tokens": ["Und", "spracht", "von", "Gott", ".", "Hier", ",", "sagst", "du", ",", "war", "es", ";", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "$.", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach! jede Wunde weinend auszuwaschen;", "tokens": ["Ach", "!", "je\u00b7de", "Wun\u00b7de", "wei\u00b7nend", "aus\u00b7zu\u00b7wa\u00b7schen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIAT", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bey ihm zu knien, bey der Todes Angst", "tokens": ["Bey", "ihm", "zu", "kni\u00b7en", ",", "bey", "der", "To\u00b7des", "Angst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKZU", "VVFIN", "$,", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Durch Seufzer seinen Geist noch aufzuhaschen", "tokens": ["Durch", "Seuf\u00b7zer", "sei\u00b7nen", "Geist", "noch", "auf\u00b7zu\u00b7ha\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies ist der Trost, nach welchem du verlangst.", "tokens": ["Dies", "ist", "der", "Trost", ",", "nach", "wel\u00b7chem", "du", "ver\u00b7langst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O welch ein Schmerz! o welche Freundes-Thr\u00e4nen!", "tokens": ["O", "welch", "ein", "Schmerz", "!", "o", "wel\u00b7che", "Freun\u00b7des\u00b7Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "$.", "FM", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ganz finstrer Kummer war dein Angesicht.", "tokens": ["Ganz", "finst\u00b7rer", "Kum\u00b7mer", "war", "dein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So stumm sitzt, sich an seiner Urne lehnen", "tokens": ["So", "stumm", "sitzt", ",", "sich", "an", "sei\u00b7ner", "Ur\u00b7ne", "leh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "PRF", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Freundschaft deren Auge Klagen", "tokens": ["Die", "Freund\u00b7schaft", "de\u00b7ren", "Au\u00b7ge", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRELAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Freund, keine Seufzer bringen dir ihn wieder!", "tokens": ["Freund", ",", "kei\u00b7ne", "Seuf\u00b7zer", "brin\u00b7gen", "dir", "ihn", "wie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und spieltest du des harten Schicksals Ohr", "tokens": ["Und", "spiel\u00b7test", "du", "des", "har\u00b7ten", "Schick\u00b7sals", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Orpheus allerflehentlichste Lieder", "tokens": ["Des", "Or\u00b7pheus", "al\u00b7ler\u00b7fle\u00b7hent\u00b7lichs\u00b7te", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf einer Steinbezwingbaren Leyer vor!", "tokens": ["Auf", "ei\u00b7ner", "Stein\u00b7be\u00b7zwing\u00b7ba\u00b7ren", "Le\u00b7yer", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Doch, riefst du zu den heiligen Gebeinen", "tokens": ["Doch", ",", "riefst", "du", "zu", "den", "hei\u00b7li\u00b7gen", "Ge\u00b7bei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihn nicht zur\u00fcck den hingeflognen Geist!", "tokens": ["Ihn", "nicht", "zu\u00b7r\u00fcck", "den", "hin\u00b7ge\u00b7flog\u00b7nen", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Zeit, o Freund, mu\u00df dich zu tr\u00f6sten weinen;", "tokens": ["Die", "Zeit", ",", "o", "Freund", ",", "mu\u00df", "dich", "zu", "tr\u00f6s\u00b7ten", "wei\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$,", "VMFIN", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Ewigkeit mi\u00dfg\u00f6nnt' ihr deinen Kleist!", "tokens": ["Die", "E\u00b7wig\u00b7keit", "mi\u00df\u00b7g\u00f6nnt'", "ihr", "dei\u00b7nen", "Kleist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}