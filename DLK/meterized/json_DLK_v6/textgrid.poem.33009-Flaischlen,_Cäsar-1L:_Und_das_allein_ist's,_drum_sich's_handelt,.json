{"textgrid.poem.33009": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und das allein ist's, drum sich's handelt,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und das allein ist's, drum sich's handelt,", "tokens": ["Und", "das", "al\u00b7lein", "ist's", ",", "drum", "sich's", "han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VAFIN", "$,", "PAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wie Welt und Zeit auch st\u00fcrmt und wandelt", "tokens": ["wie", "Welt", "und", "Zeit", "auch", "st\u00fcrmt", "und", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mit allem, was du je begannst:", "tokens": ["mit", "al\u00b7lem", ",", "was", "du", "je", "be\u00b7gannst", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df ohne Vorwurf, ohne L\u00fcge,", "tokens": ["da\u00df", "oh\u00b7ne", "Vor\u00b7wurf", ",", "oh\u00b7ne", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df ohne Reue, ohne R\u00fcge,", "tokens": ["da\u00df", "oh\u00b7ne", "Reu\u00b7e", ",", "oh\u00b7ne", "R\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "auch vor dem eigenen Tribunal,", "tokens": ["auch", "vor", "dem", "ei\u00b7ge\u00b7nen", "Tri\u00b7bu\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "da\u00df du mit ruhigem Gewissen", "tokens": ["da\u00df", "du", "mit", "ru\u00b7hi\u00b7gem", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "zur\u00fcck- und vorw\u00e4rtsblicken kannst", "tokens": ["zu\u00b7r\u00fcck", "und", "vor\u00b7w\u00e4rts\u00b7bli\u00b7cken", "kannst"], "token_info": ["word", "word", "word", "word"], "pos": ["TRUNC", "KON", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "auf deines Jahres stille M\u00fche ...", "tokens": ["auf", "dei\u00b7nes", "Jah\u00b7res", "stil\u00b7le", "M\u00fc\u00b7he", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "ob du verlorst, ob du gewannst.", "tokens": ["ob", "du", "ver\u00b7lorst", ",", "ob", "du", "ge\u00b7wannst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht fremden Anderen zu Dank ...", "tokens": ["Nicht", "frem\u00b7den", "An\u00b7de\u00b7ren", "zu", "Dank", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "was denn auch sollen diese Andern!", "tokens": ["was", "denn", "auch", "sol\u00b7len", "die\u00b7se", "An\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VMFIN", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "es ist ja doch ein stetes Wandern", "tokens": ["es", "ist", "ja", "doch", "ein", "ste\u00b7tes", "Wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "voll Mi\u00dfgunst \u00fcberall und Zank!", "tokens": ["voll", "Mi\u00df\u00b7gunst", "\u00fc\u00b7be\u00b7rall", "und", "Zank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nein, dir allein zu Recht und Ehre,", "tokens": ["Nein", ",", "dir", "al\u00b7lein", "zu", "Recht", "und", "Eh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "dir allein zu Lust und Last:", "tokens": ["dir", "al\u00b7lein", "zu", "Lust", "und", "Last", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mag man's dann loben oder tadeln,", "tokens": ["Mag", "man's", "dann", "lo\u00b7ben", "o\u00b7der", "ta\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "was liegt daran!?", "tokens": ["was", "liegt", "da\u00b7ran", "!?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es wird sich immer adeln,", "tokens": ["Es", "wird", "sich", "im\u00b7mer", "a\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "trotz Acht und Bann:", "tokens": ["trotz", "Acht", "und", "Bann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "wer ohne Vorwurf, ohne L\u00fcge,", "tokens": ["wer", "oh\u00b7ne", "Vor\u00b7wurf", ",", "oh\u00b7ne", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wer ohne Reue, ohne R\u00fcge", "tokens": ["wer", "oh\u00b7ne", "Reu\u00b7e", ",", "oh\u00b7ne", "R\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "APPR", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "zur\u00fccksehn darf und sagen kann", "tokens": ["zu\u00b7r\u00fcck\u00b7sehn", "darf", "und", "sa\u00b7gen", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVINF", "VMFIN", "KON", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "von seines Jahres stiller M\u00fche:", "tokens": ["von", "sei\u00b7nes", "Jah\u00b7res", "stil\u00b7ler", "M\u00fc\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "er habe seine Pflicht getan ...", "tokens": ["er", "ha\u00b7be", "sei\u00b7ne", "Pflicht", "ge\u00b7tan", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "ob er verlor, ob er gewann ...", "tokens": ["ob", "er", "ver\u00b7lor", ",", "ob", "er", "ge\u00b7wann", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und weder Gl\u00fcck noch Ungl\u00fcck", "tokens": ["Und", "we\u00b7der", "Gl\u00fcck", "noch", "Un\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "hab je was \u00fcber ihn vermocht,", "tokens": ["hab", "je", "was", "\u00fc\u00b7ber", "ihn", "ver\u00b7mocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und weder T\u00e4uschung noch Erf\u00fcllung", "tokens": ["und", "we\u00b7der", "T\u00e4u\u00b7schung", "noch", "Er\u00b7f\u00fcl\u00b7lung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "das freie Herz ihm unterjocht!", "tokens": ["das", "frei\u00b7e", "Herz", "ihm", "un\u00b7ter\u00b7jocht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}