"Pflas\u00b7ter", ",", "\u2013", "da\u00df", "ich", "rasch", "mich", "b\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "$(", "KOUS", "PPER", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und deinen Hauch noch warm vom Munde nahm", "tokens": ["und", "dei\u00b7nen", "Hauch", "noch", "warm", "vom", "Mun\u00b7de", "nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der schweren Rosen, deren Gru\u00df den Gram", "tokens": ["der", "schwe\u00b7ren", "Ro\u00b7sen", ",", "de\u00b7ren", "Gru\u00df", "den", "Gram"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der kurzen Trennung duftend \u00fcberbr\u00fcckte.", "tokens": ["der", "kur\u00b7zen", "Tren\u00b7nung", "duf\u00b7tend", "\u00fc\u00b7berb\u00b7r\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}