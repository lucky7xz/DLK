{"textgrid.poem.38729": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heilig, reine, milde Fluth,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heilig, reine, milde Fluth,", "tokens": ["Hei\u00b7lig", ",", "rei\u00b7ne", ",", "mil\u00b7de", "Fluth", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kind der Liebe, klares Wasser!", "tokens": ["Kind", "der", "Lie\u00b7be", ",", "kla\u00b7res", "Was\u00b7ser", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als die neue Welt dem Zorne", "tokens": ["Als", "die", "neu\u00b7e", "Welt", "dem", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War im ersten Seyn erstarret,", "tokens": ["War", "im", "ers\u00b7ten", "Seyn", "er\u00b7star\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Kr\u00e4fte ihr entflohen", "tokens": ["Al\u00b7le", "Kr\u00e4f\u00b7te", "ihr", "ent\u00b7flo\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr innres Herz erkaltet,", "tokens": ["Und", "ihr", "inn\u00b7res", "Herz", "er\u00b7kal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwebte sie ein harter Leichnam", "tokens": ["Schweb\u00b7te", "sie", "ein", "har\u00b7ter", "Leich\u00b7nam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die leeren Himmelbahnen,", "tokens": ["Durch", "die", "lee\u00b7ren", "Him\u00b7mel\u00b7bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In sich keine Lebensgeister,", "tokens": ["In", "sich", "kei\u00b7ne", "Le\u00b7bens\u00b7geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ueber sich nicht Sternverwandten.", "tokens": ["Ue\u00b7ber", "sich", "nicht", "Stern\u00b7ver\u00b7wand\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und es regte sich ein Schmerz,", "tokens": ["Und", "es", "reg\u00b7te", "sich", "ein", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Liebe ganz und ganz Erbarmen,", "tokens": ["Lie\u00b7be", "ganz", "und", "ganz", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "In den allerreinsten Himmeln,", "tokens": ["In", "den", "al\u00b7ler\u00b7reins\u00b7ten", "Him\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Legte sich wie weiche Arme", "tokens": ["Leg\u00b7te", "sich", "wie", "wei\u00b7che", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Um den stumm gewordnen Busen,", "tokens": ["Um", "den", "stumm", "ge\u00b7word\u00b7nen", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und das Herz drinnen erwarmte:", "tokens": ["Und", "das", "Herz", "drin\u00b7nen", "er\u00b7warm\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Und es f\u00fchlte erst ein Zittern,", "tokens": ["Und", "es", "f\u00fchl\u00b7te", "erst", "ein", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Dann ein tief erbebend Bangen,", "tokens": ["Dann", "ein", "tief", "er\u00b7be\u00b7bend", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und es ri\u00df sich von der Furcht", "tokens": ["Und", "es", "ri\u00df", "sich", "von", "der", "Furcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Und dem ungewissen Zagen,", "tokens": ["Und", "dem", "un\u00b7ge\u00b7wis\u00b7sen", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Gab sich ganz und voll dem Schmerz hin,", "tokens": ["Gab", "sich", "ganz", "und", "voll", "dem", "Schmerz", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "KON", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Da\u00df umher nur Todten-Halle,", "tokens": ["Da\u00df", "um\u00b7her", "nur", "Tod\u00b7ten\u00b7Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.21": {"text": "Alle Jugend ihm entschwunden", "tokens": ["Al\u00b7le", "Ju\u00b7gend", "ihm", "ent\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und die Lust erstarb, die alte.", "tokens": ["Und", "die", "Lust", "er\u00b7starb", ",", "die", "al\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Wie die Welt in Schmerz und Wehen,", "tokens": ["Wie", "die", "Welt", "in", "Schmerz", "und", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und Erinnerungen kamen,", "tokens": ["Und", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Und die Himmelsliebe au\u00dfen", "tokens": ["Und", "die", "Him\u00b7mels\u00b7lie\u00b7be", "au\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Sie noch sanfter, weicher fa\u00dfte,", "tokens": ["Sie", "noch", "sanf\u00b7ter", ",", "wei\u00b7cher", "fa\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wollt' sie sterbend ganz vergehen;", "tokens": ["Wollt'", "sie", "ster\u00b7bend", "ganz", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und die starren Riegel sprangen,", "tokens": ["Und", "die", "star\u00b7ren", "Rie\u00b7gel", "spran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Und den harten Tod zerri\u00df", "tokens": ["Und", "den", "har\u00b7ten", "Tod", "zer\u00b7ri\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.30": {"text": "Nach dem Tode das Verlangen,", "tokens": ["Nach", "dem", "To\u00b7de", "das", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Heil'ge Lebensthr\u00e4nen, s\u00fc\u00dfe,", "tokens": ["Heil'\u00b7ge", "Le\u00b7benst\u00b7hr\u00e4\u00b7nen", ",", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Aus der innern Tiefe rannen", "tokens": ["Aus", "der", "in\u00b7nern", "Tie\u00b7fe", "ran\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Ueber das erbla\u00dfte Antlitz,", "tokens": ["Ue\u00b7ber", "das", "er\u00b7bla\u00df\u00b7te", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Ueber die entstellten Wangen:", "tokens": ["Ue\u00b7ber", "die", "ent\u00b7stell\u00b7ten", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Und im Schmerz entz\u00fcndete", "tokens": ["Und", "im", "Schmerz", "ent\u00b7z\u00fcn\u00b7de\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Sich die Freude pl\u00f6tzlich, brannte,", "tokens": ["Sich", "die", "Freu\u00b7de", "pl\u00f6tz\u00b7lich", ",", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Und das Licht flog schnell empor,", "tokens": ["Und", "das", "Licht", "flog", "schnell", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Kehrte wieder und umarmte", "tokens": ["Kehr\u00b7te", "wie\u00b7der", "und", "um\u00b7arm\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Sie, die liebe arme Mutter", "tokens": ["Sie", ",", "die", "lie\u00b7be", "ar\u00b7me", "Mut\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Und das Kind, das heil'ge Wasser:", "tokens": ["Und", "das", "Kind", ",", "das", "heil'\u00b7ge", "Was\u00b7ser", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Blumen, gr\u00fcne Kr\u00e4uter spro\u00dften,", "tokens": ["Blu\u00b7men", ",", "gr\u00fc\u00b7ne", "Kr\u00e4u\u00b7ter", "spro\u00df\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Str\u00f6me fluteten und brachen", "tokens": ["Str\u00f6\u00b7me", "flu\u00b7te\u00b7ten", "und", "bra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "In das Meer, das neu geboren,", "tokens": ["In", "das", "Meer", ",", "das", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Und Gestirn' im goldnen Glanze", "tokens": ["Und", "Ge\u00b7stirn'", "im", "gold\u00b7nen", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.45": {"text": "Sahen liebend hoch hernieder,", "tokens": ["Sa\u00b7hen", "lie\u00b7bend", "hoch", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Sonne mit dem klaren Antlitz,", "tokens": ["Son\u00b7ne", "mit", "dem", "kla\u00b7ren", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Mond mit seinem stillen Troste,", "tokens": ["Mond", "mit", "sei\u00b7nem", "stil\u00b7len", "Tros\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Kleine Lichter magisch wandelnd", "tokens": ["Klei\u00b7ne", "Lich\u00b7ter", "ma\u00b7gisch", "wan\u00b7delnd"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Blumen in der blauen Tiefe;", "tokens": ["Blu\u00b7men", "in", "der", "blau\u00b7en", "Tie\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Und die Thiere waren alle", "tokens": ["Und", "die", "Thie\u00b7re", "wa\u00b7ren", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Schon in Lebensregung, endlich", "tokens": ["Schon", "in", "Le\u00b7bens\u00b7re\u00b7gung", ",", "end\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "NN", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Kam der fromme Mensch gegangen,", "tokens": ["Kam", "der", "from\u00b7me", "Mensch", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Und die Thiere und die Steine,", "tokens": ["Und", "die", "Thie\u00b7re", "und", "die", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.54": {"text": "Und die Fische und die Pflanzen,", "tokens": ["Und", "die", "Fi\u00b7sche", "und", "die", "Pflan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Und die Sterne und die Lichter,", "tokens": ["Und", "die", "Ster\u00b7ne", "und", "die", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Und die Menschen betend dankten", "tokens": ["Und", "die", "Men\u00b7schen", "be\u00b7tend", "dank\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Dir Erzeuger, heil'ges, reines,", "tokens": ["Dir", "Er\u00b7zeu\u00b7ger", ",", "heil'\u00b7ges", ",", "rei\u00b7nes", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Frucht erregend klares Wasser.", "tokens": ["Frucht", "er\u00b7re\u00b7gend", "kla\u00b7res", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}