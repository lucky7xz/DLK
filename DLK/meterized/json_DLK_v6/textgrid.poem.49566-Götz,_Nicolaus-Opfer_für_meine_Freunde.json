{"textgrid.poem.49566": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Opfer f\u00fcr meine Freunde", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann ich ein Lamm, ein Kr\u00e4nzchen, eine Taube", "tokens": ["Wann", "ich", "ein", "Lamm", ",", "ein", "Kr\u00e4nz\u00b7chen", ",", "ei\u00b7ne", "Tau\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den G\u00f6ttern des Olymps zum Opfer bring',", "tokens": ["Den", "G\u00f6t\u00b7tern", "des", "O\u00b7lymps", "zum", "Op\u00b7fer", "bring'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ersuch ich sie, auf meinem Knie, im Staube,", "tokens": ["Er\u00b7such", "ich", "sie", ",", "auf", "mei\u00b7nem", "Knie", ",", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um Reichthum nicht; er ist ein mi\u00dflich Ding!", "tokens": ["Um", "Reicht\u00b7hum", "nicht", ";", "er", "ist", "ein", "mi\u00df\u00b7lich", "Ding", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PTKNEG", "$.", "PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erhaltet mir, sprech ich, was ich empfieng.", "tokens": ["Er\u00b7hal\u00b7tet", "mir", ",", "sprech", "ich", ",", "was", "ich", "emp\u00b7fi\u00b7eng", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ihr lenket ja, Unsterbliche, die Triebe", "tokens": ["Ihr", "len\u00b7ket", "ja", ",", "U\u00b7nsterb\u00b7li\u00b7che", ",", "die", "Trie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Der Herzen. Ach! bewahret bi\u00df ins Grab,", "tokens": ["Der", "Her\u00b7zen", ".", "Ach", "!", "be\u00b7wah\u00b7ret", "bi\u00df", "ins", "Grab", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ITJ", "$.", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bewahret mir die Herzen die ich liebe;", "tokens": ["Be\u00b7wah\u00b7ret", "mir", "die", "Her\u00b7zen", "die", "ich", "lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und schlagt mir sonst, was euch beliebet, ab!", "tokens": ["Und", "schlagt", "mir", "sonst", ",", "was", "euch", "be\u00b7lie\u00b7bet", ",", "ab", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}