{"textgrid.poem.65319": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Hyazinthenstrau\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebtes M\u00e4dchen, geh' und setz' in's Wasser", "tokens": ["Ge\u00b7lieb\u00b7tes", "M\u00e4d\u00b7chen", ",", "geh'", "und", "setz'", "in's", "Was\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Hyazinthenstrau\u00df je ehr, je besser,", "tokens": ["Den", "Hy\u00b7a\u00b7zin\u00b7then\u00b7strau\u00df", "je", "ehr", ",", "je", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sonst wird er in der hei\u00dfen Luft verschmachten.", "tokens": ["Sonst", "wird", "er", "in", "der", "hei\u00b7\u00dfen", "Luft", "ver\u00b7schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie wagte wohl mit meines Herzens Gluthen", "tokens": ["Wie", "wag\u00b7te", "wohl", "mit", "mei\u00b7nes", "Her\u00b7zens", "Glu\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Augustus jemals um den Preis zu streiten,", "tokens": ["Au\u00b7gus\u00b7tus", "je\u00b7mals", "um", "den", "Preis", "zu", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "W\u00e4r' auch Scirocco noch mit ihm verbunden!", "tokens": ["W\u00e4r'", "auch", "Sci\u00b7roc\u00b7co", "noch", "mit", "ihm", "ver\u00b7bun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Du aber baust mir keine k\u00fchle Laube,", "tokens": ["Du", "a\u00b7ber", "baust", "mir", "kei\u00b7ne", "k\u00fch\u00b7le", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In der ausruhen k\u00f6nnte meine Liebe", "tokens": ["In", "der", "aus\u00b7ru\u00b7hen", "k\u00f6nn\u00b7te", "mei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVINF", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beim sanften Mondenschimmer deiner Augen.", "tokens": ["Beim", "sanf\u00b7ten", "Mon\u00b7den\u00b7schim\u00b7mer", "dei\u00b7ner", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}