{"textgrid.poem.62860": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohlth\u00e4tigkeit, wie selbst die Bibel saget,", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlth\u00e4tigkeit, wie selbst die Bibel saget,", "tokens": ["Wohlt\u00b7h\u00e4\u00b7tig\u00b7keit", ",", "wie", "selbst", "die", "Bi\u00b7bel", "sa\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rief Harpagon, ist gro\u00df, ist g\u00f6ttlich sch\u00f6n!", "tokens": ["Rief", "Har\u00b7pa\u00b7gon", ",", "ist", "gro\u00df", ",", "ist", "g\u00f6tt\u00b7lich", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weh dem, der einen Bettler von sich jaget!", "tokens": ["Weh", "dem", ",", "der", "ei\u00b7nen", "Bett\u00b7ler", "von", "sich", "ja\u00b7get", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum will ich auch von nun an ... betteln gehn.", "tokens": ["Drum", "will", "ich", "auch", "von", "nun", "an", "...", "bet\u00b7teln", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "APPR", "ADV", "PTKVZ", "$(", "VVINF", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}