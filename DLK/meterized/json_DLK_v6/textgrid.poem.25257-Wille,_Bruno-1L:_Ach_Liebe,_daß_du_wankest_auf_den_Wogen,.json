{"textgrid.poem.25257": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Liebe, da\u00df du wankest auf den Wogen,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Liebe, da\u00df du wankest auf den Wogen,", "tokens": ["Ach", "Lie\u00b7be", ",", "da\u00df", "du", "wan\u00b7kest", "auf", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein morscher Kahn,", "tokens": ["Ein", "mor\u00b7scher", "Kahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zerfetzt das Segel, steuerlos gezogen", "tokens": ["Zer\u00b7fetzt", "das", "Se\u00b7gel", ",", "steu\u00b7er\u00b7los", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Nebelbahn.", "tokens": ["Auf", "Ne\u00b7bel\u00b7bahn", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Des Tages Herz ist blutig hingesunken", "tokens": ["Des", "Ta\u00b7ges", "Herz", "ist", "blu\u00b7tig", "hin\u00b7ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In d\u00fcstre See.", "tokens": ["In", "d\u00fcst\u00b7re", "See", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wo bist du, armer Kahn? Zerschellt, ertrunken?", "tokens": ["Wo", "bist", "du", ",", "ar\u00b7mer", "Kahn", "?", "Zer\u00b7schellt", ",", "er\u00b7trun\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$.", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach Lieb, ade!", "tokens": ["Ach", "Lieb", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Nun will auch ich hintaumeln und versinken", "tokens": ["Nun", "will", "auch", "ich", "hin\u00b7tau\u00b7meln", "und", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In feuchte Gruft.", "tokens": ["In", "feuch\u00b7te", "Gruft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch warnt ein Stern, der Meere Stern, mit Winken", "tokens": ["Doch", "warnt", "ein", "Stern", ",", "der", "Mee\u00b7re", "Stern", ",", "mit", "Win\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus blauem Duft:", "tokens": ["Aus", "blau\u00b7em", "Duft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "\u00bbnur Unrast wirf hinab, die eiteln Sorgen", "tokens": ["\u00bb", "nur", "Un\u00b7rast", "wirf", "hin\u00b7ab", ",", "die", "ei\u00b7teln", "Sor\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "NN", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der w\u00fcsten Welt!", "tokens": ["Der", "w\u00fcs\u00b7ten", "Welt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dein Lieben gib empor! Es sei geborgen", "tokens": ["Dein", "Lie\u00b7ben", "gib", "em\u00b7por", "!", "Es", "sei", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVIMP", "PTKVZ", "$.", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Sternenzelt!", "tokens": ["Im", "Ster\u00b7nen\u00b7zelt", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Was in der Zeiten Brandung ging verloren,", "tokens": ["Was", "in", "der", "Zei\u00b7ten", "Bran\u00b7dung", "ging", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df nichtig sein.", "tokens": ["Mu\u00df", "nich\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Herz allein, dir liebend eingeboren,", "tokens": ["Ein", "Herz", "al\u00b7lein", ",", "dir", "lie\u00b7bend", "ein\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bleibt ewig dein.", "tokens": ["Bleibt", "e\u00b7wig", "dein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und schl\u00fcg es auch am deinen nur f\u00fcr Stunden,", "tokens": ["Und", "schl\u00fcg", "es", "auch", "am", "dei\u00b7nen", "nur", "f\u00fcr", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "PPOSAT", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch Reim bei Reim", "tokens": ["Doch", "Reim", "bei", "Reim"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Seid ihr dem Chor der Seligkeit verbunden", "tokens": ["Seid", "ihr", "dem", "Chor", "der", "Se\u00b7lig\u00b7keit", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "PPER", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seid daheim.\u00ab", "tokens": ["Und", "seid", "da\u00b7heim", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}