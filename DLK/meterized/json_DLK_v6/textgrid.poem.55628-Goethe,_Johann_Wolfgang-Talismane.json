{"textgrid.poem.55628": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Talismane", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gottes ist der Orient!", "tokens": ["Got\u00b7tes", "ist", "der", "O\u00b7rient", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Gottes ist der Okzident!", "tokens": ["Got\u00b7tes", "ist", "der", "Ok\u00b7zi\u00b7dent", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nord- und s\u00fcdliches Gel\u00e4nde", "tokens": ["Nord", "und", "s\u00fcd\u00b7li\u00b7ches", "Ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["TRUNC", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruht im Frieden seiner H\u00e4nde.", "tokens": ["Ruht", "im", "Frie\u00b7den", "sei\u00b7ner", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Er, der einzige Gerechte,", "tokens": ["Er", ",", "der", "ein\u00b7zi\u00b7ge", "Ge\u00b7rech\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Will f\u00fcr jedermann das Rechte.", "tokens": ["Will", "f\u00fcr", "je\u00b7der\u00b7mann", "das", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIS", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei von seinen hundert Namen", "tokens": ["Sei", "von", "sei\u00b7nen", "hun\u00b7dert", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser hochgelobet! Amen.", "tokens": ["Die\u00b7ser", "hoch\u00b7ge\u00b7lo\u00b7bet", "!", "A\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}