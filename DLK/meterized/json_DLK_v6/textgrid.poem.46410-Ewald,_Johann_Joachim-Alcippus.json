{"textgrid.poem.46410": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Alcippus", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alcippus \u00f6ffnet seinen Mund und will uns neue Weisheit lehren:", "tokens": ["Al\u00b7cip\u00b7pus", "\u00f6ff\u00b7net", "sei\u00b7nen", "Mund", "und", "will", "uns", "neu\u00b7e", "Weis\u00b7heit", "leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "KON", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wer Weisheit sch\u00e4tzt der eil und kom ihn anzuh\u00f6ren!", "tokens": ["Wer", "Weis\u00b7heit", "sch\u00e4tzt", "der", "eil", "und", "kom", "ihn", "an\u00b7zu\u00b7h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er hustet laut und spricht: des Leibnitz S\u00e4tze sto\u00df' ich um,", "tokens": ["Er", "hus\u00b7tet", "laut", "und", "spricht", ":", "des", "Leib\u00b7nitz", "S\u00e4t\u00b7ze", "sto\u00df'", "ich", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$.", "ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und mache seine Sch\u00fcler stumm,", "tokens": ["Und", "ma\u00b7che", "sei\u00b7ne", "Sch\u00fc\u00b7ler", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sage wieder ihn: Was in der Welt geschieht, geschiehet ohne Gruno!", "tokens": ["Und", "sa\u00b7ge", "wie\u00b7der", "ihn", ":", "Was", "in", "der", "Welt", "ge\u00b7schieht", ",", "ge\u00b7schie\u00b7het", "oh\u00b7ne", "Gru\u00b7no", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "$.", "PWS", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Aus welchem Grund' o gro\u00dfer Mann willst du was du uns lehrst beweisen?", "tokens": ["Aus", "wel\u00b7chem", "Grund'", "o", "gro\u00b7\u00dfer", "Mann", "willst", "du", "was", "du", "uns", "lehrst", "be\u00b7wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "FM", "ADJA", "NN", "VMFIN", "PPER", "PWS", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.7": {"text": "Entdeck ihn uns! damit wir dich so viel wir sind mit Grunde preisen.", "tokens": ["Ent\u00b7deck", "ihn", "uns", "!", "da\u00b7mit", "wir", "dich", "so", "viel", "wir", "sind", "mit", "Grun\u00b7de", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$.", "KOUS", "PPER", "PRF", "ADV", "ADV", "PPER", "VAFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.8": {"text": "Alcippus hustet noch einmahl und \u00f6ffnet seinen Mund", "tokens": ["Al\u00b7cip\u00b7pus", "hus\u00b7tet", "noch", "ein\u00b7mahl", "und", "\u00f6ff\u00b7net", "sei\u00b7nen", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADV", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Und wiederhohlt: es ist kein Grund.", "tokens": ["Und", "wie\u00b7der\u00b7hohlt", ":", "es", "ist", "kein", "Grund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}