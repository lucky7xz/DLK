{"dta.poem.23695": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Sehnsucht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.85", "tl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J&#252;ngst ging ich in den Garten,", "tokens": ["J", "&#252;", "ngst", "ging", "ich", "in", "den", "Gar\u00b7ten", ","], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohin mich oft die Liebe,", "tokens": ["Wo\u00b7hin", "mich", "oft", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In sichre Schatten lokket;", "tokens": ["In", "sich\u00b7re", "Schat\u00b7ten", "lok\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohin mich oft der Fr\u00fcling", "tokens": ["Wo\u00b7hin", "mich", "oft", "der", "Fr\u00fc\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu meiner Doris f\u00fchret;", "tokens": ["Zu", "mei\u00b7ner", "Do\u00b7ris", "f\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohin mich meine Doris,", "tokens": ["Wo\u00b7hin", "mich", "mei\u00b7ne", "Do\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wann sie allein spatziret,", "tokens": ["Wann", "sie", "al\u00b7lein", "spat\u00b7zi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Selbst oft durch Hekken winket.", "tokens": ["Selbst", "oft", "durch", "Hek\u00b7ken", "win\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Itzt sah ich keine Doris.", "tokens": ["Itzt", "sah", "ich", "kei\u00b7ne", "Do\u00b7ris", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Ich w\u00fcnschte sie zu sehen.", "tokens": ["Ich", "w\u00fcnschte", "sie", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ich schlich in alle G\u00e4nge,", "tokens": ["Ich", "schlich", "in", "al\u00b7le", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und w\u00fcnschte sie zu sehen.", "tokens": ["Und", "w\u00fcnschte", "sie", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Ich lauscht an allen B\u00fcschen,", "tokens": ["Ich", "lauscht", "an", "al\u00b7len", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Und w\u00fcnschte sie zu sehen.", "tokens": ["Und", "w\u00fcnschte", "sie", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Ich strekkte mich im Schatten,", "tokens": ["Ich", "strekk\u00b7te", "mich", "im", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Auf ein bebl\u00fcmtes Lager,", "tokens": ["Auf", "ein", "be\u00b7bl\u00fcm\u00b7tes", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und w\u00fcnschte sie zu sehen.", "tokens": ["Und", "w\u00fcnschte", "sie", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Erm\u00fcdet von den W\u00fcnschen,", "tokens": ["Er\u00b7m\u00fc\u00b7det", "von", "den", "W\u00fcn\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "War ich hier eingeschlafen.", "tokens": ["War", "ich", "hier", "ein\u00b7ge\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ich schlief, und fing im Schlafe,", "tokens": ["Ich", "schlief", ",", "und", "fing", "im", "Schla\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Von neuem an zu w\u00fcnschen.", "tokens": ["Von", "neu\u00b7em", "an", "zu", "w\u00fcn\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}