{"textgrid.poem.67547": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Mein Schicksal", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meines Lebens verworrene", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "ver\u00b7wor\u00b7re\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Schattenfabel! o fr\u00fche, fr\u00fche begann sie schon", "tokens": ["Schat\u00b7ten\u00b7fa\u00b7bel", "!", "o", "fr\u00fc\u00b7he", ",", "fr\u00fc\u00b7he", "be\u00b7gann", "sie", "schon"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "FM", "ADJA", "$,", "ADJA", "VVFIN", "PPER", "ADV"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dunkel. Bebte den kommenden", "tokens": ["Dun\u00b7kel", ".", "Beb\u00b7te", "den", "kom\u00b7men\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "VVFIN", "ART", "ADJA"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Lebensfl\u00fcchtling ein Schau'r hier auf die W\u00fcste der", "tokens": ["Le\u00b7bens\u00b7fl\u00fccht\u00b7ling", "ein", "Schau'r", "hier", "auf", "die", "W\u00fcs\u00b7te", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Erde, da\u00df er in W\u00fcste sich", "tokens": ["Er\u00b7de", ",", "da\u00df", "er", "in", "W\u00fcs\u00b7te", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Unterm Klange der Nacht inne ward, da\u00df ihm Schau'r", "tokens": ["Un\u00b7term", "Klan\u00b7ge", "der", "Nacht", "in\u00b7ne", "ward", ",", "da\u00df", "ihm", "Schau'r"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "PTKVZ", "VAFIN", "$,", "KOUS", "PPER", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "M\u00e4chtig ewig ins Innre klang;", "tokens": ["M\u00e4ch\u00b7tig", "e\u00b7wig", "ins", "Inn\u00b7re", "klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Da\u00df ihm Leben und Tod, Schlummer und Auferstehn,", "tokens": ["Da\u00df", "ihm", "Le\u00b7ben", "und", "Tod", ",", "Schlum\u00b7mer", "und", "Auf\u00b7er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Freud' und Wonne des Lebens ihm,", "tokens": ["Freud'", "und", "Won\u00b7ne", "des", "Le\u00b7bens", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "PPER", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.10": {"text": "Hoher G\u00f6ttergedank' und der zerflie\u00dfenden", "tokens": ["Ho\u00b7her", "G\u00f6t\u00b7ter\u00b7ge\u00b7dank'", "und", "der", "zer\u00b7flie\u00b7\u00dfen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Seele F\u00fclle wie Wandeltraum", "tokens": ["See\u00b7le", "F\u00fcl\u00b7le", "wie", "Wan\u00b7del\u00b7traum"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KOKOM", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Hindurchschwebet; da\u00df ihm seine Erlesenen", "tokens": ["Hin\u00b7durch\u00b7schwe\u00b7bet", ";", "da\u00df", "ihm", "sei\u00b7ne", "Er\u00b7le\u00b7se\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Stets im Wetter vor\u00fcbergehn,", "tokens": ["Stets", "im", "Wet\u00b7ter", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Stets, aus dunkelm Gew\u00f6lk Blitze, die weckenden", "tokens": ["Stets", ",", "aus", "dun\u00b7kelm", "Ge\u00b7w\u00f6lk", "Blit\u00b7ze", ",", "die", "we\u00b7cken\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "V\u00e4terstimmen ihm Mitternachts", "tokens": ["V\u00e4\u00b7ter\u00b7stim\u00b7men", "ihm", "Mit\u00b7ter\u00b7nachts"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Kommen, reden und hinwandeln in Mitternachts-", "tokens": ["Kom\u00b7men", ",", "re\u00b7den", "und", "hin\u00b7wan\u00b7deln", "in", "Mit\u00b7ter\u00b7nachts"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVINF", "KON", "VVFIN", "APPR", "TRUNC"], "meter": "+-----+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Dunkel, und er wandelt allein! \u2013", "tokens": ["Dun\u00b7kel", ",", "und", "er", "wan\u00b7delt", "al\u00b7lein", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "KON", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Schicksalsschwestern, warum? die Ihr sein Tageloos", "tokens": ["Schick\u00b7sals\u00b7schwes\u00b7tern", ",", "wa\u00b7rum", "?", "die", "Ihr", "sein", "Ta\u00b7ge\u00b7loos"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "$.", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Warfet, warfet Ihr's unhold stets", "tokens": ["War\u00b7fet", ",", "war\u00b7fet", "Ih\u00b7r's", "un\u00b7hold", "stets"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PIS", "ADJD", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Irrhin\u00fcber, wohin nimmer das G\u00f6tterbild", "tokens": ["I\u00b7rrhin\u00b7\u00fc\u00b7ber", ",", "wo\u00b7hin", "nim\u00b7mer", "das", "G\u00f6t\u00b7ter\u00b7bild"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Seines leitenden D\u00e4mons wies?", "tokens": ["Sei\u00b7nes", "lei\u00b7ten\u00b7den", "D\u00e4\u00b7mons", "wies", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "Irrhin\u00fcber, wohin aller erstrebenden", "tokens": ["I\u00b7rrhin\u00b7\u00fc\u00b7ber", ",", "wo\u00b7hin", "al\u00b7ler", "er\u00b7stre\u00b7ben\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PIAT", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Ahnung Kr\u00e4fte nicht ahneten? \u2013", "tokens": ["Ah\u00b7nung", "Kr\u00e4f\u00b7te", "nicht", "ah\u00b7ne\u00b7ten", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.24": {"text": "Ach, da weben sie nun meiner erzogenen", "tokens": ["Ach", ",", "da", "we\u00b7ben", "sie", "nun", "mei\u00b7ner", "er\u00b7zo\u00b7ge\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.25": {"text": "Hoffnung Bl\u00fcthe! da weben sie", "tokens": ["Hoff\u00b7nung", "Bl\u00fc\u00b7the", "!", "da", "we\u00b7ben", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.26": {"text": "Einsam, Waisen, wie Wurf n\u00e4chtlich erstarreter", "tokens": ["Ein\u00b7sam", ",", "Wai\u00b7sen", ",", "wie", "Wurf", "n\u00e4cht\u00b7lich", "er\u00b7star\u00b7re\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "$,", "PWAV", "NN", "ADJD", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Fr\u00fchlingsbl\u00e4tter! da flatterst Du,", "tokens": ["Fr\u00fch\u00b7lings\u00b7bl\u00e4t\u00b7ter", "!", "da", "flat\u00b7terst", "Du", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.28": {"text": "Schattenfabel, zerst\u00fcckt, scenezerrissen! Wurf", "tokens": ["Schat\u00b7ten\u00b7fa\u00b7bel", ",", "zer\u00b7st\u00fcckt", ",", "sce\u00b7ne\u00b7zer\u00b7ris\u00b7sen", "!", "Wurf"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVPP", "$,", "VVINF", "$.", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.29": {"text": "Dort und hinnen verlorner Zeit!", "tokens": ["Dort", "und", "hin\u00b7nen", "ver\u00b7lor\u00b7ner", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.30": {"text": "Schicksalsschwestern! o wie? Sammeln sich, sammeln sich", "tokens": ["Schick\u00b7sals\u00b7schwes\u00b7tern", "!", "o", "wie", "?", "Sam\u00b7meln", "sich", ",", "sam\u00b7meln", "sich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "FM", "PWAV", "$.", "NN", "PRF", "$,", "VVFIN", "PRF"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Dem ermatteten Lebensblick", "tokens": ["Dem", "er\u00b7mat\u00b7te\u00b7ten", "Le\u00b7bens\u00b7blick"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.32": {"text": "Einst die Scenen? ersieht er in den wehenden", "tokens": ["Einst", "die", "Sce\u00b7nen", "?", "er\u00b7sieht", "er", "in", "den", "we\u00b7hen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.33": {"text": "Bl\u00e4ttern je der Vorsehung Buch?", "tokens": ["Bl\u00e4t\u00b7tern", "je", "der", "Vor\u00b7se\u00b7hung", "Buch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.34": {"text": "Je einst Ernte der Saat? jener verflogenen", "tokens": ["Je", "einst", "Ern\u00b7te", "der", "Saat", "?", "je\u00b7ner", "ver\u00b7flo\u00b7ge\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "NN", "ART", "NN", "$.", "PDAT", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Erdbegrabnen, gemoderten", "tokens": ["Erd\u00b7be\u00b7grab\u00b7nen", ",", "ge\u00b7mo\u00b7der\u00b7ten"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Keime Fr\u00fchling? und rauscht Aehrengefild hinab,", "tokens": ["Kei\u00b7me", "Fr\u00fch\u00b7ling", "?", "und", "rauscht", "A\u00b7eh\u00b7ren\u00b7ge\u00b7fild", "hin\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "KON", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Rauscht durch fr\u00fcchtebelastete", "tokens": ["Rauscht", "durch", "fr\u00fcch\u00b7te\u00b7be\u00b7las\u00b7te\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ADJA"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.38": {"text": "Zweige? siehet erstaunt sich die verworrene", "tokens": ["Zwei\u00b7ge", "?", "sie\u00b7het", "er\u00b7staunt", "sich", "die", "ver\u00b7wor\u00b7re\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ADJD", "PRF", "ART", "ADJA"], "meter": "+----+-+-+--", "measure": "dactylic.init"}, "line.39": {"text": "Schattenfabel zum Prachttriumph", "tokens": ["Schat\u00b7ten\u00b7fa\u00b7bel", "zum", "Pracht\u00b7tri\u00b7umph"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.40": {"text": "Sammeln? siehet erstaunt Kr\u00fcmmen und Mi\u00dfgestalt", "tokens": ["Sam\u00b7meln", "?", "sie\u00b7het", "er\u00b7staunt", "Kr\u00fcm\u00b7men", "und", "Mi\u00df\u00b7ge\u00b7stalt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ADJD", "NN", "KON", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.41": {"text": "Sich zur Sch\u00f6ne des Ganzen ziehn? \u2013", "tokens": ["Sich", "zur", "Sch\u00f6\u00b7ne", "des", "Gan\u00b7zen", "ziehn", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "APPRART", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.42": {"text": "Schicksalsschwestern, o sprecht! Wie oder liegen mir", "tokens": ["Schick\u00b7sals\u00b7schwes\u00b7tern", ",", "o", "sprecht", "!", "Wie", "o\u00b7der", "lie\u00b7gen", "mir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "VVFIN", "$.", "PWAV", "KON", "VVFIN", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.43": {"text": "In der n\u00e4chtlichen Zukunft Schoo\u00df", "tokens": ["In", "der", "n\u00e4cht\u00b7li\u00b7chen", "Zu\u00b7kunft", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.44": {"text": "Dort noch immer das Heer wartender Schauer? harrt", "tokens": ["Dort", "noch", "im\u00b7mer", "das", "Heer", "war\u00b7ten\u00b7der", "Schau\u00b7er", "?", "harrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$.", "VVFIN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.45": {"text": "Meinem Gange noch bis ans Ziel", "tokens": ["Mei\u00b7nem", "Gan\u00b7ge", "noch", "bis", "ans", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.46": {"text": "Ungewitter? \u2013 Ich h\u00f6r', h\u00f6re sie fernher schon", "tokens": ["Un\u00b7ge\u00b7wit\u00b7ter", "?", "\u2013", "Ich", "h\u00f6r'", ",", "h\u00f6\u00b7re", "sie", "fern\u00b7her", "schon"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.47": {"text": "Fl\u00fcgel schwingen: \u00bbWir werden sein,", "tokens": ["Fl\u00fc\u00b7gel", "schwin\u00b7gen", ":", "\u00bb", "Wir", "wer\u00b7den", "sein", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "$(", "PPER", "VAFIN", "VAINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.48": {"text": "Wie wir waren! o Sohn schaudernder Mitternacht,", "tokens": ["Wie", "wir", "wa\u00b7ren", "!", "o", "Sohn", "schau\u00b7dern\u00b7der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$.", "FM", "NN", "ADJA", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.49": {"text": "Wie wir waren!\u00ab Ihr brauset mir", "tokens": ["Wie", "wir", "wa\u00b7ren", "!", "\u00ab", "Ihr", "brau\u00b7set", "mir"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "$.", "$(", "PPER", "VVFIN", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.50": {"text": "Meinen Wandrergesang, St\u00fcrme! Du, feuriger", "tokens": ["Mei\u00b7nen", "Wand\u00b7rer\u00b7ge\u00b7sang", ",", "St\u00fcr\u00b7me", "!", "Du", ",", "feu\u00b7ri\u00b7ger"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$.", "PPER", "$,", "ADJA"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.51": {"text": "Zeuchst Du, Wette