{"textgrid.poem.52519": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Im Lenz", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie duftig wallt durch Wies und Wald", "tokens": ["Wie", "duf\u00b7tig", "wallt", "durch", "Wies", "und", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die erste Fr\u00fchlingsluft!", "tokens": ["Die", "ers\u00b7te", "Fr\u00fch\u00b7lings\u00b7luft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O kommt heraus, da Jung und Alt", "tokens": ["O", "kommt", "he\u00b7raus", ",", "da", "Jung", "und", "Alt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der frohe Kukuk ruft.", "tokens": ["Der", "fro\u00b7he", "Ku\u00b7kuk", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es ist so still im Sonnenschein,", "tokens": ["Es", "ist", "so", "still", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blumen schlummern noch,", "tokens": ["Die", "Blu\u00b7men", "schlum\u00b7mern", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es rieseln so munter die W\u00e4sserlein", "tokens": ["Es", "rie\u00b7seln", "so", "mun\u00b7ter", "die", "W\u00e4s\u00b7ser\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und h\u00fcpfen vor Freuden hoch.", "tokens": ["Und", "h\u00fcp\u00b7fen", "vor", "Freu\u00b7den", "hoch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort \u00fcber die Matte zum Bl\u00fcthensaft", "tokens": ["Dort", "\u00fc\u00b7ber", "die", "Mat\u00b7te", "zum", "Bl\u00fc\u00b7then\u00b7saft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Fliegt langsam der Schmetterling,", "tokens": ["Fliegt", "lang\u00b7sam", "der", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es l\u00e4uten so leis und geisterhaft", "tokens": ["Es", "l\u00e4u\u00b7ten", "so", "leis", "und", "geis\u00b7ter\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Maiengl\u00f6ckchen klingkling!", "tokens": ["Die", "Mai\u00b7en\u00b7gl\u00f6\u00b7ck\u00b7chen", "kling\u00b7kling", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im W\u00e4ldchen, wo die M\u00e4dchenschaar", "tokens": ["Im", "W\u00e4ld\u00b7chen", ",", "wo", "die", "M\u00e4d\u00b7chen\u00b7schaar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lautscherzend sich ergeht,", "tokens": ["Laut\u00b7scher\u00b7zend", "sich", "er\u00b7geht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hab ich, mit ihrem Ringelhaar,", "tokens": ["Hab", "ich", ",", "mit", "ih\u00b7rem", "Rin\u00b7gel\u00b7haar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Liebchen auch ersp\u00e4ht.", "tokens": ["Mein", "Lieb\u00b7chen", "auch", "er\u00b7sp\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie blickt her\u00fcber und erschrickt", "tokens": ["Sie", "blickt", "her\u00b7\u00fc\u00b7ber", "und", "er\u00b7schrickt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nickt verstohlnerweis;", "tokens": ["Und", "nickt", "ver\u00b7stohl\u00b7ner\u00b7weis", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ist die lose doch geschickt,", "tokens": ["Wie", "ist", "die", "lo\u00b7se", "doch", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie will der Schlauheit Preis!", "tokens": ["Sie", "will", "der", "Schlau\u00b7heit", "Preis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}