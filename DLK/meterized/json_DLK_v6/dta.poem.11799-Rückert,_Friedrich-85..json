{"dta.poem.11799": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott, der den Frieden gibt Friedfert'gen zum Geleit,", "tokens": ["Gott", ",", "der", "den", "Frie\u00b7den", "gibt", "Fried\u00b7fert'\u00b7gen", "zum", "Ge\u00b7leit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "In jedem Sinne geb' er dir Harmlosigkeit.", "tokens": ["In", "je\u00b7dem", "Sin\u00b7ne", "geb'", "er", "dir", "Harm\u00b7lo\u00b7sig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Harmlosigkeit im Ohr h\u00f6rt \u00fcberall Musik,", "tokens": ["Harm\u00b7lo\u00b7sig\u00b7keit", "im", "Ohr", "h\u00f6rt", "\u00fc\u00b7be\u00b7rall", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Sch\u00f6nes \u00fcberall sieht ein harmloser Blick.", "tokens": ["Und", "Sch\u00f6\u00b7nes", "\u00fc\u00b7be\u00b7rall", "sieht", "ein", "harm\u00b7lo\u00b7ser", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Harmlosigkeit im Mund macht niemals Herzen wund,", "tokens": ["Harm\u00b7lo\u00b7sig\u00b7keit", "im", "Mund", "macht", "nie\u00b7mals", "Her\u00b7zen", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ein harmloses Herz ist selbst im Weh gesund.", "tokens": ["Und", "ein", "harm\u00b7lo\u00b7ses", "Herz", "ist", "selbst", "im", "Weh", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Mann ist harmlos, der macht andern keinen Harm,", "tokens": ["Der", "Mann", "ist", "harm\u00b7los", ",", "der", "macht", "an\u00b7dern", "kei\u00b7nen", "Harm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "VVFIN", "PIS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selber sich nicht h\u00e4rmt, er sei reich oder arm.", "tokens": ["Und", "sel\u00b7ber", "sich", "nicht", "h\u00e4rmt", ",", "er", "sei", "reich", "o\u00b7der", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "PTKNEG", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}