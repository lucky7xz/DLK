{"dta.poem.19977": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Glocken haben ausgeklungen,", "tokens": ["Die", "Glo\u00b7cken", "ha\u00b7ben", "aus\u00b7ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schwarzen Kleider zog man aus,", "tokens": ["Die", "schwar\u00b7zen", "Klei\u00b7der", "zog", "man", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Blum' und Bl\u00fcthe ist gedrungen", "tokens": ["Und", "Blum'", "und", "Bl\u00fc\u00b7the", "ist", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Glanzreich an's Licht aus dunklem Haus.", "tokens": ["Glanz\u00b7reich", "an's", "Licht", "aus", "dunk\u00b7lem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Mag noch so bunt die Aue prangen,", "tokens": ["Mag", "noch", "so", "bunt", "die", "Au\u00b7e", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steht paradiesisch Feld und Hain,", "tokens": ["Steht", "pa\u00b7ra\u00b7die\u00b7sisch", "Feld", "und", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schmerz, da\u00df Sie von uns gegangen,", "tokens": ["Der", "Schmerz", ",", "da\u00df", "Sie", "von", "uns", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der dringt in's Herz durch Bl\u00fcthen ein.", "tokens": ["Der", "dringt", "in's", "Herz", "durch", "Bl\u00fc\u00b7then", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch ist's, als k\u00e4m von ", "tokens": ["Doch", "ist's", ",", "als", "k\u00e4m", "von"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "$,", "KOUS", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der Bl\u00fcthenhimmel reich und klar,", "tokens": ["Der", "Bl\u00fc\u00b7then\u00b7him\u00b7mel", "reich", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Die Heilige im Leidensjahr.", "tokens": ["Die", "Hei\u00b7li\u00b7ge", "im", "Lei\u00b7dens\u00b7jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch ist's, als fl\u00f6\u00df, was noch von Segen", "tokens": ["Doch", "ist's", ",", "als", "fl\u00f6\u00df", ",", "was", "noch", "von", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "KOUS", "NN", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Himmels f\u00fchlt die\u00df arme Land:", "tokens": ["Des", "Him\u00b7mels", "f\u00fchlt", "die\u00df", "ar\u00b7me", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mondlicht und Sonnenschein und Regen", "tokens": ["Mond\u00b7licht", "und", "Son\u00b7nen\u00b7schein", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Herab aus ", "tokens": ["Her\u00b7ab", "aus"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Was Menschen thun kann nimmer frommen,", "tokens": ["Was", "Men\u00b7schen", "thun", "kann", "nim\u00b7mer", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns retten Gottes Engel nur;", "tokens": ["Uns", "ret\u00b7ten", "Got\u00b7tes", "En\u00b7gel", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "NN", "NE", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nie wird ein Hungerjahr mehr kommen, \u2014", "tokens": ["Nie", "wird", "ein", "Hun\u00b7ger\u00b7jahr", "mehr", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schwebt ein Schutzgeist ob der Flur.", "tokens": ["Sie", "schwebt", "ein", "Schutz\u00b7geist", "ob", "der", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}