{"textgrid.poem.42214": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Allbesiegerin Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kind, jetzt stehst du auf der H\u00f6he", "tokens": ["Kind", ",", "jetzt", "stehst", "du", "auf", "der", "H\u00f6\u00b7he"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Kultur, das ist gewi\u00df;", "tokens": ["Der", "Kul\u00b7tur", ",", "das", "ist", "ge\u00b7wi\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du hast Wanzen, L\u00e4use, Fl\u00f6he", "tokens": ["Du", "hast", "Wan\u00b7zen", ",", "L\u00e4u\u00b7se", ",", "Fl\u00f6\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T . . . ., S . . . ., S . . . .", "tokens": ["T", ".", ".", ".", ".", ",", "S", ".", ".", ".", ".", ",", "S", ".", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct", "punct", "punct", "word", "punct", "punct", "punct", "punct", "punct", "word", "punct", "punct", "punct", "punct"], "pos": ["NE", "$.", "$.", "$.", "$.", "$,", "NE", "$.", "$.", "$.", "$.", "$,", "NE", "$.", "$.", "$.", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Haut und Haare ", "tokens": ["Haut", "und", "Haa\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Von der Stirne bis zur Zeh;", "tokens": ["Von", "der", "Stir\u00b7ne", "bis", "zur", "Zeh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich durchschauert schon der Ekel,", "tokens": ["Mich", "durch\u00b7schau\u00b7ert", "schon", "der", "E\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich deinen Schatten seh.", "tokens": ["Wenn", "ich", "dei\u00b7nen", "Schat\u00b7ten", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber wenn wir Nachts uns lausen", "tokens": ["A\u00b7ber", "wenn", "wir", "Nachts", "uns", "lau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Liebe schafft sich Bahn,", "tokens": ["Und", "die", "Lie\u00b7be", "schafft", "sich", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Preis ich mich als deines grausen", "tokens": ["Preis", "ich", "mich", "als", "dei\u00b7nes", "grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reiches treusten Untertan.", "tokens": ["Rei\u00b7ches", "treus\u00b7ten", "Un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}