{"dta.poem.12923": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria.  \n Jm namen eines cavalier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich deine bande k\u00fcssen?", "tokens": ["Soll", "ich", "dei\u00b7ne", "ban\u00b7de", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kleiner Amor! nein, ach nein!", "tokens": ["Klei\u00b7ner", "A\u00b7mor", "!", "nein", ",", "ach", "nein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PTKANT", "$,", "XY", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, ach nein! an meinen f\u00fc\u00dfen", "tokens": ["Nein", ",", "ach", "nein", "!", "an", "mei\u00b7nen", "f\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "XY", "PTKANT", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcssen keine fessel seyn.", "tokens": ["M\u00fcs\u00b7sen", "kei\u00b7ne", "fes\u00b7sel", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll ich deine bande k\u00fcssen?", "tokens": ["Soll", "ich", "dei\u00b7ne", "ban\u00b7de", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kleiner Amor! nein, ach nein!", "tokens": ["Klei\u00b7ner", "A\u00b7mor", "!", "nein", ",", "ach", "nein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PTKANT", "$,", "XY", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sclaven lauffen mit den ketten;", "tokens": ["Scla\u00b7ven", "lauf\u00b7fen", "mit", "den", "ket\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gro\u00dfe seelen herrschen nur:", "tokens": ["Gro\u00b7\u00dfe", "see\u00b7len", "herr\u00b7schen", "nur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den weichen wohllust-betten", "tokens": ["In", "den", "wei\u00b7chen", "wohl\u00b7lust\u00b7bet\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4st die gro\u00dfmuth keine spur.", "tokens": ["L\u00e4st", "die", "gro\u00df\u00b7muth", "kei\u00b7ne", "spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sclaven lauffen mit den ketten;", "tokens": ["Scla\u00b7ven", "lauf\u00b7fen", "mit", "den", "ket\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gro\u00dfe seelen herrschen nur.", "tokens": ["Gro\u00b7\u00dfe", "see\u00b7len", "herr\u00b7schen", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein! doch ja! was? edles k\u00fcssen", "tokens": ["Nein", "!", "doch", "ja", "!", "was", "?", "ed\u00b7les", "k\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "ADV", "$.", "PWS", "$.", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehet auch wol Helden an:", "tokens": ["Ste\u00b7het", "auch", "wol", "Hel\u00b7den", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil man so die zeit vers\u00fc\u00dfen,", "tokens": ["Weil", "man", "so", "die", "zeit", "ver\u00b7s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und doch r\u00fchmlich leben kan.", "tokens": ["Und", "doch", "r\u00fchm\u00b7lich", "le\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein! doch ja! was? edles k\u00fcssen", "tokens": ["Nein", "!", "doch", "ja", "!", "was", "?", "ed\u00b7les", "k\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "ADV", "$.", "PWS", "$.", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stehet auch wol Helden an.", "tokens": ["Ste\u00b7het", "auch", "wol", "Hel\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}