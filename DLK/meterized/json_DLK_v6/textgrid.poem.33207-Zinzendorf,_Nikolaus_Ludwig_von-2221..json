{"textgrid.poem.33207": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2221.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem gesammten Synodo unsrer creuz-geschwister, und dem Seminario zum zeugen-register,", "tokens": ["Dem", "ge\u00b7samm\u00b7ten", "Sy\u00b7no\u00b7do", "uns\u00b7rer", "creuz\u00b7ge\u00b7schwis\u00b7ter", ",", "und", "dem", "Se\u00b7mi\u00b7na\u00b7rio", "zum", "zeu\u00b7gen\u00b7re\u00b7gis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,", "KON", "ART", "NE", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-+-+-+-+--+--+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit der h\u00e4lfte f\u00fchlbarkeit mein's geists darzulegen, denn da w\u00fcrden unsrer leut' eingeweide rege.", "tokens": ["Mit", "der", "h\u00e4lf\u00b7te", "f\u00fchl\u00b7bar\u00b7keit", "mein's", "geists", "dar\u00b7zu\u00b7le\u00b7gen", ",", "denn", "da", "w\u00fcr\u00b7den", "uns\u00b7rer", "leut'", "ein\u00b7ge\u00b7wei\u00b7de", "re\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "PAV", "$,", "KON", "ADV", "VAFIN", "PPOSAT", "VVFIN", "PIS", "VVFIN", "$."], "meter": "--+-+-+-+--+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Niemals hab ich einen Mann unbill'ger gehasset, niemals hat mir iemands plan wen'ger eingepasset.", "tokens": ["Nie\u00b7mals", "hab", "ich", "ei\u00b7nen", "Mann", "un\u00b7bill'\u00b7ger", "ge\u00b7has\u00b7set", ",", "nie\u00b7mals", "hat", "mir", "ie\u00b7mands", "plan", "wen'\u00b7ger", "ein\u00b7ge\u00b7pas\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+----+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "M\u00f6cht ich nur ein kleines theil aller der reellen absichten zu meinem heil, die Er hatt', erzehlen.", "tokens": ["M\u00f6cht", "ich", "nur", "ein", "klei\u00b7nes", "theil", "al\u00b7ler", "der", "re\u00b7el\u00b7len", "ab\u00b7sich\u00b7ten", "zu", "mei\u00b7nem", "heil", ",", "die", "Er", "hatt'", ",", "er\u00b7zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PIAT", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,", "VVINF", "$."], "meter": "+-+-+--+-+-+-+---+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}}}}