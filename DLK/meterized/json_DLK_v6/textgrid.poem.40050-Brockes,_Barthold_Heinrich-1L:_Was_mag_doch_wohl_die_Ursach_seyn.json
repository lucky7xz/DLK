{"textgrid.poem.40050": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was mag doch wohl die Ursach seyn", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mag doch wohl die Ursach seyn", "tokens": ["Was", "mag", "doch", "wohl", "die", "Ur\u00b7sach", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "NN", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Irrthum, der so grob, so allgemein,", "tokens": ["Vom", "Irr\u00b7thum", ",", "der", "so", "grob", ",", "so", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df f\u00fcr die Creatur fast alle Menschen blind,", "tokens": ["Da\u00df", "f\u00fcr", "die", "Crea\u00b7tur", "fast", "al\u00b7le", "Men\u00b7schen", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Geh\u00f6r- Geruch- Geschmack- und F\u00fchl-los sind?", "tokens": ["Ge\u00b7h\u00f6r", "Ge\u00b7ruch", "Ge\u00b7schmack", "und", "F\u00fchl\u00b7los", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "TRUNC", "TRUNC", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da doch die Bibel selbst uns deutlich lehret,", "tokens": ["Da", "doch", "die", "Bi\u00b7bel", "selbst", "uns", "deut\u00b7lich", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie sehr man Gott in Seinen Wercken ehret,", "tokens": ["Wie", "sehr", "man", "Gott", "in", "Sei\u00b7nen", "Wer\u00b7cken", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wie die Creatur, zu ihres Sch\u00f6pfers Preise,", "tokens": ["Und", "wie", "die", "Crea\u00b7tur", ",", "zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Den grossen Sch\u00f6pfer selber weise.", "tokens": ["Den", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", "sel\u00b7ber", "wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Giebt uns Sanct Paulus die\u00df nicht deutlich gnug zu lesen?", "tokens": ["Giebt", "uns", "Sanct", "Pau\u00b7lus", "die\u00df", "nicht", "deut\u00b7lich", "gnug", "zu", "le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "NE", "PDS", "PTKNEG", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er saget: Da\u00df man weis, da\u00df GOTT sey, ist ja klar,", "tokens": ["Er", "sa\u00b7get", ":", "Da\u00df", "man", "weis", ",", "da\u00df", "GoTT", "sey", ",", "ist", "ja", "klar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PIS", "PTKVZ", "$,", "KOUS", "NE", "VAFIN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und allen Menschen offenbar.", "tokens": ["Und", "al\u00b7len", "Men\u00b7schen", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "GoTT offenbahrt' es selbst, und gab es zu verstehn,", "tokens": ["GoTT", "of\u00b7fen\u00b7bahrt'", "es", "selbst", ",", "und", "gab", "es", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df GOTTES unsichtbares Wesen,", "tokens": ["Da\u00df", "GoT\u00b7TES", "un\u00b7sicht\u00b7ba\u00b7res", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das ist, Sein' ew'ge Kraft und Gottheit wird ersehn,", "tokens": ["Das", "ist", ",", "Sein'", "ew'\u00b7ge", "Kraft", "und", "Got\u00b7theit", "wird", "er\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "KON", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So man die\u00df wahrnimmt an den Wercken,", "tokens": ["So", "man", "die\u00df", "wahr\u00b7nimmt", "an", "den", "Wer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wie von der Welt Erschaffung an zu mercken,", "tokens": ["Wie", "von", "der", "Welt", "Er\u00b7schaf\u00b7fung", "an", "zu", "mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "So da\u00df sie keinen Grund, sich zu entschuld'gen, haben.", "tokens": ["So", "da\u00df", "sie", "kei\u00b7nen", "Grund", ",", "sich", "zu", "ent\u00b7schuld'\u00b7gen", ",", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PIAT", "NN", "$,", "PRF", "PTKZU", "VVINF", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch halt, mit f\u00e4llt ein' Ursach bey,", "tokens": ["Doch", "halt", ",", "mit", "f\u00e4llt", "ein'", "Ur\u00b7sach", "bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wovon ich \u00fcberf\u00fchret,", "tokens": ["Wo\u00b7von", "ich", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie gewi\u00df der kleinsten keine sey:", "tokens": ["Da\u00df", "sie", "ge\u00b7wi\u00df", "der", "kleins\u00b7ten", "kei\u00b7ne", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "PIAT", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da nehmlich alle Pracht von unsers Sch\u00f6pfers Gaben", "tokens": ["Da", "nehm\u00b7lich", "al\u00b7le", "Pracht", "von", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch fromme Seelen selbst so wenig r\u00fchret,", "tokens": ["Auch", "from\u00b7me", "See\u00b7len", "selbst", "so", "we\u00b7nig", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So wenig reitzt und lockt; weil ich bemercke,", "tokens": ["So", "we\u00b7nig", "reitzt", "und", "lockt", ";", "weil", "ich", "be\u00b7mer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "KON", "VVFIN", "$.", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df GOTTES und des Teufels Wercke", "tokens": ["Da\u00df", "GoT\u00b7TES", "und", "des", "Teu\u00b7fels", "Wer\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "KON", "ART", "ADJA", "NN"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Im Worte Welt nur einen Namen haben.", "tokens": ["Im", "Wor\u00b7te", "Welt", "nur", "ei\u00b7nen", "Na\u00b7men", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Man heisset Welt, was gottlos, lasterhaft,", "tokens": ["Man", "heis\u00b7set", "Welt", ",", "was", "gott\u00b7los", ",", "las\u00b7ter\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "$,", "PRELS", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Was b\u00f6s und eitel ist. Von unsrer Leidenschaft", "tokens": ["Was", "b\u00f6s", "und", "ei\u00b7tel", "ist", ".", "Von", "uns\u00b7rer", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Misbrauch, Hochmuth, Neid, die Wollust, Schm\u00e4hsucht, Geld", "tokens": ["Der", "Mis\u00b7brauch", ",", "Hoch\u00b7muth", ",", "Neid", ",", "die", "Wol\u00b7lust", ",", "Schm\u00e4h\u00b7sucht", ",", "Geld"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und Ungerechtigkeit heisst Weltlich, nennt man Welt.", "tokens": ["Und", "Un\u00b7ge\u00b7rech\u00b7tig\u00b7keit", "heisst", "Welt\u00b7lich", ",", "nennt", "man", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "$,", "VVFIN", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So bald man nun die Welt, das herrliche Gef\u00e4sse", "tokens": ["So", "bald", "man", "nun", "die", "Welt", ",", "das", "herr\u00b7li\u00b7che", "Ge\u00b7f\u00e4s\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der sch\u00f6nen Creatur, die unsers Sch\u00f6pfers Gr\u00f6sse", "tokens": ["Der", "sch\u00f6\u00b7nen", "Crea\u00b7tur", ",", "die", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Gr\u00f6s\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Und Weisheit, Lieb' und Macht uns recht mit Fingern zeigt,", "tokens": ["Und", "Weis\u00b7heit", ",", "Lieb'", "und", "Macht", "uns", "recht", "mit", "Fin\u00b7gern", "zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mit ihrem Namen nennt;", "tokens": ["Mit", "ih\u00b7rem", "Na\u00b7men", "nennt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Wird leider auch so gar von Frommen", "tokens": ["Wird", "lei\u00b7der", "auch", "so", "gar", "von", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Das eine f\u00fcr das andere genommen.", "tokens": ["Das", "ei\u00b7ne", "f\u00fcr", "das", "an\u00b7de\u00b7re", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "APPR", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der ungl\u00fcckseel'ge Gleich-Laut macht,", "tokens": ["Der", "un\u00b7gl\u00fcck\u00b7seel'\u00b7ge", "Gleich\u00b7Laut", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.20": {"text": "Da\u00df, da man ohne die\u00df gewohnt, nicht drauf zu achten,", "tokens": ["Da\u00df", ",", "da", "man", "oh\u00b7ne", "die\u00df", "ge\u00b7wohnt", ",", "nicht", "drauf", "zu", "ach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PIS", "APPR", "PDS", "VVPP", "$,", "PTKNEG", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Man so verf\u00e4hrt mit der Gesch\u00f6pfe Pracht,", "tokens": ["Man", "so", "ver\u00b7f\u00e4hrt", "mit", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Als w\u00e4r' es S\u00fcnde, sie betrachten.", "tokens": ["Als", "w\u00e4r'", "es", "S\u00fcn\u00b7de", ",", "sie", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die Heyden machten es so arg noch lange nicht,", "tokens": ["Die", "Hey\u00b7den", "mach\u00b7ten", "es", "so", "arg", "noch", "lan\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wovon das Weisheit-Buch recht unvergleichlich spricht:", "tokens": ["Wo\u00b7von", "das", "Weis\u00b7heit\u00b7Buch", "recht", "un\u00b7ver\u00b7gleich\u00b7lich", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nat\u00fcrlich eitel ist zwar jedes Menschen-Kind,", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "ei\u00b7tel", "ist", "zwar", "je\u00b7des", "Men\u00b7schen\u00b7Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil alle nichts von GOTT verstehen,", "tokens": ["Weil", "al\u00b7le", "nichts", "von", "GoTT", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PIS", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und an der G\u00fcter Zahl, die sichtbar sind,", "tokens": ["Und", "an", "der", "G\u00fc\u00b7ter", "Zahl", ",", "die", "sicht\u00b7bar", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Den, Der es ist, nicht kennen. Sie ersehen", "tokens": ["Den", ",", "Der", "es", "ist", ",", "nicht", "ken\u00b7nen", ".", "Sie", "er\u00b7se\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "PPER", "VAFIN", "$,", "PTKNEG", "VVINF", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "An allen sch\u00f6nen Wercken nicht", "tokens": ["An", "al\u00b7len", "sch\u00f6\u00b7nen", "Wer\u00b7cken", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Den Meister, der sie zugericht't.", "tokens": ["Den", "Meis\u00b7ter", ",", "der", "sie", "zu\u00b7ge\u00b7richt'", "t."], "token_info": ["word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Theils halten sie die Gluht,", "tokens": ["Theils", "hal\u00b7ten", "sie", "die", "Gluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Theils schnelle Luft, theils m\u00e4cht'ge Fluth,", "tokens": ["Theils", "schnel\u00b7le", "Luft", ",", "theils", "m\u00e4cht'\u00b7ge", "Fluth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Theils Lichter, die den Himmel zieren,", "tokens": ["Theils", "Lich\u00b7ter", ",", "die", "den", "Him\u00b7mel", "zie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "F\u00fcr G\u00f6tter, so die Welt regieren.", "tokens": ["F\u00fcr", "G\u00f6t\u00b7ter", ",", "so", "die", "Welt", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Allein, da sie von ihrer Zier", "tokens": ["Al\u00b7lein", ",", "da", "sie", "von", "ih\u00b7rer", "Zier"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und lieblichen Gestalt so viel Vergn\u00fcgen f\u00fchlten,", "tokens": ["Und", "lieb\u00b7li\u00b7chen", "Ge\u00b7stalt", "so", "viel", "Ver\u00b7gn\u00fc\u00b7gen", "f\u00fchl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und sie also f\u00fcr G\u00f6tter hielten:", "tokens": ["Und", "sie", "al\u00b7so", "f\u00fcr", "G\u00f6t\u00b7ter", "hiel\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "So h\u00e4tten sie ja billig m\u00fcssen,", "tokens": ["So", "h\u00e4t\u00b7ten", "sie", "ja", "bil\u00b7lig", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Wie gar viel besser Der, der aller Herr ist, wissen.", "tokens": ["Wie", "gar", "viel", "bes\u00b7ser", "Der", ",", "der", "al\u00b7ler", "Herr", "ist", ",", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "ART", "$,", "PRELS", "PIAT", "NN", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Denn Der, so Meister ist von aller Sch\u00f6nheit-Pracht,", "tokens": ["Denn", "Der", ",", "so", "Meis\u00b7ter", "ist", "von", "al\u00b7ler", "Sch\u00f6n\u00b7heit\u00b7Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "NN", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Hat solches alles ja gemacht,", "tokens": ["Hat", "sol\u00b7ches", "al\u00b7les", "ja", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Und so sie sich der Macht und Kraft", "tokens": ["Und", "so", "sie", "sich", "der", "Macht", "und", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "PRF", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Verwunderten: So sollten sie", "tokens": ["Ver\u00b7wun\u00b7der\u00b7ten", ":", "So", "soll\u00b7ten", "sie"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VMFIN", "PPER"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.44": {"text": "Ja billig auch die Eigenschaft,", "tokens": ["Ja", "bil\u00b7lig", "auch", "die", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Und wie viel m\u00e4chtiger Der sey, der alle Gaben", "tokens": ["Und", "wie", "viel", "m\u00e4ch\u00b7ti\u00b7ger", "Der", "sey", ",", "der", "al\u00b7le", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PIAT", "ADJA", "PDS", "VAFIN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Bereitet hat, gemercket haben.", "tokens": ["Be\u00b7rei\u00b7tet", "hat", ",", "ge\u00b7mer\u00b7cket", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Denn es kann am Gesch\u00f6pf und Schmuck der Erden,", "tokens": ["Denn", "es", "kann", "am", "Ge\u00b7sch\u00f6pf", "und", "Schmuck", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPRART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.48": {"text": "Ihr Sch\u00f6pfer, als im Bild', erkennet werden.", "tokens": ["Ihr", "Sch\u00f6p\u00b7fer", ",", "als", "im", "Bild'", ",", "er\u00b7ken\u00b7net", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "APPRART", "NN", "$,", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Wiewohl doch \u00fcber die", "tokens": ["Wie\u00b7wohl", "doch", "\u00fc\u00b7ber", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.50": {"text": "Nicht so gar hoch zu klagen,", "tokens": ["Nicht", "so", "gar", "hoch", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Indem auch sie", "tokens": ["In\u00b7dem", "auch", "sie"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.52": {"text": "Wohl irren k\u00f6nnen, wenn sie hie", "tokens": ["Wohl", "ir\u00b7ren", "k\u00f6n\u00b7nen", ",", "wenn", "sie", "hie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "VMINF", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "GoTT suchen, und nach Ihm Verlangen tragen.", "tokens": ["GoTT", "su\u00b7chen", ",", "und", "nach", "Ihm", "Ver\u00b7lan\u00b7gen", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "KON", "APPR", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.54": {"text": "Denn so sie ihren Geist auf die Gesch\u00f6pfe lencken,", "tokens": ["Denn", "so", "sie", "ih\u00b7ren", "Geist", "auf", "die", "Ge\u00b7sch\u00f6p\u00b7fe", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Um ihnen nachzudencken:", "tokens": ["Um", "ih\u00b7nen", "nach\u00b7zu\u00b7den\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "So werden sie im Ansehn ihrer Pracht", "tokens": ["So", "wer\u00b7den", "sie", "im", "An\u00b7sehn", "ih\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.57": {"text": "Gefangen, weil nur gar zu sch\u00f6n", "tokens": ["Ge\u00b7fan\u00b7gen", ",", "weil", "nur", "gar", "zu", "sch\u00f6n"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADV", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Die Creaturen, die wir sehn.", "tokens": ["Die", "Crea\u00b7tu\u00b7ren", ",", "die", "wir", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Doch sind sie damit nicht entschuldigt. Denn da sie", "tokens": ["Doch", "sind", "sie", "da\u00b7mit", "nicht", "ent\u00b7schul\u00b7digt", ".", "Denn", "da", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PAV", "PTKNEG", "VVPP", "$.", "KON", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "So viel erkennen, da\u00df sie hie", "tokens": ["So", "viel", "er\u00b7ken\u00b7nen", ",", "da\u00df", "sie", "hie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Die Creatur zu achten, sind verbunden:", "tokens": ["Die", "Crea\u00b7tur", "zu", "ach\u00b7ten", ",", "sind", "ver\u00b7bun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.62": {"text": "Warum denn haben sie nicht noch viel eh", "tokens": ["Wa\u00b7rum", "denn", "ha\u00b7ben", "sie", "nicht", "noch", "viel", "eh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.63": {"text": "Den Herrn derselbigen gefunden?", "tokens": ["Den", "Herrn", "der\u00b7sel\u00b7bi\u00b7gen", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Heyden triebens ohne Massen", "tokens": ["Die", "Hey\u00b7den", "trie\u00b7bens", "oh\u00b7ne", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit sichtbaren Gesch\u00f6pfen und vergassen", "tokens": ["Mit", "sicht\u00b7ba\u00b7ren", "Ge\u00b7sch\u00f6p\u00b7fen", "und", "ver\u00b7gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Sch\u00f6pfers, der unsichtbar, gantz.", "tokens": ["Des", "Sch\u00f6p\u00b7fers", ",", "der", "un\u00b7sicht\u00b7bar", ",", "gantz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wir aber leider!", "tokens": ["Wir", "a\u00b7ber", "lei\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Vergessen aller beyder;", "tokens": ["Ver\u00b7ges\u00b7sen", "al\u00b7ler", "bey\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sind dahero von den Heyden", "tokens": ["Und", "sind", "da\u00b7he\u00b7ro", "von", "den", "Hey\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PAV", "APPR", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Gar wohl zu unterscheiden.", "tokens": ["Gar", "wohl", "zu", "un\u00b7ter\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Abg\u00f6tter waren sie: Hingegen viele Christen", "tokens": ["Ab\u00b7g\u00f6t\u00b7ter", "wa\u00b7ren", "sie", ":", "Hin\u00b7ge\u00b7gen", "vie\u00b7le", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "ADV", "PIAT", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Sind, durch der Creatur Verachtung, Atheisten.", "tokens": ["Sind", ",", "durch", "der", "Crea\u00b7tur", "Ver\u00b7ach\u00b7tung", ",", "A\u00b7theis\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ART", "NN", "NN", "$,", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}