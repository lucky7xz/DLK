{"dta.poem.14531": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Umkehrende .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Waldeinsamkeit!", "tokens": ["Wal\u00b7de\u00b7in\u00b7sam\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Du gr\u00fcnes Revier,", "tokens": ["Du", "gr\u00fc\u00b7nes", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie liegt so weit", "tokens": ["Wie", "liegt", "so", "weit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die Welt von hier!", "tokens": ["Die", "Welt", "von", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Schlaf' nur, wie bald", "tokens": ["Schlaf'", "nur", ",", "wie", "bald"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Kommt der Abend sch\u00f6n,", "tokens": ["Kommt", "der", "A\u00b7bend", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Durch den stillen Wald", "tokens": ["Durch", "den", "stil\u00b7len", "Wald"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Die Quellen gehn,", "tokens": ["Die", "Quel\u00b7len", "gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die Mutter Gottes wacht,", "tokens": ["Die", "Mut\u00b7ter", "Got\u00b7tes", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mit ihrem Sternen-Kleid", "tokens": ["Mit", "ih\u00b7rem", "Ster\u00b7nen\u00b7Kleid"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Bedeckt sie Dich sacht", "tokens": ["Be\u00b7deckt", "sie", "Dich", "sacht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "In der Waldeinsamkeit,", "tokens": ["In", "der", "Wal\u00b7de\u00b7in\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Gute Nacht, gute Nacht! \u2014", "tokens": ["Gu\u00b7te", "Nacht", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}