{"dta.poem.2122": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einige Gedanken \u00fcber Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, was war doch alles die\u00df, im Gegenhalt der", "tokens": ["Al\u00b7lein", ",", "was", "war", "doch", "al\u00b7les", "die\u00df", ",", "im", "Ge\u00b7gen\u00b7halt", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "VAFIN", "ADV", "PIS", "PDS", "$,", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "bunten Glut,", "tokens": ["bun\u00b7ten", "Glut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die mich als wie ein Blitz durchstralte, und mein da-", "tokens": ["Die", "mich", "als", "wie", "ein", "Blitz", "durch\u00b7stral\u00b7te", ",", "und", "mein", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "KOUS", "KOKOM", "ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "durch erregtes Blut", "tokens": ["durch", "er\u00b7reg\u00b7tes", "Blut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Vor schneller Freude wallen machte! Von meinen Kin-", "tokens": ["Vor", "schnel\u00b7ler", "Freu\u00b7de", "wal\u00b7len", "mach\u00b7te", "!", "Von", "mei\u00b7nen", "Kin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VVFIN", "$.", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "dern hatten zwey,", "tokens": ["dern", "hat\u00b7ten", "zwey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "CARD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und zwar die j\u00fcngsten, einen Tisch, mich unvermuthet", "tokens": ["Und", "zwar", "die", "j\u00fcng\u00b7sten", ",", "ei\u00b7nen", "Tisch", ",", "mich", "un\u00b7ver\u00b7mu\u00b7thet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "$,", "ART", "NN", "$,", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zu vergn\u00fcgen,", "tokens": ["zu", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Mit frischen Rosen ganz bedeckt. Mein Gott, wie viel-", "tokens": ["Mit", "fri\u00b7schen", "Ro\u00b7sen", "ganz", "be\u00b7deckt", ".", "Mein", "Gott", ",", "wie", "viel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "$.", "PPOSAT", "NN", "$,", "PWAV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wie mancherley", "tokens": ["wie", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word"], "pos": ["PWAV", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Ist dieser Blumen r\u00f6thlichs Brennen! Hier sieht man", "tokens": ["Ist", "die\u00b7ser", "Blu\u00b7men", "r\u00f6th\u00b7lichs", "Bren\u00b7nen", "!", "Hier", "sieht", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "ADJA", "NN", "$.", "ADV", "VVFIN", "PIS"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wei\u00df und Noth sich f\u00fcgen", "tokens": ["Wei\u00df", "und", "Noth", "sich", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "PRF", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "In solcher lieblichen Vermischung! Wie gl\u00e4nzet, funkelt,", "tokens": ["In", "sol\u00b7cher", "lieb\u00b7li\u00b7chen", "Ver\u00b7mi\u00b7schung", "!", "Wie", "gl\u00e4n\u00b7zet", ",", "fun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$.", "PWAV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "gl\u00fchet, scheint", "tokens": ["gl\u00fc\u00b7het", ",", "scheint"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Die sonst vertheilte Pracht des Gartens hier auf so", "tokens": ["Die", "sonst", "ver\u00b7theil\u00b7te", "Pracht", "des", "Gar\u00b7tens", "hier", "auf", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "ART", "NN", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.16": {"text": "engem Platz vereint!", "tokens": ["en\u00b7gem", "Platz", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Was ich in dieser innern Schoo\u00df, was ich in jener", "tokens": ["Was", "ich", "in", "die\u00b7ser", "in\u00b7nern", "Schoo\u00df", ",", "was", "ich", "in", "je\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,", "PWS", "PPER", "APPR", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u00e4u\u00dfern R\u00fcnde", "tokens": ["\u00e4u\u00b7\u00dfern", "R\u00fcn\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "F\u00fcr eine volle dunkle R\u00f6the, f\u00fcr eine rothe Wei\u00dfe finde,", "tokens": ["F\u00fcr", "ei\u00b7ne", "vol\u00b7le", "dunk\u00b7le", "R\u00f6\u00b7the", ",", "f\u00fcr", "ei\u00b7ne", "ro\u00b7the", "Wei\u00b7\u00dfe", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.20": {"text": "Ist unbeschreiblich, unausdr\u00fccklich! Um alles mehr noch", "tokens": ["Ist", "un\u00b7be\u00b7schreib\u00b7lich", ",", "un\u00b7aus\u00b7dr\u00fcck\u00b7lich", "!", "Um", "al\u00b7les", "mehr", "noch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "$.", "APPR", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "zu erh\u00f6hn,", "tokens": ["zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Und da\u00df sie durch die Nachbarschaft noch sch\u00f6ner w\u00e4ren", "tokens": ["Und", "da\u00df", "sie", "durch", "die", "Nach\u00b7bar\u00b7schaft", "noch", "sch\u00f6\u00b7ner", "w\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "anzusehn,", "tokens": ["an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "punct"], "pos": ["VVIZU", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "So hatten sie den Tisch vorher mit dunklem Weinlaub", "tokens": ["So", "hat\u00b7ten", "sie", "den", "Tisch", "vor\u00b7her", "mit", "dunk\u00b7lem", "Wein\u00b7laub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u00fcberdecket:", "tokens": ["\u00fc\u00b7berd\u00b7e\u00b7cket", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.26": {"text": "Durch welcher sch\u00f6nen Fulge denn ihr Glanz sich weiter", "tokens": ["Durch", "wel\u00b7cher", "sch\u00f6\u00b7nen", "Ful\u00b7ge", "denn", "ihr", "Glanz", "sich", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "noch erstrecket.", "tokens": ["noch", "er\u00b7stre\u00b7cket", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Wobey ich auch noch hie und da", "tokens": ["Wo\u00b7bey", "ich", "auch", "noch", "hie", "und", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Den gelben Brand der Ringelblume, die bl\u00e4uliche Cam-", "tokens": ["Den", "gel\u00b7ben", "Brand", "der", "Rin\u00b7gel\u00b7blu\u00b7me", ",", "die", "bl\u00e4u\u00b7li\u00b7che", "Cam"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "panula,", "tokens": ["pa\u00b7nu\u00b7la", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "---", "measure": "unknown.measure.zero"}, "line.31": {"text": "Den gl\u00fcenden Nasturtium, nebst wei\u00dfen Rosen liegen", "tokens": ["Den", "gl\u00fcen\u00b7den", "Nas\u00b7tur\u00b7ti\u00b7um", ",", "nebst", "wei\u00b7\u00dfen", "Ro\u00b7sen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "sah,", "tokens": ["sah", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}}}}}