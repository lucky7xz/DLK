{"textgrid.poem.49680": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Nach den Wahlen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schreit nicht mehr in fetten Schriften", "tokens": ["Es", "schreit", "nicht", "mehr", "in", "fet\u00b7ten", "Schrif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das F\u00fcr und Wider von der Wand.", "tokens": ["Das", "F\u00fcr", "und", "Wi\u00b7der", "von", "der", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00dft uns alle Frieden stiften!", "tokens": ["So", "la\u00dft", "uns", "al\u00b7le", "Frie\u00b7den", "stif\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder reiche seine Hand!", "tokens": ["Ein", "je\u00b7der", "rei\u00b7che", "sei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zur Menschheit wird auf diesem Wege", "tokens": ["Zur", "Menschheit", "wird", "auf", "die\u00b7sem", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die hei\u00dfentflammte W\u00e4hlerschar;", "tokens": ["Die", "hei\u00b7\u00dfent\u00b7flamm\u00b7te", "W\u00e4h\u00b7ler\u00b7schar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wieder Nachbar und Kollege", "tokens": ["Und", "wie\u00b7der", "Nach\u00b7bar", "und", "Kol\u00b7le\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist, wer noch gestern Schurke war.", "tokens": ["Ist", ",", "wer", "noch", "ge\u00b7stern", "Schur\u00b7ke", "war", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}