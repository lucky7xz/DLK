{"textgrid.poem.38724": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus Wolken winken H\u00e4nde,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Wolken winken H\u00e4nde,", "tokens": ["Aus", "Wol\u00b7ken", "win\u00b7ken", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An jedem Finger rothe Rosen,", "tokens": ["An", "je\u00b7dem", "Fin\u00b7ger", "ro\u00b7the", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie winken dir mit schmeichlerischem Kosen,", "tokens": ["Sie", "win\u00b7ken", "dir", "mit", "schmeich\u00b7le\u00b7ri\u00b7schem", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du stehst und fragst: wohin der Weg sich wende?", "tokens": ["Du", "stehst", "und", "fragst", ":", "wo\u00b7hin", "der", "Weg", "sich", "wen\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da singen alle Fr\u00fchlingsl\u00fcfte,", "tokens": ["Da", "sin\u00b7gen", "al\u00b7le", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da duften und klingen die Blumend\u00fcfte,", "tokens": ["Da", "duf\u00b7ten", "und", "klin\u00b7gen", "die", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Lieblich Rauschen geht das Thal entlang:", "tokens": ["Lieb\u00b7lich", "Rau\u00b7schen", "geht", "das", "Thal", "ent\u00b7lang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbsei muthig, nicht bang!", "tokens": ["\u00bb", "sei", "mut\u00b7hig", ",", "nicht", "bang", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$,", "PTKNEG", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Siehst du des Mondes Schimmer,", "tokens": ["Siehst", "du", "des", "Mon\u00b7des", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Quellen h\u00fcpfendes Geflimmer?", "tokens": ["Der", "Quel\u00b7len", "h\u00fcp\u00b7fen\u00b7des", "Ge\u00b7flim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Wolken hoch die goldnen H\u00fcgel,", "tokens": ["In", "Wol\u00b7ken", "hoch", "die", "gold\u00b7nen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Morgenr\u00f6the himmelbreite Fl\u00fcgel?", "tokens": ["Der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "him\u00b7mel\u00b7brei\u00b7te", "Fl\u00fc\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dir entgegen ziehn so Gl\u00fcck als Liebe,", "tokens": ["Dir", "ent\u00b7ge\u00b7gen", "ziehn", "so", "Gl\u00fcck", "als", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VVFIN", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dich als Beute mit goldnen Netzen zu fahn,", "tokens": ["Dich", "als", "Beu\u00b7te", "mit", "gold\u00b7nen", "Net\u00b7zen", "zu", "fahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "So leise lieblich, da\u00df keine Ausflucht bliebe", "tokens": ["So", "lei\u00b7se", "lieb\u00b7lich", ",", "da\u00df", "kei\u00b7ne", "Aus\u00b7flucht", "blie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "$,", "KOUS", "PIAT", "NN", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Umzingeln sie dich, bald ist's um dich gethan.\u00ab", "tokens": ["Um\u00b7zin\u00b7geln", "sie", "dich", ",", "bald", "ist's", "um", "dich", "ge\u00b7than", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "\u2013 Was will das Gl\u00fcck mit mir beginnen?", "tokens": ["\u2013", "Was", "will", "das", "Gl\u00fcck", "mit", "mir", "be\u00b7gin\u00b7nen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Fr\u00fchlingsnachtigall, singst du drein?", "tokens": ["O", "Fr\u00fch\u00b7lings\u00b7nach\u00b7ti\u00b7gall", ",", "singst", "du", "drein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schon dringt die sehnende Lieb' auf mich ein,", "tokens": ["Schon", "dringt", "die", "seh\u00b7nen\u00b7de", "Lieb'", "auf", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie Mondglanz webt's um meine Sinnen. \u2013", "tokens": ["Wie", "Mond\u00b7glanz", "webt's", "um", "mei\u00b7ne", "Sin\u00b7nen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie bang' ist mir's, gefangen mich zu geben,", "tokens": ["Wie", "bang'", "ist", "mir's", ",", "ge\u00b7fan\u00b7gen", "mich", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NE", "$,", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie nah'n, die Schaaren der Wonne mit Heeresmacht!", "tokens": ["Sie", "nah'n", ",", "die", "Schaa\u00b7ren", "der", "Won\u00b7ne", "mit", "Hee\u00b7res\u00b7macht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verloren, vertr\u00e4umt ist das fliehende Leben,", "tokens": ["Ver\u00b7lo\u00b7ren", ",", "ver\u00b7tr\u00e4umt", "ist", "das", "flie\u00b7hen\u00b7de", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Schon r\u00fcstet sich Lieb' und Gl\u00fcck zur Schlacht.", "tokens": ["Schon", "r\u00fcs\u00b7tet", "sich", "Lieb'", "und", "Gl\u00fcck", "zur", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Kampf ist begonnen,", "tokens": ["Der", "Kampf", "ist", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich f\u00fchle die Wonnen", "tokens": ["Ich", "f\u00fch\u00b7le", "die", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Durchstr\u00f6men die Brust:", "tokens": ["Durch\u00b7str\u00f6\u00b7men", "die", "Brust", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "O, seel'ge Gefilde,", "tokens": ["O", ",", "seel'\u00b7ge", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich komme, wie milde", "tokens": ["Ich", "kom\u00b7me", ",", "wie", "mil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Erquickt und ermattet des Lebens Lust.", "tokens": ["Er\u00b7quickt", "und", "er\u00b7mat\u00b7tet", "des", "Le\u00b7bens", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Es winket vom Himmel", "tokens": ["Es", "win\u00b7ket", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Freuden Gewimmel,", "tokens": ["Der", "Freu\u00b7den", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und lagert sich hier:", "tokens": ["Und", "la\u00b7gert", "sich", "hier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Im Boden, ich f\u00fchle", "tokens": ["Im", "Bo\u00b7den", ",", "ich", "f\u00fch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der Freuden Gew\u00fchle,", "tokens": ["Der", "Freu\u00b7den", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie streben und dr\u00e4ngen entgegen mir.", "tokens": ["Sie", "stre\u00b7ben", "und", "dr\u00e4n\u00b7gen", "ent\u00b7ge\u00b7gen", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Der Quellen Get\u00f6ne,", "tokens": ["Der", "Quel\u00b7len", "Ge\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Bl\u00fcmelein Sch\u00f6ne,", "tokens": ["Der", "Bl\u00fc\u00b7me\u00b7lein", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ihr lieblicher Blick,", "tokens": ["Ihr", "lieb\u00b7li\u00b7cher", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Sie winken so eigen,", "tokens": ["Sie", "win\u00b7ken", "so", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich deute das Schweigen:", "tokens": ["Ich", "deu\u00b7te", "das", "Schwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie w\u00fcnschen mir alle zum Leben Gl\u00fcck. \u2013", "tokens": ["Sie", "w\u00fcn\u00b7schen", "mir", "al\u00b7le", "zum", "Le\u00b7ben", "Gl\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPRART", "NN", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "Nun geht das Kind auf gr\u00fcnen Wegen,", "tokens": ["Nun", "geht", "das", "Kind", "auf", "gr\u00fc\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den goldgl\u00e4nzenden Strahlen entgegen,", "tokens": ["Den", "gold\u00b7gl\u00e4n\u00b7zen\u00b7den", "Strah\u00b7len", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Im bangen Harren geht es weit,", "tokens": ["Im", "ban\u00b7gen", "Har\u00b7ren", "geht", "es", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es klopft das Herz, es flieht die Zeit.", "tokens": ["Es", "klopft", "das", "Herz", ",", "es", "flieht", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Es ist, als wenn die Quellen schwiegen,", "tokens": ["Es", "ist", ",", "als", "wenn", "die", "Quel\u00b7len", "schwie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm d\u00fcnkt, als dunkle Schatten stiegen,", "tokens": ["Ihm", "d\u00fcnkt", ",", "als", "dunk\u00b7le", "Schat\u00b7ten", "stie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00f6schten des Waldes gr\u00fcne Flammen,", "tokens": ["Und", "l\u00f6schten", "des", "Wal\u00b7des", "gr\u00fc\u00b7ne", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es falten die Blumen den Putz zusammen.", "tokens": ["Es", "fal\u00b7ten", "die", "Blu\u00b7men", "den", "Putz", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.12": {"line.1": {"text": "Die freundlichen Bl\u00fcthen sind nun fort,", "tokens": ["Die", "freund\u00b7li\u00b7chen", "Bl\u00fc\u00b7then", "sind", "nun", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Fr\u00fcchte stehn an selbigem Ort.", "tokens": ["Und", "Fr\u00fcch\u00b7te", "stehn", "an", "sel\u00b7bi\u00b7gem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Nachtigall versteckt die Ges\u00e4nge im Wald,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "ver\u00b7steckt", "die", "Ge\u00b7s\u00e4n\u00b7ge", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nur Echo durch die Einsamkeit schallt.", "tokens": ["Nur", "E\u00b7cho", "durch", "die", "Ein\u00b7sam\u00b7keit", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.13": {"line.1": {"text": "\u00bbmorgenr\u00f6the, bist du nach Haus gegangen?", "tokens": ["\u00bb", "mor\u00b7gen\u00b7r\u00f6\u00b7the", ",", "bist", "du", "nach", "Haus", "ge\u00b7gan\u00b7gen", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ruft das Kind, und streckt die H\u00e4nd' und weint;", "tokens": ["Ruft", "das", "Kind", ",", "und", "streckt", "die", "H\u00e4nd'", "und", "weint", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O komm', ich bin erl\u00f6s't vom Bangen,", "tokens": ["O", "komm'", ",", "ich", "bin", "er\u00b7l\u00f6s't", "vom", "Ban\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du wolltest mich mit goldnen Netzen fangen,", "tokens": ["Du", "woll\u00b7test", "mich", "mit", "gold\u00b7nen", "Net\u00b7zen", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du hast es gewi\u00df nicht b\u00f6se gemeint.", "tokens": ["Du", "hast", "es", "ge\u00b7wi\u00df", "nicht", "b\u00f6\u00b7se", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Ich will mich gerne drein ergeben,", "tokens": ["Ich", "will", "mich", "ger\u00b7ne", "drein", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es kann und soll nicht anders seyn:", "tokens": ["Es", "kann", "und", "soll", "nicht", "an\u00b7ders", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich opfre dir mein junges Leben,", "tokens": ["Ich", "opf\u00b7re", "dir", "mein", "jun\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O! komm' zur\u00fcck, du Himmelsschein!\u00ab", "tokens": ["O", "!", "komm'", "zu\u00b7r\u00fcck", ",", "du", "Him\u00b7mels\u00b7schein", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "VVFIN", "PTKVZ", "$,", "PPER", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Aber hoch und h\u00f6her steigt das Licht,", "tokens": ["A\u00b7ber", "hoch", "und", "h\u00f6\u00b7her", "steigt", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und bescheint das thr\u00e4nende Gesicht;", "tokens": ["Und", "be\u00b7scheint", "das", "thr\u00e4\u00b7nen\u00b7de", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die Nachtigall flieht waldw\u00e4rts weiter,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "flieht", "wald\u00b7w\u00e4rts", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Quell wird zum Flu\u00df und immer breiter.", "tokens": ["Quell", "wird", "zum", "Flu\u00df", "und", "im\u00b7mer", "brei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "KON", "ADV", "ADJD", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.16": {"line.1": {"text": "\u00bbach, und ich kann nicht hin\u00fcberfliegen!", "tokens": ["\u00bb", "ach", ",", "und", "ich", "kann", "nicht", "hin\u00b7\u00fc\u00b7berf\u00b7lie\u00b7gen", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "KON", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was mich erst lockte, ist nun so weit,", "tokens": ["Was", "mich", "erst", "lock\u00b7te", ",", "ist", "nun", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Morgenglanz, die T\u00f6ne m\u00fcssen jenseits liegen,", "tokens": ["Der", "Mor\u00b7gen\u00b7glanz", ",", "die", "T\u00f6\u00b7ne", "m\u00fcs\u00b7sen", "jen\u00b7seits", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich stehe hier, und f\u00fchle nur mein Leid.\u00ab", "tokens": ["Ich", "ste\u00b7he", "hier", ",", "und", "f\u00fch\u00b7le", "nur", "mein", "Leid", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "\u2013 Die Nachtigall singet aus weiter Fern':", "tokens": ["\u2013", "Die", "Nach\u00b7ti\u00b7gall", "sin\u00b7get", "aus", "wei\u00b7ter", "Fern'", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbwir locken, damit du lebest gern,", "tokens": ["\u00bb", "wir", "lo\u00b7cken", ",", "da\u00b7mit", "du", "le\u00b7best", "gern", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df du dich nach uns sehnst, und immer matter sehnst,", "tokens": ["Da\u00df", "du", "dich", "nach", "uns", "sehnst", ",", "und", "im\u00b7mer", "mat\u00b7ter", "sehnst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "ADV", "$,", "KON", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist, was du th\u00f6richt dein Leben w\u00e4hnst.\u00ab \u2013", "tokens": ["Ist", ",", "was", "du", "th\u00f6\u00b7richt", "dein", "Le\u00b7ben", "w\u00e4hnst", ".", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "$,", "PWS", "PPER", "VVFIN", "PPOSAT", "NN", "VAFIN", "$.", "$(", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}