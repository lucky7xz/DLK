{"textgrid.poem.39865": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wie mein Blick das Paar auf's neu' betrachtet,", "tokens": ["Und", "wie", "mein", "Blick", "das", "Paar", "auf's", "neu'", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ART", "NN", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Werd' ich gewahr, da\u00df ihre ganze Kraft", "tokens": ["Werd'", "ich", "ge\u00b7wahr", ",", "da\u00df", "ih\u00b7re", "gan\u00b7ze", "Kraft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Mutter deutungsschwer zusammenrafft,", "tokens": ["Die", "Mut\u00b7ter", "deu\u00b7tungs\u00b7schwer", "zu\u00b7sam\u00b7men\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie dem Kind sich zu verst\u00e4nd'gen trachtet.", "tokens": ["Wenn", "sie", "dem", "Kind", "sich", "zu", "ver\u00b7st\u00e4n\u00b7d'\u00b7gen", "trach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Des theuren Wesens Geist ist wohl umnachtet,", "tokens": ["Des", "theu\u00b7ren", "We\u00b7sens", "Geist", "ist", "wohl", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und einer seiner Sinne liegt in Haft.", "tokens": ["Und", "ei\u00b7ner", "sei\u00b7ner", "Sin\u00b7ne", "liegt", "in", "Haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, ihr Geberdenspiel wird Leidenschaft,", "tokens": ["Ach", ",", "ihr", "Ge\u00b7ber\u00b7den\u00b7spiel", "wird", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Inde\u00df empor es halbverstehend schmachtet.", "tokens": ["In\u00b7de\u00df", "em\u00b7por", "es", "halb\u00b7ver\u00b7ste\u00b7hend", "schmach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und nun begreif' ich's: dieses Kind ist stumm!", "tokens": ["Und", "nun", "be\u00b7greif'", "ich's", ":", "die\u00b7ses", "Kind", "ist", "stumm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$.", "PDAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nun wird's klar mir: dieses Kind ist taub,", "tokens": ["Und", "nun", "wird's", "klar", "mir", ":", "die\u00b7ses", "Kind", "ist", "taub", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "PPER", "$.", "PDAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und liebende Verzweiflung seine Pflege!", "tokens": ["Und", "lie\u00b7ben\u00b7de", "Ver\u00b7zwei\u00b7flung", "sei\u00b7ne", "Pfle\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie mit Gespensterfurcht seh' ich mich um,", "tokens": ["Wie", "mit", "Ge\u00b7spens\u00b7ter\u00b7furcht", "seh'", "ich", "mich", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kalt bl\u00e4s't ein Wind im Trauerweidenlaub;", "tokens": ["Kalt", "bl\u00e4s't", "ein", "Wind", "im", "Trau\u00b7er\u00b7wei\u00b7den\u00b7laub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, gleich gescheuchtem Wild, meid' ich die Wege.", "tokens": ["Und", ",", "gleich", "ge\u00b7scheuch\u00b7tem", "Wild", ",", "meid'", "ich", "die", "We\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}