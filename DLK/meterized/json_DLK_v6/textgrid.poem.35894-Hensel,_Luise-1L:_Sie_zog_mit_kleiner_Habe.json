{"textgrid.poem.35894": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie zog mit kleiner Habe", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie zog mit kleiner Habe", "tokens": ["Sie", "zog", "mit", "klei\u00b7ner", "Ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum reichen Bauern hin,", "tokens": ["Zum", "rei\u00b7chen", "Bau\u00b7ern", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch manche sch\u00f6ne Gabe", "tokens": ["Doch", "man\u00b7che", "sch\u00f6\u00b7ne", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat ihr Natur verliehn.", "tokens": ["Hat", "ihr", "Na\u00b7tur", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Hofes jungem Erben", "tokens": ["Des", "Ho\u00b7fes", "jun\u00b7gem", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lacht sie in's Herz hinein;", "tokens": ["Lacht", "sie", "in's", "Herz", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lieber will er sterben,", "tokens": ["Und", "lie\u00b7ber", "will", "er", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als eine And're frei'n.", "tokens": ["Als", "ei\u00b7ne", "An\u00b7d'\u00b7re", "frei'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "ART", "ADJA", "NN", "NE"], "meter": "-+----+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Der harte Vater schm\u00e4het", "tokens": ["Der", "har\u00b7te", "Va\u00b7ter", "schm\u00e4\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und treibt die Magd hinaus;", "tokens": ["Und", "treibt", "die", "Magd", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie auch der J\u00fcngling flehet,", "tokens": ["Wie", "auch", "der", "J\u00fcng\u00b7ling", "fle\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Maid verl\u00e4\u00dft das Haus.", "tokens": ["Die", "Maid", "ver\u00b7l\u00e4\u00dft", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie dient im Nachbarhause", "tokens": ["Sie", "dient", "im", "Nach\u00b7bar\u00b7hau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um k\u00e4rglichen Gewinn,", "tokens": ["Um", "k\u00e4rg\u00b7li\u00b7chen", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Nachts aus armer Klause", "tokens": ["Und", "Nachts", "aus", "ar\u00b7mer", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schaut sie zum Hofe hin.", "tokens": ["Schaut", "sie", "zum", "Ho\u00b7fe", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der breitet weit und d\u00fcster", "tokens": ["Der", "brei\u00b7tet", "weit", "und", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor ihrem Blick sich aus.", "tokens": ["Vor", "ih\u00b7rem", "Blick", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Birnbaum und die R\u00fcster", "tokens": ["Der", "Birn\u00b7baum", "und", "die", "R\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verdecken schier das Haus.", "tokens": ["Ver\u00b7de\u00b7cken", "schier", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wenn ein tr\u00fcbes Leuchten", "tokens": ["Und", "wenn", "ein", "tr\u00fc\u00b7bes", "Leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich durch die Zweige bricht,", "tokens": ["Sich", "durch", "die", "Zwei\u00b7ge", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann reiche Thr\u00e4nen feuchten", "tokens": ["Dann", "rei\u00b7che", "Thr\u00e4\u00b7nen", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das stille Angesicht.", "tokens": ["Das", "stil\u00b7le", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Im Herzen nagt der Jammer,", "tokens": ["Im", "Her\u00b7zen", "nagt", "der", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zernagt des Lebens Kern.", "tokens": ["Zer\u00b7nagt", "des", "Le\u00b7bens", "Kern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald tr\u00e4gt zur armen Kammer", "tokens": ["Bald", "tr\u00e4gt", "zur", "ar\u00b7men", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Priester Gott, den Herrn. \u2013", "tokens": ["Der", "Pries\u00b7ter", "Gott", ",", "den", "Herrn", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Als Braut des Bauernsohnes", "tokens": ["Als", "Braut", "des", "Bau\u00b7ern\u00b7soh\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verschm\u00e4ht, du arme Maid,", "tokens": ["Ver\u00b7schm\u00e4ht", ",", "du", "ar\u00b7me", "Maid", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bist werth du nun des Thrones", "tokens": ["Bist", "werth", "du", "nun", "des", "Thro\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der h\u00f6chsten Herrlichkeit.", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er, aller Himmel K\u00f6nig,", "tokens": ["Er", ",", "al\u00b7ler", "Him\u00b7mel", "K\u00f6\u00b7nig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat dich zur Braut erw\u00e4hlt;", "tokens": ["Hat", "dich", "zur", "Braut", "er\u00b7w\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O, lache nun der Thr\u00e4nen,", "tokens": ["O", ",", "la\u00b7che", "nun", "der", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die th\u00f6richt du geweint,", "tokens": ["Die", "th\u00f6\u00b7richt", "du", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als noch dein krankes Sehnen", "tokens": ["Als", "noch", "dein", "kran\u00b7kes", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Erdensohn gemeint.", "tokens": ["Den", "Er\u00b7den\u00b7sohn", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O, schlage hoch die Schwingen,", "tokens": ["O", ",", "schla\u00b7ge", "hoch", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mild der Tod befreit:", "tokens": ["Die", "mild", "der", "Tod", "be\u00b7freit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sollst nun aufw\u00e4rts dringen", "tokens": ["Du", "sollst", "nun", "auf\u00b7w\u00e4rts", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Thron, der dir bereit. \u2013", "tokens": ["Zum", "Thron", ",", "der", "dir", "be\u00b7reit", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Glocken ", "tokens": ["Die", "Glo\u00b7cken"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die Jungfrau'nkerze scheint,", "tokens": ["Die", "Jung\u00b7frau'\u00b7nker\u00b7ze", "scheint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geschm\u00fcckt die Tr\u00e4ger schreiten,", "tokens": ["Ge\u00b7schm\u00fcckt", "die", "Tr\u00e4\u00b7ger", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und manches Auge weint.", "tokens": ["Und", "man\u00b7ches", "Au\u00b7ge", "weint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Priester milde beten", "tokens": ["Die", "Pries\u00b7ter", "mil\u00b7de", "be\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und gehn dem Zug voran,", "tokens": ["Und", "gehn", "dem", "Zug", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die Gespielen treten", "tokens": ["Und", "die", "Ge\u00b7spie\u00b7len", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}}, "stanza.14": {"line.1": {"text": "Es schwankt vor ", "tokens": ["Es", "schwankt", "vor"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die Bahre hoch empor \u2013", "tokens": ["Die", "Bah\u00b7re", "hoch", "em\u00b7por", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob dort wohl Einer sp\u00fcre,", "tokens": ["Ob", "dort", "wohl", "Ei\u00b7ner", "sp\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er ein ", "tokens": ["Da\u00df", "er", "ein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.15": {"line.1": {"text": "Und ist ", "tokens": ["Und", "ist"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Und hab' ich ", "tokens": ["Und", "hab'", "ich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch will ich treu mit deinem Zuge wallen,", "tokens": ["Doch", "will", "ich", "treu", "mit", "dei\u00b7nem", "Zu\u00b7ge", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den armen Deinen gern dabei geeint.", "tokens": ["Den", "ar\u00b7men", "Dei\u00b7nen", "gern", "da\u00b7bei", "ge\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Und wenn der Priester zum Altar getreten,", "tokens": ["Und", "wenn", "der", "Pries\u00b7ter", "zum", "Al\u00b7tar", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das heil'ge S\u00fchnungsopfer Gott zu weihn,", "tokens": ["Das", "heil'\u00b7ge", "S\u00fch\u00b7nungs\u00b7op\u00b7fer", "Gott", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will ich mein ", "tokens": ["Will", "ich", "mein"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Da\u00df froh du eingehst zu der Sel'gen Reih'n.", "tokens": ["Da\u00df", "froh", "du", "ein\u00b7gehst", "zu", "der", "Sel'\u00b7gen", "Reih'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "ADJD", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.17": {"line.1": {"text": "O bet' auch, Schwester, du mit sel'gem Munde,", "tokens": ["O", "bet'", "auch", ",", "Schwes\u00b7ter", ",", "du", "mit", "sel'\u00b7gem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "NN", "$,", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df treu ich wandle, wie die Kirche lehrt,", "tokens": ["Da\u00df", "treu", "ich", "wand\u00b7le", ",", "wie", "die", "Kir\u00b7che", "lehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df mir gnadenvoll in letzter Stunde", "tokens": ["Und", "da\u00df", "mir", "gna\u00b7den\u00b7voll", "in", "letz\u00b7ter", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Herr im Sacramente sich bescheert. Amen.", "tokens": ["Der", "Herr", "im", "Sa\u00b7cra\u00b7men\u00b7te", "sich", "be\u00b7scheert", ".", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "VVPP", "$.", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}