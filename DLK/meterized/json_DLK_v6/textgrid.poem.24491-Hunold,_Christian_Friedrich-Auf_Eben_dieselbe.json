{"textgrid.poem.24491": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Eben dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein anderer mag schalten oder walten/", "tokens": ["Ein", "an\u00b7de\u00b7rer", "mag", "schal\u00b7ten", "o\u00b7der", "wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich achte nicht der Jungfern Spiel/", "tokens": ["Ich", "ach\u00b7te", "nicht", "der", "Jung\u00b7fern", "Spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die allzu viel", "tokens": ["Die", "all\u00b7zu", "viel"], "token_info": ["word", "word", "word"], "pos": ["ART", "PTKA", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Von einem Stichblat halten.", "tokens": ["Von", "ei\u00b7nem", "Stich\u00b7blat", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}