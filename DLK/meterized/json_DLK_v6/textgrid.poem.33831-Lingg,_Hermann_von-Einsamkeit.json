{"textgrid.poem.33831": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stern und Sternbild funkeln,", "tokens": ["Stern", "und", "Stern\u00b7bild", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schwarze Wolken ziehn,", "tokens": ["Schwar\u00b7ze", "Wol\u00b7ken", "ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Tief vereinsamt dunkeln", "tokens": ["Tief", "ver\u00b7ein\u00b7samt", "dun\u00b7keln"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVPP", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mir die Stunden hin.", "tokens": ["Mir", "die", "Stun\u00b7den", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Fernher schallt ein frohes", "tokens": ["Fern\u00b7her", "schallt", "ein", "fro\u00b7hes"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fremdes Gl\u00fccklichsein;", "tokens": ["Frem\u00b7des", "Gl\u00fcck\u00b7lich\u00b7sein", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mich beseelt ein hohes", "tokens": ["Mich", "be\u00b7seelt", "ein", "ho\u00b7hes"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schwesterpaar allein:", "tokens": ["Schwes\u00b7ter\u00b7paar", "al\u00b7lein", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Weisheit, du voll Goldes", "tokens": ["Weis\u00b7heit", ",", "du", "voll", "Gol\u00b7des"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "ADJD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In dem ernsten Blick,", "tokens": ["In", "dem", "erns\u00b7ten", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und du, himmlisch holdes", "tokens": ["Und", "du", ",", "himm\u00b7lisch", "hol\u00b7des"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "ADJD", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Feenkind, Musik!", "tokens": ["Fe\u00b7en\u00b7kind", ",", "Mu\u00b7sik", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}