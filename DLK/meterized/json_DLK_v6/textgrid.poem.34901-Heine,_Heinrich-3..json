{"textgrid.poem.34901": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Tracht der Beguinen,", "tokens": ["In", "der", "Tracht", "der", "Be\u00b7gu\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Mantel mit der Kappe", "tokens": ["In", "dem", "Man\u00b7tel", "mit", "der", "Kap\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der gr\u00f6bsten schwarzen Serge,", "tokens": ["Von", "der", "gr\u00f6bs\u00b7ten", "schwar\u00b7zen", "Ser\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist vermummt die junge Nonne.", "tokens": ["Ist", "ver\u00b7mummt", "die", "jun\u00b7ge", "Non\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hastig l\u00e4ngs des Rheines Ufern", "tokens": ["Has\u00b7tig", "l\u00e4ngs", "des", "Rhei\u00b7nes", "U\u00b7fern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreitet sie hinab die Landstra\u00df',", "tokens": ["Schrei\u00b7tet", "sie", "hin\u00b7ab", "die", "Land\u00b7stra\u00df'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die nach Holland f\u00fchrt, und hastig", "tokens": ["Die", "nach", "Hol\u00b7land", "f\u00fchrt", ",", "und", "has\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "NE", "VVFIN", "$,", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fragt sie jeden, der vorbeikommt:", "tokens": ["Fragt", "sie", "je\u00b7den", ",", "der", "vor\u00b7bei\u00b7kommt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "\u00bbhabt Ihr nicht gesehn Apollo?", "tokens": ["\u00bb", "habt", "Ihr", "nicht", "ge\u00b7sehn", "A\u00b7pol\u00b7lo", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PTKNEG", "VVPP", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Einen roten Mantel tr\u00e4gt er,", "tokens": ["Ei\u00b7nen", "ro\u00b7ten", "Man\u00b7tel", "tr\u00e4gt", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblich singt er, spielt die Leier,", "tokens": ["Lieb\u00b7lich", "singt", "er", ",", "spielt", "die", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er ist mein holder Abgott.\u00ab", "tokens": ["Und", "er", "ist", "mein", "hol\u00b7der", "Ab\u00b7gott", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keiner will ihr Rede stehen,", "tokens": ["Kei\u00b7ner", "will", "ihr", "Re\u00b7de", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mancher dreht ihr stumm den R\u00fccken,", "tokens": ["Man\u00b7cher", "dreht", "ihr", "stumm", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mancher glotzt sie an und l\u00e4chelt,", "tokens": ["Man\u00b7cher", "glotzt", "sie", "an", "und", "l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mancher seufzet: \u00bbArmes Kind!\u00ab", "tokens": ["Man\u00b7cher", "seuf\u00b7zet", ":", "\u00bb", "Ar\u00b7mes", "Kind", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "$.", "$(", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch des Wegs herangetrottelt", "tokens": ["Doch", "des", "Wegs", "her\u00b7an\u00b7ge\u00b7trot\u00b7telt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt ein schlottrig alter Mensch,", "tokens": ["Kommt", "ein", "schlott\u00b7rig", "al\u00b7ter", "Mensch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fingert in der Luft, wie rechnend,", "tokens": ["Fin\u00b7gert", "in", "der", "Luft", ",", "wie", "rech\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4selnd singt er vor sich hin.", "tokens": ["N\u00e4\u00b7selnd", "singt", "er", "vor", "sich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Einen schlappen Quersack tr\u00e4gt er,", "tokens": ["Ei\u00b7nen", "schlap\u00b7pen", "Quer\u00b7sack", "tr\u00e4gt", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch ein klein dreieckig H\u00fctchen;", "tokens": ["Auch", "ein", "klein", "drei\u00b7ec\u00b7kig", "H\u00fct\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "CARD", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und mit schmunzelnd klugen \u00c4uglein", "tokens": ["Und", "mit", "schmun\u00b7zelnd", "klu\u00b7gen", "\u00c4ug\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "H\u00f6rt er an den Spruch der Nonne:", "tokens": ["H\u00f6rt", "er", "an", "den", "Spruch", "der", "Non\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbhabt Ihr nicht gesehn Apollo?", "tokens": ["\u00bb", "habt", "Ihr", "nicht", "ge\u00b7sehn", "A\u00b7pol\u00b7lo", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PTKNEG", "VVPP", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Einen roten Mantel tr\u00e4gt er,", "tokens": ["Ei\u00b7nen", "ro\u00b7ten", "Man\u00b7tel", "tr\u00e4gt", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblich singt er, spielt die Leier,", "tokens": ["Lieb\u00b7lich", "singt", "er", ",", "spielt", "die", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er ist mein holder Abgott.\u00ab", "tokens": ["Und", "er", "ist", "mein", "hol\u00b7der", "Ab\u00b7gott", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Jener aber gab zur Antwort,", "tokens": ["Je\u00b7ner", "a\u00b7ber", "gab", "zur", "Ant\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4hrend er sein K\u00f6pfchen wiegte", "tokens": ["W\u00e4h\u00b7rend", "er", "sein", "K\u00f6pf\u00b7chen", "wieg\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hin und her, und gar possierlich", "tokens": ["Hin", "und", "her", ",", "und", "gar", "pos\u00b7sier\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "PTKVZ", "$,", "KON", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zupfte an dem spitzen B\u00e4rtchen:", "tokens": ["Zupf\u00b7te", "an", "dem", "spit\u00b7zen", "B\u00e4rt\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbob ich ihn gesehen habe?", "tokens": ["\u00bb", "ob", "ich", "ihn", "ge\u00b7se\u00b7hen", "ha\u00b7be", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja, ich habe ihn gesehen", "tokens": ["Ja", ",", "ich", "ha\u00b7be", "ihn", "ge\u00b7se\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft genug zu Amsterdam,", "tokens": ["Oft", "ge\u00b7nug", "zu", "A\u00b7mster\u00b7dam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der deutschen Synagoge.", "tokens": ["In", "der", "deut\u00b7schen", "Syn\u00b7a\u00b7go\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Denn er war Vors\u00e4nger dorten,", "tokens": ["Denn", "er", "war", "Vor\u00b7s\u00e4n\u00b7ger", "dor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und da hie\u00df er Rabbi Faibisch,", "tokens": ["Und", "da", "hie\u00df", "er", "Rab\u00b7bi", "Fai\u00b7bisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Was auf Hochdeutsch hei\u00dfe Apollo \u2013", "tokens": ["Was", "auf", "Hoch\u00b7deutsch", "hei\u00b7\u00dfe", "A\u00b7pol\u00b7lo", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "NE", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Doch mein Abgott ist er nicht.", "tokens": ["Doch", "mein", "Ab\u00b7gott", "ist", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Roter Mantel? Auch den roten", "tokens": ["Ro\u00b7ter", "Man\u00b7tel", "?", "Auch", "den", "ro\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mantel kenn ich. Echter Scharlach,", "tokens": ["Man\u00b7tel", "kenn", "ich", ".", "Ech\u00b7ter", "Schar\u00b7lach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kostet acht Florin die Elle,", "tokens": ["Kos\u00b7tet", "acht", "Flo\u00b7rin", "die", "El\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ist noch nicht ganz bezahlt.", "tokens": ["Und", "ist", "noch", "nicht", "ganz", "be\u00b7zahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Seinen Vater Moses Jitscher", "tokens": ["Sei\u00b7nen", "Va\u00b7ter", "Mo\u00b7ses", "Jit\u00b7scher"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kenn ich gut. Vorhautabschneider", "tokens": ["Kenn", "ich", "gut", ".", "Vor\u00b7haut\u00b7ab\u00b7schnei\u00b7der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADJD", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist er bei den Portugiesen.", "tokens": ["Ist", "er", "bei", "den", "Por\u00b7tu\u00b7gie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er beschnitt auch Souver\u00e4ne.", "tokens": ["Er", "be\u00b7schnitt", "auch", "Sou\u00b7ve\u00b7r\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Seine Mutter ist Cousine", "tokens": ["Sei\u00b7ne", "Mut\u00b7ter", "ist", "Cou\u00b7si\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Schwagers, und sie handelt", "tokens": ["Mei\u00b7nes", "Schwa\u00b7gers", ",", "und", "sie", "han\u00b7delt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Gracht mit sauern Gurken", "tokens": ["Auf", "der", "Gracht", "mit", "sau\u00b7ern", "Gur\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit abgelebten Hosen.", "tokens": ["Und", "mit", "ab\u00b7ge\u00b7leb\u00b7ten", "Ho\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Haben kein Pl\u00e4sier am Sohne.", "tokens": ["Ha\u00b7ben", "kein", "Pl\u00e4\u00b7sier", "am", "Soh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Dieser spielt sehr gut die Leier,", "tokens": ["Die\u00b7ser", "spielt", "sehr", "gut", "die", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber leider noch viel besser", "tokens": ["A\u00b7ber", "lei\u00b7der", "noch", "viel", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielt er oft Tarock und L'hombre.", "tokens": ["Spielt", "er", "oft", "Ta\u00b7rock", "und", "L'\u00b7hom\u00b7bre", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Auch ein Freigeist ist er, a\u00df", "tokens": ["Auch", "ein", "Frei\u00b7geist", "ist", "er", ",", "a\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweinefleisch, verlor sein Amt,", "tokens": ["Schwei\u00b7ne\u00b7fleisch", ",", "ver\u00b7lor", "sein", "Amt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er zog herum im Lande", "tokens": ["Und", "er", "zog", "he\u00b7rum", "im", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit geschminkten Kom\u00f6dianten.", "tokens": ["Mit", "ge\u00b7schmink\u00b7ten", "Ko\u00b7m\u00f6\u00b7di\u00b7an\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "In den Buden, auf den M\u00e4rkten,", "tokens": ["In", "den", "Bu\u00b7den", ",", "auf", "den", "M\u00e4rk\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielte er den Pickelhering,", "tokens": ["Spiel\u00b7te", "er", "den", "Pi\u00b7ckel\u00b7he\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Holofernes, K\u00f6nig David,", "tokens": ["Ho\u00b7lo\u00b7fer\u00b7nes", ",", "K\u00f6\u00b7nig", "Da\u00b7vid", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesen mit dem besten Beifall.", "tokens": ["Die\u00b7sen", "mit", "dem", "bes\u00b7ten", "Bei\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Denn des K\u00f6nigs eigne Lieder", "tokens": ["Denn", "des", "K\u00f6\u00b7nigs", "eig\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sang er in des K\u00f6nigs eigner", "tokens": ["Sang", "er", "in", "des", "K\u00f6\u00b7nigs", "eig\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Muttersprache, tremulierend", "tokens": ["Mut\u00b7ter\u00b7spra\u00b7che", ",", "tre\u00b7mu\u00b7lie\u00b7rend"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Nigens alter Weise.", "tokens": ["In", "des", "Ni\u00b7gens", "al\u00b7ter", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Aus dem Amsterdamer Spielhuis", "tokens": ["Aus", "dem", "A\u00b7mster\u00b7da\u00b7mer", "Spiel\u00b7hu\u00b7is"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zog er j\u00fcngst etwelche Dirnen,", "tokens": ["Zog", "er", "j\u00fcngst", "et\u00b7wel\u00b7che", "Dir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit diesen Musen zieht er", "tokens": ["Und", "mit", "die\u00b7sen", "Mu\u00b7sen", "zieht", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt herum als ein Apollo.", "tokens": ["Jetzt", "he\u00b7rum", "als", "ein", "A\u00b7pol\u00b7lo", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KOKOM", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Eine dicke ist darunter,", "tokens": ["Ei\u00b7ne", "di\u00b7cke", "ist", "da\u00b7run\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die vorz\u00fcglich quiekt und gr\u00fcnzelt;", "tokens": ["Die", "vor\u00b7z\u00fcg\u00b7lich", "qui\u00b7ekt", "und", "gr\u00fcn\u00b7zelt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ob dem gro\u00dfen Lorbeerkopfputz", "tokens": ["Ob", "dem", "gro\u00b7\u00dfen", "Lor\u00b7beer\u00b7kopf\u00b7putz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nennt man sie die gr\u00fcne Sau.\u00ab", "tokens": ["Nennt", "man", "sie", "die", "gr\u00fc\u00b7ne", "Sau", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}