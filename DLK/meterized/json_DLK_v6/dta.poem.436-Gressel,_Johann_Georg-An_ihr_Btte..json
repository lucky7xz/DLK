{"dta.poem.436": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An ihr Btte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Federn h\u00f6rt ihr nicht aus meiner Liebsten Munde", "tokens": ["Ihr", "Fe\u00b7dern", "h\u00f6rt", "ihr", "nicht", "aus", "mei\u00b7ner", "Liebs\u00b7ten", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gebrochne Worte gehn/ wenn sie zur Morgen-Stunde", "tokens": ["Ge\u00b7broch\u00b7ne", "Wor\u00b7te", "gehn", "/", "wenn", "sie", "zur", "Mor\u00b7gen\u00b7Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVINF", "$(", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Halb schlaffend/ halb erwacht auf ihren Lager spricht/", "tokens": ["Halb", "schlaf\u00b7fend", "/", "halb", "er\u00b7wacht", "auf", "ih\u00b7ren", "La\u00b7ger", "spricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$(", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dencket sie im Schlaff an ihrem ", "tokens": ["Und", "den\u00b7cket", "sie", "im", "Schlaff", "an", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}