{"dta.poem.7715": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir leben nur zum Schein in Einer Welt zusammen,", "tokens": ["Wir", "le\u00b7ben", "nur", "zum", "Schein", "in", "Ei\u00b7ner", "Welt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In die zusammen gar verschiedne Welten schwammen.", "tokens": ["In", "die", "zu\u00b7sam\u00b7men", "gar", "ver\u00b7schied\u00b7ne", "Wel\u00b7ten", "schwam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So, \u00e4u\u00dferlich vereint, und innerlich getrennt,", "tokens": ["So", ",", "\u00e4u\u00b7\u00dfer\u00b7lich", "ver\u00b7eint", ",", "und", "in\u00b7ner\u00b7lich", "ge\u00b7trennt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VVPP", "$,", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwimmt jeder eigne Geist im eignen Element.", "tokens": ["Schwimmt", "je\u00b7der", "eig\u00b7ne", "Geist", "im", "eig\u00b7nen", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}