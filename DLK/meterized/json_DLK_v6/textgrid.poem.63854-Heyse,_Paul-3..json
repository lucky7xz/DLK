{"textgrid.poem.63854": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich's vers\u00e4umte, weckt mir ew'ge Reue.", "tokens": ["Da\u00df", "ich's", "ver\u00b7s\u00e4um\u00b7te", ",", "weckt", "mir", "ew'\u00b7ge", "Reu\u00b7e", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie sollt' ich in sein leuchtend Auge blicken,", "tokens": ["Nie", "sollt'", "ich", "in", "sein", "leuch\u00b7tend", "Au\u00b7ge", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Niemals versuchen, stammelnd auszudr\u00fccken,", "tokens": ["Nie\u00b7mals", "ver\u00b7su\u00b7chen", ",", "stam\u00b7melnd", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADJD", "VVIZU", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wie innig seines Sangs ich mich erfreue.", "tokens": ["Wie", "in\u00b7nig", "sei\u00b7nes", "Sangs", "ich", "mich", "er\u00b7freu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun h\u00e4ng' ich um so mehr mit sp\u00e4ter Treue", "tokens": ["Nun", "h\u00e4ng'", "ich", "um", "so", "mehr", "mit", "sp\u00e4\u00b7ter", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An seinem Bilde, dem so kalt den R\u00fccken", "tokens": ["An", "sei\u00b7nem", "Bil\u00b7de", ",", "dem", "so", "kalt", "den", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Mode kehrt, die immer mit Entz\u00fccken", "tokens": ["Die", "Mo\u00b7de", "kehrt", ",", "die", "im\u00b7mer", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Preis't das Verg\u00e4ngliche, das glei\u00dfend Neue.", "tokens": ["Preis't", "das", "Ver\u00b7g\u00e4ng\u00b7li\u00b7che", ",", "das", "glei\u00b7\u00dfend", "Neu\u00b7e", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "ADJD", "ADJA", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Mir aber ist, bl\u00e4ttr' ich in seinen Liedern,", "tokens": ["Mir", "a\u00b7ber", "ist", ",", "bl\u00e4t\u00b7tr'", "ich", "in", "sei\u00b7nen", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als h\u00f6rt' ich eines Freundes Stimme t\u00f6nen", "tokens": ["Als", "h\u00f6rt'", "ich", "ei\u00b7nes", "Freun\u00b7des", "Stim\u00b7me", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und m\u00fc\u00dfte jetzt noch seinen Gru\u00df erwidern,", "tokens": ["Und", "m\u00fc\u00df\u00b7te", "jetzt", "noch", "sei\u00b7nen", "Gru\u00df", "er\u00b7wi\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit Lorbeer seine Dichterstirne kr\u00f6nen", "tokens": ["Mit", "Lor\u00b7beer", "sei\u00b7ne", "Dich\u00b7ter\u00b7stir\u00b7ne", "kr\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sein Gem\u00fct, verwundet durch den niedern", "tokens": ["Und", "sein", "Ge\u00b7m\u00fct", ",", "ver\u00b7wun\u00b7det", "durch", "den", "nie\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "VVPP", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Undank der nachgebornen Welt, vers\u00f6hnen.", "tokens": ["Un\u00b7dank", "der", "nach\u00b7ge\u00b7bor\u00b7nen", "Welt", ",", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}