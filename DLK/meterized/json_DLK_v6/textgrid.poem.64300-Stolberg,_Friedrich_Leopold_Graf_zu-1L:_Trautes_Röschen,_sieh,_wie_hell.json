{"textgrid.poem.64300": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Trautes R\u00f6schen, sieh, wie hell", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trautes R\u00f6schen, sieh, wie hell", "tokens": ["Trau\u00b7tes", "R\u00f6\u00b7schen", ",", "sieh", ",", "wie", "hell"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Gei\u00dfblatt dieser Quell", "tokens": ["\u00dc\u00b7ber", "Gei\u00df\u00b7blatt", "die\u00b7ser", "Quell"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch Vergi\u00dfmeinnichtchen flie\u00dfet!", "tokens": ["Durch", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht\u00b7chen", "flie\u00b7\u00dfet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rei\u00dfender rauscht dort sein Fall,", "tokens": ["Rei\u00b7\u00dfen\u00b7der", "rauscht", "dort", "sein", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Wo er mit des Donners Schall,", "tokens": ["Wo", "er", "mit", "des", "Don\u00b7ners", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und des Thales Wiederhall,", "tokens": ["Und", "des", "Tha\u00b7les", "Wie\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00dcber Felsen sich ergie\u00dfet!", "tokens": ["\u00dc\u00b7ber", "Fel\u00b7sen", "sich", "er\u00b7gie\u00b7\u00dfet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber s\u00fc\u00dfer ist er mir,", "tokens": ["A\u00b7ber", "s\u00fc\u00b7\u00dfer", "ist", "er", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein geliebtes R\u00f6schen, hier;", "tokens": ["Mein", "ge\u00b7lieb\u00b7tes", "R\u00f6\u00b7schen", ",", "hier", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn er gleichet unserm Leben.", "tokens": ["Denn", "er", "glei\u00b7chet", "un\u00b7serm", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh' ich ihn so sanft und rein", "tokens": ["Seh'", "ich", "ihn", "so", "sanft", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleiten in des Mondes Schein;", "tokens": ["Glei\u00b7ten", "in", "des", "Mon\u00b7des", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "R\u00f6schen, dann gedenk' ich dein,", "tokens": ["R\u00f6\u00b7schen", ",", "dann", "ge\u00b7denk'", "ich", "dein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und des Gl\u00fccks, so Gott gegeben.", "tokens": ["Und", "des", "Gl\u00fccks", ",", "so", "Gott", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}