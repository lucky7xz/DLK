{"textgrid.poem.61927": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Sch\u00f6nheit goldner Pfeil", "tokens": ["Wenn", "der", "Sch\u00f6n\u00b7heit", "gold\u00b7ner", "Pfeil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitten dich ins Herz getroffen,", "tokens": ["Mit\u00b7ten", "dich", "ins", "Herz", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Konntest du ein gr\u00f6\u00dfer Heil,", "tokens": ["Konn\u00b7test", "du", "ein", "gr\u00f6\u00b7\u00dfer", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frohverj\u00fcngter, jemals hoffen?", "tokens": ["Froh\u00b7ver\u00b7j\u00fcng\u00b7ter", ",", "je\u00b7mals", "hof\u00b7fen", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was verlangst du nach Besitz?", "tokens": ["Was", "ver\u00b7langst", "du", "nach", "Be\u00b7sitz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lern auf so viel Gl\u00fcck entbehren!", "tokens": ["Lern", "auf", "so", "viel", "Gl\u00fcck", "ent\u00b7beh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "War doch Seligkeit der Blitz,", "tokens": ["War", "doch", "Se\u00b7lig\u00b7keit", "der", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dessen Flammen dich verzehren.", "tokens": ["Des\u00b7sen", "Flam\u00b7men", "dich", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}