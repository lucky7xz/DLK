{"textgrid.poem.58832": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "51. Verborgen sein bringt Ruh", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ganz vergess'ner B\u00fcrger sein,", "tokens": ["Ein", "ganz", "ver\u00b7ge\u00b7ss'\u00b7ner", "B\u00fcr\u00b7ger", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von dem man nicht viel wei\u00df noch h\u00f6ret,", "tokens": ["Von", "dem", "man", "nicht", "viel", "wei\u00df", "noch", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "PTKNEG", "ADV", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Gott bekannt und Gott gemein:", "tokens": ["Nur", "Gott", "be\u00b7kannt", "und", "Gott", "ge\u00b7mein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lebt man frei und ungest\u00f6ret.", "tokens": ["So", "lebt", "man", "frei", "und", "un\u00b7ge\u00b7st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}