{"textgrid.poem.24246": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Tulpenflammen und Narzissen", "tokens": ["Zwi\u00b7schen", "Tul\u00b7pen\u00b7flam\u00b7men", "und", "Nar\u00b7zis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Springen unter schweren Fliederb\u00fcschen", "tokens": ["Sprin\u00b7gen", "un\u00b7ter", "schwe\u00b7ren", "Flie\u00b7der\u00b7b\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kleine M\u00e4dchen losen Haars im Garten.", "tokens": ["Klei\u00b7ne", "M\u00e4d\u00b7chen", "lo\u00b7sen", "Haars", "im", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lerne, Herz! Die kleinen M\u00e4dchen wissen", "tokens": ["Ler\u00b7ne", ",", "Herz", "!", "Die", "klei\u00b7nen", "M\u00e4d\u00b7chen", "wis\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mehr vom Gl\u00fcck, als du; mit ihrem Springen", "tokens": ["Mehr", "vom", "Gl\u00fcck", ",", "als", "du", ";", "mit", "ih\u00b7rem", "Sprin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Loben sie den heiligen Geist der Pfingsten", "tokens": ["Lo\u00b7ben", "sie", "den", "hei\u00b7li\u00b7gen", "Geist", "der", "Pfings\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Zwischen Tulpenflammen und Narzissen.", "tokens": ["Zwi\u00b7schen", "Tul\u00b7pen\u00b7flam\u00b7men", "und", "Nar\u00b7zis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Denn der heilige Geist ist ausgegossen", "tokens": ["Denn", "der", "hei\u00b7li\u00b7ge", "Geist", "ist", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "In den glutenbunten Tulpenflammen,", "tokens": ["In", "den", "glu\u00b7ten\u00b7bun\u00b7ten", "Tul\u00b7pen\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und er hei\u00dft: Seid fr\u00f6hlich, Menschenkinder!", "tokens": ["Und", "er", "hei\u00dft", ":", "Seid", "fr\u00f6h\u00b7lich", ",", "Men\u00b7schen\u00b7kin\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "VAIMP", "ADJD", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jede Blume, glorienumflossen,", "tokens": ["Je\u00b7de", "Blu\u00b7me", ",", "glo\u00b7ri\u00b7en\u00b7um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ist, dem Haupt Mariens gleich, ein Abbild", "tokens": ["Ist", ",", "dem", "Haupt", "Ma\u00b7ri\u00b7ens", "gleich", ",", "ein", "Ab\u00b7bild"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "ART", "NN", "NE", "ADV", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Milder, tiefer, s\u00fc\u00dfer Gottesliebe ...", "tokens": ["Mil\u00b7der", ",", "tie\u00b7fer", ",", "s\u00fc\u00b7\u00dfer", "Got\u00b7tes\u00b7lie\u00b7be", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Denn der heilige Geist ist ausgegossen.", "tokens": ["Denn", "der", "hei\u00b7li\u00b7ge", "Geist", "ist", "aus\u00b7ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}}}}