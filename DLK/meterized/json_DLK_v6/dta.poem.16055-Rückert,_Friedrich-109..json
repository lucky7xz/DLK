{"dta.poem.16055": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "109.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ohn' einen h\u00f6chsten Gott und ohn' ein k\u00fcnft'ges Leben,", "tokens": ["Ohn'", "ei\u00b7nen", "h\u00f6chs\u00b7ten", "Gott", "und", "ohn'", "ein", "k\u00fcnft'\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sagst du, sei kein Gesetz der Sittlichkeit gegeben.", "tokens": ["Sagst", "du", ",", "sei", "kein", "Ge\u00b7setz", "der", "Sitt\u00b7lich\u00b7keit", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch die Geschichte sagt, da\u00df, in die Brust gepr\u00e4gt,", "tokens": ["Doch", "die", "Ge\u00b7schich\u00b7te", "sagt", ",", "da\u00df", ",", "in", "die", "Brust", "ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KOUS", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das sittliche Gesetz sich selber h\u00e4lt und tr\u00e4gt.", "tokens": ["Das", "sitt\u00b7li\u00b7che", "Ge\u00b7setz", "sich", "sel\u00b7ber", "h\u00e4lt", "und", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer dort es eingepr\u00e4gt, kann freilich Gott nur seyn,", "tokens": ["Wer", "dort", "es", "ein\u00b7ge\u00b7pr\u00e4gt", ",", "kann", "frei\u00b7lich", "Gott", "nur", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,", "VMFIN", "ADV", "NN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcr dis Leben nicht ists eingepr\u00e4gt allein.", "tokens": ["Und", "f\u00fcr", "dis", "Le\u00b7ben", "nicht", "ists", "ein\u00b7ge\u00b7pr\u00e4gt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "NN", "PTKNEG", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch kann vergessen seyn, wozu er es gegeben,", "tokens": ["Doch", "kann", "ver\u00b7ges\u00b7sen", "seyn", ",", "wo\u00b7zu", "er", "es", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVPP", "VAINF", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergessen, der es gab, und das Gesetz doch leben.", "tokens": ["Ver\u00b7ges\u00b7sen", ",", "der", "es", "gab", ",", "und", "das", "Ge\u00b7setz", "doch", "le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So sind von Gott bedacht, auch die ihn nicht erkennen,", "tokens": ["So", "sind", "von", "Gott", "be\u00b7dacht", ",", "auch", "die", "ihn", "nicht", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "VVPP", "$,", "ADV", "ART", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ehren seine Macht, auch wenn sie's anders nennen.", "tokens": ["Und", "eh\u00b7ren", "sei\u00b7ne", "Macht", ",", "auch", "wenn", "sie's", "an\u00b7ders", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "KOUS", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}