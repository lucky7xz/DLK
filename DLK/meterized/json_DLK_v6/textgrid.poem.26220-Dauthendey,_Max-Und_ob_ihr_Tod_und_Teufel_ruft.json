{"textgrid.poem.26220": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Und ob ihr Tod und Teufel ruft", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Land, im Meer und in der Luft", "tokens": ["Zu", "Land", ",", "im", "Meer", "und", "in", "der", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stehn alle auf, mit uns zu ringen.", "tokens": ["Stehn", "al\u00b7le", "auf", ",", "mit", "uns", "zu", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ob ihr Tod und Teufel ruft, \u2013", "tokens": ["Doch", "ob", "ihr", "Tod", "und", "Teu\u00b7fel", "ruft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heil, Deutschland ist nicht umzubringen.", "tokens": ["Heil", ",", "Deutschland", "ist", "nicht", "um\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "VAFIN", "PTKNEG", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die deutsche Kraft, den deutschen Geist", "tokens": ["Die", "deut\u00b7sche", "Kraft", ",", "den", "deut\u00b7schen", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr nicht so leicht in St\u00fccke rei\u00dft.", "tokens": ["Ihr", "nicht", "so", "leicht", "in", "St\u00fc\u00b7cke", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein ehrlich Volk mit gradem Sinn", "tokens": ["Ein", "ehr\u00b7lich", "Volk", "mit", "gra\u00b7dem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stiebt nicht in alle Winde hin.", "tokens": ["Stiebt", "nicht", "in", "al\u00b7le", "Win\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir wollen es euch lohnen,", "tokens": ["Wir", "wol\u00b7len", "es", "euch", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ihr gar hoch uns eingesch\u00e4tzt", "tokens": ["Da\u00df", "ihr", "gar", "hoch", "uns", "ein\u00b7ge\u00b7sch\u00e4tzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rund in allen Zonen", "tokens": ["Und", "rund", "in", "al\u00b7len", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Eisen heftig gen uns wetzt.", "tokens": ["Das", "Ei\u00b7sen", "hef\u00b7tig", "gen", "uns", "wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir wollen blutig danken,", "tokens": ["Wir", "wol\u00b7len", "blu\u00b7tig", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht vor dem Sturme wanken.", "tokens": ["Nicht", "vor", "dem", "Stur\u00b7me", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "O, sch\u00f6nes Deutschland, bist es wert,", "tokens": ["O", ",", "sch\u00f6\u00b7nes", "Deutschland", ",", "bist", "es", "wert", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die gro\u00dfe Feindschaft hoch dich ehrt.", "tokens": ["Die", "gro\u00b7\u00dfe", "Feind\u00b7schaft", "hoch", "dich", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gern stirbt f\u00fcr dich der letzte Mann!", "tokens": ["Gern", "stirbt", "f\u00fcr", "dich", "der", "letz\u00b7te", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schicksal aber wird's nicht wollen.", "tokens": ["Das", "Schick\u00b7sal", "a\u00b7ber", "wird's", "nicht", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PTKNEG", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Seht euch die deutschen W\u00e4lder an,", "tokens": ["Seht", "euch", "die", "deut\u00b7schen", "W\u00e4l\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob alle Eichen st\u00fcrzen sollen.", "tokens": ["Ob", "al\u00b7le", "Ei\u00b7chen", "st\u00fcr\u00b7zen", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Im Wald, im Meer und in der Luft", "tokens": ["Im", "Wald", ",", "im", "Meer", "und", "in", "der", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll manches deutsche Lied noch klingen.", "tokens": ["Soll", "man\u00b7ches", "deut\u00b7sche", "Lied", "noch", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ob ihr Tod und Teufel ruft, \u2013", "tokens": ["Und", "ob", "ihr", "Tod", "und", "Teu\u00b7fel", "ruft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heil, Deutschland ist nicht umzubringen.", "tokens": ["H