{"textgrid.poem.47639": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[tiefe Kenner der Natur]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tiefe Kenner der Natur,", "tokens": ["Tie\u00b7fe", "Ken\u00b7ner", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alte Weise sagten aus:", "tokens": ["Al\u00b7te", "Wei\u00b7se", "sag\u00b7ten", "aus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4nde sich im Weltenhaus", "tokens": ["F\u00e4n\u00b7de", "sich", "im", "Wel\u00b7ten\u00b7haus"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und im fernen Osten w\u00e4r'", "tokens": ["Und", "im", "fer\u00b7nen", "Os\u00b7ten", "w\u00e4r'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Beide z\u00f6gen so sich an,", "tokens": ["Bei\u00b7de", "z\u00f6\u00b7gen", "so", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df sie fingen an zu wandern,", "tokens": ["Da\u00df", "sie", "fin\u00b7gen", "an", "zu", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKVZ", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und das Eine bei dem Andern", "tokens": ["Und", "das", "Ei\u00b7ne", "bei", "dem", "An\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ART", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "K\u00e4m' am Ende richtig an.", "tokens": ["K\u00e4m'", "am", "En\u00b7de", "rich\u00b7tig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}