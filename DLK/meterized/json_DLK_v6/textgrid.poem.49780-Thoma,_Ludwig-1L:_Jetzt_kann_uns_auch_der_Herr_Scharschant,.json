{"textgrid.poem.49780": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzt kann uns auch der Herr Scharschant,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt kann uns auch der Herr Scharschant,", "tokens": ["Jetzt", "kann", "uns", "auch", "der", "Herr", "Schar\u00b7schant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ade! Ade, Kaserne!", "tokens": ["A\u00b7de", "!", "A\u00b7de", ",", "Ka\u00b7ser\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wir gr\u00fc\u00dfen dich und allerhand", "tokens": ["Wir", "gr\u00fc\u00b7\u00dfen", "dich", "und", "al\u00b7ler\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und winken aus der Ferne.", "tokens": ["Und", "win\u00b7ken", "aus", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jetzt kann uns auch der Herr Major", "tokens": ["Jetzt", "kann", "uns", "auch", "der", "Herr", "Ma\u00b7jor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum letzten Lebewohle;", "tokens": ["Zum", "letz\u00b7ten", "Le\u00b7be\u00b7woh\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir ziehens fr\u00f6hlich aus dem Tor", "tokens": ["Wir", "zie\u00b7hens", "fr\u00f6h\u00b7lich", "aus", "dem", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rufens die Parole.", "tokens": ["Und", "ru\u00b7fens", "die", "Pa\u00b7ro\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Jetzt, M\u00e4dchen, mu\u00df geschieden sein", "tokens": ["Jetzt", ",", "M\u00e4d\u00b7chen", ",", "mu\u00df", "ge\u00b7schie\u00b7den", "sein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VMFIN", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und darfst dich nicht zergr\u00e4men!", "tokens": ["Und", "darfst", "dich", "nicht", "zer\u00b7gr\u00e4\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es rucken schon die andern ein,", "tokens": ["Es", "ru\u00b7cken", "schon", "die", "an\u00b7dern", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die wo dich wieder nehmen.", "tokens": ["Die", "wo", "dich", "wie\u00b7der", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PWAV", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}