{"textgrid.poem.55951": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Todes-Erfahrung", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir wissen nichts von diesem Hingehn, das", "tokens": ["Wir", "wis\u00b7sen", "nichts", "von", "die\u00b7sem", "Hin\u00b7gehn", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PDAT", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nicht mit uns teilt. Wir haben keinen Grund,", "tokens": ["nicht", "mit", "uns", "teilt", ".", "Wir", "ha\u00b7ben", "kei\u00b7nen", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bewunderung und Liebe oder Ha\u00df", "tokens": ["Be\u00b7wun\u00b7de\u00b7rung", "und", "Lie\u00b7be", "o\u00b7der", "Ha\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "dem Tod zu zeigen, den ein Maskenmund", "tokens": ["dem", "Tod", "zu", "zei\u00b7gen", ",", "den", "ein", "Mas\u00b7ken\u00b7mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "tragischer Klage wunderlich entstellt.", "tokens": ["tra\u00b7gi\u00b7scher", "Kla\u00b7ge", "wun\u00b7der\u00b7lich", "ent\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Noch ist die Welt voll Rollen, die wir spielen.", "tokens": ["Noch", "ist", "die", "Welt", "voll", "Rol\u00b7len", ",", "die", "wir", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Solang wir sorgen, ob wir auch gefielen,", "tokens": ["So\u00b7lang", "wir", "sor\u00b7gen", ",", "ob", "wir", "auch", "ge\u00b7fie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "spielt auch der Tod, obwohl er nicht gef\u00e4llt.", "tokens": ["spielt", "auch", "der", "Tod", ",", "ob\u00b7wohl", "er", "nicht", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Doch als du gingst, da brach in diese B\u00fchne", "tokens": ["Doch", "als", "du", "gingst", ",", "da", "brach", "in", "die\u00b7se", "B\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ein Streifen Wirklichkeit durch jenen Spalt", "tokens": ["ein", "Strei\u00b7fen", "Wirk\u00b7lich\u00b7keit", "durch", "je\u00b7nen", "Spalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "durch den du hingingst: Gr\u00fcn wirklicher Gr\u00fcne,", "tokens": ["durch", "den", "du", "hin\u00b7gingst", ":", "Gr\u00fcn", "wirk\u00b7li\u00b7cher", "Gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VVFIN", "$.", "NN", "ADJA", "NN", "$,"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "wirklicher Sonnenschein, wirklicher Wald.", "tokens": ["wirk\u00b7li\u00b7cher", "Son\u00b7nen\u00b7schein", ",", "wirk\u00b7li\u00b7cher", "Wald", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir spielen weiter. Bang und schwer Erlerntes", "tokens": ["Wir", "spie\u00b7len", "wei\u00b7ter", ".", "Bang", "und", "schwer", "Er\u00b7lern\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "hersagend und Geb\u00e4rden dann und wann", "tokens": ["her\u00b7sa\u00b7gend", "und", "Ge\u00b7b\u00e4r\u00b7den", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "NN", "ADV", "KON", "PWAV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "aufhebend; aber dein von uns entferntes,", "tokens": ["auf\u00b7he\u00b7bend", ";", "a\u00b7ber", "dein", "von", "uns", "ent\u00b7fern\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ADV", "PPOSAT", "APPR", "PPER", "ADJA", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "aus unserm St\u00fcck entr\u00fccktes Dasein kann", "tokens": ["aus", "un\u00b7serm", "St\u00fcck", "ent\u00b7r\u00fcck\u00b7tes", "Da\u00b7sein", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "uns manchmal \u00fcberkommen, wie ein Wissen", "tokens": ["uns", "manch\u00b7mal", "\u00fc\u00b7ber\u00b7kom\u00b7men", ",", "wie", "ein", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von jener Wirklichkeit sich niedersenkend,", "tokens": ["von", "je\u00b7ner", "Wirk\u00b7lich\u00b7keit", "sich", "nie\u00b7der\u00b7sen\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so da\u00df wir eine Weile hingerissen", "tokens": ["so", "da\u00df", "wir", "ei\u00b7ne", "Wei\u00b7le", "hin\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das Leben spielen, nicht an Beifall denkend.", "tokens": ["das", "Le\u00b7ben", "spie\u00b7len", ",", "nicht", "an", "Bei\u00b7fall", "den\u00b7kend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}