{"textgrid.poem.63046": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst fiel dem L\u00f6wen ein, es w\u00e4re", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst fiel dem L\u00f6wen ein, es w\u00e4re", "tokens": ["Einst", "fiel", "dem", "L\u00f6\u00b7wen", "ein", ",", "es", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch gegen eines K\u00f6nigs Ehre", "tokens": ["Doch", "ge\u00b7gen", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und gegen das ", "tokens": ["Und", "ge\u00b7gen", "das"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "Da\u00df er sich selbst sein Futter schaffe.", "tokens": ["Da\u00df", "er", "sich", "selbst", "sein", "Fut\u00b7ter", "schaf\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein weises Ministerium,", "tokens": ["Sein", "wei\u00b7ses", "Mi\u00b7nis\u00b7te\u00b7ri\u00b7um", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Der Fuchs, der B\u00fcffel und der Affe,", "tokens": ["Der", "Fuchs", ",", "der", "B\u00fcf\u00b7fel", "und", "der", "Af\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Trat des Monarchen Meynung bey.", "tokens": ["Trat", "des", "Mon\u00b7ar\u00b7chen", "Mey\u00b7nung", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sogleich gebot er allen Thieren,", "tokens": ["Sog\u00b7leich", "ge\u00b7bot", "er", "al\u00b7len", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ihm einen Scho\u00df von Korn und Heu", "tokens": ["Ihm", "ei\u00b7nen", "Scho\u00df", "von", "Korn", "und", "Heu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und Wildpret j\u00e4hrlich abzuf\u00fchren.", "tokens": ["Und", "Wild\u00b7pret", "j\u00e4hr\u00b7lich", "ab\u00b7zu\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der Esel mu\u00dfte das Edict,", "tokens": ["Der", "E\u00b7sel", "mu\u00df\u00b7te", "das", "E\u00b7dict", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Als Wappenherold bunt geschm\u00fcckt,", "tokens": ["Als", "Wap\u00b7pen\u00b7he\u00b7rold", "bunt", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "An allen Ecken ausposaunen.", "tokens": ["An", "al\u00b7len", "E\u00b7cken", "aus\u00b7po\u00b7sau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das Volk vernahm es mit Erstaunen:", "tokens": ["Das", "Volk", "ver\u00b7nahm", "es", "mit", "Er\u00b7stau\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Es drang sich in Proce\u00dfion,", "tokens": ["Es", "drang", "sich", "in", "Pro\u00b7ce\u00b7\u00dfi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wie dort in Vater Noahs Kasten,", "tokens": ["Wie", "dort", "in", "Va\u00b7ter", "No\u00b7ahs", "Kas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Vereint vor des Monarchen Thron", "tokens": ["Ver\u00b7eint", "vor", "des", "Mon\u00b7ar\u00b7chen", "Thron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und wollte von den neuen Lasten", "tokens": ["Und", "woll\u00b7te", "von", "den", "neu\u00b7en", "Las\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Befreyet seyn. Der Elephant", "tokens": ["Be\u00b7fre\u00b7yet", "seyn", ".", "Der", "E\u00b7le\u00b7phant"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "VAINF", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sprach m\u00e4nnlich als Repr\u00e4sentant:", "tokens": ["Sprach", "m\u00e4nn\u00b7lich", "als", "Re\u00b7pr\u00e4\u00b7sen\u00b7tant", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wie, Herr, was konnte dich bewegen,", "tokens": ["Wie", ",", "Herr", ",", "was", "konn\u00b7te", "dich", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "PWS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Uns diese Steuer aufzulegen?", "tokens": ["Uns", "die\u00b7se", "Steu\u00b7er", "auf\u00b7zu\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Schweig, fiel ihm der Despot hier ein,", "tokens": ["Schweig", ",", "fiel", "ihm", "der", "Des\u00b7pot", "hier", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Uns K\u00f6nige darf Zevs allein", "tokens": ["Uns", "K\u00f6\u00b7ni\u00b7ge", "darf", "Zevs", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VMFIN", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Zur Rechnung ziehen. Loser Sp\u00f6tter!", "tokens": ["Zur", "Rech\u00b7nung", "zie\u00b7hen", ".", "Lo\u00b7ser", "Sp\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Versetzt der B\u00e4r, erst gestern noch", "tokens": ["Ver\u00b7setzt", "der", "B\u00e4r", ",", "erst", "ge\u00b7stern", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "ADV", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Sprachst du, es g\u00e4be keine G\u00f6tter.", "tokens": ["Sprachst", "du", ",", "es", "g\u00e4\u00b7be", "kei\u00b7ne", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Nun ward man laut. Der Menge kroch", "tokens": ["Nun", "ward", "man", "laut", ".", "Der", "Men\u00b7ge", "kroch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Das Ding zu Kopfe. Schlie\u00dflich machte", "tokens": ["Das", "Ding", "zu", "Kop\u00b7fe", ".", "Schlie\u00df\u00b7lich", "mach\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Das Volk mit reifem Vorbedachte", "tokens": ["Das", "Volk", "mit", "rei\u00b7fem", "Vor\u00b7be\u00b7dach\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Die Bill: da\u00df, weil ein Gro\u00dfsultan", "tokens": ["Die", "Bill", ":", "da\u00df", ",", "weil", "ein", "Gro\u00df\u00b7sul\u00b7tan"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KOUS", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Den h\u00f6chsten Richter unsrer Thaten", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Rich\u00b7ter", "uns\u00b7rer", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Verachten oder l\u00e4ugnen kann,", "tokens": ["Ver\u00b7ach\u00b7ten", "o\u00b7der", "l\u00e4ug\u00b7nen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Man vor der Hand den Autokraten", "tokens": ["Man", "vor", "der", "Hand", "den", "Au\u00b7to\u00b7kra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Verpflichten soll, der Nation", "tokens": ["Ver\u00b7pflich\u00b7ten", "soll", ",", "der", "Na\u00b7tion"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "$,", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.36": {"text": "Von seiner Wirtschaft auf dem Thron,", "tokens": ["Von", "sei\u00b7ner", "Wirt\u00b7schaft", "auf", "dem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Mit unter auch von seinem Leben", "tokens": ["Mit", "un\u00b7ter", "auch", "von", "sei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Genaue Rechenschaft zu geben.", "tokens": ["Ge\u00b7nau\u00b7e", "Re\u00b7chen\u00b7schaft", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}