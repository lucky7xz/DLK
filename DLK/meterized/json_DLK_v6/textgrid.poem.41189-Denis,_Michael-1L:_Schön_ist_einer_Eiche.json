{"textgrid.poem.41189": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6n ist einer Eiche", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist einer Eiche", "tokens": ["Sch\u00f6n", "ist", "ei\u00b7ner", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wolkennaher Wipfel,", "tokens": ["Wol\u00b7ken\u00b7na\u00b7her", "Wip\u00b7fel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn ihr jung' Gescho\u00df", "tokens": ["Wenn", "ihr", "jung'", "Ge\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Unter ihren Armen", "tokens": ["Un\u00b7ter", "ih\u00b7ren", "Ar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Fette Spitzen hebet;", "tokens": ["Fet\u00b7te", "Spit\u00b7zen", "he\u00b7bet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Aber sch\u00f6ner ist doch Eine noch.", "tokens": ["A\u00b7ber", "sch\u00f6\u00b7ner", "ist", "doch", "Ei\u00b7ne", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n ist in der Mitte", "tokens": ["Sch\u00f6n", "ist", "in", "der", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Silberwei\u00dfer Kinder", "tokens": ["Sil\u00b7ber\u00b7wei\u00b7\u00dfer", "Kin\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ein erhab'ner Schwan,", "tokens": ["Ein", "er\u00b7hab'\u00b7ner", "Schwan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn er auf dem Teiche", "tokens": ["Wenn", "er", "auf", "dem", "Tei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Frohbegleitet segelt:", "tokens": ["Froh\u00b7be\u00b7glei\u00b7tet", "se\u00b7gelt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Aber sch\u00f6ner ist doch Eine noch.", "tokens": ["A\u00b7ber", "sch\u00f6\u00b7ner", "ist", "doch", "Ei\u00b7ne", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n ist an des Himmels", "tokens": ["Sch\u00f6n", "ist", "an", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Blauem Nachtgesichte", "tokens": ["Blau\u00b7em", "Nacht\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "D\u00fcnsteloser Mond,", "tokens": ["D\u00fcns\u00b7te\u00b7lo\u00b7ser", "Mond", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn er unter Sternen", "tokens": ["Wenn", "er", "un\u00b7ter", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sanftbeleuchtend wandelt;", "tokens": ["Sanft\u00b7be\u00b7leuch\u00b7tend", "wan\u00b7delt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Aber sch\u00f6ner ist doch Eine noch.", "tokens": ["A\u00b7ber", "sch\u00f6\u00b7ner", "ist", "doch", "Ei\u00b7ne", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6ner in dem Kreise", "tokens": ["Sch\u00f6\u00b7ner", "in", "dem", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "G\u00f6ttergleicher Kinder", "tokens": ["G\u00f6t\u00b7ter\u00b7glei\u00b7cher", "Kin\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist Theresia;", "tokens": ["Ist", "The\u00b7re\u00b7sia", ";"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sch\u00f6ner unter S\u00f6hnen,", "tokens": ["Sch\u00f6\u00b7ner", "un\u00b7ter", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sch\u00f6ner unter T\u00f6chtern.", "tokens": ["Sch\u00f6\u00b7ner", "un\u00b7ter", "T\u00f6ch\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Weicht der Mutter, Eiche, Schwan und Mond!", "tokens": ["Weicht", "der", "Mut\u00b7ter", ",", "Ei\u00b7che", ",", "Schwan", "und", "Mond", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wie das Aug' des Tages", "tokens": ["Wie", "das", "Aug'", "des", "Ta\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auf der hellen Quelle", "tokens": ["Auf", "der", "hel\u00b7len", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Seine Schimmer sieht;", "tokens": ["Sei\u00b7ne", "Schim\u00b7mer", "sieht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Also sieht an jedem", "tokens": ["Al\u00b7so", "sieht", "an", "je\u00b7dem"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ihrer Liebespf\u00e4nder", "tokens": ["Ih\u00b7rer", "Lie\u00b7be\u00b7spf\u00e4n\u00b7der"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ihr entz\u00fcckend Bild Theresia.", "tokens": ["Ihr", "ent\u00b7z\u00fc\u00b7ckend", "Bild", "The\u00b7re\u00b7sia", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "NN", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Als Allvater ihre", "tokens": ["Als", "All\u00b7va\u00b7ter", "ih\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gabenvollen Geister", "tokens": ["Ga\u00b7ben\u00b7vol\u00b7len", "Geis\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weisheithauchend schuf,", "tokens": ["Weis\u00b7heit\u00b7hau\u00b7chend", "schuf", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Feierten die Himmel,", "tokens": ["Fei\u00b7er\u00b7ten", "die", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Und Walhallas Barden", "tokens": ["Und", "Wal\u00b7hal\u00b7las", "Bar\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sangen einen neuen Weihgesang.", "tokens": ["San\u00b7gen", "ei\u00b7nen", "neu\u00b7en", "Weih\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Als Allvater ihre", "tokens": ["Als", "All\u00b7va\u00b7ter", "ih\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Reizevollen Leiber", "tokens": ["Rei\u00b7ze\u00b7vol\u00b7len", "Lei\u00b7ber"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wonnel\u00e4chelnd schuf,", "tokens": ["Won\u00b7ne\u00b7l\u00e4\u00b7chelnd", "schuf", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Blinketen die Sonnen,", "tokens": ["Blin\u00b7ke\u00b7ten", "die", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Dufteten die Rosen,", "tokens": ["Duf\u00b7te\u00b7ten", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hub sich aller Nachtigallen Lied.", "tokens": ["Hub", "sich", "al\u00b7ler", "Nach\u00b7ti\u00b7gal\u00b7len", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Denn zum V\u00f6lkerheile,", "tokens": ["Denn", "zum", "V\u00f6l\u00b7ker\u00b7hei\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zu der Erde Freude", "tokens": ["Zu", "der", "Er\u00b7de", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schuf Allvater sie,", "tokens": ["Schuf", "All\u00b7va\u00b7ter", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Las f\u00fcr sie von allen", "tokens": ["Las", "f\u00fcr", "sie", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "APPR", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Menschenherrscherinnen", "tokens": ["Men\u00b7schen\u00b7herr\u00b7sche\u00b7rin\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nur Theresien zur Mutter aus.", "tokens": ["Nur", "The\u00b7re\u00b7si\u00b7en", "zur", "Mut\u00b7ter", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Schwer sind Mutterpflichten,", "tokens": ["Schwer", "sind", "Mut\u00b7ter\u00b7pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "F\u00fcllen ganz die Seele,", "tokens": ["F\u00fcl\u00b7len", "ganz", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie die L\u00fcfte Raum;", "tokens": ["Wie", "die", "L\u00fcf\u00b7te", "Raum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Schwer sind Herrscherpflichten,", "tokens": ["Schwer", "sind", "Herr\u00b7scher\u00b7pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Liegen auf der Seele,", "tokens": ["Lie\u00b7gen", "auf", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wie ein Berg auf Donaufl\u00e4chen liegt.", "tokens": ["Wie", "ein", "Berg", "auf", "Don\u00b7a\u00b7uf\u00b7l\u00e4\u00b7chen", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Doch im Riesenschwunge", "tokens": ["Doch", "im", "Rie\u00b7sen\u00b7schwun\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Warf sich beide Pflichten", "tokens": ["Warf", "sich", "bei\u00b7de", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Uns're F\u00fcrstinn auf.", "tokens": ["Un\u00b7s'\u00b7re", "F\u00fcrs\u00b7tinn", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wie Sie V\u00f6lker weidet,", "tokens": ["Wie", "Sie", "V\u00f6l\u00b7ker", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bildete Sie Kinder:", "tokens": ["Bil\u00b7de\u00b7te", "Sie", "Kin\u00b7der", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Folge, Feierlied, der Bildenden!", "tokens": ["Fol\u00b7ge", ",", "Fei\u00b7er\u00b7lied", ",", "der", "Bil\u00b7den\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.11": {"line.1": {"text": "Jede zarte Regung,", "tokens": ["Je\u00b7de", "zar\u00b7te", "Re\u00b7gung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die vom Herzen Ihrer", "tokens": ["Die", "vom", "Her\u00b7zen", "Ih\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "PPO