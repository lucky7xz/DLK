{"dta.poem.10526": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXXIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber die Berge steigt schon die Sonne,", "tokens": ["Ue\u00b7ber", "die", "Ber\u00b7ge", "steigt", "schon", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die L\u00e4mmerheerde l\u00e4utet fern;", "tokens": ["Die", "L\u00e4m\u00b7mer\u00b7heer\u00b7de", "l\u00e4u\u00b7tet", "fern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Liebchen, mein Lamm, meine Sonne und Wonne,", "tokens": ["Mein", "Lieb\u00b7chen", ",", "mein", "Lamm", ",", "mei\u00b7ne", "Son\u00b7ne", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Noch einmal s\u00e4h' ich dich gar zu gern!", "tokens": ["Noch", "ein\u00b7mal", "s\u00e4h'", "ich", "dich", "gar", "zu", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich schaue hinauf, mit sp\u00e4hender Miene \u2014", "tokens": ["Ich", "schau\u00b7e", "hin\u00b7auf", ",", "mit", "sp\u00e4\u00b7hen\u00b7der", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Leb' wohl, mein Kind, ich wandre von hier!", "tokens": ["Leb'", "wohl", ",", "mein", "Kind", ",", "ich", "wand\u00b7re", "von", "hier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Vergebens! Es regt sich keine Gardine; \u2014", "tokens": ["Ver\u00b7ge\u00b7bens", "!", "Es", "regt", "sich", "kei\u00b7ne", "Gar\u00b7di\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PRF", "PIAT", "NN", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie liegt noch und schl\u00e4ft, und tr\u00e4umt von mir.", "tokens": ["Sie", "liegt", "noch", "und", "schl\u00e4ft", ",", "und", "tr\u00e4umt", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "$,", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}