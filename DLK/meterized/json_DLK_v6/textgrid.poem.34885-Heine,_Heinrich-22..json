{"textgrid.poem.34885": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Michel! fallen dir die Schuppen", "tokens": ["Mi\u00b7chel", "!", "fal\u00b7len", "dir", "die", "Schup\u00b7pen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Augen? Merkst du itzt,", "tokens": ["Von", "den", "Au\u00b7gen", "?", "Merkst", "du", "itzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df man dir die besten Suppen", "tokens": ["Da\u00df", "man", "dir", "die", "bes\u00b7ten", "Sup\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem Maule wegstibitzt?", "tokens": ["Vor", "dem", "Mau\u00b7le", "weg\u00b7sti\u00b7bitzt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als Ersatz ward dir versprochen", "tokens": ["Als", "Er\u00b7satz", "ward", "dir", "ver\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reinverkl\u00e4rte Himmelsfreud'", "tokens": ["Rein\u00b7ver\u00b7kl\u00e4r\u00b7te", "Him\u00b7mels\u00b7freud'"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Droben, wo die Engel kochen", "tokens": ["Dro\u00b7ben", ",", "wo", "die", "En\u00b7gel", "ko\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Fleisch die Seligkeit!", "tokens": ["Oh\u00b7ne", "Fleisch", "die", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Michel! wird dein Glaube schw\u00e4cher", "tokens": ["Mi\u00b7chel", "!", "wird", "dein", "Glau\u00b7be", "schw\u00e4\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VAFIN", "PPOSAT", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder st\u00e4rker dein App'tit?", "tokens": ["O\u00b7der", "st\u00e4r\u00b7ker", "dein", "Ap\u00b7p'\u00b7tit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Du ergreifst den Lebensbecher,", "tokens": ["Du", "er\u00b7greifst", "den", "Le\u00b7bens\u00b7be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du singst ein Heidenlied!", "tokens": ["Und", "du", "singst", "ein", "Hei\u00b7den\u00b7lied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Michel! f\u00fcrchte nichts und labe", "tokens": ["Mi\u00b7chel", "!", "f\u00fcrch\u00b7te", "nichts", "und", "la\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PIS", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon hienieden deinen Wanst,", "tokens": ["Schon", "hien\u00b7ie\u00b7den", "dei\u00b7nen", "Wanst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sp\u00e4ter liegen wir im Grabe,", "tokens": ["Sp\u00e4\u00b7ter", "lie\u00b7gen", "wir", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo du still verdauen kannst.", "tokens": ["Wo", "du", "still", "ver\u00b7dau\u00b7en", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}