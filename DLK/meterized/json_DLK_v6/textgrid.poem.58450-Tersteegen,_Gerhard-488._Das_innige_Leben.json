{"textgrid.poem.58450": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "488. Das innige Leben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df alles \u00c4u\u00dfre gehn, la\u00df alles andre fallen,", "tokens": ["La\u00df", "al\u00b7les", "\u00c4u\u00df\u00b7re", "gehn", ",", "la\u00df", "al\u00b7les", "and\u00b7re", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "VVINF", "$,", "VVFIN", "PIS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Geist mu\u00df frei und still und unverbildet sein;", "tokens": ["Dein", "Geist", "mu\u00df", "frei", "und", "still", "und", "un\u00b7ver\u00b7bil\u00b7det", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Wollen sei in dir als Gottes Wohlgefallen,", "tokens": ["Kein", "Wol\u00b7len", "sei", "in", "dir", "als", "Got\u00b7tes", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "KOUS", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So leb beim Herzensfreund im Herzensk\u00e4mmerlein!", "tokens": ["So", "leb", "beim", "Her\u00b7zens\u00b7freund", "im", "Her\u00b7zens\u00b7k\u00e4m\u00b7mer\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}