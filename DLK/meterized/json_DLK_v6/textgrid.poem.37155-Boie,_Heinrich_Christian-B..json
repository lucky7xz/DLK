{"textgrid.poem.37155": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "B.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus elysischen Myrtenhainen,", "tokens": ["Aus", "e\u00b7ly\u00b7si\u00b7schen", "Myr\u00b7ten\u00b7hai\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wo abgeschiedne Liebende weinen,", "tokens": ["Wo", "ab\u00b7ge\u00b7schied\u00b7ne", "Lie\u00b7ben\u00b7de", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von stillen B\u00e4chen mondbegl\u00e4nzt,", "tokens": ["Von", "stil\u00b7len", "B\u00e4\u00b7chen", "mond\u00b7be\u00b7gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ewig bl\u00fchender Fr\u00fchling kr\u00e4nzt,", "tokens": ["Die", "e\u00b7wig", "bl\u00fc\u00b7hen\u00b7der", "Fr\u00fch\u00b7ling", "kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kommt mit Grabeslumpen umhangen,", "tokens": ["Kommt", "mit", "Gra\u00b7bes\u00b7lum\u00b7pen", "um\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Kommt die arme Hanne hergegangen,", "tokens": ["Kommt", "die", "ar\u00b7me", "Han\u00b7ne", "her\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Linderung wo und Ruhe zu erlangen.", "tokens": ["Lin\u00b7de\u00b7rung", "wo", "und", "Ru\u00b7he", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Ha! wie blutig, wie d\u00fcster", "tokens": ["Ha", "!", "wie", "blu\u00b7tig", ",", "wie", "d\u00fcs\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Der sinkende Mond da scheint!", "tokens": ["Der", "sin\u00b7ken\u00b7de", "Mond", "da", "scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie im Pappelgefl\u00fcster", "tokens": ["Wie", "im", "Pap\u00b7pel\u00b7ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der Nachtigall Stimme weint!", "tokens": ["Der", "Nach\u00b7ti\u00b7gall", "Stim\u00b7me", "weint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie dort am quelligen Berge,", "tokens": ["Wie", "dort", "am", "quel\u00b7li\u00b7gen", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wo Feuerw\u00fcrmer gl\u00fchn,", "tokens": ["Wo", "Feu\u00b7er\u00b7w\u00fcr\u00b7mer", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Elfen, Nixen und Zwerge", "tokens": ["Die", "El\u00b7fen", ",", "Ni\u00b7xen", "und", "Zwer\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Den ringelnden Reigen ziehn!", "tokens": ["Den", "rin\u00b7geln\u00b7den", "Rei\u00b7gen", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "In jener Primel Kelch begraben,", "tokens": ["In", "je\u00b7ner", "Pri\u00b7mel", "Kelch", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ruht Er, gek\u00fchlt von hellem Thau. \u2013", "tokens": ["Ruht", "Er", ",", "ge\u00b7k\u00fchlt", "von", "hel\u00b7lem", "Thau", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVPP", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "T\u00e4glich soll dich meine Thr\u00e4ne laben,", "tokens": ["T\u00e4g\u00b7lich", "soll", "dich", "mei\u00b7ne", "Thr\u00e4\u00b7ne", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df du nicht welkst, o Bl\u00fcmchen der Au!", "tokens": ["Da\u00df", "du", "nicht", "welkst", ",", "o", "Bl\u00fcm\u00b7chen", "der", "Au", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Denn seit man ihn begraben,", "tokens": ["Denn", "seit", "man", "ihn", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat Hanne keine Freude mehr.", "tokens": ["Hat", "Han\u00b7ne", "kei\u00b7ne", "Freu\u00b7de", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vergi\u00dfmeinnicht und Rosen will ich finden,", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "und", "Ro\u00b7sen", "will", "ich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihm einen Totenkranz zu binden,", "tokens": ["Ihm", "ei\u00b7nen", "To\u00b7ten\u00b7kranz", "zu", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von meinen Thr\u00e4nen schwer.", "tokens": ["Von", "mei\u00b7nen", "Thr\u00e4\u00b7nen", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Nicht Glockenklang,", "tokens": ["Nicht", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Nicht Grabgesang,", "tokens": ["Nicht", "Grab\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Mein Seufzer nur erschall umher.", "tokens": ["Mein", "Seuf\u00b7zer", "nur", "er\u00b7schall", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo ist der hole Baum im Hain?", "tokens": ["Wo", "ist", "der", "ho\u00b7le", "Baum", "im", "Hain", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allein will ich mir betten, allein!", "tokens": ["Al\u00b7lein", "will", "ich", "mir", "bet\u00b7ten", ",", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "$,", "ADV", "$."], "meter": "-++--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem Tode l\u00e4cheln, segnen das Verderben \u2013", "tokens": ["Dem", "To\u00b7de", "l\u00e4\u00b7cheln", ",", "seg\u00b7nen", "das", "Ver\u00b7der\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sterben.", "tokens": ["Und", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Nachtraben und Flederm\u00e4use", "tokens": ["Nach\u00b7tra\u00b7ben", "und", "Fle\u00b7der\u00b7m\u00e4u\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In banger, gr\u00e4ulicher Weise,", "tokens": ["In", "ban\u00b7ger", ",", "gr\u00e4u\u00b7li\u00b7cher", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Uhu und Eulen", "tokens": ["U\u00b7hu", "und", "Eu\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sollen mein Grablied heulen. \u2013", "tokens": ["Sol\u00b7len", "mein", "Grab\u00b7lied", "heu\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Sahet ihr ihn?", "tokens": ["Sa\u00b7het", "ihr", "ihn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wie die schwarzen Augen ihm gl\u00fchn?", "tokens": ["Wie", "die", "schwar\u00b7zen", "Au\u00b7gen", "ihm", "gl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "O f\u00fcrchtet, M\u00e4dchen, ihre Macht!", "tokens": ["O", "f\u00fcrch\u00b7tet", ",", "M\u00e4d\u00b7chen", ",", "ih\u00b7re", "Macht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nehmt euer Herz in Acht!", "tokens": ["Nehmt", "eu\u00b7er", "Herz", "in", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie w\u00fcrdet ihr dem Mann entfliehn? \u2013", "tokens": ["Wie", "w\u00fcr\u00b7det", "ihr", "dem", "Mann", "ent\u00b7fliehn", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Horch, horch! Aus Moderduft", "tokens": ["Horch", ",", "horch", "!", "Aus", "Mo\u00b7der\u00b7duft"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVIMP", "$.", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der alte Charon ruft!", "tokens": ["Der", "al\u00b7te", "Cha\u00b7ron", "ruft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mit Gei\u00dfeln nahm die Erynnen", "tokens": ["Mit", "Gei\u00b7\u00dfeln", "nahm", "die", "E\u00b7ryn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und rufen: von hinnen, von hinnen! \u2013", "tokens": ["Und", "ru\u00b7fen", ":", "von", "hin\u00b7nen", ",", "von", "hin\u00b7nen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "APPR", "ADV", "$,", "APPR", "ADV", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "So kehr ich denn, woher ich kam.", "tokens": ["So", "kehr", "ich", "denn", ",", "wo\u00b7her", "ich", "kam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ist viel zu toll: nichts lindert meinen Gram.", "tokens": ["Die", "Welt", "ist", "viel", "zu", "toll", ":", "nichts", "lin\u00b7dert", "mei\u00b7nen", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$.", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was sollt ich l\u00e4nger umsonst hier schmachten?", "tokens": ["Was", "sollt", "ich", "l\u00e4n\u00b7ger", "um\u00b7sonst", "hier", "schmach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Lieb' ist alles Elends Same,", "tokens": ["Die", "Lieb'", "ist", "al\u00b7les", "E\u00b7lends", "Sa\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist Seifenblas' und Schatten und Name,", "tokens": ["Ist", "Sei\u00b7fen\u00b7blas'", "und", "Schat\u00b7ten", "und", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Narren bewundern und Weise verachten.", "tokens": ["Den", "Nar\u00b7ren", "be\u00b7wun\u00b7dern", "und", "Wei\u00b7se", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Kalt und hungrig bin ich nun.", "tokens": ["Kalt", "und", "hung\u00b7rig", "bin", "ich", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Blumen will ich ruhn,", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "will", "ich", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4umend auf sonnige Rasen sinken,", "tokens": ["Tr\u00e4u\u00b7mend", "auf", "son\u00b7ni\u00b7ge", "Ra\u00b7sen", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ambrosia kosten und Nektar trinken", "tokens": ["A\u00b7mbro\u00b7sia", "kos\u00b7ten", "und", "Nek\u00b7tar", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "NN", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und singen:", "tokens": ["Und", "sin\u00b7gen", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wer heiter ist und froh,", "tokens": ["Wer", "hei\u00b7ter", "ist", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kann jeden Gram bezwingen.", "tokens": ["Kann", "je\u00b7den", "Gram", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bei Wa\u0