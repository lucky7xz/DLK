{"textgrid.poem.39207": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe den Narren oft gemacht", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe den Narren oft gemacht", "tokens": ["Ich", "ha\u00b7be", "den", "Nar\u00b7ren", "oft", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit wunder Brust, mit wunder Brust;", "tokens": ["Mit", "wun\u00b7der", "Brust", ",", "mit", "wun\u00b7der", "Brust", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe am Tage laut gelacht", "tokens": ["Ich", "ha\u00b7be", "am", "Ta\u00b7ge", "laut", "ge\u00b7lacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADJD", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und habe geweint in stiller Nacht,", "tokens": ["Und", "ha\u00b7be", "ge\u00b7weint", "in", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und keiner hat mein Leid gewu\u00dft.", "tokens": ["Und", "kei\u00b7ner", "hat", "mein", "Leid", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich habe die Lippe gezogen krumm", "tokens": ["Ich", "ha\u00b7be", "die", "Lip\u00b7pe", "ge\u00b7zo\u00b7gen", "krumm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In frischem Spott, in frischem Spott;", "tokens": ["In", "fri\u00b7schem", "Spott", ",", "in", "fri\u00b7schem", "Spott", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wenn der sonnige Tag herum,", "tokens": ["Doch", "wenn", "der", "son\u00b7ni\u00b7ge", "Tag", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da bin ich zusammengebrochen stumm", "tokens": ["Da", "bin", "ich", "zu\u00b7sam\u00b7men\u00b7ge\u00b7bro\u00b7chen", "stumm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und bin verzweifelt an mir und Gott!", "tokens": ["Und", "bin", "ver\u00b7zwei\u00b7felt", "an", "mir", "und", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "APPR", "PPER", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Ihr wi\u00dft, Ihr ahnt es nimmermehr,", "tokens": ["Ihr", "wi\u00dft", ",", "Ihr", "ahnt", "es", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ein Herze tr\u00e4gt, was ein Herze tr\u00e4gt;", "tokens": ["Was", "ein", "Her\u00b7ze", "tr\u00e4gt", ",", "was", "ein", "Her\u00b7ze", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr t\u00e4ndelt \u00fcber die Woge her,", "tokens": ["Ihr", "t\u00e4n\u00b7delt", "\u00fc\u00b7ber", "die", "Wo\u00b7ge", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch was in der Tiefe braut das Meer,", "tokens": ["Doch", "was", "in", "der", "Tie\u00b7fe", "braut", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das l\u00e4\u00dft Euch ewig unbewegt!", "tokens": ["Das", "l\u00e4\u00dft", "Euch", "e\u00b7wig", "un\u00b7be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr wollt von mir ein Lied, ein Lied", "tokens": ["Ihr", "wollt", "von", "mir", "ein", "Lied", ",", "ein", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom goldnen Mai, vom goldnen Mai,", "tokens": ["Vom", "gold\u00b7nen", "Mai", ",", "vom", "gold\u00b7nen", "Mai", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich greife zur Harfe tr\u00fcb' und m\u00fcd';", "tokens": ["Ich", "grei\u00b7fe", "zur", "Har\u00b7fe", "tr\u00fcb'", "und", "m\u00fcd'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Jugend leuchtet, das Leben bl\u00fcht,", "tokens": ["Die", "Ju\u00b7gend", "leuch\u00b7tet", ",", "das", "Le\u00b7ben", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und ich wollte herzlich, es w\u00e4r' vorbei!", "tokens": ["Und", "ich", "woll\u00b7te", "herz\u00b7lich", ",", "es", "w\u00e4r'", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "$,", "PPER", "VAFIN", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}