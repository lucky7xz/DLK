{"textgrid.poem.36836": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freunde glaubt' ich im Vaterland nur einen,", "tokens": ["Freun\u00b7de", "glaubt'", "ich", "im", "Va\u00b7ter\u00b7land", "nur", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ART", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dich zu haben, o gro\u00dfes Herz. Der Jugend", "tokens": ["Dich", "zu", "ha\u00b7ben", ",", "o", "gro\u00b7\u00dfes", "Herz", ".", "Der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKZU", "VAINF", "$,", "FM", "ADJA", "NN", "$.", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Irrthum deutet die Welt zu schwer, und wenig", "tokens": ["Irr\u00b7thum", "deu\u00b7tet", "die", "Welt", "zu", "schwer", ",", "und", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKA", "ADJD", "$,", "KON", "PIS"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wird, wer gr\u00f6\u00dfer als sie, erkannt. O Alles,", "tokens": ["Wird", ",", "wer", "gr\u00f6\u00b7\u00dfer", "als", "sie", ",", "er\u00b7kannt", ".", "O", "Al\u00b7les", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "ADJD", "KOKOM", "PPER", "$,", "VVPP", "$.", "NE", "PIS", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Alles that sie, da\u00df ich sie ha\u00dft', und dennoch", "tokens": ["Al\u00b7les", "that", "sie", ",", "da\u00df", "ich", "sie", "ha\u00dft'", ",", "und", "den\u00b7noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "KON", "ADV"], "meter": "+-+-+--++-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Mit verh\u00fclltem Gesicht und feuchten Augen", "tokens": ["Mit", "ver\u00b7h\u00fcll\u00b7tem", "Ge\u00b7sicht", "und", "feuch\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Von mir sto\u00dfend, was sie mir gab, begann ich", "tokens": ["Von", "mir", "sto\u00b7\u00dfend", ",", "was", "sie", "mir", "gab", ",", "be\u00b7gann", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "ADJD", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Die Verbannung; und mich nur, meiner Feinde", "tokens": ["Die", "Ver\u00b7ban\u00b7nung", ";", "und", "mich", "nur", ",", "mei\u00b7ner", "Fein\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "PPER", "ADV", "$,", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Grimm und h\u00e4mischen Neid, nicht dich anklagend,", "tokens": ["Grimm", "und", "h\u00e4\u00b7mi\u00b7schen", "Neid", ",", "nicht", "dich", "an\u00b7kla\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$,", "PTKNEG", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Heimath, pilgert' ich in ersehnte Lande,", "tokens": ["Hei\u00b7math", ",", "pil\u00b7gert'", "ich", "in", "er\u00b7sehn\u00b7te", "Lan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Jung wie Konradin noch, wie er der Hoffnung", "tokens": ["Jung", "wie", "Kon\u00b7ra\u00b7din", "noch", ",", "wie", "er", "der", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "NE", "ADV", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Und hochherzigen Muthes voll, im Kampfe", "tokens": ["Und", "hoch\u00b7her\u00b7zi\u00b7gen", "Mu\u00b7thes", "voll", ",", "im", "Kamp\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJD", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Mit dem Kinde der Nacht, dem stolzen Priester.", "tokens": ["Mit", "dem", "Kin\u00b7de", "der", "Nacht", ",", "dem", "stol\u00b7zen", "Pries\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Mag anma\u00dfender Geistesdruck und Bl\u00f6dsinn,", "tokens": ["Mag", "an\u00b7ma\u00b7\u00dfen\u00b7der", "Geis\u00b7tes\u00b7druck", "und", "Bl\u00f6d\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Mag, o Freunde, der Ghibelline siegen,", "tokens": ["Mag", ",", "o", "Freun\u00b7de", ",", "der", "Ghi\u00b7bel\u00b7li\u00b7ne", "sie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "FM", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "La\u00dft uns streiten! Der Lohn ist eine Krone!", "tokens": ["La\u00dft", "uns", "strei\u00b7ten", "!", "Der", "Lohn", "ist", "ei\u00b7ne", "Kro\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "So oft denk' ich auf meerumsp\u00fcltem Felsen,", "tokens": ["So", "oft", "denk'", "ich", "auf", "meer\u00b7um\u00b7sp\u00fcl\u00b7tem", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So im Hause des Tasso, da dem Dichter", "tokens": ["So", "im", "Hau\u00b7se", "des", "Tas\u00b7so", ",", "da", "dem", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Vom Balkone herab des Golfes Anmuth", "tokens": ["Vom", "Bal\u00b7ko\u00b7ne", "her\u00b7ab", "des", "Gol\u00b7fes", "An\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und der Liebreiz der Berge sich entfaltet.", "tokens": ["Und", "der", "Lieb\u00b7reiz", "der", "Ber\u00b7ge", "sich", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Lorbeerheiliges Haus, wo oft im Dufte", "tokens": ["Lor\u00b7beer\u00b7hei\u00b7li\u00b7ges", "Haus", ",", "wo", "oft", "im", "Duf\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADV", "APPRART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Fremder Sieg' und Triumphe sich zum eignen", "tokens": ["Frem\u00b7der", "Sieg'", "und", "Tri\u00b7um\u00b7phe", "sich", "zum", "eig\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "PRF", "APPRART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Volksbegeisternden Lied mein Herz ermuthigt.", "tokens": ["Volks\u00b7be\u00b7geis\u00b7tern\u00b7den", "Lied", "mein", "Herz", "er\u00b7mut\u00b7higt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Freund, wohl wei\u00df ich, den Hohenstaufen schm\u00fcckte", "tokens": ["Freund", ",", "wohl", "wei\u00df", "ich", ",", "den", "Ho\u00b7hen\u00b7stau\u00b7fen", "schm\u00fcck\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Schon im zwanzigsten Jahr die K\u00f6nigskrone;", "tokens": ["Schon", "im", "zwan\u00b7zigs\u00b7ten", "Jahr", "die", "K\u00f6\u00b7nigs\u00b7kro\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.10": {"text": "F\u00fcnf der Lustern durchlebt' ich bald, und ruhmlos", "tokens": ["F\u00fcnf", "der", "Lus\u00b7tern", "durch\u00b7lebt'", "ich", "bald", ",", "und", "ruh\u00b7mlos"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["CARD", "ART", "NN", "VVFIN", "PPER", "ADV", "$,", "KON", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Bin ich noch!", "tokens": ["Bin", "ich", "noch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Und in tiefster Seele f\u00fchl' ich", "tokens": ["Und", "in", "tiefs\u00b7ter", "See\u00b7le", "f\u00fchl'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich betr\u00fcbt. O was that ich, euch zu preisen,", "tokens": ["Mich", "be\u00b7tr\u00fcbt", ".", "O", "was", "that", "ich", ",", "euch", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ITJ", "PWS", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Im gewalt'gen Gesang die deutsche Vorwelt", "tokens": ["Im", "ge\u00b7walt'\u00b7gen", "Ge\u00b7sang", "die", "deut\u00b7sche", "Vor\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Als ein Deutscher und Kampf und Herrschergenius,", "tokens": ["Als", "ein", "Deut\u00b7scher", "und", "Kampf", "und", "Herr\u00b7scher\u00b7ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "NN", "KON", "NE", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wahrheit, Kraft und des V\u00f6lkerlebens Gr\u00f6\u00dfe,", "tokens": ["Wahr\u00b7heit", ",", "Kraft", "und", "des", "V\u00f6l\u00b7ker\u00b7le\u00b7bens", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Hohe Menschen und Thaten zu verew'gen?", "tokens": ["Ho\u00b7he", "Men\u00b7schen", "und", "Tha\u00b7ten", "zu", "ver\u00b7ew'\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Denn im Tempel der Weltgeschichte, d\u00fcnkt mir,", "tokens": ["Denn", "im", "Tem\u00b7pel", "der", "Welt\u00b7ge\u00b7schich\u00b7te", ",", "d\u00fcnkt", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ist der Dichter der Priester, und den Vorhang", "tokens": ["Ist", "der", "Dich\u00b7ter", "der", "Pries\u00b7ter", ",", "und", "den", "Vor\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Vor dem Heiligsten wahret seine Obhut.", "tokens": ["Vor", "dem", "Hei\u00b7ligs\u00b7ten", "wah\u00b7ret", "sei\u00b7ne", "Ob\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "---+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Da, wenn oft mir die Scham die Stirne r\u00f6thet,", "tokens": ["Da", ",", "wenn", "oft", "mir", "die", "Scham", "die", "Stir\u00b7ne", "r\u00f6\u00b7thet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Ruf' ich flehend Torquato's Genius, ruf' ich", "tokens": ["Ruf'", "ich", "fle\u00b7hend", "Tor\u00b7qua\u00b7to's", "Ge\u00b7nius", ",", "ruf'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "NE", "NE", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Meinen Helden, und siehe, er naht mir langsam", "tokens": ["Mei\u00b7nen", "Hel\u00b7den", ",", "und", "sie\u00b7he", ",", "er", "naht", "mir", "lang\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVIMP", "$,", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Aus des Lorbeers Umschattungen, der J\u00fcngling,", "tokens": ["Aus", "des", "Lor\u00b7beers", "Um\u00b7schat\u00b7tun\u00b7gen", ",", "der", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Friedrich's Sohn, der apul'sche K\u00f6nig naht mir,", "tokens": ["Fried\u00b7rich's", "Sohn", ",", "der", "a\u00b7pul'\u00b7sche", "K\u00f6\u00b7nig", "naht", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Sch\u00f6n und fr\u00f6hlich, wie einst, da er Epirus'", "tokens": ["Sch\u00f6n", "und", "fr\u00f6h\u00b7lich", ",", "wie", "einst", ",", "da", "er", "E\u00b7pi\u00b7rus'"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "$,", "PWAV", "ADV", "$,", "KOUS", "PPER", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Tochter, Helena, mit des Vaters Kraft und", "tokens": ["Toch\u00b7ter", ",", "He\u00b7le\u00b7na", ",", "mit", "des", "Va\u00b7ters", "Kraft", "und"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "APPR", "ART", "NN", "NN", "KON"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.17": {"text": "Hohenstaufischem Arm als Braut umfangen,", "tokens": ["Ho\u00b7hen\u00b7stau\u00b7fi\u00b7schem", "Arm", "als", "Braut", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KOUS", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Minnes\u00e4nger und saracen'sche M\u00e4dchen", "tokens": ["Min\u00b7ne\u00b7s\u00e4n\u00b7ger", "und", "sa\u00b7ra\u00b7cen'\u00b7sche", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Einst den Dichter, den K\u00f6nig, einst das junge", "tokens": ["Einst", "den", "Dich\u00b7ter", ",", "den", "K\u00f6\u00b7nig", ",", "einst", "das", "jun\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,", "ADV", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Liebensw\u00fcrdigste Paar mit Jubel gr\u00fc\u00dften!", "tokens": ["Lie\u00b7bens\u00b7w\u00fcr\u00b7digs\u00b7te", "Paar", "mit", "Ju\u00b7bel", "gr\u00fc\u00df\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.21": {"text": "Aber gro\u00df und gebietrisch, wie das Erbland", "tokens": ["A\u00b7ber", "gro\u00df", "und", "ge\u00b7bie\u00b7trisch", ",", "wie", "das", "Er\u00b7bland"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Tausendj\u00e4hrigem Vorurtheil und Wahnwitz,", "tokens": ["Tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gem", "Vor\u00b7urt\u00b7heil", "und", "Wahn\u00b7witz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Und Roms heil'gen Tyrannen er entrissen,", "tokens": ["Und", "Roms", "heil'\u00b7gen", "Ty\u00b7ran\u00b7nen", "er", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.24": {"text": "Wie er einst mit dem Schwerdt der fr\u00e4nk'schen R\u00e4uber", "tokens": ["Wie", "er", "einst", "mit", "dem", "Schwerdt", "der", "fr\u00e4nk'\u00b7schen", "R\u00e4u\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.25": {"text": "S