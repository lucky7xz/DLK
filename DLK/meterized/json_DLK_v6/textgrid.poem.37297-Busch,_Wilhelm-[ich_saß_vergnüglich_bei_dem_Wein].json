{"textgrid.poem.37297": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[ich sa\u00df vergn\u00fcglich bei dem Wein]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df vergn\u00fcglich bei dem Wein", "tokens": ["Ich", "sa\u00df", "ver\u00b7gn\u00fcg\u00b7lich", "bei", "dem", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schenkte eben wieder ein.", "tokens": ["Und", "schenk\u00b7te", "e\u00b7ben", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf einmal fuhr mir in die Zeh", "tokens": ["Auf", "ein\u00b7mal", "fuhr", "mir", "in", "die", "Zeh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein sonderbar pikantes Weh.", "tokens": ["Ein", "son\u00b7der\u00b7bar", "pi\u00b7kan\u00b7tes", "Weh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich schob mein Glas sogleich beiseit", "tokens": ["Ich", "schob", "mein", "Glas", "sog\u00b7leich", "bei\u00b7seit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hinkte in die Einsamkeit", "tokens": ["Und", "hink\u00b7te", "in", "die", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wu\u00dfte, was ich nicht gewu\u00dft:", "tokens": ["Und", "wu\u00df\u00b7te", ",", "was", "ich", "nicht", "ge\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Schmerz ist Herr und Sklavin ist die Lust.", "tokens": ["Der", "Schmerz", "ist", "Herr", "und", "Skla\u00b7vin", "ist", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}