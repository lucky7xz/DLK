{"textgrid.poem.50634": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6nheit ohne Verstand", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts, als nur falsche M\u00fcnz' ist Sch\u00f6nheit ohne Witz;", "tokens": ["Nichts", ",", "als", "nur", "fal\u00b7sche", "M\u00fcnz'", "ist", "Sch\u00f6n\u00b7heit", "oh\u00b7ne", "Witz", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ADV", "ADJA", "NN", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn das Gepr\u00e4g' ist gut, doch ist das Erz nichts n\u00fctz'.", "tokens": ["Denn", "das", "Ge\u00b7pr\u00e4g'", "ist", "gut", ",", "doch", "ist", "das", "Erz", "nichts", "n\u00fctz'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "VAFIN", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}