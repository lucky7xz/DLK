{"textgrid.poem.40660": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Arm zuckt fiebernd auf der Seidendecke:", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Arm zuckt fiebernd auf der Seidendecke:", "tokens": ["Dein", "Arm", "zuckt", "fie\u00b7bernd", "auf", "der", "Sei\u00b7den\u00b7de\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er sucht den Reisestab, so will's mir scheinen,", "tokens": ["Er", "sucht", "den", "Rei\u00b7se\u00b7stab", ",", "so", "will's", "mir", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df die Zeit der Kleinen und Gemeinen", "tokens": ["Und", "da\u00df", "die", "Zeit", "der", "Klei\u00b7nen", "und", "Ge\u00b7mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Wanderlust der Gro\u00dfen, Edeln wecke.", "tokens": ["Die", "Wan\u00b7der\u00b7lust", "der", "Gro\u00b7\u00dfen", ",", "E\u00b7deln", "we\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie bl\u00e4hn sich hoch die erst so winzig Kleinen,", "tokens": ["Wie", "bl\u00e4hn", "sich", "hoch", "die", "erst", "so", "win\u00b7zig", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADJD", "ART", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie klingt der erst so Zahmen Wort so kecke,", "tokens": ["Wie", "klingt", "der", "erst", "so", "Zah\u00b7men", "Wort", "so", "ke\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADV", "ADV", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Scheintodte springen dreist aus dem Verstecke,", "tokens": ["Schein\u00b7tod\u00b7te", "sprin\u00b7gen", "dreist", "aus", "dem", "Ver\u00b7ste\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Seit sie geb\u00e4ndigt die Unb\u00e4nd'gen meinen.", "tokens": ["Seit", "sie", "ge\u00b7b\u00e4n\u00b7digt", "die", "Un\u00b7b\u00e4n\u00b7d'\u00b7gen", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Vergrabnen Truh'n entsteigt in welken Flittern", "tokens": ["Ver\u00b7grab\u00b7nen", "Truh'n", "ent\u00b7steigt", "in", "wel\u00b7ken", "Flit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Manch abgestreifter Balg von M\u00f6nchen, Rittern; \u2013", "tokens": ["Manch", "ab\u00b7ge\u00b7streif\u00b7ter", "Balg", "von", "M\u00f6n\u00b7chen", ",", "Rit\u00b7tern", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gelernt, vergessen nichts! gleich jenen Andern.", "tokens": ["Ge\u00b7lernt", ",", "ver\u00b7ges\u00b7sen", "nichts", "!", "gleich", "je\u00b7nen", "An\u00b7dern", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PIS", "$.", "ADV", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo Unkensang sich mengt dem Wolfsgeheule,", "tokens": ["Wo", "Un\u00b7ken\u00b7sang", "sich", "mengt", "dem", "Wolfs\u00b7ge\u00b7heu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und in den Wipfeln Kukuk thront und Eule,", "tokens": ["Und", "in", "den", "Wip\u00b7feln", "Ku\u00b7kuk", "thront", "und", "Eu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "KON", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da m\u00fcssen Nachtigall und Adler wandern.", "tokens": ["Da", "m\u00fcs\u00b7sen", "Nach\u00b7ti\u00b7gall", "und", "Ad\u00b7ler", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}