{"textgrid.poem.36342": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Frankenberg bei Aachen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In des Maien linden Tagen", "tokens": ["In", "des", "Mai\u00b7en", "lin\u00b7den", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt' ich die alte Sage,", "tokens": ["H\u00f6rt'", "ich", "die", "al\u00b7te", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort wo bei warmen Quellen", "tokens": ["Dort", "wo", "bei", "war\u00b7men", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sanften H\u00fcgel gr\u00fcnend schwellen,", "tokens": ["Die", "sanf\u00b7ten", "H\u00fc\u00b7gel", "gr\u00fc\u00b7nend", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von dem Wunderringe,", "tokens": ["Von", "dem", "Wun\u00b7der\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Der Kaiser Karol konnte zwingen,", "tokens": ["Der", "Kai\u00b7ser", "Ka\u00b7rol", "konn\u00b7te", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In Lieb' ihn binden,", "tokens": ["In", "Lieb'", "ihn", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Da\u00df er nach Aachens heitern Gr\u00fcnden", "tokens": ["Da\u00df", "er", "nach", "Aa\u00b7chens", "hei\u00b7tern", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sich wie zur Heimat sehnte,", "tokens": ["Sich", "wie", "zur", "Hei\u00b7mat", "sehn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So weit sein Reich sich dehnte,", "tokens": ["So", "weit", "sein", "Reich", "sich", "dehn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Vor allen Burgen, Landen,", "tokens": ["Vor", "al\u00b7len", "Bur\u00b7gen", ",", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Gebunden hier, wo s\u00fc\u00dfe Lieb' ihn bannte.", "tokens": ["Ge\u00b7bun\u00b7den", "hier", ",", "wo", "s\u00fc\u00b7\u00dfe", "Lieb'", "ihn", "bann\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Spiegelhelle Seen,", "tokens": ["Spie\u00b7gel\u00b7hel\u00b7le", "Seen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ringsum die B\u00fcsche stehen", "tokens": ["Ring\u00b7sum", "die", "B\u00fc\u00b7sche", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sah ich auf der H\u00fcgel R\u00fccken,", "tokens": ["Sah", "ich", "auf", "der", "H\u00fc\u00b7gel", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo zwischen G\u00e4ngen, kleinen Br\u00fccken,", "tokens": ["Wo", "zwi\u00b7schen", "G\u00e4n\u00b7gen", ",", "klei\u00b7nen", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "B\u00e4che durch den Wiesengrund hinfliehen,", "tokens": ["B\u00e4\u00b7che", "durch", "den", "Wie\u00b7sen\u00b7grund", "hin\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schw\u00e4ne auf den stillen Wassern ziehen,", "tokens": ["Schw\u00e4\u00b7ne", "auf", "den", "stil\u00b7len", "Was\u00b7sern", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "K\u00fchl' und warme Wellen", "tokens": ["K\u00fchl'", "und", "war\u00b7me", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Aus einem Boden quellen,", "tokens": ["Aus", "ei\u00b7nem", "Bo\u00b7den", "quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Kinder an dem Brunnen spielen,", "tokens": ["Kin\u00b7der", "an", "dem", "Brun\u00b7nen", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die laue Luft so lind zu f\u00fchlen.", "tokens": ["Die", "lau\u00b7e", "Luft", "so", "lind", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dort wo sich die Mauern zeigen,", "tokens": ["Dort", "wo", "sich", "die", "Mau\u00b7ern", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.12": {"text": "Tr\u00fcmmer aus dem See aufsteigen,", "tokens": ["Tr\u00fcm\u00b7mer", "aus", "dem", "See", "auf\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Von gr\u00fcnem Schilf und Moos umgeben;", "tokens": ["Von", "gr\u00fc\u00b7nem", "Schilf", "und", "Moos", "um\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da hat das Wunder sich begeben,", "tokens": ["Da", "hat", "das", "Wun\u00b7der", "sich", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df durch mag'sche Kraft gebunden,", "tokens": ["Da\u00df", "durch", "mag'\u00b7sche", "Kraft", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Karl nicht eher Ruh' gefunden,", "tokens": ["Karl", "nicht", "e\u00b7her", "Ruh'", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Wie alte Sage uns berichtet,", "tokens": ["Wie", "al\u00b7te", "Sa\u00b7ge", "uns", "be\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Bis er hier die Burg errichtet,", "tokens": ["Bis", "er", "hier", "die", "Burg", "er\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Wovon die Spur wir froh noch schauen,", "tokens": ["Wo\u00b7von", "die", "Spur", "wir", "froh", "noch", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Jedweden Fr\u00fchling in den stillen Auen.", "tokens": ["Jed\u00b7we\u00b7den", "Fr\u00fch\u00b7ling", "in", "den", "stil\u00b7len", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "In s\u00fc\u00dfer Lust gefangen,", "tokens": ["In", "s\u00fc\u00b7\u00dfer", "Lust", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den sehnenden Schmerzen nachzuhangen,", "tokens": ["Den", "seh\u00b7nen\u00b7den", "Schmer\u00b7zen", "nach\u00b7zu\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bezaubert alle Sinne,", "tokens": ["Be\u00b7zau\u00b7bert", "al\u00b7le", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zwingt Karlen holde Minne,", "tokens": ["Zwingt", "Kar\u00b7len", "hol\u00b7de", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem tiefen Sehnen sich ergebend,", "tokens": ["Dem", "tie\u00b7fen", "Seh\u00b7nen", "sich", "er\u00b7ge\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Einzig sein Leben liebend, in Liebe lebend.", "tokens": ["Ein\u00b7zig", "sein", "Le\u00b7ben", "lie\u00b7bend", ",", "in", "Lie\u00b7be", "le\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "ADJD", "$,", "APPR", "NN", "ADJD", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Doch nimmer ward noch Minne", "tokens": ["Doch", "nim\u00b7mer", "ward", "noch", "Min\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Selig der sel'gen Sch\u00e4tze inne.", "tokens": ["Se\u00b7lig", "der", "sel'\u00b7gen", "Sch\u00e4t\u00b7ze", "in\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Tod will mit Minne streiten,", "tokens": ["Tod", "will", "mit", "Min\u00b7ne", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ein bittres Ende s\u00fc\u00dfer Lust bereiten,", "tokens": ["Ein", "bitt\u00b7res", "En\u00b7de", "s\u00fc\u00b7\u00dfer", "Lust", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So mu\u00df auch Karles Herz vergehen,", "tokens": ["So", "mu\u00df", "auch", "Kar\u00b7les", "Herz", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Huldin sterben sehen.", "tokens": ["Die", "Hul\u00b7din", "ster\u00b7ben", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auch tot noch will er sich von ihr nicht trennen,", "tokens": ["Auch", "tot", "noch", "will", "er", "sich", "von", "ihr", "nicht", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VMFIN", "PPER", "PRF", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4hnt, das sie wieder ihn wird kennen.", "tokens": ["W\u00e4hnt", ",", "das", "sie", "wie\u00b7der", "ihn", "wird", "ken\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "ADV", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Grabmal zu durchschauen,", "tokens": ["Das", "Grab\u00b7mal", "zu", "durch\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "L\u00e4\u00dft er von Glas den Sarg erbauen,", "tokens": ["L\u00e4\u00dft", "er", "von", "Glas", "den", "Sarg", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und br\u00fcnstig noch zu lieben", "tokens": ["Und", "br\u00fcns\u00b7tig", "noch", "zu", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den s\u00fc\u00dfen K\u00f6rper f\u00fchlt er sich getrieben.", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "K\u00f6r\u00b7per", "f\u00fchlt", "er", "sich", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "An dem Sarge festgebunden,", "tokens": ["An", "dem", "Sar\u00b7ge", "fest\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schwinden ihm die schnellen Stunden.", "tokens": ["Schwin\u00b7den", "ihm", "die", "schnel\u00b7len", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nicht Durst noch Hunger f\u00fchlend,", "tokens": ["Nicht", "Durst", "noch", "Hun\u00b7ger", "f\u00fch\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Spricht er mit seinem Schmerz nur spielend.", "tokens": ["Spricht", "er", "mit", "sei\u00b7nem", "Schmerz", "nur", "spie\u00b7lend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Diener sehn mit Trauern", "tokens": ["Die", "Die\u00b7ner", "sehn", "mit", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Immer den wilden Wahn noch dauern;", "tokens": ["Im\u00b7mer", "den", "wil\u00b7den", "Wahn", "noch", "dau\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Da naht Turpin der Weise,", "tokens": ["Da", "naht", "Tur\u00b7pin", "der", "Wei\u00b7se", ","], "token_info": ["word",