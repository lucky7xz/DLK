": ["Das", "frei", "nun", "wird", "von", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Der Zauber ist verschwunden,", "tokens": ["Der", "Zau\u00b7ber", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Von dem Wahn entbunden,", "tokens": ["Von", "dem", "Wahn", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Will Karl schon entfliehen,", "tokens": ["Will", "Karl", "schon", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Einsam auf Berge ziehen.", "tokens": ["Ein\u00b7sam", "auf", "Ber\u00b7ge", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.25": {"text": "Da sieht er stille Seen", "tokens": ["Da", "sieht", "er", "stil\u00b7le", "Seen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Vor seinen Augen stehen.", "tokens": ["Vor", "sei\u00b7nen", "Au\u00b7gen", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Sind die Schmerzen gleich verschwunden,", "tokens": ["Sind", "die", "Schmer\u00b7zen", "gleich", "ver\u00b7schwun\u00b7den", 