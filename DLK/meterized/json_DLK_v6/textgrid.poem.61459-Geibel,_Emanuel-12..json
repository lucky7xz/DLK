{"textgrid.poem.61459": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der Liebe eigen,", "tokens": ["Das", "ist", "der", "Lie\u00b7be", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Worten mu\u00df sie schweigen;", "tokens": ["Mit", "Wor\u00b7ten", "mu\u00df", "sie", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie spricht mit s\u00fc\u00dfen Zeichen", "tokens": ["Sie", "spricht", "mit", "s\u00fc\u00b7\u00dfen", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Dingen ohne Gleichen.", "tokens": ["Von", "Din\u00b7gen", "oh\u00b7ne", "Glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es sagt die Hand am Herzen:", "tokens": ["Es", "sagt", "die", "Hand", "am", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier innen trag' ich Schmerzen", "tokens": ["Hier", "in\u00b7nen", "trag'", "ich", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und m\u00f6chte doch dies Leiden", "tokens": ["Und", "m\u00f6ch\u00b7te", "doch", "dies", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PDS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um alle Welt nicht meiden.", "tokens": ["Um", "al\u00b7le", "Welt", "nicht", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im Auge spricht die Tr\u00e4ne:", "tokens": ["Im", "Au\u00b7ge", "spricht", "die", "Tr\u00e4\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ich nach dir mich sehne!", "tokens": ["Wie", "ich", "nach", "dir", "mich", "seh\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Wollen, Denken, Sinnen,", "tokens": ["Mein", "Wol\u00b7len", ",", "Den\u00b7ken", ",", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es will in deins verrinnen.", "tokens": ["Es", "will", "in", "deins", "ver\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es spricht der Lippe Z\u00fccken:", "tokens": ["Es", "spricht", "der", "Lip\u00b7pe", "Z\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O la\u00df dich an mich dr\u00fccken,", "tokens": ["O", "la\u00df", "dich", "an", "mich", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf da\u00df im Feuerhauche", "tokens": ["Auf", "da\u00df", "im", "Feu\u00b7er\u00b7hau\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich Seel' in Seele tauche!", "tokens": ["Sich", "Seel'", "in", "See\u00b7le", "tau\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So webt in stummen Zeichen", "tokens": ["So", "webt", "in", "stum\u00b7men", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich Botschaft sonder Gleichen;", "tokens": ["Sich", "Bot\u00b7schaft", "son\u00b7der", "Glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Herz zu Herzen geht sie,", "tokens": ["Von", "Herz", "zu", "Her\u00b7zen", "geht", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch nur, wer liebt, versteht sie.", "tokens": ["Doch", "nur", ",", "wer", "liebt", ",", "ver\u00b7steht", "sie", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}