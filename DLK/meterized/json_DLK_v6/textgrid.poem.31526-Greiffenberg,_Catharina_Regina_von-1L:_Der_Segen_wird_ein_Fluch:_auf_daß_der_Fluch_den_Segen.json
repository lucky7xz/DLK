{"textgrid.poem.31526": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Segen wird ein Fluch: auf da\u00df der Fluch den Segen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Segen wird ein Fluch: auf da\u00df der Fluch den Segen", "tokens": ["Der", "Se\u00b7gen", "wird", "ein", "Fluch", ":", "auf", "da\u00df", "der", "Fluch", "den", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "APPR", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "vom Fluch erlangen kan: Gerechtigkeit zur S\u00fcnd:", "tokens": ["vom", "Fluch", "er\u00b7lan\u00b7gen", "kan", ":", "Ge\u00b7rech\u00b7tig\u00b7keit", "zur", "S\u00fcnd", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$.", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auf da\u00df Rechtf\u00e4rtigung in ihr die S\u00fcnde find.", "tokens": ["auf", "da\u00df", "Recht\u00b7f\u00e4r\u00b7ti\u00b7gung", "in", "ihr", "die", "S\u00fcn\u00b7de", "find", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "NN", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "der Haubt-Gerechte / will die Schuld auf Vnschuld legen /", "tokens": ["der", "Haub\u00b7tGe\u00b7rech\u00b7te", "/", "will", "die", "Schuld", "auf", "Vn\u00b7schuld", "le\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und die selbstschuldigen lossprechen auch dargegen.", "tokens": ["und", "die", "selbst\u00b7schul\u00b7di\u00b7gen", "los\u00b7spre\u00b7chen", "auch", "dar\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier lieb' ein Zornes Feur / f\u00fcrs liebste Herz entz\u00fcnd.", "tokens": ["Hier", "lieb'", "ein", "Zor\u00b7nes", "Feur", "/", "f\u00fcrs", "liebs\u00b7te", "Herz", "ent\u00b7z\u00fcnd", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "die Erzempfindlichkeit sich selber \u00fcberwind /", "tokens": ["die", "Er\u00b7zemp\u00b7find\u00b7lich\u00b7keit", "sich", "sel\u00b7ber", "\u00fc\u00b7berw\u00b7ind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "hasst ihren innern Zweck / der Feinde mit zupflegen.", "tokens": ["hasst", "ih\u00b7ren", "in\u00b7nern", "Zweck", "/", "der", "Fein\u00b7de", "mit", "zu\u00b7pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es senkt ins Schmerzen-Meer / der Freuden-Vrsprung sich.", "tokens": ["Es", "senkt", "ins", "Schmer\u00b7zen\u00b7Meer", "/", "der", "Freu\u00b7den\u00b7Vr\u00b7sprung", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$(", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "des Wesens Quell und Ziel / die Selbstheit alles Lustes /", "tokens": ["des", "We\u00b7sens", "Quell", "und", "Ziel", "/", "die", "Selbst\u00b7heit", "al\u00b7les", "Lus\u00b7tes", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$(", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "verst\u00fcrzt sich / uns zu lieb / in Abgrund unsers wustes /", "tokens": ["ver\u00b7st\u00fcrzt", "sich", "/", "uns", "zu", "lieb", "/", "in", "Ab\u00b7grund", "un\u00b7sers", "wus\u00b7tes", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$(", "PPER", "PTKA", "ADJD", "$(", "APPR", "NN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "in S\u00fcnd-und Schmerzenh\u00f6ll: so welt-verwunderlich.", "tokens": ["in", "S\u00fcn\u00b7d\u00b7und", "Schmer\u00b7zen\u00b7h\u00f6ll", ":", "so", "welt\u00b7ver\u00b7wun\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$.", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Sein' Heiligkeit / wolt nicht des Drachen Rachen fliehen:", "tokens": ["Sein'", "Hei\u00b7lig\u00b7keit", "/", "wolt", "nicht", "des", "Dra\u00b7chen", "Ra\u00b7chen", "flie\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VMFIN", "PTKNEG", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "da\u00df uns verschlungene sie k\u00f6nt aus solchem ziehen.", "tokens": ["da\u00df", "uns", "ver\u00b7schlun\u00b7ge\u00b7ne", "sie", "k\u00f6nt", "aus", "sol\u00b7chem", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VMFIN", "APPR", "PIAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}