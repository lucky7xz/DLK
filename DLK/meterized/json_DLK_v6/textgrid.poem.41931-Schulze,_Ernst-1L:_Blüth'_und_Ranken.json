{"textgrid.poem.41931": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bl\u00fcth' und Ranken", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00fcth' und Ranken", "tokens": ["Bl\u00fcth'", "und", "Ran\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wehn und schwanken", "tokens": ["Wehn", "und", "schwan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "In der lauen Fr\u00fchlingsluft;", "tokens": ["In", "der", "lau\u00b7en", "Fr\u00fch\u00b7lings\u00b7luft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie m\u00f6chten gern sich finden", "tokens": ["Und", "sie", "m\u00f6ch\u00b7ten", "gern", "sich", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in bl\u00fchenden Gewinden", "tokens": ["Und", "in", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebend mischen Farb' und Duft.", "tokens": ["Lie\u00b7bend", "mi\u00b7schen", "Fa\u00b7rb'", "und", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Doch die kalten", "tokens": ["Doch", "die", "kal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Tiefen halten,", "tokens": ["Tie\u00b7fen", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was im Licht sich sehnend regt.", "tokens": ["Was", "im", "Licht", "sich", "seh\u00b7nend", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWS", "APPRART", "NN", "PRF", "ADJD", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Manches darf nur fern sich gr\u00fc\u00dfen,", "tokens": ["Man\u00b7ches", "darf", "nur", "fern", "sich", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df verschmerzen und verschlie\u00dfen,", "tokens": ["Mu\u00df", "ver\u00b7schmer\u00b7zen", "und", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was es still im Busen tr\u00e4gt.", "tokens": ["Was", "es", "still", "im", "Bu\u00b7sen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Um die Kleinen", "tokens": ["Um", "die", "Klei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Zu vereinen,", "tokens": ["Zu", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wind' ich sie zum Kranz dir gern.", "tokens": ["Wind'", "ich", "sie", "zum", "Kranz", "dir", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPRART", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die Kr\u00e4nze doch ein Zeichen,", "tokens": ["Sind", "die", "Kr\u00e4n\u00b7ze", "doch", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df auch Ferne sich erreichen,", "tokens": ["Da\u00df", "auch", "Fer\u00b7ne", "sich", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wie bald bin ich dir fern!", "tokens": ["Und", "wie", "bald", "bin", "ich", "dir", "fern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VAFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}