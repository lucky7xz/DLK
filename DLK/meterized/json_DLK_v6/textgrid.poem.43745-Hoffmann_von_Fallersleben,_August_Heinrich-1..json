{"textgrid.poem.43745": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sah'n die Reben bl\u00fchen", "tokens": ["Wir", "sah'n", "die", "Re\u00b7ben", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im milden Fr\u00fchlingshauch,", "tokens": ["Im", "mil\u00b7den", "Fr\u00fch\u00b7lings\u00b7hauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sah'n f\u00fcr uns're M\u00fchen", "tokens": ["Und", "sah'n", "f\u00fcr", "un\u00b7s'\u00b7re", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Erbl\u00fch'n die Hoffnung auch.", "tokens": ["Er\u00b7bl\u00fch'n", "die", "Hoff\u00b7nung", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Trauben sind gediehen,", "tokens": ["Die", "Trau\u00b7ben", "sind", "ge\u00b7die\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sind w\u00fcrzig s\u00fc\u00df und klar.", "tokens": ["Sind", "w\u00fcr\u00b7zig", "s\u00fc\u00df", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00dft uns zur Lese ziehen!", "tokens": ["La\u00dft", "uns", "zur", "Le\u00b7se", "zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was wir gehofft, ward wahr.", "tokens": ["Was", "wir", "ge\u00b7hofft", ",", "ward", "wahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Sommer that das Seine,", "tokens": ["Der", "Som\u00b7mer", "that", "das", "Sei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Herbst hat es vollbracht,", "tokens": ["Der", "Herbst", "hat", "es", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit warmem Sonnenscheine", "tokens": ["Mit", "war\u00b7mem", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und k\u00fchlem Thau der Nacht.", "tokens": ["Und", "k\u00fch\u00b7lem", "Thau", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Trauben sind gediehen,", "tokens": ["Die", "Trau\u00b7ben", "sind", "ge\u00b7die\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sind w\u00fcrzig, s\u00fc\u00df und klar.", "tokens": ["Sind", "w\u00fcr\u00b7zig", ",", "s\u00fc\u00df", "und", "klar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00dft uns zur Lese ziehen!", "tokens": ["La\u00dft", "uns", "zur", "Le\u00b7se", "zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was wir gehofft, ward wahr.", "tokens": ["Was", "wir", "ge\u00b7hofft", ",", "ward", "wahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Blick voll Wohlbehagen!", "tokens": ["O", "Blick", "voll", "Wohl\u00b7be\u00b7ha\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn Beer' an Beere gl\u00e4nzt,", "tokens": ["Wenn", "Beer'", "an", "Bee\u00b7re", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn wir zur Kelter tragen,", "tokens": ["Wenn", "wir", "zur", "Kel\u00b7ter", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was Freuden uns kredenzt.", "tokens": ["Was", "Freu\u00b7den", "uns", "kre\u00b7denzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Trauben sind gediehen,", "tokens": ["Die", "Trau\u00b7ben", "sind", "ge\u00b7die\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sind w\u00fcrzig, s\u00fc\u00df und klar.", "tokens": ["Sind", "w\u00fcr\u00b7zig", ",", "s\u00fc\u00df", "und", "klar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00dft uns zur Lese ziehen!", "tokens": ["La\u00dft", "uns", "zur", "Le\u00b7se", "zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was wir gehofft, ward wahr.", "tokens": ["Was", "wir", "ge\u00b7hofft", ",", "ward", "wahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}