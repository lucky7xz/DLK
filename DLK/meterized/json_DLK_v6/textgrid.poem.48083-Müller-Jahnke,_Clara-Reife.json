{"textgrid.poem.48083": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Reife", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An verstaubten Stra\u00dfenr\u00e4ndern,", "tokens": ["An", "ver\u00b7staub\u00b7ten", "Stra\u00b7\u00dfen\u00b7r\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "am verbl\u00fchten Schlehdornhag", "tokens": ["am", "ver\u00b7bl\u00fch\u00b7ten", "Schleh\u00b7dorn\u00b7hag"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "durch den reifen Sommertag", "tokens": ["durch", "den", "rei\u00b7fen", "Som\u00b7mer\u00b7tag"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wunschlos, wahllos, ziellos schlendern . . .", "tokens": ["wun\u00b7schlos", ",", "wahl\u00b7los", ",", "ziel\u00b7los", "schlen\u00b7dern", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "VVINF", "$.", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonnentrunkne Falter irren", "tokens": ["Son\u00b7nen\u00b7trunk\u00b7ne", "Fal\u00b7ter", "ir\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "taumelnd \u00fcber Korn und Mohn, \u2013", "tokens": ["tau\u00b7melnd", "\u00fc\u00b7ber", "Korn", "und", "Mohn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "aus den Feldern kommt ein Ton", "tokens": ["aus", "den", "Fel\u00b7dern", "kommt", "ein", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "leis und scharf wie Sensenschwirren.", "tokens": ["leis", "und", "scharf", "wie", "Sen\u00b7sen\u00b7schwir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}