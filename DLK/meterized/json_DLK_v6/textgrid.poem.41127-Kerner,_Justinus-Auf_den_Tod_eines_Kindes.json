{"textgrid.poem.41127": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod eines Kindes", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wohl ist dir gebettet,", "tokens": ["Wie", "wohl", "ist", "dir", "ge\u00b7bet\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Mein Kind, im Erdenscho\u00df!", "tokens": ["Mein", "Kind", ",", "im", "Er\u00b7den\u00b7scho\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast aus der Welt gerettet", "tokens": ["Hast", "aus", "der", "Welt", "ge\u00b7ret\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich, eh' du wurdest gro\u00df.", "tokens": ["Dich", ",", "eh'", "du", "wur\u00b7dest", "gro\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn in des Lenzes Tagen", "tokens": ["Wenn", "in", "des", "Len\u00b7zes", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Bl\u00fcte f\u00e4llt vom Baum,", "tokens": ["Die", "Bl\u00fc\u00b7te", "f\u00e4llt", "vom", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann man mit Fug wohl sagen:", "tokens": ["Kann", "man", "mit", "Fug", "wohl", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie war ein lichter Traum.", "tokens": ["Sie", "war", "ein", "lich\u00b7ter", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch wenn vom Wurm gestochen", "tokens": ["Doch", "wenn", "vom", "Wurm", "ge\u00b7sto\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als Frucht sie h\u00e4ngt am Baum", "tokens": ["Als", "Frucht", "sie", "h\u00e4ngt", "am", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und faul wird abgebrochen,", "tokens": ["Und", "faul", "wird", "ab\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War sie ein b\u00f6ser Traum.", "tokens": ["War", "sie", "ein", "b\u00f6\u00b7ser", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So viele Fr\u00fcchte prangen,", "tokens": ["So", "vie\u00b7le", "Fr\u00fcch\u00b7te", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die leis ein Wurm zerfri\u00dft.", "tokens": ["Die", "leis", "ein", "Wurm", "zer\u00b7fri\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer wei\u00df, ob du entgangen", "tokens": ["Wer", "wei\u00df", ",", "ob", "du", "ent\u00b7gan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht solchem Lose bist.", "tokens": ["Nicht", "sol\u00b7chem", "Lo\u00b7se", "bist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Engel schwebt vor\u00fcber,", "tokens": ["Ein", "En\u00b7gel", "schwebt", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Haucht an die Bl\u00fcten nur,", "tokens": ["Haucht", "an", "die", "Bl\u00fc\u00b7ten", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wehen sie hin\u00fcber", "tokens": ["Da", "we\u00b7hen", "sie", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf eine be\u00dfre Flur.", "tokens": ["Auf", "ei\u00b7ne", "be\u00df\u00b7re", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich blick' dir nach mit Sehnen,", "tokens": ["Ich", "blick'", "dir", "nach", "mit", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Bl\u00fcte! fortgeweht,", "tokens": ["Du", "Bl\u00fc\u00b7te", "!", "fort\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch flie\u00dfen keine Tr\u00e4nen,", "tokens": ["Doch", "flie\u00b7\u00dfen", "kei\u00b7ne", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil es dir wohlergeht.", "tokens": ["Weil", "es", "dir", "woh\u00b7ler\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}