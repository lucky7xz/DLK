{"textgrid.poem.36292": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Weltseele", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom tr\u00fcben Schlaf erwacht zu lichtem Denken,", "tokens": ["Vom", "tr\u00fc\u00b7ben", "Schlaf", "er\u00b7wacht", "zu", "lich\u00b7tem", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat sich der Mensch zum Himmel aufgerichtet,", "tokens": ["Hat", "sich", "der", "Mensch", "zum", "Him\u00b7mel", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kann nun, wo tr\u00e4ge Furcht ihn sonst vernichtet,", "tokens": ["Kann", "nun", ",", "wo", "tr\u00e4\u00b7ge", "Furcht", "ihn", "sonst", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "PWAV", "VVFIN", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Wunder des Bewu\u00dftseins schaffend denken.", "tokens": ["Die", "Wun\u00b7der", "des", "Be\u00b7wu\u00dfts\u00b7eins", "schaf\u00b7fend", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zum ersten Lohn, den ihm die G\u00f6tter schenken,", "tokens": ["Zum", "ers\u00b7ten", "Lohn", ",", "den", "ihm", "die", "G\u00f6t\u00b7ter", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df innre Kraft den innern Streit geschlichtet,", "tokens": ["Da\u00df", "inn\u00b7re", "Kraft", "den", "in\u00b7nern", "Streit", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vernimmt er, was vom \u00c4ther sie gedichtet,", "tokens": ["Ver\u00b7nimmt", "er", ",", "was", "vom", "\u00c4\u00b7ther", "sie", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und will mit Liebe sich ins Lichtmeer senken.", "tokens": ["Und", "will", "mit", "Lie\u00b7be", "sich", "ins", "Licht\u00b7meer", "sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie dennoch eins die Kraft in allen Schranken,", "tokens": ["Wie", "den\u00b7noch", "eins", "die", "Kraft", "in", "al\u00b7len", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und leichter \u00c4ther m\u00e4cht'ger als die Masse;", "tokens": ["Und", "leich\u00b7ter", "\u00c4\u00b7ther", "m\u00e4cht'\u00b7ger", "als", "die", "Mas\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das lebt und brennt in solchem k\u00fchnen Streben!", "tokens": ["Das", "lebt", "und", "brennt", "in", "sol\u00b7chem", "k\u00fch\u00b7nen", "Stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es sinnt der Geist, wie er das Ew'ge fasse;", "tokens": ["Es", "sinnt", "der", "Geist", ",", "wie", "er", "das", "Ew'\u00b7ge", "fas\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In toter Bildung sieht er T\u00e4uschung schwanken,", "tokens": ["In", "to\u00b7ter", "Bil\u00b7dung", "sieht", "er", "T\u00e4u\u00b7schung", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das innre Wesen blitzt im freien Leben.", "tokens": ["Das", "inn\u00b7re", "We\u00b7sen", "blitzt", "im", "frei\u00b7en", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}