{"textgrid.poem.66622": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Maimarkt", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut ist Jahrmarkt. Von den Buden", "tokens": ["Heut", "ist", "Jahr\u00b7markt", ".", "Von", "den", "Bu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wehn knallrote Taschent\u00fccher,", "tokens": ["Wehn", "knall\u00b7ro\u00b7te", "Ta\u00b7schen\u00b7t\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Abgefeimte Schacherjuden", "tokens": ["Ab\u00b7ge\u00b7feim\u00b7te", "Scha\u00b7ch\u00b7er\u00b7ju\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Recken ihre krummen Riecher", "tokens": ["Re\u00b7cken", "ih\u00b7re", "krum\u00b7men", "Rie\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geierm\u00e4\u00dfig mit Geschrei", "tokens": ["Gei\u00b7er\u00b7m\u00e4\u00b7\u00dfig", "mit", "Ge\u00b7schrei"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In den lindengr\u00fcnen Mai.", "tokens": ["In", "den", "lin\u00b7den\u00b7gr\u00fc\u00b7nen", "Mai", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Emmenthaler K\u00e4seriesen,", "tokens": ["Em\u00b7men\u00b7tha\u00b7ler", "K\u00e4\u00b7se\u00b7rie\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Frischer Stiefel Lederduft ...", "tokens": ["Fri\u00b7scher", "Stie\u00b7fel", "Le\u00b7der\u00b7duft", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Staub bewei\u00dft die jungen Wiesen,", "tokens": ["Staub", "be\u00b7wei\u00dft", "die", "jun\u00b7gen", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Kr\u00e4merdunst verdickt die Luft.", "tokens": ["Kr\u00e4\u00b7mer\u00b7dunst", "ver\u00b7dickt", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wachstuch in den grellsten Farben:", "tokens": ["Wach\u00b7stuch", "in", "den", "grells\u00b7ten", "Far\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "\u00bbeinen Franken f\u00fcr den Rest!\u00ab", "tokens": ["\u00bb", "ei\u00b7nen", "Fran\u00b7ken", "f\u00fcr", "den", "Rest", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Blumenh\u00fcte, R\u00fcschen, Barben \u2013", "tokens": ["Blu\u00b7men\u00b7h\u00fc\u00b7te", ",", "R\u00fc\u00b7schen", ",", "Bar\u00b7ben", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "B\u00e4rbel, denk aufs Pfingstenfest!", "tokens": ["B\u00e4r\u00b7bel", ",", "denk", "aufs", "Pfings\u00b7ten\u00b7fest", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Rudolf, Barone\u00df Vetsera,", "tokens": ["Ru\u00b7dolf", ",", "Ba\u00b7ro\u00b7ne\u00df", "Vet\u00b7se\u00b7ra", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Farbenblutdruckkatastrophen ...", "tokens": ["Far\u00b7ben\u00b7blut\u00b7druck\u00b7ka\u00b7tast\u00b7ro\u00b7phen", "..."], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Firuli und Firulera", "tokens": ["Fi\u00b7ru\u00b7li", "und", "Fi\u00b7ru\u00b7le\u00b7ra"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.18": {"text": "Spielt die Orgel. Spitze Zofen", "tokens": ["Spielt", "die", "Or\u00b7gel", ".", "Spit\u00b7ze", "Zo\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Mit den Kleinen f\u00fcrnehm eilen,", "tokens": ["Mit", "den", "Klei\u00b7nen", "f\u00fcr\u00b7nehm", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Schrupperfeen gierig weilen.", "tokens": ["Schrup\u00b7per\u00b7feen", "gie\u00b7rig", "wei\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Ein Student zieht durchs Getriebe", "tokens": ["Ein", "Stu\u00b7dent", "zieht", "durchs", "Ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Mit der schwesterlichen Liebe.", "tokens": ["Mit", "der", "schwes\u00b7ter\u00b7li\u00b7chen", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Die hat immer was nach hinten,", "tokens": ["Die", "hat", "im\u00b7mer", "was", "nach", "hin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PWS", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Maiprinz Amor l\u00e4dt die Flinten.", "tokens": ["Mai\u00b7prinz", "A\u00b7mor", "l\u00e4dt", "die", "Flin\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Aus des Busens Knopfsaum wedelt", "tokens": ["Aus", "des", "Bu\u00b7sens", "Knopf\u00b7saum", "we\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Rotverf\u00fchrerisch ein Zipfel,", "tokens": ["Rot\u00b7ver\u00b7f\u00fch\u00b7re\u00b7risch", "ein", "Zip\u00b7fel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Da wird auch was eingef\u00e4delt,", "tokens": ["Da", "wird", "auch", "was", "ein\u00b7ge\u00b7f\u00e4\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Angeb\u00e4ndelt, liebgem\u00e4delt ...", "tokens": ["An\u00b7ge\u00b7b\u00e4n\u00b7delt", ",", "lieb\u00b7ge\u00b7m\u00e4\u00b7delt", "..."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Wollust weht der Lindenwipfel.", "tokens": ["Wol\u00b7lust", "weht", "der", "Lin\u00b7den\u00b7wip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}