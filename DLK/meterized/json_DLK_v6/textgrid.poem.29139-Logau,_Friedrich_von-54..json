{"textgrid.poem.29139": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich d\u00fcnckt, da\u00df Annia ist niemals jung gewesen;", "tokens": ["Mich", "d\u00fcnckt", ",", "da\u00df", "An\u00b7nia", "ist", "nie\u00b7mals", "jung", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NE", "VAFIN", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich habe nichts davon geh\u00f6rt, gesehn, gelesen.", "tokens": ["Ich", "ha\u00b7be", "nichts", "da\u00b7von", "ge\u00b7h\u00f6rt", ",", "ge\u00b7sehn", ",", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PAV", "VVFIN", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}