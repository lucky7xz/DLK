{"dta.poem.10071": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anmuth in Betrachtung der  \n Creaturen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir des Sch\u00f6pfers Werck bewundernd \u00fcberle-\ngen,", "tokens": ["Wenn", "wir", "des", "Sch\u00f6p\u00b7fers", "Werck", "be\u00b7wun\u00b7dernd", "\u00fc\u00b7ber\u00b7le", "gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "TRUNC", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So finden wir, da\u00df nicht allein", "tokens": ["So", "fin\u00b7den", "wir", ",", "da\u00df", "nicht", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Creaturen uns zu GOTT zu f\u00fchren pflegen,", "tokens": ["Die", "Crea\u00b7tu\u00b7ren", "uns", "zu", "GoTT", "zu", "f\u00fch\u00b7ren", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NE", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Und gleichsam Himmels-Leitern seyn;", "tokens": ["Und", "gleich\u00b7sam", "Him\u00b7mels\u00b7Lei\u00b7tern", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir finden, wenn wir sie recht mit Vernunft erwegen,", "tokens": ["Wir", "fin\u00b7den", ",", "wenn", "wir", "sie", "recht", "mit", "Ver\u00b7nunft", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sie ein Freuden-Licht, und einen Anmuths-Schein", "tokens": ["Da\u00df", "sie", "ein", "Freu\u00b7den\u00b7Licht", ",", "und", "ei\u00b7nen", "An\u00b7muths\u00b7Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In ihrem Wesen wircklich hegen.", "tokens": ["In", "ih\u00b7rem", "We\u00b7sen", "wir\u00b7ck\u00b7lich", "he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die unser Sch\u00f6pfer in der That", "tokens": ["Die", "un\u00b7ser", "Sch\u00f6p\u00b7fer", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als einen Lohn darin zu legen,", "tokens": ["Als", "ei\u00b7nen", "Lohn", "da\u00b7rin", "zu", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sie gn\u00e4diglich gew\u00fcrdigt hat.", "tokens": ["Sie", "gn\u00e4\u00b7di\u00b7glich", "ge\u00b7w\u00fcr\u00b7digt", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sprich nicht, geliebter Mensch: ich seh ein solches Licht,", "tokens": ["Sprich", "nicht", ",", "ge\u00b7lieb\u00b7ter", "Mensch", ":", "ich", "seh", "ein", "sol\u00b7ches", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Solch einen Anmuths-Schein und Lohn in ihnen nicht.", "tokens": ["Solch", "ei\u00b7nen", "An\u00b7muths\u00b7Schein", "und", "Lohn", "in", "ih\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "KON", "NN", "APPR", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn solch ein Freuden-Glantz die Creatur erf\u00fcllt,", "tokens": ["Wenn", "solch", "ein", "Freu\u00b7den\u00b7Glantz", "die", "Crea\u00b7tur", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So w\u00e4r sie ja wol nicht so sehr darin verh\u00fcllt,", "tokens": ["So", "w\u00e4r", "sie", "ja", "wol", "nicht", "so", "sehr", "da\u00b7rin", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verborgen und versteckt. Du irrest dich. Denn h\u00f6re,", "tokens": ["Ver\u00b7bor\u00b7gen", "und", "ver\u00b7steckt", ".", "Du", "ir\u00b7rest", "dich", ".", "Denn", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$.", "PPER", "VVFIN", "PPER", "$.", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wofern dein Mund nie Honigseim geschmeckt,", "tokens": ["Wo\u00b7fern", "dein", "Mund", "nie", "Ho\u00b7ni\u00b7gseim", "ge\u00b7schmeckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wo Erfahrung dir es nicht entdeckt,", "tokens": ["Und", "wo", "Er\u00b7fah\u00b7rung", "dir", "es", "nicht", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "PPER", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df diese S\u00fcssigkeit in bunten Bluhmen steckt,", "tokens": ["Da\u00df", "die\u00b7se", "S\u00fcs\u00b7sig\u00b7keit", "in", "bun\u00b7ten", "Bluh\u00b7men", "steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du leugnetest gewi\u00df, so starck, als unsre Lehre,", "tokens": ["Du", "leug\u00b7ne\u00b7test", "ge\u00b7wi\u00df", ",", "so", "starck", ",", "als", "uns\u00b7re", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df in den Bluhmen Honig w\u00e4re,", "tokens": ["Da\u00df", "in", "den", "Bluh\u00b7men", "Ho\u00b7nig", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der doch wahrhaftig da. So steckt auf gleiche Weise,", "tokens": ["Der", "doch", "wahr\u00b7haf\u00b7tig", "da", ".", "So", "steckt", "auf", "glei\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zu deiner Lust, und deinem GOTT zum Preise", "tokens": ["Zu", "dei\u00b7ner", "Lust", ",", "und", "dei\u00b7nem", "GoTT", "zum", "Prei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Auch eine wahre Seelen-Speise", "tokens": ["Auch", "ei\u00b7ne", "wah\u00b7re", "See\u00b7len\u00b7Spei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ein Seelen-Honig in der Bl\u00fchte,", "tokens": ["Ein", "See\u00b7len\u00b7Ho\u00b7nig", "in", "der", "Bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ja \u00fcberall in ieder Creatur.", "tokens": ["Ja", "\u00fc\u00b7be\u00b7rall", "in", "ie\u00b7der", "Crea\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "La\u00df dein aufmercksames vern\u00fcnftiges Gem\u00fcthe", "tokens": ["La\u00df", "dein", "auf\u00b7merck\u00b7sa\u00b7mes", "ver\u00b7n\u00fcnf\u00b7ti\u00b7ges", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der regen Bienen Stelle nur", "tokens": ["Der", "re\u00b7gen", "Bie\u00b7nen", "Stel\u00b7le", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In fleissigen Betrachtungen vertreten:", "tokens": ["In", "fleis\u00b7si\u00b7gen", "Be\u00b7trach\u00b7tun\u00b7gen", "ver\u00b7tre\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wird, wie sie, mit frohem sumsen, thun,", "tokens": ["So", "wird", ",", "wie", "sie", ",", "mit", "fro\u00b7hem", "sum\u00b7sen", ",", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "PPER", "$,", "APPR", "ADJA", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch dein vergn\u00fcgter Sinn nicht ruhn,", "tokens": ["Auch", "dein", "ver\u00b7gn\u00fcg\u00b7ter", "Sinn", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Voll froher Danckbarkeit, den Sch\u00f6pfer anzubeten.", "tokens": ["Voll", "fro\u00b7her", "Dan\u00b7ck\u00b7bar\u00b7keit", ",", "den", "Sch\u00f6p\u00b7fer", "an\u00b7zu\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Je mehr du nun darin Sein\u2019 Allmacht wirst ergr\u00fcnden;", "tokens": ["Je", "mehr", "du", "nun", "da\u00b7rin", "Sein'", "All\u00b7macht", "wirst", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "PAV", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je gr\u00f6ss\u2019re S\u00fc\u00dfigkeit, Lust, Anmuth und Vergn\u00fcgen,", "tokens": ["Je", "gr\u00f6ss'\u00b7re", "S\u00fc\u00b7\u00dfig\u00b7keit", ",", "Lust", ",", "An\u00b7muth", "und", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die wunderbar darin verborgen liegen,", "tokens": ["Die", "wun\u00b7der\u00b7bar", "da\u00b7rin", "ver\u00b7bor\u00b7gen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PAV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird dein erquickter Geist, zu seiner Nahrung, finden.", "tokens": ["Wird", "dein", "er\u00b7quick\u00b7ter", "Geist", ",", "zu", "sei\u00b7ner", "Nah\u00b7rung", ",", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja nicht nur dir allein;", "tokens": ["Ja", "nicht", "nur", "dir", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKNEG", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auch andern wird der s\u00fcsse Safft", "tokens": ["Auch", "an\u00b7dern", "wird", "der", "s\u00fcs\u00b7se", "Safft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der in der Creatur verhandnen Eigenschaft", "tokens": ["Der", "in", "der", "Crea\u00b7tur", "ver\u00b7hand\u00b7nen", "Ei\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Ein s\u00fcsser Seelen-Honig seyn.", "tokens": ["Ein", "s\u00fcs\u00b7ser", "See\u00b7len\u00b7Ho\u00b7nig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}