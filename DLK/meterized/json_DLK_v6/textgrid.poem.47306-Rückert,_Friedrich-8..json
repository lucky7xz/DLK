{"textgrid.poem.47306": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tag ist's, auf, steh auf, o J\u00fcngling Muselmane!", "tokens": ["Tag", "ist's", ",", "auf", ",", "steh", "auf", ",", "o", "J\u00fcng\u00b7ling", "Mu\u00b7sel\u00b7ma\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "FM", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Packe dein Ger\u00e4t und komm zur Karawane.", "tokens": ["Pa\u00b7cke", "dein", "Ge\u00b7r\u00e4t", "und", "komm", "zur", "Ka\u00b7ra\u00b7wa\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Horch, o horch, sie ziehet schon, indes du schl\u00e4fest.", "tokens": ["Horch", ",", "o", "horch", ",", "sie", "zie\u00b7het", "schon", ",", "in\u00b7des", "du", "schl\u00e4\u00b7fest", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "FM", "$,", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Horch! ihr Gl\u00f6cklein, da\u00df es nicht zu sp\u00e4t dich mahne!", "tokens": ["Horch", "!", "ihr", "Gl\u00f6c\u00b7klein", ",", "da\u00df", "es", "nicht", "zu", "sp\u00e4t", "dich", "mah\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "PTKA", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Wann der W\u00fcste Sand verweht hat ihre Spuren,", "tokens": ["Wann", "der", "W\u00fcs\u00b7te", "Sand", "ver\u00b7weht", "hat", "ih\u00b7re", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Hoffe nicht, da\u00df sie dein Fu\u00dftritt wieder bahne.", "tokens": ["Hof\u00b7fe", "nicht", ",", "da\u00df", "sie", "dein", "Fu\u00df\u00b7tritt", "wie\u00b7der", "bah\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Auf dich raffe! sei ein Mann, ein Held, ein K\u00e4mpe,", "tokens": ["Auf", "dich", "raf\u00b7fe", "!", "sei", "ein", "Mann", ",", "ein", "Held", ",", "ein", "K\u00e4m\u00b7pe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$.", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Bringe nicht das Leben hin in eitlem Wahne.", "tokens": ["Brin\u00b7ge", "nicht", "das", "Le\u00b7ben", "hin", "in", "eit\u00b7lem", "Wah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Sei gedenk des Ahnenstammes, Perserj\u00fcngling,", "tokens": ["Sei", "ge\u00b7denk", "des", "Ah\u00b7nen\u00b7stam\u00b7mes", ",", "Per\u00b7ser\u00b7j\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAIMP", "ADJD", "ART", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Wie Rostem ein Held, wie Sal ein Pehlewane.", "tokens": ["Wie", "Ros\u00b7tem", "ein", "Held", ",", "wie", "Sal", "ein", "Peh\u00b7le\u00b7wa\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,", "PWAV", "VMFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Mann des Lichtes, Held des Rechtes, Sonnenk\u00e4mpe!", "tokens": ["Mann", "des", "Lich\u00b7tes", ",", "Held", "des", "Rech\u00b7tes", ",", "Son\u00b7nen\u00b7k\u00e4m\u00b7pe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Falle nicht anheim dem dunklen Ahrimane.", "tokens": ["Fal\u00b7le", "nicht", "an\u00b7heim", "dem", "dunk\u00b7len", "Ah\u00b7ri\u00b7ma\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Wenn du hast die ird'sche Seel' im Kampf get\u00f6tet,", "tokens": ["Wenn", "du", "hast", "die", "ird'\u00b7sche", "Seel'", "im", "Kampf", "ge\u00b7t\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Schwingt die himmlische des Lebens Siegesfahne.", "tokens": ["Schwingt", "die", "himm\u00b7li\u00b7sche", "des", "Le\u00b7bens", "Sie\u00b7ges\u00b7fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Wann du dich dem\u00fctigtest zum Staub der Schwelle,", "tokens": ["Wann", "du", "dich", "de\u00b7m\u00fc\u00b7tig\u00b7test", "zum", "Staub", "der", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "Wirst du Siegelring in unsres Schachs Diwane.", "tokens": ["Wirst", "du", "Sie\u00b7gel\u00b7ring", "in", "uns\u00b7res", "Schachs", "Di\u00b7wa\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}