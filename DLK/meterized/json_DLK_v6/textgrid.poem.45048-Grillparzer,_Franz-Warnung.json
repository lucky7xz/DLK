{"textgrid.poem.45048": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleib nur der alten Kunst getreu,", "tokens": ["Bleib", "nur", "der", "al\u00b7ten", "Kunst", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ist zu allen Zeiten eine,", "tokens": ["Sie", "ist", "zu", "al\u00b7len", "Zei\u00b7ten", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer sich unter die volkst\u00fcmlichen Kleien mischt,", "tokens": ["Wer", "sich", "un\u00b7ter", "die", "volks\u00b7t\u00fcm\u00b7li\u00b7chen", "Klei\u00b7en", "mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Den fressen die patriotischen Schweine.", "tokens": ["Den", "fres\u00b7sen", "die", "pat\u00b7ri\u00b7o\u00b7ti\u00b7schen", "Schwei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}