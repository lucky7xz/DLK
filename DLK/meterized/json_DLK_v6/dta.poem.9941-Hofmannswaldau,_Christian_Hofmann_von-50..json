{"dta.poem.9941": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df was nagen meine z\u00e4hne/ das genie\u00df und e\u00df ich nicht/", "tokens": ["Di\u00df", "was", "na\u00b7gen", "mei\u00b7ne", "z\u00e4h\u00b7ne", "/", "das", "ge\u00b7nie\u00df", "und", "e\u00df", "ich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPOSAT", "NN", "$(", "PDS", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie aus meinem magrem leibe man es sieht/ und dem gesicht:", "tokens": ["Wie", "aus", "mei\u00b7nem", "mag\u00b7rem", "lei\u00b7be", "man", "es", "sieht", "/", "und", "dem", "ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "ADJA", "VVFIN", "PIS", "PPER", "VVFIN", "$(", "KON", "ART", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Was von mir zermalmet ist/ wird vom wind herum gejaget/", "tokens": ["Was", "von", "mir", "zer\u00b7mal\u00b7met", "ist", "/", "wird", "vom", "wind", "he\u00b7rum", "ge\u00b7ja\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVPP", "VAFIN", "$(", "VAFIN", "APPRART", "NN", "APZR", "VVPP", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Von dem allerschlechtsten volck werd am meisten ich geplaget.", "tokens": ["Von", "dem", "al\u00b7ler\u00b7schlechts\u00b7ten", "volck", "werd", "am", "meis\u00b7ten", "ich", "ge\u00b7pla\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "APPRART", "VVFIN", "PPER", "VVPP", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}