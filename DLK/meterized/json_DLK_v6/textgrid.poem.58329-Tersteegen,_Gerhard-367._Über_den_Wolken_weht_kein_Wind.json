{"textgrid.poem.58329": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "367. \u00dcber den Wolken weht kein Wind", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie selig ist ein Herz, das abgeschieden lebt", "tokens": ["Wie", "se\u00b7lig", "ist", "ein", "Herz", ",", "das", "ab\u00b7ge\u00b7schie\u00b7den", "lebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcber Freud' und Leid sich blo\u00df in Gott erhebt!", "tokens": ["Und", "\u00fc\u00b7ber", "Freud'", "und", "Leid", "sich", "blo\u00df", "in", "Gott", "er\u00b7hebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es stehet fest und still auch in Ver\u00e4nderungen,", "tokens": ["Es", "ste\u00b7het", "fest", "und", "still", "auch", "in", "Ver\u00b7\u00e4n\u00b7de\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dieweil es durch den Tod ins Leben eingedrungen.", "tokens": ["Die\u00b7weil", "es", "durch", "den", "Tod", "ins", "Le\u00b7ben", "ein\u00b7ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}