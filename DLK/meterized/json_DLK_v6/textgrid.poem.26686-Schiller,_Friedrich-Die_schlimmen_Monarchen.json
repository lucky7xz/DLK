{"textgrid.poem.26686": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die schlimmen Monarchen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euren Preis erklimme meine Leier \u2013", "tokens": ["Eu\u00b7ren", "Preis", "er\u00b7klim\u00b7me", "mei\u00b7ne", "Lei\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Erdeng\u00f6tter \u2013 die der s\u00fc\u00dfen Feier", "tokens": ["Er\u00b7den\u00b7g\u00f6t\u00b7ter", "\u2013", "die", "der", "s\u00fc\u00b7\u00dfen", "Fei\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "PRELS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Anadyomenens sanft nur klang;", "tokens": ["A\u00b7nad\u00b7yo\u00b7me\u00b7nens", "sanft", "nur", "klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Leiser um das pompende Get\u00f6se,", "tokens": ["Lei\u00b7ser", "um", "das", "pom\u00b7pen\u00b7de", "Ge\u00b7t\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+---+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Sch\u00fcchtern um die Purpurflammen eurer Gr\u00f6\u00dfe", "tokens": ["Sch\u00fcch\u00b7tern", "um", "die", "Pur\u00b7pur\u00b7flam\u00b7men", "eu\u00b7rer", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Zittert der Gesang.", "tokens": ["Zit\u00b7tert", "der", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Redet! soll ich goldne Saiten schlagen,", "tokens": ["Re\u00b7det", "!", "soll", "ich", "gold\u00b7ne", "Sai\u00b7ten", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn, vom Jubelruf emporgetragen,", "tokens": ["Wenn", ",", "vom", "Ju\u00b7bel\u00b7ruf", "em\u00b7por\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Euer Wagen durch den Walplatz rauscht?", "tokens": ["Eu\u00b7er", "Wa\u00b7gen", "durch", "den", "Wal\u00b7platz", "rauscht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn ihr, schlapp vom eisernen Umarmen,", "tokens": ["Wenn", "ihr", ",", "schlapp", "vom", "ei\u00b7ser\u00b7nen", "Um\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+---+-", "measure": "anapaest.init"}, "line.5": {"text": "Schwere Panzer mit den weichen Rosenarmen", "tokens": ["Schwe\u00b7re", "Pan\u00b7zer", "mit", "den", "wei\u00b7chen", "Ro\u00b7se\u00b7nar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Eurer Phrynen tauscht? \u2013", "tokens": ["Eu\u00b7rer", "Phry\u00b7nen", "tauscht", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Soll vielleicht im Schimmer goldner Reifen,", "tokens": ["Soll", "viel\u00b7leicht", "im", "Schim\u00b7mer", "gold\u00b7ner", "Rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "G\u00f6tter, euch die k\u00fchne Hymne greifen,", "tokens": ["G\u00f6t\u00b7ter", ",", "euch", "die", "k\u00fch\u00b7ne", "Hym\u00b7ne", "grei\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo, in mystisch Dunkel eingemummt,", "tokens": ["Wo", ",", "in", "mys\u00b7tisch", "Dun\u00b7kel", "ein\u00b7ge\u00b7mummt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Euer Spleen mit Donnerkeilen t\u00e4ndelt,", "tokens": ["Eu\u00b7er", "Spleen", "mit", "Don\u00b7ner\u00b7kei\u00b7len", "t\u00e4n\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mit ", "tokens": ["Mit"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Bis \u2013 das Grab verstummt?", "tokens": ["Bis", "\u2013", "das", "Grab", "ver\u00b7stummt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sing ich ", "tokens": ["Sing", "ich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Soll ich, F\u00fcrsten, eure ", "tokens": ["Soll", "ich", ",", "F\u00fcrs\u00b7ten", ",", "eu\u00b7re"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "$,", "NN", "$,", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn der Wurm am K\u00f6nigsherzen zehrt,", "tokens": ["Wenn", "der", "Wurm", "am", "K\u00f6\u00b7nigs\u00b7her\u00b7zen", "zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Weht der goldne Schlummer um den Mohren,", "tokens": ["Weht", "der", "gold\u00b7ne", "Schlum\u00b7mer", "um", "den", "Moh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der den Schatz bewacht an des Palastes Toren,", "tokens": ["Der", "den", "Schatz", "be\u00b7wacht", "an", "des", "Pa\u00b7las\u00b7tes", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Und \u2013 ihn nicht begehrt.", "tokens": ["Und", "\u2013", "ihn", "nicht", "be\u00b7gehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Zeig, o Muse, wie mit Rudersklaven", "tokens": ["Zeig", ",", "o", "Mu\u00b7se", ",", "wie", "mit", "Ru\u00b7ders\u00b7kla\u00b7ven"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00f6nige auf ", "tokens": ["K\u00f6\u00b7ni\u00b7ge", "auf"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Wo nun nimmer ihre Launen foltern,", "tokens": ["Wo", "nun", "nim\u00b7mer", "ih\u00b7re", "Lau\u00b7nen", "fol\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nimmer die Theaterminotaure poltern", "tokens": ["Nim\u00b7mer", "die", "The\u00b7a\u00b7ter\u00b7mi\u00b7no\u00b7tau\u00b7re", "pol\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Und \u2013 die L\u00f6wen ruhn.", "tokens": ["Und", "\u2013", "die", "L\u00f6\u00b7wen", "ruhn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Auf! Betaste mit dem Zaubersiegel,", "tokens": ["Auf", "!", "Be\u00b7tas\u00b7te", "mit", "dem", "Zau\u00b7ber\u00b7sie\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Hekate, des Gruftgew\u00f6lbes Riegel!", "tokens": ["He\u00b7ka\u00b7te", ",", "des", "Gruft\u00b7ge\u00b7w\u00f6l\u00b7bes", "Rie\u00b7gel", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Horch! die Fl\u00fcgel donnern jach zur\u00fcck!", "tokens": ["Horch", "!", "die", "Fl\u00fc\u00b7gel", "don\u00b7nern", "jach", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo des Todes Odem dumpfig s\u00e4uselt,", "tokens": ["Wo", "des", "To\u00b7des", "O\u00b7dem", "dump\u00b7fig", "s\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schauerluft die starren Locken aufw\u00e4rts kr\u00e4uselt,", "tokens": ["Schau\u00b7er\u00b7luft", "die", "star\u00b7ren", "Lo\u00b7cken", "auf\u00b7w\u00e4rts", "kr\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Sing ich \u2013 ", "tokens": ["Sing", "ich", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$("], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Hier das Ufer? \u2013 Hier in diesen Grotten", "tokens": ["Hier", "das", "U\u00b7fer", "?", "\u2013", "Hier", "in", "die\u00b7sen", "Grot\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "$(", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stranden eurer W\u00fcnsche stolze Flotten?", "tokens": ["Stran\u00b7den", "eu\u00b7rer", "W\u00fcn\u00b7sche", "stol\u00b7ze", "Flot\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hier \u2013 wo eurer Gr\u00f6\u00dfe Flut sich st\u00f6\u00dft?", "tokens": ["Hier", "\u2013", "wo", "eu\u00b7rer", "Gr\u00f6\u00b7\u00dfe", "Flut", "sich", "st\u00f6\u00dft", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "PPOSAT", "NN", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ewig nie dem Ruhme zu erwarmen,", "tokens": ["E\u00b7wig", "nie", "dem", "Ruh\u00b7me", "zu", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schmiedet hier die Nacht mit schwarzen Schauerarmen", "tokens": ["Schmie\u00b7det", "hier", "die", "Nacht", "mit", "schwar\u00b7zen", "Schau\u00b7er\u00b7ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Potentaten fest.", "tokens": ["Po\u00b7ten\u00b7ta\u00b7ten", "fest", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Traurig funkelt auf dem Totenkasten", "tokens": ["Trau\u00b7rig", "fun\u00b7kelt", "auf", "dem", "To\u00b7ten\u00b7kas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eurer Kronen, der umperlten Lasten,", "tokens": ["Eu\u00b7rer", "Kro\u00b7nen", ",", "der", "um\u00b7perl\u00b7ten", "Las\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eurer Szepter undankbare Pracht.", "tokens": ["Eu\u00b7rer", "Szep\u00b7ter", "un\u00b7dank\u00b7ba\u00b7re", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie so sch\u00f6n man Moder \u00fcbergoldet!", "tokens": ["Wie", "so", "sch\u00f6n", "man", "Mo\u00b7der", "\u00fc\u00b7ber\u00b7gol\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PIS", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Doch nur W\u00fcrmer werden mit dem Leib besoldet,", "tokens": ["Doch", "nur", "W\u00fcr\u00b7mer", "wer\u00b7den", "mit", "dem", "Leib", "be\u00b7sol\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Dem \u2013 die Welt gewacht.", "tokens": ["Dem", "\u2013", "die", "Welt", "ge\u00b7wacht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Stolze Pflanzen in so niedern Beeten!", "tokens": ["Stol\u00b7ze", "Pflan\u00b7zen", "in", "so", "nie\u00b7dern", "Bee\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seht doch! \u2013 wie mit welken Majest\u00e4ten", "tokens": ["Seht", "doch", "!", "\u2013", "wie", "mit", "wel\u00b7ken", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "$(", "KOKOM", "APPR", "PWAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Garstig spa\u00dft der unversch\u00e4mte Tod!", "tokens": ["Gars\u00b7tig", "spa\u00dft", "der", "un\u00b7ver\u00b7sch\u00e4m\u00b7te", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die durch Nord und Ost und West geboten \u2013", "tokens": ["Die", "durch", "Nord", "und", "Ost", "und", "West", "ge\u00b7bo\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "KON", "NN", "KON", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dulden sie des Unholds ekelhafte Zoten,", "tokens": ["Dul\u00b7den", "sie", "des", "Un\u00b7holds", "e\u00b7kel\u00b7haf\u00b7te", "Zo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Und \u2013 kein Sultan droht?", "tokens": ["Und", "\u2013", "kein", "Sul\u00b7tan", "droht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Springt doch auf, ihr st\u00f6rrige Verstummer,", "tokens": ["Springt", "doch", "auf", ",", "ihr", "st\u00f6r\u00b7ri\u00b7ge", "Ver\u00b7stum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": 