{"dta.poem.13121": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eso lang, Freundinnen ihr in diesem Krei\u00df gesessen,", "tokens": ["\u201e", "so", "lang", ",", "Freun\u00b7din\u00b7nen", "ihr", "in", "die\u00b7sem", "Krei\u00df", "ge\u00b7ses\u00b7sen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "NN", "PPER", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ehabt ihr das Wichtigste, das Edelste vergessen.", "tokens": ["\u201e", "habt", "ihr", "das", "Wich\u00b7tigs\u00b7te", ",", "das", "E\u00b7dels\u00b7te", "ver\u00b7ges\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "ADJA", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ees seynd zwey Prinzen da. Wem ist es unbekannt?", "tokens": ["\u201e", "es", "seynd", "zwey", "Prin\u00b7zen", "da", ".", "Wem", "ist", "es", "un\u00b7be\u00b7kannt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "CARD", "NN", "PTKVZ", "$.", "PWS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eder ", "tokens": ["\u201e", "der"], "token_info": ["punct", "word"], "pos": ["$(", "ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "565\u201dDas K\u00f6nigliche Paar, um welches man gestritten,", "tokens": ["\"", "Das", "K\u00f6\u00b7nig\u00b7li\u00b7che", "Paar", ",", "um", "wel\u00b7ches", "man", "ge\u00b7strit\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eo ewig gr\u00fcner Baum! erzeuget solche Bl\u00fchten.", "tokens": ["\u201e", "o", "e\u00b7wig", "gr\u00fc\u00b7ner", "Baum", "!", "er\u00b7zeu\u00b7get", "sol\u00b7che", "Bl\u00fch\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJD", "ADJA", "NN", "$.", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eerw\u00e4hlet diese Zwey zum vorgesezten Ziel!", "tokens": ["\u201e", "er\u00b7w\u00e4h\u00b7let", "die\u00b7se", "Zwey", "zum", "vor\u00b7ge\u00b7sez\u00b7ten", "Ziel", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDAT", "CARD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eich wei\u00df, da\u00df es dem Herz der K\u00f6niginn gefiel.", "tokens": ["\u201e", "ich", "wei\u00df", ",", "da\u00df", "es", "dem", "Herz", "der", "K\u00f6\u00b7ni\u00b7ginn", "ge\u00b7fiel", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eich glaube nicht, da\u00df ich durch diesen Vorschlag fehle;", "tokens": ["\u201e", "ich", "glau\u00b7be", "nicht", ",", "da\u00df", "ich", "durch", "die\u00b7sen", "Vor\u00b7schlag", "feh\u00b7le", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "570\u201dSie seynd der K\u00f6niginn Vergn\u00fcgen, Trost; ja Seele.", "tokens": ["\"", "Sie", "seynd", "der", "K\u00f6\u00b7ni\u00b7ginn", "Ver\u00b7gn\u00fc\u00b7gen", ",", "Trost", ";", "ja", "See\u00b7le", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "NN", "$,", "NN", "$.", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201egeht, fragt das Vaterland! tragt ihm die Meinung vor!", "tokens": ["\u201e", "geht", ",", "fragt", "das", "Va\u00b7ter\u00b7land", "!", "tragt", "ihm", "die", "Mei\u00b7nung", "vor", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ewas sagt der Treueste dem Treuesten ins Ohr?", "tokens": ["\u201e", "was", "sagt", "der", "Treu\u00b7es\u00b7te", "dem", "Treu\u00b7es\u00b7ten", "ins", "Ohr", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eo schickte GOtt dem Thron, so spricht und sprach man, Erben!", "tokens": ["\u201e", "o", "schick\u00b7te", "Gott", "dem", "Thron", ",", "so", "spricht", "und", "sprach", "man", ",", "Er\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "PIS", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201esonst mu\u00df der Baum des Heils, des Wohlergehns verderben!", "tokens": ["\u201e", "sonst", "mu\u00df", "der", "Baum", "des", "Heils", ",", "des", "Woh\u00b7ler\u00b7gehns", "ver\u00b7der\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "575\u201dIst dieser Ruff euch fremd, so werfft nur einen Blick", "tokens": ["\"", "Ist", "die\u00b7ser", "Ruff", "euch", "fremd", ",", "so", "werfft", "nur", "ei\u00b7nen", "Blick"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PDAT", "NN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eauf jene Sorg und Furcht, auf jenes Leid zur\u00fcck,", "tokens": ["\u201e", "auf", "je\u00b7ne", "Sorg", "und", "Furcht", ",", "auf", "je\u00b7nes", "Leid", "zu\u00b7r\u00fcck", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "NN", "KON", "NN", "$,", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201ein dem wir lang verzagt und halb verzweifelnd standen,", "tokens": ["\u201e", "in", "dem", "wir", "lang", "ver\u00b7zagt", "und", "halb", "ver\u00b7zwei\u00b7felnd", "stan\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "ADJD", "VVPP", "KON", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eweil, wo wir hingesehn, wir keine Kinder fanden.", "tokens": ["\u201e", "weil", ",", "wo", "wir", "hin\u00b7ge\u00b7sehn", ",", "wir", "kei\u00b7ne", "Kin\u00b7der", "fan\u00b7den", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "$,", "PWAV", "PPER", "VVINF", "$,", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201edie seynd der V\u00f6lcker Trost, Wunsch, Hoffnung, Zuversicht,", "tokens": ["\u201e", "die", "seynd", "der", "V\u00f6l\u00b7cker", "Trost", ",", "Wunsch", ",", "Hoff\u00b7nung", ",", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "580\u201dDer Kronen Werth und Schmuck, der Staaten Gleichgewicht.", "tokens": ["\"", "Der", "Kro\u00b7nen", "Werth", "und", "Schmuck", ",", "der", "Staa\u00b7ten", "Gleich\u00b7ge\u00b7wicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "KON", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201ewird von der Feinde Groll gleich etwas angesponnen,", "tokens": ["\u201e", "wird", "von", "der", "Fein\u00b7de", "Groll", "gleich", "et\u00b7was", "an\u00b7ge\u00b7spon\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "APPR", "ART", "NN", "NN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201eso baut man auf den Glanz dergleichen Morgen-Sonnen.", "tokens": ["\u201e", "so", "baut", "man", "auf", "den", "Glanz", "derg\u00b7lei\u00b7chen", "Mor\u00b7gen\u00b7Son\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "\u201ewas aber mich betrifft, so seht die Mutter an", "tokens": ["\u201e", "was", "a\u00b7ber", "mich", "be\u00b7tr\u00b7ifft", ",", "so", "seht", "die", "Mut\u00b7ter", "an"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "ADV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "\u201eund sagt, ob mein Bem\u00fchn auch mir was nuzen kann.", "tokens": ["\u201e", "und", "sagt", ",", "ob", "mein", "Be\u00b7m\u00fchn", "auch", "mir", "was", "nu\u00b7zen", "kann", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}