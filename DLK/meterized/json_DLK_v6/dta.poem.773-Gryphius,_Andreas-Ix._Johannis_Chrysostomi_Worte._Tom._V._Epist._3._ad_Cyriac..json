{"dta.poem.773": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n Johannis Chrysostomi  Worte.  Tom. V.  \n Epist. 3. ad Cyriac.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "De\u00df Keysers Fraw' ergrimm't: Gedult kan \u00fcberwinden. ", "tokens": ["De\u00df", "Key\u00b7sers", "Fra\u00b7w'", "er\u00b7grim\u00b7m't", ":", "Ge\u00b7dult", "kan", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$.", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie jagt mich au\u00df der Statt: de\u00df Herren ist die Welt", "tokens": ["Sie", "jagt", "mich", "au\u00df", "der", "Statt", ":", "de\u00df", "Her\u00b7ren", "ist", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie schneidet der Prophet/ ward durch die quaal gef\u00e4llt/", "tokens": ["Sie", "schnei\u00b7det", "der", "Pro\u00b7phet", "/", "ward", "durch", "die", "qua\u00b7al", "ge\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "Sie hat das Meer; im Meer Kan Jonas rettung finden-", "tokens": ["Sie", "hat", "das", "Meer", ";", "im", "Meer", "Kan", "Jo\u00b7nas", "ret\u00b7tung", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "APPRART", "NN", "VMFIN", "NE", "NE", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie drewt mit glutt: die nicht die Knaben k\u00f6nt entz\u00fcnden.", "tokens": ["Sie", "drewt", "mit", "glutt", ":", "die", "nicht", "die", "Kna\u00b7ben", "k\u00f6nt", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "$.", "ART", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit L\u00f6wen: Daniel wurd durch sie frey gestellt/", "tokens": ["Mit", "L\u00f6\u00b7wen", ":", "Da\u00b7ni\u00b7el", "wurd", "durch", "sie", "frey", "ge\u00b7stellt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "NE", "VAFIN", "APPR", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Steinen: ", "tokens": ["Mit", "Stei\u00b7nen", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Sie zuckt das Schwerdt: das vor den T\u00e4uffer kont entbinden/", "tokens": ["Sie", "zuckt", "das", "Schwerdt", ":", "das", "vor", "den", "T\u00e4uf\u00b7fer", "kont", "ent\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "APPR", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie greifft nach meinem Gutt: Verlust ist mein gewinn", "tokens": ["Sie", "greifft", "nach", "mei\u00b7nem", "Gutt", ":", "Ver\u00b7lust", "ist", "mein", "ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Blo\u00df kam ich auff die Welt: bla\u00df fahr ich wieder hin/", "tokens": ["Blo\u00df", "kam", "ich", "auff", "die", "Welt", ":", "bla\u00df", "fahr", "ich", "wie\u00b7der", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie schm\u00e4ht: di\u00df ist der ruhmb der Seelen die Gott lieben-", "tokens": ["Sie", "schm\u00e4ht", ":", "di\u00df", "ist", "der", "ruhmb", "der", "See\u00b7len", "die", "Gott", "lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PDS", "VAFIN", "ART", "NN", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie schl\u00e4gt mein bitten au\u00df: der Himmel nimmt es an-", "tokens": ["Sie", "schl\u00e4gt", "mein", "bit\u00b7ten", "au\u00df", ":", "der", "Him\u00b7mel", "nimmt", "es", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wegert mir verh\u00f6r; jtzt h\u00f6rt mich jederman.", "tokens": ["Sie", "we\u00b7gert", "mir", "ver\u00b7h\u00f6r", ";", "jtzt", "h\u00f6rt", "mich", "je\u00b7der\u00b7man", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie bann\u2019t: ich bin ins Buch de\u00df Lebens eingeschrieben-", "tokens": ["Sie", "bann't", ":", "ich", "bin", "ins", "Buch", "de\u00df", "Le\u00b7bens", "ein\u00b7ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}