{"dta.poem.8257": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "erster  A ufftritt.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Es soll noch zu meinen F\u00fc\u00dfen", "tokens": ["Es", "soll", "noch", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was nur Krohn und Zepter tr\u00e4gt/", "tokens": ["Was", "nur", "Krohn", "und", "Zep\u00b7ter", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Welt vor Machten hegt/", "tokens": ["Und", "die", "Welt", "vor", "Mach\u00b7ten", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In geb\u00fcckter Demuth b\u00fc\u00dfen:", "tokens": ["In", "ge\u00b7b\u00fcck\u00b7ter", "De\u00b7muth", "b\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort/ wo Ph&#339;bus auf will gehn/", "tokens": ["Dort", "/", "wo", "Ph", "&#339;", "bus", "auf", "will", "gehn", "/"], "token_info": ["word", "punct", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "NE", "$(", "NE", "APPR", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wo er die Pferde tr&#228;ncket/", "tokens": ["Und", "wo", "er", "die", "Pfer\u00b7de", "tr", "&#228;", "ncket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "NE", "$(", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Wann er in die See sich sencket/", "tokens": ["Wann", "er", "in", "die", "See", "sich", "sen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll er meine Siege sehn.", "tokens": ["Soll", "er", "mei\u00b7ne", "Sie\u00b7ge", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}