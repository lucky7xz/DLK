{"dta.poem.11396": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als Daphne in seiner gegenwart die  \n violine strich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! warum flieh ich nicht, wenn unsre Daphne spielt?", "tokens": ["Ach", "!", "wa\u00b7rum", "flieh", "ich", "nicht", ",", "wenn", "uns\u00b7re", "Daph\u00b7ne", "spielt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df doch, da\u00df ihr spiel auf mein begr\u00e4bni\u00df zielt:", "tokens": ["Ich", "wei\u00df", "doch", ",", "da\u00df", "ihr", "spiel", "auf", "mein", "be\u00b7gr\u00e4b\u00b7ni\u00df", "zielt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn ihre linden strich\u2019 und wohl-gesetzte noten", "tokens": ["Denn", "ih\u00b7re", "lin\u00b7den", "strich'", "und", "wohl\u00b7ge\u00b7setz\u00b7te", "no\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind freylich weiter nichts, als s\u00fcsse todes-boten.", "tokens": ["Sind", "frey\u00b7lich", "wei\u00b7ter", "nichts", ",", "als", "s\u00fcs\u00b7se", "to\u00b7des\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIS", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der bogen, den sie f\u00fchrt, ist Paphiens gescho\u00df.", "tokens": ["Der", "bo\u00b7gen", ",", "den", "sie", "f\u00fchrt", ",", "ist", "Pa\u00b7phi\u00b7ens", "ge\u00b7scho\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "L\u00e4st sie, dem ansehn nach, gleich keine pfeile los;", "tokens": ["L\u00e4st", "sie", ",", "dem", "an\u00b7sehn", "nach", ",", "gleich", "kei\u00b7ne", "pfei\u00b7le", "los", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "VVFIN", "PTKVZ", "$,", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So seh ich dennoch wohl, da\u00df eine violine", "tokens": ["So", "seh", "ich", "den\u00b7noch", "wohl", ",", "da\u00df", "ei\u00b7ne", "vi\u00b7o\u00b7li\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der schlauen Daphne mehr, als hundert k\u00f6cher, diene.", "tokens": ["Der", "schlau\u00b7en", "Daph\u00b7ne", "mehr", ",", "als", "hun\u00b7dert", "k\u00f6\u00b7cher", ",", "die\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "KOUS", "CARD", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach ja! ich f\u00fchle schon der tremulanten krafft,", "tokens": ["Ach", "ja", "!", "ich", "f\u00fch\u00b7le", "schon", "der", "tre\u00b7mu\u00b7lan\u00b7ten", "krafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die gegen uns so viel, als scharffe pfeile, schafft:", "tokens": ["Die", "ge\u00b7gen", "uns", "so", "viel", ",", "als", "scharf\u00b7fe", "pfei\u00b7le", ",", "schafft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADV", "ADV", "$,", "KOUS", "ADJA", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein wer wolte nicht den stich des todes f\u00fchlen,", "tokens": ["Al\u00b7lein", "wer", "wol\u00b7te", "nicht", "den", "stich", "des", "to\u00b7des", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "PTKNEG", "ART", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn liebes-engel uns die sterbe-lieder spielen?", "tokens": ["Wenn", "lie\u00b7bes\u00b7en\u00b7gel", "uns", "die", "ster\u00b7be\u00b7lie\u00b7der", "spie\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}