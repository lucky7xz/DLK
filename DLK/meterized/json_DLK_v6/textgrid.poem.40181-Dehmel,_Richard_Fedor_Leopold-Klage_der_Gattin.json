{"textgrid.poem.40181": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Klage der Gattin", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verla\u00dfne Nester h\u00e4ngen am Bache in den Weiden,", "tokens": ["Ver\u00b7la\u00df\u00b7ne", "Nes\u00b7ter", "h\u00e4n\u00b7gen", "am", "Ba\u00b7che", "in", "den", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "die Zittergr\u00e4ser beben im Winde auf der Haiden.", "tokens": ["die", "Zit\u00b7ter\u00b7gr\u00e4\u00b7ser", "be\u00b7ben", "im", "Win\u00b7de", "auf", "der", "Hai\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Oh Lieber, warum wandte", "tokens": ["Oh", "Lie\u00b7ber", ",", "wa\u00b7rum", "wand\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADJD", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "sich ", "tokens": ["sich"], "token_info": ["word"], "pos": ["PRF"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "O Herz, mein Herz, wie wehe", "tokens": ["O", "Herz", ",", "mein", "Herz", ",", "wie", "we\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "dein Bangen thut!", "tokens": ["dein", "Ban\u00b7gen", "thut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Am Grabesh\u00fcgel rauschen ein schaurig Lied die R\u00fcstern,", "tokens": ["Am", "Gra\u00b7bes\u00b7h\u00fc\u00b7gel", "rau\u00b7schen", "ein", "schau\u00b7rig", "Lied", "die", "R\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "am Ufersaum beklommen im Schilf die L\u00fcfte fl\u00fcstern.", "tokens": ["am", "U\u00b7fer\u00b7saum", "be\u00b7klom\u00b7men", "im", "Schilf", "die", "L\u00fcf\u00b7te", "fl\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Oh Lieber, ach! wer raunte", "tokens": ["Oh", "Lie\u00b7ber", ",", "ach", "!", "wer", "raun\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "ADJD", "$,", "ITJ", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "von ", "tokens": ["von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "O Herz, mein Herz, nun findest", "tokens": ["O", "Herz", ",", "mein", "Herz", ",", "nun", "fin\u00b7dest"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "du nimmer Ruh'!", "tokens": ["du", "nim\u00b7mer", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}