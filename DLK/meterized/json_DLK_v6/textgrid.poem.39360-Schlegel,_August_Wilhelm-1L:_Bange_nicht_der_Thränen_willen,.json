{"textgrid.poem.39360": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bange nicht der Thr\u00e4nen willen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bange nicht der Thr\u00e4nen willen,", "tokens": ["Ban\u00b7ge", "nicht", "der", "Thr\u00e4\u00b7nen", "wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in Str\u00f6men mir entquillen,", "tokens": ["Die", "in", "Str\u00f6\u00b7men", "mir", "ent\u00b7quil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ich nicht zu hemmen wei\u00df.", "tokens": ["Die", "ich", "nicht", "zu", "hem\u00b7men", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4chtlich um den Busen starrte", "tokens": ["N\u00e4cht\u00b7lich", "um", "den", "Bu\u00b7sen", "starr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und umsonst auf Sonne harrte", "tokens": ["Und", "um\u00b7sonst", "auf", "Son\u00b7ne", "harr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aufgebirgt des Grames Eis.", "tokens": ["Auf\u00b7ge\u00b7birgt", "des", "Gra\u00b7mes", "Eis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deiner Blicke trautes L\u00e4cheln,", "tokens": ["Dei\u00b7ner", "Bli\u00b7cke", "trau\u00b7tes", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Worte lindes F\u00e4cheln", "tokens": ["Dei\u00b7ner", "Wor\u00b7te", "lin\u00b7des", "F\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Strahlte, hauchte mir in's Herz.", "tokens": ["Strahl\u00b7te", ",", "hauch\u00b7te", "mir", "in's", "Herz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmelskind! da sind die stolzen", "tokens": ["Him\u00b7mel\u00b7skind", "!", "da", "sind", "die", "stol\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Felsen willig hingeschmolzen,", "tokens": ["Fel\u00b7sen", "wil\u00b7lig", "hin\u00b7ge\u00b7schmol\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist gel\u00f6st der alte Schmerz.", "tokens": ["Ist", "ge\u00b7l\u00f6st", "der", "al\u00b7te", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie Thau und Regen flie\u00dfen,", "tokens": ["Und", "wie", "Thau", "und", "Re\u00b7gen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchl' ich frische Blumen sprie\u00dfen", "tokens": ["F\u00fchl'", "ich", "fri\u00b7sche", "Blu\u00b7men", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief aus sehnendem Gem\u00fcth,", "tokens": ["Tief", "aus", "seh\u00b7nen\u00b7dem", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich weihe dir zu Kr\u00e4nzen", "tokens": ["Und", "ich", "wei\u00b7he", "dir", "zu", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Allen Schmuck des neuen Lenzen,", "tokens": ["Al\u00b7len", "Schmuck", "des", "neu\u00b7en", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der in meiner Seele bl\u00fcht.", "tokens": ["Der", "in", "mei\u00b7ner", "See\u00b7le", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}