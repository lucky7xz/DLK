{"textgrid.poem.56383": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbund w\u00e4ren von Gold sie,", "tokens": ["\u00bb", "und", "w\u00e4\u00b7ren", "von", "Gold", "sie", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "APPR", "NN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich g\u00e4be sie dir;", "tokens": ["Ich", "g\u00e4\u00b7be", "sie", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und w\u00e4ren sie Silber,", "tokens": ["Und", "w\u00e4\u00b7ren", "sie", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Du solltest sie haben.\u00ab", "tokens": ["Du", "soll\u00b7test", "sie", "ha\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VAFIN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da flog auf Loki flugs,", "tokens": ["Da", "flog", "auf", "Lo\u00b7ki", "flugs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Fl\u00fcgelschlag rauschte,", "tokens": ["Der", "Fl\u00fc\u00b7gel\u00b7schlag", "rauschte", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Bis hinten er lie\u00df", "tokens": ["Bis", "hin\u00b7ten", "er", "lie\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Das Land der G\u00f6tter,", "tokens": ["Das", "Land", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Und er erreichte", "tokens": ["Und", "er", "er\u00b7reich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Der Riesen Reich.", "tokens": ["Der", "Rie\u00b7sen", "Reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}