{"dta.poem.7777": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als er die\u00df fest gestellt, und noch in etwas wachte,", "tokens": ["Als", "er", "die\u00df", "fest", "ge\u00b7stellt", ",", "und", "noch", "in", "et\u00b7was", "wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADJD", "VVPP", "$,", "KON", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf seiner L\u00e4gerst\u00e4tt, und Gott sein Hertze brachte,", "tokens": ["Auf", "sei\u00b7ner", "L\u00e4\u00b7ger\u00b7st\u00e4tt", ",", "und", "Gott", "sein", "Hert\u00b7ze", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "NN", "VAINF", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Deucht ihm, es k\u00e4m ein Wag f\u00fcr seine H\u00f6l gerannt,", "tokens": ["Deucht", "ihm", ",", "es", "k\u00e4m", "ein", "Wag", "f\u00fcr", "sei\u00b7ne", "H\u00f6l", "ge\u00b7rannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der R\u00e4der rollen macht ihm dieses so bekannt.", "tokens": ["Der", "R\u00e4\u00b7der", "rol\u00b7len", "macht", "ihm", "die\u00b7ses", "so", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "PPER", "PDS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch lie\u00df das Ungest\u00fcm nicht zu, es zu verh\u00f6ren,", "tokens": ["Doch", "lie\u00df", "das", "Un\u00b7ge\u00b7st\u00fcm", "nicht", "zu", ",", "es", "zu", "ver\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was die\u00df bedeuten m\u00f6cht, und ob es Leute w\u00e4ren,", "tokens": ["Was", "die\u00df", "be\u00b7deu\u00b7ten", "m\u00f6cht", ",", "und", "ob", "es", "Leu\u00b7te", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVINF", "VMFIN", "$,", "KON", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch pl\u00f6tzlich hellt die H\u00f6l des starcken Blitzes Schein,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "hellt", "die", "H\u00f6l", "des", "star\u00b7cken", "Blit\u00b7zes", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df er drey Weiber schaut, die allda treten ein.", "tokens": ["Da\u00df", "er", "drey", "Wei\u00b7ber", "schaut", ",", "die", "all\u00b7da", "tre\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das gleich verschwundne Licht l\u00e4st ihn nichts mehrers sehen,", "tokens": ["Das", "gleich", "ver\u00b7schwund\u00b7ne", "Licht", "l\u00e4st", "ihn", "nichts", "meh\u00b7rers", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "VVFIN", "PPER", "PIS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch h\u00f6ret er, da\u00df sie nah bey ihm \u00fcber gehen,", "tokens": ["Doch", "h\u00f6\u00b7ret", "er", ",", "da\u00df", "sie", "nah", "bey", "ihm", "\u00fc\u00b7ber", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPER", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Horcht also in der Still. Mitde\u00df sagt eine Stimm:", "tokens": ["Horcht", "al\u00b7so", "in", "der", "Still", ".", "Mit\u00b7de\u00df", "sagt", "ei\u00b7ne", "Stimm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wohl uns, da\u00df wir allhie nun f\u00fcr des Donners Grimm", "tokens": ["Wohl", "uns", ",", "da\u00df", "wir", "all\u00b7hie", "nun", "f\u00fcr", "des", "Don\u00b7ners", "Grimm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In trockner Sicherheit. Worauf die andre sagte:", "tokens": ["In", "trock\u00b7ner", "Si\u00b7cher\u00b7heit", ".", "Wo\u00b7rauf", "die", "and\u00b7re", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PAV", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ach! wann ich w\u00fcnschen k\u00f6nnt, so wolt ich, da\u00df es tagte,", "tokens": ["Ach", "!", "wann", "ich", "w\u00fcn\u00b7schen", "k\u00f6nnt", ",", "so", "wolt", "ich", ",", "da\u00df", "es", "tag\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "PPER", "VVINF", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich bin so sehr erk\u00e4lt. Dies b\u00fc\u00dft recht unsre Lust,", "tokens": ["Ich", "bin", "so", "sehr", "er\u00b7k\u00e4lt", ".", "Dies", "b\u00fc\u00dft", "recht", "uns\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVFIN", "$.", "PDS", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sprach drauf die dritte Stimm. Ja wer hat die\u00df gewust,", "tokens": ["Sprach", "drauf", "die", "drit\u00b7te", "Stimm", ".", "Ja", "wer", "hat", "die\u00df", "ge\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ART", "ADJA", "NN", "$.", "PTKANT", "PWS", "VAFIN", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die erste wiederredt, wir m\u00fcssen auch erfahren,", "tokens": ["Die", "ers\u00b7te", "wie\u00b7der\u00b7redt", ",", "wir", "m\u00fcs\u00b7sen", "auch", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was Ungemach bedeut, gleich unsern Kriegesschaaren.", "tokens": ["Was", "Un\u00b7ge\u00b7mach", "be\u00b7deut", ",", "gleich", "un\u00b7sern", "Krie\u00b7ges\u00b7schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ja du Soldaten-Weib, sagt eine drauf zu ihr,", "tokens": ["Ja", "du", "Sol\u00b7da\u00b7ten\u00b7Weib", ",", "sagt", "ei\u00b7ne", "drauf", "zu", "ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "NN", "$,", "VVFIN", "ART", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du achtest nichts, du bist es mehr gewohnt, als wir.", "tokens": ["Du", "ach\u00b7test", "nichts", ",", "du", "bist", "es", "mehr", "ge\u00b7wohnt", ",", "als", "wir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du must es lernen auch, die erste wiederholte,", "tokens": ["Du", "must", "es", "ler\u00b7nen", "auch", ",", "die", "ers\u00b7te", "wie\u00b7der\u00b7hol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADV", "$,", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dann wann der Adriel dich noch bekommen sollte,", "tokens": ["Dann", "wann", "der", "Ad\u00b7riel", "dich", "noch", "be\u00b7kom\u00b7men", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.23": {"text": "Must du des weichen Betts noch offte m\u00fc\u00dfig gehn.", "tokens": ["Must", "du", "des", "wei\u00b7chen", "Betts", "noch", "off\u00b7te", "m\u00fc\u00b7\u00dfig", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wohlan, versetzte die, wer wei\u00df was kan geschehn;", "tokens": ["Wo\u00b7hlan", ",", "ver\u00b7setz\u00b7te", "die", ",", "wer", "wei\u00df", "was", "kan", "ge\u00b7schehn", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "$,", "PWS", "VVFIN", "PWS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Allein, was f\u00e4ngt man an? Wir wollen uns hinsetzen,", "tokens": ["Al\u00b7lein", ",", "was", "f\u00e4ngt", "man", "an", "?", "Wir", "wol\u00b7len", "uns", "hin\u00b7set\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PIS", "PTKVZ", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sagt bald die dritte Stimm, und mit einander schw\u00e4tzen.", "tokens": ["Sagt", "bald", "die", "drit\u00b7te", "Stimm", ",", "und", "mit", "ein\u00b7an\u00b7der", "schw\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,", "KON", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ach! Schwester, sing ein Lied, wandt drauf die erste ein,", "tokens": ["Ach", "!", "Schwes\u00b7ter", ",", "sing", "ein", "Lied", ",", "wandt", "drauf", "die", "ers\u00b7te", "ein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "PAV", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Zu Gott soll unser Hertz anitzt gerichtet seyn.", "tokens": ["Zu", "Gott", "soll", "un\u00b7ser", "Hertz", "a\u00b7nitzt", "ge\u00b7rich\u00b7tet", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPOSAT", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "H\u00f6rt wie der H\u00f6chste z\u00f6rnt, wohlan, thu mein Begehren,", "tokens": ["H\u00f6rt", "wie", "der", "H\u00f6chs\u00b7te", "z\u00f6rnt", ",", "wo\u00b7hlan", ",", "thu", "mein", "Be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "VVFIN", "$,", "PWAV", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Du, f\u00fchr die Stimm, und wir, wir wollen es anh\u00f6ren", "tokens": ["Du", ",", "f\u00fchr", "die", "Stimm", ",", "und", "wir", ",", "wir", "wol\u00b7len", "es", "an\u00b7h\u00f6\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Hierauf gab David acht, der keine noch nicht kennt,", "tokens": ["Hier\u00b7auf", "gab", "Da\u00b7vid", "acht", ",", "der", "kei\u00b7ne", "noch", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "CARD", "$,", "PRELS", "PIAT", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Was kommen wird, und h\u00f6rt, da\u00df sie die\u00df Lied ernennt.", "tokens": ["Was", "kom\u00b7men", "wird", ",", "und", "h\u00f6rt", ",", "da\u00df", "sie", "die\u00df", "Lied", "er\u00b7nennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VAFIN", "$,", "KON", "VVFIN", "$,", "KOUS", "PPER", "PDS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}