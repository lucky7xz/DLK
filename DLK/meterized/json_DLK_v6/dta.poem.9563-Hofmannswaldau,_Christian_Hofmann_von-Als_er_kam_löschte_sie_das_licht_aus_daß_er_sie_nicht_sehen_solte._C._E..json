{"dta.poem.9563": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er kam/ l\u00f6schte sie das licht aus/ da\u00df  \n er sie nicht sehen solte.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Versteck dich immer hin! schleich heimlich zu der wand!", "tokens": ["Ver\u00b7steck", "dich", "im\u00b7mer", "hin", "!", "schleich", "heim\u00b7lich", "zu", "der", "wand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKVZ", "$.", "ADJD", "ADJD", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "L\u00f6sch alle lichter aus! du kanst mir nicht verschwinden;", "tokens": ["L\u00f6sch", "al\u00b7le", "lich\u00b7ter", "aus", "!", "du", "kanst", "mir", "nicht", "ver\u00b7schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJD", "PTKVZ", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Halt dich noch eins so still; ich will dich dennoch sinden:", "tokens": ["Halt", "dich", "noch", "eins", "so", "still", ";", "ich", "will", "dich", "den\u00b7noch", "sin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PIS", "ADV", "ADJD", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wird aus dem schatten nicht das helle licht erkant?", "tokens": ["Wird", "aus", "dem", "schat\u00b7ten", "nicht", "das", "hel\u00b7le", "licht", "er\u00b7kant", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gl\u00e4ntzt nicht bey dunckler nacht ein echter diamant?", "tokens": ["Gl\u00e4ntzt", "nicht", "bey", "dunck\u00b7ler", "nacht", "ein", "ech\u00b7ter", "di\u00b7a\u00b7mant", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja/ ob die finsterni\u00df sich selbst wolt unterwinden/", "tokens": ["Ja", "/", "ob", "die", "fins\u00b7ter\u00b7ni\u00df", "sich", "selbst", "wolt", "un\u00b7ter\u00b7win\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "ART", "VVFIN", "PRF", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der sonnen g\u00f6ldnes rad in wolcken einzusp\u00fcnden/", "tokens": ["Der", "son\u00b7nen", "g\u00f6ld\u00b7nes", "rad", "in", "wol\u00b7cken", "ein\u00b7zu\u00b7sp\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So bricht sie doch hervor durch ihren lichten brand.", "tokens": ["So", "bricht", "sie", "doch", "her\u00b7vor", "durch", "ih\u00b7ren", "lich\u00b7ten", "brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Licht/ demant/ sonne bleibt in ihrem werth und pracht:", "tokens": ["Licht", "/", "de\u00b7mant", "/", "son\u00b7ne", "bleibt", "in", "ih\u00b7rem", "werth", "und", "pracht", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "KON", "VVFIN", "APPR", "PPOSAT", "ADJD", "KON", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Was schad\u2019t die finsterni\u00df/ der schatten und die nacht?", "tokens": ["Was", "schad't", "die", "fins\u00b7ter\u00b7ni\u00df", "/", "der", "schat\u00b7ten", "und", "die", "nacht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhr glantz kan desto eh\u2019 uns in die augen steigen.", "tokens": ["Ihr", "glantz", "kan", "des\u00b7to", "eh'", "uns", "in", "die", "au\u00b7gen", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Bistu mir nun mein licht/ demant und sonnen-schein/", "tokens": ["Bis\u00b7tu", "mir", "nun", "mein", "licht", "/", "de\u00b7mant", "und", "son\u00b7nen\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PPOSAT", "NN", "$(", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So wird die demmerung selbst dein verr\u00e4ther seyn/", "tokens": ["So", "wird", "die", "dem\u00b7me\u00b7rung", "selbst", "dein", "ver\u00b7r\u00e4\u00b7ther", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PPOSAT", "ADJD", "VAINF", "$("], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.14": {"text": "Und dich durch schatten/ nacht und finsterni\u00df mir zeigen.", "tokens": ["Und", "dich", "durch", "schat\u00b7ten", "/", "nacht", "und", "fins\u00b7ter\u00b7ni\u00df", "mir", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "$(", "NN", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}