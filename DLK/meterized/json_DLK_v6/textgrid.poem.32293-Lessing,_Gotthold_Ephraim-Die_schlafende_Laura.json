{"textgrid.poem.32293": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die schlafende Laura", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachl\u00e4ssig hingestreckt,", "tokens": ["Nach\u00b7l\u00e4s\u00b7sig", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Brust mit Flor bedeckt,", "tokens": ["Die", "Brust", "mit", "Flor", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der jedem L\u00fcftchen wich,", "tokens": ["Der", "je\u00b7dem", "L\u00fcft\u00b7chen", "wich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das s\u00e4uselnd ihn durchstrich,", "tokens": ["Das", "s\u00e4u\u00b7selnd", "ihn", "durch\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lie\u00df unter jenen Linden", "tokens": ["Lie\u00df", "un\u00b7ter", "je\u00b7nen", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Gl\u00fcck mich Lauren finden.", "tokens": ["Mein", "Gl\u00fcck", "mich", "Lau\u00b7ren", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie schlief, und weit und breit", "tokens": ["Sie", "schlief", ",", "und", "weit", "und", "breit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schlug jede Blum' ihr Haupt zur Erden,", "tokens": ["Schlug", "je\u00b7de", "Blum'", "ihr", "Haupt", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Aus mi\u00dfvergn\u00fcgter Traurigkeit,", "tokens": ["Aus", "mi\u00df\u00b7ver\u00b7gn\u00fcg\u00b7ter", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von Lauren nicht gesehn zu werden.", "tokens": ["Von", "Lau\u00b7ren", "nicht", "ge\u00b7sehn", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sie schlief, und weit und breit", "tokens": ["Sie", "schlief", ",", "und", "weit", "und", "breit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Erschallten keine Nachtigallen,", "tokens": ["Er\u00b7schall\u00b7ten", "kei\u00b7ne", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Aus weiser Furchtsamkeit,", "tokens": ["Aus", "wei\u00b7ser", "Furcht\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Ihr minder zu gefallen,", "tokens": ["Ihr", "min\u00b7der", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Als ihr der Schlaf gefiel,", "tokens": ["Als", "ihr", "der", "Schlaf", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Als ihr der Traum gefiel,", "tokens": ["Als", "ihr", "der", "Traum", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Den sie vielleicht jetzt tr\u00e4umte,", "tokens": ["Den", "sie", "viel\u00b7leicht", "jetzt", "tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.18": {"text": "Von dem, ich hoff' es, tr\u00e4umte,", "tokens": ["Von", "dem", ",", "ich", "hoff'", "es", ",", "tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Der staunend bei ihr stand,", "tokens": ["Der", "stau\u00b7nend", "bei", "ihr", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Und viel zu viel empfand,", "tokens": ["Und", "viel", "zu", "viel", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Um deutlich zu empfinden,", "tokens": ["Um", "deut\u00b7lich", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Um noch es zu empfinden,", "tokens": ["Um", "noch", "es", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Wie viel er da empfand.", "tokens": ["Wie", "viel", "er", "da", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Ich lie\u00df mich sanfte nieder,", "tokens": ["Ich", "lie\u00df", "mich", "sanf\u00b7te", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Ich segnete, ich k\u00fc\u00dfte sie,", "tokens": ["Ich", "seg\u00b7ne\u00b7te", ",", "ich", "k\u00fc\u00df\u00b7te", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ich segnete, und k\u00fc\u00dfte wieder:", "tokens": ["Ich", "seg\u00b7ne\u00b7te", ",", "und", "k\u00fc\u00df\u00b7te", "wie\u00b7der", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und schnell erwachte sie.", "tokens": ["Und", "schnell", "er\u00b7wach\u00b7te", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Schnell taten sich die Augen auf.", "tokens": ["Schnell", "ta\u00b7ten", "sich", "die", "Au\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die Augen? \u2013 nein, der Himmel tat sich auf.", "tokens": ["Die", "Au\u00b7gen", "?", "\u2013", "nein", ",", "der", "Him\u00b7mel", "tat", "sich", "auf", "."], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "PTKANT", "$,", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}