{"dta.poem.20021": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An Peter Bruckmann   Bekannt durch seine geistreich erdachten Ornamente, Denkm\u00fcn-  \n zen und andere kunstreiche Gepr\u00e4ge in Silber und Erz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Silbert\u00f6ne h\u00f6rt' ich klingen", "tokens": ["Sil\u00b7ber\u00b7t\u00f6\u00b7ne", "h\u00f6rt'", "ich", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch vom Schlafe nicht erwacht,", "tokens": ["Noch", "vom", "Schla\u00b7fe", "nicht", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah, wie Mondlicht durch die Nacht,", "tokens": ["Sah", ",", "wie", "Mond\u00b7licht", "durch", "die", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zarte Lichtgestalten dringen.", "tokens": ["Zar\u00b7te", "Licht\u00b7ge\u00b7stal\u00b7ten", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ein Kn\u00e4blein sah ich liegen", "tokens": ["Und", "ein", "Kn\u00e4\u00b7blein", "sah", "ich", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem reinsten Silberschein,", "tokens": ["In", "dem", "reins\u00b7ten", "Sil\u00b7ber\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "J\u00fcngst gebohren mocht' es seyn,", "tokens": ["J\u00fcngst", "ge\u00b7boh\u00b7ren", "mocht'", "es", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcnstlergeist in allen Z\u00fcgen.", "tokens": ["K\u00fcnst\u00b7ler\u00b7geist", "in", "al\u00b7len", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hell umschwebt von Lichtgestalten", "tokens": ["Hell", "um\u00b7schwebt", "von", "Licht\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War das wunderreiche Kind,", "tokens": ["War", "das", "wun\u00b7der\u00b7rei\u00b7che", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Muse nahm es lind,", "tokens": ["Ei\u00b7ne", "Mu\u00b7se", "nahm", "es", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Himmel es zu halten.", "tokens": ["In", "den", "Him\u00b7mel", "es", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und der Himmel feuerspr\u00fchend", "tokens": ["Und", "der", "Him\u00b7mel", "feu\u00b7er\u00b7spr\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelte sein Silber, Gold,", "tokens": ["Wan\u00b7del\u00b7te", "sein", "Sil\u00b7ber", ",", "Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flugs in Bilder reich und hold", "tokens": ["Flugs", "in", "Bil\u00b7der", "reich", "und", "hold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in Blumen lichtergl\u00fchend.", "tokens": ["Und", "in", "Blu\u00b7men", "lich\u00b7ter\u00b7gl\u00fc\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hebend in die Wunderklarheit", "tokens": ["He\u00b7bend", "in", "die", "Wun\u00b7der\u00b7klar\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fc\u00dft die Mus' das Kind und spricht.", "tokens": ["K\u00fc\u00dft", "die", "Mus'", "das", "Kind", "und", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201esey geweiht dem Feu'r und Licht!", "tokens": ["\u201e", "sey", "ge\u00b7weiht", "dem", "Feu'r", "und", "Licht", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "VVPP", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Kunst sey Treu' und Wahrheit.\u201c", "tokens": ["Dei\u00b7ne", "Kunst", "sey", "Treu'", "und", "Wahr\u00b7heit", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nach der Welt verschiednen Enden", "tokens": ["Nach", "der", "Welt", "ver\u00b7schied\u00b7nen", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Str\u00f6mten Blumen, Bilder klar,", "tokens": ["Str\u00f6m\u00b7ten", "Blu\u00b7men", ",", "Bil\u00b7der", "klar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Helden, treu gestellet dar,", "tokens": ["Hel\u00b7den", ",", "treu", "ge\u00b7stel\u00b7let", "dar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzten hell an Busen, H\u00e4nden.", "tokens": ["Gl\u00e4nz\u00b7ten", "hell", "an", "Bu\u00b7sen", ",", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber als ich tief verloren", "tokens": ["A\u00b7ber", "als", "ich", "tief", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch in all die Klarheit lag,", "tokens": ["Noch", "in", "all", "die", "Klar\u00b7heit", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Brach herein der laute Tag", "tokens": ["Brach", "her\u00b7ein", "der", "lau\u00b7te", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Traumbild gieng verloren.", "tokens": ["Und", "mein", "Traum\u00b7bild", "gieng", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und mein Traumbild gieng verloren!", "tokens": ["Und", "mein", "Traum\u00b7bild", "gieng", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber immer ist es mir,", "tokens": ["A\u00b7ber", "im\u00b7mer", "ist", "es", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als sey heut' \u2014 ich sag's nur dir, \u2014", "tokens": ["Als", "sey", "heut'", "ich", "sag's", "nur", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VAFIN", "ADV", "$(", "PPER", "VVFIN", "ADV", "PPER", "$,", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Jener Tag, der dich geboren.", "tokens": ["Je\u00b7ner", "Tag", ",", "der", "dich", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}