{"textgrid.poem.39692": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Die eine Klage", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer die tiefste aller Wunden", "tokens": ["Wer", "die", "tiefs\u00b7te", "al\u00b7ler", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat in Geist und Sinn empfunden", "tokens": ["Hat", "in", "Geist", "und", "Sinn", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bittrer Trennung Schmerz;", "tokens": ["Bit\u00b7trer", "Tren\u00b7nung", "Schmerz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wer geliebt was er verlohren,", "tokens": ["Wer", "ge\u00b7liebt", "was", "er", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lassen mu\u00df was er erkohren,", "tokens": ["Las\u00b7sen", "mu\u00df", "was", "er", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das geliebte Herz,", "tokens": ["Das", "ge\u00b7lieb\u00b7te", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Der versteht in Lust die Thr\u00e4nen", "tokens": ["Der", "ver\u00b7steht", "in", "Lust", "die", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Liebe ewig Sehnen", "tokens": ["Und", "der", "Lie\u00b7be", "e\u00b7wig", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eins in Zwei zu sein,", "tokens": ["Eins", "in", "Zwei", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Eins im Andern sich zu finden,", "tokens": ["Eins", "im", "An\u00b7dern", "sich", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df der Zweiheit Gr\u00e4nzen schwinden", "tokens": ["Da\u00df", "der", "Zwei\u00b7heit", "Gr\u00e4n\u00b7zen", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und des Daseins Pein.", "tokens": ["Und", "des", "Da\u00b7seins", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wer so ganz in Herz und Sinnen", "tokens": ["Wer", "so", "ganz", "in", "Herz", "und", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Konnt' ein Wesen liebgewinnen", "tokens": ["Konnt'", "ein", "We\u00b7sen", "lieb\u00b7ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O! den tr\u00f6stet's nicht", "tokens": ["O", "!", "den", "tr\u00f6s\u00b7tet's", "nicht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ART", "NE", "PTKNEG"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df f\u00fcr Freuden, die verlohren,", "tokens": ["Da\u00df", "f\u00fcr", "Freu\u00b7den", ",", "die", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Neue werden neu gebohren:", "tokens": ["Neu\u00b7e", "wer\u00b7den", "neu", "ge\u00b7boh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jene sind's doch nicht.", "tokens": ["Je\u00b7ne", "sin\u00b7d's", "doch", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Das geliebte, s\u00fc\u00dfe Leben,", "tokens": ["Das", "ge\u00b7lieb\u00b7te", ",", "s\u00fc\u00b7\u00dfe", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Nehmen und dies Geben,", "tokens": ["Die\u00b7ses", "Neh\u00b7men", "und", "dies", "Ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wort und Sinn und Blick,", "tokens": ["Wort", "und", "Sinn", "und", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Dieses Suchen und dies Finden,", "tokens": ["Die\u00b7ses", "Su\u00b7chen", "und", "dies", "Fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Denken und Empfinden", "tokens": ["Die\u00b7ses", "Den\u00b7ken", "und", "Emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Giebt kein Gott zur\u00fcck.", "tokens": ["Giebt", "kein", "Gott", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}