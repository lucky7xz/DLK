{"textgrid.poem.64144": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Leichenzug Otto III", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Welschen, weicht und gebt uns Raum", "tokens": ["Ihr", "Wel\u00b7schen", ",", "weicht", "und", "gebt", "uns", "Raum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und scheut die grimmen Streiche:", "tokens": ["Und", "scheut", "die", "grim\u00b7men", "Strei\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir tragen einen Kaisertraum", "tokens": ["Wir", "tra\u00b7gen", "ei\u00b7nen", "Kai\u00b7ser\u00b7traum"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eine Kaiserleiche.", "tokens": ["Und", "ei\u00b7ne", "Kai\u00b7ser\u00b7lei\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dem J\u00fcngling schien zu nebelgrau", "tokens": ["Dem", "J\u00fcng\u00b7ling", "schien", "zu", "ne\u00b7bel\u00b7grau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schlichte Land der Sachsen,", "tokens": ["Das", "schlich\u00b7te", "Land", "der", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihn zog's nach S\u00fcdens goldner Au,", "tokens": ["Ihn", "zo\u00b7g's", "nach", "S\u00fc\u00b7dens", "gold\u00b7ner", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo stolz die Lorbeern wachsen.", "tokens": ["Wo", "stolz", "die", "Lor\u00b7beern", "wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Romstadt, die am Tiber prangt,", "tokens": ["Der", "Rom\u00b7stadt", ",", "die", "am", "Ti\u00b7ber", "prangt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr galten seine Taten: \u2013", "tokens": ["Ihr", "gal\u00b7ten", "sei\u00b7ne", "Ta\u00b7ten", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die R\u00f6mer haben's ihm gedankt,", "tokens": ["Die", "R\u00f6\u00b7mer", "ha\u00b7ben's", "ihm", "ge\u00b7dankt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und haben ihn verraten.", "tokens": ["Und", "ha\u00b7ben", "ihn", "ver\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er ruhte nicht, bis er aufs neu'", "tokens": ["Er", "ruh\u00b7te", "nicht", ",", "bis", "er", "aufs", "neu'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "APPRART", "ADJA"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ihr stolzes Reich gestiftet: \u2013", "tokens": ["Ihr", "stol\u00b7zes", "Reich", "ge\u00b7stif\u00b7tet", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die R\u00f6mer schwuren ew'ge Treu'", "tokens": ["Die", "R\u00f6\u00b7mer", "schwu\u00b7ren", "ew'\u00b7ge", "Treu'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und haben ihn vergiftet.", "tokens": ["Und", "ha\u00b7ben", "ihn", "ver\u00b7gif\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als sein Herz litt Sterbensqual,", "tokens": ["Und", "als", "sein", "Herz", "litt", "Ster\u00b7bens\u00b7qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begann es, deutsch zu schlagen: \u2013", "tokens": ["Be\u00b7gann", "es", ",", "deutsch", "zu", "schla\u00b7gen", ":", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das war das erst- und letztemal", "tokens": ["Das", "war", "das", "er\u00b7st", "und", "letz\u00b7te\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "TRUNC", "KON", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In allen seinen Tagen.", "tokens": ["In", "al\u00b7len", "sei\u00b7nen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er sprach: \u00bbIhr Freunde treu und schlicht,", "tokens": ["Er", "sprach", ":", "\u00bb", "Ihr", "Freun\u00b7de", "treu", "und", "schlicht", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPOSAT", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tragt mich zum Heimatlande,", "tokens": ["Tragt", "mich", "zum", "Hei\u00b7mat\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft einsam meine Asche nicht", "tokens": ["La\u00dft", "ein\u00b7sam", "mei\u00b7ne", "A\u00b7sche", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf fremdem, falschem Strande.\u00ab", "tokens": ["Auf", "frem\u00b7dem", ",", "fal\u00b7schem", "Stran\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und als er hob zum letztenmal", "tokens": ["Und", "als", "er", "hob", "zum", "letz\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Haupt in goldnen Locken,", "tokens": ["Das", "Haupt", "in", "gold\u00b7nen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da heulten dr\u00f6hnend in den Saal", "tokens": ["Da", "heul\u00b7ten", "dr\u00f6h\u00b7nend", "in", "den", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Sturm die r\u00f6m'schen Glocken.", "tokens": ["Zum", "Sturm", "die", "r\u00f6m'\u00b7schen", "Glo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und als sein Blick den Glanz verlor", "tokens": ["Und", "als", "sein", "Blick", "den", "Glanz", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stand das Haus in Flammen:", "tokens": ["Da", "stand", "das", "Haus", "in", "Flam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir aber brachen aus dem Tor", "tokens": ["Wir", "a\u00b7ber", "bra\u00b7chen", "aus", "dem", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hieben sie zusammen.", "tokens": ["Und", "hie\u00b7ben", "sie", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da gab's ein m\u00e4chtig Schrein und Fliehn,", "tokens": ["Da", "gab's", "ein", "m\u00e4ch\u00b7tig", "Schrein", "und", "Fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tiber ging in Leichen,", "tokens": ["Der", "Ti\u00b7ber", "ging", "in", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Forum und der Palatin", "tokens": ["Das", "Fo\u00b7rum", "und", "der", "Pa\u00b7la\u00b7tin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Erscholl von deutschen Streichen.", "tokens": ["Er\u00b7scholl", "von", "deut\u00b7schen", "Strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wir trugen ihn von hinnen frei,", "tokens": ["Wir", "tru\u00b7gen", "ihn", "von", "hin\u00b7nen", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Blut den Schritt erworben,", "tokens": ["Mit", "Blut", "den", "Schritt", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und unter unserm Siegsgeschrei", "tokens": ["Und", "un\u00b7ter", "un\u00b7serm", "Siegs\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist l\u00e4chelnd er gestorben.", "tokens": ["Ist", "l\u00e4\u00b7chelnd", "er", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wir tragen auf zwei Lanzen quer", "tokens": ["Wir", "tra\u00b7gen", "auf", "zwei", "Lan\u00b7zen", "quer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Sarg bei Sturmgel\u00e4ute:", "tokens": ["Den", "Sarg", "bei", "Sturm\u00b7ge\u00b7l\u00e4u\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Welschen schw\u00e4rmen um uns her", "tokens": ["Die", "Wel\u00b7schen", "schw\u00e4r\u00b7men", "um", "uns", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie W\u00f6lfe nach der Beute.", "tokens": ["Wie", "W\u00f6l\u00b7fe", "nach", "der", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Von jedem Dach fliegt Stein und Erz,", "tokens": ["Von", "je\u00b7dem", "Dach", "fliegt", "Stein", "und", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es gellt der Weiber Stimme:", "tokens": ["Es", "gellt", "der", "Wei\u00b7ber", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir ziehn dahin mit Stolz und Schmerz,", "tokens": ["Wir", "ziehn", "da\u00b7hin", "mit", "Stolz", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit stillem, hei\u00dfem Grimme.", "tokens": ["Mit", "stil\u00b7lem", ",", "hei\u00b7\u00dfem", "Grim\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Den Helm geschlossen, nackt das Schwert,", "tokens": ["Den", "Helm", "ge\u00b7schlos\u00b7sen", ",", "nackt", "das", "Schwert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schild umklirrt von Pfeilen,", "tokens": ["Den", "Schild", "um\u00b7klirrt", "von", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ziehn wir, den Alpen zugekehrt,", "tokens": ["Ziehn", "wir", ",", "den", "Al\u00b7pen", "zu\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Still, langsam, sonder Eilen.", "tokens": ["Still", ",", "lang\u00b7sam", ",", "son\u00b7der", "Ei\u00b7len", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Denn eine edel heil'ge Last", "tokens": ["Denn", "ei\u00b7ne", "e\u00b7del", "heil'\u00b7ge", "Last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir tragen in der Mitte:", "tokens": ["Wir", "tra\u00b7gen", "in", "der", "Mit\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ziemet keine schn\u00f6de Hast,", "tokens": ["Da", "zie\u00b7met", "kei\u00b7ne", "schn\u00f6\u00b7de", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ziemen stete Schritte.", "tokens": ["Da", "zie\u00b7men", "ste\u00b7te", "Schrit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Die k\u00fchnen Schwaben schreiten vorn,", "tokens": ["Die", "k\u00fch\u00b7nen", "Schwa\u00b7ben", "schrei\u00b7ten", "vorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Links Bayern, rechts die Franken.", "tokens": ["Links", "Bay\u00b7ern", ",", "rechts", "die", "Fran\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den R\u00fccken decken, j\u00e4h im Zorn,", "tokens": ["Den", "R\u00fc\u00b7cken", "de\u00b7cken", ",", "j\u00e4h", "im", "Zorn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sachsen, die nicht wanken.", "tokens": ["Die", "Sach\u00b7sen", ",", "die", "nicht", "wan\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "So ziehn wir traurig, grimmig, stolz:", "tokens": ["So", "ziehn", "wir", "trau\u00b7rig", ",", "grim\u00b7mig", ",", "stolz", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Tag trotzt uns kein Degen:", "tokens": ["Am", "Tag", "trotzt", "uns", "kein", "De\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von r\u00fcckw\u00e4rts nur zischt Pfeil und Bolz", "tokens": ["Von", "r\u00fcck\u00b7w\u00e4rts", "nur", "zischt", "Pfeil", "und", "Bolz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus \u00d6l- und Weingehegen.", "tokens": ["Aus", "\u00d6l", "und", "Wein\u00b7ge\u00b7he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Und fall'n sie uns zur Nachtzeit an, \u2013", "tokens": ["Und", "fall'n", "sie", "uns", "zur", "Nacht\u00b7zeit", "an", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie finden wache Herzen,", "tokens": ["Sie", "fin\u00b7den", "wa\u00b7che", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir z\u00fcnden ihre D\u00f6rfer an", "tokens": ["Wir", "z\u00fcn\u00b7den", "ih\u00b7re", "D\u00f6r\u00b7fer", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu roten Leichenkerzen.", "tokens": ["Zu", "ro\u00b7ten", "Lei\u00b7chen\u00b7ker\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Haut nieder, was heran sich wagt,", "tokens": ["Haut", "nie\u00b7der", ",", "was", "he\u00b7ran", "sich", "wagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWS", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schont Weiber nur und Kinder,", "tokens": ["Schont", "Wei\u00b7ber", "nur", "und", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder, den ihr niederschlagt,", "tokens": ["Und", "je\u00b7der", ",", "den", "ihr", "nie\u00b7der\u00b7schlagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist ein Todfeind minder.", "tokens": ["Das", "ist", "ein", "Tod\u00b7feind", "min\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "So ziehn wir fort durch Land und Strom,", "tokens": ["So", "ziehn", "wir", "fort", "durch", "Land", "und", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Vaterland entgegen,", "tokens": ["Dem", "Va\u00b7ter\u00b7land", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis wir die heil'ge Last im Dom", "tokens": ["Bis", "wir", "die", "heil'\u00b7ge", "Last", "im", "Dom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Aachen niederlegen.", "tokens": ["Zu", "Aa\u00b7chen", "nie\u00b7der\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}