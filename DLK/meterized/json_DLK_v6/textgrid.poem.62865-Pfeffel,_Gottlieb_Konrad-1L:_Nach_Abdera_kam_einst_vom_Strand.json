{"textgrid.poem.62865": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach Abdera kam einst vom Strand", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Abdera kam einst vom Strand", "tokens": ["Nach", "Ab\u00b7de\u00b7ra", "kam", "einst", "vom", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Nils ein Haufe Colonisten.", "tokens": ["Des", "Nils", "ein", "Hau\u00b7fe", "Co\u00b7lo\u00b7nis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie setzten sich in \u00f6de W\u00fcsten", "tokens": ["Sie", "setz\u00b7ten", "sich", "in", "\u00f6\u00b7de", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schufen sie zum Weitzenland.", "tokens": ["Und", "schu\u00b7fen", "sie", "zum", "Weit\u00b7zen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Ding gefiel den Abderiten,", "tokens": ["Das", "Ding", "ge\u00b7fiel", "den", "Ab\u00b7de\u00b7ri\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und weislich schlo\u00df der Magistrat,", "tokens": ["Und", "weis\u00b7lich", "schlo\u00df", "der", "Ma\u00b7gist\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Pflanzer sollten sich vom Staat", "tokens": ["Die", "Pflan\u00b7zer", "soll\u00b7ten", "sich", "vom", "Staat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Merkmal seiner Huld erbitten:", "tokens": ["Ein", "Merk\u00b7mal", "sei\u00b7ner", "Huld", "er\u00b7bit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das V\u00f6lkchen gieng nicht lang zu Rath.", "tokens": ["Das", "V\u00f6lk\u00b7chen", "gieng", "nicht", "lang", "zu", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Drey Greise mit gebleichten Haaren,", "tokens": ["Drey", "Grei\u00b7se", "mit", "ge\u00b7bleich\u00b7ten", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die V\u00e4ter dieser Pflanzstadt waren,", "tokens": ["Die", "V\u00e4\u00b7ter", "die\u00b7ser", "Pflanz\u00b7stadt", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Erschienen froh vor dem Senat:", "tokens": ["Er\u00b7schie\u00b7nen", "froh", "vor", "dem", "Se\u00b7nat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "La\u00dft uns, so flehte voll Vertrauen", "tokens": ["La\u00dft", "uns", ",", "so", "fleh\u00b7te", "voll", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "ADV", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ihr Haupt, f\u00fcr unsrer Arbeit Lohn,", "tokens": ["Ihr", "Haupt", ",", "f\u00fcr", "uns\u00b7rer", "Ar\u00b7beit", "Lohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der G\u00f6ttin unsrer Nation,", "tokens": ["Der", "G\u00f6t\u00b7tin", "uns\u00b7rer", "Na\u00b7tion", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Der Isis, einen Tempel bauen.", "tokens": ["Der", "I\u00b7sis", ",", "ei\u00b7nen", "Tem\u00b7pel", "bau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Archon bebt auf seinem Thron;", "tokens": ["Der", "Ar\u00b7chon", "bebt", "auf", "sei\u00b7nem", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sein Blick verk\u00fcndigt Angst und Grauen:", "tokens": ["Sein", "Blick", "ver\u00b7k\u00fcn\u00b7digt", "Angst", "und", "Grau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ihr guten Leute, rief er aus,", "tokens": ["Ihr", "gu\u00b7ten", "Leu\u00b7te", ",", "rief", "er", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "K\u00f6nnt, wenn ihr wollt, ein Hurenhaus,", "tokens": ["K\u00f6nnt", ",", "wenn", "ihr", "wollt", ",", "ein", "Hu\u00b7ren\u00b7haus", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VMFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Nur keinen fremden Tempel bauen.", "tokens": ["Nur", "kei\u00b7nen", "frem\u00b7den", "Tem\u00b7pel", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}