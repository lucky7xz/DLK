{"textgrid.poem.37022": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Ein Sultanchen", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein f\u00fcrchterlich Geheul durcht\u00f6nt die Gasse.", "tokens": ["Ein", "f\u00fcrch\u00b7ter\u00b7lich", "Ge\u00b7heul", "durch\u00b7t\u00f6nt", "die", "Gas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was schreit, wor\u00fcber klagt des Volkes Masse?", "tokens": ["Was", "schreit", ",", "wo\u00b7r\u00fc\u00b7ber", "klagt", "des", "Vol\u00b7kes", "Mas\u00b7se", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was ist's, um das das Weib so fest umklammert", "tokens": ["Was", "ist's", ",", "um", "das", "das", "Weib", "so", "fest", "um\u00b7klam\u00b7mert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "KOUI", "ART", "ART", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Gatten und mit ihren Kindern jammert?", "tokens": ["Den", "Gat\u00b7ten", "und", "mit", "ih\u00b7ren", "Kin\u00b7dern", "jam\u00b7mert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie kommt's, da\u00df ich die ernsten M\u00e4nner sehe", "tokens": ["Wie", "kommt's", ",", "da\u00df", "ich", "die", "erns\u00b7ten", "M\u00e4n\u00b7ner", "se\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verzweifelnd stehn, und da\u00df sie rufen: Wehe!?", "tokens": ["Ver\u00b7zwei\u00b7felnd", "stehn", ",", "und", "da\u00df", "sie", "ru\u00b7fen", ":", "We\u00b7he", "!?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "VVINF", "$,", "KON", "KOUS", "PPER", "VVINF", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was schrein so schmerzlich selbst die Million\u00e4re", "tokens": ["Was", "schrein", "so", "schmerz\u00b7lich", "selbst", "die", "Mil\u00b7li\u00b7o\u00b7n\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als ob der Zins herabgegangen w\u00e4re?", "tokens": ["Als", "ob", "der", "Zins", "her\u00b7ab\u00b7ge\u00b7gan\u00b7gen", "w\u00e4\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Welch Schicksal, da\u00df die Mufti's an den S\u00e4ulen", "tokens": ["Welch", "Schick\u00b7sal", ",", "da\u00df", "die", "Muf\u00b7ti's", "an", "den", "S\u00e4u\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Tempels stehn und mit den Junkern heulen?", "tokens": ["Des", "Tem\u00b7pels", "stehn", "und", "mit", "den", "Jun\u00b7kern", "heu\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbdie Antwort, Fremdling, br\u00fcllen vor den Thoren", "tokens": ["\u00bb", "die", "Ant\u00b7wort", ",", "Fremd\u00b7ling", ",", "br\u00fcl\u00b7len", "vor", "den", "Tho\u00b7ren"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kanonen Dir: Ein Prinz ist heut geboren!", "tokens": ["Ka\u00b7no\u00b7nen", "Dir", ":", "Ein", "Prinz", "ist", "heut", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$.", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Es ist der F\u00fcnfunddrei\u00dfigste des Schlosses,", "tokens": ["Es", "ist", "der", "F\u00fcn\u00b7fund\u00b7drei\u00b7\u00dfigs\u00b7te", "des", "Schlos\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Sultan Pumpel-Pampelschen Gesprosses!", "tokens": ["Des", "Sul\u00b7tan", "Pum\u00b7pel\u00b7Pam\u00b7pel\u00b7schen", "Ge\u00b7spros\u00b7ses", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wir klagen nicht um uns; denn ", "tokens": ["Wir", "kla\u00b7gen", "nicht", "um", "uns", ";", "denn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$.", "KON"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Jedweder Prinz! Wir klagen ", "tokens": ["Jed\u00b7we\u00b7der", "Prinz", "!", "Wir", "kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es schmerzt uns, da\u00df schon wieder, ach! ein Wesen", "tokens": ["Es", "schmerzt", "uns", ",", "da\u00df", "schon", "wie\u00b7der", ",", "ach", "!", "ein", "We\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ADV", "$,", "ITJ", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Opfer f\u00fcr das Volkswohl auserlesen!", "tokens": ["Zum", "Op\u00b7fer", "f\u00fcr", "das", "Volks\u00b7wohl", "au\u00b7ser\u00b7le\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ein Wesen, o! auf dem schon in der Wiegen", "tokens": ["Ein", "We\u00b7sen", ",", "o", "!", "auf", "dem", "schon", "in", "der", "Wie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "FM", "$.", "APPR", "ART", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So schwere Lasten f\u00fcr das Volkswohl liegen!", "tokens": ["So", "schwe\u00b7re", "Las\u00b7ten", "f\u00fcr", "das", "Volks\u00b7wohl", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Ein Wesen, dem das Loos, statt eins zu w\u00e4hlen,", "tokens": ["Ein", "We\u00b7sen", ",", "dem", "das", "Loos", ",", "statt", "eins", "zu", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "$,", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bestimmt ist, sich f\u00fcr's Volkswohl abzuqu\u00e4len!", "tokens": ["Be\u00b7stimmt", "ist", ",", "sich", "f\u00fcr's", "Volks\u00b7wohl", "ab\u00b7zu\u00b7qu\u00e4\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRF", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Da\u00df er sein Leben mu\u00df dem Volkswohl weihen!\u00ab", "tokens": ["Da\u00df", "er", "sein", "Le\u00b7ben", "mu\u00df", "dem", "Volks\u00b7wohl", "wei\u00b7hen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Ich nickte schmerzlich, ging hinab zur Gasse", "tokens": ["Ich", "nick\u00b7te", "schmerz\u00b7lich", ",", "ging", "hin\u00b7ab", "zur", "Gas\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schrie laut auf und heulte mit der Masse.", "tokens": ["Und", "schrie", "laut", "auf", "und", "heul\u00b7te", "mit", "der", "Mas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}