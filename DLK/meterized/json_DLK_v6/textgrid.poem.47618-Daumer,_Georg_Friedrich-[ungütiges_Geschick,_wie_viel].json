{"textgrid.poem.47618": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ung\u00fctiges Geschick, wie viel]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ung\u00fctiges Geschick, wie viel", "tokens": ["Un\u00b7g\u00fc\u00b7ti\u00b7ges", "Ge\u00b7schick", ",", "wie", "viel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Giebst du dem Herzen des Begehrens!", "tokens": ["Giebst", "du", "dem", "Her\u00b7zen", "des", "Be\u00b7geh\u00b7rens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schreit zu dir das flammende,", "tokens": ["Und", "schreit", "zu", "dir", "das", "flam\u00b7men\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Welch eine Karge des Gew\u00e4hrens!", "tokens": ["Welch", "ei\u00b7ne", "Kar\u00b7ge", "des", "Ge\u00b7w\u00e4h\u00b7rens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}