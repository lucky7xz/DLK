{"textgrid.poem.33449": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Der Traurige", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, dies Leben fa\u00df ich nicht mehr an.", "tokens": ["Nein", ",", "dies", "Le\u00b7ben", "fa\u00df", "ich", "nicht", "mehr", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mag man mich f\u00fcr n\u00e4rrisch halten \u2013", "tokens": ["Mag", "man", "mich", "f\u00fcr", "n\u00e4r\u00b7risch", "hal\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "APPR", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute geh ich nicht ins Gasthaus.", "tokens": ["Heu\u00b7te", "geh", "ich", "nicht", "ins", "Gast\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcde bin ich l\u00e4ngst der Kellnerkerle,", "tokens": ["M\u00fc\u00b7de", "bin", "ich", "l\u00e4ngst", "der", "Kell\u00b7ner\u00b7ker\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Die uns mit blasierten Fratzen,", "tokens": ["Die", "uns", "mit", "bla\u00b7sier\u00b7ten", "Frat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6hnisch, schwarze Biere bringen", "tokens": ["H\u00f6h\u00b7nisch", ",", "schwar\u00b7ze", "Bie\u00b7re", "brin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und uns ganz verworren machen,", "tokens": ["Und", "uns", "ganz", "ver\u00b7wor\u00b7ren", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df wir nicht nach Hause finden", "tokens": ["Da\u00df", "wir", "nicht", "nach", "Hau\u00b7se", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die t\u00f6richten Laternen", "tokens": ["Und", "die", "t\u00f6\u00b7rich\u00b7ten", "La\u00b7ter\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Mit den schwachen H\u00e4nden st\u00fctzen", "tokens": ["Mit", "den", "schwa\u00b7chen", "H\u00e4n\u00b7den", "st\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "M\u00fcssen.", "tokens": ["M\u00fcs\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Heute hab ich gr\u00f6\u00dfre Dinge vor \u2013", "tokens": ["Heu\u00b7te", "hab", "ich", "gr\u00f6\u00df\u00b7re", "Din\u00b7ge", "vor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "APPR", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Ach, ich will den Sinn des Daseins suchen.", "tokens": ["Ach", ",", "ich", "will", "den", "Sinn", "des", "Da\u00b7seins", "su\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und am Abend werd ich etwas Rollschuh laufen", "tokens": ["Und", "am", "A\u00b7bend", "werd", "ich", "et\u00b7was", "Roll\u00b7schuh", "lau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Oder mal in einen Judentempel gehn.", "tokens": ["O\u00b7der", "mal", "in", "ei\u00b7nen", "Ju\u00b7den\u00b7tem\u00b7pel", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}