{"textgrid.poem.64920": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wen einmal du geliebt, der sei f\u00fcr alle Zeit,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen einmal du geliebt, der sei f\u00fcr alle Zeit,", "tokens": ["Wen", "ein\u00b7mal", "du", "ge\u00b7liebt", ",", "der", "sei", "f\u00fcr", "al\u00b7le", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVPP", "$,", "PRELS", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jedem Lebensdrang dir heilig und geweiht.", "tokens": ["In", "je\u00b7dem", "Le\u00b7bens\u00b7drang", "dir", "hei\u00b7lig", "und", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ob er der Liebe, die du einst f\u00fcr ihn getragen,", "tokens": ["Ob", "er", "der", "Lie\u00b7be", ",", "die", "du", "einst", "f\u00fcr", "ihn", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch werth gewesen sei? das hast du nicht zu fragen.", "tokens": ["Auch", "werth", "ge\u00b7we\u00b7sen", "sei", "?", "das", "hast", "du", "nicht", "zu", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAPP", "VAFIN", "$.", "PDS", "VAFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Steht doch das Eine fest, du hast ihn einst geliebt!", "tokens": ["Steht", "doch", "das", "Ei\u00b7ne", "fest", ",", "du", "hast", "ihn", "einst", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist's, was ihm ein Recht, ein ew'ges auf dich gibt.", "tokens": ["Das", "ist's", ",", "was", "ihm", "ein", "Recht", ",", "ein", "ew'\u00b7ges", "auf", "dich", "gibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "ART", "NN", "$,", "ART", "ADJA", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "W\u00e4r' er der Schonung auch ganz unwerth zu erkl\u00e4ren,", "tokens": ["W\u00e4r'", "er", "der", "Scho\u00b7nung", "auch", "ganz", "un\u00b7werth", "zu", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du m\u00fc\u00dftest das Gef\u00fchl, das du ihm weihtest, ehren.", "tokens": ["Du", "m\u00fc\u00df\u00b7test", "das", "Ge\u00b7f\u00fchl", ",", "das", "du", "ihm", "weih\u00b7test", ",", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und ehren kannst du's nur durch immer gleiche Huld", "tokens": ["Und", "eh\u00b7ren", "kannst", "du's", "nur", "durch", "im\u00b7mer", "glei\u00b7che", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PIS", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr Jenen, dem es galt, wie gro\u00df auch seine Schuld.", "tokens": ["F\u00fcr", "Je\u00b7nen", ",", "dem", "es", "galt", ",", "wie", "gro\u00df", "auch", "sei\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "PRELS", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nicht lieben sollst du ihn, ist falsch und schlecht sein Wesen;", "tokens": ["Nicht", "lie\u00b7ben", "sollst", "du", "ihn", ",", "ist", "falsch", "und", "schlecht", "sein", "We\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "PPER", "$,", "VAFIN", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch auch vergessen nicht, da\u00df er dir lieb gewesen.", "tokens": ["Doch", "auch", "ver\u00b7ges\u00b7sen", "nicht", ",", "da\u00df", "er", "dir", "lieb", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wenn eine ird'sche Kron' so gro\u00dfe Macht schon hegt,", "tokens": ["Wenn", "ei\u00b7ne", "ird'\u00b7sche", "Kron'", "so", "gro\u00b7\u00dfe", "Macht", "schon", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df unverletzlich wird, wer sie auch immer tr\u00e4gt:", "tokens": ["Da\u00df", "un\u00b7ver\u00b7letz\u00b7lich", "wird", ",", "wer", "sie", "auch", "im\u00b7mer", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "$,", "PWS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wie m\u00f6chtest du ein Haupt wohl zu verletzen wagen,", "tokens": ["Wie", "m\u00f6ch\u00b7test", "du", "ein", "Haupt", "wohl", "zu", "ver\u00b7let\u00b7zen", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das einst das Diadem der Liebe hat getragen?", "tokens": ["Das", "einst", "das", "Di\u00b7a\u00b7dem", "der", "Lie\u00b7be", "hat", "ge\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}