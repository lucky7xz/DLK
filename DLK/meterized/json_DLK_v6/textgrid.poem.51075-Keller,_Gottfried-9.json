{"textgrid.poem.51075": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "9", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zw\u00f6lf hat's geschlagen \u2013 warum denn Mittag?", "tokens": ["Zw\u00f6lf", "hat's", "ge\u00b7schla\u00b7gen", "\u2013", "wa\u00b7rum", "denn", "Mit\u00b7tag", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "VVPP", "$(", "PWAV", "ADV", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vielleicht der Mitternacht ja galt der Schlag,", "tokens": ["Viel\u00b7leicht", "der", "Mit\u00b7ter\u00b7nacht", "ja", "galt", "der", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df oben nun des Himmels Sterne gehn:", "tokens": ["Da\u00df", "o\u00b7ben", "nun", "des", "Him\u00b7mels", "Ster\u00b7ne", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich wei\u00df es nicht und kann es ja nicht sehn!", "tokens": ["Ich", "wei\u00df", "es", "nicht", "und", "kann", "es", "ja", "nicht", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ha, Mitternacht! Ein heller Hoffnungsstrahl!", "tokens": ["Ha", ",", "Mit\u00b7ter\u00b7nacht", "!", "Ein", "hel\u00b7ler", "Hoff\u00b7nungs\u00b7strahl", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der n\u00e4chtlich wohl schon manches Grab bestahl,", "tokens": ["Der", "n\u00e4cht\u00b7lich", "wohl", "schon", "man\u00b7ches", "Grab", "be\u00b7stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Totengr\u00e4ber schleicht vielleicht herbei", "tokens": ["Der", "To\u00b7ten\u00b7gr\u00e4\u00b7ber", "schleicht", "viel\u00b7leicht", "her\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und macht erschrocken mich Lebend'gen frei!", "tokens": ["Und", "macht", "er\u00b7schro\u00b7cken", "mich", "Le\u00b7ben\u00b7d'\u00b7gen", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch was f\u00fcr Kleinod sollt er suchen hier?", "tokens": ["Doch", "was", "f\u00fcr", "Klei\u00b7nod", "sollt", "er", "su\u00b7chen", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er wei\u00df zu gut, er findet nichts bei mir!", "tokens": ["Er", "wei\u00df", "zu", "gut", ",", "er", "fin\u00b7det", "nichts", "bei", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein golden Ringlein nun erl\u00f6ste mich,", "tokens": ["Ein", "gol\u00b7den", "Rin\u00b7glein", "nun", "er\u00b7l\u00f6s\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jedoch umsonst ist nur der Tod f\u00fcr dich!", "tokens": ["Je\u00b7doch", "um\u00b7sonst", "ist", "nur", "der", "Tod", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}