{"textgrid.poem.25017": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In deines Auges klare Quelle", "tokens": ["In", "dei\u00b7nes", "Au\u00b7ges", "kla\u00b7re", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Taucht sich mein Geist wie in ein Bad;", "tokens": ["Taucht", "sich", "mein", "Geist", "wie", "in", "ein", "Bad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "KOKOM", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt strahlt ihm in reinrer Helle,", "tokens": ["Die", "Welt", "strahlt", "ihm", "in", "rein\u00b7rer", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn er in ihr vom Staub gekl\u00e4rt sich hat.", "tokens": ["Wenn", "er", "in", "ihr", "vom", "Staub", "ge\u00b7kl\u00e4rt", "sich", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPRART", "NN", "VVPP", "PRF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er schwebt dahin mit lichter Schwinge,", "tokens": ["Er", "schwebt", "da\u00b7hin", "mit", "lich\u00b7ter", "Schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als ob erstanden aus dem Grab;", "tokens": ["Als", "ob", "er\u00b7stan\u00b7den", "aus", "dem", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durchsichtig werden ihm die Dinge;", "tokens": ["Durch\u00b7sich\u00b7tig", "wer\u00b7den", "ihm", "die", "Din\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis auf den tiefsten Grund schaut er hinab.", "tokens": ["Bis", "auf", "den", "tiefs\u00b7ten", "Grund", "schaut", "er", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was vor Jahrtausenden gewesen,", "tokens": ["Was", "vor", "Jahr\u00b7tau\u00b7sen\u00b7den", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie was in Zukunft unser harrt,", "tokens": ["Wie", "was", "in", "Zu\u00b7kunft", "un\u00b7ser", "harrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWS", "APPR", "NN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann er in ", "tokens": ["Kann", "er", "in"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und alles doch ist holde Gegenwart!", "tokens": ["Und", "al\u00b7les", "doch", "ist", "hol\u00b7de", "Ge\u00b7gen\u00b7wart", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}