{"textgrid.poem.48577": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9. Auf eine Hochzeit in Leipzig", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df fest nicht, was ich dichten,", "tokens": ["Ich", "wei\u00df", "fest", "nicht", ",", "was", "ich", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKNEG", "$,", "PWS", "PPER", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Br\u00e4utgam, was ich setzen soll.", "tokens": ["Br\u00e4ut\u00b7gam", ",", "was", "ich", "set\u00b7zen", "soll", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du bist Freud' und Leides voll.", "tokens": ["Du", "bist", "Freud'", "und", "Lei\u00b7des", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll ich mich nach dir denn richten,", "tokens": ["Soll", "ich", "mich", "nach", "dir", "denn", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wie ich soll, so mu\u00df auch ich", "tokens": ["wie", "ich", "soll", ",", "so", "mu\u00df", "auch", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VMFIN", "$,", "ADV", "VMFIN", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "leid und froh geberden mich.", "tokens": ["leid", "und", "froh", "ge\u00b7ber\u00b7den", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Neulich sahen wir zu Grabe", "tokens": ["Neu\u00b7lich", "sa\u00b7hen", "wir", "zu", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "deiner liebsten Schwester ziehn.", "tokens": ["dei\u00b7ner", "liebs\u00b7ten", "Schwes\u00b7ter", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun ist auch die Mutter hin.", "tokens": ["Nun", "ist", "auch", "die", "Mut\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Heute setzt der gro\u00dfe Knabe", "tokens": ["Heu\u00b7te", "setzt", "der", "gro\u00b7\u00dfe", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "den Termin der Trauung auf", "tokens": ["den", "Ter\u00b7min", "der", "Trau\u00b7ung", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "und zahlt eurer Liebe Kauf.", "tokens": ["und", "zahlt", "eu\u00b7rer", "Lie\u00b7be", "Kauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Weinend m\u00fcsset ihr nun lachen,", "tokens": ["Wei\u00b7nend", "m\u00fcs\u00b7set", "ihr", "nun", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "lachend m\u00fcsset weinen ihr,", "tokens": ["la\u00b7chend", "m\u00fcs\u00b7set", "wei\u00b7nen", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VVINF", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe; Br\u00e4utgam, sie mit dir,", "tokens": ["Lie\u00b7be", ";", "Br\u00e4ut\u00b7gam", ",", "sie", "mit", "dir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "$,", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "du mit ihr, die di\u00df kan machen,", "tokens": ["du", "mit", "ihr", ",", "die", "di\u00df", "kan", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "$,", "PRELS", "PDS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df du seufzen bei der Lust", "tokens": ["da\u00df", "du", "seuf\u00b7zen", "bei", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und im Trauren froh sein mu\u00dft.", "tokens": ["und", "im", "Trau\u00b7ren", "froh", "sein", "mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer sich in sein Gl\u00fccke schicket,", "tokens": ["Wer", "sich", "in", "sein", "Gl\u00fc\u00b7cke", "schi\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "der tut was Gott selbsten wil,", "tokens": ["der", "tut", "was", "Gott", "selbs\u00b7ten", "wil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "zagt in N\u00f6ten nicht zu viel,", "tokens": ["zagt", "in", "N\u00f6\u00b7ten", "nicht", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKNEG", "PTKA", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "braucht der Zeit, so ihn erquicket;", "tokens": ["braucht", "der", "Zeit", ",", "so", "ihn", "er\u00b7quic\u00b7ket", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "sein Verh\u00e4ngn\u00fc\u00df nimmt er auf,", "tokens": ["sein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "nimmt", "er", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wie es mengt der Sternen Lauf.", "tokens": ["wie", "es", "mengt", "der", "Ster\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwar es l\u00e4\u00dft sich \u00fcbel stellen,", "tokens": ["Zwar", "es", "l\u00e4\u00dft", "sich", "\u00fc\u00b7bel", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wann das Leid vom Herzen k\u00f6mmt", "tokens": ["wann", "das", "Leid", "vom", "Her\u00b7zen", "k\u00f6mmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und bis an die Tr\u00e4nen glimmt.", "tokens": ["und", "bis", "an", "die", "Tr\u00e4\u00b7nen", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Also wenig Glut und Wellen", "tokens": ["Al\u00b7so", "we\u00b7nig", "Glut", "und", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "k\u00f6nnen unvermieden sein,", "tokens": ["k\u00f6n\u00b7nen", "un\u00b7ver\u00b7mie\u00b7den", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "also wenig ernste Pein.", "tokens": ["al\u00b7so", "we\u00b7nig", "erns\u00b7te", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch so ist di\u00df auch nichts Neues,", "tokens": ["Doch", "so", "ist", "di\u00df", "auch", "nichts", "Neu\u00b7es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PDS", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df die Sonn' im Regen scheint.", "tokens": ["da\u00df", "die", "Sonn'", "im", "Re\u00b7gen", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also lacht man, wenn man weint,", "tokens": ["Al\u00b7so", "lacht", "man", ",", "wenn", "man", "weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wer nur auch hat etwas Treues,", "tokens": ["wer", "nur", "auch", "hat", "et\u00b7was", "Treu\u00b7es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "das mit ihm die Wage h\u00e4lt,", "tokens": ["das", "mit", "ihm", "die", "Wa\u00b7ge", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "wo die leichte Schal' hinf\u00e4llt.", "tokens": ["wo", "die", "leich\u00b7te", "Schal'", "hin\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein vertrauter Freund im Leben,", "tokens": ["Ein", "ver\u00b7trau\u00b7ter", "Freund", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "der halbirt uns unser Leid,", "tokens": ["der", "hal\u00b7birt", "uns", "un\u00b7ser", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "duppelt gleichfals alle Freud'", "tokens": ["dup\u00b7pelt", "gleich\u00b7fals", "al\u00b7le", "Freud'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und versichert uns beineben,", "tokens": ["und", "ver\u00b7si\u00b7chert", "uns", "bei\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.5": {"text": "da\u00df die Not, so uns betr\u00fcbt,", "tokens": ["da\u00df", "die", "Not", ",", "so", "uns", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ihm auch gleiche St\u00f6\u00dfe giebt.", "tokens": ["ihm", "auch", "glei\u00b7che", "St\u00f6\u00b7\u00dfe", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Jene Tage sind zum Klagen,", "tokens": ["Je\u00b7ne", "Ta\u00b7ge", "sind", "zum", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die zur Fr\u00f6ligkeit bestimmt.", "tokens": ["die", "zur", "Fr\u00f6\u00b7lig\u00b7keit", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selig ist, der mitte nimmt,", "tokens": ["Se\u00b7lig", "ist", ",", "der", "mit\u00b7te", "nimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "was f\u00fcr Lust die Zeiten tragen!", "tokens": ["was", "f\u00fcr", "Lust", "die", "Zei\u00b7ten", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gegenw\u00e4rtigs ist Gewin;", "tokens": ["Ge\u00b7gen\u00b7w\u00e4r\u00b7tigs", "ist", "Ge\u00b7win", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "was schon hin ist, das ist hin.", "tokens": ["was", "schon", "hin", "ist", ",", "das", "ist", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VAFIN", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Es ist ohne di\u00df ein Schatten", "tokens": ["Es", "ist", "oh\u00b7ne", "di\u00df", "ein", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "unser Leben, Last und wir.", "tokens": ["un\u00b7ser", "Le\u00b7ben", ",", "Last", "und", "wir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns entk\u00f6mmet f\u00fcr und f\u00fcr,", "tokens": ["Uns", "ent\u00b7k\u00f6m\u00b7met", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "was wir vor in Volmacht hatten.", "tokens": ["was", "wir", "vor", "in", "Vol\u00b7macht", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Letzlich, wenn denn alles port,", "tokens": ["Letz\u00b7lich", ",", "wenn", "denn", "al\u00b7les", "port", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "mu\u00df sein Rest, wir selbst, auch fort.", "tokens": ["mu\u00df", "sein", "Rest", ",", "wir", "selbst", ",", "auch", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "PPER", "ADV", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Unser sauers\u00fc\u00dfes Leben", "tokens": ["Un\u00b7ser", "sau\u00b7er\u00b7s\u00fc\u00b7\u00dfes", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ist ein Apothekertrank,", "tokens": ["ist", "ein", "A\u00b7pot\u00b7he\u00b7ker\u00b7trank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da vermischte Ruh' und Stank,", "tokens": ["da", "ver\u00b7mischte", "Ruh'", "und", "Stank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "herb' und s\u00fc\u00df', ein Grauen machen,", "tokens": ["herb'", "und", "s\u00fc\u00df'", ",", "ein", "Grau\u00b7en", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "den man, was man auch fang' an,", "tokens": ["den", "man", ",", "was", "man", "auch", "fang'", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "PIS", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "scheiden nicht, nur trinken kan.", "tokens": ["schei\u00b7den", "nicht", ",", "nur", "trin\u00b7ken", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "M\u00e4\u00dfigt, sch\u00f6ne Braut, das Trauren", "tokens": ["M\u00e4\u00b7\u00dfigt", ",", "sch\u00f6\u00b7ne", "Braut", ",", "das", "Trau\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und seht auf den Liebsten hin,", "tokens": ["und", "seht", "auf", "den", "Liebs\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "der mit gleichbetr\u00fcbtem Sinn'", "tokens": ["der", "mit", "gleich\u00b7be\u00b7tr\u00fcb\u00b7tem", "Sinn'"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "eure Freunde hilft betauren,", "tokens": ["eu\u00b7re", "Freun\u00b7de", "hilft", "be\u00b7tau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "eure Freunde, die gemein", "tokens": ["eu\u00b7re", "Freun\u00b7de", ",", "die", "ge\u00b7mein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ihm mit euch in k\u00fcnftig sein!", "tokens": ["ihm", "mit", "euch", "in", "k\u00fcnf\u00b7tig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "APPR", "ADJD", "VAINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.12": {"line.1": {"text": "Habt ihr ihn betr\u00fcben k\u00f6nnen,", "tokens": ["Habt", "ihr", "ihn", "be\u00b7tr\u00fc\u00b7ben", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "so macht ihn auch wieder froh!", "tokens": ["so", "macht", "ihn", "auch", "wie\u00b7der", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sein Gesicht' ist rot und roh", "tokens": ["Sein", "Ge\u00b7sicht'", "ist", "rot", "und", "roh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "von der Z\u00e4hren scharfen Rinnen.", "tokens": ["von", "der", "Z\u00e4h\u00b7ren", "schar\u00b7fen", "Rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eurer K\u00fcsse feuchter Schwamm", "tokens": ["Eu\u00b7rer", "K\u00fcs\u00b7se", "feuch\u00b7ter", "Schwamm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "streicht hin diese Flut und Flamm'.", "tokens": ["streicht", "hin", "die\u00b7se", "Flut", "und", "Flamm'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ob di\u00df Leid verbeut zu schauen", "tokens": ["Ob", "di\u00df", "Leid", "ver\u00b7beut", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "NN", "VVPP", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "eurer Hochzeit offne Zier,", "tokens": ["eu\u00b7rer", "Hoch\u00b7zeit", "off\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "hinterh\u00e4lt uns der Begier", "tokens": ["hin\u00b7ter\u00b7h\u00e4lt", "uns", "der", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und l\u00e4\u00dft euch zu Hause trauen,", "tokens": ["und", "l\u00e4\u00dft", "euch", "zu", "Hau\u00b7se", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "so wei\u00df doch ein Ieder wol,", "tokens": ["so", "wei\u00df", "doch", "ein", "Ie\u00b7der", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "wie er f\u00fcr euch w\u00fcndschen soll.", "tokens": ["wie", "er", "f\u00fcr", "euch", "w\u00fcnd\u00b7schen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Euch will f\u00f6rderhin geb\u00fchren,", "tokens": ["Euch", "will", "f\u00f6r\u00b7der\u00b7hin", "ge\u00b7b\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "liebstes Paar, bei Lieb' und Leid',", "tokens": ["liebs\u00b7tes", "Paar", ",", "bei", "Lieb'", "und", "Leid'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "als ihr schon gewohnet seid,", "tokens": ["als", "ihr", "schon", "ge\u00b7woh\u00b7net", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "gleiche Sinnen stets zu f\u00fchren.", "tokens": ["glei\u00b7che", "Sin\u00b7nen", "stets", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Helfe Gott, da\u00df diese Treu'", "tokens": ["Hel\u00b7fe", "Gott", ",", "da\u00df", "die\u00b7se", "Treu'"], "token_inf