{"textgrid.poem.54317": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lisettgen sah man letzt in heisser Andacht beten,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lisettgen sah man letzt in heisser Andacht beten,", "tokens": ["Li\u00b7sett\u00b7gen", "sah", "man", "letzt", "in", "heis\u00b7ser", "An\u00b7dacht", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und als Musophilus ihr auf den Rock getreten,", "tokens": ["Und", "als", "Mu\u00b7so\u00b7phi\u00b7lus", "ihr", "auf", "den", "Rock", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "PPOSAT", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So fieng das fromme Kind ein grosses Lermen an,", "tokens": ["So", "fi\u00b7eng", "das", "from\u00b7me", "Kind", "ein", "gros\u00b7ses", "Ler\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Als h\u00e4tt er selbiges aus Schertz und Spott gethan.", "tokens": ["Als", "h\u00e4tt", "er", "sel\u00b7bi\u00b7ges", "aus", "Schertz", "und", "Spott", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du wirst, versetzt er drauff, dich nicht deswegen zancken,", "tokens": ["Du", "wirst", ",", "ver\u00b7setzt", "er", "drauff", ",", "dich", "nicht", "des\u00b7we\u00b7gen", "zan\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "PTKNEG", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bist du, Lisette, so voll heiliger Gedancken,", "tokens": ["Bist", "du", ",", "Li\u00b7set\u00b7te", ",", "so", "voll", "hei\u00b7li\u00b7ger", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NE", "$,", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich schw\u00f6re, da\u00df es nicht aus Vorsatz ist geschehn.", "tokens": ["Ich", "schw\u00f6\u00b7re", ",", "da\u00df", "es", "nicht", "aus", "Vor\u00b7satz", "ist", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mich jammert, da\u00df ich dich soll so entr\u00fcstet sehn.", "tokens": ["Mich", "jam\u00b7mert", ",", "da\u00df", "ich", "dich", "soll", "so", "ent\u00b7r\u00fcs\u00b7tet", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wei\u00df deine Heiligkeit so meisterlich zu fluchen,", "tokens": ["Wei\u00df", "dei\u00b7ne", "Hei\u00b7lig\u00b7keit", "so", "meis\u00b7ter\u00b7lich", "zu", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die\u00df w\u00fcrde keiner nicht in deinen Minen suchen!", "tokens": ["Die\u00df", "w\u00fcr\u00b7de", "kei\u00b7ner", "nicht", "in", "dei\u00b7nen", "Mi\u00b7nen", "su\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil l\u00e4ngst der Augen-Schein mir zu verstehen gab,", "tokens": ["Weil", "l\u00e4ngst", "der", "Au\u00b7gen\u00b7Schein", "mir", "zu", "ver\u00b7ste\u00b7hen", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du bi\u00dft den Heiligen die Z\u00e4hen w\u00fcrcklich ab.", "tokens": ["Du", "bi\u00dft", "den", "Hei\u00b7li\u00b7gen", "die", "Z\u00e4\u00b7hen", "w\u00fcrck\u00b7lich", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch dein verstelltes Thun wird mich nicht leicht beth\u00f6ren,", "tokens": ["Doch", "dein", "ver\u00b7stell\u00b7tes", "Thun", "wird", "mich", "nicht", "leicht", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil man kan hier und dar dergleichen V\u00f6gel h\u00f6ren.", "tokens": ["Weil", "man", "kan", "hier", "und", "dar", "derg\u00b7lei\u00b7chen", "V\u00f6\u00b7gel", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "ADV", "KON", "PTKVZ", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Lisettgen, bete nur und lieb incognito,", "tokens": ["Li\u00b7sett\u00b7gen", ",", "be\u00b7te", "nur", "und", "lieb", "in\u00b7co\u00b7gni\u00b7to", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die deines gleichen sind, die machens alle so.", "tokens": ["Die", "dei\u00b7nes", "glei\u00b7chen", "sind", ",", "die", "ma\u00b7chens", "al\u00b7le", "so", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "VAFIN", "$,", "PRELS", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du bist so b\u00f6se nicht, als du dich pflegst zu stellen,", "tokens": ["Du", "bist", "so", "b\u00f6\u00b7se", "nicht", ",", "als", "du", "dich", "pflegst", "zu", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich kehre warlich mich nicht an dein falsches Bellen.", "tokens": ["Ich", "keh\u00b7re", "war\u00b7lich", "mich", "nicht", "an", "dein", "fal\u00b7sches", "Bel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein Hertze meynt vielleicht darbey das Gegentheil,", "tokens": ["Dein", "Hert\u00b7ze", "meynt", "viel\u00b7leicht", "dar\u00b7bey", "das", "Ge\u00b7gen\u00b7theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es w\u00e4re, spricht das Volck, zum Kauff dir alles feil.", "tokens": ["Es", "w\u00e4\u00b7re", ",", "spricht", "das", "Volck", ",", "zum", "Kauff", "dir", "al\u00b7les", "feil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}