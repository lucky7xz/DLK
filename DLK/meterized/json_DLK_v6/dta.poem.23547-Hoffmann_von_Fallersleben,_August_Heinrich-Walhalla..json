{"dta.poem.23547": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Walhalla.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sei gegr\u00fc\u00dft, du hehre Halle", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "du", "heh\u00b7re", "Hal\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutscher Gr\u00f6\u00df' und Herrlichkeit!", "tokens": ["Deut\u00b7scher", "Gr\u00f6\u00df'", "und", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seid gegr\u00fc\u00dft, ihr Helden alle", "tokens": ["Seid", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "Hel\u00b7den", "al\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "VVPP", "$,", "PPOSAT", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der alt' und neuen Zeit!", "tokens": ["Aus", "der", "alt'", "und", "neu\u00b7en", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O ihr Helden in der Halle,", "tokens": ["O", "ihr", "Hel\u00b7den", "in", "der", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nntet ihr lebendig sein!", "tokens": ["K\u00f6nn\u00b7tet", "ihr", "le\u00b7ben\u00b7dig", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, ein K\u00f6nig hat euch alle", "tokens": ["Nein", ",", "ein", "K\u00f6\u00b7nig", "hat", "euch", "al\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "PPER", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber doch in Erz und Stein.", "tokens": ["Lie\u00b7ber", "doch", "in", "Erz", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}