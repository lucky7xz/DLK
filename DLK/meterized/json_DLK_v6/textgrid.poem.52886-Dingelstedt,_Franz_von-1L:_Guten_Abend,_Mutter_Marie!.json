{"textgrid.poem.52886": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Guten Abend, Mutter Marie!", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guten Abend, Mutter Marie!", "tokens": ["Gu\u00b7ten", "A\u00b7bend", ",", "Mut\u00b7ter", "Ma\u00b7rie", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In deinem kleinen Schrein,", "tokens": ["In", "dei\u00b7nem", "klei\u00b7nen", "Schrein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den toten Sohn auf wei\u00dfem Knie,", "tokens": ["Den", "to\u00b7ten", "Sohn", "auf", "wei\u00b7\u00dfem", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sitzest du mild und lieblich drein!", "tokens": ["Wie", "sit\u00b7zest", "du", "mild", "und", "lieb\u00b7lich", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Lichtchen haben sie angesteckt,", "tokens": ["Ein", "Licht\u00b7chen", "ha\u00b7ben", "sie", "an\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von frommen Gel\u00fcbden gezollt,", "tokens": ["Von", "from\u00b7men", "Ge\u00b7l\u00fcb\u00b7den", "ge\u00b7zollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und dich mit k\u00f6stlichen Lappen bedeckt,", "tokens": ["Und", "dich", "mit", "k\u00f6st\u00b7li\u00b7chen", "Lap\u00b7pen", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Kronen von Flittergold.", "tokens": ["Mit", "Kro\u00b7nen", "von", "Flit\u00b7ter\u00b7gold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dich k\u00fcmmert der Putz nicht und der Schein,", "tokens": ["Dich", "k\u00fcm\u00b7mert", "der", "Putz", "nicht", "und", "der", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "KON", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dein w\u00e4chsern' Gesicht ist bla\u00df,", "tokens": ["Dein", "w\u00e4ch\u00b7sern'", "Ge\u00b7sicht", "ist", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die siehst nur auf dein Jesulein,", "tokens": ["Die", "siehst", "nur", "auf", "dein", "Je\u00b7su\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wangen und Augen ewig na\u00df.", "tokens": ["Wan\u00b7gen", "und", "Au\u00b7gen", "e\u00b7wig", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Hab' niemals eine Mutter gekannt,", "tokens": ["Hab'", "nie\u00b7mals", "ei\u00b7ne", "Mut\u00b7ter", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Niemals ein Kindlein geherzt,", "tokens": ["Nie\u00b7mals", "ein", "Kin\u00b7dlein", "ge\u00b7herzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Habe auch f\u00fcr kein Weib gebrannt", "tokens": ["Ha\u00b7be", "auch", "f\u00fcr", "kein", "Weib", "ge\u00b7brannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN", "VVPP"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und mit keiner Schwester gescherzt.", "tokens": ["Und", "mit", "kei\u00b7ner", "Schwes\u00b7ter", "ge\u00b7scherzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Nun mein' ich, da\u00df es nichts Rechtes w\u00e4r'", "tokens": ["Nun", "mein'", "ich", ",", "da\u00df", "es", "nichts", "Rech\u00b7tes", "w\u00e4r'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PIS", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit der Familien-Klerisei;", "tokens": ["Mit", "der", "Fa\u00b7mi\u00b7li\u00b7en\u00b7K\u00b7le\u00b7ri\u00b7sei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Komm' ich aber des Weges her,", "tokens": ["Komm'", "ich", "a\u00b7ber", "des", "We\u00b7ges", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "An der Jungfrau Bild vorbei,", "tokens": ["An", "der", "Jung\u00b7frau", "Bild", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann tut's mir wohl, dann tut's mir weh,", "tokens": ["Dann", "tut's", "mir", "wohl", ",", "dann", "tut's", "mir", "weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00df selber nicht, wo und wie?", "tokens": ["Wei\u00df", "sel\u00b7ber", "nicht", ",", "wo", "und", "wie", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "KON", "PWAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und ich fl\u00fcstere, weil ich von dannen geh':", "tokens": ["Und", "ich", "fl\u00fcs\u00b7te\u00b7re", ",", "weil", "ich", "von", "dan\u00b7nen", "geh'", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Guten Abend, Mutter Marie!", "tokens": ["Gu\u00b7ten", "A\u00b7bend", ",", "Mut\u00b7ter", "Ma\u00b7rie", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}