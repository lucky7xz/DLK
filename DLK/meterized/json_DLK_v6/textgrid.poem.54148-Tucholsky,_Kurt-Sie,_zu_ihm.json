{"textgrid.poem.54148": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Sie, zu ihm", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab dir alles hingegeben:", "tokens": ["Ich", "hab", "dir", "al\u00b7les", "hin\u00b7ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mich, meine Seele, Zeit und Geld.", "tokens": ["mich", ",", "mei\u00b7ne", "See\u00b7le", ",", "Zeit", "und", "Geld", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist ein Mann \u2013 du bist mein Leben,", "tokens": ["Du", "bist", "ein", "Mann", "\u2013", "du", "bist", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "du meine kleine Unterwelt.", "tokens": ["du", "mei\u00b7ne", "klei\u00b7ne", "Un\u00b7ter\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch habe ich mein Gl\u00fcck gefunden,", "tokens": ["Doch", "ha\u00b7be", "ich", "mein", "Gl\u00fcck", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "seh ich dir manchmal ins Gesicht:", "tokens": ["seh", "ich", "dir", "manch\u00b7mal", "ins", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ich kenn dich in so vielen Stunden \u2013", "tokens": ["Ich", "kenn", "dich", "in", "so", "vie\u00b7len", "Stun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "nein, z\u00e4rtlich bist du nicht.", "tokens": ["nein", ",", "z\u00e4rt\u00b7lich", "bist", "du", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du k\u00fc\u00dft recht gut. Auf manche Weise", "tokens": ["Du", "k\u00fc\u00dft", "recht", "gut", ".", "Auf", "man\u00b7che", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zeigst du mir, was das ist: Genu\u00df.", "tokens": ["zeigst", "du", "mir", ",", "was", "das", "ist", ":", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PWS", "PDS", "VAFIN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du h\u00f6rst gern Klatsch. Du sagst mir leise,", "tokens": ["Du", "h\u00f6rst", "gern", "Klatsch", ".", "Du", "sagst", "mir", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wann ich die Lippen nachziehn mu\u00df.", "tokens": ["wann", "ich", "die", "Lip\u00b7pen", "nach\u00b7ziehn", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bleibst sogar vor andern Frauen", "tokens": ["Du", "bleibst", "so\u00b7gar", "vor", "an\u00b7dern", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "in gut gespieltem Gleichgewicht;", "tokens": ["in", "gut", "ge\u00b7spiel\u00b7tem", "Gleich\u00b7ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "man kann dir manchmal sogar trauen . . .", "tokens": ["man", "kann", "dir", "manch\u00b7mal", "so\u00b7gar", "trau\u00b7en", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "aber z\u00e4rtlich bist du nicht.", "tokens": ["a\u00b7ber", "z\u00e4rt\u00b7lich", "bist", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O w\u00e4rst du z\u00e4rtlich!", "tokens": ["O", "w\u00e4rst", "du", "z\u00e4rt\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Meinetwegen", "tokens": ["Mei\u00b7net\u00b7we\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "kannst du sogar gef\u00fchlvoll sein.", "tokens": ["kannst", "du", "so\u00b7gar", "ge\u00b7f\u00fchl\u00b7voll", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mensch, wie ein warmer Fr\u00fchlingsregen", "tokens": ["Mensch", ",", "wie", "ein", "war\u00b7mer", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "so h\u00fcllte Z\u00e4rtlichkeit mich ein!", "tokens": ["so", "h\u00fcll\u00b7te", "Z\u00e4rt\u00b7lich\u00b7keit", "mich", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00e4rst du der Weiche von uns beiden,", "tokens": ["W\u00e4rst", "du", "der", "Wei\u00b7che", "von", "uns", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "PPER", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "w\u00e4rst du der Dumme. Bube sticht.", "tokens": ["w\u00e4rst", "du", "der", "Dum\u00b7me", ".", "Bu\u00b7be", "sticht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn wer mehr liebt, der mu\u00df mehr leiden.", "tokens": ["Denn", "wer", "mehr", "liebt", ",", "der", "mu\u00df", "mehr", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "ART", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nein, z\u00e4rtlich bist du nicht.", "tokens": ["Nein", ",", "z\u00e4rt\u00b7lich", "bist", "du", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}