{"dta.poem.4417": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Kind.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein kluger Wund-Arzt schneidet drein,", "tokens": ["Ein", "klu\u00b7ger", "Wun\u00b7dA\u00b7rzt", "schnei\u00b7det", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eh\u2019 er vom schneiden viel erz\u00e4let.", "tokens": ["Eh'", "er", "vom", "schnei\u00b7den", "viel", "er\u00b7z\u00e4\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Warum? er wei\u00df, da\u00df insgemein", "tokens": ["Wa\u00b7rum", "?", "er", "wei\u00df", ",", "da\u00df", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Furcht mehr, als das Uebel, qv\u00e4let.", "tokens": ["Die", "Furcht", "mehr", ",", "als", "das", "Ue\u00b7bel", ",", "qv\u00e4\u00b7let", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}