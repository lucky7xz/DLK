{"textgrid.poem.66932": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kaiser, die Deutschen harren,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaiser, die Deutschen harren,", "tokens": ["Kai\u00b7ser", ",", "die", "Deut\u00b7schen", "har\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Kaiser, die Stunde brennt!", "tokens": ["Kai\u00b7ser", ",", "die", "Stun\u00b7de", "brennt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Soll unser Blick erstarren,", "tokens": ["Soll", "un\u00b7ser", "Blick", "er\u00b7star\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er Dich nicht mehr kennt?", "tokens": ["Da\u00df", "er", "Dich", "nicht", "mehr", "kennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Nicht einem Fremden zu Willen,", "tokens": ["Nicht", "ei\u00b7nem", "Frem\u00b7den", "zu", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht, weil ein Feind sie Dir raubt \u2013", "tokens": ["Nicht", ",", "weil", "ein", "Feind", "sie", "Dir", "raubt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "ART", "NN", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Unseren Notschrei zu stillen,", "tokens": ["Un\u00b7se\u00b7ren", "Not\u00b7schrei", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Setze die Krone vom Haupt!", "tokens": ["Set\u00b7ze", "die", "Kro\u00b7ne", "vom", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Opfer sind furchtbar gefallen,", "tokens": ["Op\u00b7fer", "sind", "furcht\u00b7bar", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Opfer f\u00fcr Kaiser und Reich,", "tokens": ["Op\u00b7fer", "f\u00fcr", "Kai\u00b7ser", "und", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Opfer f\u00fcr alle von allen,", "tokens": ["Op\u00b7fer", "f\u00fcr", "al\u00b7le", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "APPR", "PIAT", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Deutsche sind frei nun und gleich.", "tokens": ["Deut\u00b7sche", "sind", "frei", "nun", "und", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "ADV", "KON", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Treue um Treue geschworen,", "tokens": ["Treu\u00b7e", "um", "Treu\u00b7e", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Kaiser, Du wei\u00dft es gewi\u00df \u2013", "tokens": ["Kai\u00b7ser", ",", "Du", "wei\u00dft", "es", "ge\u00b7wi\u00df", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Aber der Schatz ging verloren,", "tokens": ["A\u00b7ber", "der", "Schatz", "ging", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und das Vertrauen zerri\u00df.", "tokens": ["Und", "das", "Ver\u00b7trau\u00b7en", "zer\u00b7ri\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht, weil das Spiel sich gewendet,", "tokens": ["Nicht", ",", "weil", "das", "Spiel", "sich", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Nicht, weil das Gl\u00fcck Dich verlie\u00df \u2013", "tokens": ["Nicht", ",", "weil", "das", "Gl\u00fcck", "Dich", "ver\u00b7lie\u00df", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nein, da\u00df ", "tokens": ["Nein", ",", "da\u00df"], "token_info": ["word", "punct", "word"], "pos": ["PTKANT", "$,", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}}, "stanza.6": {"line.1": {"text": "Willst Du die Fassung uns retten,", "tokens": ["Willst", "Du", "die", "Fas\u00b7sung", "uns", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Deiner Krone zerbrach,", "tokens": ["Die", "Dei\u00b7ner", "Kro\u00b7ne", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Willst Du die Volkskraft verketten,", "tokens": ["Willst", "Du", "die", "Volks\u00b7kraft", "ver\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Da\u00df wir obsiegen der Schmach \u2013", "tokens": ["Da\u00df", "wir", "ob\u00b7sie\u00b7gen", "der", "Schmach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Willst Du den Adler befreien,", "tokens": ["Willst", "Du", "den", "Ad\u00b7ler", "be\u00b7frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Der aus umklammernder Not", "tokens": ["Der", "aus", "um\u00b7klam\u00b7mern\u00b7der", "Not"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ringt mit ersch\u00fctterndem Schreien", "tokens": ["Ringt", "mit", "er\u00b7sch\u00fct\u00b7tern\u00b7dem", "Schrei\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aufw\u00e4rts ins Morgenrot ...", "tokens": ["Auf\u00b7w\u00e4rts", "ins", "Mor\u00b7gen\u00b7rot", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Ehe die feurige Brunst", "tokens": ["E\u00b7he", "die", "feu\u00b7ri\u00b7ge", "Brunst"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Fri\u00dft in des Volks Eingeweiden,", "tokens": ["Fri\u00dft", "in", "des", "Volks", "Ein\u00b7ge\u00b7wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist majest\u00e4tische Kunst.", "tokens": ["Ist", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7sche", "Kunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}