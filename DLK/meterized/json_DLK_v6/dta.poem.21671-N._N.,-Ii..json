{"dta.poem.21671": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Lobet seinen heiligen Namen,", "tokens": ["Lo\u00b7bet", "sei\u00b7nen", "hei\u00b7li\u00b7gen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ihn, der nie sein Thun bereut,", "tokens": ["Ihn", ",", "der", "nie", "sein", "Thun", "be\u00b7reut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der des Gl\u00fcckes goldnen Samen", "tokens": ["Der", "des", "Gl\u00fc\u00b7ckes", "gold\u00b7nen", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Lebenspfad dir streut.", "tokens": ["Auf", "den", "Le\u00b7ben\u00b7spfad", "dir", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn so hoch der Himmel waltet", "tokens": ["Denn", "so", "hoch", "der", "Him\u00b7mel", "wal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber diesem Erdenball \u2014", "tokens": ["Ue\u00b7ber", "die\u00b7sem", "Er\u00b7den\u00b7ball"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also seine Gnade schaltet", "tokens": ["Al\u00b7so", "sei\u00b7ne", "Gna\u00b7de", "schal\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber seine Diener all\u2019.", "tokens": ["Ue\u00b7ber", "sei\u00b7ne", "Die\u00b7ner", "all'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so fern der fr\u00fche Morgen", "tokens": ["Und", "so", "fern", "der", "fr\u00fc\u00b7he", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem sp\u00e4ten Abendlicht \u2014", "tokens": ["Von", "dem", "sp\u00e4\u00b7ten", "A\u00b7bend\u00b7licht"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So entr\u00fcckt er alle Sorgen", "tokens": ["So", "ent\u00b7r\u00fcckt", "er", "al\u00b7le", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern von unserm Angesicht.", "tokens": ["Fern", "von", "un\u00b7serm", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn er wei\u00df vom Menschenthume,", "tokens": ["Denn", "er", "wei\u00df", "vom", "Men\u00b7schen\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df wir ja nur eitel Staub,", "tokens": ["Da\u00df", "wir", "ja", "nur", "ei\u00b7tel", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir bl\u00fchen wie die Blume,", "tokens": ["Da\u00df", "wir", "bl\u00fc\u00b7hen", "wie", "die", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die gar bald des Windes Raub.", "tokens": ["Die", "gar", "bald", "des", "Win\u00b7des", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die St\u00e4tte, wo sie bl\u00fchte,", "tokens": ["Und", "die", "St\u00e4t\u00b7te", ",", "wo", "sie", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ist verwischt f\u00fcr alle Zeit \u2014", "tokens": ["Ist", "ver\u00b7wischt", "f\u00fcr", "al\u00b7le", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Gottes Gnad\u2019 und G\u00fcte", "tokens": ["A\u00b7ber", "Got\u00b7tes", "Gnad'", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4hrt durch alle Ewigkeit.", "tokens": ["W\u00e4hrt", "durch", "al\u00b7le", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}