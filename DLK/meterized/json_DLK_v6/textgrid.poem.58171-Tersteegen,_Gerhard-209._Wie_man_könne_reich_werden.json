{"textgrid.poem.58171": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "209. Wie man k\u00f6nne reich werden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer gerne reich und selig w\u00e4r',", "tokens": ["Wer", "ger\u00b7ne", "reich", "und", "se\u00b7lig", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Halt' seinen Geist stets arm und leer", "tokens": ["Halt'", "sei\u00b7nen", "Geist", "stets", "arm", "und", "leer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Lust und Liebe aller Dingen.", "tokens": ["Von", "Lust", "und", "Lie\u00b7be", "al\u00b7ler", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sink in dein tiefes Nichts hinein,", "tokens": ["Sink", "in", "dein", "tie\u00b7fes", "Nichts", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Heil und Gut sei Gott allein,", "tokens": ["Dein", "Heil", "und", "Gut", "sei", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies wird den Himmel in dich bringen!", "tokens": ["Dies", "wird", "den", "Him\u00b7mel", "in", "dich", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}