{"dta.poem.19400": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "133.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In jedem Zustand ist ein Haben und ein Missen,", "tokens": ["In", "je\u00b7dem", "Zu\u00b7stand", "ist", "ein", "Ha\u00b7ben", "und", "ein", "Mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und das Gemisste bist zu haben du beflissen.", "tokens": ["Und", "das", "Ge\u00b7miss\u00b7te", "bist", "zu", "ha\u00b7ben", "du", "be\u00b7flis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKZU", "VAINF", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Darum bei jedem Gl\u00fcck ist noch ein Wunsch zur\u00fcck,", "tokens": ["Da\u00b7rum", "bei", "je\u00b7dem", "Gl\u00fcck", "ist", "noch", "ein", "Wunsch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PIAT", "NN", "VAFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eben ist davon ein zubeh\u00f6rig St\u00fcck.", "tokens": ["Der", "e\u00b7ben", "ist", "da\u00b7von", "ein", "zu\u00b7be\u00b7h\u00f6\u00b7rig", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "PAV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wie ein Zustand in den andern \u00fcbergeht,", "tokens": ["Und", "wie", "ein", "Zu\u00b7stand", "in", "den", "an\u00b7dern", "\u00fc\u00b7ber\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Siehst du, da\u00df aus dem Wunsch ein neuer Wunsch entsteht.", "tokens": ["Siehst", "du", ",", "da\u00df", "aus", "dem", "Wunsch", "ein", "neu\u00b7er", "Wunsch", "ent\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn jeder Wunsch, erlangt, ist nicht mehr w\u00fcnschenswerth;", "tokens": ["Denn", "je\u00b7der", "Wunsch", ",", "er\u00b7langt", ",", "ist", "nicht", "mehr", "w\u00fcn\u00b7schens\u00b7werth", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "VVPP", "$,", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Gl\u00fcck ist auch, da\u00df man stets neu ein Gl\u00fcck begehrt,", "tokens": ["Doch", "Gl\u00fcck", "ist", "auch", ",", "da\u00df", "man", "stets", "neu", "ein", "Gl\u00fcck", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "$,", "KOUS", "PIS", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}