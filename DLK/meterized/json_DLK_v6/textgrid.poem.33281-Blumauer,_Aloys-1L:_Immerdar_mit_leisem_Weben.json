{"textgrid.poem.33281": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: Immerdar mit leisem Weben", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immerdar mit leisem Weben", "tokens": ["Im\u00b7mer\u00b7dar", "mit", "lei\u00b7sem", "We\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebt dein s\u00fcsses Bild vor mir,", "tokens": ["Schwebt", "dein", "s\u00fcs\u00b7ses", "Bild", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein liebesehnend Beben", "tokens": ["Und", "ein", "lie\u00b7be\u00b7seh\u00b7nend", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zittert durch die Seele mir.", "tokens": ["Zit\u00b7tert", "durch", "die", "See\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weg aus deinem Zauberkreise,", "tokens": ["Weg", "aus", "dei\u00b7nem", "Zau\u00b7ber\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo du mich so fest gebannt,", "tokens": ["Wo", "du", "mich", "so", "fest", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zog durch eine weite Reise", "tokens": ["Zog", "durch", "ei\u00b7ne", "wei\u00b7te", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich die Freundschaft auf das Land.", "tokens": ["Mich", "die", "Freund\u00b7schaft", "auf", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier im Mutterarm der sch\u00f6nen", "tokens": ["Hier", "im", "Mut\u00b7ter\u00b7arm", "der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allerfreuenden Natur,", "tokens": ["Al\u00b7ler\u00b7freu\u00b7en\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fehlt zum Allgenu\u00df des Sch\u00f6nen,", "tokens": ["Fehlt", "zum", "All\u00b7ge\u00b7nu\u00df", "des", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herrliche! dein Ku\u00df mir nur.", "tokens": ["Herr\u00b7li\u00b7che", "!", "dein", "Ku\u00df", "mir", "nur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Halbgenossen glitscht die Freude", "tokens": ["Halb\u00b7ge\u00b7nos\u00b7sen", "glitscht", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber meinem Herzen hin,", "tokens": ["Ue\u00b7ber", "mei\u00b7nem", "Her\u00b7zen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Natur im Fr\u00fchlingskleide", "tokens": ["Die", "Na\u00b7tur", "im", "Fr\u00fch\u00b7lings\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh' ich nur mit halbem Sinn.", "tokens": ["Seh'", "ich", "nur", "mit", "hal\u00b7bem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Todt sind ohne dich die Fluren,", "tokens": ["Todt", "sind", "oh\u00b7ne", "dich", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine W\u00fcste die Natur,", "tokens": ["Ei\u00b7ne", "W\u00fcs\u00b7te", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den B\u00e4umen find' ich Spuren", "tokens": ["An", "den", "B\u00e4u\u00b7men", "find'", "ich", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner heissen Sehnsucht nur.", "tokens": ["Mei\u00b7ner", "heis\u00b7sen", "Sehn\u00b7sucht", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn ein liebesehnend Dr\u00fccken", "tokens": ["Wenn", "ein", "lie\u00b7be\u00b7seh\u00b7nend", "Dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich hinaus in's Freie zieht,", "tokens": ["Mich", "hin\u00b7aus", "in's", "Frei\u00b7e", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APZR", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Such' ich oft des Berges R\u00fccken,", "tokens": ["Such'", "ich", "oft", "des", "Ber\u00b7ges", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dich meinem Aug' entzieht;", "tokens": ["Der", "dich", "mei\u00b7nem", "Aug'", "ent\u00b7zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bleibe dann, wie eine B\u00fcste,", "tokens": ["Blei\u00b7be", "dann", ",", "wie", "ei\u00b7ne", "B\u00fcs\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starr nach dir hinsehend, steh'n,", "tokens": ["Starr", "nach", "dir", "hin\u00b7se\u00b7hend", ",", "steh'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "Seh' und seh', und mein, ich m\u00fc\u00dfte", "tokens": ["Seh'", "und", "seh'", ",", "und", "mein", ",", "ich", "m\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "KON", "PPOSAT", "$,", "PPER", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich zu mir her\u00fcber seh'n,", "tokens": ["Dich", "zu", "mir", "her\u00b7\u00fc\u00b7ber", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber still heraufgegangen", "tokens": ["A\u00b7ber", "still", "her\u00b7auf\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt der Mond statt deiner dann,", "tokens": ["Kommt", "der", "Mond", "statt", "dei\u00b7ner", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein inniger Verlangen", "tokens": ["Und", "ein", "in\u00b7ni\u00b7ger", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Flammt in meiner Brust sich an.", "tokens": ["Flammt", "in", "mei\u00b7ner", "Brust", "sich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Hin, ach, hin zu seinen H\u00f6hen", "tokens": ["Hin", ",", "ach", ",", "hin", "zu", "sei\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ITJ", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' ich fliegen, und auf dich,", "tokens": ["M\u00f6cht'", "ich", "flie\u00b7gen", ",", "und", "auf", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KON", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach auf dich herniedersehen,", "tokens": ["Ach", "auf", "dich", "her\u00b7nie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und hernieder schwingen mich.", "tokens": ["Und", "her\u00b7nie\u00b7der", "schwin\u00b7gen", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}