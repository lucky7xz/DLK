{"textgrid.poem.37766": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ein Trinklied beim Sternenklang", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe Hand, Dich darf ich dr\u00fccken,", "tokens": ["Lie\u00b7be", "Hand", ",", "Dich", "darf", "ich", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringst mir einen Becher Wein,", "tokens": ["Bringst", "mir", "ei\u00b7nen", "Be\u00b7cher", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die holden Sterne blicken", "tokens": ["Und", "die", "hol\u00b7den", "Ster\u00b7ne", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Becher froh hinein;", "tokens": ["In", "den", "Be\u00b7cher", "froh", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zweifelnd bin ich im Entz\u00fccken,", "tokens": ["Zwei\u00b7felnd", "bin", "ich", "im", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Trink ich erst den duftgen Wein?", "tokens": ["Trink", "ich", "erst", "den", "duft\u00b7gen", "Wein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll ein Ku\u00df mich erst begl\u00fccken?", "tokens": ["Soll", "ein", "Ku\u00df", "mich", "erst", "be\u00b7gl\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Beides, beides ist nun mein!", "tokens": ["Bei\u00b7des", ",", "bei\u00b7des", "ist", "nun", "mein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Rathet mir treulich, liebliche Sterne,", "tokens": ["Ra\u00b7thet", "mir", "treu\u00b7lich", ",", "lieb\u00b7li\u00b7che", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Gr\u00fc\u00dfe Euch alle, nahe und ferne!", "tokens": ["Gr\u00fc\u00b7\u00dfe", "Euch", "al\u00b7le", ",", "na\u00b7he", "und", "fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "$,", "ADJD", "KON", "ADJA", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Fliehst Du scheu vor meinem Blicke", "tokens": ["Fliehst", "Du", "scheu", "vor", "mei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und versch\u00fcttest meinen Wein,", "tokens": ["Und", "ver\u00b7sch\u00fct\u00b7test", "mei\u00b7nen", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchrt mein Ruf dich nicht zur\u00fccke,", "tokens": ["F\u00fchrt", "mein", "Ruf", "dich", "nicht", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, so bist Du doch nicht mein!", "tokens": ["Ach", ",", "so", "bist", "Du", "doch", "nicht", "mein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hei\u00dfe Liebe, Deine T\u00fccke", "tokens": ["Hei\u00b7\u00dfe", "Lie\u00b7be", ",", "Dei\u00b7ne", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4\u00dft mich schmerzlich hier allein,", "tokens": ["L\u00e4\u00dft", "mich", "schmerz\u00b7lich", "hier", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als ich meinem stillen Gl\u00fccke", "tokens": ["Als", "ich", "mei\u00b7nem", "stil\u00b7len", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wollte froh entgegen schrein;", "tokens": ["Woll\u00b7te", "froh", "ent\u00b7ge\u00b7gen", "schrein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Feurige Zungen sind da erklungen,", "tokens": ["Feu\u00b7ri\u00b7ge", "Zun\u00b7gen", "sind", "da", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Aber mein Liebchen ist mir entsprungen.", "tokens": ["A\u00b7ber", "mein", "Lieb\u00b7chen", "ist", "mir", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Wandelt weiter, kalte Sterne,", "tokens": ["Wan\u00b7delt", "wei\u00b7ter", ",", "kal\u00b7te", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiegelnd im vergo\u00dfnen Wein,", "tokens": ["Spie\u00b7gelnd", "im", "ver\u00b7go\u00df\u00b7nen", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Suchet Ihr doch stets die Ferne,", "tokens": ["Su\u00b7chet", "Ihr", "doch", "stets", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nah und ferne, nichts ist mein.", "tokens": ["Nah", "und", "fer\u00b7ne", ",", "nichts", "ist", "mein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "$,", "PIS", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur der Tropfen, den ich hege,", "tokens": ["Nur", "der", "Trop\u00b7fen", ",", "den", "ich", "he\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "L\u00f6set meines Herzens Klang,", "tokens": ["L\u00f6\u00b7set", "mei\u00b7nes", "Her\u00b7zens", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schweigend geht Ihr Eure Wege,", "tokens": ["Schwei\u00b7gend", "geht", "Ihr", "Eu\u00b7re", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Euren stillen, gleichen Gang;", "tokens": ["Eu\u00b7ren", "stil\u00b7len", ",", "glei\u00b7chen", "Gang", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Als ich noch hoffte, seid Ihr erklungen,", "tokens": ["Als", "ich", "noch", "hoff\u00b7te", ",", "seid", "Ihr", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Jetzt wie so stille, feurige Zungen.", "tokens": ["Jetzt", "wie", "so", "stil\u00b7le", ",", "feu\u00b7ri\u00b7ge", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}