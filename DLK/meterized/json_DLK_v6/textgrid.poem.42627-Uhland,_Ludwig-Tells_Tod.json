{"textgrid.poem.42627": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Tells Tod", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fcn wird die Alpe werden,", "tokens": ["Gr\u00fcn", "wird", "die", "Al\u00b7pe", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VAINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "St\u00fcrzt die Lawin einmal;", "tokens": ["St\u00fcrzt", "die", "La\u00b7win", "ein\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zu Berge ziehn die Herden,", "tokens": ["Zu", "Ber\u00b7ge", "ziehn", "die", "Her\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fuhr erst der Schnee zu Tal.", "tokens": ["Fuhr", "erst", "der", "Schnee", "zu", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Euch stellt, ihr Alpens\u00f6hne,", "tokens": ["Euch", "stellt", ",", "ihr", "Al\u00b7pen\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit jedem neuen Jahr", "tokens": ["Mit", "je\u00b7dem", "neu\u00b7en", "Jahr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Eises Bruch vom F\u00f6hne", "tokens": ["Des", "Ei\u00b7ses", "Bruch", "vom", "F\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Kampf der Freiheit dar.", "tokens": ["Den", "Kampf", "der", "Frei\u00b7heit", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da braust der wilde Sch\u00e4chen", "tokens": ["Da", "braust", "der", "wil\u00b7de", "Sch\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hervor aus seiner Schlucht,", "tokens": ["Her\u00b7vor", "aus", "sei\u00b7ner", "Schlucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Fels und Tanne brechen", "tokens": ["Und", "Fels", "und", "Tan\u00b7ne", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor seiner j\u00e4hen Flucht.", "tokens": ["Vor", "sei\u00b7ner", "j\u00e4\u00b7hen", "Flucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er hat den Steg begraben,", "tokens": ["Er", "hat", "den", "Steg", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der ob der St\u00e4ube hing,", "tokens": ["Der", "ob", "der", "St\u00e4u\u00b7be", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hat weggesp\u00fclt den Knaben,", "tokens": ["Hat", "weg\u00b7ge\u00b7sp\u00fclt", "den", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der auf dem Stege ging.", "tokens": ["Der", "auf", "dem", "Ste\u00b7ge", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und eben schritt ein andrer", "tokens": ["Und", "e\u00b7ben", "schritt", "ein", "an\u00b7drer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Br\u00fccke, da sie brach;", "tokens": ["Zur", "Br\u00fc\u00b7cke", ",", "da", "sie", "brach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht stutzt der greise Wandrer,", "tokens": ["Nicht", "stutzt", "der", "grei\u00b7se", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wirft sich dem Knaben nach,", "tokens": ["Wirft", "sich", "dem", "Kna\u00b7ben", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fa\u00dft ihn mit Adlerschnelle,", "tokens": ["Fa\u00dft", "ihn", "mit", "Ad\u00b7ler\u00b7schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Tr\u00e4gt ihn zum sichern Ort;", "tokens": ["Tr\u00e4gt", "ihn", "zum", "si\u00b7chern", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Kind entspringt der Welle,", "tokens": ["Das", "Kind", "ent\u00b7springt", "der", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Alten rei\u00dft sie fort.", "tokens": ["Den", "Al\u00b7ten", "rei\u00dft", "sie", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch als nun ausgesto\u00dfen", "tokens": ["Doch", "als", "nun", "aus\u00b7ge\u00b7sto\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Flut den toten Leib,", "tokens": ["Die", "Flut", "den", "to\u00b7ten", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da stehn um ihn, ergossen", "tokens": ["Da", "stehn", "um", "ihn", ",", "er\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Jammer, Mann und Weib;", "tokens": ["In", "Jam\u00b7mer", ",", "Mann", "und", "Weib", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als kracht' in seinem Grunde", "tokens": ["Als", "kracht'", "in", "sei\u00b7nem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Rotstocks Felsgestell,", "tokens": ["Des", "Rot\u00b7stocks", "Fels\u00b7ge\u00b7stell", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erschallt's aus ", "tokens": ["Er\u00b7schallt's", "aus"], "token_info": ["word", "word"], "pos": ["NE", "PTKVZ"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Der Tell ist tot, der Tell!", "tokens": ["Der", "Tell", "ist", "tot", ",", "der", "Tell", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "W\u00e4r ich ein Sohn der Berge,", "tokens": ["W\u00e4r", "ich", "ein", "Sohn", "der", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Hirt am ew'gen Schnee,", "tokens": ["Ein", "Hirt", "am", "ew'\u00b7gen", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4r ich ein kecker Ferge", "tokens": ["W\u00e4r", "ich", "ein", "ke\u00b7cker", "Fer\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Uris gr\u00fcnem See", "tokens": ["Aus", "U\u00b7ris", "gr\u00fc\u00b7nem", "See"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und tr\u00e4t in meinem Harme", "tokens": ["Und", "tr\u00e4t", "in", "mei\u00b7nem", "Har\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Tell, wo er verschied,", "tokens": ["Zum", "Tell", ",", "wo", "er", "ver\u00b7schied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Toten Haupt im Arme,", "tokens": ["Des", "To\u00b7ten", "Haupt", "im", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Spr\u00e4ch ich mein Klagelied:", "tokens": ["Spr\u00e4ch", "ich", "mein", "Kla\u00b7ge\u00b7lied", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbda liegst du, eine Leiche,", "tokens": ["\u00bb", "da", "liegst", "du", ",", "ei\u00b7ne", "Lei\u00b7che", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der aller Leben war;", "tokens": ["Der", "al\u00b7ler", "Le\u00b7ben", "war", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir trieft noch um das bleiche", "tokens": ["Dir", "trieft", "noch", "um", "das", "blei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gesicht dein greises Haar.", "tokens": ["Ge\u00b7sicht", "dein", "grei\u00b7ses", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier steht, den du gerettet,", "tokens": ["Hier", "steht", ",", "den", "du", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Kind wie Milch und Blut;", "tokens": ["Ein", "Kind", "wie", "Milch", "und", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Land, das du entkettet,", "tokens": ["Das", "Land", ",", "das", "du", "ent\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Steht rings in Alpenglut.", "tokens": ["Steht", "rings", "in", "Al\u00b7pen\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Kraft derselben Liebe,", "tokens": ["Die", "Kraft", "der\u00b7sel\u00b7ben", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die du dem Knaben trugst,", "tokens": ["Die", "du", "dem", "Kna\u00b7ben", "trugst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ward einst in dir zum Triebe,", "tokens": ["Ward", "einst", "in", "dir", "zum", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df du den Zwingherrn schlugst.", "tokens": ["Da\u00df", "du", "den", "Zwing\u00b7herrn", "schlugst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nie schlummernd, nie erschrocken,", "tokens": ["Nie", "schlum\u00b7mernd", ",", "nie", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War Retten stets dein Brauch,", "tokens": ["War", "Ret\u00b7ten", "stets", "dein", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie in den braunen Locken,", "tokens": ["Wie", "in", "den", "brau\u00b7nen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So in den grauen auch.", "tokens": ["So", "in", "den", "grau\u00b7en", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "W\u00e4rst du noch jung gewesen,", "tokens": ["W\u00e4rst", "du", "noch", "jung", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als du den Knaben fingst,", "tokens": ["Als", "du", "den", "Kna\u00b7ben", "fingst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4rst du dann genesen,", "tokens": ["Und", "w\u00e4rst", "du", "dann", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie du nun untergingst,", "tokens": ["Wie", "du", "nun", "un\u00b7ter\u00b7gingst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir h\u00e4tten draus geschlossen", "tokens": ["Wir", "h\u00e4t\u00b7ten", "draus", "ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf k\u00fcnft'ger Taten Ruhm:", "tokens": ["Auf", "k\u00fcnft'\u00b7ger", "Ta\u00b7ten", "Ruhm", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch sch\u00f6n ist ", "tokens": ["Doch", "sch\u00f6n", "ist"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Das schlichte Heldentum.", "tokens": ["Das", "schlich\u00b7te", "Hel\u00b7den\u00b7tum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dir hat dein Ohr geklungen", "tokens": ["Dir", "hat", "dein", "Ohr", "ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Lob, das man dir bot,", "tokens": ["Vom", "Lob", ",", "das", "man", "dir", "bot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ist zu ihm gedrungen", "tokens": ["Doch", "ist", "zu", "ihm", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein schwacher Ruf der Not.", "tokens": ["Ein", "schwa\u00b7cher", "Ruf", "der", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der ist ein Held der Freien,", "tokens": ["Der", "ist", "ein", "Held", "der", "Frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der, wann der Sieg ihn kr\u00e4nzt,", "tokens": ["Der", ",", "wann", "der", "Sieg", "ihn", "kr\u00e4nzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Noch gl\u00fcht, sich dem zu weihen,", "tokens": ["Noch", "gl\u00fcht", ",", "sich", "dem", "zu", "wei\u00b7hen", ","], "token_info": ["word", "w