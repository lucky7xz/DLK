{"textgrid.poem.50876": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Berliner Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute sah ich ein Gesicht,", "tokens": ["Heu\u00b7te", "sah", "ich", "ein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wonnevoll zu deuten:", "tokens": ["Won\u00b7ne\u00b7voll", "zu", "deu\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In dem fr\u00fchen Pfingstenlicht", "tokens": ["In", "dem", "fr\u00fc\u00b7hen", "Pfings\u00b7ten\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und beim Glockenl\u00e4uten", "tokens": ["Und", "beim", "Glo\u00b7cken\u00b7l\u00e4u\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schritten Weiber drei einher,", "tokens": ["Schrit\u00b7ten", "Wei\u00b7ber", "drei", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "CARD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Feierlich im Gange,", "tokens": ["Fei\u00b7er\u00b7lich", "im", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "W\u00e4scherinnen, fest und schwer!", "tokens": ["W\u00e4\u00b7sche\u00b7rin\u00b7nen", ",", "fest", "und", "schwer", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jede trug 'ne Stange.", "tokens": ["Je\u00b7de", "trug", "'ne", "Stan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "M\u00e4dchensommerkleider drei", "tokens": ["M\u00e4d\u00b7chen\u00b7som\u00b7mer\u00b7klei\u00b7der", "drei"], "token_info": ["word", "word"], "pos": ["NN", "CARD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Flaggten von den Stangen;", "tokens": ["Flagg\u00b7ten", "von", "den", "Stan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sch\u00f6nre Fahnen, stolz und frei,", "tokens": ["Sch\u00f6n\u00b7re", "Fah\u00b7nen", ",", "stolz", "und", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als je Krieger schwangen,", "tokens": ["Als", "je", "Krie\u00b7ger", "schwan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VVFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Blau und wei\u00df und rot gestreift,", "tokens": ["Blau", "und", "wei\u00df", "und", "rot", "ge\u00b7streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wunderbar befl\u00fcgelt,", "tokens": ["Wun\u00b7der\u00b7bar", "be\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Frisch gewaschen und gesteift,", "tokens": ["Frisch", "ge\u00b7wa\u00b7schen", "und", "ge\u00b7steift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Tadellos geb\u00fcgelt.", "tokens": ["Ta\u00b7del\u00b7los", "ge\u00b7b\u00fc\u00b7gelt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Lustig blies der Wind, der Schuft,", "tokens": ["Lus\u00b7tig", "blies", "der", "Wind", ",", "der", "Schuft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenden auf und B\u00fcste,", "tokens": ["Len\u00b7den", "auf", "und", "B\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und von frischer Morgenluft", "tokens": ["Und", "von", "fri\u00b7scher", "Mor\u00b7gen\u00b7luft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00e4hten sich die Br\u00fcste!", "tokens": ["Bl\u00e4h\u00b7ten", "sich", "die", "Br\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und ich sang, als ich gesehn", "tokens": ["Und", "ich", "sang", ",", "als", "ich", "ge\u00b7sehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ferne sie entschweben:", "tokens": ["Fer\u00b7ne", "sie", "ent\u00b7schwe\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Auf und la\u00dft die Fahnen wehn,", "tokens": ["Auf", "und", "la\u00dft", "die", "Fah\u00b7nen", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6n ist doch das Leben!", "tokens": ["Sch\u00f6n", "ist", "doch", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}