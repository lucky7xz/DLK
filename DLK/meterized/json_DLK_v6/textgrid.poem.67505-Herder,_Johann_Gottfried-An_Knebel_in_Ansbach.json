{"textgrid.poem.67505": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Knebel in Ansbach", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flieg her\u00fcber zu uns, Du zarter fr\u00e4nkischer Vogel!", "tokens": ["Flieg", "her\u00b7\u00fc\u00b7ber", "zu", "uns", ",", "Du", "zar\u00b7ter", "fr\u00e4n\u00b7ki\u00b7scher", "Vo\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "$,", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ueber den Th\u00fcringer Wald steure den fr\u00f6hlichen Flug", "tokens": ["Ue\u00b7ber", "den", "Th\u00fc\u00b7rin\u00b7ger", "Wald", "steu\u00b7re", "den", "fr\u00f6h\u00b7li\u00b7chen", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Gl\u00fccklich und ruh dann aus in Deinen Jenischen Auen,", "tokens": ["Gl\u00fcck\u00b7lich", "und", "ruh", "dann", "aus", "in", "Dei\u00b7nen", "Je\u00b7ni\u00b7schen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "ADV", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Ehe die Bl\u00fcthe noch, die Dich erwartet, sinkt!", "tokens": ["E\u00b7he", "die", "Bl\u00fc\u00b7the", "noch", ",", "die", "Dich", "er\u00b7war\u00b7tet", ",", "sinkt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Fr\u00fchlingsl\u00fcfte bringe zu uns; wo die liebliche Schwester", "tokens": ["Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", "brin\u00b7ge", "zu", "uns", ";", "wo", "die", "lieb\u00b7li\u00b7che", "Schwes\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Athmet, wehet die Luft sanfter und heilig um Euch.", "tokens": ["Ath\u00b7met", ",", "we\u00b7het", "die", "Luft", "sanf\u00b7ter", "und", "hei\u00b7lig", "um", "Euch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "APPR", "PPER", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}