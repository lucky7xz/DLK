{"textgrid.poem.52354": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "4. De B\u00f6rgers bi Regenweder", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Fredland was en ollen Paster namens Meier,", "tokens": ["In", "Fred\u00b7land", "was", "en", "ol\u00b7len", "Pas\u00b7ter", "na\u00b7mens", "Mei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "FM", "FM", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dat was en ollen Mann, gottsf\u00fcrchtiglich,", "tokens": ["Dat", "was", "en", "ol\u00b7len", "Mann", ",", "gotts\u00b7f\u00fcrch\u00b7tig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Un noch en annern Paster, namens Dreier,", "tokens": ["Un", "noch", "en", "an\u00b7nern", "Pas\u00b7ter", ",", "na\u00b7mens", "Drei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "FM", "FM", "NN", "$,", "ADV", "CARD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "De was ", "tokens": ["De", "was"], "token_info": ["word", "word"], "pos": ["NE", "PWS"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Nu kam in'n Aust denn mal 'ne Tid,", "tokens": ["Nu", "kam", "in'n", "Aust", "denn", "mal", "'ne", "Tid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dat dat drei Wochen fort in eine Swit", "tokens": ["Dat", "dat", "drei", "Wo\u00b7chen", "fort", "in", "ei\u00b7ne", "Swit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "CARD", "NN", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dagd\u00e4glich von den Hewen got.", "tokens": ["Dag\u00b7d\u00e4g\u00b7lich", "von", "den", "He\u00b7wen", "got."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dat was denn nu 'ne grote Not.", "tokens": ["Dat", "was", "denn", "nu", "'ne", "gro\u00b7te", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "De B\u00fcrgerschaft, de kamm tausamen,", "tokens": ["De", "B\u00fcr\u00b7ger\u00b7schaft", ",", "de", "kamm", "tau\u00b7sa\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "\u00dcm Rat tau holl'n, wat dorbi wir tau maken.", "tokens": ["\u00dcm", "Rat", "tau", "holl'n", ",", "wat", "dor\u00b7bi", "wir", "tau", "ma\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PRELS", "ADV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dor w\u00fcrd' denn hen un wedder spraken,", "tokens": ["Dor", "w\u00fcrd'", "denn", "hen", "un", "wed\u00b7der", "spra\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "FM", "FM", "KON", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Bet s' endlich \u00e4werein s\u00fcnd kamen,", "tokens": ["Bet", "s'", "end\u00b7lich", "\u00e4\u00b7wer\u00b7ein", "s\u00fcnd", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Sei wull'n den Preister beden laten.", "tokens": ["Sei", "wull'n", "den", "Preis\u00b7ter", "be\u00b7den", "la\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "So wid was't gaud. Dit was nu woll beslaten.", "tokens": ["So", "wid", "was't", "gaud", ".", "Dit", "was", "nu", "woll", "bes\u00b7la\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "$.", "APPR", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "De Frag was \u00e4werst nu: Wen von de beiden?", "tokens": ["De", "Frag", "was", "\u00e4\u00b7werst", "nu", ":", "Wen", "von", "de", "bei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PWS", "ADV", "ADV", "$.", "PWS", "APPR", "NE", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "De irst s\u00e4d: \u00bbDreier!\u00ab Un ok den tweiten", "tokens": ["De", "irst", "s\u00e4d", ":", "\u00bb", "Drei\u00b7er", "!", "\u00ab", "Un", "ok", "den", "twei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "$(", "CARD", "$.", "$(", "NE", "NE", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Schint Dreier as de Best; un \u00bbDreier, Dreier, Dreier!\u00ab", "tokens": ["Schint", "Drei\u00b7er", "as", "de", "Best", ";", "un", "\u00bb", "Drei\u00b7er", ",", "Drei\u00b7er", ",", "Drei\u00b7er", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "FM", "$(", "CARD", "$,", "CARD", "$,", "CARD", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "G\u00fcng't d\u00f6rch de ganze B\u00f6rgerschaft;", "tokens": ["G\u00fcng't", "d\u00f6rch", "de", "gan\u00b7ze", "B\u00f6r\u00b7ger\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Man blot oll Meister N\u00e4w' s\u00e4d: \u00bbMeier!\u00ab", "tokens": ["Man", "blot", "oll", "Meis\u00b7ter", "N\u00e4w'", "s\u00e4d", ":", "\u00bb", "Mei\u00b7er", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["PIS", "FM.en", "FM.en", "FM.en", "FM.en", "FM.en", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Dat h\u00fclp em nich, sin Stimm was unnerlegen.", "tokens": ["Dat", "h\u00fclp", "em", "nich", ",", "sin", "Stimm", "was", "un\u00b7ner\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PPOSAT", "NN", "PWS", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Den annern S\u00fcnndag predigt nu mit grote Kraft", "tokens": ["Den", "an\u00b7nern", "S\u00fcnn\u00b7dag", "pre\u00b7digt", "nu", "mit", "gro\u00b7te", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "De Paster Dreier gegen Regen;", "tokens": ["De", "Pas\u00b7ter", "Drei\u00b7er", "ge\u00b7gen", "Re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Je, hadd't irst regent, regent't nu irst recht!", "tokens": ["Je", ",", "hadd't", "irst", "re\u00b7gent", ",", "re\u00b7gent't", "nu", "irst", "recht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ADV", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "De Regen f\u00f6ll in G\u00e4ten nedder.", "tokens": ["De", "Re\u00b7gen", "f\u00f6ll", "in", "G\u00e4\u00b7ten", "ned\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "As in de negste Woch' dunn wedder", "tokens": ["As", "in", "de", "negs\u00b7te", "Wo\u00b7ch'", "dunn", "wed\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "De B\u00f6rgerschaft tausam is, seggt", "tokens": ["De", "B\u00f6r\u00b7ger\u00b7schaft", "tau\u00b7sam", "is", ",", "seggt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["FM", "FM", "FM", "FM", "$,", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Stadtspreker P\u00e4pk': \u00bbNa h\u00fcrt, mi d\u00fccht,", "tokens": ["Stadt\u00b7spre\u00b7ker", "P\u00e4p\u00b7k'", ":", "\u00bb", "Na", "h\u00fcrt", ",", "mi", "d\u00fccht", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "$(", "ITJ", "VVFIN", "$,", "NE", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Dor uns dat mi\u00dfgl\u00fcckt is mit Paster Dreiern,", "tokens": ["Dor", "uns", "dat", "mi\u00df\u00b7gl\u00fcckt", "is", "mit", "Pas\u00b7ter", "Drei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "FM.nl", "FM.nl", "FM.nl", "APPR", "NN", "NN", "$,"], "meter": "----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So nem wi nu mal Paster Meiern,", "tokens": ["So", "nem", "wi", "nu", "mal", "Pas\u00b7ter", "Mei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "ADV", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit desen gl\u00fcckt uns dat villicht.\u00ab", "tokens": ["Mit", "de\u00b7sen", "gl\u00fcckt", "uns", "dat", "vil\u00b7licht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Oll Paster Meier bedt ok, wat hei k\u00fcnn,", "tokens": ["Oll", "Pas\u00b7ter", "Mei\u00b7er", "bedt", "ok", ",", "wat", "hei", "k\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Un as hei noch in't beste Beden st\u00fcnn,", "tokens": ["Un", "as", "hei", "noch", "in't", "bes\u00b7te", "Be\u00b7den", "st\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADV", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Dunn kickt de S\u00fcnn all in de Finsterruten,", "tokens": ["Dunn", "kickt", "de", "S\u00fcnn", "all", "in", "de", "Fins\u00b7ter\u00b7ru\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Un't beste Weder schint dor buten.", "tokens": ["Un't", "bes\u00b7te", "We\u00b7der", "schint", "dor", "bu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "As Meister N\u00e4w' nu ut de Kirch 'rutgeiht,", "tokens": ["As", "Meis\u00b7ter", "N\u00e4w'", "nu", "ut", "de", "Kirch", "'rut\u00b7geiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADV", "FM", "FM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kloppt P\u00e4pken up de Schuller hei un seggt:", "tokens": ["Kloppt", "P\u00e4p\u00b7ken", "up", "de", "Schul\u00b7ler", "hei", "un", "seggt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "FM", "FM", "FM", "FM", "FM", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbna, Nahwer, heww ick nu nich recht?", "tokens": ["\u00bb", "na", ",", "Nah\u00b7wer", ",", "heww", "ick", "nu", "nich", "recht", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heww ick nich seggt, wenn Meier beden deiht,", "tokens": ["Heww", "ick", "nich", "seggt", ",", "wenn", "Mei\u00b7er", "be\u00b7den", "deiht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Un wenn dat ok mit Emmern g\u00f6t,", "tokens": ["Un", "wenn", "dat", "ok", "mit", "Em\u00b7mern", "g\u00f6t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Uns' Herrgott mag nu willen oder nich, hei m\u00f6t!\u00ab", "tokens": ["Un\u00b7s'", "Herr\u00b7gott", "mag", "nu", "wil\u00b7len", "o\u00b7der", "nich", ",", "hei", "m\u00f6t", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "NN", "KON", "PTKNEG", "$,", "FM", "FM", "$.", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}