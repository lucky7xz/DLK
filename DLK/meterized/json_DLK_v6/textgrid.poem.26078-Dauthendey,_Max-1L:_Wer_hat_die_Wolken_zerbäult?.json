": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wem ist seine Qual hell w\u00fctend Genu\u00df?", "tokens": ["Wem", "ist", "sei\u00b7ne", "Qual", "hell", "w\u00fc\u00b7tend", "Ge\u00b7nu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "ADJD", "ADJD", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und wer verflucht sich finster und stier?", "tokens": ["Und", "wer", "ver\u00b7flucht", "sich", "fins\u00b7ter", "und", "stier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ist es die Nacht?", "tokens": ["Ist", "es", "die", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Oder ein St\u00fcck Schatten von mir?", "tokens": ["O\u00b7der", "ein", "St\u00fcck", "Schat\u00b7ten", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}