{"textgrid.poem.60776": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn zwei Lose vor uns legt ein Beschlu\u00df der Zeit,", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn zwei Lose vor uns legt ein Beschlu\u00df der Zeit,", "tokens": ["Wenn", "zwei", "Lo\u00b7se", "vor", "uns", "legt", "ein", "Be\u00b7schlu\u00df", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "APPR", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schwer ist's, wirklichem Ruf folgen und falschen fliehn:", "tokens": ["Schwer", "ist's", ",", "wirk\u00b7li\u00b7chem", "Ruf", "fol\u00b7gen", "und", "fal\u00b7schen", "fliehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "ADJA", "NN", "VVINF", "KON", "VVINF", "VVINF", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "F\u00fcrs Leben hinaus entscheidet", "tokens": ["F\u00fcrs", "Le\u00b7ben", "hin\u00b7aus", "ent\u00b7schei\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "APZR", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der entschiedene kurze Schritt.", "tokens": ["Der", "ent\u00b7schie\u00b7de\u00b7ne", "kur\u00b7ze", "Schritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Ehmals d\u00e4mmerten uns mutige Hoffnungen,", "tokens": ["Eh\u00b7mals", "d\u00e4m\u00b7mer\u00b7ten", "uns", "mu\u00b7ti\u00b7ge", "Hoff\u00b7nun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+--++-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ja, wir wollten Genu\u00df aus Arethusas Quell", "tokens": ["Ja", ",", "wir", "woll\u00b7ten", "Ge\u00b7nu\u00df", "aus", "A\u00b7ret\u00b7hu\u00b7sas", "Quell"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "NN", "APPR", "NE", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Einschl\u00fcrfen; der k\u00fchnre Wunsch war", "tokens": ["Ein\u00b7schl\u00fcr\u00b7fen", ";", "der", "k\u00fchn\u00b7re", "Wunsch", "war"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aganippische Flut zu schaun!", "tokens": ["A\u00b7gan\u00b7ip\u00b7pi\u00b7sche", "Flut", "zu", "schaun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Doch dich lockten indes heimische Triebe bald", "tokens": ["Doch", "dich", "lock\u00b7ten", "in\u00b7des", "hei\u00b7mi\u00b7sche", "Trie\u00b7be", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJA", "NN", "ADV"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Fernhin (wo in des Nords Winter ein edler F\u00fcrst", "tokens": ["Fern\u00b7hin", "(", "wo", "in", "des", "Nords", "Win\u00b7ter", "ein", "ed\u00b7ler", "F\u00fcrst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "PWAV", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN"], "meter": "---+--+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auss\u00e4t ein Athen des Geistes)", "tokens": ["Aus\u00b7s\u00e4t", "ein", "A\u00b7then", "des", "Geis\u00b7tes", ")"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "An die skythische, kalte Spree.", "tokens": ["An", "die", "sky\u00b7thi\u00b7sche", ",", "kal\u00b7te", "Spree", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Mir auch schien' es vielleicht r\u00fchmlicher, hinzuziehn,", "tokens": ["Mir", "auch", "schien'", "es", "viel\u00b7leicht", "r\u00fchm\u00b7li\u00b7cher", ",", "hin\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "VVIZU", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wo hinweist der Magnet; aber dem tr\u00e4gen Fu\u00df", "tokens": ["Wo", "hin\u00b7weist", "der", "Mag\u00b7net", ";", "a\u00b7ber", "dem", "tr\u00e4\u00b7gen", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sind Brenner zugleich und Gotthard", "tokens": ["Sind", "Bren\u00b7ner", "zu\u00b7gleich", "und", "Got\u00b7thard"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADV", "KON", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Unersteigliche Berge l\u00e4ngst.", "tokens": ["Un\u00b7er\u00b7steig\u00b7li\u00b7che", "Ber\u00b7ge", "l\u00e4ngst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "R\u00fcckw\u00e4rts liegen so weit fr\u00fchere Tage mir,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "lie\u00b7gen", "so", "weit", "fr\u00fc\u00b7he\u00b7re", "Ta\u00b7ge", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ADJA", "NN", "PPER", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Als frohsinnig und nicht ohne befeuernden", "tokens": ["Als", "froh\u00b7sin\u00b7nig", "und", "nicht", "oh\u00b7ne", "be\u00b7feu\u00b7ern\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Beifall in der Freunde Kreis ich", "tokens": ["Bei\u00b7fall", "in", "der", "Freun\u00b7de", "Kreis", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Ges\u00e4nge der Jugend las.", "tokens": ["Die", "Ge\u00b7s\u00e4n\u00b7ge", "der", "Ju\u00b7gend", "las", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Hier nun sing ich allein, freundliches Lob verhallt", "tokens": ["Hier", "nun", "sing", "ich", "al\u00b7lein", ",", "freund\u00b7li\u00b7ches", "Lob", "ver\u00b7hallt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Fernab, selten geh\u00f6rt; aber es schweigen auch", "tokens": ["Fern\u00b7ab", ",", "sel\u00b7ten", "ge\u00b7h\u00f6rt", ";", "a\u00b7ber", "es", "schwei\u00b7gen", "auch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADJD", "VVFIN", "$.", "KON", "PPER", "VVFIN", "ADV"], "meter": "-+---+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Lautgellende P\u00f6belstimmen,", "tokens": ["Laut\u00b7gel\u00b7len\u00b7de", "P\u00f6\u00b7bel\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der kleinere Schrei des Neids.", "tokens": ["Und", "der", "klei\u00b7ne\u00b7re", "Schrei", "des", "Neids", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}