{"dta.poem.2919": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "118.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer hat es nicht erlebt, da\u00df etwas tief ihn kr\u00e4nkt,", "tokens": ["Wer", "hat", "es", "nicht", "er\u00b7lebt", ",", "da\u00df", "et\u00b7was", "tief", "ihn", "kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "KOUS", "PIS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich den Augenblick sein Haupt in Unmut senkt?", "tokens": ["Und", "sich", "den", "Au\u00b7gen\u00b7blick", "sein", "Haupt", "in", "Un\u00b7mut", "senkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch oft nach einem Tag, oft schon nach einer Stunde,", "tokens": ["Doch", "oft", "nach", "ei\u00b7nem", "Tag", ",", "oft", "schon", "nach", "ei\u00b7ner", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bel\u00e4chelst du den Schmerz, und f\u00fchlst nicht mehr die Wunde.", "tokens": ["Be\u00b7l\u00e4\u00b7chelst", "du", "den", "Schmerz", ",", "und", "f\u00fchlst", "nicht", "mehr", "die", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Darum, zur Stunde wo dich etwas kr\u00e4nkt, o denke", "tokens": ["Da\u00b7rum", ",", "zur", "Stun\u00b7de", "wo", "dich", "et\u00b7was", "kr\u00e4nkt", ",", "o", "den\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "$,", "APPRART", "NN", "PWAV", "PPER", "ADV", "VVFIN", "$,", "FM", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der n\u00e4chsten Stunde gleich, damit dichs gar nicht kr\u00e4nke.", "tokens": ["Der", "n\u00e4chs\u00b7ten", "Stun\u00b7de", "gleich", ",", "da\u00b7mit", "dichs", "gar", "nicht", "kr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "KOUS", "PIS", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch leichter ist gesagt dergleichen als gethan;", "tokens": ["Doch", "leich\u00b7ter", "ist", "ge\u00b7sagt", "derg\u00b7lei\u00b7chen", "als", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "VVPP", "PIS", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gegenwart r\u00fchrt hart, die Zukunft leiser an.", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", "r\u00fchrt", "hart", ",", "die", "Zu\u00b7kunft", "lei\u00b7ser", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da wo der Sto\u00df dich trifft, wird ihn der Sinn empfinden,", "tokens": ["Da", "wo", "der", "Sto\u00df", "dich", "trifft", ",", "wird", "ihn", "der", "Sinn", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch die Ersch\u00fcttrung hilft der Geist dir \u00fcberwinden.", "tokens": ["Doch", "die", "Er\u00b7sch\u00fct\u00b7trung", "hilft", "der", "Geist", "dir", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}