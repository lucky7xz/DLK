{"textgrid.poem.59382": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchs Leben schleichen feindlich fremde Stunden,", "tokens": ["Durchs", "Le\u00b7ben", "schlei\u00b7chen", "feind\u00b7lich", "frem\u00b7de", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo \u00c4ngsten aus der Brust hinunterlauschen,", "tokens": ["Wo", "\u00c4ngs\u00b7ten", "aus", "der", "Brust", "hin\u00b7un\u00b7ter\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verworrne Worte mit dem Abgrund tauschen,", "tokens": ["Ver\u00b7worr\u00b7ne", "Wor\u00b7te", "mit", "dem", "Ab\u00b7grund", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drin bodenlose Nacht nur ward erfunden.", "tokens": ["Drin", "bo\u00b7den\u00b7lo\u00b7se", "Nacht", "nur", "ward", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl ist des Dichters Seele stumm verbunden", "tokens": ["Wohl", "ist", "des", "Dich\u00b7ters", "See\u00b7le", "stumm", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit M\u00e4chten, die am Volk vor\u00fcberrauschen;", "tokens": ["Mit", "M\u00e4ch\u00b7ten", ",", "die", "am", "Volk", "vor\u00b7\u00fc\u00b7berr\u00b7au\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sehnsucht mu\u00df wachsen an der Tiefe Rauschen", "tokens": ["Sehn\u00b7sucht", "mu\u00df", "wach\u00b7sen", "an", "der", "Tie\u00b7fe", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "VVINF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach hellerm Licht und nach des Himmels Kunden.", "tokens": ["Nach", "hel\u00b7lerm", "Licht", "und", "nach", "des", "Him\u00b7mels", "Kun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Herr! du kennst allein den treuen Willen,", "tokens": ["O", "Herr", "!", "du", "kennst", "al\u00b7lein", "den", "treu\u00b7en", "Wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Befrei ihn von der Kerkerluft des B\u00f6sen,", "tokens": ["Be\u00b7frei", "ihn", "von", "der", "Ker\u00b7ker\u00b7luft", "des", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df nicht die eigne Brust mich feig zerschlagen!", "tokens": ["La\u00df", "nicht", "die", "eig\u00b7ne", "Brust", "mich", "feig", "zer\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ART", "ADJA", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie ich schreibe hier, den Schmerz zu stillen,", "tokens": ["Und", "wie", "ich", "schrei\u00b7be", "hier", ",", "den", "Schmerz", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchl ich den Engel schon die Riegel l\u00f6sen,", "tokens": ["F\u00fchl", "ich", "den", "En\u00b7gel", "schon", "die", "Rie\u00b7gel", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kann vor Glanze nicht mehr weiterklagen.", "tokens": ["Und", "kann", "vor", "Glan\u00b7ze", "nicht", "mehr", "wei\u00b7ter\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}