{"textgrid.poem.48754": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "41. Als das Holsteinische Schiff Friedrich wieder an die Persische Flotte gelangete, von der es wegen widrigen Windes in die dritte Woche abgewesen war, 1636 den 3. Sept. 150. Werste \u00fcber Zariza", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mach nun die Focke voll und schw\u00e4ngre den Meisan,", "tokens": ["Mach", "nun", "die", "Fo\u00b7cke", "voll", "und", "schw\u00e4ng\u00b7re", "den", "Mei\u00b7san", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "o g\u00fcnstiger Nordwest, hol' Atem aus dem Grunde,", "tokens": ["o", "g\u00fcns\u00b7ti\u00b7ger", "Nord\u00b7west", ",", "hol'", "A\u00b7tem", "aus", "dem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$,", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "blas unsern ", "tokens": ["blas", "un\u00b7sern"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "der deiner weiter darf, als bis auf ", "tokens": ["der", "dei\u00b7ner", "wei\u00b7ter", "darf", ",", "als", "bis", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADV", "VMFIN", "$,", "KOUS", "ADV", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Blas hurtig, wie du tust. Dort ist die Karavan,", "tokens": ["Blas", "hur\u00b7tig", ",", "wie", "du", "tust", ".", "Dort", "ist", "die", "Ka\u00b7ra\u00b7van", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die sich nun wieder zeigt nach mancher b\u00f6sen Stunde,", "tokens": ["die", "sich", "nun", "wie\u00b7der", "zeigt", "nach", "man\u00b7cher", "b\u00f6\u00b7sen", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "ADV", "ADV", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so unsern Lauf hielt auf. Wir sind in einem Bunde", "tokens": ["so", "un\u00b7sern", "Lauf", "hielt", "auf", ".", "Wir", "sind", "in", "ei\u00b7nem", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und hoffen bald zu sehn mit ihr ihr ", "tokens": ["und", "hof\u00b7fen", "bald", "zu", "sehn", "mit", "ihr", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKZU", "VVINF", "APPR", "PPER", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Eilt, steckt die Flaggen auf, r\u00fchrt Trummeln und Trombetten,", "tokens": ["Eilt", ",", "steckt", "die", "Flag\u00b7gen", "auf", ",", "r\u00fchrt", "Trum\u00b7meln", "und", "Trom\u00b7bet\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "gebt Feuer alsobald aus allen Falkenetten,", "tokens": ["gebt", "Feu\u00b7er", "al\u00b7so\u00b7bald", "aus", "al\u00b7len", "Fal\u00b7ke\u00b7net\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "l\u00f6st die Musketen ab, ihr Br\u00fcder, und ruft laut:", "tokens": ["l\u00f6st", "die", "Mus\u00b7ke\u00b7ten", "ab", ",", "ihr", "Br\u00fc\u00b7der", ",", "und", "ruft", "laut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Gl\u00fcck her! Gl\u00fcck allzeit her! Wir reisen mit Verlangen", "tokens": ["Gl\u00fcck", "her", "!", "Gl\u00fcck", "all\u00b7zeit", "her", "!", "Wir", "rei\u00b7sen", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "NN", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "die Tochter des Gestirns bald fr\u00f6lich zu empfangen,", "tokens": ["die", "Toch\u00b7ter", "des", "Ge\u00b7stirns", "bald", "fr\u00f6\u00b7lich", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die unser ", "tokens": ["die", "un\u00b7ser"], "token_info": ["word", "word"], "pos": ["ART", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}}}}