{"dta.poem.11302": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der jungfern ja-wort.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die m\u00e4gdgen machen sich gemeiniglich gar breit,", "tokens": ["Die", "m\u00e4gd\u00b7gen", "ma\u00b7chen", "sich", "ge\u00b7mei\u00b7nig\u00b7lich", "gar", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als k\u00f6nten sie vergn\u00fcgt auch ohne m\u00e4nner leben;", "tokens": ["Als", "k\u00f6n\u00b7ten", "sie", "ver\u00b7gn\u00fcgt", "auch", "oh\u00b7ne", "m\u00e4n\u00b7ner", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein ich glaub es nicht, ihr schlu\u00df geht viel zu weit.", "tokens": ["Al\u00b7lein", "ich", "glaub", "es", "nicht", ",", "ihr", "schlu\u00df", "geht", "viel", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn seh man nur einmal ein paar zusammen geben;", "tokens": ["Denn", "seh", "man", "nur", "ein\u00b7mal", "ein", "paar", "zu\u00b7sam\u00b7men", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADV", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So hat der prediger die Gretgen kaum gefragt,", "tokens": ["So", "hat", "der", "pre\u00b7di\u00b7ger", "die", "Gret\u00b7gen", "kaum", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn sie das ja-wort schon mit vollem halse sagt.", "tokens": ["Wenn", "sie", "das", "ja\u00b7wort", "schon", "mit", "vol\u00b7lem", "hal\u00b7se", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}