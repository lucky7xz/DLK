{"dta.poem.6199": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Jahrs-Zeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann sie nun fern von unserm Nord", "tokens": ["Wann", "sie", "nun", "fern", "von", "un\u00b7serm", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nach den S\u00fcd-Pol scheint zu fliehen,", "tokens": ["Und", "nach", "den", "S\u00fcd\u00b7Pol", "scheint", "zu", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bemercken wir bey allen Schritten dort,", "tokens": ["Be\u00b7mer\u00b7cken", "wir", "bey", "al\u00b7len", "Schrit\u00b7ten", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Frost und Eis bey uns die Felder \u00fcberziehen.", "tokens": ["Wie", "Frost", "und", "Eis", "bey", "uns", "die", "Fel\u00b7der", "\u00fc\u00b7ber\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Von Schatten-reichen-Wald und B\u00fcschen", "tokens": ["Von", "Schat\u00b7ten\u00b7rei\u00b7chen\u00b7Wald", "und", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00e4llt ihr begr\u00fcntes Haar herab.", "tokens": ["F\u00e4llt", "ihr", "be\u00b7gr\u00fcn\u00b7tes", "Haar", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der wilde Nord-Wind f\u00fchrt mit scharffen Zischen", "tokens": ["Der", "wil\u00b7de", "Nord\u00b7Wind", "f\u00fchrt", "mit", "scharf\u00b7fen", "Zi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die strenge K\u00e4lt herbey, versteint die Furchen, bindet", "tokens": ["Die", "stren\u00b7ge", "K\u00e4lt", "her\u00b7bey", ",", "ver\u00b7steint", "die", "Fur\u00b7chen", ",", "bin\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und fesselt Flu\u00df und Bach. Es siehet alles hart,", "tokens": ["Und", "fes\u00b7selt", "Flu\u00df", "und", "Bach", ".", "Es", "sie\u00b7het", "al\u00b7les", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "KON", "NE", "$.", "PPER", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ver\u00f6det, traurig aus,", "tokens": ["Ver\u00b7\u00f6\u00b7det", ",", "trau\u00b7rig", "aus", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die Nahrungs-Krafft verschwindet,", "tokens": ["Die", "Nah\u00b7rungs\u00b7Krafft", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Der feuchte Safft empfindet", "tokens": ["Der", "feuch\u00b7te", "Safft", "emp\u00b7fin\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Nun kein Bewegen mehr;", "tokens": ["Nun", "kein", "Be\u00b7we\u00b7gen", "mehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Es liegt die Erd erstarrt.", "tokens": ["Es", "liegt", "die", "Erd", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Sie scheinet tr\u00e4g. Es bleibt in ihrer Schoo\u00df entlaubt", "tokens": ["Sie", "schei\u00b7net", "tr\u00e4g", ".", "Es", "bleibt", "in", "ih\u00b7rer", "Schoo\u00df", "ent\u00b7laubt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Heer der Nahrungs-reichen-Sprossen:", "tokens": ["Das", "Heer", "der", "Nah\u00b7rungs\u00b7rei\u00b7chen\u00b7Spros\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "So lang der kalte Nord-Wind schnaubt,", "tokens": ["So", "lang", "der", "kal\u00b7te", "Nord\u00b7Wind", "schnaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Als unbeseelet eingeschlossen.", "tokens": ["Als", "un\u00b7be\u00b7see\u00b7let", "ein\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}