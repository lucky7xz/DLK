{"textgrid.poem.63834": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00f6ckchen", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still! o still!", "tokens": ["Still", "!", "o", "still", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "FM", "FM", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "H\u00f6rst du durch die Abendluft,", "tokens": ["H\u00f6rst", "du", "durch", "die", "A\u00b7ben\u00b7dluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das leise Gl\u00f6ckchen ruft?", "tokens": ["Wie", "das", "lei\u00b7se", "Gl\u00f6ck\u00b7chen", "ruft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00dft du, was es sagen will?", "tokens": ["Wei\u00dft", "du", ",", "was", "es", "sa\u00b7gen", "will", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sanft zur Ruh'", "tokens": ["Sanft", "zur", "Ruh'"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ward ein m\u00fcdes Herz gebracht,", "tokens": ["Ward", "ein", "m\u00fc\u00b7des", "Herz", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und noch eine gute Nacht", "tokens": ["Und", "noch", "ei\u00b7ne", "gu\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruft ihm Mutter Erde zu.", "tokens": ["Ruft", "ihm", "Mut\u00b7ter", "Er\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}