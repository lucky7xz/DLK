{"textgrid.poem.50784": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "5", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ins Innre jedes Sarges sollte man", "tokens": ["Ins", "Inn\u00b7re", "je\u00b7des", "Sar\u00b7ges", "soll\u00b7te", "man"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PIAT", "NN", "VMFIN", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hell von Metall 'nen Spiegel schlagen an,", "tokens": ["Hell", "von", "Me\u00b7tall", "'nen", "Spie\u00b7gel", "schla\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der, wie man sagt, in tiefster Dunkelheit", "tokens": ["Der", ",", "wie", "man", "sagt", ",", "in", "tiefs\u00b7ter", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "PIS", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Getreu die Leichenz\u00fcge konterfeit.", "tokens": ["Ge\u00b7treu", "die", "Lei\u00b7chen\u00b7z\u00fc\u00b7ge", "kon\u00b7ter\u00b7feit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das w\u00e4r ein Schatzfund, wenn aus Gras und Kraut", "tokens": ["Das", "w\u00e4r", "ein", "Schatz\u00b7fund", ",", "wenn", "aus", "Gras", "und", "Kraut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man grauend diese Bilder dann erschaut',", "tokens": ["Man", "grau\u00b7end", "die\u00b7se", "Bil\u00b7der", "dann", "er\u00b7schaut'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "PDAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie hingehaucht, vom Rost leicht \u00fcberwebt,", "tokens": ["Wie", "hin\u00b7ge\u00b7haucht", ",", "vom", "Rost", "leicht", "\u00fc\u00b7ber\u00b7webt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unheimlich hell vom Sonnenlicht belebt!", "tokens": ["Un\u00b7heim\u00b7lich", "hell", "vom", "Son\u00b7nen\u00b7licht", "be\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die man lebendig einst zu Grabe trug,", "tokens": ["Die", "man", "le\u00b7ben\u00b7dig", "einst", "zu", "Gra\u00b7be", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gesunden Herzens in die Erde schlug:", "tokens": ["Ge\u00b7sun\u00b7den", "Her\u00b7zens", "in", "die", "Er\u00b7de", "schlug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit den zerri\u00dfnen Z\u00fcgen w\u00e4ren sie", "tokens": ["Mit", "den", "zer\u00b7ri\u00df\u00b7nen", "Z\u00fc\u00b7gen", "w\u00e4\u00b7ren", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Perlen einer Totengalerie.", "tokens": ["Die", "Per\u00b7len", "ei\u00b7ner", "To\u00b7ten\u00b7ga\u00b7le\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn irgendwo ein reicher K\u00f6nig pra\u00dft,", "tokens": ["Wenn", "ir\u00b7gend\u00b7wo", "ein", "rei\u00b7cher", "K\u00f6\u00b7nig", "pra\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Licht und Leben und die Jugend ha\u00dft,", "tokens": ["Der", "Licht", "und", "Le\u00b7ben", "und", "die", "Ju\u00b7gend", "ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch heuchlerisch um tote Musen freit:", "tokens": ["Doch", "heuch\u00b7le\u00b7risch", "um", "to\u00b7te", "Mu\u00b7sen", "freit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihm w\u00e4r ein solcher Kunstschatz dann geweiht!", "tokens": ["Ihm", "w\u00e4r", "ein", "sol\u00b7cher", "Kunst\u00b7schatz", "dann", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}