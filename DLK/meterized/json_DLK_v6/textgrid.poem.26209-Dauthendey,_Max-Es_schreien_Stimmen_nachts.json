{"textgrid.poem.26209": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Es schreien Stimmen nachts", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6re heller, als die Ohren wissen.", "tokens": ["Ich", "h\u00f6\u00b7re", "hel\u00b7ler", ",", "als", "die", "Oh\u00b7ren", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es schmerzen Echos, tief in mir geboren.", "tokens": ["Es", "schmer\u00b7zen", "E\u00b7chos", ",", "tief", "in", "mir", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es schreien Stimmen nachts aus meinen Kissen,", "tokens": ["Es", "schrei\u00b7en", "Stim\u00b7men", "nachts", "aus", "mei\u00b7nen", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Stimmen Mutiger, die ihre Kraft verloren,", "tokens": ["Die", "Stim\u00b7men", "Mu\u00b7ti\u00b7ger", ",", "die", "ih\u00b7re", "Kraft", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die auf des Krieges Feldern liegen, verblutet und zerrissen.", "tokens": ["Die", "auf", "des", "Krie\u00b7ges", "Fel\u00b7dern", "lie\u00b7gen", ",", "ver\u00b7blu\u00b7tet", "und", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}}}}}