{"textgrid.poem.66268": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Aufruf", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erhebe dich, o Mensch, vom Schwanenbette", "tokens": ["Er\u00b7he\u00b7be", "dich", ",", "o", "Mensch", ",", "vom", "Schwa\u00b7nen\u00b7bet\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Weichlichkeit, die dich entehrt!", "tokens": ["Der", "Weich\u00b7lich\u00b7keit", ",", "die", "dich", "ent\u00b7ehrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerrei\u00df die diamantne Kette,", "tokens": ["Zer\u00b7rei\u00df", "die", "di\u00b7a\u00b7mant\u00b7ne", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die deinen Geist beschwert.", "tokens": ["Die", "dei\u00b7nen", "Geist", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du tauchst dich immer tiefer in die Pf\u00fctze", "tokens": ["Du", "tauchst", "dich", "im\u00b7mer", "tie\u00b7fer", "in", "die", "Pf\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der thierischgroben Sinnlichkeit,", "tokens": ["Der", "thie\u00b7rischgro\u00b7ben", "Sinn\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und blinzest vor dem Zackenblitze", "tokens": ["Und", "blin\u00b7zest", "vor", "dem", "Za\u00b7cken\u00b7blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nahen Ewigkeit.", "tokens": ["Der", "na\u00b7hen", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich strecke meine Rechte in die Wolke,", "tokens": ["Ich", "stre\u00b7cke", "mei\u00b7ne", "Rech\u00b7te", "in", "die", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(o Mitgeschaffner, h\u00f6re mich!)", "tokens": ["(", "o", "Mit\u00b7ge\u00b7schaff\u00b7ner", ",", "h\u00f6\u00b7re", "mich", "!", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$,", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schw\u00f6re vor dem Geistervolke:", "tokens": ["Und", "schw\u00f6\u00b7re", "vor", "dem", "Geis\u00b7ter\u00b7vol\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es wartet ", "tokens": ["Es", "war\u00b7tet"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Erst harrt auf dich der grauenvolle Alte,", "tokens": ["Erst", "harrt", "auf", "dich", "der", "grau\u00b7en\u00b7vol\u00b7le", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens finstrer Hasser \u2013 ", "tokens": ["Des", "Le\u00b7bens", "finst\u00b7rer", "Has\u00b7ser", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der furchtbarstumm im Hinterhalte", "tokens": ["Der", "furcht\u00b7bar\u00b7stumm", "im", "Hin\u00b7ter\u00b7hal\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus deinem Leben droht.", "tokens": ["Aus", "dei\u00b7nem", "Le\u00b7ben", "droht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann irrt auch deine Seele durch die N\u00e4chte", "tokens": ["Dann", "irrt", "auch", "dei\u00b7ne", "See\u00b7le", "durch", "die", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Todeswohnungen allein;", "tokens": ["Der", "To\u00b7des\u00b7woh\u00b7nun\u00b7gen", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Und ewig wirst du ", "tokens": ["Und", "e\u00b7wig", "wirst", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Der kalte Trost ", "tokens": ["Der", "kal\u00b7te", "Trost"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du bleibst! und deiner harrt Gericht.", "tokens": ["Du", "bleibst", "!", "und", "dei\u00b7ner", "harrt", "Ge\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PPOSAT", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vernichtung kennt der Gott des Lebens,", "tokens": ["Ver\u00b7nich\u00b7tung", "kennt", "der", "Gott", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gott der Liebe nicht.", "tokens": ["Der", "Gott", "der", "Lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Einst wird sein Odem Todte neu beleben;", "tokens": ["Einst", "wird", "sein", "O\u00b7dem", "Tod\u00b7te", "neu", "be\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch du wirst dich im Dranggew\u00fchl", "tokens": ["Auch", "du", "wirst", "dich", "im", "Drang\u00b7ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Auferstandenen erheben", "tokens": ["Der", "Auf\u00b7er\u00b7stan\u00b7de\u00b7nen", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit h\u00f6herm Seyngef\u00fchl.", "tokens": ["Mit", "h\u00f6\u00b7herm", "Seyn\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dann wirst auch du, auch du den Hochgeschm\u00e4hten,", "tokens": ["Dann", "wirst", "auch", "du", ",", "auch", "du", "den", "Hoch\u00b7ge\u00b7schm\u00e4h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "$,", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Hochgepries'nen richten sehn,", "tokens": ["Den", "Hoch\u00b7ge\u00b7pries'\u00b7nen", "rich\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor dem der Erde Majest\u00e4ten", "tokens": ["Vor", "dem", "der", "Er\u00b7de", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entthront und schweigend stehn.", "tokens": ["Ent\u00b7thront", "und", "schwei\u00b7gend", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Auch du wirst sehn Entwicklung der Geschichten,", "tokens": ["Auch", "du", "wirst", "sehn", "Ent\u00b7wick\u00b7lung", "der", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "CARD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirst's h\u00f6ren, wenn Messias spricht:", "tokens": ["Wir\u00b7st's", "h\u00f6\u00b7ren", ",", "wenn", "Mes\u00b7si\u00b7as", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "KOUS", "NE", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihr Menschen, eure ", "tokens": ["Ihr", "Men\u00b7schen", ",", "eu\u00b7re"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ich aber richte nicht.", "tokens": ["Ich", "a\u00b7ber", "rich\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du siehst es mit, wenn Todesengel winken,", "tokens": ["Du", "siehst", "es", "mit", ",", "wenn", "To\u00b7de\u00b7sen\u00b7gel", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dann die Frevler tief hinab", "tokens": ["Und", "dann", "die", "Frev\u00b7ler", "tief", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch ihre eigne Schwere ", "tokens": ["Durch", "ih\u00b7re", "eig\u00b7ne", "Schwe\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins gluthgef\u00fcllte Grab.", "tokens": ["Ins", "gluth\u00b7ge\u00b7f\u00fcll\u00b7te", "Grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Auch wirst du Christus Wonnelispel h\u00f6ren:", "tokens": ["Auch", "wirst", "du", "Chris\u00b7tus", "Won\u00b7ne\u00b7lis\u00b7pel", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(o tr\u00e4f' sein gro\u00dfes Lob auch dich!)", "tokens": ["(", "o", "tr\u00e4f'", "sein", "gro\u00b7\u00dfes", "Lob", "auch", "dich", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbkommt her, o ihr der Menschheit Ehren,", "tokens": ["\u00bb", "kommt", "her", ",", "o", "ihr", "der", "Menschheit", "Eh\u00b7ren", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "FM", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und setzt euch neben mich.\u00ab", "tokens": ["Und", "setzt", "euch", "ne\u00b7ben", "mich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dann steht die Uhr von kleinen Erdetagen,", "tokens": ["Dann", "steht", "die", "Uhr", "von", "klei\u00b7nen", "Er\u00b7de\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die kurze Zeit ist ausgereift;", "tokens": ["Die", "kur\u00b7ze", "Zeit", "ist", "aus\u00b7ge\u00b7reift", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eine Uhr f\u00e4ngt an zu schlagen,", "tokens": ["Und", "ei\u00b7ne", "Uhr", "f\u00e4ngt", "an", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ohne Ende l\u00e4uft.", "tokens": ["Die", "oh\u00b7ne", "En\u00b7de", "l\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Drum hebe dich, o Mensch, vom Erdenthale,", "tokens": ["Drum", "he\u00b7be", "dich", ",", "o", "Mensch", ",", "vom", "Er\u00b7dent\u00b7ha\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschm\u00e4h den Tand der Eitelkeit,", "tokens": ["Ver\u00b7schm\u00e4h", "den", "Tand", "der", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sonne dich allein im Strahle", "tokens": ["Und", "son\u00b7ne", "dich", "al\u00b7lein", "im", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nahen Ewigkeit.", "tokens": ["Der", "na\u00b7hen", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}