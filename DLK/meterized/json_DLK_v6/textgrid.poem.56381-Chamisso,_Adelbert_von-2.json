{"textgrid.poem.56381": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und es war sein Wort,", "tokens": ["Und", "es", "war", "sein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Welches zuerst er sprach:", "tokens": ["Wel\u00b7ches", "zu\u00b7erst", "er", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u00bbh\u00f6re nun, Loki,", "tokens": ["\u00bb", "h\u00f6\u00b7re", "nun", ",", "Lo\u00b7ki", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "H\u00f6r, was ich sage,", "tokens": ["H\u00f6r", ",", "was", "ich", "sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Was weder auf Erden", "tokens": ["Was", "we\u00b7der", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "KON", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wei\u00df irgend einer,", "tokens": ["Wei\u00df", "ir\u00b7gend", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Noch hoch im Himmel:", "tokens": ["Noch", "hoch", "im", "Him\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Mein Hammer ist geraubt.\u00ab", "tokens": ["Mein", "Ham\u00b7mer", "ist", "ge\u00b7raubt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}