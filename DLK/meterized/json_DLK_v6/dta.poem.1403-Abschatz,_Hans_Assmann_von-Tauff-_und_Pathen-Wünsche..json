{"dta.poem.1403": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "R\u00fchmt heisser Brunnen Nutz und saurer Quellen Krafft/", "tokens": ["R\u00fchmt", "heis\u00b7ser", "Brun\u00b7nen", "Nutz", "und", "sau\u00b7rer", "Quel\u00b7len", "Krafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "NN", "KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sie verlohrne St\u00e4rck und neues Leben bringen/", "tokens": ["Wie", "sie", "ver\u00b7lohr\u00b7ne", "St\u00e4rck", "und", "neu\u00b7es", "Le\u00b7ben", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich seh ein edler Bad in Christi Kirch entspringen/", "tokens": ["Ich", "seh", "ein", "ed\u00b7ler", "Bad", "in", "Chris\u00b7ti", "Kirch", "ent\u00b7sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE", "APPR", "NE", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Geist und Wort geweyht: das hat die Eigenschafft/", "tokens": ["Durch", "Geist", "und", "Wort", "ge\u00b7weyht", ":", "das", "hat", "die", "Ei\u00b7gen\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$.", "PDS", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die angebohrne Schuld zu machen hell und rein.", "tokens": ["Die", "an\u00b7ge\u00b7bohr\u00b7ne", "Schuld", "zu", "ma\u00b7chen", "hell", "und", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Di\u00df/ Pathe/ soll dir auch ein sicher Denckmahl geben/", "tokens": ["Di\u00df", "/", "Pa\u00b7the", "/", "soll", "dir", "auch", "ein", "si\u00b7cher", "Denck\u00b7mahl", "ge\u00b7ben", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "NE", "$(", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df du in GOttes Huld wirst unver\u00e4ndert leben/", "tokens": ["Da\u00df", "du", "in", "Got\u00b7tes", "Huld", "wirst", "un\u00b7ver\u00b7\u00e4n\u00b7dert", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "NN", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Wohlfarth/ Gl\u00fcck und Heyl dein Erbtheil werden seyn.", "tokens": ["Und", "Wohl\u00b7farth", "/", "Gl\u00fcck", "und", "Heyl", "dein", "E\u00b7rbtheil", "wer\u00b7den", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "NN", "KON", "NN", "PPOSAT", "NN", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}