{"textgrid.poem.33705": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wieder schritt ich zu der St\u00e4tte", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder schritt ich zu der St\u00e4tte", "tokens": ["Wie\u00b7der", "schritt", "ich", "zu", "der", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alter Liebe heut zur\u00fcck,", "tokens": ["Al\u00b7ter", "Lie\u00b7be", "heut", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, als ob das Haus noch h\u00e4tte,", "tokens": ["Ach", ",", "als", "ob", "das", "Haus", "noch", "h\u00e4t\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOKOM", "KOUS", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Was es einst umschlo\u00df \u2013 mein Gl\u00fcck.", "tokens": ["Was", "es", "einst", "um\u00b7schlo\u00df", "\u2013", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Keine Spur blieb jener Tage \u2013", "tokens": ["Kei\u00b7ne", "Spur", "blieb", "je\u00b7ner", "Ta\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und was hat mich hervermocht,", "tokens": ["Und", "was", "hat", "mich", "her\u00b7ver\u00b7mocht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo mit tiefbewegtem Schlage", "tokens": ["Wo", "mit", "tief\u00b7be\u00b7weg\u00b7tem", "Schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam dieses Herz nur pocht?", "tokens": ["Ein\u00b7sam", "die\u00b7ses", "Herz", "nur", "pocht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}