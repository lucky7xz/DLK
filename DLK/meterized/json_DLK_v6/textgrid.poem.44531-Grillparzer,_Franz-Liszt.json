{"textgrid.poem.44531": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Liszt", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gleichst dem Engel mit dem Flammenschwerte,", "tokens": ["Du", "gleichst", "dem", "En\u00b7gel", "mit", "dem", "Flam\u00b7men\u00b7schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der aufgestellt vor unsrer Unschuld Garten;", "tokens": ["Der", "auf\u00b7ge\u00b7stellt", "vor", "uns\u00b7rer", "Un\u00b7schuld", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein strenger Spruch, gerecht in seiner H\u00e4rte,", "tokens": ["Ein", "stren\u00b7ger", "Spruch", ",", "ge\u00b7recht", "in", "sei\u00b7ner", "H\u00e4r\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Straft durch sich selber jegliches Entarten.", "tokens": ["Straft", "durch", "sich", "sel\u00b7ber", "jeg\u00b7li\u00b7ches", "Ent\u00b7ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.2": {"line.1": {"text": "Doch weigerst du die Pforten jener R\u00e4ume,", "tokens": ["Doch", "wei\u00b7gerst", "du", "die", "Pfor\u00b7ten", "je\u00b7ner", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Unschuld mit sich selber ging zufrieden,", "tokens": ["Wo", "Un\u00b7schuld", "mit", "sich", "sel\u00b7ber", "ging", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PRF", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So zeigst du uns, ein Traumbild wacher Tr\u00e4ume,", "tokens": ["So", "zeigst", "du", "uns", ",", "ein", "Traum\u00b7bild", "wa\u00b7cher", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Bild des Gl\u00fccks, das nicht mehr weilt hienieden.", "tokens": ["Das", "Bild", "des", "Gl\u00fccks", ",", "das", "nicht", "mehr", "weilt", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Eintauchend in die Welt der Leidenschaften,", "tokens": ["Ein\u00b7tau\u00b7chend", "in", "die", "Welt", "der", "Lei\u00b7den\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Kampfs, des Streits, der wildverworrnen Grenzen,", "tokens": ["Des", "Kampfs", ",", "des", "Streits", ",", "der", "wild\u00b7ver\u00b7worr\u00b7nen", "Gren\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4\u00dft du aus Augen, die an Eden haften,", "tokens": ["L\u00e4\u00dft", "du", "aus", "Au\u00b7gen", ",", "die", "an", "E\u00b7den", "haf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Widerschein des dort Gesehnen gl\u00e4nzen.", "tokens": ["Den", "Wi\u00b7der\u00b7schein", "des", "dort", "Ge\u00b7seh\u00b7nen", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Donner wird zum Strahl, der Strahl zum Lichte,", "tokens": ["Der", "Don\u00b7ner", "wird", "zum", "Strahl", ",", "der", "Strahl", "zum", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Augenblicke schwinden Nebeld\u00fcnste,", "tokens": ["Auf", "Au\u00b7gen\u00b7bli\u00b7cke", "schwin\u00b7den", "Ne\u00b7bel\u00b7d\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Luft der Heimat weht durch die Gesichte \u2013", "tokens": ["Die", "Luft", "der", "Hei\u00b7mat", "weht", "durch", "die", "Ge\u00b7sich\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eintracht in Zwietracht ist das Reich der K\u00fcnste.", "tokens": ["Ein\u00b7tracht", "in", "Zwiet\u00b7racht", "ist", "das", "Reich", "der", "K\u00fcns\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}