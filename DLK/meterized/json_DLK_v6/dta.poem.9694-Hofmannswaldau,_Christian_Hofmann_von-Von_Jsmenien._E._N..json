{"dta.poem.9694": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von Jsmenien.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wem gleicht sich doch Jsmeniens gestalt?", "tokens": ["Wem", "gleicht", "sich", "doch", "Js\u00b7me\u00b7ni\u00b7ens", "ge\u00b7stalt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "NE", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ist wol die brust ein alabaster-stein?", "tokens": ["Ist", "wol", "die", "brust", "ein", "a\u00b7la\u00b7bas\u00b7ter\u00b7stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So w\u00e4re sie auch kalt.", "tokens": ["So", "w\u00e4\u00b7re", "sie", "auch", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch ist der mund ein gl\u00e4ntzender rubin/", "tokens": ["Doch", "ist", "der", "mund", "ein", "gl\u00e4nt\u00b7zen\u00b7der", "ru\u00b7bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So m\u00fcst er harte seyn.", "tokens": ["So", "m\u00fcst", "er", "har\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Soll purpur wol die vollen wangen mahlen?", "tokens": ["Soll", "pur\u00b7pur", "wol", "die", "vol\u00b7len", "wan\u00b7gen", "mah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auch dieses trift nicht ein/", "tokens": ["Auch", "die\u00b7ses", "trift", "nicht", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sie w\u00fcrden bleicher seyn.", "tokens": ["Sie", "w\u00fcr\u00b7den", "blei\u00b7cher", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Sind ihrer augen strahlen", "tokens": ["Sind", "ih\u00b7rer", "au\u00b7gen", "strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ein helles sonnen-licht?", "tokens": ["Ein", "hel\u00b7les", "son\u00b7nen\u00b7licht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Auch di\u00df ist nicht zu gl\u00e4uben/", "tokens": ["Auch", "di\u00df", "ist", "nicht", "zu", "gl\u00e4u\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "So k\u00f6nte man vor ihren strahlen bleiben.", "tokens": ["So", "k\u00f6n\u00b7te", "man", "vor", "ih\u00b7ren", "strah\u00b7len", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Was ist sie denn? Ich wei\u00df es selber nicht/", "tokens": ["Was", "ist", "sie", "denn", "?", "Ich", "wei\u00df", "es", "sel\u00b7ber", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wie man sie soll benennen/", "tokens": ["Wie", "man", "sie", "soll", "be\u00b7nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ach solch ein wunderbild ist schwerlich zu erkennen.", "tokens": ["Ach", "solch", "ein", "wun\u00b7der\u00b7bild", "ist", "schwer\u00b7lich", "zu", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "ART", "ADJD", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}