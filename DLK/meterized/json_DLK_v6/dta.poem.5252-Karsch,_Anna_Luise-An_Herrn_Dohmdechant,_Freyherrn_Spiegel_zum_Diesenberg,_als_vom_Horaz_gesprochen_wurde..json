{"dta.poem.5252": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Dohmdechant, Freyherrn  \n Spiegel zum Diesenberg, als vom  \n Horaz gesprochen wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Horaz verband Natur und Kunst,", "tokens": ["Ho\u00b7raz", "ver\u00b7band", "Na\u00b7tur", "und", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und sang ein Lied voll Dichterhitze,", "tokens": ["Und", "sang", "ein", "Lied", "voll", "Dich\u00b7ter\u00b7hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gieng nicht, bey vieler Sch\u00fcsseln Dunst,", "tokens": ["Gieng", "nicht", ",", "bey", "vie\u00b7ler", "Sch\u00fcs\u00b7seln", "Dunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu C\u00e4sars Tafelsitze,", "tokens": ["Zu", "C\u00e4\u00b7sars", "Ta\u00b7fel\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schlo\u00df auf der Meyerey geruhig seinen Lauf.", "tokens": ["Schlo\u00df", "auf", "der", "Me\u00b7ye\u00b7rey", "ge\u00b7ru\u00b7hig", "sei\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du hast der F\u00fcrsten Gunst und bist nicht stolz darauf!", "tokens": ["Du", "hast", "der", "F\u00fcrs\u00b7ten", "Gunst", "und", "bist", "nicht", "stolz", "da\u00b7rauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "VAFIN", "PTKNEG", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}