{"textgrid.poem.42510": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei sch\u00f6ne Augen sah ich gestern,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei sch\u00f6ne Augen sah ich gestern,", "tokens": ["Zwei", "sch\u00f6\u00b7ne", "Au\u00b7gen", "sah", "ich", "ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da war die Liebe drin und auch das Leid.", "tokens": ["Da", "war", "die", "Lie\u00b7be", "drin", "und", "auch", "das", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe und das Leid sind Schwestern,", "tokens": ["Die", "Lie\u00b7be", "und", "das", "Leid", "sind", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es trennt sie keine Ewigkeit.", "tokens": ["Es", "trennt", "sie", "kei\u00b7ne", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}