{"textgrid.poem.65332": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den Mond", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleicher Mond, geh' nicht zu Bette, geh' f\u00fcr mich erst einen Gang,", "tokens": ["Blei\u00b7cher", "Mond", ",", "geh'", "nicht", "zu", "Bet\u00b7te", ",", "geh'", "f\u00fcr", "mich", "erst", "ei\u00b7nen", "Gang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Geh' zu meinem Ungetreuen, sag' ihm, da\u00df ich todeskrank.", "tokens": ["Geh'", "zu", "mei\u00b7nem", "Un\u00b7ge\u00b7treu\u00b7en", ",", "sag'", "ihm", ",", "da\u00df", "ich", "to\u00b7des\u00b7krank", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Gestern hat er mir geschworen, mein zu sein vor aller Welt,", "tokens": ["Ge\u00b7stern", "hat", "er", "mir", "ge\u00b7schwo\u00b7ren", ",", "mein", "zu", "sein", "vor", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "VVPP", "$,", "PPOSAT", "PTKZU", "VAINF", "APPR", "PIAT", "NN", "$,"], "meter": "-++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Heute hat er mich verlassen, wie ein abgem\u00e4htes Feld,", "tokens": ["Heu\u00b7te", "hat", "er", "mich", "ver\u00b7las\u00b7sen", ",", "wie", "ein", "ab\u00b7ge\u00b7m\u00e4h\u00b7tes", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wie ein Kirchlein, das der Priester hat mit einem Bann belegt,", "tokens": ["Wie", "ein", "Kirch\u00b7lein", ",", "das", "der", "Pries\u00b7ter", "hat", "mit", "ei\u00b7nem", "Bann", "be\u00b7legt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wie ein St\u00e4dtlein, das der Pascha hat mit Eisen ausgefegt.", "tokens": ["Wie", "ein", "St\u00e4dt\u00b7lein", ",", "das", "der", "Pa\u00b7scha", "hat", "mit", "Ei\u00b7sen", "aus\u00b7ge\u00b7fegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Und so w\u00fcnsch' ihm denn, dem Argen, w\u00fcnsch' ihm Arges dies und das,", "tokens": ["Und", "so", "w\u00fcn\u00b7sch'", "ihm", "denn", ",", "dem", "Ar\u00b7gen", ",", "w\u00fcn\u00b7sch'", "ihm", "Ar\u00b7ges", "dies", "und", "das", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "NN", "PDS", "KON", "PDS", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Da\u00df er schmelze, gleich dem Wachse, da\u00df er breche, wie ein Glas.", "tokens": ["Da\u00df", "er", "schmel\u00b7ze", ",", "gleich", "dem", "Wach\u00b7se", ",", "da\u00df", "er", "bre\u00b7che", ",", "wie", "ein", "Glas", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Durch der T\u00fcrken S\u00e4bel soll er in der Franken Dolche gehn \u2013", "tokens": ["Durch", "der", "T\u00fcr\u00b7ken", "S\u00e4\u00b7bel", "soll", "er", "in", "der", "Fran\u00b7ken", "Dol\u00b7che", "gehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "F\u00fcnf Chirurgen, ihn zu halten, ihn zu heilen, mehr als zehn!", "tokens": ["F\u00fcnf", "Chi\u00b7rur\u00b7gen", ",", "ihn", "zu", "hal\u00b7ten", ",", "ihn", "zu", "hei\u00b7len", ",", "mehr", "als", "zehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,", "PIAT", "KOKOM", "CARD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}