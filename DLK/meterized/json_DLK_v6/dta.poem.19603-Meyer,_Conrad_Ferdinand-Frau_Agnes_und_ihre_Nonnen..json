{"dta.poem.19603": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Frau Agnes und ihre Nonnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Klosterhof, ein Lenzestag!", "tokens": ["Ein", "Klos\u00b7ter\u00b7hof", ",", "ein", "Len\u00b7zes\u00b7tag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schwarzer Lindenschatten,", "tokens": ["Ein", "schwar\u00b7zer", "Lin\u00b7den\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo der gekr\u00f6nte Habsburg lag", "tokens": ["Wo", "der", "ge\u00b7kr\u00f6n\u00b7te", "Habs\u00b7burg", "lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erstochen auf den Matten.", "tokens": ["Er\u00b7sto\u00b7chen", "auf", "den", "Mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Frau Agnes, die gestrenge Frau,", "tokens": ["Frau", "Ag\u00b7nes", ",", "die", "ge\u00b7stren\u00b7ge", "Frau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Vaters Blut zu r\u00e4chen,", "tokens": ["Des", "Va\u00b7ters", "Blut", "zu", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Rief mordend aus: \u201eIch bad' in Thau!\u201c", "tokens": ["Rief", "mor\u00b7dend", "aus", ":", "\u201e", "Ich", "bad'", "in", "Thau", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schritt in rothen B\u00e4chen.", "tokens": ["Und", "schritt", "in", "ro\u00b7then", "B\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie freute sich in warmes Blut", "tokens": ["Sie", "freu\u00b7te", "sich", "in", "war\u00b7mes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kn\u00f6chel einzutauchen,", "tokens": ["Die", "Kn\u00f6\u00b7chel", "ein\u00b7zu\u00b7tau\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie warf in stille D\u00f6rfer Glut,", "tokens": ["Sie", "warf", "in", "stil\u00b7le", "D\u00f6r\u00b7fer", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie lie\u00df die Burgen rauchen.", "tokens": ["Sie", "lie\u00df", "die", "Bur\u00b7gen", "rau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nachdem Gericht gehalten war,", "tokens": ["Nach\u00b7dem", "Ge\u00b7richt", "ge\u00b7hal\u00b7ten", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vollbracht die Todtenfeier,", "tokens": ["Voll\u00b7bracht", "die", "Tod\u00b7ten\u00b7fei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verbarg sie das Medusenhaar", "tokens": ["Ver\u00b7barg", "sie", "das", "Me\u00b7du\u00b7sen\u00b7haar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem Nonnenschleier.", "tokens": ["Mit", "ei\u00b7nem", "Non\u00b7nen\u00b7schlei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie schuf ein Kloster, wo hervor", "tokens": ["Sie", "schuf", "ein", "Klos\u00b7ter", ",", "wo", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Gr\u00fcften Geister schweben,", "tokens": ["Aus", "Gr\u00fcf\u00b7ten", "Geis\u00b7ter", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie f\u00fcllt' mit Blumen an den Chor,", "tokens": ["Sie", "f\u00fcllt'", "mit", "Blu\u00b7men", "an", "den", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit lauter jungem Leben:", "tokens": ["Mit", "lau\u00b7ter", "jun\u00b7gem", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie raubt das krause Blondgelock", "tokens": ["Sie", "raubt", "das", "krau\u00b7se", "Blond\u00b7ge\u00b7lock"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch einem Edelkinde,", "tokens": ["Manch", "ei\u00b7nem", "E\u00b7del\u00b7kin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beschert ihm einen schwarzen Rock", "tokens": ["Be\u00b7schert", "ihm", "ei\u00b7nen", "schwar\u00b7zen", "Rock"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eine blanke Binde.", "tokens": ["Und", "ei\u00b7ne", "blan\u00b7ke", "Bin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie gei\u00dfelt sich den wei\u00dfen Leib,", "tokens": ["Sie", "gei\u00b7\u00dfelt", "sich", "den", "wei\u00b7\u00dfen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis rothe Tropfen rinnen,", "tokens": ["Bis", "ro\u00b7the", "Trop\u00b7fen", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie will, das unbarmherz'ge Weib,", "tokens": ["Sie", "will", ",", "das", "un\u00b7barm\u00b7her\u00b7z'\u00b7ge", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den zarten Heiland minnen.", "tokens": ["Den", "zar\u00b7ten", "Hei\u00b7land", "min\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dort sitzt sie unter Lindennacht", "tokens": ["Dort", "sitzt", "sie", "un\u00b7ter", "Lin\u00b7den\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am k\u00fchlen Klosterbronnen,", "tokens": ["Am", "k\u00fch\u00b7len", "Klos\u00b7ter\u00b7bron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat die Bibel mitgebracht", "tokens": ["Sie", "hat", "die", "Bi\u00b7bel", "mit\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Andacht ihrer Nonnen.", "tokens": ["Zur", "An\u00b7dacht", "ih\u00b7rer", "Non\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Am Gatter lauschen Kinder scheu", "tokens": ["Am", "Gat\u00b7ter", "lau\u00b7schen", "Kin\u00b7der", "scheu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit frischgepfl\u00fcckten Veilchen,", "tokens": ["Mit", "frischge\u00b7pfl\u00fcck\u00b7ten", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ein Weiblein hinkt mit Holz vorbei,", "tokens": ["Ein", "Weib\u00b7lein", "hinkt", "mit", "Holz", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "B\u00fcckt tief sich vor der Heil'gen.", "tokens": ["B\u00fcckt", "tief", "sich", "vor", "der", "Heil'\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dem j\u00fcngsten N\u00f6nnchen giebt das Buch", "tokens": ["Dem", "j\u00fcng\u00b7sten", "N\u00f6nn\u00b7chen", "giebt", "das", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie jetzt, der lieblich Bleichen:", "tokens": ["Sie", "jetzt", ",", "der", "lieb\u00b7lich", "Blei\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ewir blieben bei Sankt Pauli Spruch.", "tokens": ["\u201e", "wir", "blie\u00b7ben", "bei", "Sankt", "Pau\u00b7li", "Spruch", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieh her! Da steckt das Zeichen!\u201c", "tokens": ["Sieh", "her", "!", "Da", "steckt", "das", "Zei\u00b7chen", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die Zarte, die das Buch empfing,", "tokens": ["Die", "Zar\u00b7te", ",", "die", "das", "Buch", "emp\u00b7fing", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beschaut Sankt Paulum denkend.", "tokens": ["Be\u00b7schaut", "Sankt", "Pau\u00b7lum", "den\u00b7kend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie liest. Ihr lauscht der Schwestern Ring,", "tokens": ["Sie", "liest", ".", "Ihr", "lauscht", "der", "Schwes\u00b7tern", "Ring", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wimper z\u00fcchtig senkend \u2014", "tokens": ["Die", "Wim\u00b7per", "z\u00fcch\u00b7tig", "sen\u00b7kend"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u201ewas frommte mir die Fastenzeit,", "tokens": ["\u201e", "was", "fromm\u00b7te", "mir", "die", "Fas\u00b7ten\u00b7zeit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was frommten Gei\u00dfelhiebe,", "tokens": ["Was", "fromm\u00b7ten", "Gei\u00b7\u00df\u00b7el\u00b7hie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was frommt' es, tr\u00fcg' ich h\u00e4ren Kleid,", "tokens": ["Was", "frommt'", "es", ",", "tr\u00fcg'", "ich", "h\u00e4\u00b7ren", "Kleid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mangelte der Liebe?\u201c", "tokens": ["Und", "man\u00b7gel\u00b7te", "der", "Lie\u00b7be", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.13": {"line.1": {"text": "Da hebt ein Seufzer manche Brust", "tokens": ["Da", "hebt", "ein", "Seuf\u00b7zer", "man\u00b7che", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Nonnenrock erbaulich", "tokens": ["Im", "Non\u00b7nen\u00b7rock", "er\u00b7bau\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und manche kecke Lebenslust", "tokens": ["Und", "man\u00b7che", "ke\u00b7cke", "Le\u00b7bens\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blickt traurig und beschaulich ...", "tokens": ["Blickt", "trau\u00b7rig", "und", "be\u00b7schau\u00b7lich", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}