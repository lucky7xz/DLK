{"textgrid.poem.40237": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Deutung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ich nun in Einsamkeiten", "tokens": ["Da", "ich", "nun", "in", "Ein\u00b7sam\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "tr\u00e4ume von dem goldnen Land,", "tokens": ["tr\u00e4u\u00b7me", "von", "dem", "gold\u00b7nen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "von den fernen Seligkeiten", "tokens": ["von", "den", "fer\u00b7nen", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "unerf\u00fcllbar sch\u00f6ner Zeiten,", "tokens": ["un\u00b7er\u00b7f\u00fcll\u00b7bar", "sch\u00f6\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und der goldne Kreis der Weiten", "tokens": ["und", "der", "gold\u00b7ne", "Kreis", "der", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "weiter sich und weiter spannt, \u2013", "tokens": ["wei\u00b7ter", "sich", "und", "wei\u00b7ter", "spannt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PRF", "KON", "ADV", "VVFIN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "r\u00fchrt auf Einmal mich ein Bangen:", "tokens": ["r\u00fchrt", "auf", "Ein\u00b7mal", "mich", "ein", "Ban\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "welchem, welchem Ziele zu?", "tokens": ["wel\u00b7chem", ",", "wel\u00b7chem", "Zie\u00b7le", "zu", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "$,", "PWAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "tief und tiefer ein Verlangen:", "tokens": ["tief", "und", "tie\u00b7fer", "ein", "Ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}