{"textgrid.poem.49075": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erneue deinen Segen heut', o Herr, und steige", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erneue deinen Segen heut', o Herr, und steige", "tokens": ["Er\u00b7neu\u00b7e", "dei\u00b7nen", "Se\u00b7gen", "heut'", ",", "o", "Herr", ",", "und", "stei\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,", "FM", "NN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In diesem Augenblick im Geiste nieder", "tokens": ["In", "die\u00b7sem", "Au\u00b7gen\u00b7blick", "im", "Geis\u00b7te", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu uns als sel'ges Kind, damit geboren wieder", "tokens": ["Zu", "uns", "als", "sel'\u00b7ges", "Kind", ",", "da\u00b7mit", "ge\u00b7bo\u00b7ren", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "KOUS", "ADJA", "NN", "$,", "PAV", "VVPP", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu dir das Herz durch dich sich rein und kindlich neige!", "tokens": ["Zu", "dir", "das", "Herz", "durch", "dich", "sich", "rein", "und", "kind\u00b7lich", "nei\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "PPER", "PRF", "ADJD", "KON", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schenk' deinen Frieden voll der Welt! Es weiche", "tokens": ["Schenk'", "dei\u00b7nen", "Frie\u00b7den", "voll", "der", "Welt", "!", "Es", "wei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Zwietracht ganz, damit nur zarte Lieder", "tokens": ["Die", "Zwiet\u00b7racht", "ganz", ",", "da\u00b7mit", "nur", "zar\u00b7te", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der frohen Eintracht mit der Unschuld Lallen", "tokens": ["Der", "fro\u00b7hen", "Ein\u00b7tracht", "mit", "der", "Un\u00b7schuld", "Lal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von frommen Lippen m\u00f6gen inniglich erschallen!", "tokens": ["Von", "from\u00b7men", "Lip\u00b7pen", "m\u00f6\u00b7gen", "in\u00b7nig\u00b7lich", "er\u00b7schal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So weit der Ocean die Erd' umschlie\u00dft, erheben", "tokens": ["So", "weit", "der", "O\u00b7cean", "die", "Erd'", "um\u00b7schlie\u00dft", ",", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Voll Andacht Stimmen sich in dieser Stunde,", "tokens": ["Voll", "An\u00b7dacht", "Stim\u00b7men", "sich", "in", "die\u00b7ser", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "NN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu feiern, Herr, dein Fest. Aus jedem Christenmunde", "tokens": ["Zu", "fei\u00b7ern", ",", "Herr", ",", "dein", "Fest", ".", "Aus", "je\u00b7dem", "Chris\u00b7ten\u00b7mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "NN", "$,", "PPOSAT", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Strebt lauter Jubel auf, dich in dem Erdenleben", "tokens": ["Strebt", "lau\u00b7ter", "Ju\u00b7bel", "auf", ",", "dich", "in", "dem", "Er\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu gr\u00fc\u00dfen neu; doch darf dir, treu ergeben", "tokens": ["Zu", "gr\u00fc\u00b7\u00dfen", "neu", ";", "doch", "darf", "dir", ",", "treu", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "ADJD", "$.", "ADV", "VMFIN", "PPER", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vor jedem Welttheil, fest im Glaubensbunde,", "tokens": ["Vor", "je\u00b7dem", "Welt\u00b7theil", ",", "fest", "im", "Glau\u00b7bens\u00b7bun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dich, Heiland uns'rer Welt, Europa preisen", "tokens": ["Dich", ",", "Hei\u00b7land", "un\u00b7s'\u00b7rer", "Welt", ",", "Eu\u00b7ro\u00b7pa", "prei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "NN", "PPOSAT", "NN", "$,", "NE", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Als hohem Mittler dir den reinsten Dienst erweisen.", "tokens": ["Als", "ho\u00b7hem", "Mitt\u00b7ler", "dir", "den", "reins\u00b7ten", "Dienst", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum lenke deine Blicke, Herr, auch voller Gnade", "tokens": ["Drum", "len\u00b7ke", "dei\u00b7ne", "Bli\u00b7cke", ",", "Herr", ",", "auch", "vol\u00b7ler", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf ihre Reiche, und will's dir gefallen,", "tokens": ["Auf", "ih\u00b7re", "Rei\u00b7che", ",", "und", "will's", "dir", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "$,", "KON", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf unser deutsches Vaterland hierbei vor allen;", "tokens": ["Auf", "un\u00b7ser", "deut\u00b7sches", "Va\u00b7ter\u00b7land", "hier\u00b7bei", "vor", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verbinde du den Sinn, nach deinem heil'gen Rathe,", "tokens": ["Ver\u00b7bin\u00b7de", "du", "den", "Sinn", ",", "nach", "dei\u00b7nem", "heil'\u00b7gen", "Ra\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von seinen S\u00f6hnen, da\u00df im rechten Pfade", "tokens": ["Von", "sei\u00b7nen", "S\u00f6h\u00b7nen", ",", "da\u00df", "im", "rech\u00b7ten", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Einigkeit sie sicher wallen,", "tokens": ["Der", "Ei\u00b7nig\u00b7keit", "sie", "si\u00b7cher", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als Br\u00fcder, jeder treu den andern achtend,", "tokens": ["Als", "Br\u00fc\u00b7der", ",", "je\u00b7der", "treu", "den", "an\u00b7dern", "ach\u00b7tend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PIS", "ADJD", "ART", "ADJA", "CARD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als Glied von einem Leibe jeder sich betrachtend!", "tokens": ["Als", "Glied", "von", "ei\u00b7nem", "Lei\u00b7be", "je\u00b7der", "sich", "be\u00b7trach\u00b7tend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "PIAT", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Damit der Feinde List (zu uns'rer Schmach und Schande", "tokens": ["Da\u00b7mit", "der", "Fein\u00b7de", "List", "(", "zu", "un\u00b7s'\u00b7rer", "Schmach", "und", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NE", "$(", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gelang's zu oft ihr) m\u00f6ge nicht ber\u00fccken", "tokens": ["Ge\u00b7lang's", "zu", "oft", "ihr", ")", "m\u00f6\u00b7ge", "nicht", "be\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "ADV", "PPOSAT", "$(", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den deutschen Mut durch Zwietracht, leichter so zu dr\u00fccken", "tokens": ["Den", "deut\u00b7schen", "Mut", "durch", "Zwiet\u00b7racht", ",", "leich\u00b7ter", "so", "zu", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Joch auf unsern Nacken, schwere Sclavenbande", "tokens": ["Das", "Joch", "auf", "un\u00b7sern", "Na\u00b7cken", ",", "schwe\u00b7re", "Scla\u00b7ven\u00b7ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "An unsern Arm; im eignen Vaterlande", "tokens": ["An", "un\u00b7sern", "Arm", ";", "im", "eig\u00b7nen", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Unth\u00e4tig fesselnd ihn uns auf den R\u00fccken;", "tokens": ["Un\u00b7th\u00e4\u00b7tig", "fes\u00b7selnd", "ihn", "uns", "auf", "den", "R\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Noch da\u00df zu fremdem Vortheil Deutsche tragen", "tokens": ["Noch", "da\u00df", "zu", "frem\u00b7dem", "Vor\u00b7theil", "Deut\u00b7sche", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "ADJA", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Waffen gegen Br\u00fcder, f\u00fcr den Feind sich schlagen.", "tokens": ["Die", "Waf\u00b7fen", "ge\u00b7gen", "Br\u00fc\u00b7der", ",", "f\u00fcr", "den", "Feind", "sich", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Erhelle ihren Geist, damit sie klarer schauen,", "tokens": ["Er\u00b7hel\u00b7le", "ih\u00b7ren", "Geist", ",", "da\u00b7mit", "sie", "kla\u00b7rer", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Einigkeit und Brudersinn vor Ketten", "tokens": ["Da\u00df", "Ei\u00b7nig\u00b7keit", "und", "Bru\u00b7der\u00b7sinn", "vor", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der fremden Tirannei nur sie verm\u00f6g' zu retten;", "tokens": ["Der", "frem\u00b7den", "Ti\u00b7ran\u00b7nei", "nur", "sie", "ver\u00b7m\u00f6g'", "zu", "ret\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn treu und br\u00fcderlich sie aufeinander bauen,", "tokens": ["Wenn", "treu", "und", "br\u00fc\u00b7der\u00b7lich", "sie", "auf\u00b7ein\u00b7an\u00b7der", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Hand sich reichen, fest in N\u00f6ten trauen", "tokens": ["Die", "Hand", "sich", "rei\u00b7chen", ",", "fest", "in", "N\u00f6\u00b7ten", "trau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF", "$,", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem eig'nen Mut; im Schlachtfeld lieber betten", "tokens": ["Dem", "eig'\u00b7nen", "Mut", ";", "im", "Schlacht\u00b7feld", "lie\u00b7ber", "bet\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPRART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf blut'ger Erde sich, als feig zu sparen", "tokens": ["Auf", "blut'\u00b7ger", "Er\u00b7de", "sich", ",", "als", "feig", "zu", "spa\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PRF", "$,", "KOUS", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Leben ehrlos, um als Sclav es zu bewahren!", "tokens": ["Das", "Le\u00b7ben", "ehr\u00b7los", ",", "um", "als", "Sclav", "es", "zu", "be\u00b7wah\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KOUI", "KOKOM", "NE", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Lenk' unsrer F\u00fcrsten Sinn, damit sie treu die Pflichten,", "tokens": ["Lenk'", "uns\u00b7rer", "F\u00fcrs\u00b7ten", "Sinn", ",", "da\u00b7mit", "sie", "treu", "die", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$,", "KOUS", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die liegen ihnen ob, als V\u00e4ter \u00fcben", "tokens": ["Die", "lie\u00b7gen", "ih\u00b7nen", "ob", ",", "als", "V\u00e4\u00b7ter", "\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "KOUS", "$,", "KOUS", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das treue Volk durch keinen Uebermut betr\u00fcben,", "tokens": ["Das", "treu\u00b7e", "Volk", "durch", "kei\u00b7nen", "Ue\u00b7ber\u00b7mut", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und aufschau'n, da\u00df getreu auch ihren Dienst verrichten", "tokens": ["Und", "auf\u00b7schau'n", ",", "da\u00df", "ge\u00b7treu", "auch", "ih\u00b7ren", "Dienst", "ver\u00b7rich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "KOUS", "ADJD", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die, welchen sie das Amt, das Volk zu richten,", "tokens": ["Die", ",", "wel\u00b7chen", "sie", "das", "Amt", ",", "das", "Volk", "zu", "rich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAT", "PPER", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vertrauet an, wie im Gesetz geschrieben;", "tokens": ["Ver\u00b7trau\u00b7et", "an", ",", "wie", "im", "Ge\u00b7setz", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Damit sie so ein r\u00fchmlich Beispiel geben,", "tokens": ["Da\u00b7mit", "sie", "so", "ein", "r\u00fchm\u00b7lich", "Bei\u00b7spiel", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In sich dem Volke, wie gerecht der Mensch soll leben.", "tokens": ["In", "sich", "dem", "Vol\u00b7ke", ",", "wie", "ge\u00b7recht", "der", "Mensch", "soll", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zuletzt gieb uns, o Herr, da\u00df unser Volk erkenne", "tokens": ["Zu\u00b7letzt", "gieb", "uns", ",", "o", "Herr", ",", "da\u00df", "un\u00b7ser", "Volk", "er\u00b7ken\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "$,", "FM", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lebendig seinen Wert, und sich nicht sch\u00e4tzen", "tokens": ["Le\u00b7ben\u00b7dig", "sei\u00b7nen", "Wert", ",", "und", "sich", "nicht", "sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "KON", "PRF", "PTKNEG", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nach andrer Anschlag soll; als Sclaven fremder G\u00f6tzen", "tokens": ["Nach", "an\u00b7drer", "An\u00b7schlag", "soll", ";", "als", "Scla\u00b7ven", "frem\u00b7der", "G\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "$.", "KOUS", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verst\u00fcmm'le seine Zung'! Hinweg! das Glied, es trenne", "tokens": ["Ver\u00b7st\u00fcm\u00b7m'\u00b7le", "sei\u00b7ne", "Zung'", "!", "Hin\u00b7weg", "!", "das", "Glied", ",", "es", "tren\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ADV", "$.", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Von uns sich, da\u00df mein Lied es hier nicht nenne,", "tokens": ["Von", "uns", "sich", ",", "da\u00df", "mein", "Lied", "es", "hier", "nicht", "nen\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "$,", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-++--", "measure": "unknown.measure.penta"}, "line.6": {"text": "Gilt mehr als M\u00e4nnerwort ihm fremd Geschw\u00e4tze!", "tokens": ["Gilt", "mehr", "als", "M\u00e4n\u00b7ner\u00b7wort", "ihm", "fremd", "Ge\u00b7schw\u00e4t\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "NN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ach, la\u00df uns achten stets das Sch\u00f6ne und das Rechte", "tokens": ["Ach", ",", "la\u00df", "uns", "ach\u00b7ten", "stets", "das", "Sch\u00f6\u00b7ne", "und", "das", "Rech\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "VVFIN", "ADV", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An fremden V\u00f6lkern, \u2013 doch als Freie, nicht als Knechte!", "tokens": ["An", "frem\u00b7den", "V\u00f6l\u00b7kern", ",", "\u2013", "doch", "als", "Frei\u00b7e", ",", "nicht", "als", "Knech\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "$(", "ADV", "KOUS", "NN", "$,", "PTKNEG", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}