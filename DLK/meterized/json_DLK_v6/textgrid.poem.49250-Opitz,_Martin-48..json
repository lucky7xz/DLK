{"textgrid.poem.49250": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Legt uns die Liebe gleich viel Widerwillen an,", "tokens": ["Legt", "uns", "die", "Lie\u00b7be", "gleich", "viel", "Wi\u00b7der\u00b7wil\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So freuet man sich doch, wann man ihr' Huld erwirbet;", "tokens": ["So", "freu\u00b7et", "man", "sich", "doch", ",", "wann", "man", "ih\u00b7r'", "Huld", "er\u00b7wir\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "ADV", "$,", "PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ohn' ihre Werke doch hier nichts bestehen kan;", "tokens": ["Ohn'", "ih\u00b7re", "Wer\u00b7ke", "doch", "hier", "nichts", "be\u00b7ste\u00b7hen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer nicht verderbet wird durch Liebe, der verdirbet.", "tokens": ["Wer", "nicht", "ver\u00b7der\u00b7bet", "wird", "durch", "Lie\u00b7be", ",", "der", "ver\u00b7dir\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "VAFIN", "APPR", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}