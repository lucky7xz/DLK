{"textgrid.poem.56903": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus der Gesellschaft L\u00e4rm und Lachen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus der Gesellschaft L\u00e4rm und Lachen", "tokens": ["Aus", "der", "Ge\u00b7sell\u00b7schaft", "L\u00e4rm", "und", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "hebt schwerm\u00fctigen Fl\u00fcgelschlags", "tokens": ["hebt", "schwer\u00b7m\u00fc\u00b7ti\u00b7gen", "Fl\u00fc\u00b7gel\u00b7schlags"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "meine einsame Seele sich", "tokens": ["mei\u00b7ne", "ein\u00b7sa\u00b7me", "See\u00b7le", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PRF"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "fernen schweigenden H\u00f6hen zu,", "tokens": ["fer\u00b7nen", "schwei\u00b7gen\u00b7den", "H\u00f6\u00b7hen", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "wo der Nachtwind klagend", "tokens": ["wo", "der", "Nacht\u00b7wind", "kla\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "in m\u00e4chtigen B\u00e4umen harft,", "tokens": ["in", "m\u00e4ch\u00b7ti\u00b7gen", "B\u00e4u\u00b7men", "harft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "und in den langen Schatten", "tokens": ["und", "in", "den", "lan\u00b7gen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "des k\u00fchlen Mondes", "tokens": ["des", "k\u00fch\u00b7len", "Mon\u00b7des"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "meine Tr\u00e4ume und W\u00fcnsche", "tokens": ["mei\u00b7ne", "Tr\u00e4u\u00b7me", "und", "W\u00fcn\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "sorgenvoll wandeln ...", "tokens": ["sor\u00b7gen\u00b7voll", "wan\u00b7deln", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ach, die ihr hier scherzt und lacht", "tokens": ["Ach", ",", "die", "ihr", "hier", "scherzt", "und", "lacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PRELS", "PPER", "ADV", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "und mit leeren T\u00f6nen", "tokens": ["und", "mit", "lee\u00b7ren", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "der Tag und N\u00e4chte", "tokens": ["der", "Tag", "und", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "kostbare Luft erf\u00fcllt \u2013", "tokens": ["kost\u00b7ba\u00b7re", "Luft", "er\u00b7f\u00fcllt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "was hab ich mit euch \u2013", "tokens": ["was", "hab", "ich", "mit", "euch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPER", "$("], "meter": "+---+", "measure": "dactylic.init"}, "line.16": {"text": "was hab ich mit euch", "tokens": ["was", "hab", "ich", "mit", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPER"], "meter": "+---+", "measure": "dactylic.init"}, "line.17": {"text": "zu schaffen!", "tokens": ["zu", "schaf\u00b7fen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}