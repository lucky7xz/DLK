{"dta.poem.11476": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als Hasaus mit dem katzen-fresser  \n h\u00e4ndel hatte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Has\u00e4us, der sonst viel verdaun und weichen kan,", "tokens": ["Ha\u00b7s\u00e4us", ",", "der", "sonst", "viel", "ver\u00b7daun", "und", "wei\u00b7chen", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ADV", "VVINF", "KON", "VVINF", "VMFIN", "$,"], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der nahm es unl\u00e4ngst doch mit einem vielfra\u00df an,", "tokens": ["Der", "nahm", "es", "un\u00b7l\u00e4ngst", "doch", "mit", "ei\u00b7nem", "viel\u00b7fra\u00df", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er spreuste sich und schrie: Der kerl wird mich nicht fressen;", "tokens": ["Er", "spreus\u00b7te", "sich", "und", "schrie", ":", "Der", "kerl", "wird", "mich", "nicht", "fres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein wer katzen fri\u00dft, kan auch wohl hasen essen.", "tokens": ["Al\u00b7lein", "wer", "kat\u00b7zen", "fri\u00dft", ",", "kan", "auch", "wohl", "ha\u00b7sen", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVINF", "VVFIN", "$,", "VMFIN", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}