{"dta.poem.11909": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel schlimmer, als wenn dich die andern hintergehn,", "tokens": ["Viel", "schlim\u00b7mer", ",", "als", "wenn", "dich", "die", "an\u00b7dern", "hin\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist dieses, von dir selbst dich hintergangen sehn.", "tokens": ["Ist", "die\u00b7ses", ",", "von", "dir", "selbst", "dich", "hin\u00b7ter\u00b7gan\u00b7gen", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "$,", "APPR", "PPER", "ADV", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gef\u00e4hrlich ist vom Feind des Hinterhaltes Lauer", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "ist", "vom", "Feind", "des", "Hin\u00b7ter\u00b7hal\u00b7tes", "Lau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Feld, gef\u00e4hrlicher in deines Hauses Mauer.", "tokens": ["Im", "Feld", ",", "ge\u00b7f\u00e4hr\u00b7li\u00b7cher", "in", "dei\u00b7nes", "Hau\u00b7ses", "Mau\u00b7er", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die \u00e4u\u00dfre Hinterlist kannst du noch hintertreiben;", "tokens": ["Die", "\u00e4u\u00df\u00b7re", "Hin\u00b7ter\u00b7list", "kannst", "du", "noch", "hin\u00b7ter\u00b7trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die hinter'm Herzen ist, die wird dahinter bleiben.", "tokens": ["Die", "hin\u00b7ter'm", "Her\u00b7zen", "ist", ",", "die", "wird", "da\u00b7hin\u00b7ter", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VAFIN", "$,", "PRELS", "VAFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}