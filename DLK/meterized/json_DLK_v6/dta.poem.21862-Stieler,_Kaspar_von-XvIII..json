{"dta.poem.21862": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Hastu/ Sch\u00f6ne/ niemals nicht gelesen/", "tokens": ["Has\u00b7tu", "/", "Sch\u00f6\u00b7ne", "/", "nie\u00b7mals", "nicht", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "ADV", "PTKNEG", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "da\u00df die Venus nakkend sey gewesen/", "tokens": ["da\u00df", "die", "Ve\u00b7nus", "nak\u00b7kend", "sey", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "VAPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "als sie den Gradiv entwehret macht.", "tokens": ["als", "sie", "den", "Gra\u00b7div", "ent\u00b7weh\u00b7ret", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Warum greiffstu/ Kind/ denn nach der Weh-", "tokens": ["Wa\u00b7rum", "greiffs\u00b7tu", "/", "Kind", "/", "denn", "nach", "der", "Weh"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$(", "NN", "$(", "KON", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Weine. Ja h\u00e4ttstu es vor bedacht!", "tokens": ["Wei\u00b7ne", ".", "Ja", "h\u00e4tts\u00b7tu", "es", "vor", "be\u00b7dacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKANT", "VAFIN", "PPER", "APPR", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}