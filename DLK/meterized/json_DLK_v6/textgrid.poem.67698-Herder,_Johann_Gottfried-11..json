{"textgrid.poem.67698": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst Du, o Mensch, in der Natur", "tokens": ["Willst", "Du", ",", "o", "Mensch", ",", "in", "der", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gottheit Abglanz finden:", "tokens": ["Der", "Got\u00b7theit", "Ab\u00b7glanz", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Such in Dir, seinem Bilde, nur,", "tokens": ["Such", "in", "Dir", ",", "sei\u00b7nem", "Bil\u00b7de", ",", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und Funke wird Dir z\u00fcnden!", "tokens": ["Und", "Fun\u00b7ke", "wird", "Dir", "z\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und werden alle Funken Dir", "tokens": ["Und", "wer\u00b7den", "al\u00b7le", "Fun\u00b7ken", "Dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann lohe Gottesflammen:", "tokens": ["Dann", "lo\u00b7he", "Got\u00b7tes\u00b7flam\u00b7men", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sieh Jesum Christum hier!", "tokens": ["Sieh", "Je\u00b7sum", "Chris\u00b7tum", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da flammen sie zusammen.", "tokens": ["Da", "flam\u00b7men", "sie", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}