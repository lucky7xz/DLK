{"textgrid.poem.67294": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gaben der Grazien spendeten Dir und der Musen die G\u00f6tter,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gaben der Grazien spendeten Dir und der Musen die G\u00f6tter,", "tokens": ["Ga\u00b7ben", "der", "Gra\u00b7zi\u00b7en", "spen\u00b7de\u00b7ten", "Dir", "und", "der", "Mu\u00b7sen", "die", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Eucharis, dankbar empfang, ernst und ger\u00fchrt ihr Geschenk.", "tokens": ["Euc\u00b7ha\u00b7ris", ",", "dank\u00b7bar", "emp\u00b7fang", ",", "ernst", "und", "ge\u00b7r\u00fchrt", "ihr", "Ge\u00b7schenk", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "$,", "ADJD", "VVFIN", "$,", "ADJD", "KON", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hoch aus den Wolken strahlet das Ziel des geweiheten K\u00f6chers,", "tokens": ["Hoch", "aus", "den", "Wol\u00b7ken", "strah\u00b7let", "das", "Ziel", "des", "ge\u00b7wei\u00b7he\u00b7ten", "K\u00f6\u00b7chers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Jedem siegenden Pfeil spro\u00dfet die Palme des Lohns.", "tokens": ["Je\u00b7dem", "sie\u00b7gen\u00b7den", "Pfeil", "spro\u00b7\u00dfet", "die", "Pal\u00b7me", "des", "Lohns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Doch in der Sterblichen H\u00e4nden sind furchtbar die G\u00f6ttergeschosse! \u2013", "tokens": ["Doch", "in", "der", "Sterb\u00b7li\u00b7chen", "H\u00e4n\u00b7den", "sind", "furcht\u00b7bar", "die", "G\u00f6t\u00b7ter\u00b7ge\u00b7schos\u00b7se", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VAFIN", "ADJD", "ART", "NN", "$.", "$("], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Warnend, ihr Opfer, erschien Held Philoctetes mir einst,", "tokens": ["War\u00b7nend", ",", "ihr", "Op\u00b7fer", ",", "er\u00b7schien", "Held", "Phil\u00b7oc\u00b7te\u00b7tes", "mir", "einst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "NN", "NE", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Er, der verwundet vom eignen der immertreffenden Pfeile,", "tokens": ["Er", ",", "der", "ver\u00b7wun\u00b7det", "vom", "eig\u00b7nen", "der", "im\u00b7mer\u00b7tref\u00b7fen\u00b7den", "Pfei\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VVPP", "APPRART", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Er, dem der einsame Strand H\u00f6rer der Klagen nur war. \u2013", "tokens": ["Er", ",", "dem", "der", "ein\u00b7sa\u00b7me", "Strand", "H\u00f6\u00b7rer", "der", "Kla\u00b7gen", "nur", "war", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "NN", "ART", "NN", "ADV", "VAFIN", "$.", "$("], "meter": "-+--+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "\u00bbwer erscheinet wohl mir?\u00ab So fragte l\u00e4chelnd die sch\u00f6ne", "tokens": ["\u00bb", "wer", "er\u00b7schei\u00b7net", "wohl", "mir", "?", "\u00ab", "So", "frag\u00b7te", "l\u00e4\u00b7chelnd", "die", "sch\u00f6\u00b7ne"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "ADV", "PPER", "$.", "$(", "ADV", "VVFIN", "ADJD", "ART", "ADJA"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Frau, und ihr zauberndes Wort rief die Erscheinung hervor.", "tokens": ["Frau", ",", "und", "ihr", "zau\u00b7bern\u00b7des", "Wort", "rief", "die", "Er\u00b7schei\u00b7nung", "her\u00b7vor", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Tyndarus Tochter war es, geschm\u00fcckt mit dem G\u00fcrtel Cythera's,", "tokens": ["Tyn\u00b7da\u00b7rus", "Toch\u00b7ter", "war", "es", ",", "ge\u00b7schm\u00fcckt", "mit", "dem", "G\u00fcr\u00b7tel", "Cy\u00b7thera's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPER", "$,", "VVPP", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Doch in dem Himmel des Aug's tr\u00fcbte die Reue den Blick:", "tokens": ["Doch", "in", "dem", "Him\u00b7mel", "des", "Aug's", "tr\u00fcb\u00b7te", "die", "Reu\u00b7e", "den", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "\u00bbsiegerinn war auch ich und z\u00fcndete Flammen, da\u00df Troja", "tokens": ["\u00bb", "sie\u00b7ge\u00b7rinn", "war", "auch", "ich", "und", "z\u00fcn\u00b7de\u00b7te", "Flam\u00b7men", ",", "da\u00df", "Tro\u00b7ja"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PPER", "KON", "ADJA", "NN", "$,", "KOUS", "NE"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Loderte, ach und es sank Priams Heroen-Geschlecht.\u00ab\u2013", "tokens": ["Lo\u00b7der\u00b7te", ",", "ach", "und", "es", "sank", "Pri\u00b7ams", "He\u00b7ro\u00b7en\u00b7Ge\u00b7schlecht", ".", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "$,", "XY", "KON", "PPER", "VVFIN", "NE", "NN", "$.", "$(", "$("], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Schone, Helena, schon'! es erg\u00f6tzt an Entflammungen sich ja", "tokens": ["Scho\u00b7ne", ",", "He\u00b7le\u00b7na", ",", "schon'", "!", "es", "er\u00b7g\u00f6tzt", "an", "Ent\u00b7flam\u00b7mun\u00b7gen", "sich", "ja"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "PRF", "ADV"], "meter": "+-+--+--+--++--+", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Eucharis nicht, die so ", "tokens": ["Euc\u00b7ha\u00b7ris", "nicht", ",", "die", "so"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "PTKNEG", "$,", "PRELS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}