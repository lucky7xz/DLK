{"textgrid.poem.43253": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn jener echte Opfermuth", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn jener echte Opfermuth", "tokens": ["Wenn", "je\u00b7ner", "ech\u00b7te", "Op\u00b7fer\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch heute Dir das Herz bewegt,", "tokens": ["Noch", "heu\u00b7te", "Dir", "das", "Herz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn es ein treu Erinnern hegt,", "tokens": ["Wenn", "es", "ein", "treu", "E\u00b7rin\u00b7nern", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So komm ... Es ging zu hoch die Fluth.", "tokens": ["So", "komm", "...", "Es", "ging", "zu", "hoch", "die", "Fluth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PPER", "VVFIN", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Strand lieg' ich in Seelennoth,", "tokens": ["Am", "Strand", "lieg'", "ich", "in", "See\u00b7len\u00b7noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gro\u00dfm\u00fcth'ger Thorheit eingedenk,", "tokens": ["Gro\u00df\u00b7m\u00fcth'\u00b7ger", "Thor\u00b7heit", "ein\u00b7ge\u00b7denk", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die, unterw\u00fcrfig, als Geschenk", "tokens": ["Die", ",", "un\u00b7ter\u00b7w\u00fcr\u00b7fig", ",", "als", "Ge\u00b7schenk"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "$,", "ADJD", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einst eine Welt von Liebe bot ...", "tokens": ["Einst", "ei\u00b7ne", "Welt", "von", "Lie\u00b7be", "bot", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}