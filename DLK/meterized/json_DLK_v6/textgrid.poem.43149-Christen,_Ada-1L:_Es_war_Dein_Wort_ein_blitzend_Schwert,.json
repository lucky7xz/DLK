{"textgrid.poem.43149": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war Dein Wort ein blitzend Schwert,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war Dein Wort ein blitzend Schwert,", "tokens": ["Es", "war", "Dein", "Wort", "ein", "blit\u00b7zend", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das f\u00fcr mich stritt;", "tokens": ["Das", "f\u00fcr", "mich", "stritt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es war Dein Wort der Seele Schrei,", "tokens": ["Es", "war", "Dein", "Wort", "der", "See\u00b7le", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die f\u00fcr mich litt.", "tokens": ["Die", "f\u00fcr", "mich", "litt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die herbe Thr\u00e4ne war Dein Wort,", "tokens": ["Die", "her\u00b7be", "Thr\u00e4\u00b7ne", "war", "Dein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geweint um mich;", "tokens": ["Ge\u00b7weint", "um", "mich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ein guter Engel war Dein Wort,", "tokens": ["Ein", "gu\u00b7ter", "En\u00b7gel", "war", "Dein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der nimmer wich!", "tokens": ["Der", "nim\u00b7mer", "wich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Dein Wort, es gab mir neuen Muth,", "tokens": ["Dein", "Wort", ",", "es", "gab", "mir", "neu\u00b7en", "Muth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es drang befreiend stolz zu mir;", "tokens": ["Es", "drang", "be\u00b7frei\u00b7end", "stolz", "zu", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Du Fremder, sieh mein schlichtes Wort,", "tokens": ["Du", "Frem\u00b7der", ",", "sieh", "mein", "schlich\u00b7tes", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Es dankt zu tausend Malen Dir!", "tokens": ["Es", "dankt", "zu", "tau\u00b7send", "Ma\u00b7len", "Dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}