{"dta.poem.10876": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wassergedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier seh ich, durch der L\u00fcfte Regen,", "tokens": ["Hier", "seh", "ich", ",", "durch", "der", "L\u00fcf\u00b7te", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fluth sich angenehm bewegen,", "tokens": ["Die", "Fluth", "sich", "an\u00b7ge\u00b7nehm", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf stets ver\u00e4nderlichen Stellen,", "tokens": ["Auf", "stets", "ver\u00b7\u00e4n\u00b7der\u00b7li\u00b7chen", "Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wellen \u00fcber andre Wellen,", "tokens": ["Die", "Wel\u00b7len", "\u00fc\u00b7ber", "and\u00b7re", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit halben Zirkeln, sanft sich legen,", "tokens": ["Mit", "hal\u00b7ben", "Zir\u00b7keln", ",", "sanft", "sich", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie es scheinet, ohn Verweilen,", "tokens": ["Und", "wie", "es", "schei\u00b7net", ",", "ohn", "Ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nach dem bebl\u00fcmten Ufer eilen,", "tokens": ["Nach", "dem", "be\u00b7bl\u00fcm\u00b7ten", "U\u00b7fer", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit feuchten Lippen es zu k\u00fcssen,", "tokens": ["Mit", "feuch\u00b7ten", "Lip\u00b7pen", "es", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und, in der, von der B\u00e4ume Pracht,", "tokens": ["Und", ",", "in", "der", ",", "von", "der", "B\u00e4u\u00b7me", "Pracht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.10": {"text": "Daselbst vorhandnen Schatten-Nacht,", "tokens": ["Da\u00b7selbst", "vor\u00b7hand\u00b7nen", "Schat\u00b7ten\u00b7Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "An ihren weich bemosten F\u00fcssen,", "tokens": ["An", "ih\u00b7ren", "weich", "be\u00b7mos\u00b7ten", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ein kurzes Ruhen zu geniessen.", "tokens": ["Ein", "kur\u00b7zes", "Ru\u00b7hen", "zu", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Auch um, nach nun mehr stillem Wallen,", "tokens": ["Auch", "um", ",", "nach", "nun", "mehr", "stil\u00b7lem", "Wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "APPR", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Mit ihren gl\u00e4nzenden Krystallen,", "tokens": ["Mit", "ih\u00b7ren", "gl\u00e4n\u00b7zen\u00b7den", "Krys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Jm glatten Spiegel, ihrem Gr\u00fcnen,", "tokens": ["Jm", "glat\u00b7ten", "Spie\u00b7gel", ",", "ih\u00b7rem", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Zum holden Wiederschein, zu dienen.", "tokens": ["Zum", "hol\u00b7den", "Wie\u00b7der\u00b7schein", ",", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ja, nach dem, von des Himmels Zier,", "tokens": ["Ja", ",", "nach", "dem", ",", "von", "des", "Him\u00b7mels", "Zier", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Getragnen Schimmer von Saphier,", "tokens": ["Ge\u00b7trag\u00b7nen", "Schim\u00b7mer", "von", "Sa\u00b7phier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Gef\u00e4rbt vom gr\u00fcnen Schmuck der Erden,", "tokens": ["Ge\u00b7f\u00e4rbt", "vom", "gr\u00fc\u00b7nen", "Schmuck", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Zum gl\u00e4nzenden Smaragd zu werden.", "tokens": ["Zum", "gl\u00e4n\u00b7zen\u00b7den", "Sma\u00b7ragd", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "So scheint es. Aber die\u00df ist wahr,", "tokens": ["So", "scheint", "es", ".", "A\u00b7ber", "die\u00df", "ist", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df, da die Fluth so glatt und klar,", "tokens": ["Da\u00df", ",", "da", "die", "Fluth", "so", "glatt", "und", "klar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Jhr Wesen blo\u00df dazu gemacht,", "tokens": ["Ihr", "We\u00b7sen", "blo\u00df", "da\u00b7zu", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Des Himmels und der Erden Pracht,", "tokens": ["Des", "Him\u00b7mels", "und", "der", "Er\u00b7den", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Zu unsrer Lust, und Gott zu Ehren,", "tokens": ["Zu", "uns\u00b7rer", "Lust", ",", "und", "Gott", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "In der Verdopplung zu vermehren.", "tokens": ["In", "der", "Ver\u00b7dopp\u00b7lung", "zu", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "So lasset uns, wenn wir erblicken,", "tokens": ["So", "las\u00b7set", "uns", ",", "wenn", "wir", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Wie sich so Fluth, als Erde, schm\u00fccken,", "tokens": ["Wie", "sich", "so", "Fluth", ",", "als", "Er\u00b7de", ",", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "NN", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und da\u00df sie beyde doppelt sch\u00f6n,", "tokens": ["Und", "da\u00df", "sie", "bey\u00b7de", "dop\u00b7pelt", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Es doch mit Dank und Freude sehn!", "tokens": ["Es", "doch", "mit", "Dank", "und", "Freu\u00b7de", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}