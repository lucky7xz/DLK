{"textgrid.poem.42742": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[ein Pinsel mit sehr talentvollen Borsten]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Pinsel mit sehr talentvollen Borsten,", "tokens": ["Ein", "Pin\u00b7sel", "mit", "sehr", "ta\u00b7lent\u00b7vol\u00b7len", "Bors\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der mu\u00dfte viel hungern und viel dorsten.", "tokens": ["Der", "mu\u00df\u00b7te", "viel", "hun\u00b7gern", "und", "viel", "dors\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVFIN", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er war 60 Jahre alt und hie\u00df Tipfelchen.", "tokens": ["Er", "war", "60", "Jah\u00b7re", "alt", "und", "hie\u00df", "Tip\u00b7fel\u00b7chen", "."], "token_info": ["word", "word", "number", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "ADJD", "KON", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus festem Tannenholz war sein Stiel.", "tokens": ["Aus", "fes\u00b7tem", "Tan\u00b7nen\u00b7holz", "war", "sein", "Stiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Er malte, und was er malte, gefiel.", "tokens": ["Er", "mal\u00b7te", ",", "und", "was", "er", "mal\u00b7te", ",", "ge\u00b7fiel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Doch, wie gesagt, er litt Hunger und Durst.", "tokens": ["Doch", ",", "wie", "ge\u00b7sagt", ",", "er", "litt", "Hun\u00b7ger", "und", "Durst", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "VVPP", "$,", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da kam eine junge fettige Wurst.", "tokens": ["Da", "kam", "ei\u00b7ne", "jun\u00b7ge", "fet\u00b7ti\u00b7ge", "Wurst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sie wog 500 Gramm und war vom Stamme Rindvieh.", "tokens": ["Sie", "wog", "500", "Gramm", "und", "war", "vom", "Stam\u00b7me", "Rind\u00b7vieh", "."], "token_info": ["word", "word", "number", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "KON", "VAFIN", "APPRART", "NN", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Kaum hatte der Pinsel die Wurst gesehn,", "tokens": ["Kaum", "hat\u00b7te", "der", "Pin\u00b7sel", "die", "Wurst", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Blieb er stehn,", "tokens": ["Blieb", "er", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "B\u00fcckte sich tief dabei,", "tokens": ["B\u00fcck\u00b7te", "sich", "tief", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PAV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Knickte dann schief entzwei.", "tokens": ["Knick\u00b7te", "dann", "schief", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Die Wurst aber, mit Namen Schulze,", "tokens": ["Die", "Wurst", "a\u00b7ber", ",", "mit", "Na\u00b7men", "Schul\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sagte: \u00bbMein lieber Tipfelchen,", "tokens": ["Sag\u00b7te", ":", "\u00bb", "Mein", "lie\u00b7ber", "Tip\u00b7fel\u00b7chen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Hier hast du ein Wurstzipfelchen,", "tokens": ["Hier", "hast", "du", "ein", "Wurst\u00b7zi\u00b7pfel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Male mir mal drei Meter Sulze.\u00ab", "tokens": ["Ma\u00b7le", "mir", "mal", "drei", "Me\u00b7ter", "Sul\u00b7ze", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADV", "CARD", "NN", "NE", "$.", "$("], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}}}}