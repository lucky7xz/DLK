{"dta.poem.19114": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XcVI.  Lied vor eine K\u00f6nigl. Erb-Prin-  \n tze\u00dfin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Christen sind ein g\u00f6ttlich Volck,", "tokens": ["Chris\u00b7ten", "sind", "ein", "g\u00f6tt\u00b7lich", "Volck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Geist des HErrn gezeuget,", "tokens": ["Aus", "dem", "Geist", "des", "Herrn", "ge\u00b7zeu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem nichts gleichet,", "tokens": ["Dem", "nichts", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und von seiner Flammen-Macht", "tokens": ["Und", "von", "sei\u00b7ner", "Flam\u00b7men\u00b7Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Angefacht;", "tokens": ["An\u00b7ge\u00b7facht", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Vor des Br\u00e4utgams Augen schweben", "tokens": ["Vor", "des", "Br\u00e4ut\u00b7gams", "Au\u00b7gen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das ist ihrer Seelen Leben,", "tokens": ["Das", "ist", "ih\u00b7rer", "See\u00b7len", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und sein Blut ist ihre Pracht.", "tokens": ["Und", "sein", "Blut", "ist", "ih\u00b7re", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! du Seelen-Br\u00e4ntigam!", "tokens": ["Ach", "!", "du", "See\u00b7len\u00b7Br\u00e4n\u00b7ti\u00b7gam", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast Du mich der Welt entzogen,", "tokens": ["Hast", "Du", "mich", "der", "Welt", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ausgesogen", "tokens": ["Aus\u00b7ge\u00b7so\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Von der alten Creatur,", "tokens": ["Von", "der", "al\u00b7ten", "Crea\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und die Cur,", "tokens": ["Und", "die", "Cur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Welche Deine Seelen heilet,", "tokens": ["Wel\u00b7che", "Dei\u00b7ne", "See\u00b7len", "hei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch mir armen mitgetheilet;", "tokens": ["Auch", "mir", "ar\u00b7men", "mit\u00b7ge\u00b7thei\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schencke mir die Geists-Natur!", "tokens": ["Schen\u00b7cke", "mir", "die", "Geist\u00b7s\u00b7Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "K\u00f6nigs-Cronen sind zu bleich,", "tokens": ["K\u00f6\u00b7nigs\u00b7Cro\u00b7nen", "sind", "zu", "bleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor der GOtt-verlobten W\u00fcrde;", "tokens": ["Vor", "der", "Got\u00b7tver\u00b7lob\u00b7ten", "W\u00fcr\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eiue H\u00fcrde", "tokens": ["Ei\u00b7ue", "H\u00fcr\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wird zum himmlischen Pallast:", "tokens": ["Wird", "zum", "himm\u00b7li\u00b7schen", "Pal\u00b7last", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Last", "tokens": ["Und", "die", "Last"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Drunter sich die Helden klagen,", "tokens": ["Drun\u00b7ter", "sich", "die", "Hel\u00b7den", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird den Kindern leicht zu tragen,", "tokens": ["Wird", "den", "Kin\u00b7dern", "leicht", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die die Creutzes-Krafft gefa\u00dft.", "tokens": ["Die", "die", "Creut\u00b7zes\u00b7Krafft", "ge\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ehe JEsus unser wird,", "tokens": ["E\u00b7he", "Je\u00b7sus", "un\u00b7ser", "wird", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehe wir uns selbst vergessen,", "tokens": ["E\u00b7he", "wir", "uns", "selbst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gesessen", "tokens": ["Und", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word"], "pos": ["KON", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Zu den F\u00fcssen unsers HErrn,", "tokens": ["Zu", "den", "F\u00fcs\u00b7sen", "un\u00b7sers", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind wir fern,", "tokens": ["Sind", "wir", "fern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Von der ewgen Bundes-Gnade,", "tokens": ["Von", "der", "ew\u00b7gen", "Bun\u00b7des\u00b7Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem schmalen Lebens-Pfade,", "tokens": ["Von", "dem", "schma\u00b7len", "Le\u00b7bens\u00b7Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von dem hellen Morgenstern.", "tokens": ["Von", "dem", "hel\u00b7len", "Mor\u00b7gens\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Pilgrimschafft zur Ewigkeit", "tokens": ["Pil\u00b7grim\u00b7schafft", "zur", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibet immerdar beschwerlich,", "tokens": ["Blei\u00b7bet", "im\u00b7mer\u00b7dar", "be\u00b7schwer\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja gef\u00e4hrlich;", "tokens": ["Ja", "ge\u00b7f\u00e4hr\u00b7lich", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Bi\u00df man ringt und dringt zu Dir,", "tokens": ["Bi\u00df", "man", "ringt", "und", "dringt", "zu", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Enge Th\u00fcr,", "tokens": ["En\u00b7ge", "Th\u00fcr", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ein\u2019ge Ursach der Vergebung,", "tokens": ["Ein'\u00b7ge", "Ur\u00b7sach", "der", "Ver\u00b7ge\u00b7bung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Glut der G\u00f6ttlichen Bewegung,", "tokens": ["Glut", "der", "G\u00f6tt\u00b7li\u00b7chen", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jesu, unser Liebs-Panier!", "tokens": ["Je\u00b7su", ",", "un\u00b7ser", "Liebs\u00b7Pa\u00b7nier", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zeuch uns hin, ", "tokens": ["Zeuch", "uns", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Zeuch uns an dein Hertz der Liebe,", "tokens": ["Zeuch", "uns", "an", "dein", "Hertz", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Triebe", "tokens": ["Dei\u00b7ne", "Trie\u00b7be"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "F\u00fchren mich, du Sieges-Held!", "tokens": ["F\u00fch\u00b7ren", "mich", ",", "du", "Sie\u00b7ges\u00b7Held", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die Welt,", "tokens": ["Durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Da\u00df ich deine Seele bleibe,", "tokens": ["Da\u00df", "ich", "dei\u00b7ne", "See\u00b7le", "blei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so lange an dich gl\u00e4ube,", "tokens": ["Und", "so", "lan\u00b7ge", "an", "dich", "gl\u00e4u\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bi\u00df ich lieb\u2019 im innern Zelt.", "tokens": ["Bi\u00df", "ich", "lieb'", "im", "in\u00b7nern", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da ist meine Hand und Hertz:", "tokens": ["Da", "ist", "mei\u00b7ne", "Hand", "und", "Hertz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Du hast deine Seel gewaget,", "tokens": ["Du", "hast", "dei\u00b7ne", "Seel", "ge\u00b7wa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unverzaget,", "tokens": ["Un\u00b7ver\u00b7za\u00b7get", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und das alles blo\u00df allein,", "tokens": ["Und", "das", "al\u00b7les", "blo\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich dein,", "tokens": ["Da\u00df", "ich", "dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und du meine heissen k\u00f6ntest,", "tokens": ["Und", "du", "mei\u00b7ne", "heis\u00b7sen", "k\u00f6n\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn du nicht vor Liebe brenntest;", "tokens": ["Wenn", "du", "nicht", "vor", "Lie\u00b7be", "brenn\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4tte das nicht k\u00f6nnen seyn.", "tokens": ["H\u00e4t\u00b7te", "das", "nicht", "k\u00f6n\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PTKNEG", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun ihr Cronen fahret hin,", "tokens": ["Nun", "ihr", "Cro\u00b7nen", "fah\u00b7ret", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fahre hin, erlaubte Freude!", "tokens": ["Fah\u00b7re", "hin", ",", "er\u00b7laub\u00b7te", "Freu\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Weide", "tokens": ["Mei\u00b7ne", "Wei\u00b7de"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Sey des HErren letztes Mahl", "tokens": ["Sey", "des", "Her\u00b7ren", "letz\u00b7tes", "Mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vor der Qval,", "tokens": ["Vor", "der", "Qval", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Meine Ehre deine Schande,", "tokens": ["Mei\u00b7ne", "Eh\u00b7re", "dei\u00b7ne", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Freyheit deine Bande,", "tokens": ["Mei\u00b7ne", "Frey\u00b7heit", "dei\u00b7ne", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mein Geschmuck die Ros\u2019 im Thal.", "tokens": ["Mein", "Ge\u00b7schmuck", "die", "Ros'", "im", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}