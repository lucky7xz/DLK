ihn erschlagen", "tokens": ["Sie", "ha\u00b7ben", "ihn", "er\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Neid und Eifersucht.", "tokens": ["Aus", "Neid", "und", "Ei\u00b7fer\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im Lenze, ach, im Lenze \u2013", "tokens": ["Im", "Len\u00b7ze", ",", "ach", ",", "im", "Len\u00b7ze", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ITJ", "$,", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie spielten Federball,", "tokens": ["Sie", "spiel\u00b7ten", "Fe\u00b7der\u00b7ball", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lord Murrays stieg am h\u00f6chsten", "tokens": ["Lord", "Mur\u00b7rays", "stieg", "am", "h\u00f6chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und \u00fcberflog sie all.", "tokens": ["Und", "\u00fc\u00b7berf\u00b7log", "sie", "all", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im Sommer, ach, im Sommer \u2013", "tokens": ["Im", "Som\u00b7mer", ",", "ach", ",", "im", "Som\u00b7mer", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ITJ", "$,", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus zogen sie z