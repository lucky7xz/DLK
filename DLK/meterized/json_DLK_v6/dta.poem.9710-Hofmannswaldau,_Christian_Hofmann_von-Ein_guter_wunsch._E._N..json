{"dta.poem.9710": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ein guter wunsch.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ferrando hat ein weib das nicht den hencker werth.", "tokens": ["Fer\u00b7ran\u00b7do", "hat", "ein", "weib", "das", "nicht", "den", "hen\u00b7cker", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "VVFIN", "PDS", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er schmiert sie weidlich ab. Ein esel oder pferd", "tokens": ["Er", "schmiert", "sie", "weid\u00b7lich", "ab", ".", "Ein", "e\u00b7sel", "o\u00b7der", "pferd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan diese schl\u00e4ge nicht auf seinem buckel tragen.", "tokens": ["Kan", "die\u00b7se", "schl\u00e4\u00b7ge", "nicht", "auf", "sei\u00b7nem", "bu\u00b7ckel", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie hat es zwar verdient. Doch dieses f\u00e4llt mir ein:", "tokens": ["Sie", "hat", "es", "zwar", "ver\u00b7dient", ".", "Doch", "die\u00b7ses", "f\u00e4llt", "mir", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "KON", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach schade/ da\u00df die frau nicht soll von golde seyn/", "tokens": ["Ach", "scha\u00b7de", "/", "da\u00df", "die", "frau", "nicht", "soll", "von", "gol\u00b7de", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$(", "KOUS", "ART", "NN", "PTKNEG", "VMFIN", "APPR", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So k\u00f6nte doch der mann ducaten aus ihr schlagen.", "tokens": ["So", "k\u00f6n\u00b7te", "doch", "der", "mann", "du\u00b7ca\u00b7ten", "aus", "ihr", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "VVFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}