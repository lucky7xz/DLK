{"textgrid.poem.24189": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Spruch", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder Mann hat seine R\u00fcpeljahr.", "tokens": ["Ein", "je\u00b7der", "Mann", "hat", "sei\u00b7ne", "R\u00fc\u00b7pel\u00b7jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der wird kein ganzer Kerl, der nie ein R\u00fcpel war.", "tokens": ["Der", "wird", "kein", "gan\u00b7zer", "Kerl", ",", "der", "nie", "ein", "R\u00fc\u00b7pel", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur freilich, da\u00df es geht, so wie mans treibt:", "tokens": ["Nur", "frei\u00b7lich", ",", "da\u00df", "es", "geht", ",", "so", "wie", "mans", "treibt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "KOKOM", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mancher sein Lebtag blo\u00df ein R\u00fcpel bleibt.", "tokens": ["Man\u00b7cher", "sein", "Leb\u00b7tag", "blo\u00df", "ein", "R\u00fc\u00b7pel", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}