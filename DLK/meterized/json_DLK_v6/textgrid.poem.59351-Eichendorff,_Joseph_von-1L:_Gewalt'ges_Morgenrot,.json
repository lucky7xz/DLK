{"textgrid.poem.59351": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gewalt'ges Morgenrot,", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewalt'ges Morgenrot,", "tokens": ["Ge\u00b7walt'\u00b7ges", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Weit, unerme\u00dflich \u2013 du verzehrst die Erde!", "tokens": ["Weit", ",", "un\u00b7er\u00b7me\u00df\u00b7lich", "\u2013", "du", "ver\u00b7zehrst", "die", "Er\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in dem Schweigen nur der Flug der Seelen,", "tokens": ["Und", "in", "dem", "Schwei\u00b7gen", "nur", "der", "Flug", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die s\u00e4uselnd heimziehn durch die stille Luft. \u2013", "tokens": ["Die", "s\u00e4u\u00b7selnd", "heim\u00b7ziehn", "durch", "die", "stil\u00b7le", "Luft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "VVINF", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}