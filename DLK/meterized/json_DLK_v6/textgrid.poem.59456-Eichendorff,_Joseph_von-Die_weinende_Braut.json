{"textgrid.poem.59456": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die weinende Braut", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du warst so herrlich anzuschauen,", "tokens": ["Du", "warst", "so", "herr\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So k\u00fchn und wild und doch so lieb,", "tokens": ["So", "k\u00fchn", "und", "wild", "und", "doch", "so", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir mu\u00dft ich Leib und Seel vertrauen,", "tokens": ["Dir", "mu\u00dft", "ich", "Leib", "und", "Seel", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mocht nichts mehr, das meine blieb!", "tokens": ["Ich", "mocht", "nichts", "mehr", ",", "das", "mei\u00b7ne", "blieb", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$,", "PRELS", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da hast du, Falscher, mich verlassen", "tokens": ["Da", "hast", "du", ",", "Fal\u00b7scher", ",", "mich", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Blumen, Lust und Fr\u00fchlingsschein,", "tokens": ["Und", "Blu\u00b7men", ",", "Lust", "und", "Fr\u00fch\u00b7lings\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ganze Welt sah ich erblassen,", "tokens": ["Die", "gan\u00b7ze", "Welt", "sah", "ich", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ach Gott, wie bin ich nun allein!", "tokens": ["Ach", "Gott", ",", "wie", "bin", "ich", "nun", "al\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl jahrlang sah ich von den H\u00f6hen", "tokens": ["Wohl", "jahr\u00b7lang", "sah", "ich", "von", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gr\u00fc\u00dfte dich vieltausendmal,", "tokens": ["Und", "gr\u00fc\u00df\u00b7te", "dich", "viel\u00b7tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unten sah ich viele gehen,", "tokens": ["Und", "un\u00b7ten", "sah", "ich", "vie\u00b7le", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch du erschienst nicht in dem Tal.", "tokens": ["Doch", "du", "er\u00b7schienst", "nicht", "in", "dem", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mancher Lenz mit bunten Scherzen", "tokens": ["Und", "man\u00b7cher", "Lenz", "mit", "bun\u00b7ten", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kam und verflog im lust'gen Lauf,", "tokens": ["Kam", "und", "ver\u00b7flog", "im", "lust'\u00b7gen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch ach! in dem betrognen Herzen", "tokens": ["Doch", "ach", "!", "in", "dem", "be\u00b7trog\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geht niemals mehr der Fr\u00fchling auf.", "tokens": ["Geht", "nie\u00b7mals", "mehr", "der", "Fr\u00fch\u00b7ling", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Kr\u00e4nzlein trag ich nun im Haare,", "tokens": ["Ein", "Kr\u00e4nz\u00b7lein", "trag", "ich", "nun", "im", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In reichen Kleidern sch\u00f6n geschm\u00fcckt,", "tokens": ["In", "rei\u00b7chen", "Klei\u00b7dern", "sch\u00f6n", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fchrt mich ein andrer zum Altare,", "tokens": ["F\u00fchrt", "mich", "ein", "an\u00b7drer", "zum", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Eltern sind so hochbegl\u00fcckt.", "tokens": ["Die", "El\u00b7tern", "sind", "so", "hoch\u00b7be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fr\u00f6hlich kann ich mich wohl zeigen,", "tokens": ["Und", "fr\u00f6h\u00b7lich", "kann", "ich", "mich", "wohl", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sonne hell wie damals scheint,", "tokens": ["Die", "Son\u00b7ne", "hell", "wie", "da\u00b7mals", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und vor dem Jauchzen und dem Geigen", "tokens": ["Und", "vor", "dem", "Jauch\u00b7zen", "und", "dem", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "H\u00f6rt keiner, wie die Braut still weint.", "tokens": ["H\u00f6rt", "kei\u00b7ner", ",", "wie", "die", "Braut", "still", "weint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Fr\u00fchlingslieder neu beginnen \u2013", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7lie\u00b7der", "neu", "be\u00b7gin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du kehrst nach manchem Jahr zur\u00fcck,", "tokens": ["Du", "kehrst", "nach", "man\u00b7chem", "Jahr", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stehest still, dich zu besinnen,", "tokens": ["Und", "ste\u00b7hest", "still", ",", "dich", "zu", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie auf ein l\u00e4ngstvergangnes Gl\u00fcck.", "tokens": ["Wie", "auf", "ein", "l\u00e4ngst\u00b7ver\u00b7gang\u00b7nes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch w\u00fcst verwachsen liegt der Garten,", "tokens": ["Doch", "w\u00fcst", "ver\u00b7wach\u00b7sen", "liegt", "der", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Haus steht lange still und leer,", "tokens": ["Das", "Haus", "steht", "lan\u00b7ge", "still", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Lieb will dein am Fenster warten,", "tokens": ["Kein", "Lieb", "will", "dein", "am", "Fens\u00b7ter", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPOSAT", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und dich und mich kennt niemand mehr.", "tokens": ["Und", "dich", "und", "mich", "kennt", "nie\u00b7mand", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch eine Lerche siehst du steigen", "tokens": ["Doch", "ei\u00b7ne", "Ler\u00b7che", "siehst", "du", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Tal zum blauen Himmelsport,", "tokens": ["Vom", "Tal", "zum", "blau\u00b7en", "Him\u00b7mel\u00b7sport", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein B\u00e4chlein rauschet da so eigen,", "tokens": ["Ein", "B\u00e4ch\u00b7lein", "rau\u00b7schet", "da", "so", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als weinte es in einem fort.", "tokens": ["Als", "wein\u00b7te", "es", "in", "ei\u00b7nem", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort haben sie mich hingetragen,", "tokens": ["Dort", "ha\u00b7ben", "sie", "mich", "hin\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bedeckten mir mit Stein den Mund \u2013", "tokens": ["Be\u00b7deck\u00b7ten", "mir", "mit", "Stein", "den", "Mund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun kann ich dir nicht einmal sagen,", "tokens": ["Nun", "kann", "ich", "dir", "nicht", "ein\u00b7mal", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie ich dich liebt aus Herzensgrund.", "tokens": ["Wie", "ich", "dich", "liebt", "aus", "Her\u00b7zens\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}