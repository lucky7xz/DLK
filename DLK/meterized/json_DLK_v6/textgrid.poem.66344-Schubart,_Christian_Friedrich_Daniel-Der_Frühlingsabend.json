{"textgrid.poem.66344": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchlingsabend", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchlender Abend! steige vom H\u00fcgel,", "tokens": ["K\u00fch\u00b7len\u00b7der", "A\u00b7bend", "!", "stei\u00b7ge", "vom", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Lieblich verguldet vom sonnigen Strahl;", "tokens": ["Lieb\u00b7lich", "ver\u00b7gul\u00b7det", "vom", "son\u00b7ni\u00b7gen", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Thaue von deinem purpurnen Fl\u00fcgel", "tokens": ["Thau\u00b7e", "von", "dei\u00b7nem", "pur\u00b7pur\u00b7nen", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Tropfen aufs durstige Bl\u00fcmlein im Thal.", "tokens": ["Trop\u00b7fen", "aufs", "durs\u00b7ti\u00b7ge", "Bl\u00fcm\u00b7lein", "im", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Gluckt, Nachtigallen, z\u00e4rtliche Lieder,", "tokens": ["Gluckt", ",", "Nach\u00b7ti\u00b7gal\u00b7len", ",", "z\u00e4rt\u00b7li\u00b7che", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Reget, ihr Weste, euer Gefieder;", "tokens": ["Re\u00b7get", ",", "ihr", "Wes\u00b7te", ",", "eu\u00b7er", "Ge\u00b7fie\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Sch\u00fcttelt vom Baum", "tokens": ["Sch\u00fct\u00b7telt", "vom", "Baum"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Seidenen Flaum!", "tokens": ["Sei\u00b7de\u00b7nen", "Flaum", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Walle, o Duft! vom Bl\u00fcthenzweig nieder.", "tokens": ["Wal\u00b7le", ",", "o", "Duft", "!", "vom", "Bl\u00fc\u00b7then\u00b7zweig", "nie\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$.", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Hier auf der Erde blumigem Scho\u00dfe", "tokens": ["Hier", "auf", "der", "Er\u00b7de", "blu\u00b7mi\u00b7gem", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ruh' ich! es ruhet mein M\u00e4dchen bei mir.", "tokens": ["Ruh'", "ich", "!", "es", "ru\u00b7het", "mein", "M\u00e4d\u00b7chen", "bei", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Meine Geliebte, kennst du die gro\u00dfe,", "tokens": ["Mei\u00b7ne", "Ge\u00b7lieb\u00b7te", ",", "kennst", "du", "die", "gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Kennst du die f\u00fchlende Freundin von dir?", "tokens": ["Kennst", "du", "die", "f\u00fch\u00b7len\u00b7de", "Freun\u00b7din", "von", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Lieblicher Abend, l\u00e4chle der Trauten!", "tokens": ["Lieb\u00b7li\u00b7cher", "A\u00b7bend", ",", "l\u00e4ch\u00b7le", "der", "Trau\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "L\u00e4chle der Schlanken, Himmlischgebauten!", "tokens": ["L\u00e4ch\u00b7le", "der", "Schlan\u00b7ken", ",", "Himm\u00b7lischge\u00b7bau\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Sch\u00f6ner war nicht", "tokens": ["Sch\u00f6\u00b7ner", "war", "nicht"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "PTKNEG"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Florens Gesicht,", "tokens": ["Flo\u00b7rens", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Als sie des Morgens Tropfen bethauten.", "tokens": ["Als", "sie", "des", "Mor\u00b7gens", "Trop\u00b7fen", "be\u00b7thau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Hesperus \u00e4ugelt hoch in der Ferne;", "tokens": ["Hes\u00b7pe\u00b7rus", "\u00e4u\u00b7gelt", "hoch", "in", "der", "Fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ziehst du schon, Mond, am Sternenfeld auf?", "tokens": ["Ziehst", "du", "schon", ",", "Mond", ",", "am", "Ster\u00b7nen\u00b7feld", "auf", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sieh doch, Geliebte, sieh doch die Sterne!", "tokens": ["Sieh", "doch", ",", "Ge\u00b7lieb\u00b7te", ",", "sieh", "doch", "die", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "NN", "$,", "VVIMP", "ADV", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sieh doch zur freundlichen Luna hinauf!", "tokens": ["Sieh", "doch", "zur", "freund\u00b7li\u00b7chen", "Lu\u00b7na", "hin\u00b7auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch seh' ich nicht im Auge der Milden", "tokens": ["Doch", "seh'", "ich", "nicht", "im", "Au\u00b7ge", "der", "Mil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Thr\u00e4nen der Liebe schimmernd sich bilden?", "tokens": ["Thr\u00e4\u00b7nen", "der", "Lie\u00b7be", "schim\u00b7mernd", "sich", "bil\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Sind sie es nicht,", "tokens": ["Sind", "sie", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die dein Gesicht,", "tokens": ["Die", "dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wie eines Engels Antlitz, verg\u00fclden?", "tokens": ["Wie", "ei\u00b7nes", "En\u00b7gels", "Ant\u00b7litz", ",", "ver\u00b7g\u00fcl\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Lieblicher Abend, Erweicher der Herzen!", "tokens": ["Lieb\u00b7li\u00b7cher", "A\u00b7bend", ",", "Er\u00b7wei\u00b7cher", "der", "Her\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Dank dir, des Fr\u00fchlings liebkosender Sohn,", "tokens": ["Dank", "dir", ",", "des", "Fr\u00fch\u00b7lings", "lieb\u00b7ko\u00b7sen\u00b7der", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+---+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da\u00df du geendigt z\u00e4rtliche Schmerzen;", "tokens": ["Da\u00df", "du", "ge\u00b7en\u00b7digt", "z\u00e4rt\u00b7li\u00b7che", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sieh doch, die Holde umarmet mich schon!", "tokens": ["Sieh", "doch", ",", "die", "Hol\u00b7de", "um\u00b7ar\u00b7met", "mich", "schon", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Schmelzende Wonne flimmt in den Blicken,", "tokens": ["Schmel\u00b7zen\u00b7de", "Won\u00b7ne", "flimmt", "in", "den", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ach ich empfinde Himmelsentz\u00fccken.", "tokens": ["Ach", "ich", "emp\u00b7fin\u00b7de", "Him\u00b7mel\u00b7sent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Liebe, nur du", "tokens": ["Lie\u00b7be", ",", "nur", "du"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Wiegst uns in Ruh';", "tokens": ["Wiegst", "uns", "in", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Kannst, wie ein Gott, allein uns begl\u00fccken.", "tokens": ["Kannst", ",", "wie", "ein", "Gott", ",", "al\u00b7lein", "uns", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PWAV", "ART", "NN", "$,", "ADV", "PPER", "VVINF", "$."], "meter": "+-+++-+-+-", "measure": "unknown.measure.hexa"}}}}}