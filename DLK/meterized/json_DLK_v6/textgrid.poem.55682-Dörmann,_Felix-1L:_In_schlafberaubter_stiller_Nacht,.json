{"textgrid.poem.55682": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: In schlafberaubter stiller Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In schlafberaubter stiller Nacht,", "tokens": ["In", "schlaf\u00b7be\u00b7raub\u00b7ter", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Alles m\u00fcde ruht,", "tokens": ["Wenn", "Al\u00b7les", "m\u00fc\u00b7de", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steigt auf aus meiner Seele Schacht,", "tokens": ["Steigt", "auf", "aus", "mei\u00b7ner", "See\u00b7le", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit trotzig ungest\u00fcmer Macht", "tokens": ["Mit", "trot\u00b7zig", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Qualgedanken Brut.", "tokens": ["Der", "Qual\u00b7ge\u00b7dan\u00b7ken", "Brut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es schwillt und quillt so hei\u00df und rot", "tokens": ["Es", "schwillt", "und", "quillt", "so", "hei\u00df", "und", "rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Blut empor zur Stirn,", "tokens": ["Das", "Blut", "em\u00b7por", "zur", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df in des Denkens wilder Not", "tokens": ["Da\u00df", "in", "des", "Den\u00b7kens", "wil\u00b7der", "Not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu bersten, zu versagen droht", "tokens": ["Zu", "bers\u00b7ten", ",", "zu", "ver\u00b7sa\u00b7gen", "droht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das m\u00fcde Hirn.", "tokens": ["Das", "m\u00fc\u00b7de", "Hirn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Will ich des Lebens Kampf bestehn,", "tokens": ["Will", "ich", "des", "Le\u00b7bens", "Kampf", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz im Schmutz verdirbt \u2013 \u2013", "tokens": ["Mein", "Herz", "im", "Schmutz", "ver\u00b7dirbt", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nein! \u2013 ich will schlafen, schlafen gehn,", "tokens": ["Nein", "!", "\u2013", "ich", "will", "schla\u00b7fen", ",", "schla\u00b7fen", "gehn", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "$(", "PPER", "VMFIN", "VVINF", "$,", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn k\u00fchl des Herbstes Winde wehn", "tokens": ["Wenn", "k\u00fchl", "des", "Herbs\u00b7tes", "Win\u00b7de", "wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Alles ringsum stirbt.", "tokens": ["Und", "Al\u00b7les", "ring\u00b7sum", "stirbt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}