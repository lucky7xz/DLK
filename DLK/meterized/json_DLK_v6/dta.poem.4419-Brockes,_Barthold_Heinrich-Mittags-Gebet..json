{"dta.poem.4419": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mittags-Gebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "GoTT, aus welchem alles qvillet,", "tokens": ["GoTT", ",", "aus", "wel\u00b7chem", "al\u00b7les", "qvil\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was so Flut als Erde f\u00fcllet,", "tokens": ["Was", "so", "Flut", "als", "Er\u00b7de", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Du, was uns not ist, schenkst,", "tokens": ["Der", "Du", ",", "was", "uns", "not", "ist", ",", "schenkst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Du Dein Gesch\u00f6pfe liebest,", "tokens": ["Der", "Du", "Dein", "Ge\u00b7sch\u00f6p\u00b7fe", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Allem Fleische Speise giebest,", "tokens": ["Al\u00b7lem", "Flei\u00b7sche", "Spei\u00b7se", "gie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles n\u00e4r\u2019st, erh\u00e4lt\u2019st und tr\u00e4nkst;", "tokens": ["Al\u00b7les", "n\u00e4r'st", ",", "er\u00b7h\u00e4lt'st", "und", "tr\u00e4nkst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gib, da\u00df wir an diesen Gaben", "tokens": ["Gib", ",", "da\u00df", "wir", "an", "die\u00b7sen", "Ga\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie den Leib, die Sel\u2019 auch laben!", "tokens": ["Wie", "den", "Leib", ",", "die", "Sel'", "auch", "la\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "La\u00df uns stets mit Dankbarkeit,", "tokens": ["La\u00df", "uns", "stets", "mit", "Dank\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Nebst des Essens Narungs-Segen,", "tokens": ["Nebst", "des", "Es\u00b7sens", "Na\u00b7rungs\u00b7Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Des Geschmacks Verschiedenheit", "tokens": ["Des", "Ge\u00b7schmacks", "Ver\u00b7schie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "In der Speis\u2019 und Zung\u2019 erwegen!", "tokens": ["In", "der", "Speis'", "und", "Zung'", "er\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Jeden Bissen, den wir schmecken,", "tokens": ["Je\u00b7den", "Bis\u00b7sen", ",", "den", "wir", "schme\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "La\u00df uns Deine Huld entdecken!", "tokens": ["La\u00df", "uns", "Dei\u00b7ne", "Huld", "ent\u00b7de\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "So gereichet Trank und Speise", "tokens": ["So", "ge\u00b7rei\u00b7chet", "Trank", "und", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Uns zur Freude, Dir zum Preise.", "tokens": ["Uns", "zur", "Freu\u00b7de", ",", "Dir", "zum", "Prei\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}