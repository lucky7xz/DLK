{"dta.poem.21191": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie toben nicht des Meeres Wogen,", "tokens": ["Wie", "to\u00b7ben", "nicht", "des", "Mee\u00b7res", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Dunkel den Olymp umzogen,", "tokens": ["Wenn", "Dun\u00b7kel", "den", "O\u00b7lymp", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und Donner auf den Fluthen br\u00fcllt!", "tokens": ["Und", "Don\u00b7ner", "auf", "den", "Flut\u00b7hen", "br\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wie viel wilder ist der Leidenschaften", "tokens": ["Doch", "wie", "viel", "wil\u00b7der", "ist", "der", "Lei\u00b7den\u00b7schaf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIAT", "ADJA", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "W\u00fcten,\nWenn Ernst und Weisheit nicht gebieten,\nUnd Tugend ihren Aufruhr stillt.", "tokens": ["W\u00fc\u00b7ten", ",", "Wenn", "Ernst", "und", "Weis\u00b7heit", "nicht", "ge\u00b7bie\u00b7ten", ",", "Und", "Tu\u00b7gend", "ih\u00b7ren", "Auf\u00b7ruhr", "stillt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "NE", "KON", "NN", "PTKNEG", "VVINF", "$,", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.invert"}}}}}