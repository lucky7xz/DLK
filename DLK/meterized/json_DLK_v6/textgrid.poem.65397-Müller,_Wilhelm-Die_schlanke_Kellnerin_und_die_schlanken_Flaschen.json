{"textgrid.poem.65397": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die schlanke Kellnerin und die schlanken Flaschen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blanke, schlanke Kellnerin,", "tokens": ["Blan\u00b7ke", ",", "schlan\u00b7ke", "Kell\u00b7ne\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blank und schlank sind deine Flaschen,", "tokens": ["Blank", "und", "schlank", "sind", "dei\u00b7ne", "Fla\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blanker, schlanker ist dein Leib.", "tokens": ["Blan\u00b7ker", ",", "schlan\u00b7ker", "ist", "dein", "Leib", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mich trinken, la\u00df mich naschen", "tokens": ["La\u00df", "mich", "trin\u00b7ken", ",", "la\u00df", "mich", "na\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sorgenbann und Leidvertreib!", "tokens": ["Sor\u00b7gen\u00b7bann", "und", "Leid\u00b7ver\u00b7treib", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blanke, schlanke Kellnerin,", "tokens": ["Blan\u00b7ke", ",", "schlan\u00b7ke", "Kell\u00b7ne\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zum Umspannen ist dein Mieder", "tokens": ["Zum", "Um\u00b7span\u00b7nen", "ist", "dein", "Mie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit vier Fingern ohn' Beschwer.", "tokens": ["Mit", "vier", "Fin\u00b7gern", "ohn'", "Be\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fclle mir den Schoppen wieder!", "tokens": ["F\u00fcl\u00b7le", "mir", "den", "Schop\u00b7pen", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit vier Z\u00fcgen ist er leer.", "tokens": ["Mit", "vier", "Z\u00fc\u00b7gen", "ist", "er", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Blanke, schlanke Kellnerin,", "tokens": ["Blan\u00b7ke", ",", "schlan\u00b7ke", "Kell\u00b7ne\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlanke Leibchen hab' ich gerne,", "tokens": ["Schlan\u00b7ke", "Leib\u00b7chen", "hab'", "ich", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber schlanke Flaschen nicht,", "tokens": ["A\u00b7ber", "schlan\u00b7ke", "Fla\u00b7schen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dank dem durstig hei\u00dfen Sterne,", "tokens": ["Dank", "dem", "durs\u00b7tig", "hei\u00b7\u00dfen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter dem ich trat an's Licht.", "tokens": ["Un\u00b7ter", "dem", "ich", "trat", "an's", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Blanke, schlanke Kellnerin,", "tokens": ["Blan\u00b7ke", ",", "schlan\u00b7ke", "Kell\u00b7ne\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fordr' ich doch den schlanksten Schoppen,", "tokens": ["Fordr'", "ich", "doch", "den", "schlanks\u00b7ten", "Schop\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sage nicht, ich sei ein Thor.", "tokens": ["Sa\u00b7ge", "nicht", ",", "ich", "sei", "ein", "Thor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn er zaubert, mich zu foppen,", "tokens": ["Denn", "er", "zau\u00b7bert", ",", "mich", "zu", "fop\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinen schlanken Leib mir vor.", "tokens": ["Dei\u00b7nen", "schlan\u00b7ken", "Leib", "mir", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Blanke, schlanke Kellnerin,", "tokens": ["Blan\u00b7ke", ",", "schlan\u00b7ke", "Kell\u00b7ne\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlanke Flaschen dir behagen,", "tokens": ["Schlan\u00b7ke", "Fla\u00b7schen", "dir", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ihr Glas auch leicht zerbricht:", "tokens": ["Ob", "ihr", "Glas", "auch", "leicht", "zer\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlanke Leibchen, la\u00df dir sagen,", "tokens": ["Schlan\u00b7ke", "Leib\u00b7chen", ",", "la\u00df", "dir", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Knacken wohl, doch brechen nicht.", "tokens": ["Kna\u00b7cken", "wohl", ",", "doch", "bre\u00b7chen", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Blanke, schlanke Kellnerin,", "tokens": ["Blan\u00b7ke", ",", "schlan\u00b7ke", "Kell\u00b7ne\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl bekomm' es deinen Kannen,", "tokens": ["Wohl", "be\u00b7komm'", "es", "dei\u00b7nen", "Kan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df so schnell mein Schoppen leer!", "tokens": ["Da\u00df", "so", "schnell", "mein", "Schop\u00b7pen", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf ich deinen Leib umspannen,", "tokens": ["Darf", "ich", "dei\u00b7nen", "Leib", "um\u00b7span\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mess' ich keine Flasche mehr.", "tokens": ["Mess'", "ich", "kei\u00b7ne", "Fla\u00b7sche", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}