{"textgrid.poem.25276": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus Rauhreif ragt ein Gartenhaus,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Rauhreif ragt ein Gartenhaus,", "tokens": ["Aus", "Rauh\u00b7reif", "ragt", "ein", "Gar\u00b7ten\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schaut so schmuck, so sonnig aus.", "tokens": ["Das", "schaut", "so", "schmuck", ",", "so", "son\u00b7nig", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "An blanken Giebel schmiegt sich hold", "tokens": ["An", "blan\u00b7ken", "Gie\u00b7bel", "schmiegt", "sich", "hold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wintersonne Abendgold.", "tokens": ["Der", "Win\u00b7ter\u00b7son\u00b7ne", "A\u00b7bend\u00b7gold", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Eiszapfen, Scheiben in rotem Glanz,", "tokens": ["Eis\u00b7zap\u00b7fen", ",", "Schei\u00b7ben", "in", "ro\u00b7tem", "Glanz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die Fenster umrahmt von Waldmooskranz.", "tokens": ["Die", "Fens\u00b7ter", "um\u00b7rahmt", "von", "Wald\u00b7moos\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Blattgr\u00fcn, Gelbkrokus, ein rosiger Bube", "tokens": ["Blatt\u00b7gr\u00fcn", ",", "Gelb\u00b7kro\u00b7kus", ",", "ein", "ro\u00b7si\u00b7ger", "Bu\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "L\u00e4cheln aus fr\u00fchlingswarmer Stube.", "tokens": ["L\u00e4\u00b7cheln", "aus", "fr\u00fch\u00b7lings\u00b7war\u00b7mer", "Stu\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Kanarienvogel schmettert so hell,", "tokens": ["Ka\u00b7na\u00b7ri\u00b7en\u00b7vo\u00b7gel", "schmet\u00b7tert", "so", "hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kinderlachen und Hundegebell.", "tokens": ["Kin\u00b7der\u00b7la\u00b7chen", "und", "Hun\u00b7de\u00b7ge\u00b7bell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Klein Hansemann und Ami spielen", "tokens": ["Klein", "Han\u00b7se\u00b7mann", "und", "A\u00b7mi", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wolfsjagd, sie balgen sich auf den Dielen.", "tokens": ["Wolfs\u00b7jagd", ",", "sie", "bal\u00b7gen", "sich", "auf", "den", "Die\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Die Mutter ging holen den Weihnachtsmann.", "tokens": ["Die", "Mut\u00b7ter", "ging", "ho\u00b7len", "den", "Weih\u00b7nachts\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der klopft an die T\u00fcre brummend an.", "tokens": ["Der", "klopft", "an", "die", "T\u00fc\u00b7re", "brum\u00b7mend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Und sieh, vermummt, ein b\u00e4rtiger Greis,", "tokens": ["Und", "sieh", ",", "ver\u00b7mummt", ",", "ein", "b\u00e4r\u00b7ti\u00b7ger", "Greis", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein Sack voll N\u00fcsse, ein Tannenreis.", "tokens": ["Ein", "Sack", "voll", "N\u00fcs\u00b7se", ",", "ein", "Tan\u00b7nen\u00b7reis", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbseid ihr auch artig?\u00ab Stumm nicken die Kleinen", "tokens": ["\u00bb", "seid", "ihr", "auch", "ar\u00b7tig", "?", "\u00ab", "Stumm", "ni\u00b7cken", "die", "Klei\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "ADV", "ADJD", "$.", "$(", "NE", "VVFIN", "ART", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und reichen die Patschhand; eins m\u00f6chte weinen.", "tokens": ["Und", "rei\u00b7chen", "die", "Patschhand", ";", "eins", "m\u00f6ch\u00b7te", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Da prasseln die N\u00fcsse, das gibt ein Haschen!", "tokens": ["Da", "pras\u00b7seln", "die", "N\u00fcs\u00b7se", ",", "das", "gibt", "ein", "Ha\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der s\u00fc\u00dfe Hagel f\u00fcllt die Taschen ...", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Ha\u00b7gel", "f\u00fcllt", "die", "Ta\u00b7schen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Fort ist der Mann. Mit Lampenschein", "tokens": ["Fort", "ist", "der", "Mann", ".", "Mit", "Lam\u00b7pen\u00b7schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tritt nun die liebe Mutter herein.", "tokens": ["Tritt", "nun", "die", "lie\u00b7be", "Mut\u00b7ter", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "Gejubel: \u00bbDer Weihnachtsmann war da!", "tokens": ["Ge\u00b7ju\u00b7bel", ":", "\u00bb", "Der", "Weih\u00b7nachts\u00b7mann", "war", "da", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O, N\u00fcsse hat er gebracht, Mama!\u00ab", "tokens": ["O", ",", "N\u00fcs\u00b7se", "hat", "er", "ge\u00b7bracht", ",", "Ma\u00b7ma", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "$,", "NN", "VAFIN", "PPER", "VVPP", "$,", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Den gro\u00dfen Tisch umringt ein Schwatzen,", "tokens": ["Den", "gro\u00b7\u00dfen", "Tisch", "um\u00b7ringt", "ein", "Schwat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schalenknacken, behaglich Schmatzen.", "tokens": ["Scha\u00b7len\u00b7kna\u00b7cken", ",", "be\u00b7hag\u00b7lich", "Schmat\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Die Mutter klatscht in die H\u00e4nde und zieht", "tokens": ["Die", "Mut\u00b7ter", "klatscht", "in", "die", "H\u00e4n\u00b7de", "und", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Spieluhr auf: \u00bbNun singt ein Lied!\u00ab", "tokens": ["Die", "Spie\u00b7luhr", "auf", ":", "\u00bb", "Nun", "singt", "ein", "Lied", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00bbihr Kinderlein kommet, o kommet doch all,", "tokens": ["\u00bb", "ihr", "Kin\u00b7der\u00b7lein", "kom\u00b7met", ",", "o", "kom\u00b7met", "doch", "all", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "$,", "FM", "VVFIN", "ADV", "PIAT", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zur Krippe her kommet, in Bethlehems Stall!\u00ab", "tokens": ["Zur", "Krip\u00b7pe", "her", "kom\u00b7met", ",", "in", "Beth\u00b7le\u00b7hems", "Stall", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "APZR", "VVFIN", "$,", "APPR", "NE", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.16": {"line.1": {"text": "Fromm t\u00f6nt's in die frostige Nacht hinaus.", "tokens": ["Fromm", "t\u00f6nt's", "in", "die", "fros\u00b7ti\u00b7ge", "Nacht", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Stern steht selig \u00fcber dem Haus.", "tokens": ["Ein", "Stern", "steht", "se\u00b7lig", "\u00fc\u00b7ber", "dem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}