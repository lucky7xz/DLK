{"textgrid.poem.27824": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Gl\u00fccke, theures Paar, sey zinsbar eurem Willen", "tokens": ["Das", "Gl\u00fc\u00b7cke", ",", "theu\u00b7res", "Paar", ",", "sey", "zins\u00b7bar", "eu\u00b7rem", "Wil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,", "VAFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und m\u00fc\u00df euch Haus und Hof mit Heil und Segen f\u00fcllen;", "tokens": ["Und", "m\u00fc\u00df", "euch", "Haus", "und", "Hof", "mit", "Heil", "und", "Se\u00b7gen", "f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Segen, der da bleibt, wenn alle Zeit verlaufft,", "tokens": ["Mit", "Se\u00b7gen", ",", "der", "da", "bleibt", ",", "wenn", "al\u00b7le", "Zeit", "ver\u00b7laufft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Segen, den man da erst nennt, wenn man ihn taufft.", "tokens": ["Mit", "Se\u00b7gen", ",", "den", "man", "da", "erst", "nennt", ",", "wenn", "man", "ihn", "taufft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIS", "ADV", "ADV", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}