{"textgrid.poem.61681": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den K\u00fcnstler frag' am fert'gen Werke:", "tokens": ["Den", "K\u00fcnst\u00b7ler", "frag'", "am", "fert'\u00b7gen", "Wer\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu scheiden wei\u00df er's nimmerdar,", "tokens": ["Zu", "schei\u00b7den", "wei\u00df", "er's", "nim\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wieviel er schuf aus freier St\u00e4rke,", "tokens": ["Wie\u00b7viel", "er", "schuf", "aus", "frei\u00b7er", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wieviel ein hold Empfangen war.", "tokens": ["Wie\u00b7viel", "ein", "hold", "Emp\u00b7fan\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}