{"dta.poem.21982": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "83. Wie kan man GOttes geniessen?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist ein Einges Ein/ wer seiner wil geniessen/", "tokens": ["Gott", "ist", "ein", "Ein\u00b7ges", "Ein", "/", "wer", "sei\u00b7ner", "wil", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "$(", "PWS", "PPOSAT", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df sich nicht weniger als Er/ in Jhn einschliessen.", "tokens": ["Mu\u00df", "sich", "nicht", "we\u00b7ni\u00b7ger", "als", "Er", "/", "in", "Jhn", "ein\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PTKNEG", "ADV", "KOUS", "PPER", "$(", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}