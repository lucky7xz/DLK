{"textgrid.poem.60454": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Welke Rose", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Buche bl\u00e4tternd, fand", "tokens": ["In", "ei\u00b7nem", "Bu\u00b7che", "bl\u00e4t\u00b7ternd", ",", "fand"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich eine Rose welk, zerdr\u00fcckt,", "tokens": ["Ich", "ei\u00b7ne", "Ro\u00b7se", "welk", ",", "zer\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wei\u00df auch nicht mehr, wessen Hand", "tokens": ["Und", "wei\u00df", "auch", "nicht", "mehr", ",", "wes\u00b7sen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADV", "$,", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie einst f\u00fcr mich gepfl\u00fcckt.", "tokens": ["Sie", "einst", "f\u00fcr", "mich", "ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, mehr und mehr im Abendhauch", "tokens": ["Ach", ",", "mehr", "und", "mehr", "im", "A\u00b7bend\u00b7hauch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verweht Erinnrung; bald zerstiebt", "tokens": ["Ver\u00b7weht", "E\u00b7rinn\u00b7rung", ";", "bald", "zer\u00b7stiebt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Erdenlos, dann wei\u00df ich auch", "tokens": ["Mein", "Er\u00b7den\u00b7los", ",", "dann", "wei\u00df", "ich", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht mehr, wer mich geliebt.", "tokens": ["Nicht", "mehr", ",", "wer", "mich", "ge\u00b7liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}