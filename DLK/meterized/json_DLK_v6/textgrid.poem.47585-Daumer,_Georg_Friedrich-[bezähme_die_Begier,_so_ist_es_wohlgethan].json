{"textgrid.poem.47585": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[bez\u00e4hme die Begier, so ist es wohlgethan]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bez\u00e4hme die Begier, so ist es wohlgethan;", "tokens": ["Be\u00b7z\u00e4h\u00b7me", "die", "Be\u00b7gier", ",", "so", "ist", "es", "wohl\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leb' als ein Engel schier, so ist es wohlgethan.", "tokens": ["Leb'", "als", "ein", "En\u00b7gel", "schier", ",", "so", "ist", "es", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOKOM", "ART", "NN", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wei\u00df ich einen Rath, der ist noch k\u00f6stlicher:", "tokens": ["Doch", "wei\u00df", "ich", "ei\u00b7nen", "Rath", ",", "der", "ist", "noch", "k\u00f6st\u00b7li\u00b7cher", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Verkaufe dein Brevier, so ist es wohlgethan.", "tokens": ["Ver\u00b7kau\u00b7fe", "dein", "Bre\u00b7vier", ",", "so", "ist", "es", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bei Weinpokal und Fl\u00f6tenhauch und Tamburin", "tokens": ["Bei", "Wein\u00b7po\u00b7kal", "und", "Fl\u00f6\u00b7ten\u00b7hauch", "und", "Tam\u00b7bu\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufschlage dein Quartier, so ist es wohlgethan.", "tokens": ["Auf\u00b7schla\u00b7ge", "dein", "Quar\u00b7tier", ",", "so", "ist", "es", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In offnem Aufruhr wider alle Heuchelei", "tokens": ["In", "off\u00b7nem", "Auf\u00b7ruhr", "wi\u00b7der", "al\u00b7le", "Heu\u00b7che\u00b7lei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufstecke dein Panier, so ist es wohlgethan.", "tokens": ["Auf\u00b7ste\u00b7cke", "dein", "Pa\u00b7nier", ",", "so", "ist", "es", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es bete fromm der Eine da, der Andre dort", "tokens": ["Es", "be\u00b7te", "fromm", "der", "Ei\u00b7ne", "da", ",", "der", "And\u00b7re", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ART", "ADV", "$,", "PRELS", "PIS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und denke: \u00bbBet' ich hier, so ist es wohlgethan.\u00ab", "tokens": ["Und", "den\u00b7ke", ":", "\u00bb", "Bet'", "ich", "hier", ",", "so", "ist", "es", "wohl\u00b7ge\u00b7than", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Knie'n ", "tokens": ["Knie'n"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Vor sch\u00f6nem Auge ", "tokens": ["Vor", "sch\u00f6\u00b7nem", "Au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Nein, denke nicht: \u00bbBehandl' ich ihn in frostiger,", "tokens": ["Nein", ",", "den\u00b7ke", "nicht", ":", "\u00bb", "Be\u00b7handl'", "ich", "ihn", "in", "fros\u00b7ti\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PTKNEG", "$.", "$(", "VVIMP", "PPER", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unfreundlicher Manier, so ist es wohlgethan!\u00ab", "tokens": ["Un\u00b7freund\u00b7li\u00b7cher", "Ma\u00b7nier", ",", "so", "ist", "es", "wohl\u00b7ge\u00b7than", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Preisw\u00fcrdig ist die Milde nur, es werd', o Kind,", "tokens": ["Preis\u00b7w\u00fcr\u00b7dig", "ist", "die", "Mil\u00b7de", "nur", ",", "es", "werd'", ",", "o", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "$,", "PPER", "VAFIN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Ku\u00df der Milde mir, so ist es wohlgethan.", "tokens": ["Ein", "Ku\u00df", "der", "Mil\u00b7de", "mir", ",", "so", "ist", "es", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Zwar wenn du dich in doppelt dichte Schleier h\u00fcllst,", "tokens": ["Zwar", "wenn", "du", "dich", "in", "dop\u00b7pelt", "dich\u00b7te", "Schlei\u00b7er", "h\u00fcllst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "APPR", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Naht das Gemeine dir, so ist es wohlgethan.", "tokens": ["Naht", "das", "Ge\u00b7mei\u00b7ne", "dir", ",", "so", "ist", "es", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Entflorst du aber lichtgebornem Dichterblick", "tokens": ["Ent\u00b7florst", "du", "a\u00b7ber", "licht\u00b7ge\u00b7bor\u00b7nem", "Dich\u00b7ter\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "All deine s\u00fc\u00dfe Zier, so ist es wohlgethan.", "tokens": ["All", "dei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Zier", ",", "so", "ist", "es", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}