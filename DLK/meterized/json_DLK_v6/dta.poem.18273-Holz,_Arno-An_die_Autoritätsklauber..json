{"dta.poem.18273": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An die Autorit\u00e4tsklauber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Auch ohne den modischen Materialismus,", "tokens": ["Auch", "oh\u00b7ne", "den", "mo\u00b7di\u00b7schen", "Ma\u00b7te\u00b7ri\u00b7a\u00b7lis\u00b7mus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So alt wie diese alte Welt", "tokens": ["So", "alt", "wie", "die\u00b7se", "al\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Drum poltert nur, poltert: Bezuckerter Mist!", "tokens": ["Drum", "pol\u00b7tert", "nur", ",", "pol\u00b7tert", ":", "Be\u00b7zu\u00b7cker\u00b7ter", "Mist", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$,", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er f\u00fcrchtet nicht eure kritischen Besen,", "tokens": ["Er", "f\u00fcrch\u00b7tet", "nicht", "eu\u00b7re", "kri\u00b7ti\u00b7schen", "Be\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ist doch der erste \u201eNaturalist\u201c", "tokens": ["Ist", "doch", "der", "ers\u00b7te", "\u201e", "Na\u00b7tu\u00b7ra\u00b7list", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$(", "NN", "$("], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schon der alte Vater Homer gewesen!", "tokens": ["Schon", "der", "al\u00b7te", "Va\u00b7ter", "Ho\u00b7mer", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NE", "VAPP", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}}}}