{"textgrid.poem.56241": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die drei Sonnen", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wallte so silbernen Scheines", "tokens": ["Es", "wall\u00b7te", "so", "sil\u00b7ber\u00b7nen", "Schei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nicht immer mein lockiges Haar,", "tokens": ["Nicht", "im\u00b7mer", "mein", "lo\u00b7cki\u00b7ges", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hat ja Zeiten gegeben,", "tokens": ["Es", "hat", "ja", "Zei\u00b7ten", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo selber ich jung auch war.", "tokens": ["Wo", "sel\u00b7ber", "ich", "jung", "auch", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADJD", "ADV", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und blick ich dich an, o M\u00e4dchen,", "tokens": ["Und", "blick", "ich", "dich", "an", ",", "o", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PRF", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So rosig und heiter und jung,", "tokens": ["So", "ro\u00b7sig", "und", "hei\u00b7ter", "und", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da taucht aus vergangenen Zeiten", "tokens": ["Da", "taucht", "aus", "ver\u00b7gan\u00b7ge\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Herauf die Erinnerung.", "tokens": ["Her\u00b7auf", "die", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Mutter von deiner Mutter \u2013", "tokens": ["Die", "Mut\u00b7ter", "von", "dei\u00b7ner", "Mut\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch sah ich die Sch\u00f6nere nicht,", "tokens": ["Noch", "sah", "ich", "die", "Sch\u00f6\u00b7ne\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich staunte sie an, wie die Sonne,", "tokens": ["Ich", "staun\u00b7te", "sie", "an", ",", "wie", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Geblendet von ihrem Licht.", "tokens": ["Ge\u00b7blen\u00b7det", "von", "ih\u00b7rem", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und einst durchbebte mit Wonne", "tokens": ["Und", "einst", "durch\u00b7beb\u00b7te", "mit", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Druck mich von ihrer Hand,", "tokens": ["Der", "Druck", "mich", "von", "ih\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie neigte darauf sich dem andern,", "tokens": ["Sie", "neig\u00b7te", "da\u00b7rauf", "sich", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PRF", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da zog ich ins fremde Land.", "tokens": ["Da", "zog", "ich", "ins", "frem\u00b7de", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Sp\u00e4t kehrt ich zur\u00fcck in die Heimat,", "tokens": ["Sp\u00e4t", "kehrt", "ich", "zu\u00b7r\u00fcck", "in", "die", "Hei\u00b7mat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein M\u00fcder nach irrem Lauf,", "tokens": ["Ein", "M\u00fc\u00b7der", "nach", "ir\u00b7rem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es stieg am heimischen Himmel", "tokens": ["Es", "stieg", "am", "hei\u00b7mi\u00b7schen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die andere Sonne schon auf.", "tokens": ["Die", "an\u00b7de\u00b7re", "Son\u00b7ne", "schon", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ja deine Mutter, o M\u00e4dchen, \u2013", "tokens": ["Ja", "dei\u00b7ne", "Mut\u00b7ter", ",", "o", "M\u00e4d\u00b7chen", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch sah ich die Sch\u00f6nere nicht,", "tokens": ["Noch", "sah", "ich", "die", "Sch\u00f6\u00b7ne\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich staunte sie an, wie die Sonne,", "tokens": ["Ich", "staun\u00b7te", "sie", "an", ",", "wie", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Geblendet von ihrem Licht.", "tokens": ["Ge\u00b7blen\u00b7det", "von", "ih\u00b7rem", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Sie reichte mir einst die Stirne", "tokens": ["Sie", "reich\u00b7te", "mir", "einst", "die", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zum Kusse, da zittert ich sehr,", "tokens": ["Zum", "Kus\u00b7se", ",", "da", "zit\u00b7tert", "ich", "sehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie neigte darauf sich dem andern,", "tokens": ["Sie", "neig\u00b7te", "da\u00b7rauf", "sich", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PRF", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da zog ich \u00fcber das Meer.", "tokens": ["Da", "zog", "ich", "\u00fc\u00b7ber", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Ich habe vertr\u00e4umt und vertrauert", "tokens": ["Ich", "ha\u00b7be", "ver\u00b7tr\u00e4umt", "und", "ver\u00b7trau\u00b7ert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mein Leben, ich bin ein Greis,", "tokens": ["Mein", "Le\u00b7ben", ",", "ich", "bin", "ein", "Greis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Heim kehr ich, die dritte Sonne", "tokens": ["Heim", "kehr", "ich", ",", "die", "drit\u00b7te", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erleuchtet den Himmelskreis.", "tokens": ["Er\u00b7leuch\u00b7tet", "den", "Him\u00b7mels\u00b7kreis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Du bist es, o Wonnereiche;", "tokens": ["Du", "bist", "es", ",", "o", "Won\u00b7ne\u00b7rei\u00b7che", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Noch sah ich die Sch\u00f6nere nicht,", "tokens": ["Noch", "sah", "ich", "die", "Sch\u00f6\u00b7ne\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich schaue dich an, wie die Sonne,", "tokens": ["Ich", "schau\u00b7e", "dich", "an", ",", "wie", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Geblendet von deinem Licht.", "tokens": ["Ge\u00b7blen\u00b7det", "von", "dei\u00b7nem", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Du reichst mir zum Kusse die Lippen,", "tokens": ["Du", "reichst", "mir", "zum", "Kus\u00b7se", "die", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mitleidig mir wohl zu tun,", "tokens": ["Mit\u00b7lei\u00b7dig", "mir", "wohl", "zu", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und neigst dich dem andern, ich gehe", "tokens": ["Und", "neigst", "dich", "dem", "an\u00b7dern", ",", "ich", "ge\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "$,", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bald unter die Erde, zu ruhn.", "tokens": ["Bald", "un\u00b7ter", "die", "Er\u00b7de", ",", "zu", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}