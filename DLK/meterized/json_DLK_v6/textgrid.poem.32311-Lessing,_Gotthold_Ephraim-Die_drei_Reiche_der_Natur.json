{"textgrid.poem.32311": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die drei Reiche der Natur", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich trink', und trinkend f\u00e4llt mir bei,", "tokens": ["Ich", "trink'", ",", "und", "trin\u00b7kend", "f\u00e4llt", "mir", "bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum Naturreich dreifach sei.", "tokens": ["Wa\u00b7rum", "Na\u00b7tur\u00b7reich", "drei\u00b7fach", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Tier' und Menschen trinken, lieben,", "tokens": ["Die", "Tier'", "und", "Men\u00b7schen", "trin\u00b7ken", ",", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jegliches nach seinen Trieben:", "tokens": ["Ein", "jeg\u00b7li\u00b7ches", "nach", "sei\u00b7nen", "Trie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Delphin und Adler, Floh und Hund", "tokens": ["Del\u00b7phin", "und", "Ad\u00b7ler", ",", "Floh", "und", "Hund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Empfindet Lieb', und netzt den Mund.", "tokens": ["Emp\u00b7fin\u00b7det", "Lieb'", ",", "und", "netzt", "den", "Mund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was also trinkt und lieben kann,", "tokens": ["Was", "al\u00b7so", "trinkt", "und", "lie\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird in das erste Reich getan.", "tokens": ["Wird", "in", "das", "ers\u00b7te", "Reich", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Pflanze macht das zweite Reich,", "tokens": ["Die", "Pflan\u00b7ze", "macht", "das", "zwei\u00b7te", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem ersten nicht an G\u00fcte gleich:", "tokens": ["Dem", "ers\u00b7ten", "nicht", "an", "G\u00fc\u00b7te", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie liebet nicht, doch kann sie trinken;", "tokens": ["Sie", "lie\u00b7bet", "nicht", ",", "doch", "kann", "sie", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Wolken tr\u00e4ufelnd niedersinken,", "tokens": ["Wenn", "Wol\u00b7ken", "tr\u00e4u\u00b7felnd", "nie\u00b7der\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So trinkt die Zeder und der Klee,", "tokens": ["So", "trinkt", "die", "Ze\u00b7der", "und", "der", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Weinstock und die Aloe.", "tokens": ["Der", "Wein\u00b7stock", "und", "die", "A\u00b7loe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum, was nicht liebt, doch trinken kann,", "tokens": ["Drum", ",", "was", "nicht", "liebt", ",", "doch", "trin\u00b7ken", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRELS", "PTKNEG", "VVFIN", "$,", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird in das zweite Reich getan.", "tokens": ["Wird", "in", "das", "zwei\u00b7te", "Reich", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Steinreich macht das dritte Reich;", "tokens": ["Das", "Stein\u00b7reich", "macht", "das", "drit\u00b7te", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hier sind Sand und Demant gleich:", "tokens": ["Und", "hier", "sind", "Sand", "und", "De\u00b7mant", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Stein f\u00fchlt Durst und zarte Triebe,", "tokens": ["Kein", "Stein", "f\u00fchlt", "Durst", "und", "zar\u00b7te", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er w\u00e4chset ohne Trunk und Liebe.", "tokens": ["Er", "w\u00e4ch\u00b7set", "oh\u00b7ne", "Trunk", "und", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum, was nicht hebt noch trinken kann,", "tokens": ["Drum", ",", "was", "nicht", "hebt", "noch", "trin\u00b7ken", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRELS", "PTKNEG", "VVFIN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird in das letzte Reich getan.", "tokens": ["Wird", "in", "das", "letz\u00b7te", "Reich", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn ohne Lieb' und ohne Wein,", "tokens": ["Denn", "oh\u00b7ne", "Lieb'", "und", "oh\u00b7ne", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sprich, Mensch, was bleibst du noch? \u2013 \u2013 Ein Stein.", "tokens": ["Sprich", ",", "Mensch", ",", "was", "bleibst", "du", "noch", "?", "\u2013", "\u2013", "Ein", "Stein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$.", "$(", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}