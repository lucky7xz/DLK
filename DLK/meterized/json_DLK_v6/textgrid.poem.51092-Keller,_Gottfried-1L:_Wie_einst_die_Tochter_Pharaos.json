{"textgrid.poem.51092": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie einst die Tochter Pharaos", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie einst die Tochter Pharaos", "tokens": ["Wie", "einst", "die", "Toch\u00b7ter", "Pha\u00b7raos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Im gr\u00fcnen Schilf des Niles ging,", "tokens": ["Im", "gr\u00fc\u00b7nen", "Schilf", "des", "Ni\u00b7les", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Auge hell, verwundrungsgro\u00df", "tokens": ["Des", "Au\u00b7ge", "hell", ",", "ver\u00b7wund\u00b7rungs\u00b7gro\u00df"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An ihren dunkeln Augen hing;", "tokens": ["An", "ih\u00b7ren", "dun\u00b7keln", "Au\u00b7gen", "hing", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sie ihr Haupt, das goldumreifte,", "tokens": ["Wie", "sie", "ihr", "Haupt", ",", "das", "gold\u00b7um\u00b7reif\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sehns\u00fcchtig leicht flut\u00fcber bog,", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "leicht", "flut\u00b7\u00fc\u00b7ber", "bog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um ihren Fu\u00df das Wasser schweifte", "tokens": ["Um", "ih\u00b7ren", "Fu\u00df", "das", "Was\u00b7ser", "schweif\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und silberne Ringe zog:", "tokens": ["Und", "sil\u00b7ber\u00b7ne", "Rin\u00b7ge", "zog", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "So seh ich dich, du tr\u00e4umrisch Kind,", "tokens": ["So", "seh", "ich", "dich", ",", "du", "tr\u00e4um\u00b7risch", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am abendlichen Rheine stehn,", "tokens": ["Am", "a\u00b7bend\u00b7li\u00b7chen", "Rhei\u00b7ne", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wo seine sch\u00f6nsten Borde sind", "tokens": ["Wo", "sei\u00b7ne", "sch\u00f6ns\u00b7ten", "Bor\u00b7de", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine gr\u00fcnsten Wellen gehn.", "tokens": ["Und", "sei\u00b7ne", "gr\u00fcns\u00b7ten", "Wel\u00b7len", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schwarz sind dein Aug und deine Haare,", "tokens": ["Schwarz", "sind", "dein", "Aug", "und", "dei\u00b7ne", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und deine Magd, die Sonne, flicht", "tokens": ["Und", "dei\u00b7ne", "Magd", ",", "die", "Son\u00b7ne", ",", "flicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dar\u00fcber eine wunderbare", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "ei\u00b7ne", "wun\u00b7der\u00b7ba\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Krone von Abendlicht.", "tokens": ["Kro\u00b7ne", "von", "A\u00b7bend\u00b7licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ich aber wandle im Gestein", "tokens": ["Ich", "a\u00b7ber", "wand\u00b7le", "im", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wolkenhoch auf schmalem Steg,", "tokens": ["Und", "wol\u00b7ken\u00b7hoch", "auf", "schma\u00b7lem", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Abgrund sch\u00e4umt der wei\u00dfe Rhein,", "tokens": ["Im", "Ab\u00b7grund", "sch\u00e4umt", "der", "wei\u00b7\u00dfe", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Via mala hei\u00dft mein Weg!", "tokens": ["Und", "Via", "ma\u00b7la", "hei\u00dft", "mein", "Weg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dir gilt das Tosen in den Kl\u00fcften,", "tokens": ["Dir", "gilt", "das", "To\u00b7sen", "in", "den", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach dir schreit dieses Tannenwehn,", "tokens": ["Nach", "dir", "schreit", "die\u00b7ses", "Tan\u00b7nen\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Bis hoch aus kalten Eisesl\u00fcften", "tokens": ["Bis", "hoch", "aus", "kal\u00b7ten", "Ei\u00b7ses\u00b7l\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Wasser jenseits niedergehn!", "tokens": ["Die", "Was\u00b7ser", "jen\u00b7seits", "nie\u00b7der\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}