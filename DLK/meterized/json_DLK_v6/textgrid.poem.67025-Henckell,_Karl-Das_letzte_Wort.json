{"textgrid.poem.67025": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Das letzte Wort", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Triumph der Schwindler und der Narren!", "tokens": ["Tri\u00b7umph", "der", "Schwind\u00b7ler", "und", "der", "Nar\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der freche Wahnsinn wuchert fort.", "tokens": ["Der", "fre\u00b7che", "Wahn\u00b7sinn", "wu\u00b7chert", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Macht dich das Bild der Welt erstarren?", "tokens": ["Macht", "dich", "das", "Bild", "der", "Welt", "er\u00b7star\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Liebe, du Erl\u00f6sungswort!", "tokens": ["O", "Lie\u00b7be", ",", "du", "Er\u00b7l\u00f6\u00b7sungs\u00b7wort", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}