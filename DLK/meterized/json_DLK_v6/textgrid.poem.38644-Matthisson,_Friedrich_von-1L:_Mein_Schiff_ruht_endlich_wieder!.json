{"textgrid.poem.38644": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Schiff ruht endlich wieder!", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Schiff ruht endlich wieder!", "tokens": ["Mein", "Schiff", "ruht", "end\u00b7lich", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du, meiner V\u00e4ter Land;", "tokens": ["Du", ",", "mei\u00b7ner", "V\u00e4\u00b7ter", "Land", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich fall' aufs Antliz nieder,", "tokens": ["Ich", "fall'", "aufs", "Ant\u00b7liz", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und k\u00fcsse deinen Strand!", "tokens": ["Und", "k\u00fcs\u00b7se", "dei\u00b7nen", "Strand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Froh werd' ich die Alt\u00e4re", "tokens": ["Froh", "werd'", "ich", "die", "Al\u00b7t\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der heimatlichen H\u00f6hn,", "tokens": ["Der", "hei\u00b7mat\u00b7li\u00b7chen", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und froh die Wonnez\u00e4hre", "tokens": ["Und", "froh", "die", "Won\u00b7ne\u00b7z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Jugendfreunde sehn!", "tokens": ["Der", "Ju\u00b7gend\u00b7freun\u00b7de", "sehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und Sie, die schon im Lenze", "tokens": ["Und", "Sie", ",", "die", "schon", "im", "Len\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der goldnen Kinderzeit", "tokens": ["Der", "gold\u00b7nen", "Kin\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich bis zur dunkeln Grenze", "tokens": ["Sich", "bis", "zur", "dun\u00b7keln", "Gren\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Lebens mir geweiht,", "tokens": ["Des", "Le\u00b7bens", "mir", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zum Kampf in Silbert\u00f6nen", "tokens": ["Zum", "Kampf", "in", "Sil\u00b7ber\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Nachruhms mich beseelt,", "tokens": ["Des", "Nach\u00b7ruhms", "mich", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und fr\u00fch mein Herz dem Sch\u00f6nen", "tokens": ["Und", "fr\u00fch", "mein", "Herz", "dem", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und G\u00f6ttlichen verm\u00e4hlt!", "tokens": ["Und", "G\u00f6tt\u00b7li\u00b7chen", "ver\u00b7m\u00e4hlt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie oft, mit Sapphos Feuer,", "tokens": ["Wie", "oft", ",", "mit", "Sap\u00b7phos", "Feu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach Memphis Tempeln schwang", "tokens": ["Nach", "Mem\u00b7phis", "Tem\u00b7peln", "schwang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich durch die Nebelschleier", "tokens": ["Sich", "durch", "die", "Ne\u00b7bel\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Trennung ihr Gesang!", "tokens": ["Der", "Tren\u00b7nung", "ihr", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir wehte Fr\u00fchlingsmilde,", "tokens": ["Mir", "weh\u00b7te", "Fr\u00fch\u00b7lings\u00b7mil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann am geliebten Ring", "tokens": ["Wann", "am", "ge\u00b7lieb\u00b7ten", "Ring"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit ihrem Onyxbilde", "tokens": ["Mit", "ih\u00b7rem", "On\u00b7yx\u00b7bil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Aug' in St\u00fcrmen hieng.", "tokens": ["Mein", "Aug'", "in", "St\u00fcr\u00b7men", "hieng", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Einst schwebt' auf Wogenspizen", "tokens": ["Einst", "schwebt'", "auf", "Wo\u00b7gen\u00b7spi\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Nachtsturm unser Schiff,", "tokens": ["Im", "Nachts\u00b7turm", "un\u00b7ser", "Schiff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, bleich erhellt von Blizen,", "tokens": ["Und", ",", "bleich", "er\u00b7hellt", "von", "Bli\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Droht' ein Korallenriff:", "tokens": ["Droht'", "ein", "Ko\u00b7ral\u00b7len\u00b7riff", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Psycharion! ich k\u00fc\u00dfte", "tokens": ["Psy\u00b7cha\u00b7ri\u00b7on", "!", "ich", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein holdes Zauberbild;", "tokens": ["Dein", "hol\u00b7des", "Zau\u00b7ber\u00b7bild", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schnell war die Wasserw\u00fcste", "tokens": ["Schnell", "war", "die", "Was\u00b7ser\u00b7w\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit G\u00f6tterglanz erf\u00fcllt.", "tokens": ["Mit", "G\u00f6t\u00b7ter\u00b7glanz", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nach Quell und Fruchtbaum schweiften", "tokens": ["Nach", "Quell", "und", "Frucht\u00b7baum", "schweif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir am verla\u00dfnen Strand;", "tokens": ["Wir", "am", "ver\u00b7la\u00df\u00b7nen", "Strand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur wilde Beeren reiften", "tokens": ["Nur", "wil\u00b7de", "Bee\u00b7ren", "reif\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An heisser Kreidewand;", "tokens": ["An", "heis\u00b7ser", "Krei\u00b7de\u00b7wand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Zelt, kein Moosdach rauchte,", "tokens": ["Kein", "Zelt", ",", "kein", "Moos\u00b7dach", "rauch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo kaum ein R\u00e4nkchen schlich;", "tokens": ["Wo", "kaum", "ein", "R\u00e4nk\u00b7chen", "schlich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In Sandgefilde tauchte", "tokens": ["In", "Sand\u00b7ge\u00b7fil\u00b7de", "tauch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Sonn' am Abend sich.", "tokens": ["Die", "Sonn'", "am", "A\u00b7bend", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da war auf Ried und Moose,", "tokens": ["Da", "war", "auf", "Ried", "und", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An rauher Felsenbank,", "tokens": ["An", "rau\u00b7her", "Fel\u00b7sen\u00b7bank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Zeitvertreib die Rose,", "tokens": ["Mein", "Zeit\u00b7ver\u00b7treib", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die deine Z\u00e4hren trank,", "tokens": ["Die", "dei\u00b7ne", "Z\u00e4h\u00b7ren", "trank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als, durch der Mondnacht Schweigen,", "tokens": ["Als", ",", "durch", "der", "Mond\u00b7nacht", "Schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Lorbeerhain du kamst,", "tokens": ["Zum", "Lor\u00b7beer\u00b7hain", "du", "kamst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Erd' und Meer zu Zeugen", "tokens": ["Und", "Erd'", "und", "Meer", "zu", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Beym Schwur der Treue nahmst.", "tokens": ["Beym", "Schwur", "der", "Treu\u00b7e", "nahmst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gedacht' ich dein, ergl\u00e4nzte", "tokens": ["Ge\u00b7dacht'", "ich", "dein", ",", "er\u00b7gl\u00e4nz\u00b7te"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "PPOSAT", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Fluten d\u00fcstres Blau,", "tokens": ["Der", "Flu\u00b7ten", "d\u00fcst\u00b7res", "Blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Blumengr\u00fcn umkr\u00e4nzte", "tokens": ["Und", "Blu\u00b7men\u00b7gr\u00fcn", "um\u00b7kr\u00e4nz\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Klippen \u00f6des Grau;", "tokens": ["Der", "Klip\u00b7pen", "\u00f6\u00b7des", "Grau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In jeder Woge malte", "tokens": ["In", "je\u00b7der", "Wo\u00b7ge", "mal\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich deine Lichtgestalt;", "tokens": ["Sich", "dei\u00b7ne", "Licht\u00b7ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dein s\u00fcsser Name stralte", "tokens": ["Dein", "s\u00fcs\u00b7ser", "Na\u00b7me", "stral\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vom Sternenchor umwallt.", "tokens": ["Vom", "Ster\u00b7nen\u00b7chor", "um\u00b7wallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie lacht am Tempelhaine,", "tokens": ["Wie", "lacht", "am", "Tem\u00b7pel\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Besp\u00fchlt von leiser Flut,", "tokens": ["Be\u00b7sp\u00fchlt", "von", "lei\u00b7ser", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im goldnen Morgenscheine", "tokens": ["Im", "gold\u00b7nen", "Mor\u00b7gen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein v\u00e4terliches Gut!", "tokens": ["Mein", "v\u00e4\u00b7ter\u00b7li\u00b7ches", "Gut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da theil' ich Herz und Habe", "tokens": ["Da", "theil'", "ich", "Herz", "und", "Ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit dir, Psycharion!", "tokens": ["Mit", "dir", ",", "Psy\u00b7cha\u00b7ri\u00b7on", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und l\u00e4chle noch am Grabe", "tokens": ["Und", "l\u00e4ch\u00b7le", "noch", "am", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Froh, wie Anakreon.", "tokens": ["Froh", ",", "wie", "A\u00b7nak\u00b7re\u00b7on", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NE", "$."], "meter": "+----+", "measure": "dactylic.init"}}, "stanza.9": {"line.1": {"text": "Da bau' ich, leite Gr\u00e4ben,", "tokens": ["Da", "bau'", "ich", ",", "lei\u00b7te", "Gr\u00e4\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bepflanze rings die H\u00f6hn,", "tokens": ["Be\u00b7pflan\u00b7ze", "rings", "die", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seh Reblaub hier an St\u00e4ben,", "tokens": ["Seh", "Re\u00b7blaub", "hier", "an", "St\u00e4\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Dort an der Ulme wehn,", "tokens": ["Dort", "an", "der", "Ul\u00b7me", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und weih' auf meinen Fluren", "tokens": ["Und", "weih'", "auf", "mei\u00b7nen", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Euch Rettern aus Gefahr", "tokens": ["Euch", "Ret\u00b7tern", "aus", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Feld, o Dioskuren!", "tokens": ["Ein", "Feld", ",", "o", "Dios\u00b7ku\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit W\u00e4ldchen und Altar.", "tokens": ["Mit", "W\u00e4ld\u00b7chen", "und", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}