{"textgrid.poem.55081": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Erlk\u00f6nig", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer reitet so sp\u00e4t durch Nacht und Wind?", "tokens": ["Wer", "rei\u00b7tet", "so", "sp\u00e4t", "durch", "Nacht", "und", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ist der Vater mit seinem Kind;", "tokens": ["Es", "ist", "der", "Va\u00b7ter", "mit", "sei\u00b7nem", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er hat den Knaben wohl in dem Arm,", "tokens": ["Er", "hat", "den", "Kna\u00b7ben", "wohl", "in", "dem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Er fa\u00dft ihn sicher, er h\u00e4lt ihn warm.", "tokens": ["Er", "fa\u00dft", "ihn", "si\u00b7cher", ",", "er", "h\u00e4lt", "ihn", "warm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbmein Sohn, was birgst du so bang dein Gesicht?\u00ab", "tokens": ["\u00bb", "mein", "Sohn", ",", "was", "birgst", "du", "so", "bang", "dein", "Ge\u00b7sicht", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbsiehst, Vater, du den Erlk\u00f6nig nicht?", "tokens": ["\u00bb", "siehst", ",", "Va\u00b7ter", ",", "du", "den", "Erl\u00b7k\u00f6\u00b7nig", "nicht", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Den Erlenk\u00f6nig mit Kron und Schweif?\u00ab", "tokens": ["Den", "Er\u00b7len\u00b7k\u00f6\u00b7nig", "mit", "Kron", "und", "Schweif", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbmein Sohn, es ist ein Nebelstreif.\u00ab", "tokens": ["\u00bb", "mein", "Sohn", ",", "es", "ist", "ein", "Ne\u00b7bel\u00b7streif", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbdu liebes Kind, komm, geh mit mir!", "tokens": ["\u00bb", "du", "lie\u00b7bes", "Kind", ",", "komm", ",", "geh", "mit", "mir", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar sch\u00f6ne Spiele spiel ich mit dir;", "tokens": ["Gar", "sch\u00f6\u00b7ne", "Spie\u00b7le", "spiel", "ich", "mit", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Manch bunte Blumen sind an dem Strand;", "tokens": ["Manch", "bun\u00b7te", "Blu\u00b7men", "sind", "an", "dem", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Meine Mutter hat manch g\u00fclden Gewand.\u00ab", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "hat", "manch", "g\u00fcl\u00b7den", "Ge\u00b7wand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "\u00bbmein Vater, mein Vater, und h\u00f6rest du nicht,", "tokens": ["\u00bb", "mein", "Va\u00b7ter", ",", "mein", "Va\u00b7ter", ",", "und", "h\u00f6\u00b7rest", "du", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Was Erlenk\u00f6nig mir leise verspricht?\u00ab", "tokens": ["Was", "Er\u00b7len\u00b7k\u00f6\u00b7nig", "mir", "lei\u00b7se", "ver\u00b7spricht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "NN", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbsei ruhig, bleibe ruhig, mein Kind;", "tokens": ["\u00bb", "sei", "ru\u00b7hig", ",", "blei\u00b7be", "ru\u00b7hig", ",", "mein", "Kind", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$,", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In d\u00fcrren Bl\u00e4ttern s\u00e4uselt der Wind.\u00ab", "tokens": ["In", "d\u00fcr\u00b7ren", "Bl\u00e4t\u00b7tern", "s\u00e4u\u00b7selt", "der", "Wind", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "\u00bbwillst, feiner Knabe, du mit mir gehn?", "tokens": ["\u00bb", "willst", ",", "fei\u00b7ner", "Kna\u00b7be", ",", "du", "mit", "mir", "gehn", "?"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "$,", "ADJA", "NN", "$,", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Meine T\u00f6chter sollen dich warten sch\u00f6n;", "tokens": ["Mei\u00b7ne", "T\u00f6ch\u00b7ter", "sol\u00b7len", "dich", "war\u00b7ten", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Meine T\u00f6chter f\u00fchren den n\u00e4chtlichen Reihn", "tokens": ["Mei\u00b7ne", "T\u00f6ch\u00b7ter", "f\u00fch\u00b7ren", "den", "n\u00e4cht\u00b7li\u00b7chen", "Reihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und wiegen und tanzen und singen dich ein.\u00ab", "tokens": ["Und", "wie\u00b7gen", "und", "tan\u00b7zen", "und", "sin\u00b7gen", "dich", "ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "KON", "VVINF", "KON", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbmein Vater, mein Vater, und siehst du nicht dort", "tokens": ["\u00bb", "mein", "Va\u00b7ter", ",", "mein", "Va\u00b7ter", ",", "und", "siehst", "du", "nicht", "dort"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Erlk\u00f6nigs T\u00f6chter am d\u00fcstern Ort?\u00ab", "tokens": ["Erl\u00b7k\u00f6\u00b7nigs", "T\u00f6ch\u00b7ter", "am", "d\u00fcs\u00b7tern", "Ort", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u00bbmein Sohn, mein Sohn, ich seh es genau:", "tokens": ["\u00bb", "mein", "Sohn", ",", "mein", "Sohn", ",", "ich", "seh", "es", "ge\u00b7nau", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Es scheinen die alten Weiden so grau.\u00ab", "tokens": ["Es", "schei\u00b7nen", "die", "al\u00b7ten", "Wei\u00b7den", "so", "grau", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbich liebe dich, mich reizt deine sch\u00f6ne Gestalt;", "tokens": ["\u00bb", "ich", "lie\u00b7be", "dich", ",", "mich", "reizt", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Ge\u00b7stalt", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und bist du nicht willig, so brauch ich Gewalt.\u00ab", "tokens": ["Und", "bist", "du", "nicht", "wil\u00b7lig", ",", "so", "brauch", "ich", "Ge\u00b7walt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "ADV", "VVFIN", "PPER", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u00bbmein Vater, mein Vater, jetzt fa\u00dft er mich an!", "tokens": ["\u00bb", "mein", "Va\u00b7ter", ",", "mein", "Va\u00b7ter", ",", "jetzt", "fa\u00dft", "er", "mich", "an", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Erlk\u00f6nig hat mir ein Leids getan!\u00ab", "tokens": ["Erl\u00b7k\u00f6\u00b7nig", "hat", "mir", "ein", "Leids", "ge\u00b7tan", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Dem Vater grauset's, er reitet geschwind,", "tokens": ["Dem", "Va\u00b7ter", "grau\u00b7set's", ",", "er", "rei\u00b7tet", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er h\u00e4lt in Armen das \u00e4chzende Kind,", "tokens": ["Er", "h\u00e4lt", "in", "Ar\u00b7men", "das", "\u00e4ch\u00b7zen\u00b7de", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erreicht den Hof mit M\u00fche und Not;", "tokens": ["Er\u00b7reicht", "den", "Hof", "mit", "M\u00fc\u00b7he", "und", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In seinen Armen das Kind war tot.", "tokens": ["In", "sei\u00b7nen", "Ar\u00b7men", "das", "Kind", "war", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}