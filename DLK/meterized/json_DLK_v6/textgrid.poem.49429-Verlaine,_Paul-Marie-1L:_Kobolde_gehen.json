ure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Weithin Spelunken,", "tokens": ["Weit\u00b7hin", "Spe\u00b7lun\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Kein wohnlich Haus.", "tokens": ["Kein", "wohn\u00b7lich", "Haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ins Land hinaus", "tokens": ["Ins", "Land", "hin\u00b7aus"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "APZR"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Lohn rote Funken.", "tokens": ["Lohn", "ro\u00b7te", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Was sp\u00fcrst du da?", "tokens": ["Was", "sp\u00fcrst", "du", "da", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dumpf dr\u00f6hnt die Br\u00fccke,", "tokens": ["Dumpf", "dr\u00f6hnt", "die", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Erstaunte Blicke:", "tokens": ["Er\u00b7staun\u00b7te", "Bli\u00b7cke", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Stadt ist nah.", "tokens": ["Die", "Stadt", "ist", "nah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Im Qualm verloren", "tokens": ["Im", "Qualm", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Welch dumpfer Klang?", "tokens": ["Welch", "dum\u00b7pfer", "Klang", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Welch Rasseln drang", "tokens": ["Welch", "Ras\u00b7seln", "drang"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Zu meinen Ohren?", "tokens": ["Zu", "mei\u00b7nen", "Oh\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Das Land haucht fahl", "tokens": ["Das", "Land", "haucht", "fahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Gl\u00fchheissen Odem,", "tokens": ["Gl\u00fch\u00b7heis\u00b7sen", "O\u00b7dem", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ein schweiss'ger Brodem,", "tokens": ["Ein", "schweiss'\u00b7ger", "Bro\u00b7dem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gekreisch von Stahl.", "tokens": ["Ge\u00b7kreisch", "von", "Stahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Kobolde gehen", "tokens": ["Ko\u00b7bol\u00b7de", "ge\u00b7hen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Durchs russ'ge Feld.", "tokens": ["Durchs", "russ'\u00b7ge", "Feld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRA