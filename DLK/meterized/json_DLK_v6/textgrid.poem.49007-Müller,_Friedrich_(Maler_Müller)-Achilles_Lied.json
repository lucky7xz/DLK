{"textgrid.poem.49007": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "Achilles Lied", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "B\u00e4ndige meines Unmuts Feuer,", "tokens": ["B\u00e4n\u00b7di\u00b7ge", "mei\u00b7nes", "Un\u00b7muts", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "B\u00e4ndige meinen lodernden Busen,", "tokens": ["B\u00e4n\u00b7di\u00b7ge", "mei\u00b7nen", "lo\u00b7dern\u00b7den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Goldumspannte Leier!", "tokens": ["Gold\u00b7um\u00b7spann\u00b7te", "Lei\u00b7er", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bringe die T\u00f6ne zur\u00fcck,", "tokens": ["Brin\u00b7ge", "die", "T\u00f6\u00b7ne", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Jene sanften T\u00f6ne,", "tokens": ["Je\u00b7ne", "sanf\u00b7ten", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Darin die goldgelockte F\u00fcrstentochter tanzte,", "tokens": ["Da\u00b7rin", "die", "gold\u00b7ge\u00b7lock\u00b7te", "F\u00fcrs\u00b7ten\u00b7toch\u00b7ter", "tanz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In dem ihr zarter Finger \u00fcber ihre Saiten lief.", "tokens": ["In", "dem", "ihr", "zar\u00b7ter", "Fin\u00b7ger", "\u00fc\u00b7ber", "ih\u00b7re", "Sai\u00b7ten", "lief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Dann kam ich, belastet von Beute \u2013", "tokens": ["Dann", "kam", "ich", ",", "be\u00b7las\u00b7tet", "von", "Beu\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Sie tanzte mir entgegen!", "tokens": ["Sie", "tanz\u00b7te", "mir", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ich ruhte in ihrem Arm.", "tokens": ["Ich", "ruh\u00b7te", "in", "ih\u00b7rem", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber nun, wo ist sie, wo?", "tokens": ["A\u00b7ber", "nun", ",", "wo", "ist", "sie", ",", "wo", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "VAFIN", "PPER", "$,", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie haben mir sie geraubt.", "tokens": ["Sie", "ha\u00b7ben", "mir", "sie", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hebe hoch, dein Haupt, Troja!", "tokens": ["He\u00b7be", "hoch", ",", "dein", "Haupt", ",", "Tro\u00b7ja", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PPOSAT", "NN", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kein tapfrer Grieche fecht' mehr,", "tokens": ["Kein", "tapf\u00b7rer", "Grie\u00b7che", "fecht'", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und du, Speer und Schild, traure!", "tokens": ["Und", "du", ",", "Speer", "und", "Schild", ",", "trau\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "KON", "NN", "$,", "VVFIN", "$."], "meter": "--+-++-", "measure": "anapaest.init"}, "line.6": {"text": "Keine Lorbeern fallen auf euch herab.", "tokens": ["Kei\u00b7ne", "Lor\u00b7beern", "fal\u00b7len", "auf", "euch", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Schon sehne ich mich zur\u00fccke,", "tokens": ["Schon", "seh\u00b7ne", "ich", "mich", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zur\u00fcck nach meinem Vaterland", "tokens": ["Zu\u00b7r\u00fcck", "nach", "mei\u00b7nem", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und den geliebten Tannenw\u00e4ldern", "tokens": ["Und", "den", "ge\u00b7lieb\u00b7ten", "Tan\u00b7nen\u00b7w\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und singe unter dem Schatten", "tokens": ["Und", "sin\u00b7ge", "un\u00b7ter", "dem", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Meiner tapfern Voreltern", "tokens": ["Mei\u00b7ner", "tap\u00b7fern", "Vo\u00b7rel\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Geschichte \u2013 jene Helden,", "tokens": ["Ge\u00b7schich\u00b7te", "\u2013", "je\u00b7ne", "Hel\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Himmel und Erde bewundert \u2013", "tokens": ["Die", "Him\u00b7mel", "und", "Er\u00b7de", "be\u00b7wun\u00b7dert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Auf, ihr Schiffe, auf, davon!", "tokens": ["Auf", ",", "ihr", "Schif\u00b7fe", ",", "auf", ",", "da\u00b7von", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$,", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}