{"textgrid.poem.33020": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und immer wieder sinkt der Winter", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und immer wieder sinkt der Winter", "tokens": ["Und", "im\u00b7mer", "wie\u00b7der", "sinkt", "der", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und immer wieder wird es Fr\u00fchling", "tokens": ["und", "im\u00b7mer", "wie\u00b7der", "wird", "es", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und immer immer wieder stehst du", "tokens": ["und", "im\u00b7mer", "im\u00b7mer", "wie\u00b7der", "stehst", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und freust dich an dem ersten Gr\u00fcn,", "tokens": ["und", "freust", "dich", "an", "dem", "ers\u00b7ten", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und wenn die kleinen Veilchen bl\u00fchn,", "tokens": ["und", "wenn", "die", "klei\u00b7nen", "Veil\u00b7chen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und immer wieder ist es sch\u00f6n", "tokens": ["und", "im\u00b7mer", "wie\u00b7der", "ist", "es", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und macht es jung und macht es froh,", "tokens": ["und", "macht", "es", "jung", "und", "macht", "es", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und ob du's tausendmal gesehn:", "tokens": ["und", "ob", "du's", "tau\u00b7send\u00b7mal", "ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "wenn hoch in lauen blauen L\u00fcften", "tokens": ["wenn", "hoch", "in", "lau\u00b7en", "blau\u00b7en", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "die ersten Schwalben lustig zwitschern ...", "tokens": ["die", "ers\u00b7ten", "Schwal\u00b7ben", "lus\u00b7tig", "zwit\u00b7schern", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "immer wieder ... jedes Jahr ...", "tokens": ["im\u00b7mer", "wie\u00b7der", "...", "je\u00b7des", "Jahr", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "sag, ist das nicht wunderbar?!", "tokens": ["sag", ",", "ist", "das", "nicht", "wun\u00b7der\u00b7bar", "?!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PDS", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Diese stille Kraft der Seele:", "tokens": ["Die\u00b7se", "stil\u00b7le", "Kraft", "der", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "immer neu sich aufzuringen", "tokens": ["im\u00b7mer", "neu", "sich", "auf\u00b7zu\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "aus dem Banne tr\u00fcber Winter,", "tokens": ["aus", "dem", "Ban\u00b7ne", "tr\u00fc\u00b7ber", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "aus dem Schatten grauer N\u00e4chte,", "tokens": ["aus", "dem", "Schat\u00b7ten", "grau\u00b7er", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "aus der Tiefe in die H\u00f6he ...", "tokens": ["aus", "der", "Tie\u00b7fe", "in", "die", "H\u00f6\u00b7he", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sag, ist das nicht wunderbar?!", "tokens": ["sag", ",", "ist", "das", "nicht", "wun\u00b7der\u00b7bar", "?!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PDS", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "diese stille Kraft der Seele,", "tokens": ["die\u00b7se", "stil\u00b7le", "Kraft", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "immer wieder", "tokens": ["im\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "sich zur Sonne zu befrein,", "tokens": ["sich", "zur", "Son\u00b7ne", "zu", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "immer wieder stolz zu werden,", "tokens": ["im\u00b7mer", "wie\u00b7der", "stolz", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "immer wieder froh zu sein?!", "tokens": ["im\u00b7mer", "wie\u00b7der", "froh", "zu", "sein", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}