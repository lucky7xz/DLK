{"textgrid.poem.64283": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: So kam der Tag, der lang gef\u00fcrchtete!", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So kam der Tag, der lang gef\u00fcrchtete!", "tokens": ["So", "kam", "der", "Tag", ",", "der", "lang", "ge\u00b7f\u00fcrch\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An ihres gr\u00f6\u00dften Sohnes Sarge steht", "tokens": ["An", "ih\u00b7res", "gr\u00f6\u00df\u00b7ten", "Soh\u00b7nes", "Sar\u00b7ge", "steht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Germania trauernd, um die Kaiserkrone,", "tokens": ["Ger\u00b7ma\u00b7nia", "trau\u00b7ernd", ",", "um", "die", "Kai\u00b7ser\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die goldene, den schwarzen Schleier schlagend.", "tokens": ["Die", "gol\u00b7de\u00b7ne", ",", "den", "schwar\u00b7zen", "Schlei\u00b7er", "schla\u00b7gend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was alles dankt sie ihm! \u2013 Vor vierzig Jahren", "tokens": ["Was", "al\u00b7les", "dankt", "sie", "ihm", "!", "\u2013", "Vor", "vier\u00b7zig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PWS", "PIS", "VVFIN", "PPER", "PPER", "$.", "$(", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sa\u00df sie im Walde, fr\u00f6stelnd und allein:", "tokens": ["Sa\u00df", "sie", "im", "Wal\u00b7de", ",", "fr\u00f6s\u00b7telnd", "und", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es hatten b\u00f6se Sch\u00e4cher l\u00e4ngst vom Haupt", "tokens": ["Es", "hat\u00b7ten", "b\u00f6\u00b7se", "Sch\u00e4\u00b7cher", "l\u00e4ngst", "vom", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gerissen ihr des alten Reiches Krone,", "tokens": ["Ge\u00b7ris\u00b7sen", "ihr", "des", "al\u00b7ten", "Rei\u00b7ches", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zerfetzt den Mantel und sie drein geteilt:", "tokens": ["Zer\u00b7fetzt", "den", "Man\u00b7tel", "und", "sie", "drein", "ge\u00b7teilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es ehrte, scheute, f\u00fcrchtete sich keiner,", "tokens": ["Es", "ehr\u00b7te", ",", "scheu\u00b7te", ",", "f\u00fcrch\u00b7te\u00b7te", "sich", "kei\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PRF", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sie war der Nachbarn Hohn und Spott geworden!", "tokens": ["Sie", "war", "der", "Nach\u00b7barn", "Hohn", "und", "Spott", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da kam der Held von echtem Siegfriedmute", "tokens": ["Da", "kam", "der", "Held", "von", "ech\u00b7tem", "Sieg\u00b7fried\u00b7mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Durchhieb mit scharfem Schwert das Dorngenist,", "tokens": ["Durch\u00b7hieb", "mit", "schar\u00b7fem", "Schwert", "das", "Dorn\u00b7ge\u00b7nist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das sie umschlo\u00df, die Schlummernde erweckend,", "tokens": ["Das", "sie", "um\u00b7schlo\u00df", ",", "die", "Schlum\u00b7mern\u00b7de", "er\u00b7we\u00b7ckend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und eine neue Kaiserkrone setzte,", "tokens": ["Und", "ei\u00b7ne", "neu\u00b7e", "Kai\u00b7ser\u00b7kro\u00b7ne", "setz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Geschmiedet in der Siegesschlachtenglut,", "tokens": ["Ge\u00b7schmie\u00b7det", "in", "der", "Sie\u00b7ges\u00b7schlach\u00b7ten\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Der Staunenden er auf das blonde Haupt! \u2013 \u2013", "tokens": ["Der", "Stau\u00b7nen\u00b7den", "er", "auf", "das", "blon\u00b7de", "Haupt", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und welche K\u00e4mpfe, welchen Lohn des Hasses", "tokens": ["Und", "wel\u00b7che", "K\u00e4mp\u00b7fe", ",", "wel\u00b7chen", "Lohn", "des", "Has\u00b7ses"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "NN", "$,", "PWAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Hat ihm daf\u00fcr sein Volk bereitet! \u2013 Wie", "tokens": ["Hat", "ihm", "da\u00b7f\u00fcr", "sein", "Volk", "be\u00b7rei\u00b7tet", "!", "\u2013", "Wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["VAFIN", "PPER", "PAV", "PPOSAT", "NN", "VVFIN", "$.", "$(", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Armin, den ersten Einiger, so hat", "tokens": ["Ar\u00b7min", ",", "den", "ers\u00b7ten", "Ei\u00b7ni\u00b7ger", ",", "so", "hat"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Auch ihn bedroht der Mord. \u2013", "tokens": ["Auch", "ihn", "be\u00b7droht", "der", "Mord", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Doch sich'rer, sch\u00e4rfer", "tokens": ["Doch", "sich'\u00b7rer", ",", "sch\u00e4r\u00b7fer"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "ADJA", "$,", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Als jene M\u00f6rder traf sein Herz des Undanks", "tokens": ["Als", "je\u00b7ne", "M\u00f6r\u00b7der", "traf", "sein", "Herz", "des", "Un\u00b7danks"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Giftschwarzer Dolch! \u2013 \u2013 \u2013", "tokens": ["Gift\u00b7schwar\u00b7zer", "Dolch", "!", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Doch dieses Herz, \u2013 kein Gift konnt' es vergiften;", "tokens": ["Doch", "die\u00b7ses", "Herz", ",", "\u2013", "kein", "Gift", "konnt'", "es", "ver\u00b7gif\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "$(", "PIAT", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Verachten durfte er die Menschen tief:", "tokens": ["Ver\u00b7ach\u00b7ten", "durf\u00b7te", "er", "die", "Men\u00b7schen", "tief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Er tat es nicht.", "tokens": ["Er", "tat", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Und wie sein alter Herr, von Mordblut wund,", "tokens": ["Und", "wie", "sein", "al\u00b7ter", "Herr", ",", "von", "Mord\u00b7blut", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Erbarmend nur der Darbenden gedachte,", "tokens": ["Er\u00b7bar\u00b7mend", "nur", "der", "Dar\u00b7ben\u00b7den", "ge\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "So hat auch das Abscheulichste niemals", "tokens": ["So", "hat", "auch", "das", "Ab\u00b7scheu\u00b7lichs\u00b7te", "nie\u00b7mals"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Dem deutschen Volk und seinem Dienst entfremdet", "tokens": ["Dem", "deut\u00b7schen", "Volk", "und", "sei\u00b7nem", "Dienst", "ent\u00b7frem\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Dies treue Herz. \u2013", "tokens": ["Dies", "treu\u00b7e", "Herz", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PDS", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Verwaist ist jetzt erst v\u00f6llig unser Volk!", "tokens": ["Ver\u00b7waist", "ist", "jetzt", "erst", "v\u00f6l\u00b7lig", "un\u00b7ser", "Volk", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Denn wo w\u00e4r' wohl der ma\u00dflos eitle Tor,", "tokens": ["Denn", "wo", "w\u00e4r'", "wohl", "der", "ma\u00df\u00b7los", "eit\u00b7le", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Der Bismarck zu ersetzen sich verm\u00e4\u00dfe?", "tokens": ["Der", "Bis\u00b7marck", "zu", "er\u00b7set\u00b7zen", "sich", "ver\u00b7m\u00e4\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Wir aber, die wir niemals ihn verleugnet,", "tokens": ["Wir", "a\u00b7ber", ",", "die", "wir", "nie\u00b7mals", "ihn", "ver\u00b7leug\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Als ihm des Herrschers Gnadensonne losch,", "tokens": ["Als", "ihm", "des", "Herr\u00b7schers", "Gna\u00b7den\u00b7son\u00b7ne", "losch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Wir haben wahrlich h\u00f6h'res Recht an ihm", "tokens": ["Wir", "ha\u00b7ben", "wahr\u00b7lich", "h\u00f6h'\u00b7res", "Recht", "an", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Als jene b\u00f6sen Zwerge, seine Neider,", "tokens": ["Als", "je\u00b7ne", "b\u00f6\u00b7sen", "Zwer\u00b7ge", ",", "sei\u00b7ne", "Nei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Die vor Europa, warnend, ihn verklagten!", "tokens": ["Die", "vor", "Eu\u00b7ro\u00b7pa", ",", "war\u00b7nend", ",", "ihn", "ver\u00b7klag\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "$,", "VVPP", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Doch dieses h\u00f6h're Recht schafft h\u00f6h're Pflicht:", "tokens": ["Doch", "die\u00b7ses", "h\u00f6h'\u00b7re", "Recht", "schafft", "h\u00f6h'\u00b7re", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "So la\u00dft uns denn an seinem Sarg geloben,", "tokens": ["So", "la\u00dft", "uns", "denn", "an", "sei\u00b7nem", "Sarg", "ge\u00b7lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "An seinem Bau, dem schwer bedrohten Haus,", "tokens": ["An", "sei\u00b7nem", "Bau", ",", "dem", "schwer", "be\u00b7droh\u00b7ten", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Mit Schild und Schwert getreulich Wacht zu halten;", "tokens": ["Mit", "Schild", "und", "Schwert", "ge\u00b7treu\u00b7lich", "Wacht", "zu", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Das Bismarck-Erbe, \u2013 treu wollen wir es h\u00fcten:", "tokens": ["Das", "Bis\u00b7ma\u00b7rck\u00b7Er\u00b7be", ",", "\u2013", "treu", "wol\u00b7len", "wir", "es", "h\u00fc\u00b7ten", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "$(", "ADJD", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "Sein Erbe wie sein glorreich Denkmal ist's", "tokens": ["Sein", "Er\u00b7be", "wie", "sein", "glor\u00b7reich", "Denk\u00b7mal", "ist's"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KOKOM", "PPOSAT", "ADJD", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Zugleich: das Deutsche Reich!", "tokens": ["Zu\u00b7gleich", ":", "das", "Deut\u00b7sche", "Reich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.48": {"text": "H\u00f6r's, Otto, tief im Grab:", "tokens": ["H\u00f6r's", ",", "Ot\u00b7to", ",", "tief", "im", "Grab", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Wir stehn zu dir nach deinem Tode noch,", "tokens": ["Wir", "stehn", "zu", "dir", "nach", "dei\u00b7nem", "To\u00b7de", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+