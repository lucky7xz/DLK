{"textgrid.poem.61953": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh', das Haus ward mir zu enge,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh', das Haus ward mir zu enge,", "tokens": ["Sieh'", ",", "das", "Haus", "ward", "mir", "zu", "en\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es trieb mich in die Welt,", "tokens": ["Und", "es", "trieb", "mich", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Tales dunkle G\u00e4nge,", "tokens": ["In", "des", "Ta\u00b7les", "dunk\u00b7le", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wo sich's wie im Traum verh\u00e4lt.", "tokens": ["wo", "sich's", "wie", "im", "Traum", "ver\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "KOKOM", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rebenh\u00fcgel, Tannenw\u00e4lder,", "tokens": ["Re\u00b7ben\u00b7h\u00fc\u00b7gel", ",", "Tan\u00b7nen\u00b7w\u00e4l\u00b7der", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitten hin des Stromes Band,", "tokens": ["Mit\u00b7ten", "hin", "des", "Stro\u00b7mes", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmucke Auen, Weizenfelder,", "tokens": ["Schmu\u00b7cke", "Au\u00b7en", ",", "Wei\u00b7zen\u00b7fel\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nes, Deutsches Vaterland!", "tokens": ["Sch\u00f6\u00b7nes", ",", "Deut\u00b7sches", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zu den blankgeputzten H\u00fctten,", "tokens": ["Zu", "den", "blank\u00b7ge\u00b7putz\u00b7ten", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Droben auf der Bergesh\u00f6h'", "tokens": ["Dro\u00b7ben", "auf", "der", "Ber\u00b7ges\u00b7h\u00f6h'"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zog es mich mit raschen Schritten,", "tokens": ["Zog", "es", "mich", "mit", "ra\u00b7schen", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verwundert still ich steh'.", "tokens": ["Und", "ver\u00b7wun\u00b7dert", "still", "ich", "steh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine Stimme ruft von innen,", "tokens": ["Ei\u00b7ne", "Stim\u00b7me", "ruft", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Stimme klar und hell:", "tokens": ["Ei\u00b7ne", "Stim\u00b7me", "klar", "und", "hell", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbguter J\u00fcngling, geh' von hinnen,", "tokens": ["\u00bb", "gu\u00b7ter", "J\u00fcng\u00b7ling", ",", "geh'", "von", "hin\u00b7nen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schreit nicht \u00fcber diese Schwell'!", "tokens": ["Schreit", "nicht", "\u00fc\u00b7ber", "die\u00b7se", "Schwell'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Steig' auf Burgen, steig' auf Zinnen,", "tokens": ["Steig'", "auf", "Bur\u00b7gen", ",", "steig'", "auf", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sieh' von au\u00dfen an das Land,", "tokens": ["Sieh'", "von", "au\u00b7\u00dfen", "an", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was Du sehen kannst da drinnen,", "tokens": ["Was", "Du", "se\u00b7hen", "kannst", "da", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es verwirrt Dir den Verstand.", "tokens": ["Es", "ver\u00b7wirrt", "Dir", "den", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Unsrer H\u00fctten tr\u00fcbe Weise", "tokens": ["Uns\u00b7rer", "H\u00fct\u00b7ten", "tr\u00fc\u00b7be", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pa\u00dft nicht zu der schmucken Au,", "tokens": ["Pa\u00dft", "nicht", "zu", "der", "schmu\u00b7cken", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Guter J\u00fcngling, wirst zum Greise,", "tokens": ["Gu\u00b7ter", "J\u00fcng\u00b7ling", ",", "wirst", "zum", "Grei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Dein Lockenhaar wird grau.\u00ab", "tokens": ["Und", "Dein", "Lo\u00b7cken\u00b7haar", "wird", "grau", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bl\u00fcmlein ranken um die Mauer,", "tokens": ["Bl\u00fcm\u00b7lein", "ran\u00b7ken", "um", "die", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n bepflanzt von welker Hand,", "tokens": ["Sch\u00f6n", "be\u00b7pflanzt", "von", "wel\u00b7ker", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "PWAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und benetzt von Tr\u00e4nenschauer,", "tokens": ["Und", "be\u00b7netzt", "von", "Tr\u00e4\u00b7nen\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fcnt das Deutsche Vaterland!", "tokens": ["Gr\u00fcnt", "das", "Deut\u00b7sche", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}