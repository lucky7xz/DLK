{"textgrid.poem.65941": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Aus Verlorenheit", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelobtes Land der Eltern mir, des Sanges,", "tokens": ["Ge\u00b7lob\u00b7tes", "Land", "der", "El\u00b7tern", "mir", ",", "des", "San\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einst w\u00e4hnt ich mich vertr\u00e4umt an deinen Rhein,", "tokens": ["Einst", "w\u00e4hnt", "ich", "mich", "ver\u00b7tr\u00e4umt", "an", "dei\u00b7nen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "APPR", "PPOSAT", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erz\u00e4hlte Nordgold, dort geh\u00f6rt zu sein:", "tokens": ["Er\u00b7z\u00e4hl\u00b7te", "Nord\u00b7gold", ",", "dort", "ge\u00b7h\u00f6rt", "zu", "sein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Horn so vollen Sonnen\u00fcberschwanges,", "tokens": ["Ein", "Horn", "so", "vol\u00b7len", "Son\u00b7nen\u00b7\u00fc\u00b7bersc\u00b7hwan\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df altes Licht, aus Magierhand am Ganges,", "tokens": ["Da\u00df", "al\u00b7tes", "Licht", ",", "aus", "Ma\u00b7gier\u00b7hand", "am", "Gan\u00b7ges", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Auf Gletscher \u00fcberst\u00fclpt \u2013 zu Urgedeihn", "tokens": ["\u2013", "Auf", "Glet\u00b7scher", "\u00fc\u00b7bers\u00b7t\u00fclpt", "\u2013", "zu", "Ur\u00b7ge\u00b7deihn"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "APPR", "NN", "VVPP", "$(", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus Finsternis, euch fordern sollte und befrein,", "tokens": ["Aus", "Fins\u00b7ter\u00b7nis", ",", "euch", "for\u00b7dern", "soll\u00b7te", "und", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PPER", "VVINF", "VMFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beschlo\u00df ich deutsch zu bringen: wohl gelang es!", "tokens": ["Be\u00b7schlo\u00df", "ich", "deutsch", "zu", "brin\u00b7gen", ":", "wohl", "ge\u00b7lang", "es", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch weil' ich fort, im Morgenland, beim Wogen", "tokens": ["Noch", "weil'", "ich", "fort", ",", "im", "Mor\u00b7gen\u00b7land", ",", "beim", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beseelter See um Hellas' hohen Strand!", "tokens": ["Be\u00b7seel\u00b7ter", "See", "um", "Hel\u00b7las'", "ho\u00b7hen", "Strand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verhei\u00dfungen, ihr habt noch nie getrogen:", "tokens": ["Ver\u00b7hei\u00b7\u00dfun\u00b7gen", ",", "ihr", "habt", "noch", "nie", "ge\u00b7tro\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Donnern holt uns bald! Dann schwankt die Wand.", "tokens": ["Ein", "Don\u00b7nern", "holt", "uns", "bald", "!", "Dann", "schwankt", "die", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer griff zum Blitz? Von Lichtschwertern beflogen,", "tokens": ["Wer", "griff", "zum", "Blitz", "?", "Von", "Licht\u00b7schwer\u00b7tern", "be\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$.", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Erspr\u00fcht das Reich. Umfa\u00df uns Flammenband!", "tokens": ["Er\u00b7spr\u00fcht", "das", "Reich", ".", "Um\u00b7fa\u00df", "uns", "Flam\u00b7men\u00b7band", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "NN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}