{"textgrid.poem.37753": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: G\u00f6ttlich ist auf Erden die Geschwindigkeit,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttlich ist auf Erden die Geschwindigkeit,", "tokens": ["G\u00f6tt\u00b7lich", "ist", "auf", "Er\u00b7den", "die", "Ge\u00b7schwin\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sie besiegt den weiten Raum, die enge Zeit,", "tokens": ["Sie", "be\u00b7siegt", "den", "wei\u00b7ten", "Raum", ",", "die", "en\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Gegenw\u00e4rtig macht sie \u00fcberall zugleich", "tokens": ["Ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "macht", "sie", "\u00fc\u00b7be\u00b7rall", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Spiegelnd hoher G\u00f6tter ewig Reich;", "tokens": ["Spie\u00b7gelnd", "ho\u00b7her", "G\u00f6t\u00b7ter", "e\u00b7wig", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Mit dem Anfang eint das Ende ihre Hand", "tokens": ["Mit", "dem", "An\u00b7fang", "eint", "das", "En\u00b7de", "ih\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Sich zum Siegeskranze; wie der Feuerbrand,", "tokens": ["Sich", "zum", "Sie\u00b7ges\u00b7kran\u00b7ze", ";", "wie", "der", "Feu\u00b7er\u00b7brand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$.", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Schnell geschwungen, wird zum Feuerkreise,", "tokens": ["Schnell", "ge\u00b7schwun\u00b7gen", ",", "wird", "zum", "Feu\u00b7er\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "So erscheinen ihres Wagens Gleise;", "tokens": ["So", "er\u00b7schei\u00b7nen", "ih\u00b7res", "Wa\u00b7gens", "Glei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Eh das Auge aufblickt ist ihr Bogen", "tokens": ["Eh", "das", "Au\u00b7ge", "auf\u00b7blickt", "ist", "ihr", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Durch die weite Rennbahn hingezogen.", "tokens": ["Durch", "die", "wei\u00b7te", "Renn\u00b7bahn", "hin\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ihr geh\u00f6rt die Sch\u00f6nheit, weil sie fl\u00fcchtig,", "tokens": ["Ihr", "ge\u00b7h\u00f6rt", "die", "Sch\u00f6n\u00b7heit", ",", "weil", "sie", "fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Der Gestirne Wallen, ruhlos richtig,", "tokens": ["Der", "Ge\u00b7stir\u00b7ne", "Wal\u00b7len", ",", "ruh\u00b7los", "rich\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Ihr vertraut der Gott die m\u00e4cht'gen Worte", "tokens": ["Ihr", "ver\u00b7traut", "der", "Gott", "die", "m\u00e4cht'\u00b7gen", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "In dem Blitzstrahl aus der Himmelspforte,", "tokens": ["In", "dem", "Blitz\u00b7strahl", "aus", "der", "Him\u00b7mel\u00b7spfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Die da aufschl\u00e4gt, Schauende verblendet,", "tokens": ["Die", "da", "auf\u00b7schl\u00e4gt", ",", "Schau\u00b7en\u00b7de", "ver\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "NN", "VVPP", "$,"], "meter": "-+--+---+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Eh sie zuschl\u00e4gt, schon ihr Leben endet.", "tokens": ["Eh", "sie", "zu\u00b7schl\u00e4gt", ",", "schon", "ihr", "Le\u00b7ben", "en\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.17": {"text": "Tr\u00e4ger rollt nach ihrer Flammengei\u00dfel Schwung", "tokens": ["Tr\u00e4\u00b7ger", "rollt", "nach", "ih\u00b7rer", "Flam\u00b7men\u00b7gei\u00b7\u00dfel", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "Donner \u00fcber alle zur Erinnerung,", "tokens": ["Don\u00b7ner", "\u00fc\u00b7ber", "al\u00b7le", "zur", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.19": {"text": "Tr\u00e4ger rollen sich die schwarzen Wolken auf", "tokens": ["Tr\u00e4\u00b7ger", "rol\u00b7len", "sich", "die", "schwar\u00b7zen", "Wol\u00b7ken", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "APPR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.20": {"text": "Nach des gl\u00fchen Donnerwagens Lauf;", "tokens": ["Nach", "des", "gl\u00fc\u00b7hen", "Don\u00b7ner\u00b7wa\u00b7gens", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Ja die Welt erschiene todt in Leere,", "tokens": ["Ja", "die", "Welt", "er\u00b7schie\u00b7ne", "todt", "in", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "H\u00fcbe nicht Geschwindigkeit die Schwere.", "tokens": ["H\u00fc\u00b7be", "nicht", "Ge\u00b7schwin\u00b7dig\u00b7keit", "die", "Schwe\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}