{"textgrid.poem.67354": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Young \u00fcber Gedanken und Rede", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fehlt Dir ein Freund zum Ausflu\u00df Deines Geistes,", "tokens": ["Fehlt", "Dir", "ein", "Freund", "zum", "Aus\u00b7flu\u00df", "Dei\u00b7nes", "Geis\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wird Dein Innres Sumpf. Verschlossene", "tokens": ["So", "wird", "Dein", "Inn\u00b7res", "Sumpf", ".", "Ver\u00b7schlos\u00b7se\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Gedanken wollen Luft oder verliegen", "tokens": ["Ge\u00b7dan\u00b7ken", "wol\u00b7len", "Luft", "o\u00b7der", "ver\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Waarenlager, denen Sonne fehlt.", "tokens": ["Wie", "Waa\u00b7ren\u00b7la\u00b7ger", ",", "de\u00b7nen", "Son\u00b7ne", "fehlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "W\u00e4r' der Gedanke Alles, h\u00e4tten wir", "tokens": ["W\u00e4r'", "der", "Ge\u00b7dan\u00b7ke", "Al\u00b7les", ",", "h\u00e4t\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PIS", "$,", "VAFIN", "PPER"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Die s\u00fc\u00dfe Rede nicht, die Rede, sie,", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Re\u00b7de", "nicht", ",", "die", "Re\u00b7de", ",", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,", "ART", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Leiter und den Pr\u00fcfstein der Gedanken \u2013", "tokens": ["Den", "Lei\u00b7ter", "und", "den", "Pr\u00fcf\u00b7stein", "der", "Ge\u00b7dan\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Was in dem Schachte liegt, kann Gold und Kies sein;", "tokens": ["Was", "in", "dem", "Schach\u00b7te", "liegt", ",", "kann", "Gold", "und", "Kies", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "VMFIN", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ans Licht gef\u00f6rdert und ins Wort gepr\u00e4gt,", "tokens": ["Ans", "Licht", "ge\u00b7f\u00f6r\u00b7dert", "und", "ins", "Wort", "ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "KON", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Erscheinet des Gedanken wahrer Werth. \u2013 \u2013", "tokens": ["Er\u00b7schei\u00b7net", "des", "Ge\u00b7dan\u00b7ken", "wah\u00b7rer", "Werth", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u2013 Je mitgetheilter, desto eigner sind", "tokens": ["\u2013", "Je", "mit\u00b7ge\u00b7theil\u00b7ter", ",", "des\u00b7to", "eig\u00b7ner", "sind"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "$,", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gedanken unser. Lehrend lernen wir;", "tokens": ["Ge\u00b7dan\u00b7ken", "un\u00b7ser", ".", "Leh\u00b7rend", "ler\u00b7nen", "wir", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$.", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geboren, werden des Verstandes Kinder", "tokens": ["Ge\u00b7bo\u00b7ren", ",", "wer\u00b7den", "des", "Ver\u00b7stan\u00b7des", "Kin\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unsern; stumm, verg\u00e4\u00dfe man sie.", "tokens": ["Die", "un\u00b7sern", ";", "stumm", ",", "ver\u00b7g\u00e4\u00b7\u00dfe", "man", "sie", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADJD", "$,", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Rede,", "tokens": ["Re\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Sie facht des Geistes Feuer an, sie mustert", "tokens": ["Sie", "facht", "des", "Geis\u00b7tes", "Feu\u00b7er", "an", ",", "sie", "mus\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die R\u00fcstungskammer, deren Waffen sie", "tokens": ["Die", "R\u00fcs\u00b7tungs\u00b7kam\u00b7mer", ",", "de\u00b7ren", "Waf\u00b7fen", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zum Schmuck poliret, zum Gebrauche wetzt.", "tokens": ["Zum", "Schmuck", "po\u00b7li\u00b7ret", ",", "zum", "Ge\u00b7brau\u00b7che", "wetzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "O, wie viel' ihrer liegen, bis ans Heft", "tokens": ["O", ",", "wie", "viel'", "ih\u00b7rer", "lie\u00b7gen", ",", "bis", "ans", "Heft"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PIS", "PPOSAT", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Versteckt in Scheiden der Gelehrsamkeit", "tokens": ["Ver\u00b7steckt", "in", "Schei\u00b7den", "der", "Ge\u00b7lehr\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ehrw\u00fcrd'ger B\u00e4nde eingerostet! Sie,", "tokens": ["Ehr\u00b7w\u00fcrd'\u00b7ger", "B\u00e4n\u00b7de", "ein\u00b7ge\u00b7ros\u00b7tet", "!", "Sie", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$.", "PPER", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.12": {"text": "Gesch\u00e4rft zur Schneide, h\u00e4tten weit umher", "tokens": ["Ge\u00b7sch\u00e4rft", "zur", "Schnei\u00b7de", ",", "h\u00e4t\u00b7ten", "weit", "um\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "VAFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Geblitzt, und w\u00e4ren sie der Mutter Zunge", "tokens": ["Ge\u00b7blitzt", ",", "und", "w\u00e4\u00b7ren", "sie", "der", "Mut\u00b7ter", "Zun\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "VAFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Auch nur mit halbem Erbtheil Kinder worden.", "tokens": ["Auch", "nur", "mit", "hal\u00b7bem", "E\u00b7rbtheil", "Kin\u00b7der", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gedankenwechsel ist's, was gleich dem Sto\u00df", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7wech\u00b7sel", "ist's", ",", "was", "gleich", "dem", "Sto\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Gegensto\u00df k\u00e4mpfender Wogen bricht,", "tokens": ["Und", "Ge\u00b7gen\u00b7sto\u00df", "k\u00e4mp\u00b7fen\u00b7der", "Wo\u00b7gen", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Bricht den gelehrten Schaum und hellet auf", "tokens": ["Bricht", "den", "ge\u00b7lehr\u00b7ten", "Schaum", "und", "hel\u00b7let", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Tiefstudirers stehnden Pfuhl. \u2013", "tokens": ["Des", "Tief\u00b7stu\u00b7di\u00b7rers", "stehn\u00b7den", "Pfuhl", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}