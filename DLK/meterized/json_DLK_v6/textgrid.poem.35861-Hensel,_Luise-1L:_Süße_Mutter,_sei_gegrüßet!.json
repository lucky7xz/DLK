{"textgrid.poem.35861": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: S\u00fc\u00dfe Mutter, sei gegr\u00fc\u00dfet!", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfe Mutter, sei gegr\u00fc\u00dfet!", "tokens": ["S\u00fc\u00b7\u00dfe", "Mut\u00b7ter", ",", "sei", "ge\u00b7gr\u00fc\u00b7\u00dfet", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeig' mir doch dies Kindelein,", "tokens": ["Zeig'", "mir", "doch", "dies", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PDS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Dein reiner Arm umschlie\u00dfet;", "tokens": ["Das", "Dein", "rei\u00b7ner", "Arm", "um\u00b7schlie\u00b7\u00dfet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sag': ist das mein Br\u00fcderlein?", "tokens": ["Sag'", ":", "ist", "das", "mein", "Br\u00fc\u00b7derl\u00b7ein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "ART", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schaut so ernsthaft und so sinnig,", "tokens": ["Schaut", "so", "ernst\u00b7haft", "und", "so", "sin\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt so z\u00e4rtlich doch und lind.", "tokens": ["Blickt", "so", "z\u00e4rt\u00b7lich", "doch", "und", "lind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sel'ge Mutter, fromm und innig,", "tokens": ["Sel'\u00b7ge", "Mut\u00b7ter", ",", "fromm", "und", "in\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, Du hast das sch\u00f6nste Kind. \u2013", "tokens": ["Ja", ",", "Du", "hast", "das", "sch\u00f6ns\u00b7te", "Kind", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kind, was bring' ich nur f\u00fcr Gaben", "tokens": ["Kind", ",", "was", "bring'", "ich", "nur", "f\u00fcr", "Ga\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Her in Deinen dunkeln Stall?", "tokens": ["Her", "in", "Dei\u00b7nen", "dun\u00b7keln", "Stall", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst Du all mein Spielzeug haben?", "tokens": ["Willst", "Du", "all", "mein", "Spiel\u00b7zeug", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Puppe? meinen Ball?", "tokens": ["Mei\u00b7ne", "Pup\u00b7pe", "?", "mei\u00b7nen", "Ball", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alle Blumen will ich bringen,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "will", "ich", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in meinem Garten stehn,", "tokens": ["Die", "in", "mei\u00b7nem", "Gar\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will Dir singen, will Dir springen,", "tokens": ["Will", "Dir", "sin\u00b7gen", ",", "will", "Dir", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer wieder von Dir gehn.", "tokens": ["Nim\u00b7mer", "wie\u00b7der", "von", "Dir", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sag' mir nur: Was soll ich machen?", "tokens": ["Sag'", "mir", "nur", ":", "Was", "soll", "ich", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kr\u00e4nz' und Kr\u00f6nlein, bunt und blank?", "tokens": ["Kr\u00e4nz'", "und", "Kr\u00f6n\u00b7lein", ",", "bunt", "und", "blank", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll ich weinen oder lachen?", "tokens": ["Soll", "ich", "wei\u00b7nen", "o\u00b7der", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Willst Du Fl\u00f6t' und Zitherklang?", "tokens": ["Willst", "Du", "Fl\u00f6t'", "und", "Zi\u00b7ther\u00b7klang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was Du willst, das sollst Du haben,", "tokens": ["Was", "Du", "willst", ",", "das", "sollst", "Du", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "PDS", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gieb mir nur Dein H\u00e4ndelein. \u2013", "tokens": ["Gieb", "mir", "nur", "Dein", "H\u00e4n\u00b7del\u00b7ein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fahrt nur hin, ihr andern Knaben!", "tokens": ["Fahrt", "nur", "hin", ",", "ihr", "an\u00b7dern", "Kna\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich kann eu'r Gespiel nicht sein. \u2013", "tokens": ["Ich", "kann", "eu'r", "Ge\u00b7spiel", "nicht", "sein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VAINF", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "La\u00df mich eine Stell' erwerben,", "tokens": ["La\u00df", "mich", "ei\u00b7ne", "Stell'", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kind, im Reich, das Du erwirbst!", "tokens": ["Kind", ",", "im", "Reich", ",", "das", "Du", "er\u00b7wirbst", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehr' mich leben, lehr' mich sterben,", "tokens": ["Lehr'", "mich", "le\u00b7ben", ",", "lehr'", "mich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Du lebst und wie Du stirbst. \u2013", "tokens": ["Wie", "Du", "lebst", "und", "wie", "Du", "stirbst", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun, Maria! voll Erbarmen", "tokens": ["Nun", ",", "Ma\u00b7ria", "!", "voll", "Er\u00b7bar\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "NE", "$.", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Reiche mir Dein Kindelein,", "tokens": ["Rei\u00b7che", "mir", "Dein", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich z\u00e4rtlich in den Armen", "tokens": ["Da\u00df", "ich", "z\u00e4rt\u00b7lich", "in", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiege solch ein Br\u00fcderlein.", "tokens": ["Wie\u00b7ge", "solch", "ein", "Br\u00fc\u00b7derl\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}