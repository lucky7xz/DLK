{"dta.poem.17170": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "21.  Der unerkandte GOtt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Was GOtt ist wei\u00df man nicht: Er ist nicht Licht/ nicht", "tokens": ["Was", "Gott", "ist", "wei\u00df", "man", "nicht", ":", "Er", "ist", "nicht", "Licht", "/", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "NN", "VAFIN", "VVFIN", "PIS", "PTKNEG", "$.", "PPER", "VAFIN", "PTKNEG", "NN", "$(", "PTKNEG"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geist/", "tokens": ["Geist", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Nicht Wonnigkeit/ nicht Eins/ nicht was man Gott-", "tokens": ["Nicht", "Won\u00b7nig\u00b7keit", "/", "nicht", "Eins", "/", "nicht", "was", "man", "Got\u00b7t"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$(", "PTKNEG", "NN", "$(", "PTKNEG", "PWS", "PIS", "TRUNC"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "heit heist:", "tokens": ["heit", "heist", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.5": {"text": "Nicht Wei\u00dfheit/ nicht Verstand/ nicht Liebe/ Wille/ G\u00fctte:", "tokens": ["Nicht", "Wei\u00df\u00b7heit", "/", "nicht", "Ver\u00b7stand", "/", "nicht", "Lie\u00b7be", "/", "Wil\u00b7le", "/", "G\u00fct\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"