{"textgrid.poem.32885": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der sch\u00f6ne Mond! still gr\u00fc\u00dft er mich!", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sch\u00f6ne Mond! still gr\u00fc\u00dft er mich!", "tokens": ["Der", "sch\u00f6\u00b7ne", "Mond", "!", "still", "gr\u00fc\u00dft", "er", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die jungen Blumen schlie\u00dfen sich,", "tokens": ["Die", "jun\u00b7gen", "Blu\u00b7men", "schlie\u00b7\u00dfen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der B\u00fcsche kleine S\u00e4nger schweigen:", "tokens": ["Der", "B\u00fc\u00b7sche", "klei\u00b7ne", "S\u00e4n\u00b7ger", "schwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur an dem nahen Wasserfall", "tokens": ["Nur", "an", "dem", "na\u00b7hen", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schl\u00e4gt die verliebte Nachtigall:", "tokens": ["Schl\u00e4gt", "die", "ver\u00b7lieb\u00b7te", "Nach\u00b7ti\u00b7gall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O m\u00f6chte sich ietzt Thyrsis zeigen!", "tokens": ["O", "m\u00f6ch\u00b7te", "sich", "ietzt", "Thyr\u00b7sis", "zei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "ADV", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nein: \u2013 \u2013 welch einen s\u00fc\u00dfen Schmerz", "tokens": ["Doch", "nein", ":", "\u2013", "\u2013", "welch", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Schmerz"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "$(", "$(", "PWAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Singt Philomel schon in mein Herz,", "tokens": ["Singt", "Phi\u00b7lo\u00b7mel", "schon", "in", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Es schmilzt bey ihren sanften Klagen:", "tokens": ["Es", "schmilzt", "bey", "ih\u00b7ren", "sanf\u00b7ten", "Kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Thyrsis k\u00e4m, ach wenn er k\u00e4m,", "tokens": ["Wenn", "Thyr\u00b7sis", "k\u00e4m", ",", "ach", "wenn", "er", "k\u00e4m", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$,", "ADV", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich k\u00fcssend in die Arme n\u00e4hm,", "tokens": ["Mich", "k\u00fcs\u00b7send", "in", "die", "Ar\u00b7me", "n\u00e4hm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was k\u00f6nnt er ietzt, ach k\u00e4m er! wagen?", "tokens": ["Was", "k\u00f6nnt", "er", "ietzt", ",", "ach", "k\u00e4m", "er", "!", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "ITJ", "VVFIN", "PPER", "$.", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}