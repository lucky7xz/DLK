{"textgrid.poem.47347": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Erziehung", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wiewohl man dir vom Nutzen spricht,", "tokens": ["Wie\u00b7wohl", "man", "dir", "vom", "Nut\u00b7zen", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den andre edle Wissenschaften schafften,", "tokens": ["Den", "and\u00b7re", "ed\u00b7le", "Wis\u00b7sen\u00b7schaf\u00b7ten", "schaff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch lasse du die Liebe nicht,", "tokens": ["Doch", "las\u00b7se", "du", "die", "Lie\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ist die edelste der Wissenschaften.", "tokens": ["Sie", "ist", "die", "e\u00b7dels\u00b7te", "der", "Wis\u00b7sen\u00b7schaf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn die Begierde nicht die Ruh',", "tokens": ["Wenn", "die", "Be\u00b7gier\u00b7de", "nicht", "die", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stille st\u00f6rt, o Seele! die du brauchest,", "tokens": ["Die", "Stil\u00b7le", "st\u00f6rt", ",", "o", "See\u00b7le", "!", "die", "du", "brau\u00b7chest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "FM", "NN", "$.", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So kommst du noch gewi\u00df dazu,", "tokens": ["So", "kommst", "du", "noch", "ge\u00b7wi\u00df", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du dich ganz ins Licht der Liebe tauchest.", "tokens": ["Da\u00df", "du", "dich", "ganz", "ins", "Licht", "der", "Lie\u00b7be", "tau\u00b7chest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O du! von deren Angesicht", "tokens": ["O", "du", "!", "von", "de\u00b7ren", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$.", "APPR", "PRELAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Fr\u00fchlingsruf erging an mein Gem\u00fcte,", "tokens": ["Der", "Fr\u00fch\u00b7lings\u00b7ruf", "er\u00b7ging", "an", "mein", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verwirre du die Triebe nicht,", "tokens": ["Ver\u00b7wir\u00b7re", "du", "die", "Trie\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hilf erziehn die zarte Himmelsbl\u00fcte!", "tokens": ["Und", "hilf", "er\u00b7ziehn", "die", "zar\u00b7te", "Him\u00b7mels\u00b7bl\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}