{"textgrid.poem.52914": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es sprengte aus dem K\u00f6nigsschlo\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sprengte aus dem K\u00f6nigsschlo\u00df", "tokens": ["Es", "spreng\u00b7te", "aus", "dem", "K\u00f6\u00b7nigs\u00b7schlo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Zug von stolzen Reitern,", "tokens": ["Ein", "Zug", "von", "stol\u00b7zen", "Rei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Paar voran dem andren Tro\u00df,", "tokens": ["Ein", "Paar", "vo\u00b7ran", "dem", "an\u00b7dren", "Tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den dienenden Begleitern.", "tokens": ["Den", "die\u00b7nen\u00b7den", "Be\u00b7glei\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wer war auf jenem braunen Ro\u00df", "tokens": ["Wer", "war", "auf", "je\u00b7nem", "brau\u00b7nen", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mann im Silberb\u00fcgel?", "tokens": ["Der", "Mann", "im", "Sil\u00b7ber\u00b7b\u00fc\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es hielt, so schien es, der Geno\u00df", "tokens": ["Es", "hielt", ",", "so", "schien", "es", ",", "der", "Ge\u00b7no\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Tier geheim am Z\u00fcgel?", "tokens": ["Sein", "Tier", "ge\u00b7heim", "am", "Z\u00fc\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbund kennst du unsres Herren Sohn", "tokens": ["\u00bb", "und", "kennst", "du", "uns\u00b7res", "Her\u00b7ren", "Sohn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht besser, unsren Prinzen!", "tokens": ["Nicht", "bes\u00b7ser", ",", "un\u00b7sren", "Prin\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der erbt vom Vater einst den Thron", "tokens": ["Der", "erbt", "vom", "Va\u00b7ter", "einst", "den", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und von uns die Provinzen.\u00ab", "tokens": ["Und", "von", "uns", "die", "Pro\u00b7vin\u00b7zen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gott sch\u00fctze, armes F\u00fcrstenkind,", "tokens": ["Gott", "sch\u00fct\u00b7ze", ",", "ar\u00b7mes", "F\u00fcrs\u00b7ten\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Auge und dein Leben!", "tokens": ["Dein", "Au\u00b7ge", "und", "dein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So jung, so gut, so klug \u2013 und blind:", "tokens": ["So", "jung", ",", "so", "gut", ",", "so", "klug", "\u2013", "und", "blind", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$(", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann's gr\u00f6\u00dfren Jammer geben?", "tokens": ["Kann's", "gr\u00f6\u00df\u00b7ren", "Jam\u00b7mer", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es sieht dein bleiches Angesicht,", "tokens": ["Es", "sieht", "dein", "blei\u00b7ches", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gefurcht von langen Leiden,", "tokens": ["Ge\u00b7furcht", "von", "lan\u00b7gen", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Bettler an der Ecke nicht,", "tokens": ["Den", "Bett\u00b7ler", "an", "der", "E\u00b7cke", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst w\u00fcrd es ihn beneiden.", "tokens": ["Sonst", "w\u00fcrd", "es", "ihn", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und auch die Liebe siehst du nicht", "tokens": ["Und", "auch", "die", "Lie\u00b7be", "siehst", "du", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Volkes dich geleiten,", "tokens": ["Des", "Vol\u00b7kes", "dich", "ge\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mechanisch gr\u00fc\u00dft dein Angesicht", "tokens": ["Me\u00b7cha\u00b7nisch", "gr\u00fc\u00dft", "dein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4chelnd aller Seiten.", "tokens": ["Und", "l\u00e4\u00b7chelnd", "al\u00b7ler", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch einst, mein Prinz, wie wird es sein,", "tokens": ["Doch", "einst", ",", "mein", "Prinz", ",", "wie", "wird", "es", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du bist K\u00f6nig 'worden,", "tokens": ["Wenn", "du", "bist", "K\u00f6\u00b7nig", "'wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn erst der schwere Szepter dein,", "tokens": ["Wenn", "erst", "der", "schwe\u00b7re", "Szep\u00b7ter", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dein des Vaters Orden?", "tokens": ["Und", "dein", "des", "Va\u00b7ters", "Or\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Soll dann f\u00fcr dich die fremde Hand", "tokens": ["Soll", "dann", "f\u00fcr", "dich", "die", "frem\u00b7de", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Volk so sicher leiten,", "tokens": ["Dein", "Volk", "so", "si\u00b7cher", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie jetzt dein Ro\u00df am G\u00e4ngelband", "tokens": ["Wie", "jetzt", "dein", "Ro\u00df", "am", "G\u00e4n\u00b7gel\u00b7band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mann zu deiner Seiten?", "tokens": ["Der", "Mann", "zu", "dei\u00b7ner", "Sei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Gen\u00fcgt es dir, so blo\u00df zum Schein", "tokens": ["Ge\u00b7n\u00fcgt", "es", "dir", ",", "so", "blo\u00df", "zum", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu f\u00fchren Zaum und Z\u00fcgel?", "tokens": ["Zu", "f\u00fch\u00b7ren", "Zaum", "und", "Z\u00fc\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wirst du fest im Herrschen sein,", "tokens": ["Und", "wirst", "du", "fest", "im", "Herr\u00b7schen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie heute fest im B\u00fcgel?", "tokens": ["Wie", "heu\u00b7te", "fest", "im", "B\u00fc\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dein Ro\u00df wird scheu \u2013 Hab acht, hab acht!", "tokens": ["Dein", "Ro\u00df", "wird", "scheu", "\u2013", "Hab", "acht", ",", "hab", "acht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "NN", "CARD", "$,", "VAFIN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das war ein schlimmes Zeichen,", "tokens": ["Das", "war", "ein", "schlim\u00b7mes", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dr\u00fcck ihm die Sporen nicht mit Macht,", "tokens": ["Dr\u00fcck", "ihm", "die", "Spo\u00b7ren", "nicht", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die goldnen, in die Weichen!", "tokens": ["Die", "gold\u00b7nen", ",", "in", "die", "Wei\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Gemach, du blindes F\u00fcrstenkind!", "tokens": ["Ge\u00b7mach", ",", "du", "blin\u00b7des", "F\u00fcrs\u00b7ten\u00b7kind", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Zaum ist bald zerrissen,", "tokens": ["Ein", "Zaum", "ist", "bald", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wilder noch als Hengste sind", "tokens": ["Und", "wil\u00b7der", "noch", "als", "Hengs\u00b7te", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADV", "KOUS", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die V\u00f6lker, mu\u00dft du wissen.", "tokens": ["Die", "V\u00f6l\u00b7ker", ",", "mu\u00dft", "du", "wis\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}