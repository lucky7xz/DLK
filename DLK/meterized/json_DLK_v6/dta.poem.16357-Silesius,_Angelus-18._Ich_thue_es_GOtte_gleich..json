{"dta.poem.16357": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "18.  Ich thue es GOtte gleich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott liebt mich \u00fcber sich: Lieb ich Jhn \u00fcber mich;", "tokens": ["Gott", "liebt", "mich", "\u00fc\u00b7ber", "sich", ":", "Lieb", "ich", "Jhn", "\u00fc\u00b7ber", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PRF", "$.", "NN", "PPER", "PPER", "APPR", "PPER", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So geb ich Jhm sovil/ als Er mir gibt au\u00df sich.", "tokens": ["So", "geb", "ich", "Jhm", "so\u00b7vil", "/", "als", "Er", "mir", "gibt", "au\u00df", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$(", "KOUS", "PPER", "PPER", "VVFIN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}