{"textgrid.poem.60993": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: O kennst du, Herz, die beiden Schwesterengel,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O kennst du, Herz, die beiden Schwesterengel,", "tokens": ["O", "kennst", "du", ",", "Herz", ",", "die", "bei\u00b7den", "Schwes\u00b7te\u00b7ren\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herabgestiegen aus dem Himmelreich:", "tokens": ["Her\u00b7ab\u00b7ge\u00b7stie\u00b7gen", "aus", "dem", "Him\u00b7mel\u00b7reich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stillsegnend Freundschaft mit dem Lilienstengel,", "tokens": ["Still\u00b7seg\u00b7nend", "Freund\u00b7schaft", "mit", "dem", "Li\u00b7li\u00b7ens\u00b7ten\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Entz\u00fcndend Liebe mit dem Rosenzweig?", "tokens": ["Ent\u00b7z\u00fcn\u00b7dend", "Lie\u00b7be", "mit", "dem", "Ro\u00b7sen\u00b7zweig", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schwarzlockig ist die Liebe, feurig gl\u00fchend,", "tokens": ["Schwarz\u00b7lo\u00b7ckig", "ist", "die", "Lie\u00b7be", ",", "feu\u00b7rig", "gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sch\u00f6n wie der Lenz, der hastig sprossen will;", "tokens": ["Sch\u00f6n", "wie", "der", "Lenz", ",", "der", "has\u00b7tig", "spros\u00b7sen", "will", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die Freundschaft blond, in sanftern Farben bl\u00fchend,", "tokens": ["Die", "Freund\u00b7schaft", "blond", ",", "in", "sanf\u00b7tern", "Far\u00b7ben", "bl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie die Sommernacht so mild und still;", "tokens": ["Und", "wie", "die", "Som\u00b7mer\u00b7nacht", "so", "mild", "und", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Lieb' ein brausend Meer, wo im Gewimmel", "tokens": ["Die", "Lieb'", "ein", "brau\u00b7send", "Meer", ",", "wo", "im", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vieltausendf\u00e4ltig Wog' an Woge schl\u00e4gt;", "tokens": ["Viel\u00b7tau\u00b7send\u00b7f\u00e4l\u00b7tig", "Wo\u00b7g'", "an", "Wo\u00b7ge", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Freundschaft ein tiefer Bergsee, der den Himmel", "tokens": ["Freund\u00b7schaft", "ein", "tie\u00b7fer", "Berg\u00b7see", ",", "der", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Klar widerspiegelnd in den Fluten tr\u00e4gt.", "tokens": ["Klar", "wi\u00b7der\u00b7spie\u00b7gelnd", "in", "den", "Flu\u00b7ten", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Die Liebe bricht herein wie Wetterblitzen,", "tokens": ["Die", "Lie\u00b7be", "bricht", "her\u00b7ein", "wie", "Wet\u00b7ter\u00b7blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Freundschaft kommt wie d\u00e4mmernd Mondenlicht;", "tokens": ["Die", "Freund\u00b7schaft", "kommt", "wie", "d\u00e4m\u00b7mernd", "Mon\u00b7den\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe will erwerben und besitzen,", "tokens": ["Die", "Lie\u00b7be", "will", "er\u00b7wer\u00b7ben", "und", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Freundschaft opfert, doch sie fordert nicht.", "tokens": ["Die", "Freund\u00b7schaft", "op\u00b7fert", ",", "doch", "sie", "for\u00b7dert", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch dreimal selig, dreimal hoch zu preisen", "tokens": ["Doch", "drei\u00b7mal", "se\u00b7lig", ",", "drei\u00b7mal", "hoch", "zu", "prei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Herz, wo beide freundlich eingekehrt,", "tokens": ["Das", "Herz", ",", "wo", "bei\u00b7de", "freund\u00b7lich", "ein\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wo die Glut der Rose nicht dem leisen,", "tokens": ["Und", "wo", "die", "Glut", "der", "Ro\u00b7se", "nicht", "dem", "lei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "PTKNEG", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geheimnisvollen Bl\u00fchn der Lilie wehrt!", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7vol\u00b7len", "Bl\u00fchn", "der", "Li\u00b7lie", "wehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}