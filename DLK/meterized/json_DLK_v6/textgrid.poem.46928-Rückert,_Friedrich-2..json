{"textgrid.poem.46928": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bl\u00fchetest die sch\u00f6nste aller Eichen,", "tokens": ["Du", "bl\u00fc\u00b7he\u00b7test", "die", "sch\u00f6ns\u00b7te", "al\u00b7ler", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Germania, im tiefsten Kern gesunde;", "tokens": ["Ger\u00b7ma\u00b7nia", ",", "im", "tiefs\u00b7ten", "Kern", "ge\u00b7sun\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Als dir der R\u00f6mer gegen\u00fcberstunde,", "tokens": ["Als", "dir", "der", "R\u00f6\u00b7mer", "ge\u00b7gen\u00b7\u00fc\u00b7bers\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "K\u00f6nnt' an die \u00c4ste dir sein Speer nicht reichen.", "tokens": ["K\u00f6nnt'", "an", "die", "\u00c4s\u00b7te", "dir", "sein", "Speer", "nicht", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da schlug ein andrer Feind mit listigen Streichen", "tokens": ["Da", "schlug", "ein", "an\u00b7drer", "Feind", "mit", "lis\u00b7ti\u00b7gen", "Strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dir von der Westseit' eine schwere Wunde,", "tokens": ["Dir", "von", "der", "West\u00b7seit'", "ei\u00b7ne", "schwe\u00b7re", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.7": {"text": "Hieb von den \u00c4sten manche dir zum Grunde,", "tokens": ["Hieb", "von", "den", "\u00c4s\u00b7ten", "man\u00b7che", "dir", "zum", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PIS", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und zimmerte daraus sich Siegeszeichen.", "tokens": ["Und", "zim\u00b7mer\u00b7te", "da\u00b7raus", "sich", "Sie\u00b7ges\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PRF", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nun will er gar den ganzen Stamm zerhauen,", "tokens": ["Nun", "will", "er", "gar", "den", "gan\u00b7zen", "Stamm", "zer\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und tr\u00f6stet dich: \u00bbIch will euch wilde \u00c4ste", "tokens": ["Und", "tr\u00f6s\u00b7tet", "dich", ":", "\u00bb", "Ich", "will", "euch", "wil\u00b7de", "\u00c4s\u00b7te"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$.", "$(", "PPER", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Zu einem wohlgefugten Haus verbauen.\u00ab", "tokens": ["Zu", "ei\u00b7nem", "wohl\u00b7ge\u00b7fug\u00b7ten", "Haus", "ver\u00b7bau\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Er baue dich zum sch\u00f6nsten der Pal\u00e4ste,", "tokens": ["Er", "bau\u00b7e", "dich", "zum", "sch\u00f6ns\u00b7ten", "der", "Pa\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch wird dir kein lebendiger Lenz mehr tauen,", "tokens": ["Doch", "wird", "dir", "kein", "le\u00b7ben\u00b7di\u00b7ger", "Lenz", "mehr", "tau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Nicht rauschen wirst du mehr im freien Weste.", "tokens": ["Nicht", "rau\u00b7schen", "wirst", "du", "mehr", "im", "frei\u00b7en", "Wes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}