{"textgrid.poem.62625": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herr! ihr habt euch nun mit Leib und Seel verpflicht.", "tokens": ["Mein", "Herr", "!", "ihr", "habt", "euch", "nun", "mit", "Leib", "und", "Seel", "ver\u00b7pflicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich wundert, da\u00df euch noch der s\u00fcsse K\u00fctzel sticht,", "tokens": ["Mich", "wun\u00b7dert", ",", "da\u00df", "euch", "noch", "der", "s\u00fcs\u00b7se", "K\u00fct\u00b7zel", "sticht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da ihr doch selber wi\u00dft, wie weh euch einst geschehen,", "tokens": ["Da", "ihr", "doch", "sel\u00b7ber", "wi\u00dft", ",", "wie", "weh", "euch", "einst", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ihr Dianens Bild zu k\u00fchnlich angesehen.", "tokens": ["Als", "ihr", "Di\u00b7a\u00b7nens", "Bild", "zu", "k\u00fchn\u00b7lich", "an\u00b7ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr schweigt! Ich merke schon, da\u00df euch das Gl\u00fccke wiegt,", "tokens": ["Ihr", "schweigt", "!", "Ich", "mer\u00b7ke", "schon", ",", "da\u00df", "euch", "das", "Gl\u00fc\u00b7cke", "wiegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nunmehr eure Brust mit G\u00f6tter-Kost vergn\u00fcgt;", "tokens": ["Und", "nun\u00b7mehr", "eu\u00b7re", "Brust", "mit", "G\u00f6t\u00b7ter\u00b7Kost", "ver\u00b7gn\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich g\u00f6nn euch diesen Tausch; ja, darf ich prophezeyhen,", "tokens": ["Ich", "g\u00f6nn", "euch", "die\u00b7sen", "Tausch", ";", "ja", ",", "darf", "ich", "pro\u00b7phe\u00b7zey\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "NN", "$.", "PTKANT", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird euch Cypripor zum Liebes-Priester weyhen.", "tokens": ["So", "wird", "euch", "Cyp\u00b7ri\u00b7por", "zum", "Lie\u00b7bes\u00b7Pries\u00b7ter", "wey\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}