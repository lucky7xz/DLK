{"dta.poem.12824": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft einer k\u00f6chin.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich machte kurtz und gut den besten schnitzer lahm,", "tokens": ["Ich", "mach\u00b7te", "kurtz", "und", "gut", "den", "bes\u00b7ten", "schnit\u00b7zer", "lahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der steiff und wohl gesch\u00e4rfft in meine k\u00fcche kam.", "tokens": ["Der", "steiff", "und", "wohl", "ge\u00b7sch\u00e4rfft", "in", "mei\u00b7ne", "k\u00fc\u00b7che", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADV", "VVPP", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den g\u00e4sten gab ich fleisch, den kern von s\u00fc\u00dfen dingen,", "tokens": ["Den", "g\u00e4s\u00b7ten", "gab", "ich", "fleisch", ",", "den", "kern", "von", "s\u00fc\u00b7\u00dfen", "din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun will ich auch dem tod die kalte schaale bringen.", "tokens": ["Nun", "will", "ich", "auch", "dem", "tod", "die", "kal\u00b7te", "schaa\u00b7le", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "wo