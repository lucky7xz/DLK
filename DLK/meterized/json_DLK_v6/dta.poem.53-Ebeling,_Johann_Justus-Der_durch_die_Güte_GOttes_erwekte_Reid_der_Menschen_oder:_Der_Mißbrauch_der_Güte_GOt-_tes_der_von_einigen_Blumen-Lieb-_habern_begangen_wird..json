{"dta.poem.53": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der durch die G\u00fcte GOttes  \n erwekte Reid der Menschen  \n oder:  \n  Der Mi\u00dfbrauch der G\u00fcte GOt-  \n tes der von einigen Blumen-Lieb-  \n habern begangen wird.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Kinder der Natur, die Blumen\nkommen mir;", "tokens": ["Die", "Kin\u00b7der", "der", "Na\u00b7tur", ",", "die", "Blu\u00b7men", "kom\u00b7men", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So oft ich solche seh, als klare Zeu-", "tokens": ["So", "oft", "ich", "sol\u00b7che", "seh", ",", "als", "kla\u00b7re", "Zeu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PIS", "VVFIN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die wie mit einem Mund zum Ruhm", "tokens": ["Die", "wie", "mit", "ei\u00b7nem", "Mund", "zum", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sei von uns als ", "tokens": ["Er", "sei", "von", "uns", "als"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "KOKOM"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.5": {"text": "Erweget sie nur recht ein aufmerksam Gem\u00fcth;", "tokens": ["Er\u00b7we\u00b7get", "sie", "nur", "recht", "ein", "auf\u00b7merk\u00b7sam", "Ge\u00b7m\u00fcth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So kommen sie uns vor, als Sprossen seiner G\u00fct,", "tokens": ["So", "kom\u00b7men", "sie", "uns", "vor", ",", "als", "Spros\u00b7sen", "sei\u00b7ner", "G\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "KOUS", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die er uns Hauffen-weis an allen Orten giebet,", "tokens": ["Die", "er", "uns", "Hauf\u00b7fen\u00b7weis", "an", "al\u00b7len", "Or\u00b7ten", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil er den Menschen hold, und sie als Kinder liebet.", "tokens": ["Weil", "er", "den", "Men\u00b7schen", "hold", ",", "und", "sie", "als", "Kin\u00b7der", "lie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "$,", "KON", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Blumen zeigen uns des Sch\u00f6pfers G\u00fctigkeit,", "tokens": ["Die", "Blu\u00b7men", "zei\u00b7gen", "uns", "des", "Sch\u00f6p\u00b7fers", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil ihr gef\u00e4rbter Schmuk das Auge sehr erfreut;", "tokens": ["Weil", "ihr", "ge\u00b7f\u00e4rb\u00b7ter", "Schmuk", "das", "Au\u00b7ge", "sehr", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Menge zeiget uns wie sehr er uns gewogen,", "tokens": ["Die", "Men\u00b7ge", "zei\u00b7get", "uns", "wie", "sehr", "er", "uns", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOKOM", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil er mit ihrer Zier Feld, Wiesen, Wald be-", "tokens": ["Weil", "er", "mit", "ih\u00b7rer", "Zier", "Feld", ",", "Wie\u00b7sen", ",", "Wald", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,", "NN", "$,", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die G\u00e4rten ausgepuzt: indem das gr\u00fcne Land,", "tokens": ["Die", "G\u00e4r\u00b7ten", "aus\u00b7ge\u00b7puzt", ":", "in\u00b7dem", "das", "gr\u00fc\u00b7ne", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit einem sammtnen Tuch von Gra\u00df gewirkt, be-", "tokens": ["Mit", "ei\u00b7nem", "sammt\u00b7nen", "Tuch", "von", "Gra\u00df", "ge\u00b7wirkt", ",", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und mit der Blumen Gold recht herrlich ausge-", "tokens": ["Und", "mit", "der", "Blu\u00b7men", "Gold", "recht", "herr\u00b7lich", "aus\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Mit Silberfarbnen Glanz recht kostbar ausgestik-", "tokens": ["Mit", "Sil\u00b7ber\u00b7farb\u00b7nen", "Glanz", "recht", "kost\u00b7bar", "aus\u00b7ges\u00b7ti\u00b7k"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und andren Farben mehr die die Natur gemahlt,", "tokens": ["Und", "an\u00b7dren", "Far\u00b7ben", "mehr", "die", "die", "Na\u00b7tur", "ge\u00b7mahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Woraus des H\u00f6chsten G\u00fct uns stets ins Auge", "tokens": ["Wo\u00b7raus", "des", "H\u00f6chs\u00b7ten", "G\u00fct", "uns", "stets", "ins", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der Schauplaz steht uns frei, da\u00df wir die Lust", "tokens": ["Der", "Schau\u00b7plaz", "steht", "uns", "frei", ",", "da\u00df", "wir", "die", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die D\u00fcfte schenkt er uns, die ins Gehirne fliessen,", "tokens": ["Die", "D\u00fcf\u00b7te", "schenkt", "er", "uns", ",", "die", "ins", "Ge\u00b7hir\u00b7ne", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und den Geruch erfreun, der ihre Lieblichkeit,", "tokens": ["Und", "den", "Ge\u00b7ruch", "er\u00b7freun", ",", "der", "ih\u00b7re", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Begierig in sich zieht zur holden Fr\u00fchlings-Zeit.", "tokens": ["Be\u00b7gie\u00b7rig", "in", "sich", "zieht", "zur", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Des Sch\u00f6pfers Meinung ist, da\u00df wir an seinen", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Mei\u00b7nung", "ist", ",", "da\u00df", "wir", "an", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "$,", "KOUS", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die jedem unverwehrt, so Aug als Herze laben:", "tokens": ["Die", "je\u00b7dem", "un\u00b7ver\u00b7wehrt", ",", "so", "Aug", "als", "Her\u00b7ze", "la\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "$,", "ADV", "NN", "KOKOM", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und darum hat er auch in Wald und freien Feld,", "tokens": ["Und", "da\u00b7rum", "hat", "er", "auch", "in", "Wald", "und", "frei\u00b7en", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So vielen Blumen-Schmuk zum Zierrath aufge-", "tokens": ["So", "vie\u00b7len", "Blu\u00b7men\u00b7Schmuk", "zum", "Zier\u00b7rath", "auf\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die Menschen haben auch die Augen dran erg\u00f6z-", "tokens": ["Die", "Men\u00b7schen", "ha\u00b7ben", "auch", "die", "Au\u00b7gen", "dran", "er\u00b7g\u00f6z"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "PAV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und aus der fremden Welt sie hie und da versezzet,", "tokens": ["Und", "aus", "der", "frem\u00b7den", "Welt", "sie", "hie", "und", "da", "ver\u00b7sez\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PPER", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und G\u00e4rten angelegt, durch ihrer Lust Bem\u00fchn,", "tokens": ["Und", "G\u00e4r\u00b7ten", "an\u00b7ge\u00b7legt", ",", "durch", "ih\u00b7rer", "Lust", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der sch\u00f6nsten Blumen Art vor sich selbst aufzu-", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Blu\u00b7men", "Art", "vor", "sich", "selbst", "auf\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "PRF", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Da w\u00e4hlen sie die aus, die sie als K\u00f6niginnen,", "tokens": ["Da", "w\u00e4h\u00b7len", "sie", "die", "aus", ",", "die", "sie", "als", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "PTKVZ", "$,", "PRELS", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Von einer jeden Art vor andern lieb gewinnen.", "tokens": ["Von", "ei\u00b7ner", "je\u00b7den", "Art", "vor", "an\u00b7dern", "lieb", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "APPR", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie schaffen Saamen an, und Zwiebeln draus der", "tokens": ["Sie", "schaf\u00b7fen", "Saa\u00b7men", "an", ",", "und", "Zwie\u00b7beln", "draus", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "KON", "NN", "PAV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "So mancherlei erzielt, die roth, gelb, gr\u00fcn und", "tokens": ["So", "man\u00b7cher\u00b7lei", "er\u00b7zielt", ",", "die", "roth", ",", "gelb", ",", "gr\u00fcn", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "$,", "PRELS", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und die vor andern sch\u00f6n nach ihrer Meinung", "tokens": ["Und", "die", "vor", "an\u00b7dern", "sch\u00f6n", "nach", "ih\u00b7rer", "Mei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PIS", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Die pflegen sie mit Ruhm vor andern zu bekr\u00e4n-", "tokens": ["Die", "pfle\u00b7gen", "sie", "mit", "Ruhm", "vor", "an\u00b7dern", "zu", "be\u00b7kr\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "APPR", "PIS", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wird eine theur geacht; als eine Seltenheit:", "tokens": ["Wird", "ei\u00b7ne", "theur", "ge\u00b7acht", ";", "als", "ei\u00b7ne", "Sel\u00b7ten\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "VVPP", "$.", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So mu\u00df des H\u00f6chsten G\u00fct den h\u00e4misch bittren", "tokens": ["So", "mu\u00df", "des", "H\u00f6chs\u00b7ten", "G\u00fct", "den", "h\u00e4\u00b7misch", "bit\u00b7tren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJD", "VVINF"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.39": {"text": "Gar oft Gelegenheit zu einer S\u00fcnde geben,", "tokens": ["Gar", "oft", "Ge\u00b7le\u00b7gen\u00b7heit", "zu", "ei\u00b7ner", "S\u00fcn\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die sch\u00e4ndlich f\u00fcr uns ist, wenn wir als Menschen", "tokens": ["Die", "sch\u00e4nd\u00b7lich", "f\u00fcr", "uns", "ist", ",", "wenn", "wir", "als", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PPER", "VAFIN", "$,", "KOUS", "PPER", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Es ist bei einigen, die man Blumisten heist,", "tokens": ["Es", "ist", "bei", "ei\u00b7ni\u00b7gen", ",", "die", "man", "Blu\u00b7mis\u00b7ten", "heist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "$,", "PRELS", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.42": {"text": "Da\u00df sie dieselbe Blum, die man als herrlich prei\u00dft,", "tokens": ["Da\u00df", "sie", "die\u00b7sel\u00b7be", "Blum", ",", "die", "man", "als", "herr\u00b7lich", "prei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NE", "$,", "PRELS", "PIS", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Nicht leichtlich einen Freund in seinen Garten g\u00f6n-", "tokens": ["Nicht", "leicht\u00b7lich", "ei\u00b7nen", "Freund", "in", "sei\u00b7nen", "Gar\u00b7ten", "g\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Sie wollen nur davon sich als Besizzer nennen.", "tokens": ["Sie", "wol\u00b7len", "nur", "da\u00b7von", "sich", "als", "Be\u00b7siz\u00b7zer", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PAV", "PRF", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und wenn sie von der Art den gr\u00f6sten Ueberflus,", "tokens": ["Und", "wenn", "sie", "von", "der", "Art", "den", "gr\u00f6s\u00b7ten", "Ue\u00b7ber\u00b7flus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "So theilen sie doch nicht mit andern den Genus;", "tokens": ["So", "thei\u00b7len", "sie", "doch", "nicht", "mit", "an\u00b7dern", "den", "Ge\u00b7nus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ein andrer sol sie nicht in seinem Garten haben,", "tokens": ["Ein", "an\u00b7drer", "sol", "sie", "nicht", "in", "sei\u00b7nem", "Gar\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und sich nicht an den Glanz, den man bewundert", "tokens": ["Und", "sich", "nicht", "an", "den", "Glanz", ",", "den", "man", "be\u00b7wun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PRF", "PTKNEG", "APPR", "ART", "NN", "$,", "PRELS", "PIS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Man wirft sie lieber weg, und l\u00e4\u00dft sie untergehn,", "tokens": ["Man", "wirft", "sie", "lie\u00b7ber", "weg", ",", "und", "l\u00e4\u00dft", "sie", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Damit sie nur nicht auch bei andern sei zu sehn.", "tokens": ["Da\u00b7mit", "sie", "nur", "nicht", "auch", "bei", "an\u00b7dern", "sei", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "PIS", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Das ist der Mi\u00dfgunst Art, der pflegt es zu ver-", "tokens": ["Das", "ist", "der", "Mi\u00df\u00b7gunst", "Art", ",", "der", "pflegt", "es", "zu", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "PRELS", "VVFIN", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "Wenn andere mit ihr ein gleiches Gut geniessen;", "tokens": ["Wenn", "an\u00b7de\u00b7re", "mit", "ihr", "ein", "glei\u00b7ches", "Gut", "ge\u00b7nies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Ich weis die Neidischen, die wenden dabei ein,", "tokens": ["Ich", "weis", "die", "Nei\u00b7di\u00b7schen", ",", "die", "wen\u00b7den", "da\u00b7bei", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ART", "NN", "$,", "PRELS", "PIS", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Sonst w\u00fcrde ihre Blum und Sch\u00f6nheit zu gemein.", "tokens": ["Sonst", "w\u00fcr\u00b7de", "ih\u00b7re", "Blum", "und", "Sch\u00f6n\u00b7heit", "zu", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Was sch\u00f6n ist bleibet sch\u00f6n, und kan uns doch ver-", "tokens": ["Was", "sch\u00f6n", "ist", "blei\u00b7bet", "sch\u00f6n", ",", "und", "kan", "uns", "doch", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "VVFIN", "ADJD", "$,", "KON", "VMFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.56": {"text": "Und solte auch die Art in allen G\u00e4rten liegen.", "tokens": ["Und", "sol\u00b7te", "auch", "die", "Art", "in", "al\u00b7len", "G\u00e4r\u00b7ten", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Man merkt die Einbildung, was selten, das sei", "tokens": ["Man", "merkt", "die", "Ein\u00b7bil\u00b7dung", ",", "was", "sel\u00b7ten", ",", "das", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "PDS", "VAFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.58": {"text": "Die kan nach der Vernunft nicht gar zu wol be-", "tokens": ["Die", "kan", "nach", "der", "Ver\u00b7nunft", "nicht", "gar", "zu", "wol", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "PTKNEG", "ADV", "PTKA", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.59": {"text": "Die Meinung die regiert; nach ihres Wahns Ge-", "tokens": ["Die", "Mei\u00b7nung", "die", "re\u00b7giert", ";", "nach", "ih\u00b7res", "Wahns", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.60": {"text": "Pflegt man der Dinge Preis und ihren Werth zu", "tokens": ["Pflegt", "man", "der", "Din\u00b7ge", "Preis", "und", "ih\u00b7ren", "Werth", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "NN", "KON", "PPOSAT", "NN", "PTKZU"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.61": {"text": "Die Eigenliebe will die Blume sol allein,", "tokens": ["Die", "Ei\u00b7gen\u00b7lie\u00b7be", "will", "die", "Blu\u00b7me", "sol", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VMFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Die kostbar wird gesch\u00e4zt, in ihren Garten seyn:", "tokens": ["Die", "kost\u00b7bar", "wird", "ge\u00b7sch\u00e4zt", ",", "in", "ih\u00b7ren", "Gar\u00b7ten", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Darum zerreisset man des g\u00fctgen Sch\u00f6pfers Ga-", "tokens": ["Da\u00b7rum", "zer\u00b7reis\u00b7set", "man", "des", "g\u00fct\u00b7gen", "Sch\u00f6p\u00b7fers", "Ga"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Die seltne Art vor sich zur Augenlust zu haben.", "tokens": ["Die", "selt\u00b7ne", "Art", "vor", "sich", "zur", "Au\u00b7gen\u00b7lust", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "APPRART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Die ewge G\u00fctigkeit, die uns die Blumen giebt,", "tokens": ["Die", "ew\u00b7ge", "G\u00fc\u00b7tig\u00b7keit", ",", "die", "uns", "die", "Blu\u00b7men", "giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Zeigt uns im Beispiel an, da\u00df sie ja alle liebt,", "tokens": ["Zeigt", "uns", "im", "Bei\u00b7spiel", "an", ",", "da\u00df", "sie", "ja", "al\u00b7le", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Und wenn sie uns mehr schenkt, als wir davon ver-", "tokens": ["Und", "wenn", "sie", "uns", "mehr", "schenkt", ",", "als", "wir",