 {"text": "Die uns so angenehm, in jeden Blumen-Gar-", "tokens": ["Die", "uns", "so", "an\u00b7ge\u00b7nehm", ",", "in", "je\u00b7den", "Blu\u00b7men\u00b7Ga\u00b7r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.73": {"text": "Es nimmt der deinen nicht der Farben Mannifalt,", "tokens": ["Es", "nimmt", "der", "dei\u00b7nen", "nicht", "der", "Far\u00b7ben", "Man\u00b7ni\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPOSAT", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Noch seine Lieblichkeit, Geruch, Form und Ge-", "tokens": ["Noch", "sei\u00b7ne", "Lieb\u00b7lich\u00b7keit", ",", "Ge\u00b7ruch", ",", "Form", "und", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.75": {"text": "Und sage was du wilt, es wird dir dein Gewissen", "tokens": ["Und", "sa\u00b7ge", "was", "du", "wilt", ",", "es", "wird", "dir", "dein", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Des Neides Bitterkeit auch hierin zeigen m\u00fcssen.", "tokens": ["Des", "Nei\u00b7des", "Bit\u00b7ter\u00b7keit", "auch", "hie\u00b7rin", "zei\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "VVINF", "VMINF", "$."], "met