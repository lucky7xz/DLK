{"dta.poem.21571": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "An die oberen Zehntausend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O kehrtet einmal Ihr aus den Pal\u00e4sten", "tokens": ["O", "kehr\u00b7tet", "ein\u00b7mal", "Ihr", "aus", "den", "Pa\u00b7l\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im dunstigen Dunkel enger Gassen ein!", "tokens": ["Im", "duns\u00b7ti\u00b7gen", "Dun\u00b7kel", "en\u00b7ger", "Gas\u00b7sen", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O kehrtet einmal Ihr von Euren Festen", "tokens": ["O", "kehr\u00b7tet", "ein\u00b7mal", "Ihr", "von", "Eu\u00b7ren", "Fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In\u2019s vierte Stockwerk, wo beim Oellichtschein", "tokens": ["In's", "vier\u00b7te", "Stock\u00b7werk", ",", "wo", "beim", "O\u00b7el\u00b7licht\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Blutarme N\u00e4hterinnen um den Bissen", "tokens": ["Blut\u00b7ar\u00b7me", "N\u00e4h\u00b7te\u00b7rin\u00b7nen", "um", "den", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des lieben Brods zehn Stunden n\u00e4hen m\u00fcssen!", "tokens": ["Des", "lie\u00b7ben", "Brods", "zehn", "Stun\u00b7den", "n\u00e4\u00b7hen", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "CARD", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kr\u00f6cht\u2019 einmal Ihr mit Eurem Schmuck behangen", "tokens": ["Kr\u00f6cht'", "ein\u00b7mal", "Ihr", "mit", "Eu\u00b7rem", "Schmuck", "be\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Kellerwohnung, wo der Schuster flickt,", "tokens": ["Zur", "Kel\u00b7ler\u00b7woh\u00b7nung", ",", "wo", "der", "Schus\u00b7ter", "flickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein armes Weib mit hungerbleichen Wangen", "tokens": ["Sein", "ar\u00b7mes", "Weib", "mit", "hun\u00b7ger\u00b7blei\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den S\u00e4ugling an die welken Br\u00fcste dr\u00fcckt,", "tokens": ["Den", "S\u00e4ug\u00b7ling", "an", "die", "wel\u00b7ken", "Br\u00fcs\u00b7te", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von Einer Mark oft sieben Menschen leben,", "tokens": ["Von", "Ei\u00b7ner", "Mark", "oft", "sie\u00b7ben", "Men\u00b7schen", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die doch dem Kaiser noch den Groschen geben!", "tokens": ["Die", "doch", "dem", "Kai\u00b7ser", "noch", "den", "Gro\u00b7schen", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}