{"textgrid.poem.62990": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zween Pilger wallten einst nach Rom,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zween Pilger wallten einst nach Rom,", "tokens": ["Zween", "Pil\u00b7ger", "wall\u00b7ten", "einst", "nach", "Rom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um sich in dem Sanct Peters-Dohm", "tokens": ["Um", "sich", "in", "dem", "Sanct", "Pe\u00b7ter\u00b7sDohm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "APPR", "ART", "VVFIN", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Mit Jubelabla\u00df zu versorgen.", "tokens": ["Mit", "Ju\u00b7be\u00b7lab\u00b7la\u00df", "zu", "ver\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie hatten, bettelarm an Geist", "tokens": ["Sie", "hat\u00b7ten", ",", "bet\u00b7tel\u00b7arm", "an", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Beutel, Welschland halb durchreist,", "tokens": ["Und", "Beu\u00b7tel", ",", "Wel\u00b7schland", "halb", "durc\u00b7hreist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als sich an einem sch\u00f6nen Morgen", "tokens": ["Als", "sich", "an", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Hauptstadt wie ein Adlernest", "tokens": ["Die", "Haupt\u00b7stadt", "wie", "ein", "Ad\u00b7ler\u00b7nest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von ferne zeigte. Welch ein Fest", "tokens": ["Von", "fer\u00b7ne", "zeig\u00b7te", ".", "Welch", "ein", "Fest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "$.", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "F\u00fcr unser Paar! Wie viele Meilen", "tokens": ["F\u00fcr", "un\u00b7ser", "Paar", "!", "Wie", "vie\u00b7le", "Mei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sinds noch bis Rom? So frug Protas", "tokens": ["Sinds", "noch", "bis", "Rom", "?", "So", "frug", "Pro\u00b7tas"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NE", "$.", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein Weiblein, das am Wege sa\u00df.", "tokens": ["Ein", "Weib\u00b7lein", ",", "das", "am", "We\u00b7ge", "sa\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "\u00bbnoch acht\u00ab Auf, Bruder! la\u00df uns eilen", "tokens": ["\u00bb", "noch", "acht", "\u00ab", "Auf", ",", "Bru\u00b7der", "!", "la\u00df", "uns", "ei\u00b7len"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "CARD", "$(", "APPR", "$,", "NN", "$.", "VVIMP", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Rief sein Gef\u00e4hrte Kasimir,", "tokens": ["Rief", "sein", "Ge\u00b7f\u00e4hr\u00b7te", "Ka\u00b7si\u00b7mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Es sind f\u00fcr jeden nur noch vier.", "tokens": ["Es", "sind", "f\u00fcr", "je\u00b7den", "nur", "noch", "vier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "ADV", "ADV", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}