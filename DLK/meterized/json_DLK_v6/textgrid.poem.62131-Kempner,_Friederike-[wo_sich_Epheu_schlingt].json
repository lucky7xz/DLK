{"textgrid.poem.62131": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[wo sich Epheu schlingt]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo sich Epheu schlingt,", "tokens": ["Wo", "sich", "E\u00b7pheu", "schlingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Eine Hand mir winkt,", "tokens": ["Ei\u00b7ne", "Hand", "mir", "winkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In der Mutter Gruft", "tokens": ["In", "der", "Mut\u00b7ter", "Gruft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Eine Stimme ruft:", "tokens": ["Ei\u00b7ne", "Stim\u00b7me", "ruft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dich hab' ich geliebt! \u2013", "tokens": ["Dich", "hab'", "ich", "ge\u00b7liebt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Weine nicht, mein Kind,", "tokens": ["Wei\u00b7ne", "nicht", ",", "mein", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Unsterblich wir sind,", "tokens": ["U\u00b7nsterb\u00b7lich", "wir", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seh'n uns wieder einst,", "tokens": ["Seh'n", "uns", "wie\u00b7der", "einst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Unw\u00fcrdig Du weinst,", "tokens": ["Un\u00b7w\u00fcr\u00b7dig", "Du", "weinst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Gott uns wieder giebt!", "tokens": ["Gott", "uns", "wie\u00b7der", "giebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}