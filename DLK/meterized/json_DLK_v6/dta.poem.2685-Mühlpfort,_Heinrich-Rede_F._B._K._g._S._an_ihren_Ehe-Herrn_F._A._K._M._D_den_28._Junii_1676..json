{"dta.poem.2685": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Rede/  \n  F. B. K. g. S. an ihren Ehe-Herrn F. A. K.  \n  M. D  den 28. Junii 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja mein Erl\u00f6ser lebt/ Mein Retter ist verhanden/ ", "tokens": ["Ja", "mein", "Er\u00b7l\u00f6\u00b7ser", "lebt", "/", "Mein", "Ret\u00b7ter", "ist", "ver\u00b7han\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "VVFIN", "$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich leitet seine Hand itzt durch das finstre Thal;", "tokens": ["Mich", "lei\u00b7tet", "sei\u00b7ne", "Hand", "itzt", "durch", "das", "finst\u00b7re", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df von keiner Angst/ ich wei\u00df von keinen Banden/", "tokens": ["Ich", "wei\u00df", "von", "kei\u00b7ner", "Angst", "/", "ich", "wei\u00df", "von", "kei\u00b7nen", "Ban\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$(", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bin nun auffgel\u00f6st von meines Leibes Qual.", "tokens": ["Und", "bin", "nun", "auff\u00b7ge\u00b7l\u00f6st", "von", "mei\u00b7nes", "Lei\u00b7bes", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verwundert/ Sterbliche/ nicht meine frische Worte/", "tokens": ["Ver\u00b7wun\u00b7dert", "/", "Sterb\u00b7li\u00b7che", "/", "nicht", "mei\u00b7ne", "fri\u00b7sche", "Wor\u00b7te", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "NN", "$(", "PTKNEG", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df bey geschlo\u00dfnem Mund ich so beredsam bin.", "tokens": ["Da\u00df", "bey", "ge\u00b7schlo\u00df\u00b7nem", "Mund", "ich", "so", "be\u00b7red\u00b7sam", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was unver g\u00e4nglich war schwebt an dem Freyheits-Orte/", "tokens": ["Was", "un\u00b7ver", "g\u00e4ng\u00b7lich", "war", "schwebt", "an", "dem", "Frey\u00b7heits\u00b7Or\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJD", "VAFIN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Tragt die verwelckte Haut und Glieder immer hin.", "tokens": ["Tragt", "die", "ver\u00b7welck\u00b7te", "Haut", "und", "Glie\u00b7der", "im\u00b7mer", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ja freylich hab ich lang im Kercker hier gesessen/", "tokens": ["Ja", "frey\u00b7lich", "hab", "ich", "lang", "im", "Ker\u00b7cker", "hier", "ge\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie schrecklich hat mich nicht die Folter-Banck gestreckt/", "tokens": ["Wie", "schreck\u00b7lich", "hat", "mich", "nicht", "die", "Fol\u00b7ter\u00b7Banck", "ge\u00b7streckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie hab ich nicht mein Brod in Angst und Tr\u00fcbsal gessen/", "tokens": ["Wie", "hab", "ich", "nicht", "mein", "Brod", "in", "Angst", "und", "Tr\u00fcb\u00b7sal", "ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn vieler Jahre Reyh mehr Kranckheit mir erweckt/", "tokens": ["Wenn", "vie\u00b7ler", "Jah\u00b7re", "Reyh", "mehr", "Kran\u00b7ck\u00b7heit", "mir", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "PIAT", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Wenn alle Krafft entfiel den abgeschw\u00e4chten Gliedern/", "tokens": ["Wenn", "al\u00b7le", "Krafft", "ent\u00b7fiel", "den", "ab\u00b7ge\u00b7schw\u00e4ch\u00b7ten", "Glie\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gesichte Mund und Ohr au\u00df Ohnmacht fast vergieng/", "tokens": ["Ge\u00b7sich\u00b7te", "Mund", "und", "Ohr", "au\u00df", "Ohn\u00b7macht", "fast", "ver\u00b7gieng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Und den gefallnen Muth kein Artzney kont erwiedern/", "tokens": ["Und", "den", "ge\u00b7fall\u00b7nen", "Muth", "kein", "Artz\u00b7ney", "kont", "er\u00b7wie\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Indem mein Leben mir an einem Faden hieng.", "tokens": ["In\u00b7dem", "mein", "Le\u00b7ben", "mir", "an", "ei\u00b7nem", "Fa\u00b7den", "hieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nun bin ich franck und frey/ gesund und lebend worden/", "tokens": ["Nun", "bin", "ich", "franck", "und", "frey", "/", "ge\u00b7sund", "und", "le\u00b7bend", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "KON", "ADJD", "$(", "ADJD", "KON", "ADJD", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nun wei\u00df ich da\u00df noch Welt/ noch Fleisch noch Blut mich", "tokens": ["Nun", "wei\u00df", "ich", "da\u00df", "noch", "Welt", "/", "noch", "Fleisch", "noch", "Blut", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ADV", "NN", "$(", "ADV", "NN", "ADV", "NN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ich wach\u2019/ ob ihr mich schon zehlt in der Todten Orden/", "tokens": ["Ich", "wach'", "/", "ob", "ihr", "mich", "schon", "zehlt", "in", "der", "Tod\u00b7ten", "Or\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Und h\u00f6re hocherfreut wie mich mein Heyland weckt.", "tokens": ["Und", "h\u00f6\u00b7re", "ho\u00b7cher\u00b7freut", "wie", "mich", "mein", "Hey\u00b7land", "weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KOKOM", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und solte der nicht seyn mein Wecker/ der mein Leben", "tokens": ["Und", "sol\u00b7te", "der", "nicht", "seyn", "mein", "We\u00b7cker", "/", "der", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "PTKNEG", "VAINF", "PPOSAT", "NN", "$(", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und meines Lebens Heil/ mein End und Anfang heist?", "tokens": ["Und", "mei\u00b7nes", "Le\u00b7bens", "Heil", "/", "mein", "End", "und", "An\u00b7fang", "heist", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$(", "PPOSAT", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Solt ich auf seinen Ruff mich au\u00df der ", "tokens": ["Solt", "ich", "auf", "sei\u00b7nen", "Ruff", "mich", "au\u00df", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Wenn durch die Todten Bein bl\u00e4st sein lebendig Geist?", "tokens": ["Wenn", "durch", "die", "Tod\u00b7ten", "Bein", "bl\u00e4st", "sein", "le\u00b7ben\u00b7dig", "Geist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.25": {"text": "Beth\u00f6rte Sterbliche! die eingeschrumpffte Wangen/", "tokens": ["Be\u00b7th\u00f6r\u00b7te", "Sterb\u00b7li\u00b7che", "!", "die", "ein\u00b7ge\u00b7schrumpff\u00b7te", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Augen tieffe Nacht/ die he\u00dfliche Gestalt/", "tokens": ["Der", "Au\u00b7gen", "tief\u00b7fe", "Nacht", "/", "die", "he\u00df\u00b7li\u00b7che", "Ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der Glieder kaltes Ei\u00df/ der Beine d\u00fcrre ", "tokens": ["Der", "Glie\u00b7der", "kal\u00b7tes", "Ei\u00df", "/", "der", "Bei\u00b7ne", "d\u00fcr\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Der F\u00e4ulnis-volle Leib der W\u00fcrmer Auffenthalt/", "tokens": ["Der", "F\u00e4ul\u00b7nis\u00b7vol\u00b7le", "Leib", "der", "W\u00fcr\u00b7mer", "Auf\u00b7fent\u00b7halt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Erschrecken zwar bey euch die Z\u00e4rtligkeit der Augen/", "tokens": ["Er\u00b7schre\u00b7cken", "zwar", "bey", "euch", "die", "Z\u00e4rt\u00b7lig\u00b7keit", "der", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.30": {"text": "Jhr wendet Nas\u2019 und Mund von todten C\u00f6rpern weg.", "tokens": ["Ihr", "wen\u00b7det", "Nas'", "und", "Mund", "von", "tod\u00b7ten", "C\u00f6r\u00b7pern", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch glaubet/ da\u00df sie euch zu stummen Lehrern taugen/", "tokens": ["Doch", "glau\u00b7bet", "/", "da\u00df", "sie", "euch", "zu", "stum\u00b7men", "Leh\u00b7rern", "tau\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und satsam zeigen an der Ewigkeiten Zweck.", "tokens": ["Und", "sat\u00b7sam", "zei\u00b7gen", "an", "der", "E\u00b7wig\u00b7kei\u00b7ten", "Zweck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Hier ruh\u2019 ich freylich itzt nur unter Wust und Schimmel/", "tokens": ["Hier", "ruh'", "ich", "frey\u00b7lich", "itzt", "nur", "un\u00b7ter", "Wust", "und", "Schim\u00b7mel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wo F\u00e4uln\u00fc\u00df Vater ist und W\u00fcrmer Schwestern seyn;", "tokens": ["Wo", "F\u00e4ul\u00b7n\u00fc\u00df", "Va\u00b7ter", "ist", "und", "W\u00fcr\u00b7mer", "Schwes\u00b7tern", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VAFIN", "KON", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Doch ein gantz neues Kleid/ das sch\u00f6ner als der Himmel", "tokens": ["Doch", "ein", "gantz", "neu\u00b7es", "Kleid", "/", "das", "sch\u00f6\u00b7ner", "als", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "$(", "ART", "ADJA", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wird von de\u00df Retters Hand empfangen mein Gebein.", "tokens": ["Wird", "von", "de\u00df", "Ret\u00b7ters", "Hand", "emp\u00b7fan\u00b7gen", "mein", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und diese meine Haut wird mich auffs neu umbgeben/", "tokens": ["Und", "die\u00b7se", "mei\u00b7ne", "Haut", "wird", "mich", "auffs", "neu", "umb\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PPOSAT", "NN", "VAFIN", "PPER", "APPRART", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df ich an Klarheit kan den Sternen gleiche gehn.", "tokens": ["Da\u00df", "ich", "an", "Klar\u00b7heit", "kan", "den", "Ster\u00b7nen", "glei\u00b7che", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VMFIN", "ART", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ja mein Erl\u00f6ser lebt/ und heist sein Glied mich leben/", "tokens": ["Ja", "mein", "Er\u00b7l\u00f6\u00b7ser", "lebt", "/", "und", "heist", "sein", "Glied", "mich", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "VVFIN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und l\u00e4st als Schwester mich zu seiner Seite stehn.", "tokens": ["Und", "l\u00e4st", "als", "Schwes\u00b7ter", "mich", "zu", "sei\u00b7ner", "Sei\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ach unersch\u00f6pffte Lust! das Fleisch/ das itzt verwesen", "tokens": ["Ach", "un\u0