{"dta.poem.10795": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n Lehrreiche Winter Gr\u00fcn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist wei\u00df zur Winterszeit,", "tokens": ["Die", "Welt", "ist", "wei\u00df", "zur", "Win\u00b7ter\u00b7szeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in dies bleiche Todtenkleid,", "tokens": ["Und", "in", "dies", "blei\u00b7che", "Tod\u00b7ten\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist alles gleichsam eingeh\u00fcllet;", "tokens": ["Ist", "al\u00b7les", "gleich\u00b7sam", "ein\u00b7ge\u00b7h\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Luft ist schw\u00e4rzlich angef\u00fcllet", "tokens": ["Die", "Luft", "ist", "schw\u00e4rz\u00b7lich", "an\u00b7ge\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit D\u00fcnsten, und ihr hell Revier,", "tokens": ["Mit", "D\u00fcns\u00b7ten", ",", "und", "ihr", "hell", "Re\u00b7vier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kommt uns, als wie ein Zimmer f\u00fcr,", "tokens": ["Kommt", "uns", ",", "als", "wie", "ein", "Zim\u00b7mer", "f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "KOKOM", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Darin die W\u00e4nde und die Bogen,", "tokens": ["Da\u00b7rin", "die", "W\u00e4n\u00b7de", "und", "die", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit schwarzen Trauer-Flor bezogen.", "tokens": ["Mit", "schwar\u00b7zen", "Trau\u00b7e\u00b7rFlor", "be\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erb\u00e4rmlich Bild! der Erden Reich,", "tokens": ["Er\u00b7b\u00e4rm\u00b7lich", "Bild", "!", "der", "Er\u00b7den", "Reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist einem Sterbaew\u00f6lbe gleich,", "tokens": ["Ist", "ei\u00b7nem", "Ster\u00b7bae\u00b7w\u00f6l\u00b7be", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Worin der kalte Todt regieret,", "tokens": ["Wo\u00b7rin", "der", "kal\u00b7te", "Todt", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das was im Sommer sch\u00f6n gezieret,", "tokens": ["Das", "was", "im", "Som\u00b7mer", "sch\u00f6n", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da unsre Erd ein Freudenthal,", "tokens": ["Da", "uns\u00b7re", "Erd", "ein", "Freu\u00b7dent\u00b7hal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist nun im Winter bla\u00df und kahl;", "tokens": ["Ist", "nun", "im", "Win\u00b7ter", "bla\u00df", "und", "kahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der B\u00e4ume Schirm, der sie umlaubet,", "tokens": ["Der", "B\u00e4u\u00b7me", "Schirm", ",", "der", "sie", "um\u00b7lau\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist durch den Frost hinweggeraubet.", "tokens": ["Ist", "durch", "den", "Frost", "hin\u00b7weg\u00b7ge\u00b7rau\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der gr\u00fcnen Gipfel sch\u00f6ner Kranz,", "tokens": ["Der", "gr\u00fc\u00b7nen", "Gip\u00b7fel", "sch\u00f6\u00b7ner", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und der smaragdnen Bl\u00e4tter Glanz", "tokens": ["Und", "der", "sma\u00b7ragd\u00b7nen", "Bl\u00e4t\u00b7ter", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist hin: die Aeste die wir schauen,", "tokens": ["Ist", "hin", ":", "die", "A\u00b7es\u00b7te", "die", "wir", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKVZ", "$.", "ART", "NN", "ART", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erwekken uns ein banges Grauen:", "tokens": ["Er\u00b7wek\u00b7ken", "uns", "ein", "ban\u00b7ges", "Grau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der B\u00fcsche Kronen gleiche Pracht,", "tokens": ["Der", "B\u00fc\u00b7sche", "Kro\u00b7nen", "glei\u00b7che", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird wie ein wilder Strauch geacht,", "tokens": ["Wird", "wie", "ein", "wil\u00b7der", "Strauch", "ge\u00b7acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da sich die Spizzen an den Hekken,", "tokens": ["Da", "sich", "die", "Spiz\u00b7zen", "an", "den", "Hek\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie starr und kahle Klauen strekken.", "tokens": ["Wie", "starr", "und", "kah\u00b7le", "Klau\u00b7en", "strek\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Erden zugefrorner Schoos,", "tokens": ["Der", "Er\u00b7den", "zu\u00b7ge\u00b7fror\u00b7ner", "Schoos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt nichts als ein verfaultes Moos,", "tokens": ["Zeigt", "nichts", "als", "ein", "ver\u00b7faul\u00b7tes", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Daran des Todes Farbe lehret,", "tokens": ["Da\u00b7ran", "des", "To\u00b7des", "Far\u00b7be", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es schon zur Verwesung kehret.", "tokens": ["Da\u00df", "es", "schon", "zur", "Ver\u00b7we\u00b7sung", "keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "So ist im Reiche der Natur,", "tokens": ["So", "ist", "im", "Rei\u00b7che", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In G\u00e4rten, Wiesen, Wald und Flur,", "tokens": ["In", "G\u00e4r\u00b7ten", ",", "Wie\u00b7sen", ",", "Wald", "und", "Flur", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Schnee und Eis erstarrt, erfroren,", "tokens": ["In", "Schnee", "und", "Eis", "er\u00b7starrt", ",", "er\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was in den Fr\u00fchling wird gebohren.", "tokens": ["Was", "in", "den", "Fr\u00fch\u00b7ling", "wird", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jedoch so todt die blasse Welt,", "tokens": ["Je\u00b7doch", "so", "todt", "die", "blas\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die durch den kalten Nord verstellt;", "tokens": ["Die", "durch", "den", "kal\u00b7ten", "Nord", "ver\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So war dennoch bei vielen Leichen,", "tokens": ["So", "war", "den\u00b7noch", "bei", "vie\u00b7len", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch hie und da ein Freudenzeichen.", "tokens": ["Noch", "hie", "und", "da", "ein", "Freu\u00b7den\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Man fand noch manches Wintergr\u00fcn,", "tokens": ["Man", "fand", "noch", "man\u00b7ches", "Win\u00b7ter\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das aus den weissen Schnee her schien,", "tokens": ["Das", "aus", "den", "weis\u00b7sen", "Schnee", "her", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "APZR", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Hofnung Bild, lies sich noch sehen,", "tokens": ["Der", "Hof\u00b7nung", "Bild", ",", "lies", "sich", "noch", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In tieffen Th\u00e4lern, auf den H\u00f6hen.", "tokens": ["In", "tief\u00b7fen", "Th\u00e4\u00b7lern", ",", "auf", "den", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es schien durch ein versteinert Eis,", "tokens": ["Es", "schien", "durch", "ein", "ver\u00b7stei\u00b7nert", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu unsers Sch\u00f6pfers Ruhm und Preis,", "tokens": ["Zu", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Ruhm", "und", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das gr\u00fcne Keimgen auf den Feldern;", "tokens": ["Das", "gr\u00fc\u00b7ne", "Keim\u00b7gen", "auf", "den", "Fel\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die fette Fichte in den W\u00e4ldern,", "tokens": ["Die", "fet\u00b7te", "Fich\u00b7te", "in", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Taxus, hielten bei den Druk,", "tokens": ["Der", "Ta\u00b7xus", ",", "hiel\u00b7ten", "bei", "den", "Druk", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der kalten Lust doch ihren Schmuk;", "tokens": ["Der", "kal\u00b7ten", "Lust", "doch", "ih\u00b7ren", "Schmuk", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Am Meeres Ufern, an den Fl\u00fcssen,", "tokens": ["Am", "Mee\u00b7res", "U\u00b7fern", ",", "an", "den", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sah man ein gr\u00fcnes Gra\u00df auch spriessen.", "tokens": ["Sah", "man", "ein", "gr\u00fc\u00b7nes", "Gra\u00df", "auch", "spries\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Felder Gr\u00fcn gab diese Lehr:\nIch keime zu des Sch\u00f6pfers Ehr,", "tokens": ["Der", "Fel\u00b7der", "Gr\u00fcn", "gab", "die\u00b7se", "Lehr", ":", "Ich", "kei\u00b7me", "zu", "des", "Sch\u00f6p\u00b7fers", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PDAT", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Der vor des Frostes kalten St\u00fcrmen,", "tokens": ["Der", "vor", "des", "Fros\u00b7tes", "kal\u00b7ten", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Saat weis weislich zu beschirmen.", "tokens": ["Die", "Saat", "weis", "weis\u00b7lich", "zu", "be\u00b7schir\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein weisser Pelz von Frost gewebt,", "tokens": ["Ein", "weis\u00b7ser", "Pelz", "von", "Frost", "ge\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der flokkigt ist, und um uns klebt,", "tokens": ["Der", "flok\u00b7kigt", "ist", ",", "und", "um", "uns", "klebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "$,", "KON", "APPR"