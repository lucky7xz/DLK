{"textgrid.poem.35898": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hin sind der Rosen D\u00fcfte,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hin sind der Rosen D\u00fcfte,", "tokens": ["Hin", "sind", "der", "Ro\u00b7sen", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Dich im Lenz erfreut;", "tokens": ["Die", "Dich", "im", "Lenz", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4ngst haben rauhe L\u00fcfte", "tokens": ["L\u00e4ngst", "ha\u00b7ben", "rau\u00b7he", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Bl\u00e4ttlein hingestreut.", "tokens": ["Die", "Bl\u00e4tt\u00b7lein", "hin\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, da\u00df so bald entschwunden", "tokens": ["Ach", ",", "da\u00df", "so", "bald", "ent\u00b7schwun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Zier der Rose ist,", "tokens": ["Die", "Zier", "der", "Ro\u00b7se", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und da\u00df vom Dorn die Wunde", "tokens": ["Und", "da\u00df", "vom", "Dorn", "die", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So lang' sich nicht vergi\u00dft!", "tokens": ["So", "lang'", "sich", "nicht", "ver\u00b7gi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, Herz, die volle Rose", "tokens": ["Ja", ",", "Herz", ",", "die", "vol\u00b7le", "Ro\u00b7se"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bl\u00fcht nur im Lebensmai,", "tokens": ["Bl\u00fcht", "nur", "im", "Le\u00b7bens\u00b7mai", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "W\u00e4hlst Du die Jahreslose:", "tokens": ["W\u00e4hlst", "Du", "die", "Jah\u00b7res\u00b7lo\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die bleibt Dir ewig treu.", "tokens": ["Die", "bleibt", "Dir", "e\u00b7wig", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie steht so still geborgen", "tokens": ["Sie", "steht", "so", "still", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im trauten K\u00e4mmerlein,", "tokens": ["Im", "trau\u00b7ten", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da bl\u00fcht sie heut wie morgen", "tokens": ["Da", "bl\u00fcht", "sie", "heut", "wie", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im holden Rosenschein.", "tokens": ["Im", "hol\u00b7den", "Ro\u00b7sen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was bl\u00fcht die sch\u00f6ne Rose?", "tokens": ["Was", "bl\u00fcht", "die", "sch\u00f6\u00b7ne", "Ro\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie bl\u00fchte Lieb' im Mai.", "tokens": ["Sie", "bl\u00fch\u00b7te", "Lieb'", "im", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was sagt die Jahreslose?", "tokens": ["Was", "sagt", "die", "Jah\u00b7res\u00b7lo\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie deutet Schwestertreu'.", "tokens": ["Sie", "deu\u00b7tet", "Schwes\u00b7ter\u00b7treu'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die bl\u00fcht so still verborgen", "tokens": ["Die", "bl\u00fcht", "so", "still", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im tiefsten Herzensschrein,", "tokens": ["Im", "tiefs\u00b7ten", "Her\u00b7zens\u00b7schrein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die bl\u00fcht Dir heut wie morgen,", "tokens": ["Die", "bl\u00fcht", "Dir", "heut", "wie", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis in das Grab hinein.", "tokens": ["Bis", "in", "das", "Grab", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die wird Dir nie vergl\u00fchen", "tokens": ["Die", "wird", "Dir", "nie", "ver\u00b7gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Freud' und nicht im Schmerz,", "tokens": ["In", "Freud'", "und", "nicht", "im", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die wird Dir ewig bl\u00fchen:", "tokens": ["Die", "wird", "Dir", "e\u00b7wig", "bl\u00fc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie bl\u00fcht einst himmelw\u00e4rts.", "tokens": ["Sie", "bl\u00fcht", "einst", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}}}}