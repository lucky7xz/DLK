{"textgrid.poem.64643": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "270. Selbstmord", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich erhenkt, erschie\u00dft, ersticht;", "tokens": ["Wer", "sich", "er\u00b7henkt", ",", "er\u00b7schie\u00dft", ",", "er\u00b7sticht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "$,", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist der wohl auch ein Christ?", "tokens": ["Ist", "der", "wohl", "auch", "ein", "Christ", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das wei\u00df ich nicht;", "tokens": ["Das", "wei\u00df", "ich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Das wei\u00df ich, da\u00df er n\u00e4rrisch ist.", "tokens": ["Das", "wei\u00df", "ich", ",", "da\u00df", "er", "n\u00e4r\u00b7risch", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}