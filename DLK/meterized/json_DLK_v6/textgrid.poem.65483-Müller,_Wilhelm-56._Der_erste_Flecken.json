{"textgrid.poem.65483": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "56. Der erste Flecken", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du durch den Koth der Stra\u00dfe mu\u00dft mit neuen Schuhen gehn,", "tokens": ["Wenn", "du", "durch", "den", "Koth", "der", "Stra\u00b7\u00dfe", "mu\u00dft", "mit", "neu\u00b7en", "Schu\u00b7hen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wirst du, trippelnd auf den Spitzen, nach den blanken Steinen sehn.", "tokens": ["Wirst", "du", ",", "trip\u00b7pelnd", "auf", "den", "Spit\u00b7zen", ",", "nach", "den", "blan\u00b7ken", "Stei\u00b7nen", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Hat sie erst beschmuzt ", "tokens": ["Hat", "sie", "erst", "be\u00b7schmuzt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "H\u00fcte, Kind, in deiner Seele vor dem ersten Flecken dich!", "tokens": ["H\u00fc\u00b7te", ",", "Kind", ",", "in", "dei\u00b7ner", "See\u00b7le", "vor", "dem", "ers\u00b7ten", "Fle\u00b7cken", "dich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "