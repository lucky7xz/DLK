{"textgrid.poem.59808": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder ist der Fr\u00fchling ins Land gekommen,", "tokens": ["Wie\u00b7der", "ist", "der", "Fr\u00fch\u00b7ling", "ins", "Land", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist in blumigem, buntem Gewand gekommen.", "tokens": ["Ist", "in", "blu\u00b7mi\u00b7gem", ",", "bun\u00b7tem", "Ge\u00b7wand", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Mit einem vollen Becher in der Hand gekommen.", "tokens": ["Mit", "ei\u00b7nem", "vol\u00b7len", "Be\u00b7cher", "in", "der", "Hand", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jetzt meid' ich ihn, denn unter seinen Blumen", "tokens": ["Jetzt", "meid'", "ich", "ihn", ",", "denn", "un\u00b7ter", "sei\u00b7nen", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich an der Verzweiflung Rand gekommen.", "tokens": ["Bin", "ich", "an", "der", "Ver\u00b7zwei\u00b7flung", "Rand", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Bin um Zul\u00e9ikha und mit der Geliebten", "tokens": ["Bin", "um", "Zul\u00e9ik\u00b7ha", "und", "mit", "der", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NE", "KON", "APPR", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Um Freude, Gl\u00fcck und Verstand gekommen.", "tokens": ["Um", "Freu\u00b7de", ",", "Gl\u00fcck", "und", "Ver\u00b7stand", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}