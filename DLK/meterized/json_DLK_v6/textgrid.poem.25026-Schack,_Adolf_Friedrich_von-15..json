{"textgrid.poem.25026": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn unsre Herzen aneinander schlagen,", "tokens": ["Wenn", "uns\u00b7re", "Her\u00b7zen", "an\u00b7ein\u00b7an\u00b7der", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jedwedem Schicksalssturme biet' ich stand;", "tokens": ["Jed\u00b7we\u00b7dem", "Schick\u00b7sals\u00b7stur\u00b7me", "biet'", "ich", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch fern von dir bef\u00e4llt mich banges Zagen,", "tokens": ["Doch", "fern", "von", "dir", "be\u00b7f\u00e4llt", "mich", "ban\u00b7ges", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Kleinmut, den ich nie gekannt.", "tokens": ["Ein", "Klein\u00b7mut", ",", "den", "ich", "nie", "ge\u00b7kannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich denke tieferschreckt: Wenn sie nicht w\u00e4re,", "tokens": ["Ich", "den\u00b7ke", "tie\u00b7fer\u00b7schreckt", ":", "Wenn", "sie", "nicht", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "KOUS", "PPER", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn auf der Welt verschwunden ihre Spur:", "tokens": ["Wenn", "auf", "der", "Welt", "ver\u00b7schwun\u00b7den", "ih\u00b7re", "Spur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie tr\u00fcg' ich nur die grenzenlose Leere,", "tokens": ["Wie", "tr\u00fcg'", "ich", "nur", "die", "gren\u00b7zen\u00b7lo\u00b7se", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den gro\u00dfen Ri\u00df in der Natur?", "tokens": ["Den", "gro\u00b7\u00dfen", "Ri\u00df", "in", "der", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann ist mir, alles Leben s\u00e4h' ich siechen;", "tokens": ["Dann", "ist", "mir", ",", "al\u00b7les", "Le\u00b7ben", "s\u00e4h'", "ich", "sie\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PIAT", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Heerrauch, drin das Gr\u00fcn des Fr\u00fchlings dorrt,", "tokens": ["Ein", "Heer\u00b7rauch", ",", "drin", "das", "Gr\u00fcn", "des", "Fr\u00fch\u00b7lings", "dorrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Scheint durch den Himmel t\u00f6tend hinzukriechen;", "tokens": ["Scheint", "durch", "den", "Him\u00b7mel", "t\u00f6\u00b7tend", "hin\u00b7zu\u00b7krie\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Angstvoll, dich suchend, st\u00fcrz' ich fort.", "tokens": ["Angst\u00b7voll", ",", "dich", "su\u00b7chend", ",", "st\u00fcrz'", "ich", "fort", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da bist du, bist du! Und, wie wilde Ranken", "tokens": ["Da", "bist", "du", ",", "bist", "du", "!", "Und", ",", "wie", "wil\u00b7de", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VAFIN", "PPER", "$.", "KON", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Baum umklammern, fest mit Herz und Geist", "tokens": ["Den", "Baum", "um\u00b7klam\u00b7mern", ",", "fest", "mit", "Herz", "und", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umschling' ich dich, Gef\u00fchlen und Gedanken;", "tokens": ["Um\u00b7schling'", "ich", "dich", ",", "Ge\u00b7f\u00fch\u00b7len", "und", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PRF", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist einer, der dich mir entrei\u00dft?", "tokens": ["Ist", "ei\u00b7ner", ",", "der", "dich", "mir", "ent\u00b7rei\u00dft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}