{"textgrid.poem.62641": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Geburts-Tag einer guten Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geehrtes Jungfer-Bild!", "tokens": ["Ge\u00b7ehr\u00b7tes", "Jung\u00b7fer\u00b7Bild", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erlaube, da\u00df mein Blat", "tokens": ["Er\u00b7lau\u00b7be", ",", "da\u00df", "mein", "Blat"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt einen holden Blick von dir zu hoffen hat.", "tokens": ["Jetzt", "ei\u00b7nen", "hol\u00b7den", "Blick", "von", "dir", "zu", "hof\u00b7fen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nim ", "tokens": ["Nim"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "An deinem ", "tokens": ["An", "dei\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Die Tugend, welche dir ins Herz gepr\u00e4get ist,", "tokens": ["Die", "Tu\u00b7gend", ",", "wel\u00b7che", "dir", "ins", "Herz", "ge\u00b7pr\u00e4\u00b7get", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die w\u00fcrket, da\u00df mein Wunsch dich ganz ergebenst gr\u00fc\u00dft.", "tokens": ["Die", "w\u00fcr\u00b7ket", ",", "da\u00df", "mein", "Wunsch", "dich", "ganz", "er\u00b7ge\u00b7benst", "gr\u00fc\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Die Tugend an sich selbst mit allem ihren Wesen,", "tokens": ["Die", "Tu\u00b7gend", "an", "sich", "selbst", "mit", "al\u00b7lem", "ih\u00b7ren", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ADV", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ward schon im Alterthum zum Schmucke auserlesen.", "tokens": ["Ward", "schon", "im", "Al\u00b7ter\u00b7thum", "zum", "Schmu\u00b7cke", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die edle Tugend zieht gern in die Seelen ein,", "tokens": ["Die", "ed\u00b7le", "Tu\u00b7gend", "zieht", "gern", "in", "die", "See\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie will beym m\u00e4nnlichen und Weibs-Geschlechten seyn.", "tokens": ["Sie", "will", "beym", "m\u00e4nn\u00b7li\u00b7chen", "und", "Weibs\u00b7Ge\u00b7schlech\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erwege, ", "tokens": ["Er\u00b7we\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "So wird desselben Wort in dir den Beyfall stiften.", "tokens": ["So", "wird", "des\u00b7sel\u00b7ben", "Wort", "in", "dir", "den", "Bey\u00b7fall", "stif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die V\u00e4ter erster Zeit, der Patriarchen Zahl,", "tokens": ["Die", "V\u00e4\u00b7ter", "ers\u00b7ter", "Zeit", ",", "der", "Pat\u00b7ri\u00b7ar\u00b7chen", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Helden, K\u00f6nige, Propheten allzumahl,", "tokens": ["Die", "Hel\u00b7den", ",", "K\u00f6\u00b7ni\u00b7ge", ",", "Pro\u00b7phe\u00b7ten", "all\u00b7zu\u00b7mahl", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Empfanden gar zu wohl, wie n\u00fctz die Tugend w\u00e4re", "tokens": ["Emp\u00b7fan\u00b7den", "gar", "zu", "wohl", ",", "wie", "n\u00fctz", "die", "Tu\u00b7gend", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADV", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem, der dem Herr der Welt Gehorsam, Furcht und Ehre", "tokens": ["Dem", ",", "der", "dem", "Herr", "der", "Welt", "Ge\u00b7hor\u00b7sam", ",", "Furcht", "und", "Eh\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "ART", "NN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zu leisten schuldig ist: Und wie man ohne sie", "tokens": ["Zu", "leis\u00b7ten", "schul\u00b7dig", "ist", ":", "Und", "wie", "man", "oh\u00b7ne", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADJD", "VAFIN", "$.", "KON", "PWAV", "PIS", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dem nicht gefallen kan, der Mittags, spat und fr\u00fch", "tokens": ["Dem", "nicht", "ge\u00b7fal\u00b7len", "kan", ",", "der", "Mit\u00b7tags", ",", "spat", "und", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "VVINF", "VMFIN", "$,", "ART", "NN", "$,", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das Land mit Regen netzt, und stets nach Wohlgefallen", "tokens": ["Das", "Land", "mit", "Re\u00b7gen", "netzt", ",", "und", "stets", "nach", "Wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Str\u00f6me seiner G\u00fct l\u00e4\u00dft auf die Menschen fallen.", "tokens": ["Die", "Str\u00f6\u00b7me", "sei\u00b7ner", "G\u00fct", "l\u00e4\u00dft", "auf", "die", "Men\u00b7schen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dergleichen sch\u00f6nen Bund gieng Sarah t\u00e4glich ein,", "tokens": ["Derg\u00b7lei\u00b7chen", "sch\u00f6\u00b7nen", "Bund", "gieng", "Sa\u00b7rah", "t\u00e4g\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "VVFIN", "NE", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Rebecca, Hanna, Ruth erwehlten sie allein;", "tokens": ["Re\u00b7bec\u00b7ca", ",", "Han\u00b7na", ",", "Ruth", "er\u00b7wehl\u00b7ten", "sie", "al\u00b7lein", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Susanna, Judith auch. Sie kannten ihre W\u00fcrde;", "tokens": ["Su\u00b7san\u00b7na", ",", "Ju\u00b7dith", "auch", ".", "Sie", "kann\u00b7ten", "ih\u00b7re", "W\u00fcr\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "ADV", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Deswegen trugen sie auch ihre sanfte B\u00fcrde.", "tokens": ["Des\u00b7we\u00b7gen", "tru\u00b7gen", "sie", "auch", "ih\u00b7re", "sanf\u00b7te", "B\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die V\u00e4ter nicht allein, und Christen neuer Zeit,", "tokens": ["Die", "V\u00e4\u00b7ter", "nicht", "al\u00b7lein", ",", "und", "Chris\u00b7ten", "neu\u00b7er", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "$,", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die haben ihre Seel der Tugend blos geweyht,", "tokens": ["Die", "ha\u00b7ben", "ih\u00b7re", "Seel", "der", "Tu\u00b7gend", "blos", "ge\u00b7weyht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und sie sehr hoch geliebt; Nein selber kluge Heyden", "tokens": ["Und", "sie", "sehr", "hoch", "ge\u00b7liebt", ";", "Nein", "sel\u00b7ber", "klu\u00b7ge", "Hey\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADJD", "VVPP", "$.", "PTKANT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bestrebten sich mit Flei\u00df ihr Aug an ihr zu weyden.", "tokens": ["Be\u00b7streb\u00b7ten", "sich", "mit", "Flei\u00df", "ihr", "Aug", "an", "ihr", "zu", "wey\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie wusten, da\u00df ihr Dienst der Seelen sehr bequem,", "tokens": ["Sie", "wus\u00b7ten", ",", "da\u00df", "ihr", "Dienst", "der", "See\u00b7len", "sehr", "be\u00b7quem", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und allzeit n\u00fctzlich sey; den G\u00f6ttern angenehm,", "tokens": ["Und", "all\u00b7zeit", "n\u00fctz\u00b7lich", "sey", ";", "den", "G\u00f6t\u00b7tern", "an\u00b7ge\u00b7nehm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$.", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Den Menschen lieb und werth, drum war es ihr Vergn\u00fcgen,", "tokens": ["Den", "Men\u00b7schen", "lieb", "und", "werth", ",", "drum", "war", "es", "ihr", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,", "PAV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn sie nur an der Brust der Tugend konten liegen.", "tokens": ["Wenn", "sie", "nur", "an", "der", "Brust", "der", "Tu\u00b7gend", "kon\u00b7ten", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein muntrer Hercules h\u00f6rt zwar den Vortrag an,", "tokens": ["Ein", "mun\u00b7trer", "Her\u00b7cu\u00b7les", "h\u00f6rt", "zwar", "den", "Vor\u00b7trag", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den ihm die Wollust dort im finstern Wald gethan;", "tokens": ["Den", "ihm", "die", "Wol\u00b7lust", "dort", "im", "fins\u00b7tern", "Wald", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch h\u00f6rt er auch zugleich der Tugend sanfte Stimme,", "tokens": ["Doch", "h\u00f6rt", "er", "auch", "zu\u00b7gleich", "der", "Tu\u00b7gend", "sanf\u00b7te", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und trieb darauf im Zorn, mit Ernst und grossem Grimme", "tokens": ["Und", "trieb", "da\u00b7rauf", "im", "Zorn", ",", "mit", "Ernst", "und", "gros\u00b7sem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "APPRART", "NN", "$,", "APPR", "NE", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die schn\u00f6de Wollust fort, und gab den Herzens-Schrein", "tokens": ["Die", "schn\u00f6\u00b7de", "Wol\u00b7lust", "fort", ",", "und", "gab", "den", "Her\u00b7zens\u00b7Schrein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit gutem Vorbedacht der Tugend eiligst ein.", "tokens": ["Mit", "gu\u00b7tem", "Vor\u00b7be\u00b7dacht", "der", "Tu\u00b7gend", "ei\u00b7ligst", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dort wolt Aspasia nach Art der edlen Nymphen,", "tokens": ["Dort", "wolt", "As\u00b7pa\u00b7sia", "nach", "Art", "der", "ed\u00b7len", "Nym\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Durch ihren Wandel nicht der Tugend Glanz beschimpfen;", "tokens": ["Durch", "ih\u00b7ren", "Wan\u00b7del", "nicht", "der", "Tu\u00b7gend", "Glanz", "be\u00b7schimp\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nein, sie erwehlte sie. Auch andre Nymphen mehr,", "tokens": ["Nein", ",", "sie", "er\u00b7wehl\u00b7te", "sie", ".", "Auch", "and\u00b7re", "Nym\u00b7phen", "mehr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "$.", "ADV", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die ich hier \u00fcbergeh, verehrten sie gar sehr.", "tokens": ["Die", "ich", "hier", "\u00fc\u00b7ber\u00b7geh", ",", "ver\u00b7ehr\u00b7ten", "sie", "gar", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O sch\u00f6n und edle Wahl, die Seel und Geist erfreuet,", "tokens"