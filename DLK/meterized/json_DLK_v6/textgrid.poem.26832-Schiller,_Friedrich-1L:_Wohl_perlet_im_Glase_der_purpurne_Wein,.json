{"textgrid.poem.26832": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl perlet im Glase der purpurne Wein,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl perlet im Glase der purpurne Wein,", "tokens": ["Wohl", "per\u00b7let", "im", "Gla\u00b7se", "der", "pur\u00b7pur\u00b7ne", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wohl gl\u00e4nzen die Augen der G\u00e4ste,", "tokens": ["Wohl", "gl\u00e4n\u00b7zen", "die", "Au\u00b7gen", "der", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es zeigt sich der S\u00e4nger, er tritt herein,", "tokens": ["Es", "zeigt", "sich", "der", "S\u00e4n\u00b7ger", ",", "er", "tritt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zu dem Guten bringt er das Beste,", "tokens": ["Zu", "dem", "Gu\u00b7ten", "bringt", "er", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Denn ohne die Leier im himmlischen Saal", "tokens": ["Denn", "oh\u00b7ne", "die", "Lei\u00b7er", "im", "himm\u00b7li\u00b7schen", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ist die Freude gemein auch beim Nektarmahl.", "tokens": ["Ist", "die", "Freu\u00b7de", "ge\u00b7mein", "auch", "beim", "Nek\u00b7tar\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Ihm gaben die G\u00f6tter das reine Gem\u00fct,", "tokens": ["Ihm", "ga\u00b7ben", "die", "G\u00f6t\u00b7ter", "das", "rei\u00b7ne", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wo die Welt sich, die ewige, spiegelt,", "tokens": ["Wo", "die", "Welt", "sich", ",", "die", "e\u00b7wi\u00b7ge", ",", "spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Er hat alles gesehn, was auf Erden geschieht,", "tokens": ["Er", "hat", "al\u00b7les", "ge\u00b7sehn", ",", "was", "auf", "Er\u00b7den", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und was uns die Zukunft versiegelt,", "tokens": ["Und", "was", "uns", "die", "Zu\u00b7kunft", "ver\u00b7sie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Er sa\u00df in der G\u00f6tter ur\u00e4ltestem Rat", "tokens": ["Er", "sa\u00df", "in", "der", "G\u00f6t\u00b7ter", "ur\u00b7\u00e4l\u00b7tes\u00b7tem", "Rat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und behorchte der Dinge geheimste Saat.", "tokens": ["Und", "be\u00b7horch\u00b7te", "der", "Din\u00b7ge", "ge\u00b7heims\u00b7te", "Saat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Er breitet es lustig und gl\u00e4nzend aus,", "tokens": ["Er", "brei\u00b7tet", "es", "lus\u00b7tig", "und", "gl\u00e4n\u00b7zend", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das zusammengefaltete Leben,", "tokens": ["Das", "zu\u00b7sam\u00b7men\u00b7ge\u00b7fal\u00b7te\u00b7te", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Zum Tempel schm\u00fcckt er das irdische Haus,", "tokens": ["Zum", "Tem\u00b7pel", "schm\u00fcckt", "er", "das", "ir\u00b7di\u00b7sche", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihm hat es die Muse gegeben,", "tokens": ["Ihm", "hat", "es", "die", "Mu\u00b7se", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Kein Dach ist so niedrig, keine H\u00fctte so klein,", "tokens": ["Kein", "Dach", "ist", "so", "nied\u00b7rig", ",", "kei\u00b7ne", "H\u00fct\u00b7te", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Er f\u00fchrt einen Himmel voll G\u00f6tter hinein.", "tokens": ["Er", "f\u00fchrt", "ei\u00b7nen", "Him\u00b7mel", "voll", "G\u00f6t\u00b7ter", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie der erfindende Sohn des Zeus", "tokens": ["Und", "wie", "der", "er\u00b7fin\u00b7den\u00b7de", "Sohn", "des", "Zeus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ART", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auf des Schildes einfachem Runde", "tokens": ["Auf", "des", "Schil\u00b7des", "ein\u00b7fa\u00b7chem", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Erde, das Meer und den Sternenkreis", "tokens": ["Die", "Er\u00b7de", ",", "das", "Meer", "und", "den", "Ster\u00b7nen\u00b7kreis"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Gebildet mit g\u00f6ttlicher Kunde,", "tokens": ["Ge\u00b7bil\u00b7det", "mit", "g\u00f6tt\u00b7li\u00b7cher", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "So dr\u00fcckt er ein Bild des unendlichen All", "tokens": ["So", "dr\u00fcckt", "er", "ein", "Bild", "des", "un\u00b7end\u00b7li\u00b7chen", "All"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "In des Augenblicks fl\u00fcchtig verrauschenden Schall.", "tokens": ["In", "des", "Au\u00b7gen\u00b7blicks", "fl\u00fcch\u00b7tig", "ver\u00b7rau\u00b7schen\u00b7den", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.5": {"line.1": {"text": "Er kommt aus dem kindlichen Alter der Welt,", "tokens": ["Er", "kommt", "aus", "dem", "kind\u00b7li\u00b7chen", "Al\u00b7ter", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wo die V\u00f6lker sich jugendlich freuten,", "tokens": ["Wo", "die", "V\u00f6l\u00b7ker", "sich", "ju\u00b7gend\u00b7lich", "freu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Er hat sich, ein fr\u00f6hlicher Wandrer, gesellt", "tokens": ["Er", "hat", "sich", ",", "ein", "fr\u00f6h\u00b7li\u00b7cher", "Wand\u00b7rer", ",", "ge\u00b7sellt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PRF", "$,", "ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zu allen Geschlechtern und Zeiten.", "tokens": ["Zu", "al\u00b7len", "Ge\u00b7schlech\u00b7tern", "und", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und l\u00e4\u00dft sie am ", "tokens": ["Und", "l\u00e4\u00dft", "sie", "am"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Erst regierte Saturnus schlicht und gerecht,", "tokens": ["Erst", "re\u00b7gier\u00b7te", "Sa\u00b7tur\u00b7nus", "schlicht", "und", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da war es heute wie morgen,", "tokens": ["Da", "war", "es", "heu\u00b7te", "wie", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da lebten die Hirten, ein harmlos Geschlecht,", "tokens": ["Da", "leb\u00b7ten", "die", "Hir\u00b7ten", ",", "ein", "harm\u00b7los", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und brauchten f\u00fcr gar nichts zu sorgen,", "tokens": ["Und", "brauch\u00b7ten", "f\u00fcr", "gar", "nichts", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie liebten und taten weiter nichts mehr,", "tokens": ["Sie", "lieb\u00b7ten", "und", "ta\u00b7ten", "wei\u00b7ter", "nichts", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PIS", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Erde gab alles freiwillig her.", "tokens": ["Die", "Er\u00b7de", "gab", "al\u00b7les", "frei\u00b7wil\u00b7lig", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Drauf kam die Arbeit, der Kampf begann", "tokens": ["Drauf", "kam", "die", "Ar\u00b7beit", ",", "der", "Kampf", "be\u00b7gann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Ungeheuern und Drachen,", "tokens": ["Mit", "Un\u00b7ge\u00b7heu\u00b7ern", "und", "Dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die Helden fingen, die Herrscher an,", "tokens": ["Und", "die", "Hel\u00b7den", "fin\u00b7gen", ",", "die", "Herr\u00b7scher", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und den M\u00e4chtigen suchten die Schwachen,", "tokens": ["Und", "den", "M\u00e4ch\u00b7ti\u00b7gen", "such\u00b7ten", "die", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und der Streit zog in des Skamanders Feld,", "tokens": ["Und", "der", "Streit", "zog", "in", "des", "Ska\u00b7man\u00b7ders", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Doch die Sch\u00f6nheit war immer der Gott der Welt.", "tokens": ["Doch", "die", "Sch\u00f6n\u00b7heit", "war", "im\u00b7mer", "der", "Gott", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Aus dem Kampf ging endlich der Sieg hervor,", "tokens": ["Aus", "dem", "Kampf", "ging", "end\u00b7lich", "der", "Sieg", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und der Kraft entbl\u00fchte die Milde,", "tokens": ["Und", "der", "Kraft", "ent\u00b7bl\u00fch\u00b7te", "die", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da sangen die Musen im himmlischen Chor,", "tokens": ["Da", "san\u00b7gen", "die", "Mu\u00b7sen", "im", "himm\u00b7li\u00b7schen", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da erhuben sich G\u00f6ttergebilde!", "tokens": ["Da", "er\u00b7hu\u00b7ben", "sich", "G\u00f6t\u00b7ter\u00b7ge\u00b7bil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Das Alter der g\u00f6ttlichen Phantasie,", "tokens": ["Das", "Al\u00b7ter", "der", "g\u00f6tt\u00b7li\u00b7chen", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Es ist verschwunden, es kehret nie.", "tokens": ["Es", "ist", "ver\u00b7schwun\u00b7den", ",", "es", "keh\u00b7ret", "nie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Die G\u00f6tter sanken vom Himmelsthron,", "tokens": ["Die", "G\u00f6t\u00b7ter", "san\u00b7ken", "vom", "Him\u00b7melst\u00b7hron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es st\u00fcrzten die herrlichen S\u00e4ulen,", "tokens": ["Es", "st\u00fcrz\u00b7ten", "die", "herr\u00b7li\u00b7chen", "S\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und geboren wurde der Jungf