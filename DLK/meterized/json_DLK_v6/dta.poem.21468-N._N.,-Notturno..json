{"dta.poem.21468": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Notturno.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O Schicksal, schweigendes Schicksal,", "tokens": ["O", "Schick\u00b7sal", ",", "schwei\u00b7gen\u00b7des", "Schick\u00b7sal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nimm von mir die d\u00fcst\u2019ren Gedanken,", "tokens": ["Nimm", "von", "mir", "die", "d\u00fcst'\u00b7ren", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die nieder wie br\u00fctende Nebel", "tokens": ["Die", "nie\u00b7der", "wie", "br\u00fc\u00b7ten\u00b7de", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PTKVZ", "KOKOM", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auf meine Seele sanken.", "tokens": ["Auf", "mei\u00b7ne", "See\u00b7le", "san\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O warum kann ich nie wieder", "tokens": ["O", "wa\u00b7rum", "kann", "ich", "nie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4umen in s\u00fc\u00dfen Gedanken,", "tokens": ["Tr\u00e4u\u00b7men", "in", "s\u00fc\u00b7\u00dfen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Gleich wie auf dem Felde die Blumen", "tokens": ["Gleich", "wie", "auf", "dem", "Fel\u00b7de", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vom Winde beseligt schwanken?", "tokens": ["Vom", "Win\u00b7de", "be\u00b7se\u00b7ligt", "schwan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Schon f\u00fchl\u2019 ich unter den F\u00fc\u00dfen", "tokens": ["Schon", "f\u00fchl'", "ich", "un\u00b7ter", "den", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Boden zittern und schwanken \u2026", "tokens": ["Den", "Bo\u00b7den", "zit\u00b7tern", "und", "schwan\u00b7ken", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gieb himmlische Fl\u00fcgel, o Schicksal,", "tokens": ["Gieb", "himm\u00b7li\u00b7sche", "Fl\u00fc\u00b7gel", ",", "o", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den schweren Todesgedanken!", "tokens": ["Den", "schwe\u00b7ren", "To\u00b7des\u00b7ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}