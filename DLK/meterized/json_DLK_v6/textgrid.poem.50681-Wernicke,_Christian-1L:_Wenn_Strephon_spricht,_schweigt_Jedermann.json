{"textgrid.poem.50681": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn Strephon spricht, schweigt Jedermann", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Strephon spricht, schweigt Jedermann", "tokens": ["Wenn", "Stre\u00b7phon", "spricht", ",", "schweigt", "Je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "$,", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6rt ihn oft mit Lust, oft mit Verwundrung an;", "tokens": ["Und", "h\u00f6rt", "ihn", "oft", "mit", "Lust", ",", "oft", "mit", "Ver\u00b7wund\u00b7rung", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wird kein Wort von ihm verschwendet.", "tokens": ["Es", "wird", "kein", "Wort", "von", "ihm", "ver\u00b7schwen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Viel ist es, was er sagt, doch, was er nicht sagt, mehr;", "tokens": ["Viel", "ist", "es", ",", "was", "er", "sagt", ",", "doch", ",", "was", "er", "nicht", "sagt", ",", "mehr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem denkt man nach, wenn er geendet,", "tokens": ["Dem", "denkt", "man", "nach", ",", "wenn", "er", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PTKVZ", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und giebt ihm dann auch noch, wann er schon schweigt, Geh\u00f6r.", "tokens": ["Und", "giebt", "ihm", "dann", "auch", "noch", ",", "wann", "er", "schon", "schweigt", ",", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}