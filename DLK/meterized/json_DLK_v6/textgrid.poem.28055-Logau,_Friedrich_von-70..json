{"textgrid.poem.28055": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df meine Reime klar, rund, klug nicht fallen k\u00fcnnen,", "tokens": ["Da\u00df", "mei\u00b7ne", "Rei\u00b7me", "klar", ",", "rund", ",", "klug", "nicht", "fal\u00b7len", "k\u00fcn\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "$,", "ADJD", "$,", "ADJD", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nicht der Sprache Schuld; die Schuld ist meiner Sinnen.", "tokens": ["Ist", "nicht", "der", "Spra\u00b7che", "Schuld", ";", "die", "Schuld", "ist", "mei\u00b7ner", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "NN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist l\u00f6blich etwa nicht, was ich hier schreibe das,", "tokens": ["Ist", "l\u00f6b\u00b7lich", "et\u00b7wa", "nicht", ",", "was", "ich", "hier", "schrei\u00b7be", "das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "PTKNEG", "$,", "PWS", "PPER", "ADV", "VVFIN", "PDS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist l\u00f6blich etwa doch, da\u00df ich versuche was.", "tokens": ["Ist", "l\u00f6b\u00b7lich", "et\u00b7wa", "doch", ",", "da\u00df", "ich", "ver\u00b7su\u00b7che", "was", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}