{"textgrid.poem.67732": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ein Fragment", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stirb, Phlegma! Du, mein Lied, sei Feuer!", "tokens": ["Stirb", ",", "Phleg\u00b7ma", "!", "Du", ",", "mein", "Lied", ",", "sei", "Feu\u00b7er", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NE", "$.", "PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich singt es und mein Gl\u00fcck!", "tokens": ["Mich", "singt", "es", "und", "mein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon horchen M\u00e4use! zaubre, Leyer!", "tokens": ["Schon", "hor\u00b7chen", "M\u00e4u\u00b7se", "!", "zaub\u00b7re", ",", "Le\u00b7yer", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$.", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dem Orpheus piepen sie zur\u00fcck.", "tokens": ["Dem", "Or\u00b7pheus", "pie\u00b7pen", "sie", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich Gymnosoph, wie viel kann ich entbehren!", "tokens": ["Ich", "Gym\u00b7no\u00b7soph", ",", "wie", "viel", "kann", "ich", "ent\u00b7beh\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWAV", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Pracht, Winter, Regen, seht!", "tokens": ["Pracht", ",", "Win\u00b7ter", ",", "Re\u00b7gen", ",", "seht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Euch trotzt mein Kleid und ird'schen Ehren,", "tokens": ["Euch", "trotzt", "mein", "Kleid", "und", "ird'\u00b7schen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und reich bin ich wie ein Poet", "tokens": ["Und", "reich", "bin", "ich", "wie", "ein", "Po\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und akademisch frei! Nur meinem Magen", "tokens": ["Und", "a\u00b7kad\u00b7e\u00b7misch", "frei", "!", "Nur", "mei\u00b7nem", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKVZ", "$.", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dien', denk' und bin ich \u2013 sonst recht frei!", "tokens": ["Dien'", ",", "denk'", "und", "bin", "ich", "\u2013", "sonst", "recht", "frei", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "KON", "VAFIN", "PPER", "$(", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}