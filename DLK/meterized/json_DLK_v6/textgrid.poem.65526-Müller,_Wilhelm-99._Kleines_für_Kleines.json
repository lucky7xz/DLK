{"textgrid.poem.65526": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "99. Kleines f\u00fcr Kleines", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die liebe kleine Zeit will Kleines haben,", "tokens": ["Die", "lie\u00b7be", "klei\u00b7ne", "Zeit", "will", "Klei\u00b7nes", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VMFIN", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drum bring' ich ihr so viele kleine Gaben.", "tokens": ["Drum", "bring'", "ich", "ihr", "so", "vie\u00b7le", "klei\u00b7ne", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus vielen Tagen wird ja doch ein Jahr \u2013", "tokens": ["Aus", "vie\u00b7len", "Ta\u00b7gen", "wird", "ja", "doch", "ein", "Jahr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sei ganz und sieh ein Ganzes in der Schaar.", "tokens": ["Sei", "ganz", "und", "sieh", "ein", "Gan\u00b7zes", "in", "der", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "VVIMP", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}