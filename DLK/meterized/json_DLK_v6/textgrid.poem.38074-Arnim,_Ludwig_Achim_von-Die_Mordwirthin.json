{"textgrid.poem.38074": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Mordwirthin", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren drei Soldaten-S\u00f6hn,", "tokens": ["Es", "wa\u00b7ren", "drei", "Sol\u00b7da\u00b7ten\u00b7S\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie haben Lust im Krieg zu gehn,", "tokens": ["Sie", "ha\u00b7ben", "Lust", "im", "Krieg", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl ins Soldaten Leben.", "tokens": ["Wohl", "ins", "Sol\u00b7da\u00b7ten", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie bleiben aus eine kleine Weil,", "tokens": ["Sie", "blei\u00b7ben", "aus", "ei\u00b7ne", "klei\u00b7ne", "Weil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie machen sich Geld und Brod dabei,", "tokens": ["Sie", "ma\u00b7chen", "sich", "Geld", "und", "Brod", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auch Ungrische Dukaten.", "tokens": ["Auch", "Un\u00b7gri\u00b7sche", "Du\u00b7ka\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie haben sich ganz kurz bedacht,", "tokens": ["Sie", "ha\u00b7ben", "sich", "ganz", "kurz", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und haben sich wieder nach Haus gemacht,", "tokens": ["Und", "ha\u00b7ben", "sich", "wie\u00b7der", "nach", "Haus", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Frau Wirthin sprang entgegen:", "tokens": ["Frau", "Wirt\u00b7hin", "sprang", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbfrau Wirthin hat sie die Gewalt,", "tokens": ["\u00bb", "frau", "Wirt\u00b7hin", "hat", "sie", "die", "Ge\u00b7walt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein'n Reiter \u00fcber Nacht aus zu behalten,", "tokens": ["Ein'n", "Rei\u00b7ter", "\u00fc\u00b7ber", "Nacht", "aus", "zu", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dazu und auch gastiren?\u00ab", "tokens": ["Da\u00b7zu", "und", "auch", "gas\u00b7ti\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "KON", "ADV", "VVINF", "$.", "$("], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Warum werd ich die Gewalt nicht hab'n,", "tokens": ["Wa\u00b7rum", "werd", "ich", "die", "Ge\u00b7walt", "nicht", "hab'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Einen Reiter \u00fcber Nacht zu behalten,", "tokens": ["Ei\u00b7nen", "Rei\u00b7ter", "\u00fc\u00b7ber", "Nacht", "zu", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dazu und auch gastiren?", "tokens": ["Da\u00b7zu", "und", "auch", "gas\u00b7ti\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "KON", "ADV", "VVINF", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Der Reiter sezt sich oben an den Tisch,", "tokens": ["Der", "Rei\u00b7ter", "sezt", "sich", "o\u00b7ben", "an", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie mag mir auftragen was sie will,", "tokens": ["Sie", "mag", "mir", "auf\u00b7tra\u00b7gen", "was", "sie", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PRELS", "PPER", "VMFIN", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Ich kanns ja wohl bezahlen.", "tokens": ["Ich", "kanns", "ja", "wohl", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie tr\u00e4gt ihm auf gebackne Fisch,", "tokens": ["Sie", "tr\u00e4gt", "ihm", "auf", "ge\u00b7back\u00b7ne", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und einen Schweinebraten,", "tokens": ["Und", "ei\u00b7nen", "Schwei\u00b7ne\u00b7bra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als es war, als da man schlief:", "tokens": ["Und", "als", "es", "war", ",", "als", "da", "man", "schlief", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach Mann ich kann nicht schlafen!", "tokens": ["Ach", "Mann", "ich", "kann", "nicht", "schla\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie macht das Pf\u00e4nnchen mit Fette hei\u00df,", "tokens": ["Sie", "macht", "das", "Pf\u00e4nn\u00b7chen", "mit", "Fet\u00b7te", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und sch\u00fctt's dem Reiter in Hals hinein.", "tokens": ["Und", "sch\u00fctt's", "dem", "Rei\u00b7ter", "in", "Hals", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Kriegt ihn an seiner schneeweissen Hand", "tokens": ["Kriegt", "ihn", "an", "sei\u00b7ner", "schnee\u00b7weis\u00b7sen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und schleift ihn in Keller in k\u00fchlen Sand:", "tokens": ["Und", "schleift", "ihn", "in", "Kel\u00b7ler", "in", "k\u00fch\u00b7len", "Sand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da kannst du liegen", "tokens": ["Da", "kannst", "du", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Bis morgen Mittag verschwiegen;", "tokens": ["Bis", "mor\u00b7gen", "Mit\u00b7tag", "ver\u00b7schwie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Des Morgens als sein Kammerad kam:", "tokens": ["Des", "Mor\u00b7gens", "als", "sein", "Kam\u00b7me\u00b7rad", "kam", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wo ist der Reiter?", "tokens": ["Wo", "ist", "der", "Rei\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "\u00bbder Reiter und der ist weiter,", "tokens": ["\u00bb", "der", "Rei\u00b7ter", "und", "der", "ist", "wei\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "ART", "VAFIN", "ADV", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Reiter der kann weiter sein.\u00ab", "tokens": ["Der", "Rei\u00b7ter", "der", "kann", "wei\u00b7ter", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "VMFIN", "ADV", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er kann in eurem Hause sein.", "tokens": ["Er", "kann", "in", "eu\u00b7rem", "Hau\u00b7se", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat sie dem Reiter was Leids gethan,", "tokens": ["Hat", "sie", "dem", "Rei\u00b7ter", "was", "Leids", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PWS", "NE", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So hat sie's ihrem lieben Sohn gethan,", "tokens": ["So", "hat", "sie's", "ih\u00b7rem", "lie\u00b7ben", "Sohn", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der aus dem Krieg ist kommen.", "tokens": ["Der", "aus", "dem", "Krieg", "ist", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie hat sich in den Brunnen gesprengt,", "tokens": ["Sie", "hat", "sich", "in", "den", "Brun\u00b7nen", "ge\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er hat sich in die Scheuer geh\u00e4ngt,", "tokens": ["Er", "hat", "sich", "in", "die", "Scheu\u00b7er", "ge\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "M\u00fcssen an einem Tag drei sterben.", "tokens": ["M\u00fcs\u00b7sen", "an", "ei\u00b7nem", "Tag", "drei", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "CARD", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}