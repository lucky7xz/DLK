{"dta.poem.4402": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wiese.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Sch\u00f6pfer Himmels und der Erde,", "tokens": ["O", "Sch\u00f6p\u00b7fer", "Him\u00b7mels", "und", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gib, da\u00df mein Herz ger\u00fchret werde!", "tokens": ["Gib", ",", "da\u00df", "mein", "Herz", "ge\u00b7r\u00fch\u00b7ret", "wer\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich, so oft ich sch\u00f6ne Wiesen", "tokens": ["Da\u00df", "ich", ",", "so", "oft", "ich", "sch\u00f6\u00b7ne", "Wie\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADV", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erblicke, sie mit Lust und Andacht seh;", "tokens": ["Er\u00b7bli\u00b7cke", ",", "sie", "mit", "Lust", "und", "An\u00b7dacht", "seh", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df ich, durch sie vergn\u00fcg\u2019t, Dich, ihren HErrn,", "tokens": ["Da\u00df", "ich", ",", "durch", "sie", "ver\u00b7gn\u00fcg't", ",", "Dich", ",", "ih\u00b7ren", "Herrn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPER", "VVFIN", "$,", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "erh\u00f6h',", "tokens": ["er\u00b7h\u00f6h'", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Und denke: Grosser GOtt, sey hier und dort ge-", "tokens": ["Und", "den\u00b7ke", ":", "Gros\u00b7ser", "Gott", ",", "sey", "hier", "und", "dort", "ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADJA", "NN", "$,", "VAFIN", "ADV", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "priesen!", "tokens": ["prie\u00b7sen", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}