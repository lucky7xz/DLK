{"textgrid.poem.25781": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Faun", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Walde Faun,", "tokens": ["Im", "Wal\u00b7de", "Faun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Tief im Grase.", "tokens": ["Tief", "im", "Gra\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die Ohren gereckt, mit schnuppernder Nase,", "tokens": ["Die", "Oh\u00b7ren", "ge\u00b7reckt", ",", "mit", "schnup\u00b7pern\u00b7der", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+---+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Horcht \u00fcber die Halme zur sinkenden Sonne.", "tokens": ["Horcht", "\u00fc\u00b7ber", "die", "Hal\u00b7me", "zur", "sin\u00b7ken\u00b7den", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Affenstirne in Falten geschoben,", "tokens": ["Und", "die", "Af\u00b7fens\u00b7tir\u00b7ne", "in", "Fal\u00b7ten", "ge\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und zitternd die geballte Rechte gehoben:", "tokens": ["Und", "zit\u00b7ternd", "die", "ge\u00b7ball\u00b7te", "Rech\u00b7te", "ge\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbverfluchte Gewalt, du sollst mich nicht zwingen!\u00ab", "tokens": ["\u00bb", "ver\u00b7fluch\u00b7te", "Ge\u00b7walt", ",", "du", "sollst", "mich", "nicht", "zwin\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Sonne sinkt.", "tokens": ["Die", "Son\u00b7ne", "sinkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Nachtd\u00fcfte steigen,", "tokens": ["Die", "Nacht\u00b7d\u00fcf\u00b7te", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Aus der Waldtiefe", "tokens": ["Aus", "der", "Wald\u00b7tie\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--++-", "measure": "anapaest.init"}, "line.4": {"text": "Schwillt rot das Schweigen.", "tokens": ["Schwillt", "rot", "das", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Faun kriecht zusammen,", "tokens": ["Faun", "kriecht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In die harten Bl\u00e4tter am Boden,", "tokens": ["In", "die", "har\u00b7ten", "Bl\u00e4t\u00b7ter", "am", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Scharrt Moos, Steine auf:", "tokens": ["Scharrt", "Moos", ",", "Stei\u00b7ne", "auf", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "\u00bberde, la\u00df mich hinein!\u00ab", "tokens": ["\u00bb", "er\u00b7de", ",", "la\u00df", "mich", "hin\u00b7ein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und er w\u00fchlt die Stirn in die Erde ein,", "tokens": ["Und", "er", "w\u00fchlt", "die", "Stirn", "in", "die", "Er\u00b7de", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bei\u00dft um sich, zerfetzt, hackt \u2013", "tokens": ["Bei\u00dft", "um", "sich", ",", "zer\u00b7fetzt", ",", "hackt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "$,", "VVPP", "$,", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Aber alles bleibt reglos.", "tokens": ["A\u00b7ber", "al\u00b7les", "bleibt", "reg\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie h\u00f6ren ihn nicht.", "tokens": ["Sie", "h\u00f6\u00b7ren", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Nur Stille rings mit versteintem Gesicht.", "tokens": ["Nur", "Stil\u00b7le", "rings", "mit", "ver\u00b7stein\u00b7tem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Mondlicht brennt.", "tokens": ["Mond\u00b7licht", "brennt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Im Walde jagen die wei\u00dfen eisigen Feuer,", "tokens": ["Im", "Wal\u00b7de", "ja\u00b7gen", "die", "wei\u00b7\u00dfen", "ei\u00b7si\u00b7gen", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und von br\u00fcnstigen Flammen entfacht,", "tokens": ["Und", "von", "br\u00fcns\u00b7ti\u00b7gen", "Flam\u00b7men", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.13": {"text": "Tanzt Faun und lacht,", "tokens": ["Tanzt", "Faun", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Und tanzt und lacht,", "tokens": ["Und", "tanzt", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Mit wunden Augen in br\u00fcnstigen Flammen.", "tokens": ["Mit", "wun\u00b7den", "Au\u00b7gen", "in", "br\u00fcns\u00b7ti\u00b7gen", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}