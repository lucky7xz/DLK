{"textgrid.poem.26774": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das weibliche Ideal", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberall weichet das Weib dem Manne, nur in dem H\u00f6chsten", "tokens": ["\u00dc\u00b7be\u00b7rall", "wei\u00b7chet", "das", "Weib", "dem", "Man\u00b7ne", ",", "nur", "in", "dem", "H\u00f6chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Weichet dem weiblichsten Weib immer der m\u00e4nnlichste Mann.", "tokens": ["Wei\u00b7chet", "dem", "weib\u00b7lichs\u00b7ten", "Weib", "im\u00b7mer", "der", "m\u00e4nn\u00b7lichs\u00b7te", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Was das H\u00f6chste mir sei? Des Sieges ruhige Klarheit,", "tokens": ["Was", "das", "H\u00f6chs\u00b7te", "mir", "sei", "?", "Des", "Sie\u00b7ges", "ru\u00b7hi\u00b7ge", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "PPER", "VAFIN", "$.", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wie sie von deiner Stirn, holde Amanda, mir strahlt.", "tokens": ["Wie", "sie", "von", "dei\u00b7ner", "Stirn", ",", "hol\u00b7de", "A\u00b7man\u00b7da", ",", "mir", "strahlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "NE", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Schwimmt auch die Wolke des Grams um die heiter gl\u00e4nzende Scheibe,", "tokens": ["Schwimmt", "auch", "die", "Wol\u00b7ke", "des", "Grams", "um", "die", "hei\u00b7ter", "gl\u00e4n\u00b7zen\u00b7de", "Schei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Sch\u00f6ner nur malt sich das Bild auf dem vergoldeten Duft.", "tokens": ["Sch\u00f6\u00b7ner", "nur", "malt", "sich", "das", "Bild", "auf", "dem", "ver\u00b7gol\u00b7de\u00b7ten", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "D\u00fcnke der Mann sich frei! Du bist es, denn ewig notwendig", "tokens": ["D\u00fcn\u00b7ke", "der", "Mann", "sich", "frei", "!", "Du", "bist", "es", ",", "denn", "e\u00b7wig", "not\u00b7wen\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PRF", "ADJD", "$.", "PPER", "VAFIN", "PPER", "$,", "KON", "ADJD", "ADJD"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Wei\u00dft du von keiner Wahl, keiner Notwendigkeit mehr.", "tokens": ["Wei\u00dft", "du", "von", "kei\u00b7ner", "Wahl", ",", "kei\u00b7ner", "Not\u00b7wen\u00b7dig\u00b7keit", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Was du auch gibst, stets gibst du dich ganz, du bist ewig nur Eines,", "tokens": ["Was", "du", "auch", "gibst", ",", "stets", "gibst", "du", "dich", "ganz", ",", "du", "bist", "e\u00b7wig", "nur", "Ei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "PPER", "VAFIN", "ADJD", "ADV", "PIS", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Auch dein z\u00e4rtester Laut ist dein harmonisches Selbst.", "tokens": ["Auch", "dein", "z\u00e4r\u00b7tes\u00b7ter", "Laut", "ist", "dein", "har\u00b7mo\u00b7ni\u00b7sches", "Selbst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "APPR", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Hier ist ewige Jugend bei niemals versiegender F\u00fclle,", "tokens": ["Hier", "ist", "e\u00b7wi\u00b7ge", "Ju\u00b7gend", "bei", "nie\u00b7mals", "ver\u00b7sie\u00b7gen\u00b7der", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Und mit der Blume zugleich brichst du die goldene Frucht.", "tokens": ["Und", "mit", "der", "Blu\u00b7me", "zu\u00b7gleich", "brichst", "du", "die", "gol\u00b7de\u00b7ne", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "---+--+---+--+", "measure": "iambic.tetra.relaxed"}}}}}