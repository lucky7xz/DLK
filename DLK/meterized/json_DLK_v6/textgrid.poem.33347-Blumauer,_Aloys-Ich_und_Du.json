{"textgrid.poem.33347": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Ich und Du", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich f\u00fchret Mars in's blut'ge Feld,", "tokens": ["Dich", "f\u00fch\u00b7ret", "Mars", "in's", "blut'\u00b7ge", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich Amor zu den Hirten:", "tokens": ["Mich", "A\u00b7mor", "zu", "den", "Hir\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du kr\u00f6nst mit Lorbeern dich als Held,", "tokens": ["Du", "kr\u00f6nst", "mit", "Lor\u00b7beern", "dich", "als", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kr\u00e4nze mich mit Myrten.", "tokens": ["Ich", "kr\u00e4n\u00b7ze", "mich", "mit", "Myr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dich st\u00f6ret fr\u00fch der Pferde Huf,", "tokens": ["Dich", "st\u00f6\u00b7ret", "fr\u00fch", "der", "Pfer\u00b7de", "Huf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und der Trompete Schallen:", "tokens": ["Und", "der", "Trom\u00b7pe\u00b7te", "Schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mich aber weckt der s\u00fcsse Ruf", "tokens": ["Mich", "a\u00b7ber", "weckt", "der", "s\u00fcs\u00b7se", "Ruf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verliebter Nachtigallen.", "tokens": ["Ver\u00b7lieb\u00b7ter", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du nahst dich jeder Festung still", "tokens": ["Du", "nahst", "dich", "je\u00b7der", "Fes\u00b7tung", "still"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In n\u00e4chtlichen Approschen,", "tokens": ["In", "n\u00e4cht\u00b7li\u00b7chen", "Ap\u00b7pro\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich mich einer n\u00e4hern will,", "tokens": ["Wenn", "ich", "mich", "ei\u00b7ner", "n\u00e4\u00b7hern", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Versteck' ich mich in Poschen.", "tokens": ["Ver\u00b7steck'", "ich", "mich", "in", "Po\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du raubst dem Feinde Hab' und Gut,", "tokens": ["Du", "raubst", "dem", "Fein\u00b7de", "Hab'", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich den M\u00e4dchen K\u00fcsse;", "tokens": ["Und", "ich", "den", "M\u00e4d\u00b7chen", "K\u00fcs\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei deinen K\u00e4mpfen setzt es Blut,", "tokens": ["Bei", "dei\u00b7nen", "K\u00e4mp\u00b7fen", "setzt", "es", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei meinen h\u00f6chstens Bisse.", "tokens": ["Bei", "mei\u00b7nen", "h\u00f6chs\u00b7tens", "Bis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Streckst du den Feind zur Erde hin,", "tokens": ["Streckst", "du", "den", "Feind", "zur", "Er\u00b7de", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bleibt er unbedecket;", "tokens": ["So", "bleibt", "er", "un\u00b7be\u00b7de\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber werfe mich auf ihn,", "tokens": ["Ich", "a\u00b7ber", "wer\u00b7fe", "mich", "auf", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sobald ich ihn gestrecket.", "tokens": ["So\u00b7bald", "ich", "ihn", "ge\u00b7stre\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du machst der Wittwen t\u00e4glich mehr,", "tokens": ["Du", "machst", "der", "Witt\u00b7wen", "t\u00e4g\u00b7lich", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und, ach! der V\u00e4ter minder:", "tokens": ["Und", ",", "ach", "!", "der", "V\u00e4\u00b7ter", "min\u00b7der", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich mach' der Wittwen weniger,", "tokens": ["Ich", "mach'", "der", "Witt\u00b7wen", "we\u00b7ni\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mehr der kleinen Kinder.", "tokens": ["Und", "mehr", "der", "klei\u00b7nen", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Von deinen Thaten wird ein Stein", "tokens": ["Von", "dei\u00b7nen", "Tha\u00b7ten", "wird", "ein", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nachwelt einst belehren:", "tokens": ["Die", "Nach\u00b7welt", "einst", "be\u00b7leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die meinen wird sie, gro\u00df und klein,", "tokens": ["Die", "mei\u00b7nen", "wird", "sie", ",", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von meinen Enkeln h\u00f6ren.", "tokens": ["Von", "mei\u00b7nen", "En\u00b7keln", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}