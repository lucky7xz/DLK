{"dta.poem.8263": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "f\u00fcnffter  A ufftritt.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Bleibt Anton mein Eigenthum/", "tokens": ["Bleibt", "An\u00b7ton", "mein", "Ei\u00b7gen\u00b7thum", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPOSAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "La\u00df ich alle Krohnen stehen;", "tokens": ["La\u00df", "ich", "al\u00b7le", "Kroh\u00b7nen", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und will mich allein erh\u00f6hen", "tokens": ["Und", "will", "mich", "al\u00b7lein", "er\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Liebe Sieges-Ruhm;", "tokens": ["Durch", "der", "Lie\u00b7be", "Sie\u00b7ges\u00b7Ruhm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleibt Anton mein Eigenthum.", "tokens": ["Bleibt", "An\u00b7ton", "mein", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}