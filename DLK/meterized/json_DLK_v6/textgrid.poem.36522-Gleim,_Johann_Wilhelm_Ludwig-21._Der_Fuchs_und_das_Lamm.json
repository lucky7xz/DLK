{"textgrid.poem.36522": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "21. Der Fuchs und das Lamm", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er schl\u00e4ft, weck' ihn nicht auf! Er sch\u00fcttelt seine M\u00e4hne,", "tokens": ["Er", "schl\u00e4ft", ",", "weck'", "ihn", "nicht", "auf", "!", "Er", "sch\u00fct\u00b7telt", "sei\u00b7ne", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIMP", "PPER", "PTKNEG", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weist seine mordgewohnten Z\u00e4hne,", "tokens": ["Weist", "sei\u00b7ne", "mord\u00b7ge\u00b7wohn\u00b7ten", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Thut einen Sprung, braucht seine Klaue,", "tokens": ["Thut", "ei\u00b7nen", "Sprung", ",", "braucht", "sei\u00b7ne", "Klau\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zerrei\u00dft in tausend St\u00fccken", "tokens": ["Zer\u00b7rei\u00dft", "in", "tau\u00b7send", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dich L\u00e4mmchen! \u00dcbern Zaun", "tokens": ["Dich", "L\u00e4mm\u00b7chen", "!", "\u00dc\u00b7bern", "Zaun"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$.", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu springen, d\u00fcrfte dir nicht gl\u00fccken!", "tokens": ["Zu", "sprin\u00b7gen", ",", "d\u00fcrf\u00b7te", "dir", "nicht", "gl\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sprach schlau der Fuchs zum Lamm, das seinen Herrn, den K\u00f6nig", "tokens": ["Sprach", "schlau", "der", "Fuchs", "zum", "Lamm", ",", "das", "sei\u00b7nen", "Herrn", ",", "den", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "ART", "NE", "APPRART", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Tiere, schlafen sah. \u2013 Das schwache L\u00e4mmchen schlich", "tokens": ["Der", "Tie\u00b7re", ",", "schla\u00b7fen", "sah", ".", "\u2013", "Das", "schwa\u00b7che", "L\u00e4mm\u00b7chen", "schlich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVINF", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bei Zeiten sich hinweg, und brachte sich", "tokens": ["Bei", "Zei\u00b7ten", "sich", "hin\u00b7weg", ",", "und", "brach\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "PRF", "APZR", "$,", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In Sicherheit! O Stier! ach! h\u00e4ttest du so wenig,", "tokens": ["In", "Si\u00b7cher\u00b7heit", "!", "O", "Stier", "!", "ach", "!", "h\u00e4t\u00b7test", "du", "so", "we\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "NE", "NN", "$.", "XY", "$.", "VAFIN", "PPER", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ges\u00e4umet wie das Lamm, f\u00fcrwahr! du lebtest noch!", "tokens": ["Ge\u00b7s\u00e4u\u00b7met", "wie", "das", "Lamm", ",", "f\u00fcr\u00b7wahr", "!", "du", "leb\u00b7test", "noch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "ADV", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}