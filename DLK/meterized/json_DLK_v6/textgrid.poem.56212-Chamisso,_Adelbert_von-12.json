{"textgrid.poem.56212": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "12", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So still das Tal geworden! \u2013 ach! die Lieder,", "tokens": ["So", "still", "das", "Tal", "ge\u00b7wor\u00b7den", "!", "\u2013", "ach", "!", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VAPP", "$.", "$(", "ITJ", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seitdem er fortgezogen, sind verhallt;", "tokens": ["Seit\u00b7dem", "er", "fort\u00b7ge\u00b7zo\u00b7gen", ",", "sind", "ver\u00b7hallt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sorglos wandl ich, aber trauernd wieder", "tokens": ["Und", "sorg\u00b7los", "wandl", "ich", ",", "a\u00b7ber", "trau\u00b7ernd", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Quell der Linden, wo sie sonst geschallt.", "tokens": ["Am", "Quell", "der", "Lin\u00b7den", ",", "wo", "sie", "sonst", "ge\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Winter schleicht heran, die B\u00e4ume zeigen", "tokens": ["Der", "Win\u00b7ter", "schleicht", "he\u00b7ran", ",", "die", "B\u00e4u\u00b7me", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die \u00c4ste schon vom falben Schmuck beraubt,", "tokens": ["Die", "\u00c4s\u00b7te", "schon", "vom", "fal\u00b7ben", "Schmuck", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Rosenbaum wird bald die Krone neigen", "tokens": ["Mein", "Ro\u00b7sen\u00b7baum", "wird", "bald", "die", "Kro\u00b7ne", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Reife schwer und schimmernd neu belaubt.", "tokens": ["Vom", "Rei\u00b7fe", "schwer", "und", "schim\u00b7mernd", "neu", "be\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und auch auf meinen Wangen, h\u00f6r ich sagen,", "tokens": ["Und", "auch", "auf", "mei\u00b7nen", "Wan\u00b7gen", ",", "h\u00f6r", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entf\u00e4rben sich die Rosen, sie sind bleich;", "tokens": ["Ent\u00b7f\u00e4r\u00b7ben", "sich", "die", "Ro\u00b7sen", ",", "sie", "sind", "bleich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mir ist wohl, ich habe nicht zu klagen,", "tokens": ["Und", "mir", "ist", "wohl", ",", "ich", "ha\u00b7be", "nicht", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin in der Erinnerung so reich!", "tokens": ["Ich", "bin", "in", "der", "E\u00b7rin\u00b7ne\u00b7rung", "so", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er hat, der Morgensonne gleich, dem Traume,", "tokens": ["Er", "hat", ",", "der", "Mor\u00b7gen\u00b7son\u00b7ne", "gleich", ",", "dem", "Trau\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem n\u00e4chtlichen, der Kindheit mich entr\u00fcckt;", "tokens": ["Dem", "n\u00e4cht\u00b7li\u00b7chen", ",", "der", "Kind\u00b7heit", "mich", "ent\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er schreite vor im lichterf\u00fcllten Raume,", "tokens": ["Er", "schrei\u00b7te", "vor", "im", "lich\u00b7ter\u00b7f\u00fcll\u00b7ten", "Rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es sinkt mein Blick geblendet und entz\u00fcckt.", "tokens": ["Es", "sinkt", "mein", "Blick", "ge\u00b7blen\u00b7det", "und", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich werde nicht, einf\u00e4lt'ges Kind, begehren,", "tokens": ["Ich", "wer\u00b7de", "nicht", ",", "ein\u00b7f\u00e4lt'\u00b7ges", "Kind", ",", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df mir die Sonne nur geh\u00f6ren soll;", "tokens": ["Da\u00df", "mir", "die", "Son\u00b7ne", "nur", "ge\u00b7h\u00f6\u00b7ren", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mag flammend mich ihr m\u00e4cht'ger Strahl verzehren,", "tokens": ["Mag", "flam\u00b7mend", "mich", "ihr", "m\u00e4cht'\u00b7ger", "Strahl", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich segne sie und sterbe freudenvoll.", "tokens": ["Ich", "seg\u00b7ne", "sie", "und", "ster\u00b7be", "freu\u00b7den\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}