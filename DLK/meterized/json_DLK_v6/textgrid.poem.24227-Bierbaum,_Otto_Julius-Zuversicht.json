{"textgrid.poem.24227": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Zuversicht", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich zu lieben, das wird Ruhe sein,", "tokens": ["Dich", "zu", "lie\u00b7ben", ",", "das", "wird", "Ru\u00b7he", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PDS", "VAFIN", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hand in Hand, getrost und ohne Bangen;", "tokens": ["Hand", "in", "Hand", ",", "ge\u00b7trost", "und", "oh\u00b7ne", "Ban\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVPP", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kein Verzagen \u2013: Glauben; kein Verlangen \u2013:", "tokens": ["Kein", "Ver\u00b7za\u00b7gen", "\u2013", ":", "Glau\u00b7ben", ";", "kein", "Ver\u00b7lan\u00b7gen", "\u2013", ":"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$(", "$.", "NN", "$.", "PIAT", "NN", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Frucht und Friede, Freiheit und Verein.", "tokens": ["Frucht", "und", "Frie\u00b7de", ",", "Frei\u00b7heit", "und", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aber Lust wird in der Ruhe sein,", "tokens": ["A\u00b7ber", "Lust", "wird", "in", "der", "Ru\u00b7he", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sommerlust, ein Schauen und Genie\u00dfen,", "tokens": ["Som\u00b7mer\u00b7lust", ",", "ein", "Schau\u00b7en", "und", "Ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Jene Lust der windbewegten Wiesen,", "tokens": ["Je\u00b7ne", "Lust", "der", "wind\u00b7be\u00b7weg\u00b7ten", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die voll Blumen sind und still gedeihn.", "tokens": ["Die", "voll", "Blu\u00b7men", "sind", "und", "still", "ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}