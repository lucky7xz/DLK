{"dta.poem.20828": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wann ich die Todes Nacht geschwinde zu erreichen", "tokens": ["Wann", "ich", "die", "To\u00b7des", "Nacht", "ge\u00b7schwin\u00b7de", "zu", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gest\u00fcrtzet wolte seyn hin in den Flu\u00df der Leichen", "tokens": ["Ge\u00b7st\u00fcrt\u00b7zet", "wol\u00b7te", "seyn", "hin", "in", "den", "Flu\u00df", "der", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPOSAT", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da Charons Nache h\u00e4lt", "tokens": ["Da", "Cha\u00b7rons", "Na\u00b7che", "h\u00e4lt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit sehnlichen Verlangen", "tokens": ["Mit", "sehn\u00b7li\u00b7chen", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zulassen diese Welt/", "tokens": ["Zu\u00b7las\u00b7sen", "die\u00b7se", "Welt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In Meinung dergestalt der Noth zu seyn entgangen/", "tokens": ["In", "Mei\u00b7nung", "der\u00b7ge\u00b7stalt", "der", "Noth", "zu", "seyn", "ent\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "PTKZU", "VAINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So w\u00fcrd ich sehr und gr\u00f6blich fehlen/", "tokens": ["So", "w\u00fcrd", "ich", "sehr", "und", "gr\u00f6b\u00b7lich", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und wer in den Gedancken bleibt", "tokens": ["Und", "wer", "in", "den", "Ge\u00b7dan\u00b7cken", "bleibt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der kan vern\u00fcnfftig nicht erzehlen/", "tokens": ["Der", "kan", "ver\u00b7n\u00fcnff\u00b7tig", "nicht", "er\u00b7zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Was ihn zu seinem Ende treibt.", "tokens": ["Was", "ihn", "zu", "sei\u00b7nem", "En\u00b7de", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Di\u00df hoff ich/ wenn mein Fleisch wird seyn von hier getragen/", "tokens": ["Di\u00df", "hoff", "ich", "/", "wenn", "mein", "Fleisch", "wird", "seyn", "von", "hier", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "VAFIN", "PPOSAT", "APPR", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So manches mal gedruckt viel tausend Angst und Plagen/", "tokens": ["So", "man\u00b7ches", "mal", "ge\u00b7druckt", "viel", "tau\u00b7send", "Angst", "und", "Pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "VVPP", "ADV", "CARD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "den G\u00f6ttern gleich zu seyn/", "tokens": ["den", "G\u00f6t\u00b7tern", "gleich", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und in dem andern Leben", "tokens": ["Und", "in", "dem", "an\u00b7dern", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "In Freuden-reichen Schein", "tokens": ["In", "Freu\u00b7den\u00b7rei\u00b7chen", "Schein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Und frommer Leute Schaar nicht ohne Lust zu schweben.", "tokens": ["Und", "from\u00b7mer", "Leu\u00b7te", "Schaar", "nicht", "oh\u00b7ne", "Lust", "zu", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "PTKNEG", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da gibt das ewige Beschliessen/", "tokens": ["Da", "gibt", "das", "e\u00b7wi\u00b7ge", "Be\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "So wol viel Wollust als viel Pein.", "tokens": ["So", "wol", "viel", "Wol\u00b7lust", "als", "viel", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die B\u00f6sen werden leiden m\u00fcssen/", "tokens": ["Die", "B\u00f6\u00b7sen", "wer\u00b7den", "lei\u00b7den", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Den Frommen bleibt die Lust allein.", "tokens": ["Den", "From\u00b7men", "bleibt", "die", "Lust", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}