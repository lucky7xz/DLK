{"textgrid.poem.36024": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[und Gottes Friede sei mit dir]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und Gottes Friede sei mit dir,", "tokens": ["Und", "Got\u00b7tes", "Frie\u00b7de", "sei", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er mag dich reichlich segnen.", "tokens": ["Er", "mag", "dich", "reich\u00b7lich", "seg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach Kind, du wirst auf Erden hier", "tokens": ["Ach", "Kind", ",", "du", "wirst", "auf", "Er\u00b7den", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "PPER", "VAFIN", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch manchem Schmerz begegnen.", "tokens": ["Noch", "man\u00b7chem", "Schmerz", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das F\u00fc\u00dfchen, das so zart jetzt ist,", "tokens": ["Das", "F\u00fc\u00df\u00b7chen", ",", "das", "so", "zart", "jetzt", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird hart auf Dornen treten.", "tokens": ["Wird", "hart", "auf", "Dor\u00b7nen", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann lerne fr\u00fch zu Jesu Christ", "tokens": ["Dann", "ler\u00b7ne", "fr\u00fch", "zu", "Je\u00b7su", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In gl\u00e4ub'ger Demut beten.", "tokens": ["In", "gl\u00e4ub'\u00b7ger", "De\u00b7mut", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jetzt, da der Herr uns Blumen schafft,", "tokens": ["Jetzt", ",", "da", "der", "Herr", "uns", "Blu\u00b7men", "schafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat er auch dich gegeben.", "tokens": ["Hat", "er", "auch", "dich", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Willkommen auf der Pilgrimschaft,", "tokens": ["Will\u00b7kom\u00b7men", "auf", "der", "Pil\u00b7grim\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du zartes, liebes Leben!", "tokens": ["Du", "zar\u00b7tes", ",", "lie\u00b7bes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du bist nach Christi Wort getauft,", "tokens": ["Du", "bist", "nach", "Chris\u00b7ti", "Wort", "ge\u00b7tauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sollst den Himmel erben,", "tokens": ["Du", "sollst", "den", "Him\u00b7mel", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Heiland hat ihn dir erkauft", "tokens": ["Dein", "Hei\u00b7land", "hat", "ihn", "dir", "er\u00b7kauft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch sein Verdienst und Sterben.", "tokens": ["Durch", "sein", "Ver\u00b7dienst", "und", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}