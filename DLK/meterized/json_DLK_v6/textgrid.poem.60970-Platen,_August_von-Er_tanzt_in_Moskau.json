{"textgrid.poem.60970": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Er tanzt in Moskau", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seht die Sterne blutigrot", "tokens": ["Ihr", "seht", "die", "Ster\u00b7ne", "blu\u00b7ti\u00b7grot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf euer Warschau flimmern;", "tokens": ["Auf", "eu\u00b7er", "Warsc\u00b7hau", "flim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch trotz, o Polen, trotz der Not", "tokens": ["Doch", "trotz", ",", "o", "Po\u00b7len", ",", "trotz", "der", "Not"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "FM", "NE", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt endlich auf zu wimmern!", "tokens": ["H\u00f6rt", "end\u00b7lich", "auf", "zu", "wim\u00b7mern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn das Herz euch auch zerbricht", "tokens": ["Und", "wenn", "das", "Herz", "euch", "auch", "zer\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "St\u00f6rt euern guten K\u00f6nig nicht:", "tokens": ["St\u00f6rt", "eu\u00b7ern", "gu\u00b7ten", "K\u00f6\u00b7nig", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der K\u00f6nig tanzt in Moskau!", "tokens": ["Der", "K\u00f6\u00b7nig", "tanzt", "in", "Mos\u00b7kau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Solch einen K\u00f6nig hat gewi\u00df", "tokens": ["Solch", "ei\u00b7nen", "K\u00f6\u00b7nig", "hat", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Volk noch nie gewonnen;", "tokens": ["Ein", "Volk", "noch", "nie", "ge\u00b7won\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Euch dr\u00fcckte Tod und Finsternis,", "tokens": ["Euch", "dr\u00fcck\u00b7te", "Tod", "und", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er blieb jedoch besonnen:", "tokens": ["Er", "blieb", "je\u00b7doch", "be\u00b7son\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Als er geh\u00f6rt von eurem Fall,", "tokens": ["Als", "er", "ge\u00b7h\u00f6rt", "von", "eu\u00b7rem", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da gab er einen gro\u00dfen Ball:", "tokens": ["Da", "gab", "er", "ei\u00b7nen", "gro\u00b7\u00dfen", "Ball", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der K\u00f6nig tanzt in Moskau!", "tokens": ["Der", "K\u00f6\u00b7nig", "tanzt", "in", "Mos\u00b7kau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ein wunderfeines Zartgef\u00fchl", "tokens": ["Ein", "wun\u00b7der\u00b7fei\u00b7nes", "Zart\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Verraten seine Schuhe;", "tokens": ["Ver\u00b7ra\u00b7ten", "sei\u00b7ne", "Schu\u00b7he", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Ihr Sklaven, bl\u00e4ht ihm einen Pf\u00fchl,", "tokens": ["Ihr", "Skla\u00b7ven", ",", "bl\u00e4ht", "ihm", "ei\u00b7nen", "Pf\u00fchl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Denn er bedarf der Ruhe!", "tokens": ["Denn", "er", "be\u00b7darf", "der", "Ru\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Er war bei seinem Volk so ganz,", "tokens": ["Er", "war", "bei", "sei\u00b7nem", "Volk", "so", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Er ist so warm, so warm \u2013 vom Tanz;", "tokens": ["Er", "ist", "so", "warm", ",", "so", "warm", "\u2013", "vom", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der K\u00f6nig tanzt in Moskau!", "tokens": ["Der", "K\u00f6\u00b7nig", "tanzt", "in", "Mos\u00b7kau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Von ihm vernehmen Schmeichelein", "tokens": ["Von", "ihm", "ver\u00b7neh\u00b7men", "Schmei\u00b7che\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Kirgisin und Mongolin,", "tokens": ["Kir\u00b7gi\u00b7sin", "und", "Mon\u00b7go\u00b7lin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Die er umstrickt mit Liebelein;", "tokens": ["Die", "er", "um\u00b7strickt", "mit", "Lie\u00b7be\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Drum weine nicht, o Polin!", "tokens": ["Drum", "wei\u00b7ne", "nicht", ",", "o", "Po\u00b7lin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "FM", "NN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.26": {"text": "Er rei\u00dft die Kinder dir vom Arm,", "tokens": ["Er", "rei\u00dft", "die", "Kin\u00b7der", "dir", "vom", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Doch tut er sonst dir keinen Harm:", "tokens": ["Doch", "tut", "er", "sonst", "dir", "kei\u00b7nen", "Harm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der K\u00f6nig tanzt in Moskau!", "tokens": ["Der", "K\u00f6\u00b7nig", "tanzt", "in", "Mos\u00b7kau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}