{"textgrid.poem.61876": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun braut es herbstlich auf den Auen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun braut es herbstlich auf den Auen,", "tokens": ["Nun", "braut", "es", "herbst\u00b7lich", "auf", "den", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den bunten Forst entlaubt der Nord,", "tokens": ["Den", "bun\u00b7ten", "Forst", "ent\u00b7laubt", "der", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schwirrend steuert hoch im Blauen", "tokens": ["Und", "schwir\u00b7rend", "steu\u00b7ert", "hoch", "im", "Blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zug der Wanderv\u00f6gel fort.", "tokens": ["Der", "Zug", "der", "Wan\u00b7der\u00b7v\u00f6\u00b7gel", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geheime Schwermut rieselt bange", "tokens": ["Ge\u00b7hei\u00b7me", "Schwer\u00b7mut", "rie\u00b7selt", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir durchs Gem\u00fct im Windeswehn \u2013", "tokens": ["Mir", "durchs", "Ge\u00b7m\u00fct", "im", "Win\u00b7des\u00b7wehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "APPRART", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Fahr wohl, mein Wald am Bergeshange!", "tokens": ["Fahr", "wohl", ",", "mein", "Wald", "am", "Ber\u00b7ges\u00b7han\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und werd' ich gr\u00fcn dich wiedersehn?", "tokens": ["Und", "werd'", "ich", "gr\u00fcn", "dich", "wie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, sicher tr\u00e4gt der Schwan die Kunde,", "tokens": ["Ach", ",", "si\u00b7cher", "tr\u00e4gt", "der", "Schwan", "die", "Kun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wann's Zeit zu wandern, in der Brust,", "tokens": ["Wann's", "Zeit", "zu", "wan\u00b7dern", ",", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wer verk\u00fcndet ", "tokens": ["Doch", "wer", "ver\u00b7k\u00fcn\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "O Herz, da du von hinnen mu\u00dft?", "tokens": ["O", "Herz", ",", "da", "du", "von", "hin\u00b7nen", "mu\u00dft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "APPR", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}