{"textgrid.poem.66558": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nachts, wenn \u00fcber bittren Fragen", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachts, wenn \u00fcber bittren Fragen", "tokens": ["Nachts", ",", "wenn", "\u00fc\u00b7ber", "bit\u00b7tren", "Fra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir der milde Schlaf entflohn,", "tokens": ["Dir", "der", "mil\u00b7de", "Schlaf", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rst du wohl, vom Wind getragen", "tokens": ["H\u00f6rst", "du", "wohl", ",", "vom", "Wind", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie vernomm'nen Glockenton.", "tokens": ["Nie", "ver\u00b7nom\u00b7m'\u00b7nen", "Glo\u00b7cken\u00b7ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wie in Aengsten geht ein Fl\u00fcstern", "tokens": ["Wie", "in", "A\u00b7engs\u00b7ten", "geht", "ein", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um das Dach, und durch die Rund", "tokens": ["Um", "das", "Dach", ",", "und", "durch", "die", "Rund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Zelle macht ein Knistern", "tokens": ["Dei\u00b7ner", "Zel\u00b7le", "macht", "ein", "Knis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leis' und unerkl\u00e4rt sich kund.", "tokens": ["Leis'", "und", "un\u00b7er\u00b7kl\u00e4rt", "sich", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In dir selbst, wer mocht' es wecken,", "tokens": ["In", "dir", "selbst", ",", "wer", "mocht'", "es", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "PWS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was dir wild die Brust durchbahnt?", "tokens": ["Was", "dir", "wild", "die", "Brust", "durch\u00b7bahnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaudernd mu\u00dft du mehr entdecken,", "tokens": ["Schau\u00b7dernd", "mu\u00dft", "du", "mehr", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als du je in dir geahnt!", "tokens": ["Als", "du", "je", "in", "dir", "ge\u00b7ahnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}