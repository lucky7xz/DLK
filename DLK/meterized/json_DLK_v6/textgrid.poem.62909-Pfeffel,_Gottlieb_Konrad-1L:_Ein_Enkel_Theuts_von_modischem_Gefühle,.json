{"textgrid.poem.62909": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Enkel Theuts von modischem Gef\u00fchle,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Enkel Theuts von modischem Gef\u00fchle,", "tokens": ["Ein", "En\u00b7kel", "Theuts", "von", "mo\u00b7di\u00b7schem", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Genius, mit Namen Legion,", "tokens": ["Ein", "Ge\u00b7nius", ",", "mit", "Na\u00b7men", "Le\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "(denn ihrer, Freund, sind nun in Deutschland viele)", "tokens": ["(", "denn", "ih\u00b7rer", ",", "Freund", ",", "sind", "nun", "in", "Deutschland", "vie\u00b7le", ")"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "$,", "NN", "$,", "VAFIN", "ADV", "APPR", "NE", "PIAT", "$("], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "That einen Ritterzug nach Albion,", "tokens": ["That", "ei\u00b7nen", "Rit\u00b7ter\u00b7zug", "nach", "Al\u00b7bi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Um, wie es G\u00f6then einst gelungen,", "tokens": ["Um", ",", "wie", "es", "G\u00f6\u00b7then", "einst", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "PWAV", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Muse Shakespeares auszusp\u00e4hn", "tokens": ["Die", "Mu\u00b7se", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7res", "aus\u00b7zu\u00b7sp\u00e4hn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVINF"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und des Kothurns Begeisterungen", "tokens": ["Und", "des", "Ko\u00b7thurns", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit k\u00fchnem Aug ihr abzusehn.", "tokens": ["Mit", "k\u00fch\u00b7nem", "Aug", "ihr", "ab\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er k\u00f6mmt nach Stratfort, k\u00fc\u00dft mit Wonneschauer", "tokens": ["Er", "k\u00f6mmt", "nach", "Strat\u00b7fort", ",", "k\u00fc\u00dft", "mit", "Won\u00b7ne\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den Maulbeerbaum vom gro\u00dfen Mann gepflanzt,", "tokens": ["Den", "Maul\u00b7be\u00b7er\u00b7baum", "vom", "gro\u00b7\u00dfen", "Mann", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Und tritt ins Heiligthum, um dessen fahle Mauer", "tokens": ["Und", "tritt", "ins", "Hei\u00b7lig\u00b7thum", ",", "um", "des\u00b7sen", "fah\u00b7le", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "KOUI", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Noch oft um Mitternacht ein Chor von Elfen tanzt.", "tokens": ["Noch", "oft", "um", "Mit\u00b7ter\u00b7nacht", "ein", "Chor", "von", "El\u00b7fen", "tanzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Finkenritter sucht in allen Ecken", "tokens": ["Der", "Fin\u00b7ken\u00b7rit\u00b7ter", "sucht", "in", "al\u00b7len", "E\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Den fernen Schatten auf; allein sein Adlerblick", "tokens": ["Den", "fer\u00b7nen", "Schat\u00b7ten", "auf", ";", "al\u00b7lein", "sein", "Ad\u00b7ler\u00b7blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Entdecket nichts; er flucht auf sein Geschick", "tokens": ["Ent\u00b7de\u00b7cket", "nichts", ";", "er", "flucht", "auf", "sein", "Ge\u00b7schick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und zog schon seinen Dolch, als er mit s\u00fc\u00dfem Schrecken", "tokens": ["Und", "zog", "schon", "sei\u00b7nen", "Dolch", ",", "als", "er", "mit", "s\u00fc\u00b7\u00dfem", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Noch unversiegt des Dichters Nachttopf fand.", "tokens": ["Noch", "un\u00b7ver\u00b7siegt", "des", "Dich\u00b7ters", "Nacht\u00b7topf", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Er bringt ihn im Triumph ins deutsche Vaterland.", "tokens": ["Er", "bringt", "ihn", "im", "Tri\u00b7umph", "ins", "deut\u00b7sche", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So prangte Manchas Held mit seinem Wunderbecken.", "tokens": ["So", "prang\u00b7te", "Man\u00b7chas", "Held", "mit", "sei\u00b7nem", "Wun\u00b7der\u00b7be\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Itzt sch\u00fcttet er ihn aus ... das war ein Wettergu\u00df!", "tokens": ["Itzt", "sch\u00fct\u00b7tet", "er", "ihn", "aus", "...", "das", "war", "ein", "Wet\u00b7ter\u00b7gu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$(", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Kopf weg, ihr Deutschen! Was darinn gewesen,", "tokens": ["Kopf", "weg", ",", "ihr", "Deut\u00b7schen", "!", "Was", "da\u00b7rinn", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "PWS", "PAV", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "K\u00f6nnt ihr im Me\u00dfkatalogus,", "tokens": ["K\u00f6nnt", "ihr", "im", "Me\u00df\u00b7ka\u00b7ta\u00b7lo\u00b7gus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Artikel Trauerspiele, lesen.", "tokens": ["Ar\u00b7ti\u00b7kel", "Trau\u00b7er\u00b7spie\u00b7le", ",", "le\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}