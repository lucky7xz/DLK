{"textgrid.poem.29957": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da die Sprache ward verwandelt, ward der Thurm nicht au\u00dfgebaut.", "tokens": ["Da", "die", "Spra\u00b7che", "ward", "ver\u00b7wan\u00b7delt", ",", "ward", "der", "Thurm", "nicht", "au\u00df\u00b7ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$,", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Weil die Kleidung sich so wandelt, wird kein deutscher Sinn geschaut.", "tokens": ["Weil", "die", "Klei\u00b7dung", "sich", "so", "wan\u00b7delt", ",", "wird", "kein", "deut\u00b7scher", "Sinn", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "VVFIN", "$,", "VAFIN", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}