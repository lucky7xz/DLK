{"textgrid.poem.59981": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Im Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O la\u00df sie tr\u00e4umen den Kaiserwahn,", "tokens": ["O", "la\u00df", "sie", "tr\u00e4u\u00b7men", "den", "Kai\u00b7ser\u00b7wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Alt-Deutschlands Ritter und Recken;", "tokens": ["Al\u00b7tDeut\u00b7schlands", "Rit\u00b7ter", "und", "Re\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie werden sich vor dem roten Hahn", "tokens": ["Wie", "wer\u00b7den", "sich", "vor", "dem", "ro\u00b7ten", "Hahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die rotern Adler verstecken!", "tokens": ["Die", "ro\u00b7tern", "Ad\u00b7ler", "ver\u00b7ste\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "O la\u00df sie tr\u00e4umen noch eine Nacht!", "tokens": ["O", "la\u00df", "sie", "tr\u00e4u\u00b7men", "noch", "ei\u00b7ne", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann wetzen wir aus die Scharte,", "tokens": ["Dann", "wet\u00b7zen", "wir", "aus", "die", "Schar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann werden Fidibusse gemacht", "tokens": ["Dann", "wer\u00b7den", "Fi\u00b7di\u00b7bus\u00b7se", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Aus der europ\u00e4ischen Karte.", "tokens": ["Aus", "der", "eu\u00b7ro\u00b7p\u00e4\u00b7i\u00b7schen", "Kar\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--++-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Die V\u00f6lker kommen und l\u00e4uten Sturm \u2013", "tokens": ["Die", "V\u00f6l\u00b7ker", "kom\u00b7men", "und", "l\u00e4u\u00b7ten", "Sturm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erwache, mein ", "tokens": ["Er\u00b7wa\u00b7che", ",", "mein"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Vom K\u00f6lner Dome zum Stefansturm", "tokens": ["Vom", "K\u00f6l\u00b7ner", "Do\u00b7me", "zum", "Ste\u00b7fans\u00b7turm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wird brausen die Rache, die Rache.", "tokens": ["Wird", "brau\u00b7sen", "die", "Ra\u00b7che", ",", "die", "Ra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Vom Stefansturm zum stillen Prag", "tokens": ["Vom", "Ste\u00b7fans\u00b7turm", "zum", "stil\u00b7len", "Prag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weiter, weiter nach Polen \u2013", "tokens": ["Und", "wei\u00b7ter", ",", "wei\u00b7ter", "nach", "Po\u00b7len", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "APPR", "NE", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das ist der K\u00f6nige J\u00fcngster Tag;", "tokens": ["Das", "ist", "der", "K\u00f6\u00b7ni\u00b7ge", "J\u00fcngs\u00b7ter", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Teufel, er wird sie holen.", "tokens": ["Der", "Teu\u00b7fel", ",", "er", "wird", "sie", "ho\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Die alten Kohorten am Tiberstrom", "tokens": ["Die", "al\u00b7ten", "Ko\u00b7hor\u00b7ten", "am", "Ti\u00b7ber\u00b7strom"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Stehn auf beim Klang der Trompeten;", "tokens": ["Stehn", "auf", "beim", "Klang", "der", "Trom\u00b7pe\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die Glocken schweigen, du ewiges Rom", "tokens": ["Die", "Glo\u00b7cken", "schwei\u00b7gen", ",", "du", "e\u00b7wi\u00b7ges", "Rom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "PPER", "ADJA", "NE"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vergi\u00df dein Singen und Beten!", "tokens": ["Ver\u00b7gi\u00df", "dein", "Sin\u00b7gen", "und", "Be\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Glocken schweigen, die Pfaffen schrein", "tokens": ["Die", "Glo\u00b7cken", "schwei\u00b7gen", ",", "die", "Pfaf\u00b7fen", "schrein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In ihren zertr\u00fcmmerten Hallen;", "tokens": ["In", "ih\u00b7ren", "zer\u00b7tr\u00fcm\u00b7mer\u00b7ten", "Hal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Den Heiligen wird der goldne Schein", "tokens": ["Den", "Hei\u00b7li\u00b7gen", "wird", "der", "gold\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom zitternden Haupte fallen.", "tokens": ["Vom", "zit\u00b7tern\u00b7den", "Haup\u00b7te", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Henker falten, vor Schrecken bleich,", "tokens": ["Die", "Hen\u00b7ker", "fal\u00b7ten", ",", "vor", "Schre\u00b7cken", "bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die blutigen H\u00e4nde zusammen;", "tokens": ["Die", "blu\u00b7ti\u00b7gen", "H\u00e4n\u00b7de", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und aus dem st\u00fcrzenden \u00d6sterreich", "tokens": ["Und", "aus", "dem", "st\u00fcr\u00b7zen\u00b7den", "\u00d6s\u00b7ter\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hoch lodern werden die Flammen.", "tokens": ["Hoch", "lo\u00b7dern", "wer\u00b7den", "die", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Das alles, das alles soll geschehn", "tokens": ["Das", "al\u00b7les", ",", "das", "al\u00b7les", "soll", "ge\u00b7schehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "$,", "PRELS", "PIS", "VMFIN", "VVINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In kommenden Fr\u00fchlingstagen \u2013", "tokens": ["In", "kom\u00b7men\u00b7den", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Herrgott, la\u00df die Welt nicht untergehn,", "tokens": ["Herr\u00b7gott", ",", "la\u00df", "die", "Welt", "nicht", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Eh die Nachtigallen schlagen!", "tokens": ["Eh", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}