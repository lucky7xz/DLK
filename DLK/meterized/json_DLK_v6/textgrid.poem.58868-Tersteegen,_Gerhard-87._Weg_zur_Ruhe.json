{"textgrid.poem.58868": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "87. Weg zur Ruhe", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mensch, suchst du Ruh, so la\u00df dein Treiben;", "tokens": ["Mensch", ",", "suchst", "du", "Ruh", ",", "so", "la\u00df", "dein", "Trei\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "NN", "$,", "ADV", "VVIMP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer Ruhe sucht, mu\u00df ruhig bleiben!", "tokens": ["Wer", "Ru\u00b7he", "sucht", ",", "mu\u00df", "ru\u00b7hig", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df eignes Wollen, eignes Tun", "tokens": ["La\u00df", "eig\u00b7nes", "Wol\u00b7len", ",", "eig\u00b7nes", "Tun"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und halt Gott still, so kannst du ruhn!", "tokens": ["Und", "halt", "Gott", "still", ",", "so", "kannst", "du", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}