{"dta.poem.19596": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Mars von Florenz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Die Th\u00fcrme von Florenz umblaut", "tokens": ["Die", "Th\u00fcr\u00b7me", "von", "Flo\u00b7renz", "um\u00b7blaut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "APPR"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Der s\u00fc\u00dfe Lenz, der junge Lenz,", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Lenz", ",", "der", "jun\u00b7ge", "Lenz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Frauen singen leis und laut", "tokens": ["Die", "Frau\u00b7en", "sin\u00b7gen", "leis", "und", "laut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In allen Gassen von Florenz.", "tokens": ["In", "al\u00b7len", "Gas\u00b7sen", "von", "Flo\u00b7renz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Rand der Arnobr\u00fccke steht", "tokens": ["Am", "Rand", "der", "Ar\u00b7no\u00b7br\u00fc\u00b7cke", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schwarzverwittert Marmelbild", "tokens": ["Ein", "schwarz\u00b7ver\u00b7wit\u00b7tert", "Mar\u00b7mel\u00b7bild"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Helmgeflatter, Kriegsger\u00e4t,", "tokens": ["Mit", "Helm\u00b7ge\u00b7flat\u00b7ter", ",", "Kriegs\u00b7ge\u00b7r\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott Mars, und l\u00e4chelt falsch und wild.", "tokens": ["Gott", "Mars", ",", "und", "l\u00e4\u00b7chelt", "falsch", "und", "wild", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u2014 \u201eGott Mars, wohl magst du finster schaun,", "tokens": ["\u201e", "Gott", "Mars", ",", "wohl", "magst", "du", "fins\u00b7ter", "schaun", ","], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NN", "NE", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drommete dr\u00f6hnt im Lenze nie,", "tokens": ["Drom\u00b7me\u00b7te", "dr\u00f6hnt", "im", "Len\u00b7ze", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Raub' eine dir von unsern Fraun!", "tokens": ["Raub'", "ei\u00b7ne", "dir", "von", "un\u00b7sern", "Fraun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch \u00fcber Venus preis' ich sie!\u201c", "tokens": ["Hoch", "\u00fc\u00b7ber", "Ve\u00b7nus", "preis'", "ich", "sie", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein J\u00fcngling ruft's dem Gott empor", "tokens": ["Ein", "J\u00fcng\u00b7ling", "ruft's", "dem", "Gott", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit lachend ausgestreckter Hand \u2014", "tokens": ["Mit", "la\u00b7chend", "aus\u00b7ge\u00b7streck\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm dringt ein Erzgedr\u00f6hn ans Ohr,", "tokens": ["Ihm", "dringt", "ein", "Erz\u00b7ge\u00b7dr\u00f6hn", "ans", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er eilt und steht am andern Strand.", "tokens": ["Er", "eilt", "und", "steht", "am", "an\u00b7dern", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Rasch tritt aus einem Haus hervor", "tokens": ["Rasch", "tritt", "aus", "ei\u00b7nem", "Haus", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Edelweib, das h\u00f6hnt und lacht:", "tokens": ["Ein", "E\u00b7del\u00b7weib", ",", "das", "h\u00f6hnt", "und", "lacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ezur Amidei? Junger Thor!", "tokens": ["\u201e", "zur", "A\u00b7mi\u00b7dei", "?", "Jun\u00b7ger", "Thor", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dir war das Sch\u00f6n're zugedacht!", "tokens": ["Dir", "war", "das", "Sch\u00f6n'\u00b7re", "zu\u00b7ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nach Gottes Rathschlu\u00df ist's geschehn!", "tokens": ["Nach", "Got\u00b7tes", "Rath\u00b7schlu\u00df", "ist's", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heut wirst du \u2014 hei\u00dft's \u2014 mit ihr getraut \u2014", "tokens": ["Heut", "wirst", "du", "hei\u00dft's", "mit", "ihr", "ge\u00b7traut"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "NE", "$(", "APPR", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt sollst du die Donati sehn:", "tokens": ["Jetzt", "sollst", "du", "die", "Do\u00b7na\u00b7ti", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blick her! Vergleich' mit deiner Braut!\u201c", "tokens": ["Blick", "her", "!", "Ver\u00b7gleich'", "mit", "dei\u00b7ner", "Braut", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "$.", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Sie zerrt ein M\u00e4gdlein an das Licht,", "tokens": ["Sie", "zerrt", "ein", "M\u00e4gd\u00b7lein", "an", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es k\u00e4mpft ins dunkle Haus zur\u00fcck,", "tokens": ["Es", "k\u00e4mpft", "ins", "dunk\u00b7le", "Haus", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im jungen bangen Angesicht", "tokens": ["Im", "jun\u00b7gen", "ban\u00b7gen", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Err\u00e4th er aller Himmel Gl\u00fcck.", "tokens": ["Er\u00b7r\u00e4\u00b7th", "er", "al\u00b7ler", "Him\u00b7mel", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "\u201ehinweg! Die Amidei harrt!", "tokens": ["\u201e", "hin\u00b7weg", "!", "Die", "A\u00b7mi\u00b7dei", "harrt", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinweg! Mein Kind ist keine Dirn!", "tokens": ["Hin\u00b7weg", "!", "Mein", "Kind", "ist", "kei\u00b7ne", "Dirn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr blicket frech!\u201c Der J\u00fcngling starrt", "tokens": ["Ihr", "bli\u00b7cket", "frech", "!", "\u201c", "Der", "J\u00fcng\u00b7ling", "starrt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf die gesenkte M\u00e4dchenstirn.", "tokens": ["Auf", "die", "ge\u00b7senk\u00b7te", "M\u00e4d\u00b7chens\u00b7tirn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Wunsch ist Glut! Die Scham ist Glut!", "tokens": ["Der", "Wunsch", "ist", "Glut", "!", "Die", "Scham", "ist", "Glut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$.", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hohe Doppelflamme loht!", "tokens": ["Die", "ho\u00b7he", "Dop\u00b7pel\u00b7flam\u00b7me", "loht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er streckt die Hand. Das h\u00f6chste Gut", "tokens": ["Er", "streckt", "die", "Hand", ".", "Das", "h\u00f6chs\u00b7te", "Gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ergreift er und ergreift den Tod.", "tokens": ["Er\u00b7greift", "er", "und", "er\u00b7greift", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u201efrau, strafet mich nicht allzuschwer!", "tokens": ["\u201e", "frau", ",", "stra\u00b7fet", "mich", "nicht", "all\u00b7zu\u00b7schwer", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das s\u00fc\u00dfe Haupt! Das blonde Haar!", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "Haupt", "!", "Das", "blon\u00b7de", "Haar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gew\u00e4hret sie mir!\u201c stammelt er.", "tokens": ["Ge\u00b7w\u00e4h\u00b7ret", "sie", "mir", "!", "\u201c", "stam\u00b7melt", "er", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "$(", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eich f\u00fchre stracks sie zum Altar!\u201c", "tokens": ["\u201e", "ich", "f\u00fch\u00b7re", "stracks", "sie", "zum", "Al\u00b7tar", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Den Ring der ihm die Hand bereift,", "tokens": ["Den", "Ring", "der", "ihm", "die", "Hand", "be\u00b7reift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Amidei Trauungsring,", "tokens": ["Der", "A\u00b7mi\u00b7dei", "Trau\u00b7ungs\u00b7ring", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hat rasend er sich abgestreift", "tokens": ["Hat", "ra\u00b7send", "er", "sich", "ab\u00b7ge\u00b7streift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schleudert ihn. Da rollt er. Kling ...", "tokens": ["Und", "schleu\u00b7dert", "ihn", ".", "Da", "rollt", "er", ".", "Kling", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "PPER", "$.", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Jetzt kniet er im Capellenraum,", "tokens": ["Jetzt", "kniet", "er", "im", "Ca\u00b7pel\u00b7len\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Freveln und an Wonnen reich,", "tokens": ["An", "Fre\u00b7veln", "und", "an", "Won\u00b7nen", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Linken kniet sein s\u00fcnd'ger Traum,", "tokens": ["Zur", "Lin\u00b7ken", "kniet", "sein", "s\u00fcn\u00b7d'\u00b7ger", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie Engel sch\u00f6n, wie Todte bleich.", "tokens": ["Wie", "En\u00b7gel", "sch\u00f6n", ",", "wie", "Tod\u00b7te", "bleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJD", "$,", "PWAV", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Dem Paar zu H\u00e4upten murmelt leer", "tokens": ["Dem", "Paar", "zu", "H\u00e4up\u00b7ten", "mur\u00b7melt", "leer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schnell ein feiles Priesterwort \u2014", "tokens": ["Und", "schnell", "ein", "fei\u00b7les", "Pries\u00b7ter\u00b7wort"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201edie Rosse her! Die Rosse her!", "tokens": ["\u201e", "die", "Ros\u00b7se", "her", "!", "Die", "Ros\u00b7se", "her", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Thor hinaus! Ins Freie fort!", "tokens": ["Zum", "Thor", "hin\u00b7aus", "!", "Ins", "Frei\u00b7e", "fort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Du lieb Gesch\u00f6pf! Du bebst wie Laub!", "tokens": ["Du", "lieb", "Ge\u00b7sch\u00f6pf", "!", "Du", "bebst", "wie", "Laub", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$.", "PPER", "VVFIN", "KOKOM", "NN", "$."], "met