{"textgrid.poem.46511": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich f\u00fcrcht', es war Entweihung]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fcrcht', es war Entweihung", "tokens": ["Ich", "f\u00fcrcht'", ",", "es", "war", "Ent\u00b7wei\u00b7hung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der stillen H\u00e4uslichkeit,", "tokens": ["Der", "stil\u00b7len", "H\u00e4us\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich sie der Beschreiung", "tokens": ["Da\u00df", "ich", "sie", "der", "Be\u00b7schrei\u00b7ung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Liebloser Welt geweiht;", "tokens": ["Lieb\u00b7lo\u00b7ser", "Welt", "ge\u00b7weiht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "In manchem Lied, gedichtet", "tokens": ["In", "man\u00b7chem", "Lied", ",", "ge\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus meiner Kinderwelt,", "tokens": ["Aus", "mei\u00b7ner", "Kin\u00b7der\u00b7welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die wie ein Traum vernichtet", "tokens": ["Die", "wie", "ein", "Traum", "ver\u00b7nich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Jetzt auseinander f\u00e4llt.", "tokens": ["Jetzt", "aus\u00b7ein\u00b7an\u00b7der", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und recht als wie zum Hohne,", "tokens": ["Und", "recht", "als", "wie", "zum", "Hoh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sie zusammenbrach,", "tokens": ["Da", "sie", "zu\u00b7sam\u00b7men\u00b7brach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kommt an mit S\u00fcndenlohne", "tokens": ["Kommt", "an", "mit", "S\u00fcn\u00b7den\u00b7loh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der neuste Almanach.", "tokens": ["Der", "neus\u00b7te", "Al\u00b7ma\u00b7nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Honorar, das reiche,", "tokens": ["Das", "Ho\u00b7no\u00b7rar", ",", "das", "rei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das man dem Vater gab,", "tokens": ["Das", "man", "dem", "Va\u00b7ter", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Reicht, um der liebsten Leiche", "tokens": ["Reicht", ",", "um", "der", "liebs\u00b7ten", "Lei\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUI", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Zu kaufen grad ein Grab.", "tokens": ["Zu", "kau\u00b7fen", "grad", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und hab' ich mich vers\u00fcndigt,", "tokens": ["Und", "hab'", "ich", "mich", "ver\u00b7s\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df statt des Herzens Schlag", "tokens": ["Da\u00df", "statt", "des", "Her\u00b7zens", "Schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Harfe Schlag verk\u00fcndigt,", "tokens": ["Der", "Har\u00b7fe", "Schlag", "ver\u00b7k\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was mir am Herzen lag?", "tokens": ["Was", "mir", "am", "Her\u00b7zen", "lag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht hab' ich mich ger\u00fchmet,", "tokens": ["Nicht", "hab'", "ich", "mich", "ge\u00b7r\u00fch\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch hab' ich mich gefreut,", "tokens": ["Doch", "hab'", "ich", "mich", "ge\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mir den Pfad bebl\u00fcmet,", "tokens": ["Und", "mir", "den", "Pfad", "be\u00b7bl\u00fc\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der mir nun Dornen beut.", "tokens": ["Der", "mir", "nun", "Dor\u00b7nen", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die allgemeine S\u00fcnde", "tokens": ["Die", "all\u00b7ge\u00b7mei\u00b7ne", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Dichtkunst war es nur,", "tokens": ["Der", "Dicht\u00b7kunst", "war", "es", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu decken auf die Gr\u00fcnde", "tokens": ["Zu", "de\u00b7cken", "auf", "die", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der innersten Natur.", "tokens": ["Der", "in\u00b7ners\u00b7ten", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und wie die Lust erklungen", "tokens": ["Und", "wie", "die", "Lust", "er\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus meiner Siedelei,", "tokens": ["Aus", "mei\u00b7ner", "Sie\u00b7de\u00b7lei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sei nun das Leid gesungen,", "tokens": ["Sei", "nun", "das", "Leid", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ob es S\u00fcnde sei.", "tokens": ["Und", "ob", "es", "S\u00fcn\u00b7de", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}