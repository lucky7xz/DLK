{"textgrid.poem.46700": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich ging mit gesenktem Haupte]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging mit gesenktem Haupte,", "tokens": ["Ich", "ging", "mit", "ge\u00b7senk\u00b7tem", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und es unm\u00f6glich glaubte,", "tokens": ["Und", "es", "un\u00b7m\u00f6g\u00b7lich", "glaub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Je wieder den Blick zu drehn,", "tokens": ["Je", "wie\u00b7der", "den", "Blick", "zu", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um Sonn' und Mond zu sehn.", "tokens": ["Um", "Sonn'", "und", "Mond", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da hatt' auf meinen Wegen", "tokens": ["Da", "hatt'", "auf", "mei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Pf\u00fctzen geweint der Regen,", "tokens": ["Pf\u00fct\u00b7zen", "ge\u00b7weint", "der", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und im Vor\u00fcbergehn", "tokens": ["Und", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hab' ich darein gesehn.", "tokens": ["Hab'", "ich", "da\u00b7rein", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da spiegelten Mond und Sonne", "tokens": ["Da", "spie\u00b7gel\u00b7ten", "Mond", "und", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich wie im reinsten Bronne,", "tokens": ["Sich", "wie", "im", "reins\u00b7ten", "Bron\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ohne das Haupt zu drehn,", "tokens": ["Und", "oh\u00b7ne", "das", "Haupt", "zu", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hab' ich sie doch gesehn.", "tokens": ["Hab'", "ich", "sie", "doch", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}