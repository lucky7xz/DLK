{"textgrid.poem.56758": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie doch ein Traum so traurig stimmt,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie doch ein Traum so traurig stimmt,", "tokens": ["Wie", "doch", "ein", "Traum", "so", "trau\u00b7rig", "stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wenn unser Geist Vergangenheit", "tokens": ["wenn", "un\u00b7ser", "Geist", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und Gegenwart als Eines nimmt!", "tokens": ["und", "Ge\u00b7gen\u00b7wart", "als", "Ei\u00b7nes", "nimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sa\u00df bei dir im Brautgemach", "tokens": ["Ich", "sa\u00df", "bei", "dir", "im", "Braut\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und sprach von deinem Br\u00e4utigam,", "tokens": ["und", "sprach", "von", "dei\u00b7nem", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und wie so alles anders kam ...", "tokens": ["und", "wie", "so", "al\u00b7les", "an\u00b7ders", "kam", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und lachte hell und scherzte laut ...", "tokens": ["Und", "lach\u00b7te", "hell", "und", "scherz\u00b7te", "laut", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch endlich ward mein Sinn zu schwer \u2013", "tokens": ["Doch", "end\u00b7lich", "ward", "mein", "Sinn", "zu", "schwer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "du warst ja eines andern Braut!", "tokens": ["du", "warst", "ja", "ei\u00b7nes", "an\u00b7dern", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Garten lag vor deinem Haus,", "tokens": ["Ein", "Gar\u00b7ten", "lag", "vor", "dei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "da trug ich meinen Schmerz hinein", "tokens": ["da", "trug", "ich", "mei\u00b7nen", "Schmerz", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und weinte meine Wehmut aus.", "tokens": ["und", "wein\u00b7te", "mei\u00b7ne", "Weh\u00b7mut", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und als ich wiederkam, da schien,", "tokens": ["Und", "als", "ich", "wie\u00b7der\u00b7kam", ",", "da", "schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "als ahntest du, was mich erregt,", "tokens": ["als", "ahn\u00b7test", "du", ",", "was", "mich", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und selber wardst du sanft bewegt.", "tokens": ["und", "sel\u00b7ber", "wardst", "du", "sanft", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dein M\u00fctterlein umfing mich still,", "tokens": ["Dein", "M\u00fct\u00b7ter\u00b7lein", "um\u00b7fing", "mich", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sie wu\u00dft' um die geheime Lieb',", "tokens": ["sie", "wu\u00dft'", "um", "die", "ge\u00b7hei\u00b7me", "Lieb'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die stumm in mir ihr Wesen trieb.", "tokens": ["die", "stumm", "in", "mir", "ihr", "We\u00b7sen", "trieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wir setzten uns den Tisch umher ...", "tokens": ["Wir", "setz\u00b7ten", "uns", "den", "Tisch", "um\u00b7her", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hattest alles selbst gekocht \u2013", "tokens": ["Du", "hat\u00b7test", "al\u00b7les", "selbst", "ge\u00b7kocht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "doch mir, mir mundete nichts mehr.", "tokens": ["doch", "mir", ",", "mir", "mun\u00b7de\u00b7te", "nichts", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}