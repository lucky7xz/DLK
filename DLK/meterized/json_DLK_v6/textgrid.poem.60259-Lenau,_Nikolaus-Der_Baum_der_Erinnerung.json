{"textgrid.poem.60259": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Baum der Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, du bist es, bl\u00fctenreicher", "tokens": ["Ja", ",", "du", "bist", "es", ",", "bl\u00fc\u00b7ten\u00b7rei\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Baum, das ist dein s\u00fc\u00dfer Hauch!", "tokens": ["Baum", ",", "das", "ist", "dein", "s\u00fc\u00b7\u00dfer", "Hauch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich auch bins, nur etwas bleicher,", "tokens": ["Ich", "auch", "bins", ",", "nur", "et\u00b7was", "blei\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Etwas trauriger wohl auch.", "tokens": ["Et\u00b7was", "trau\u00b7ri\u00b7ger", "wohl", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hinter deinen Bl\u00fctenzweigen", "tokens": ["Hin\u00b7ter", "dei\u00b7nen", "Bl\u00fc\u00b7ten\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nte Nachtigallenschlag,", "tokens": ["T\u00f6n\u00b7te", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schlag", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Holde war mein eigen,", "tokens": ["Und", "die", "Hol\u00b7de", "war", "mein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die an meinem Herzen lag.", "tokens": ["Die", "an", "mei\u00b7nem", "Her\u00b7zen", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wir meinten selig beide,", "tokens": ["Und", "wir", "mein\u00b7ten", "se\u00b7lig", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich meint es bis zur Stund,", "tokens": ["Und", "ich", "meint", "es", "bis", "zur", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df so herrlich du vor Freude", "tokens": ["Da\u00df", "so", "herr\u00b7lich", "du", "vor", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchtest \u00fcber unsern Bund.", "tokens": ["Bl\u00fch\u00b7test", "\u00fc\u00b7ber", "un\u00b7sern", "Bund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Treulos hat sie mich verlassen;", "tokens": ["Treu\u00b7los", "hat", "sie", "mich", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch du bl\u00fchst wie dazumal,", "tokens": ["Doch", "du", "bl\u00fchst", "wie", "da\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kannst dich freilich nicht befassen", "tokens": ["Kannst", "dich", "frei\u00b7lich", "nicht", "be\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der fremden Liebesqual.", "tokens": ["Mit", "der", "frem\u00b7den", "Lie\u00b7bes\u00b7qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bballzulieblich scheint die Sonne,", "tokens": ["\u00bb", "all\u00b7zu\u00b7lieb\u00b7lich", "scheint", "die", "Son\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weht der linde Maienwind,", "tokens": ["Weht", "der", "lin\u00b7de", "Mai\u00b7en\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Bl\u00fchen und die Wonne", "tokens": ["Und", "das", "Bl\u00fc\u00b7hen", "und", "die", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allzubald vor\u00fcber sind!\u00ab", "tokens": ["A\u00b7llzu\u00b7bald", "vor\u00b7\u00fc\u00b7ber", "sind", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mahnend s\u00e4useln mir die Lehre", "tokens": ["Mah\u00b7nend", "s\u00e4u\u00b7seln", "mir", "die", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine frohen Bl\u00fcten zu;", "tokens": ["Dei\u00b7ne", "fro\u00b7hen", "Bl\u00fc\u00b7ten", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ungl\u00e4ubig flie\u00dft die Z\u00e4hre,", "tokens": ["Doch", "un\u00b7gl\u00e4u\u00b7big", "flie\u00dft", "die", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und mein Herz verlor die Ruh.", "tokens": ["Und", "mein", "Herz", "ver\u00b7lor", "die", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}