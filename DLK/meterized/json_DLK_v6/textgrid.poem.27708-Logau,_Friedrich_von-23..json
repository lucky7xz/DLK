{"textgrid.poem.27708": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seyd gl\u00fccklich hier und dort; seyd selig denn gepreist,", "tokens": ["Seyd", "gl\u00fcck\u00b7lich", "hier", "und", "dort", ";", "seyd", "se\u00b7lig", "denn", "ge\u00b7preist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "KON", "ADV", "$.", "VAFIN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr, die man heute Braut und Br\u00e4utigam euch heist!", "tokens": ["Ihr", ",", "die", "man", "heu\u00b7te", "Braut", "und", "Br\u00e4u\u00b7ti\u00b7gam", "euch", "heist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PIS", "ADV", "NN", "KON", "NE", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Seyd morgen Mann und Frau, seyd Eltern \u00fcbers Jahr;", "tokens": ["Seyd", "mor\u00b7gen", "Mann", "und", "Frau", ",", "seyd", "El\u00b7tern", "\u00fc\u00b7bers", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "NN", "KON", "NN", "$,", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So habt ihr denn erlangt, was zu erlangen war.", "tokens": ["So", "habt", "ihr", "denn", "er\u00b7langt", ",", "was", "zu", "er\u00b7lan\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PRELS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}