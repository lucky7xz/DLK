{"dta.poem.771": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  S Icut uri nata ligna, nata messis demeti,  \n Sic ab alta lege fati, ferre nos mortalia  \n Ad Cruces & ad flagella NVMEN, alvus projicit.  \n Eccepr\u00e6it Imperator. quid deccbit militem?", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Das Holtz das f\u00fcr dir gr\u00fcn't/ wird zu der glut erkohren. ", "tokens": ["Das", "Holtz", "das", "f\u00fcr", "dir", "gr\u00fcn't", "/", "wird", "zu", "der", "glut", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "PPER", "VVFIN", "$(", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die krumme Sichel wird auff reiffe Saat gewetzt,", "tokens": ["Die", "krum\u00b7me", "Si\u00b7chel", "wird", "auff", "reif\u00b7fe", "Saat", "ge\u00b7wetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Pre\u00df ist vor die Traub/ ein L\u00f6we wird gehetzt.", "tokens": ["Die", "Pre\u00df", "ist", "vor", "die", "Traub", "/", "ein", "L\u00f6\u00b7we", "wird", "ge\u00b7hetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$(", "ART", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Vogel hat im flug/ offt seinen flug verlohren.", "tokens": ["Der", "Vo\u00b7gel", "hat", "im", "flug", "/", "offt", "sei\u00b7nen", "flug", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "APPRART", "NN", "$(", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Man wird mit dem beding\u2019 auff diese welt gebohren/", "tokens": ["Man", "wird", "mit", "dem", "be\u00b7ding'", "auff", "die\u00b7se", "welt", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "ADJD", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu tragen was der schlu\u00df de\u00df Htmmels auffgesetzt.", "tokens": ["Zu", "tra\u00b7gen", "was", "der", "schlu\u00df", "de\u00df", "Htm\u00b7mels", "auff\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PWS", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer sterblich/ wird von dem was sterben heist/ verletzt.", "tokens": ["Wer", "sterb\u00b7lich", "/", "wird", "von", "dem", "was", "ster\u00b7ben", "heist", "/", "ver\u00b7letzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "$(", "VAFIN", "APPR", "ART", "PIS", "VVINF", "VAFIN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Todt vnd fall hat sich auff fleisch vnd stehn verschworen.", "tokens": ["Der", "Todt", "vnd", "fall", "hat", "sich", "auff", "fleisch", "vnd", "stehn", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "PRF", "APPR", "ADJD", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gott stellt sich in die Erd\u2019 auff Creutz vnd Geisseln eyn.", "tokens": ["Gott", "stellt", "sich", "in", "die", "Erd'", "auff", "Creutz", "vnd", "Geis\u00b7seln", "eyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mutter Leib gebahr in armuth Jhn zur pein.", "tokens": ["Der", "Mut\u00b7ter", "Leib", "ge\u00b7bahr", "in", "ar\u00b7muth", "Jhn", "zur", "pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NE", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er schlo\u00df in h\u00f6chster Angst das schmertzen volle zagen.", "tokens": ["Er", "schlo\u00df", "in", "h\u00f6chs\u00b7ter", "Angst", "das", "schmert\u00b7zen", "vol\u00b7le", "za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was sucht ein blosser Mensch/ wenn ", "tokens": ["Was", "sucht", "ein", "blos\u00b7ser", "Mensch", "/", "wenn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$(", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Begehrt der Kriegsman Rast/ wenn man den F\u00fcrsten", "tokens": ["Be\u00b7gehrt", "der", "Kriegs\u00b7man", "Rast", "/", "wenn", "man", "den", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NE", "$(", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schl\u00e4gt", "tokens": ["schl\u00e4gt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Der Feld Herr/ schaw/ geht vor/ was soll der Knecht nicht wa-", "tokens": ["Der", "Feld", "Herr", "/", "schaw", "/", "geht", "vor", "/", "was", "soll", "der", "Knecht", "nicht", "wa"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "VVFIN", "$(", "VVFIN", "PTKVZ", "$(", "PWS", "VMFIN", "ART", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "gen.", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}}}}}