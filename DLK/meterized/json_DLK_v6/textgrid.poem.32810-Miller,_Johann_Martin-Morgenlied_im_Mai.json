{"textgrid.poem.32810": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied im Mai", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie t\u00f6nt um mich so s\u00fc\u00dfer Schall!", "tokens": ["Wie", "t\u00f6nt", "um", "mich", "so", "s\u00fc\u00b7\u00dfer", "Schall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon weckt die fr\u00fche Nachtigall", "tokens": ["Schon", "weckt", "die", "fr\u00fc\u00b7he", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den k\u00fchlen Maienmorgen;", "tokens": ["Den", "k\u00fch\u00b7len", "Mai\u00b7en\u00b7mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und froh begr\u00fc\u00dft vom V\u00f6gelchor,", "tokens": ["Und", "froh", "be\u00b7gr\u00fc\u00dft", "vom", "V\u00f6\u00b7gelc\u00b7hor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Steigt gro\u00df und hell die Sonn' empor,", "tokens": ["Steigt", "gro\u00df", "und", "hell", "die", "Sonn'", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die kurz ihr Licht verborgen.", "tokens": ["Die", "kurz", "ihr", "Licht", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Lerch' erwacht auf frischer Au',", "tokens": ["Die", "Ler\u00b7ch'", "er\u00b7wacht", "auf", "fri\u00b7scher", "Au'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sch\u00fcttelt sich den Perlentau", "tokens": ["Und", "sch\u00fct\u00b7telt", "sich", "den", "Per\u00b7len\u00b7tau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom br\u00e4unlichen Gefieder;", "tokens": ["Vom", "br\u00e4un\u00b7li\u00b7chen", "Ge\u00b7fie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie schwebt und gl\u00e4nzet, kaum gesehn,", "tokens": ["Sie", "schwebt", "und", "gl\u00e4n\u00b7zet", ",", "kaum", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und trillert aus besonnten H\u00f6hn", "tokens": ["Und", "tril\u00b7lert", "aus", "be\u00b7sonn\u00b7ten", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Morgenlied hernieder.", "tokens": ["Ihr", "Mor\u00b7gen\u00b7lied", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohlauf, o neubelebtes Herz,", "tokens": ["Wohl\u00b7auf", ",", "o", "neu\u00b7be\u00b7leb\u00b7tes", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der S\u00e4ngerin dich himmelw\u00e4rts", "tokens": ["Der", "S\u00e4n\u00b7ge\u00b7rin", "dich", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADV"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mit Jubel nachzuschwingen!", "tokens": ["Mit", "Ju\u00b7bel", "nach\u00b7zu\u00b7schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohlauf, dem Herrn, der weit und breit", "tokens": ["Wohl\u00b7auf", ",", "dem", "Herrn", ",", "der", "weit", "und", "breit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Alliebend Lust und Leben streut,", "tokens": ["Al\u00b7lie\u00b7bend", "Lust", "und", "Le\u00b7ben", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Morgenlied zu singen!", "tokens": ["Dein", "Mor\u00b7gen\u00b7lied", "zu", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja, dich, du Segensquell, erhebt,", "tokens": ["Ja", ",", "dich", ",", "du", "Se\u00b7gens\u00b7quell", ",", "er\u00b7hebt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "$,", "PPER", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was nur ein halbes Leben lebt;", "tokens": ["Was", "nur", "ein", "hal\u00b7bes", "Le\u00b7ben", "lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und freut sich deiner G\u00fcte.", "tokens": ["Und", "freut", "sich", "dei\u00b7ner", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir zollt das niedre Veilchen Duft;", "tokens": ["Dir", "zollt", "das", "nied\u00b7re", "Veil\u00b7chen", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich preist, ges\u00e4t in blaue Luft,", "tokens": ["Dich", "preist", ",", "ge\u00b7s\u00e4t", "in", "blau\u00b7e", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Apfelbaumes Bl\u00fcte.", "tokens": ["Des", "Ap\u00b7fel\u00b7bau\u00b7mes", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der kleine Fisch im klaren See,", "tokens": ["Der", "klei\u00b7ne", "Fisch", "im", "kla\u00b7ren", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der rege Wurm auf buntem Klee,", "tokens": ["Der", "re\u00b7ge", "Wurm", "auf", "bun\u00b7tem", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dankt freudig dir sein Leben.", "tokens": ["Dankt", "freu\u00b7dig", "dir", "sein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir dankt der laute Bienenschwarm,", "tokens": ["Dir", "dankt", "der", "lau\u00b7te", "Bie\u00b7nen\u00b7schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir Schmetterlinge, frei von Harm,", "tokens": ["Dir", "Schmet\u00b7ter\u00b7lin\u00b7ge", ",", "frei", "von", "Harm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ihrer H\u00fcll' entschweben.", "tokens": ["Die", "ih\u00b7rer", "H\u00fcll'", "ent\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und hell aus tausend Kehlen schallt", "tokens": ["Und", "hell", "aus", "tau\u00b7send", "Keh\u00b7len", "schallt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der V\u00f6gel Lied vom Birkenwald", "tokens": ["Der", "V\u00f6\u00b7gel", "Lied", "vom", "Bir\u00b7ken\u00b7wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu deines Thrones Stufen.", "tokens": ["Zu", "dei\u00b7nes", "Thro\u00b7nes", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch deine Hand ges\u00e4ttigt, ruht", "tokens": ["Durch", "dei\u00b7ne", "Hand", "ge\u00b7s\u00e4t\u00b7tigt", ",", "ruht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im hohen Nest des Raben Brut,", "tokens": ["Im", "ho\u00b7hen", "Nest", "des", "Ra\u00b7ben", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die fr\u00fch dich angerufen.", "tokens": ["Die", "fr\u00fch", "dich", "an\u00b7ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Wild im gr\u00fcnen Dickicht preist", "tokens": ["Das", "Wild", "im", "gr\u00fc\u00b7nen", "Di\u00b7ckicht", "preist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, der's in dunkler Nacht gespeist,", "tokens": ["Dich", ",", "der's", "in", "dunk\u00b7ler", "Nacht", "ge\u00b7speist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und legt aufs Moos sich nieder.", "tokens": ["Und", "legt", "aufs", "Moos", "sich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Froh kehret Ro\u00df und Rind und Schaf,", "tokens": ["Froh", "keh\u00b7ret", "Ro\u00df", "und", "Rind", "und", "Schaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Froh kehrt der Mensch, erquickt vom Schlaf,", "tokens": ["Froh", "kehrt", "der", "Mensch", ",", "er\u00b7quickt", "vom", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Weid' und Arbeit wieder.", "tokens": ["Zu", "Weid'", "und", "Ar\u00b7beit", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Auch ich, o Sch\u00f6pfer, jauchz' empor", "tokens": ["Auch", "ich", ",", "o", "Sch\u00f6p\u00b7fer", ",", "jauch\u00b7z'", "em\u00b7por"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In deiner Sch\u00f6pfung gro\u00dfem Chor,", "tokens": ["In", "dei\u00b7ner", "Sch\u00f6p\u00b7fung", "gro\u00b7\u00dfem", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Kraft zum Werk erquicket;", "tokens": ["Mit", "Kraft", "zum", "Werk", "er\u00b7quic\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da rings mein Auge, hell und frisch,", "tokens": ["Da", "rings", "mein", "Au\u00b7ge", ",", "hell", "und", "frisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ins tausendfache Lustgemisch", "tokens": ["Ins", "tau\u00b7send\u00b7fa\u00b7che", "Lust\u00b7ge\u00b7misch"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Belebter Wesen blicket!", "tokens": ["Be\u00b7leb\u00b7ter", "We\u00b7sen", "bli\u00b7cket", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O Gott, wie du, von Lieb' erf\u00fcllt,", "tokens": ["O", "Gott", ",", "wie", "du", ",", "von", "Lieb'", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df immer mich, dein Ebenbild,", "tokens": ["La\u00df", "im\u00b7mer", "mich", ",", "dein", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durchs Erdenleben wallen;", "tokens": ["Durchs", "Er\u00b7den\u00b7le\u00b7ben", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wann dies Leben einst verbl\u00fcht,", "tokens": ["Und", "wann", "dies", "Le\u00b7ben", "einst", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PDS", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vollkommner dir mein Jubellied", "tokens": ["Voll\u00b7komm\u00b7ner", "dir", "mein", "Ju\u00b7bel\u00b7lied"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Himmelschor erschallen!", "tokens": ["Im", "Him\u00b7mel\u00b7schor", "er\u00b7schal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}