{"textgrid.poem.37380": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Schadenfrohe", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Dornstrauch stand im Wiesental", "tokens": ["Ein", "Dorn\u00b7strauch", "stand", "im", "Wie\u00b7sen\u00b7tal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An einer Stiege, welche schmal,", "tokens": ["An", "ei\u00b7ner", "Stie\u00b7ge", ",", "wel\u00b7che", "schmal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ging vor\u00fcber irgendwer,", "tokens": ["Und", "ging", "vor\u00b7\u00fc\u00b7ber", "ir\u00b7gend\u00b7wer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den griff er an und kratzte er.", "tokens": ["Den", "griff", "er", "an", "und", "kratz\u00b7te", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein L\u00e4mmlein kam dahergehupft.", "tokens": ["Ein", "L\u00e4mm\u00b7lein", "kam", "da\u00b7her\u00b7ge\u00b7hupft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das hat er ebenfalls gerupft.", "tokens": ["Das", "hat", "er", "e\u00b7ben\u00b7falls", "ge\u00b7rupft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es sieht ihn traurig an und spricht:", "tokens": ["Es", "sieht", "ihn", "trau\u00b7rig", "an", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du brauchst doch meine Wolle nicht,", "tokens": ["Du", "brauchst", "doch", "mei\u00b7ne", "Wol\u00b7le", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und niemals tat ich dir ein Leid.", "tokens": ["Und", "nie\u00b7mals", "tat", "ich", "dir", "ein", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Weshalb zerrupfst du denn mein Kleid?", "tokens": ["We\u00b7shalb", "zer\u00b7rupfst", "du", "denn", "mein", "Kleid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es tut mir weh und ist auch schad.", "tokens": ["Es", "tut", "mir", "weh", "und", "ist", "auch", "schad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ei, rief der Freche, darum grad.", "tokens": ["Ei", ",", "rief", "der", "Fre\u00b7che", ",", "da\u00b7rum", "grad", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "PAV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}