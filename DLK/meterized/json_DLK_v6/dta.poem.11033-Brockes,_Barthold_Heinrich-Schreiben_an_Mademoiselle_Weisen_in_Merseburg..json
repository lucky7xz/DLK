{"dta.poem.11033": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schreiben  \n an  Mademoiselle  Weisen  \n in Merseburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Bey dieser H\u00e4nde Werke, das gar zu ungemein;", "tokens": ["Bey", "die\u00b7ser", "H\u00e4n\u00b7de", "Wer\u00b7ke", ",", "das", "gar", "zu", "un\u00b7ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,", "PRELS", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was ist von ihrem Brief, und ihrem Vers zu sagen?", "tokens": ["Was", "ist", "von", "ih\u00b7rem", "Brief", ",", "und", "ih\u00b7rem", "Vers", "zu", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Richts anders, als da\u00df sich ihr Geist,", "tokens": ["Richts", "an\u00b7ders", ",", "als", "da\u00df", "sich", "ihr", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOKOM", "KOUS", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Worten ja so k\u00fcnstlich weist,", "tokens": ["Mit", "Wor\u00b7ten", "ja", "so", "k\u00fcnst\u00b7lich", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wie, mit F\u00e4den, ihre Hand,", "tokens": ["Als", "wie", ",", "mit", "F\u00e4\u00b7den", ",", "ih\u00b7re", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "$,", "APPR", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So da\u00df von ihr mit h\u00f6chstem Rechte,", "tokens": ["So", "da\u00df", "von", "ihr", "mit", "h\u00f6chs\u00b7tem", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Man recht zum Wunder sagen m\u00f6chte:", "tokens": ["Man", "recht", "zum", "Wun\u00b7der", "sa\u00b7gen", "m\u00f6ch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Jhr ganzes Wesen sey Verstand.", "tokens": ["Ihr", "gan\u00b7zes", "We\u00b7sen", "sey", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}