{"dta.poem.13083": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas Kunst und Tugenden durch ihren Flei\u00df verm\u00f6gen,", "tokens": ["\u201e", "was", "Kunst", "und", "Tu\u00b7gen\u00b7den", "durch", "ih\u00b7ren", "Flei\u00df", "ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edas schwebt gemeiniglich auf Denck- und Ehren-B\u00f6gen.", "tokens": ["\u201e", "das", "schwebt", "ge\u00b7mei\u00b7nig\u00b7lich", "auf", "Den\u00b7ck", "und", "Eh\u00b7ren\u00b7B\u00f6\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "ADV", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "145\u201dWas wi\u00dft ihr Kostbarers, als selbst das Welt-Geb\u00e4u?", "tokens": ["\"", "Was", "wi\u00dft", "ihr", "Kost\u00b7ba\u00b7rers", ",", "als", "selbst", "das", "Welt\u00b7Ge\u00b7b\u00e4u", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewer hat es aufgef\u00fchrt? sagt, wer der Meister sey.", "tokens": ["\u201e", "wer", "hat", "es", "auf\u00b7ge\u00b7f\u00fchrt", "?", "sagt", ",", "wer", "der", "Meis\u00b7ter", "sey", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "VVPP", "$.", "VVFIN", "$,", "PWS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewer ist, der das Geschlecht der Menschen auf den Wellen", "tokens": ["\u201e", "wer", "ist", ",", "der", "das", "Ge\u00b7schlecht", "der", "Men\u00b7schen", "auf", "den", "Wel\u00b7len"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "$,", "PRELS", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ein einem Bau verwahrt, es aus Gefahr zu stellen?", "tokens": ["\u201e", "in", "ei\u00b7nem", "Bau", "ver\u00b7wahrt", ",", "es", "aus", "Ge\u00b7fahr", "zu", "stel\u00b7len", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVPP", "$,", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df die Stadt Wienn von dem\nsechsten Jahr-hundert an/ bi\u00df um das\nJahr 1100. verst\u00f6rt/ w\u00fcst und \u00f6d in\n\nDornen und Geh\u00e4cken samt ihrem Nah-\nmen Fabiana vergraben gelegen sey/ er-\nz\u00e4hlt Wolfg. Lazius l. 2. c. 2.", "tokens": ["Da\u00df", "die", "Stadt", "Wi\u00b7enn", "von", "dem", "sechs\u00b7ten", "Jahr\u00b7hun\u00b7dert", "an", "/", "bi\u00df", "um", "das", "Jahr", "1100", ".", "ver\u00b7st\u00f6rt", "/", "w\u00fcst", "und", "\u00f6d", "in", "Dor\u00b7nen", "und", "Ge\u00b7h\u00e4\u00b7cken", "samt", "ih\u00b7rem", "Nah", "men", "Fa\u00b7bi\u00b7a\u00b7na", "ver\u00b7gra\u00b7ben", "ge\u00b7le\u00b7gen", "sey", "/", "er", "z\u00e4hlt", "Wolfg", ".", "La\u00b7zius", "l.", "2.", "c.", "2."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "number", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "abbreviation", "ordinal", "abbreviation", "ordinal"], "pos": ["KOUS", "ART", "NN", "NE", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$(", "ADV", "APPR", "ART", "NN", "CARD", "$.", "VVPP", "$(", "VVFIN", "KON", "ADJD", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "TRUNC", "NN", "NE", "VVPP", "VVPP", "VAFIN", "$(", "TRUNC", "VVFIN", "NE", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "---+---+--+-+---+-+--+-+---+-+--+-+--+-+-+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "\u201eich brachte dazumahl ein solch\u2019 Geb\u00e4u zu Stand,", "tokens": ["\u201e", "ich", "brach\u00b7te", "da\u00b7zu\u00b7mahl", "ein", "solch'", "Ge\u00b7b\u00e4u", "zu", "Stand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ART", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "150\u201dIn dessen Fachungen sich alle Welt befand.", "tokens": ["\"", "In", "des\u00b7sen", "Fac\u00b7hun\u00b7gen", "sich", "al\u00b7le", "Welt", "be\u00b7fand", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELAT", "NN", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edie Allmacht hatte mich dergleichen Kunst gelehret,", "tokens": ["\u201e", "die", "All\u00b7macht", "hat\u00b7te", "mich", "derg\u00b7lei\u00b7chen", "Kunst", "ge\u00b7leh\u00b7ret", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "PIS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eund also mich zum Haupt der Kunst-Gemeind\u2019 erkl\u00e4ret.", "tokens": ["\u201e", "und", "al\u00b7so", "mich", "zum", "Haupt", "der", "Kunst\u00b7Ge\u00b7meind'", "er\u00b7kl\u00e4\u00b7ret", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eweil nun der Himmel selbst mir diesen Vorzug gab,", "tokens": ["\u201e", "weil", "nun", "der", "Him\u00b7mel", "selbst", "mir", "die\u00b7sen", "Vor\u00b7zug", "gab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "ART", "NN", "ADV", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eso wei\u00df ich nicht, was ich noch mehr zu reden hab.", "tokens": ["\u201e", "so", "wei\u00df", "ich", "nicht", ",", "was", "ich", "noch", "mehr", "zu", "re\u00b7den", "hab", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "155\u201dJhr werdet also mir, gef\u00e4llt es euch, befehlen,", "tokens": ["\"", "Ihr", "wer\u00b7det", "al\u00b7so", "mir", ",", "ge\u00b7f\u00e4llt", "es", "euch", ",", "be\u00b7feh\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PPER", "$,", "VVFIN", "PPER", "PPER", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201ewas ich vor einen Plan zum bauen soll erw\u00e4hlen.", "tokens": ["\u201e", "was", "ich", "vor", "ei\u00b7nen", "Plan", "zum", "bau\u00b7en", "soll", "er\u00b7w\u00e4h\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "APPR", "ART", "NN", "APPRART", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}