{"textgrid.poem.61887": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf den dunkelgr\u00fcnen See", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den dunkelgr\u00fcnen See", "tokens": ["Auf", "den", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "See"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut vom Berge die Kapelle,", "tokens": ["Schaut", "vom", "Ber\u00b7ge", "die", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fernher gl\u00e4nzt der Alpen Schnee", "tokens": ["Fern\u00b7her", "gl\u00e4nzt", "der", "Al\u00b7pen", "Schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In entw\u00f6lkter Mittagshelle.", "tokens": ["In", "ent\u00b7w\u00f6lk\u00b7ter", "Mit\u00b7tags\u00b7hel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie lieb' ich diesen Ort,", "tokens": ["O", "wie", "lieb'", "ich", "die\u00b7sen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Welle Schaum im Grunde,", "tokens": ["Wo", "der", "Wel\u00b7le", "Schaum", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die stillen Riesen dort", "tokens": ["Wo", "die", "stil\u00b7len", "Rie\u00b7sen", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeugen waren unserm Bunde!", "tokens": ["Zeu\u00b7gen", "wa\u00b7ren", "un\u00b7serm", "Bun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ganz wie damals braust zu mir", "tokens": ["Ganz", "wie", "da\u00b7mals", "braust", "zu", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADV", "VVFIN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dumpf herauf der Schlag der Fluten,", "tokens": ["Dumpf", "her\u00b7auf", "der", "Schlag", "der", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wir weltvergessen hier", "tokens": ["Als", "wir", "welt\u00b7ver\u00b7ges\u00b7sen", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hand in Hand am Kirchlein ruhten,", "tokens": ["Hand", "in", "Hand", "am", "Kirch\u00b7lein", "ruh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Als dein Auge feuchten Blicks", "tokens": ["Als", "dein", "Au\u00b7ge", "feuch\u00b7ten", "Blicks"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Selig nah in meines schaute,", "tokens": ["Se\u00b7lig", "nah", "in", "mei\u00b7nes", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Himmel alles Gl\u00fccks", "tokens": ["Und", "ein", "Him\u00b7mel", "al\u00b7les", "Gl\u00fccks"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir aus seinen Tiefen blaute.", "tokens": ["Mir", "aus", "sei\u00b7nen", "Tie\u00b7fen", "blau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Heut, Geliebte, bist du weit,", "tokens": ["Heut", ",", "Ge\u00b7lieb\u00b7te", ",", "bist", "du", "weit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch du bist mir nicht entschwunden,", "tokens": ["Doch", "du", "bist", "mir", "nicht", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer scheiden Raum und Zeit", "tokens": ["Nim\u00b7mer", "schei\u00b7den", "Raum", "und", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herzen, die sich so gefunden.", "tokens": ["Her\u00b7zen", ",", "die", "sich", "so", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ob zum fernsten Lorbeerhain", "tokens": ["Ob", "zum", "ferns\u00b7ten", "Lor\u00b7beer\u00b7hain"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fcdw\u00e4rts du die Schritte lenkest,", "tokens": ["S\u00fcd\u00b7w\u00e4rts", "du", "die", "Schrit\u00b7te", "len\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcndlich, wie ich denke dein,", "tokens": ["St\u00fcnd\u00b7lich", ",", "wie", "ich", "den\u00b7ke", "dein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "VVFIN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df ich, da\u00df du mein gedenkest.", "tokens": ["Wei\u00df", "ich", ",", "da\u00df", "du", "mein", "ge\u00b7den\u00b7kest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Und aus der Erinnrung Lust", "tokens": ["Und", "aus", "der", "E\u00b7rinn\u00b7rung", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Pocht mein Herz mit frohen Schl\u00e4gen,", "tokens": ["Pocht", "mein", "Herz", "mit", "fro\u00b7hen", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner treuen Huld bewu\u00dft,", "tokens": ["Dei\u00b7ner", "treu\u00b7en", "Huld", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon dem Wiedersehn entgegen.", "tokens": ["Schon", "dem", "Wie\u00b7der\u00b7sehn", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}