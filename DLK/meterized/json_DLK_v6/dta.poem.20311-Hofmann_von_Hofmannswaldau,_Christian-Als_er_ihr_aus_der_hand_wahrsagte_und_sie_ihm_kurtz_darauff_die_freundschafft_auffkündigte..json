{"dta.poem.20311": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Als er ihr aus der hand wahrsagte/  \n und sie ihm kurtz darauff die freundschafft  \n auffk\u00fcndigte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was sucht ihr sterblichen doch l\u00e4nger zu ergr\u00fcnden/", "tokens": ["Was", "sucht", "ihr", "sterb\u00b7li\u00b7chen", "doch", "l\u00e4n\u00b7ger", "zu", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob elend oder lust in euren h\u00e4nden bl\u00fcht?", "tokens": ["Ob", "e\u00b7lend", "o\u00b7der", "lust", "in", "eu\u00b7ren", "h\u00e4n\u00b7den", "bl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich habe neulich mich noch allererst bem\u00fcht", "tokens": ["Ich", "ha\u00b7be", "neu\u00b7lich", "mich", "noch", "al\u00b7le\u00b7rerst", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Phillis perlen-hand mein gutes gl\u00fcck zu finden.", "tokens": ["In", "Phil\u00b7lis", "per\u00b7len\u00b7hand", "mein", "gu\u00b7tes", "gl\u00fcck", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich \u00e4rmster sang mir auch von tausend sussen blicken/", "tokens": ["Ich", "\u00e4rms\u00b7ter", "sang", "mir", "auch", "von", "tau\u00b7send", "sus\u00b7sen", "bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "CARD", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und prophezeyte nichts als lauter sonnenschein.", "tokens": ["Und", "pro\u00b7phe\u00b7zey\u00b7te", "nichts", "als", "lau\u00b7ter", "son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch die erf\u00fcllung traff nicht mit der deutung ein:", "tokens": ["Doch", "die", "er\u00b7f\u00fcl\u00b7lung", "traff", "nicht", "mit", "der", "deu\u00b7tung", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn sie erz\u00fcrnte sich/ und zeigte mir den r\u00fccken.", "tokens": ["Denn", "sie", "er\u00b7z\u00fcrn\u00b7te", "sich", "/", "und", "zeig\u00b7te", "mir", "den", "r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "$(", "KON", "VVFIN", "PPER", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Betriegerische hand! die rosen uns verspricht/", "tokens": ["Be\u00b7trie\u00b7ge\u00b7ri\u00b7sche", "hand", "!", "die", "ro\u00b7sen", "uns", "ver\u00b7spricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "ADJA", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und doch am Ende nichts als dornen uns gew\u00e4hret/", "tokens": ["Und", "doch", "am", "En\u00b7de", "nichts", "als", "dor\u00b7nen", "uns", "ge\u00b7w\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PIS", "KOKOM", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was hab ich dir gethan/ da\u00df du dich so verkehret?", "tokens": ["Was", "hab", "ich", "dir", "ge\u00b7than", "/", "da\u00df", "du", "dich", "so", "ver\u00b7keh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "VVPP", "$(", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch/ Phillis hand ist gut/ ihr hertze taugt nur nicht.", "tokens": ["Doch", "/", "Phil\u00b7lis", "hand", "ist", "gut", "/", "ihr", "hert\u00b7ze", "taugt", "nur", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NE", "NN", "VAFIN", "ADJD", "$(", "PPOSAT", "NN", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}