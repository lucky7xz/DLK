{"textgrid.poem.53440": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[herr Gott, meine Seele bringet]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Gott, meine Seele bringet", "tokens": ["Herr", "Gott", ",", "mei\u00b7ne", "See\u00b7le", "brin\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir zum Opffer Prei\u00df vnd Danck:", "tokens": ["Dir", "zum", "Opf\u00b7fer", "Prei\u00df", "vnd", "Danck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Zung in Frewden singet", "tokens": ["Mei\u00b7ne", "Zung", "in", "Frew\u00b7den", "sin\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen newen Lobgesang.", "tokens": ["Ei\u00b7nen", "ne\u00b7wen", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Vberall bey allen Leuten", "tokens": ["Vbe\u00b7rall", "bey", "al\u00b7len", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wil ich deinen Rhum au\u00dfbreiten", "tokens": ["Wil", "ich", "dei\u00b7nen", "Rhum", "au\u00df\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Itzo und mein lebenlang.", "tokens": ["It\u00b7zo", "und", "mein", "le\u00b7ben\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPOSAT", "ADJD", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Zwar du liesset mich empfinden", "tokens": ["Zwar", "du", "lies\u00b7set", "mich", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Zornes schwere Macht:", "tokens": ["Dei\u00b7nes", "Zor\u00b7nes", "schwe\u00b7re", "Macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welchen ich mit meinen S\u00fcnden", "tokens": ["Wel\u00b7chen", "ich", "mit", "mei\u00b7nen", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hatte \u00fcber mich gebracht.", "tokens": ["Hat\u00b7te", "\u00fc\u00b7ber", "mich", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer die S\u00fcnde nicht wil meiden,", "tokens": ["Wer", "die", "S\u00fcn\u00b7de", "nicht", "wil", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df viel schwere Plagen leiden,", "tokens": ["Mu\u00df", "viel", "schwe\u00b7re", "Pla\u00b7gen", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn dein Eyfer recht erwacht.", "tokens": ["Wenn", "dein", "Ey\u00b7fer", "recht", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist aber auch sehr g\u00fctig,", "tokens": ["Du", "bist", "a\u00b7ber", "auch", "sehr", "g\u00fc\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man sich zu dir bekehrt,", "tokens": ["Wenn", "man", "sich", "zu", "dir", "be\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd von Hertzen grund dem\u00fctig", "tokens": ["Vnd", "von", "Hert\u00b7zen", "grund", "de\u00b7m\u00fc\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine H\u00fclff vnn Gnad begehrt,", "tokens": ["Dei\u00b7ne", "H\u00fclff", "vnn", "Gnad", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird im Augenblick geendet", "tokens": ["Wird", "im", "Au\u00b7gen\u00b7blick", "ge\u00b7en\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Eyfer, vnd gewendet", "tokens": ["Al\u00b7ler", "Ey\u00b7fer", ",", "vnd", "ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In Trost, der viel Frewd beschert.", "tokens": ["In", "Trost", ",", "der", "viel", "Frewd", "be\u00b7schert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Drum mein Hertz ohn Furchten lebet", "tokens": ["Drum", "mein", "Hertz", "ohn", "Furch\u00b7ten", "le\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In gewisser Sicherheit,", "tokens": ["In", "ge\u00b7wis\u00b7ser", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil es in Gott selbsten schwebet", "tokens": ["Weil", "es", "in", "Gott", "selbs\u00b7ten", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJA", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der mein Heyl bleibt allezeit,", "tokens": ["Der", "mein", "Heyl", "bleibt", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich mit seinen Fl\u00fcgeln decket,", "tokens": ["Mich", "mit", "sei\u00b7nen", "Fl\u00fc\u00b7geln", "de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn mein Feind die Hand au\u00dfstrecket", "tokens": ["Wenn", "mein", "Feind", "die", "Hand", "au\u00df\u00b7str\u00b7e\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wieder mich in schwerem streit.", "tokens": ["Wie\u00b7der", "mich", "in", "schwe\u00b7rem", "streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In mir hilfft des Herren St\u00e4rke,", "tokens": ["In", "mir", "hilfft", "des", "Her\u00b7ren", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich alles \u00fcberwind,", "tokens": ["Da\u00df", "ich", "al\u00b7les", "\u00fc\u00b7berw\u00b7ind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Feinde List vnd Wercke", "tokens": ["Al\u00b7ler", "Fein\u00b7de", "List", "vnd", "Wer\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Machet sie zu nicht geschwind;", "tokens": ["Ma\u00b7chet", "sie", "zu", "nicht", "ge\u00b7schwind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn mich ein gantz Heer bekrieget,", "tokens": ["Wenn", "mich", "ein", "gantz", "Heer", "be\u00b7krie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hab ich dennoch stets gesieget,", "tokens": ["Hab", "ich", "den\u00b7noch", "stets", "ge\u00b7sie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil ich bey Gott Zuflucht find.", "tokens": ["Weil", "ich", "bey", "Gott", "Zu\u00b7flucht", "find", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Solte denn mein hertz nicht bringen", "tokens": ["Sol\u00b7te", "denn", "mein", "hertz", "nicht", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm zum Opffer Prei\u00df und Danck?", "tokens": ["Ihm", "zum", "Opf\u00b7fer", "Prei\u00df", "und", "Danck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Solt Ihm nicht die Zunge singen", "tokens": ["Solt", "Ihm", "nicht", "die", "Zun\u00b7ge", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Psalm und Lobgesang?", "tokens": ["Ei\u00b7nen", "Psalm", "und", "Lob\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja ich wil bey allen Leuten", "tokens": ["Ja", "ich", "wil", "bey", "al\u00b7len", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VMFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deinen Ruhm, mein Heyl, au\u00dfbreiten", "tokens": ["Dei\u00b7nen", "Ruhm", ",", "mein", "Heyl", ",", "au\u00df\u00b7brei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Itzo vnd mein lebenlang.", "tokens": ["It\u00b7zo", "vnd", "mein", "le\u00b7ben\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}