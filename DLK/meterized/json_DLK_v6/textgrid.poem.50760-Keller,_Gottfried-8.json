{"textgrid.poem.50760": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Leib meiner Dame, du k\u00f6stlicher Schrein,", "tokens": ["O", "Leib", "mei\u00b7ner", "Da\u00b7me", ",", "du", "k\u00f6st\u00b7li\u00b7cher", "Schrein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wo Gott seine k\u00f6stlichste Perl' legt' hinein!", "tokens": ["Wo", "Gott", "sei\u00b7ne", "k\u00f6st\u00b7lichs\u00b7te", "Perl'", "legt'", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun ruhst du und schl\u00e4fst du, doch in dir erstrahlt", "tokens": ["Nun", "ruhst", "du", "und", "schl\u00e4fst", "du", ",", "doch", "in", "dir", "er\u00b7strahlt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die tr\u00e4umende Perle im sonnigsten Schein!", "tokens": ["Die", "tr\u00e4u\u00b7men\u00b7de", "Per\u00b7le", "im", "son\u00b7nigs\u00b7ten", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Den zartesten Liliengeist bergender Kelch,", "tokens": ["Den", "zar\u00b7tes\u00b7ten", "Li\u00b7li\u00b7en\u00b7geist", "ber\u00b7gen\u00b7der", "Kelch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Des reinsten Gedankens still bl\u00fchendes Sein:", "tokens": ["Des", "reins\u00b7ten", "Ge\u00b7dan\u00b7kens", "still", "bl\u00fc\u00b7hen\u00b7des", "Sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "O w\u00e4r ich, du Kleinod, dein Schatzmeister nur,", "tokens": ["O", "w\u00e4r", "ich", ",", "du", "Klei\u00b7nod", ",", "dein", "Schatz\u00b7meis\u00b7ter", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "PPER", "NN", "$,", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "D\u00fcrft ich mich, du Blume, zum G\u00e4rtner dir weihn!", "tokens": ["D\u00fcrft", "ich", "mich", ",", "du", "Blu\u00b7me", ",", "zum", "G\u00e4rt\u00b7ner", "dir", "weihn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPER", "NN", "$,", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Mit Liebe umschlie\u00dfen dich innig und fest,", "tokens": ["Mit", "Lie\u00b7be", "um\u00b7schlie\u00b7\u00dfen", "dich", "in\u00b7nig", "und", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "KON", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Wie treu sch\u00fctzend Gold einen funkelnden Stein!", "tokens": ["Wie", "treu", "sch\u00fct\u00b7zend", "Gold", "ei\u00b7nen", "fun\u00b7keln\u00b7den", "Stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Dann tr\u00fcg ich die Erde, den Himmel, die Welt", "tokens": ["Dann", "tr\u00fcg", "ich", "die", "Er\u00b7de", ",", "den", "Him\u00b7mel", ",", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "Beisammen als Herzschmuck, gel\u00e4utert und rein;", "tokens": ["Bei\u00b7sam\u00b7men", "als", "Herz\u00b7schmuck", ",", "ge\u00b7l\u00e4u\u00b7tert", "und", "rein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "$,", "VVPP", "KON", "ADJD", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Dann tr\u00e4nk ich die klareste Seele aus dir,", "tokens": ["Dann", "tr\u00e4nk", "ich", "die", "kla\u00b7res\u00b7te", "See\u00b7le", "aus", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.14": {"text": "Du zierlicher Becher, wie perlenden Wein!", "tokens": ["Du", "zier\u00b7li\u00b7cher", "Be\u00b7cher", ",", "wie", "per\u00b7len\u00b7den", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.15": {"text": "Schlaf sanft und schlaf selig, du k\u00f6stlicher Leib,", "tokens": ["Schlaf", "sanft", "und", "schlaf", "se\u00b7lig", ",", "du", "k\u00f6st\u00b7li\u00b7cher", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "ADJD", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Indessen ist tr\u00e4umend die Seele ja mein!", "tokens": ["In\u00b7des\u00b7sen", "ist", "tr\u00e4u\u00b7mend", "die", "See\u00b7le", "ja", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "ADV", "PPOSAT", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}