{"textgrid.poem.53252": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich steh in Angst vnd Pein,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich steh in Angst vnd Pein,", "tokens": ["Ich", "steh", "in", "Angst", "vnd", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vnd wei\u00df nicht au\u00df nicht ein,", "tokens": ["Vnd", "wei\u00df", "nicht", "au\u00df", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sinnen Krafft sinckt nieder:", "tokens": ["Der", "Sin\u00b7nen", "Krafft", "sinckt", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Hertz wil mir zergehn,", "tokens": ["Mein", "Hertz", "wil", "mir", "zer\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Zunge bleibet stehn,", "tokens": ["Die", "Zun\u00b7ge", "blei\u00b7bet", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mir starren alle Glieder,", "tokens": ["Mir", "star\u00b7ren", "al\u00b7le", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So offt als die Gewalt", "tokens": ["So", "offt", "als", "die", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Stimm' in mir erschallt:", "tokens": ["Der", "Stimm'", "in", "mir", "er\u00b7schallt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Todten in der Erden,", "tokens": ["Ihr", "Tod\u00b7ten", "in", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steht auf, vnd seumt euch nicht,", "tokens": ["Steht", "auf", ",", "vnd", "seumt", "euch", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kompt vor das Hals-Gericht", "tokens": ["Kompt", "vor", "das", "Hals\u00b7Ge\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So jetzt gehegt sol werden!", "tokens": ["So", "jetzt", "ge\u00b7hegt", "sol", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach Gott! kein harter Schlag", "tokens": ["Ach", "Gott", "!", "kein", "har\u00b7ter", "Schlag"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des rawen Wetters mag", "tokens": ["Des", "ra\u00b7wen", "Wet\u00b7ters", "mag"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Felsen so ersch\u00fcttern,", "tokens": ["Die", "Fel\u00b7sen", "so", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als dieser Thon mein Hertz:", "tokens": ["Als", "die\u00b7ser", "Thon", "mein", "Hertz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd wer' ich Stahl vnd Ertz,", "tokens": ["Vnd", "wer'", "ich", "Stahl", "vnd", "Ertz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich m\u00fcst' hief\u00fcr erzittern.", "tokens": ["Ich", "m\u00fcst'", "hie\u00b7f\u00fcr", "er\u00b7zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich ess', ich wach' vnd ruh',", "tokens": ["Ich", "ess'", ",", "ich", "wach'", "vnd", "ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich thu auch was ich thu,", "tokens": ["Ich", "thu", "auch", "was", "ich", "thu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sey, wo ich wil, zu sp\u00fcren,", "tokens": ["Sey", ",", "wo", "ich", "wil", ",", "zu", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So m\u00fcssen fort vnd fort", "tokens": ["So", "m\u00fcs\u00b7sen", "fort", "vnd", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir diese Donner-Wort", "tokens": ["Mir", "die\u00b7se", "Don\u00b7ner\u00b7Wort"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hertz, Geist vnd Seele r\u00fchren.", "tokens": ["Hertz", ",", "Geist", "vnd", "See\u00b7le", "r\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denn werd' ich nicht gewar,", "tokens": ["Denn", "werd'", "ich", "nicht", "ge\u00b7war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie in so grosser Schaar", "tokens": ["Wie", "in", "so", "gros\u00b7ser", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Menschen stets verbleichen?", "tokens": ["Die", "Men\u00b7schen", "stets", "ver\u00b7blei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den rafft die Pest, den Glut,", "tokens": ["Den", "rafft", "die", "Pest", ",", "den", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den schickt die wilde Flut", "tokens": ["Den", "schickt", "die", "wil\u00b7de", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hinunter zu den Leichen.", "tokens": ["Hin\u00b7un\u00b7ter", "zu", "den", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Reyh kompt auch an mich,", "tokens": ["Die", "Reyh", "kompt", "auch", "an", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Ende f\u00f6rdert sich,", "tokens": ["Das", "En\u00b7de", "f\u00f6r\u00b7dert", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das keinen kan begnaden,", "tokens": ["Das", "kei\u00b7nen", "kan", "be\u00b7gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Todt ist vor der Th\u00fcr,", "tokens": ["Der", "Todt", "ist", "vor", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd klopffet an bey mir,", "tokens": ["Vnd", "klopf\u00b7fet", "an", "bey", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mich schon dorthin zu laden.", "tokens": ["Mich", "schon", "dor\u00b7thin", "zu", "la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wen flieh' ich doch nun an?", "tokens": ["Wen", "flieh'", "ich", "doch", "nun", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wer ist der helffen kan?", "tokens": ["Wer", "ist", "der", "helf\u00b7fen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer wird das Wort mir sprechen?", "tokens": ["Wer", "wird", "das", "Wort", "mir", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier hilfft nicht Gut nicht Geldt,", "tokens": ["Hier", "hilfft", "nicht", "Gut", "nicht", "Geldt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADJD", "PTKNEG", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der den Gerichts-Tag helt", "tokens": ["Der", "den", "Ge\u00b7richts\u00b7Tag", "helt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "L\u00e4sst gantz sich nicht bestechen;", "tokens": ["L\u00e4sst", "gantz", "sich", "nicht", "be\u00b7ste\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hat nicht auff Purpur acht,", "tokens": ["Hat", "nicht", "auff", "Pur\u00b7pur", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "NN", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nicht auff der Krohnen Pracht,", "tokens": ["Nicht", "auff", "der", "Kroh\u00b7nen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch auff Gewalt vnd Tittel,", "tokens": ["Noch", "auff", "Ge\u00b7walt", "vnd", "Tit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Begehrt nicht zu verstehn,", "tokens": ["Be\u00b7gehrt", "nicht", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df die in Seide gehn,", "tokens": ["Da\u00df", "die", "in", "Sei\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vnd die im groben Kittel.", "tokens": ["Vnd", "die", "im", "gro\u00b7ben", "Kit\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ach komm, Herr Jesu Christ,", "tokens": ["Ach", "komm", ",", "Herr", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "NN", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Komm, dieses einig ist,", "tokens": ["Komm", ",", "die\u00b7ses", "ei\u00b7nig", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDAT", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Worumb du Mensch geboren!", "tokens": ["Wo\u00b7rumb", "du", "Mensch", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm, mache durch dein Blut", "tokens": ["Komm", ",", "ma\u00b7che", "durch", "dein", "Blut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die b\u00f6se Sache gut,", "tokens": ["Die", "b\u00f6\u00b7se", "Sa\u00b7che", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sonst bin ich gantz verlohren!", "tokens": ["Sonst", "bin", "ich", "gantz", "ver\u00b7loh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Komm, f\u00fchre Du mein Wort,", "tokens": ["Komm", ",", "f\u00fch\u00b7re", "Du", "mein", "Wort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vnd la\u00df mich, O mein Hort,", "tokens": ["Vnd", "la\u00df", "mich", ",", "O", "mein", "Hort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Spruch der Gnaden h\u00f6ren!", "tokens": ["Den", "Spruch", "der", "Gna\u00b7den", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wil auch jederzeit", "tokens": ["Ich", "wil", "auch", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt vnd in Ewigkeit", "tokens": ["Jetzt", "vnd", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KON", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dich, meinen F\u00fcrsprach, ehren.", "tokens": ["Dich", ",", "mei\u00b7nen", "F\u00fcr\u00b7sprach", ",", "eh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}