{"dta.poem.6313": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In dieser hefftigen Bewegungs-Krafft", "tokens": ["In", "die\u00b7ser", "heff\u00b7ti\u00b7gen", "Be\u00b7we\u00b7gungs\u00b7Krafft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Besteht des Feuers Grund und Eigenschafft.", "tokens": ["Be\u00b7steht", "des", "Feu\u00b7ers", "Grund", "und", "Ei\u00b7gen\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da seine Krafft so gro\u00df zu trennen und zu scheiden;", "tokens": ["Da", "sei\u00b7ne", "Krafft", "so", "gro\u00df", "zu", "tren\u00b7nen", "und", "zu", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df, um selbige zu treiben und zu regen,", "tokens": ["So", "mu\u00df", ",", "um", "sel\u00b7bi\u00b7ge", "zu", "trei\u00b7ben", "und", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "KOUI", "ADJA", "PTKZU", "VVINF", "KON", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es sich nothwendig selbst bewegen.", "tokens": ["Es", "sich", "noth\u00b7wen\u00b7dig", "selbst", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}