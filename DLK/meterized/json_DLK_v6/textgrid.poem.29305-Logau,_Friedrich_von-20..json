{"textgrid.poem.29305": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Ottomannisch K\u00e4yser wollen Geistliche regiren,", "tokens": ["Wie", "ein", "Ot\u00b7to\u00b7man\u00b7nisch", "K\u00e4y\u00b7ser", "wol\u00b7len", "Geist\u00b7li\u00b7che", "re\u00b7gi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Der, den Scepter zu versichern, l\u00e4st die Br\u00fcder stranguliren;", "tokens": ["Der", ",", "den", "Scep\u00b7ter", "zu", "ver\u00b7si\u00b7chern", ",", "l\u00e4st", "die", "Br\u00fc\u00b7der", "stran\u00b7gu\u00b7li\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Also sie in Glaubens-Sachen wollen herrschen und die Br\u00fcder", "tokens": ["Al\u00b7so", "sie", "in", "Glau\u00b7bens\u00b7Sa\u00b7chen", "wol\u00b7len", "herr\u00b7schen", "und", "die", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "NN", "VMFIN", "VVINF", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Lieber r\u00e4umen von dem Brote, wann sie ihrem Wahn zu wider.", "tokens": ["Lie\u00b7ber", "r\u00e4u\u00b7men", "von", "dem", "Bro\u00b7te", ",", "wann", "sie", "ih\u00b7rem", "Wahn", "zu", "wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "PTKZU", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}